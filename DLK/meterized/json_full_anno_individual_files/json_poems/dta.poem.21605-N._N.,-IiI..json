{"dta.poem.21605": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "IiI.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Leuchtet mir nicht allein des Taghimmels flammende Stirne,", "tokens": ["Leuch\u00b7tet", "mir", "nicht", "al\u00b7lein", "des", "Tag\u00b7him\u00b7mels", "flam\u00b7men\u00b7de", "Stir\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "ADV", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+--+-+-+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Kr\u00f6nt sich mir nicht die Nacht mit des Mondes silbernem Reife?", "tokens": ["Kr\u00f6nt", "sich", "mir", "nicht", "die", "Nacht", "mit", "des", "Mon\u00b7des", "sil\u00b7ber\u00b7nem", "Rei\u00b7fe", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PPER", "PTKNEG", "ART", "NN", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+--+-+--+-+--+-", "measure": "hexameter"}, "line.3": {"text": "Waren des dunkelen Waldes melodische Stimmen", "tokens": ["Wa\u00b7ren", "des", "dun\u00b7ke\u00b7len", "Wal\u00b7des", "me\u00b7lo\u00b7di\u00b7sche", "Stim\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN", "ADJA", "NN"], "meter": "+--+--+--+--+-", "measure": "dactylic.penta"}, "line.4": {"text": "Nicht nur ein Wiederhall dieser wundersam str\u00f6menden Kl\u00e4nge,", "tokens": ["Nicht", "nur", "ein", "Wie\u00b7der\u00b7hall", "die\u00b7ser", "wun\u00b7der\u00b7sam", "str\u00f6\u00b7men\u00b7den", "Kl\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ART", "NN", "PDAT", "ADJD", "ADJA", "NN", "$,"], "meter": "+--+----+--+--+-", "measure": "dactylic.di.plus"}, "line.5": {"text": "Die Seligkeit athmend meines Herzens Kirche durchwallten?", "tokens": ["Die", "Se\u00b7lig\u00b7keit", "ath\u00b7mend", "mei\u00b7nes", "Her\u00b7zens", "Kir\u00b7che", "durch\u00b7wall\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "PPOSAT", "NN", "NN", "VVFIN", "$."], "meter": "-+--+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Stammte aus himmlischen H\u00f6hen der befiederte Pfeil nicht,", "tokens": ["Stamm\u00b7te", "aus", "himm\u00b7li\u00b7schen", "H\u00f6\u00b7hen", "der", "be\u00b7fie\u00b7der\u00b7te", "Pfeil", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "ART", "ADJA", "NN", "PTKNEG", "$,"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.7": {"text": "Der mir die Brust durchbohrte und die rosige Wunde mir schlug,", "tokens": ["Der", "mir", "die", "Brust", "durch\u00b7bohr\u00b7te", "und", "die", "ro\u00b7si\u00b7ge", "Wun\u00b7de", "mir", "schlug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "VVFIN", "KON", "ART", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+--+--+", "measure": "iambic.septa.relaxed"}, "line.8": {"text": "Der entquollen mein Glaube, die Kraft und das Wagni\u00df? \u2014", "tokens": ["Der", "ent\u00b7quol\u00b7len", "mein", "Glau\u00b7be", ",", "die", "Kraft", "und", "das", "Wag\u00b7ni\u00df", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "VVFIN", "PPOSAT", "NN", "$,", "ART", "NN", "KON", "ART", "NN", "$.", "$("], "meter": "--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.9": {"text": "Schon schaute ich mich mit dem Pfluge des Geistes", "tokens": ["Schon", "schau\u00b7te", "ich", "mich", "mit", "dem", "Pflu\u00b7ge", "des", "Geis\u00b7tes"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.10": {"text": "Die Lande durchfurchend, die Seelen der Menschen,", "tokens": ["Die", "Lan\u00b7de", "durch\u00b7fur\u00b7chend", ",", "die", "See\u00b7len", "der", "Men\u00b7schen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "ART", "NN", "ART", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.11": {"text": "Brennender Worte lohende Fackel tief in die Herzen versenkend,", "tokens": ["Bren\u00b7nen\u00b7der", "Wor\u00b7te", "lo\u00b7hen\u00b7de", "Fa\u00b7ckel", "tief", "in", "die", "Her\u00b7zen", "ver\u00b7sen\u00b7kend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJA", "NN", "ADJD", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "+--+-+--+-+--+--+-", "measure": "iambic.septa.invert"}, "line.12": {"text": "In saphirnem Gewande hinschreitend zum sonnigen Aether, zum Lichte. \u2014", "tokens": ["In", "sa\u00b7phir\u00b7nem", "Ge\u00b7wan\u00b7de", "hin\u00b7schrei\u00b7tend", "zum", "son\u00b7ni\u00b7gen", "A\u00b7e\u00b7ther", ",", "zum", "Lich\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "APPRART", "ADJA", "NN", "$,", "APPRART", "NN", "$.", "$("], "meter": "+-+--+--+--+---+--+-", "measure": "trochaic.septa.relaxed"}, "line.13": {"text": "Alles zerschlagen nun, alles zerr\u00fcttet;", "tokens": ["Al\u00b7les", "zer\u00b7schla\u00b7gen", "nun", ",", "al\u00b7les", "zer\u00b7r\u00fct\u00b7tet", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "$,", "PIS", "VVPP", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.14": {"text": "Traumgleich versch\u00e4umen die blendenden Bilder", "tokens": ["Traum\u00b7gleich", "ver\u00b7sch\u00e4u\u00b7men", "die", "blen\u00b7den\u00b7den", "Bil\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "ADJA", "NN"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.15": {"text": "In entmastetem Boote treib\u2019 ich auf uferloser, unendlicher See,", "tokens": ["In", "ent\u00b7mas\u00b7te\u00b7tem", "Boo\u00b7te", "treib'", "ich", "auf", "u\u00b7fer\u00b7lo\u00b7ser", ",", "un\u00b7end\u00b7li\u00b7cher", "See", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "APPR", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+--+--+--+", "measure": "trochaic.octa.plus.relaxed"}, "line.16": {"text": "D\u00fcstre Gestalten saugen sich fest mit spitzigen N\u00e4geln", "tokens": ["D\u00fcst\u00b7re", "Ge\u00b7stal\u00b7ten", "sau\u00b7gen", "sich", "fest", "mit", "spit\u00b7zi\u00b7gen", "N\u00e4\u00b7geln"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "PRF", "ADJD", "APPR", "ADJA", "NN"], "meter": "+--+-+--+-+--+-", "measure": "hexameter"}, "line.17": {"text": "An meiner Seele zum k\u00fchn aufstrebenden Schwunge ge\u00f6ffneten Fittich;", "tokens": ["An", "mei\u00b7ner", "See\u00b7le", "zum", "k\u00fchn", "auf\u00b7stre\u00b7ben\u00b7den", "Schwun\u00b7ge", "ge\u00b7\u00f6ff\u00b7ne\u00b7ten", "Fit\u00b7tich", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPRART", "ADJD", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "+--+--+-+--+--+--+-", "measure": "dactylic.di.plus"}, "line.18": {"text": "Bitterer Zweifel schleicht sich heran mit blutleerem Auge,", "tokens": ["Bit\u00b7te\u00b7rer", "Zwei\u00b7fel", "schleicht", "sich", "he\u00b7ran", "mit", "blut\u00b7lee\u00b7rem", "Au\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PRF", "PTKVZ", "APPR", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-+-+-", "measure": "iambic.septa.invert"}, "line.19": {"text": "In fahler Finsterni\u00df versanken meines Lichtes Strahlengarben,", "tokens": ["In", "fah\u00b7ler", "Fins\u00b7ter\u00b7ni\u00df", "ver\u00b7san\u00b7ken", "mei\u00b7nes", "Lich\u00b7tes", "Strah\u00b7len\u00b7gar\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.20": {"text": "Zwerghaft verschrumpft ist meines Muthes stolzer Stamm,", "tokens": ["Zwerg\u00b7haft", "ver\u00b7schrumpft", "ist", "mei\u00b7nes", "Mu\u00b7thes", "stol\u00b7zer", "Stamm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "VAFIN", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.21": {"text": "Aus schmerzzerrissenem Herzen fleh\u2019 ich Rettungsh\u00fclfe,", "tokens": ["Aus", "schmerz\u00b7zer\u00b7ris\u00b7se\u00b7nem", "Her\u00b7zen", "fleh'", "ich", "Ret\u00b7tungs\u00b7h\u00fcl\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "NN", "$,"], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.22": {"text": "Und bebend stammeln meine Lippen auf zu der Sterne goldenen R\u00e4thselzeichen!", "tokens": ["Und", "be\u00b7bend", "stam\u00b7meln", "mei\u00b7ne", "Lip\u00b7pen", "auf", "zu", "der", "Ster\u00b7ne", "gol\u00b7de\u00b7nen", "R\u00e4th\u00b7sel\u00b7zei\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPOSAT", "NN", "APPR", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+--+-+-+-+--+-+--+-+-", "measure": "iambic.octa.plus.invert"}, "line.23": {"text": "Warum \u2014 warum bin ich so tief herniedergesunken? \u2014 \u2014 \u2014", "tokens": ["Wa\u00b7rum", "wa\u00b7rum", "bin", "ich", "so", "tief", "her\u00b7nie\u00b7der\u00b7ge\u00b7sun\u00b7ken", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct", "punct"], "pos": ["PWAV", "$(", "PWAV", "VAFIN", "PPER", "ADV", "ADJD", "VVINF", "$.", "$(", "$(", "$("], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.24": {"text": "\u2014 \u2014 \u2014 \u2014 \u2014 \u2014 \u2014 \u2014 \u2014 \u2014 \u2014 \u2014 \u2014 \u2014 \u2014 \u2014 \u2014 \u2014 \u2014 \u2014", "tokens": [], "token_info": ["punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct"], "pos": ["$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$("]}, "line.25": {"text": "Nein, nein, es soll nicht sein, es darf nicht sein!", "tokens": ["Nein", ",", "nein", ",", "es", "soll", "nicht", "sein", ",", "es", "darf", "nicht", "sein", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PTKANT", "$,", "PPER", "VMFIN", "PTKNEG", "VAINF", "$,", "PPER", "VMFIN", "PTKNEG", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.26": {"text": "Zerschlag\u2019, mein ermattetes Herz, mit wagendem Schwerte", "tokens": ["Zer\u00b7schlag'", ",", "mein", "er\u00b7mat\u00b7te\u00b7tes", "Herz", ",", "mit", "wa\u00b7gen\u00b7dem", "Schwer\u00b7te"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "PPOSAT", "ADJA", "NN", "$,", "APPR", "ADJA", "NN"], "meter": "-+--+--+-+--+-", "measure": "amphibrach.tri.plus"}, "line.27": {"text": "Des Zagens b\u00e4nglich bedr\u00fcckende Sargesumh\u00fcllung,", "tokens": ["Des", "Za\u00b7gens", "b\u00e4ng\u00b7lich", "be\u00b7dr\u00fc\u00b7cken\u00b7de", "Sar\u00b7ge\u00b7sum\u00b7h\u00fcl\u00b7lung", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+--+--+--+-", "measure": "iambic.penta.relaxed"}, "line.28": {"text": "Sch\u00fcttele von dir den aschgrauen Staubesmantel,", "tokens": ["Sch\u00fct\u00b7te\u00b7le", "von", "dir", "den", "aschgrau\u00b7en", "Stau\u00b7bes\u00b7man\u00b7tel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.29": {"text": "Dessen Falten zu Falle gebracht deinen Muth.", "tokens": ["Des\u00b7sen", "Fal\u00b7ten", "zu", "Fal\u00b7le", "ge\u00b7bracht", "dei\u00b7nen", "Muth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "APPR", "NN", "VVPP", "PPOSAT", "NN", "$."], "meter": "+-+--+--+--+", "measure": "trochaic.penta.relaxed"}, "line.30": {"text": "Bin ich doch Herr meiner selber geworden,", "tokens": ["Bin", "ich", "doch", "Herr", "mei\u00b7ner", "sel\u00b7ber", "ge\u00b7wor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "NN", "PPOSAT", "ADV", "VAPP", "$,"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.31": {"text": "Hab\u2019 ich nicht ges\u00fchnet all\u2019 meine s\u00fcndigen Thaten?", "t