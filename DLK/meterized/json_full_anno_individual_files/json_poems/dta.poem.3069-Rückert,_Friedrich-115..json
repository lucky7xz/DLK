{"dta.poem.3069": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "115.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Und meinest du da\u00df dich die meisten h\u00f6ren werden,", "tokens": ["Und", "mei\u00b7nest", "du", "da\u00df", "dich", "die", "meis\u00b7ten", "h\u00f6\u00b7ren", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "KOUS", "PPER", "ART", "PIS", "VVINF", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die ihres Weges gehn im Staube wie die Herden?", "tokens": ["Die", "ih\u00b7res", "We\u00b7ges", "gehn", "im", "Stau\u00b7be", "wie", "die", "Her\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "APPRART", "NN", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Der Hirte dieser Welt f\u00fchrt sie zu Lust und Leide,", "tokens": ["Der", "Hir\u00b7te", "die\u00b7ser", "Welt", "f\u00fchrt", "sie", "zu", "Lust", "und", "Lei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PDAT", "NN", "VVFIN", "PPER", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zur Schlachtbank f\u00fchrt er sie, zuvor zur Sinnenweide.", "tokens": ["Zur", "Schlacht\u00b7bank", "f\u00fchrt", "er", "sie", ",", "zu\u00b7vor", "zur", "Sin\u00b7nen\u00b7wei\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "PPER", "$,", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "La\u00df ihrer blinden Lust sie nachgehn, der zu schwach", "tokens": ["La\u00df", "ih\u00b7rer", "blin\u00b7den", "Lust", "sie", "nach\u00b7gehn", ",", "der", "zu", "schwach"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVIMP", "PPOSAT", "ADJA", "NN", "PPER", "VVINF", "$,", "PRELS", "PTKZU", "VVFIN"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Sie sind zu widerstehn, und geh nicht ihnen nach.", "tokens": ["Sie", "sind", "zu", "wi\u00b7der\u00b7stehn", ",", "und", "geh", "nicht", "ih\u00b7nen", "nach", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKZU", "VVINF", "$,", "KON", "VVFIN", "PTKNEG", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Nach geh du jeder Spur, die nur das Auge sp\u00fcrt", "tokens": ["Nach", "geh", "du", "je\u00b7der", "Spur", ",", "die", "nur", "das", "Au\u00b7ge", "sp\u00fcrt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "VVFIN", "PPER", "PIAT", "NN", "$,", "PRELS", "ADV", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Des Geistes, wo der Geist je Geistige gef\u00fchrt.", "tokens": ["Des", "Geis\u00b7tes", ",", "wo", "der", "Geist", "je", "Geis\u00b7ti\u00b7ge", "ge\u00b7f\u00fchrt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "ART", "NN", "ADV", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Auf, ehr am Boden wo die leise Spur erlischt,", "tokens": ["Auf", ",", "ehr", "am", "Bo\u00b7den", "wo", "die", "lei\u00b7se", "Spur", "er\u00b7lischt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$,", "NN", "APPRART", "NN", "PWAV", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sei sie von deinem Tritt nacheifernd angefrischt.", "tokens": ["Sei", "sie", "von", "dei\u00b7nem", "Tritt", "na\u00b7ch\u00b7ei\u00b7fernd", "an\u00b7ge\u00b7frischt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "PPOSAT", "NN", "VVPP", "VVPP", "$."], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.6": {"line.1": {"text": "Das wird zu Statten nicht nur dir, auch jenen kommen,", "tokens": ["Das", "wird", "zu", "Stat\u00b7ten", "nicht", "nur", "dir", ",", "auch", "je\u00b7nen", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "NN", "PTKNEG", "ADV", "PPER", "$,", "ADV", "PDS", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die nach dir gleichen Wegs mit gleichem Sehnen kommen.", "tokens": ["Die", "nach", "dir", "glei\u00b7chen", "Wegs", "mit", "glei\u00b7chem", "Seh\u00b7nen", "kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPER", "ADJA", "NN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}