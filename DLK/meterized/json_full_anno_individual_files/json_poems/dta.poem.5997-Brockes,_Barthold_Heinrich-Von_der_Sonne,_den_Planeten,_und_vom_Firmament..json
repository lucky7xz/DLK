{"dta.poem.5997": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von der Sonne, den Planeten,  \n und vom Firmament.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Das, was am st\u00e4rcksten uns in Geist und Augen f\u00e4llt,", "tokens": ["Das", ",", "was", "am", "st\u00e4rcks\u00b7ten", "uns", "in", "Geist", "und", "Au\u00b7gen", "f\u00e4llt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PRELS", "APPRART", "ADJA", "PPER", "APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ist jener Lichter Meng\u2019 in den Saphirnen H\u00f6hen,", "tokens": ["Ist", "je\u00b7ner", "Lich\u00b7ter", "Meng'", "in", "den", "Sa\u00b7phir\u00b7nen", "H\u00f6\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDAT", "NN", "NE", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wovon verschiedene best\u00e4ndig stille stehen;", "tokens": ["Wo\u00b7von", "ver\u00b7schie\u00b7de\u00b7ne", "be\u00b7st\u00e4n\u00b7dig", "stil\u00b7le", "ste\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ADJD", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wann in gemessnem Lauff die andern immer gehen.", "tokens": ["Wann", "in", "ge\u00b7mess\u00b7nem", "Lauff", "die", "an\u00b7dern", "im\u00b7mer", "ge\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ADJA", "NN", "ART", "ADJA", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Verschiedne gl\u00e4ntzen stets in ihrem eignen Schein;", "tokens": ["Ver\u00b7schied\u00b7ne", "gl\u00e4nt\u00b7zen", "stets", "in", "ih\u00b7rem", "eig\u00b7nen", "Schein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wann vom erborgten Feur die andern helle seyn.", "tokens": ["Wann", "vom", "er\u00b7borg\u00b7ten", "Feur", "die", "an\u00b7dern", "hel\u00b7le", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPRART", "ADJA", "NN", "ART", "ADJA", "ADJA", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}