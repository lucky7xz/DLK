{"dta.poem.13137": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Neuntes Buch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.85", "af:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Hierauf ward alles still: die ", "tokens": ["Hier\u00b7auf", "ward", "al\u00b7les", "still", ":", "die"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["PAV", "VAFIN", "PIS", "PTKVZ", "$.", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Stund wieder auf und warff den Gegensaz darein:", "tokens": ["Stund", "wie\u00b7der", "auf", "und", "warff", "den", "Ge\u00b7gen\u00b7saz", "da\u00b7rein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PTKVZ", "KON", "VVFIN", "ART", "NN", "PAV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "\u201ered! sprich nur was du willst; doch wirst du nicht erweisen,", "tokens": ["\u201e", "red", "!", "sprich", "nur", "was", "du", "willst", ";", "doch", "wirst", "du", "nicht", "er\u00b7wei\u00b7sen", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "$.", "ADV", "ADV", "PWS", "PPER", "VMFIN", "$.", "ADV", "VAFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "\u201eda\u00df deine Feder mehr, als meine Leyr zu preisen.", "tokens": ["\u201e", "da\u00df", "dei\u00b7ne", "Fe\u00b7der", "mehr", ",", "als", "mei\u00b7ne", "Leyr", "zu", "prei\u00b7sen", "."], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PPOSAT", "NN", "ADV", "$,", "KOUS", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "65\u201dDu sagst man wisse nichts von jenem Alterthum,", "tokens": ["\"", "Du", "sagst", "man", "wis\u00b7se", "nichts", "von", "je\u00b7nem", "Al\u00b7ter\u00b7thum", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PIS", "VVFIN", "PIS", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "\u201eaus dem das Vaterland Herz, Redlichkeit und Ruhm", "tokens": ["\u201e", "aus", "dem", "das", "Va\u00b7ter\u00b7land", "Herz", ",", "Red\u00b7lich\u00b7keit", "und", "Ruhm"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "APPR", "PRELS", "ART", "NN", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "\u201eauch selbst den Nahmen f\u00fchrt. Was machte deine Feder?", "tokens": ["\u201e", "auch", "selbst", "den", "Nah\u00b7men", "f\u00fchrt", ".", "Was", "mach\u00b7te", "dei\u00b7ne", "Fe\u00b7der", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ADV", "ART", "NN", "VVFIN", "$.", "PWS", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "\u201ekonnt Buch\u2019 und Eiche nicht dir dienen wie die Ceder?", "tokens": ["\u201e", "konnt", "Buch'", "und", "Ei\u00b7che", "nicht", "dir", "die\u00b7nen", "wie", "die", "Ce\u00b7der", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VMFIN", "NN", "KON", "NN", "PTKNEG", "PPER", "VVINF", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "\u201edu selbst, redst du nicht oft von jener Helden Zahl,", "tokens": ["\u201e", "du", "selbst", ",", "redst", "du", "nicht", "oft", "von", "je\u00b7ner", "Hel\u00b7den", "Zahl", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "ADV", "$,", "VVFIN", "PPER", "PTKNEG", "ADV", "APPR", "PDAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "70\u201dDer dieses Vaterland so Schuz als Heil empfahl?", "tokens": ["\"", "Der", "die\u00b7ses", "Va\u00b7ter\u00b7land", "so", "Schuz", "als", "Heil", "emp\u00b7fahl", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "PDAT", "NN", "ADV", "NN", "KOUS", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "\u201edu sagst, da\u00df ihre Brust stets ohne Furcht gestritten;", "tokens": ["\u201e", "du", "sagst", ",", "da\u00df", "ih\u00b7re", "Brust", "stets", "oh\u00b7ne", "Furcht", "ge\u00b7strit\u00b7ten", ";"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "$,", "KOUS", "PPOSAT", "NN", "ADV", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "\u201eda\u00df sie mit Redlichkeit, mit wahrhaft deutschen Sitten,", "tokens": ["\u201e", "da\u00df", "sie", "mit", "Red\u00b7lich\u00b7keit", ",", "mit", "wahr\u00b7haft", "deut\u00b7schen", "Sit\u00b7ten", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PPER", "APPR", "NN", "$,", "APPR", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "\u201emit Herzen voller Treu f\u00fcr Land und Leut gewacht;", "tokens": ["\u201e", "mit", "Her\u00b7zen", "vol\u00b7ler", "Treu", "f\u00fcr", "Land", "und", "Leut", "ge\u00b7wacht", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "NN", "ADJA", "NN", "APPR", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "\u201enur die\u00df ein Gl\u00fcck genennt, was Br\u00fcder gl\u00fccklich macht.", "tokens": ["\u201e", "nur", "die\u00df", "ein", "Gl\u00fcck", "ge\u00b7nennt", ",", "was", "Br\u00fc\u00b7der", "gl\u00fcck\u00b7lich", "macht", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "PDS", "ART", "NN", "VVPP", "$,", "PWS", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "75\u201dDu sagst, in diesen sey der Deutschen Ruhm entglommen;", "tokens": ["\"", "Du", "sagst", ",", "in", "die\u00b7sen", "sey", "der", "Deut\u00b7schen", "Ruhm", "ent\u00b7glom\u00b7men", ";"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "$,", "APPR", "PDS", "VAFIN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "\u201evon ihnen sey die Treu und Gro\u00dfmuth hergekommen.", "tokens": ["\u201e", "von", "ih\u00b7nen", "sey", "die", "Treu", "und", "Gro\u00df\u00b7muth", "her\u00b7ge\u00b7kom\u00b7men", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "PPER", "VAFIN", "ART", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "\u201edort habe man den Trieb, der Falschheit liebt, geha\u00dft;", "tokens": ["\u201e", "dort", "ha\u00b7be", "man", "den", "Trieb", ",", "der", "Falschheit", "liebt", ",", "ge\u00b7ha\u00dft", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["$(", "ADV", "VAFIN", "PIS", "ART", "NN", "$,", "ART", "NN", "VVFIN", "$,", "VVPP", "$."], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.18": {"text": "\u201edie Tugend kaum gewu\u00dft, sich doch damit gefa\u00dft.", "tokens": ["\u201e", "die", "Tu\u00b7gend", "kaum", "ge\u00b7wu\u00dft", ",", "sich", "doch", "da\u00b7mit", "ge\u00b7fa\u00dft", "."], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "ADV", "VVPP", "$,", "PRF", "ADV", "PAV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "\u201eder Ehrgeiz, frey zu seyn, und nicht verkauft zu leben", "tokens": ["\u201e", "der", "Ehr\u00b7geiz", ",", "frey", "zu", "seyn", ",", "und", "nicht", "ver\u00b7kauft", "zu", "le\u00b7ben"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["$(", "ART", "NN", "$,", "ADJD", "PTKZU", "VAINF", "$,", "KON", "PTKNEG", "VVPP", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "80\u201dSey von denselben her den Deutschen eingegeben.", "tokens": ["\"", "Sey", "von", "den\u00b7sel\u00b7ben", "her", "den", "Deut\u00b7schen", "ein\u00b7ge\u00b7ge\u00b7ben", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "APPR", "PDS", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "\u201eerz\u00e4hlst du nicht: da\u00df dort der theure Leyer-Klang,", "tokens": ["\u201e", "er\u00b7z\u00e4hlst", "du", "nicht", ":", "da\u00df", "dort", "der", "theu\u00b7re", "Leyer\u00b7Klang", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "PTKNEG", "$.", "KOUS", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.22": {"text": "\u201eweil er dem Krieger Muth von Sieg und Lorbern sang,", "tokens": ["\u201e", "weil", "er", "dem", "Krie\u00b7ger", "Muth", "von", "Sieg", "und", "Lor\u00b7bern", "sang", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PPER", "ART", "NN", "NN", "APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "\u201eden Barden zu dem Haupt desselben Volcks erhoben?", "tokens": ["\u201e", "den", "Bar\u00b7den", "zu", "dem", "Haupt", "des\u00b7sel\u00b7ben", "Volcks", "er\u00b7ho\u00b7ben", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "APPR", "ART", "NN", "PDAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "\u201ewie wei\u00dft du dann so viel an jener Schaar zu loben?", "tokens": ["\u201e", "wie", "wei\u00dft", "du", "dann", "so", "viel", "an", "je\u00b7ner", "Schaar", "zu", "lo\u00b7ben", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "VVFIN", "PPER", "ADV", "ADV", "ADV", "APPR", "PDAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "85\u201dWie wei\u00dft du die\u00df und das von jener finstern Zeit?", "tokens": ["\"", "Wie", "wei\u00dft", "du", "die\u00df", "und", "das", "von", "je\u00b7ner", "fins\u00b7tern", "Zeit", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "VVFIN", "PPER", "PDS", "KON", "ART", "APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "\u201eda nichts als mein Gesang die Helden eingeweiht.", "tokens": ["\u201e", "da", "nichts", "als", "mein", "Ge\u00b7sang", "die", "Hel\u00b7den", "ein\u00b7ge\u00b7weiht", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "PIS", "KOKOM", "PPOSAT", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "\u201ewoher ists dir bekannt? die Barden wirst du sagen,", "tokens": ["\u201e", "wo\u00b7her", "ists", "dir", "be\u00b7kannt", "?", "die", "Bar\u00b7den", "wirst", "du", "sa\u00b7gen", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "VAFIN", "PPER", "PTKVZ", "$.", "ART", "NN", "VAFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "\u201edie wu\u00dften Ehr und Lob der Helden vorzutragen.", "tokens": ["\u201e", "die", "wu\u00df\u00b7ten", "Ehr", "und", "Lob", "der", "Hel\u00b7den", "vor\u00b7zu\u00b7tra\u00b7gen", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "ADJA", "NN", "KON", "NN", "ART", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "\u201eja! Barden: die\u00df Geschlecht ists, da\u00df den Ruhm besang,", "tokens": ["\u201e", "ja", "!", "Bar\u00b7den", ":", "die\u00df", "Ge\u00b7schlecht", "ists", ",", "da\u00df", "den", "Ruhm", "be\u00b7sang", ","], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PTKANT", "$.", "NN", "$.", "PDS", "NN", "VAFIN", "$,", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "90\u201dEs wu\u00dfte nichts um dich, nur um den Cither-Klang.", "tokens": ["\"", "Es", "wu\u00df\u00b7te", "nichts", "um", "dich", ",", "nur", "um", "den", "Cither\u00b7Klang", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PIS", "APPR", "PPER", "$,", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.31": {"text": "\u201eso wiederhohl\u2019 ich frey, da\u00df, was ich jezt besinge,", "tokens": ["\u201e", "so", "wie\u00b7der\u00b7hohl'", "ich", "frey", ",", "da\u00df", ",", "was", "ich", "jezt", "be\u00b7sin\u00b7ge", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPER", "ADJD", "$,", "KOUS", "$,", "PWS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "\u201ebi\u00df in die sp\u00e4tste Welt mit seinem Nachruhm dringe.", "tokens": ["\u201e", "bi\u00df", "in", "die", "sp\u00e4ts\u00b7te", "Welt", "mit", "sei\u00b7nem", "Nach\u00b7ruhm", "drin\u00b7ge", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "APPR", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}