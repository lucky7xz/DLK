{"textgrid.poem.58320": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "358. Des Hirten Stimme", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn ich bin unvermerkt zerstreuet in die Sinnen,", "tokens": ["Wenn", "ich", "bin", "un\u00b7ver\u00b7merkt", "zer\u00b7streu\u00b7et", "in", "die", "Sin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VAFIN", "ADV", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So lockt ein sanfter Zug mich wieder bald von innen;", "tokens": ["So", "lockt", "ein", "sanf\u00b7ter", "Zug", "mich", "wie\u00b7der", "bald", "von", "in\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "PPER", "ADV", "ADV", "APPR", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So treulich sorgt mein Hirt und sch\u00fctzt sein Sch\u00e4felein \u2013", "tokens": ["So", "treu\u00b7lich", "sorgt", "mein", "Hirt", "und", "sch\u00fctzt", "sein", "Sch\u00e4\u00b7fe\u00b7lein", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "PPOSAT", "NN", "KON", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "K\u00f6nnt' ich nur unverr\u00fcckt in ihm gesammelt sein!", "tokens": ["K\u00f6nnt'", "ich", "nur", "un\u00b7ver\u00b7r\u00fcckt", "in", "ihm", "ge\u00b7sam\u00b7melt", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADJD", "APPR", "PPER",