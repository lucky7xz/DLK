{"dta.poem.19298": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "31.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Gott ist ein Denkender, sonst w\u00e4r ich \u00fcber ihn,", "tokens": ["Gott", "ist", "ein", "Den\u00b7ken\u00b7der", ",", "sonst", "w\u00e4r", "ich", "\u00fc\u00b7ber", "ihn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$,", "ADV", "VAFIN", "PPER", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich aber denke, da\u00df ich unter ihm nur bin.", "tokens": ["Ich", "a\u00b7ber", "den\u00b7ke", ",", "da\u00df", "ich", "un\u00b7ter", "ihm", "nur", "bin", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "$,", "KOUS", "PPER", "APPR", "PPER", "ADV", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Gott ist ein Wollender, sonst h\u00e4tt' ich mehr als er,", "tokens": ["Gott", "ist", "ein", "Wol\u00b7len\u00b7der", ",", "sonst", "h\u00e4tt'", "ich", "mehr", "als", "er", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$,", "ADV", "VAFIN", "PPER", "ADV", "KOUS", "PPER", "$,"], "meter": "+--+-+-+-++-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Mein Wollen aber kommt von seinem Wollen her.", "tokens": ["Mein", "Wol\u00b7len", "a\u00b7ber", "kommt", "von", "sei\u00b7nem", "Wol\u00b7len", "her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "VVFIN", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Mit deinem Denken sei, mit deinem Wollen still", "tokens": ["Mit", "dei\u00b7nem", "Den\u00b7ken", "sei", ",", "mit", "dei\u00b7nem", "Wol\u00b7len", "still"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "$,", "APPR", "PPOSAT", "NN", "PTKVZ"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vor seinem, liebes Herz! er denkt in dir und will.", "tokens": ["Vor", "sei\u00b7nem", ",", "lie\u00b7bes", "Herz", "!", "er", "denkt", "in", "dir", "und", "will", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "$,", "ADJA", "NN", "$.", "PPER", "VVFIN", "APPR", "PPER", "KON", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}