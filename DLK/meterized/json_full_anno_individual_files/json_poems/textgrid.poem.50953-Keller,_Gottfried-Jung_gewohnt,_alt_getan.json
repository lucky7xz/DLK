{"textgrid.poem.50953": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Jung gewohnt, alt getan", "genre": "verse", "period": "N.A.", "pub_year": 1850, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Schenke dr\u00f6hnt, und an dem langen Tisch", "tokens": ["Die", "Schen\u00b7ke", "dr\u00f6hnt", ",", "und", "an", "dem", "lan\u00b7gen", "Tisch"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ragt Kopf an Kopf verkommener Gesellen;", "tokens": ["Ragt", "Kopf", "an", "Kopf", "ver\u00b7kom\u00b7me\u00b7ner", "Ge\u00b7sel\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "APPR", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+---+-", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Man pfeift, man lacht; Geschrei, Fluch und Gezisch", "tokens": ["Man", "pfeift", ",", "man", "lacht", ";", "Ge\u00b7schrei", ",", "Fluch", "und", "Ge\u00b7zisch"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PIS", "VVFIN", "$,", "PIS", "VVFIN", "$.", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-++--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Ert\u00f6nte an des Bieres tr\u00fcben Wellen.", "tokens": ["Er\u00b7t\u00f6n\u00b7te", "an", "des", "Bie\u00b7res", "tr\u00fc\u00b7ben", "Wel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "In dieser W\u00fcste gl\u00e4nzt' ein wei\u00dfes Brot,", "tokens": ["In", "die\u00b7ser", "W\u00fcs\u00b7te", "gl\u00e4nzt'", "ein", "wei\u00b7\u00dfes", "Brot", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sah man es an, so ward dem Herzen besser;", "tokens": ["Sah", "man", "es", "an", ",", "so", "ward", "dem", "Her\u00b7zen", "bes\u00b7ser", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PPER", "PTKVZ", "$,", "ADV", "VAFIN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sie drehten eifrig draus ein schwarzes Schrot", "tokens": ["Sie", "dreh\u00b7ten", "eif\u00b7rig", "draus", "ein", "schwar\u00b7zes", "Schrot"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "PAV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und wischten dran die blinden Schenkemesser.", "tokens": ["Und", "wischten", "dran", "die", "blin\u00b7den", "Schen\u00b7ke\u00b7mes\u00b7ser", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PAV", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Doch einem, der da mit den andern schrie,", "tokens": ["Doch", "ei\u00b7nem", ",", "der", "da", "mit", "den", "an\u00b7dern", "schrie", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "$,", "PRELS", "ADV", "APPR", "ART", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Fiel untern Tisch des Brots ein kleiner Bissen;", "tokens": ["Fiel", "un\u00b7tern", "Tisch", "des", "Brots", "ein", "klei\u00b7ner", "Bis\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Schnell fuhr er nieder, wo sich Knie an Knie", "tokens": ["Schnell", "fuhr", "er", "nie\u00b7der", ",", "wo", "sich", "Knie", "an", "Knie"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "PTKVZ", "$,", "PWAV", "PRF", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Gebogen dr\u00e4ngte in den Finsternissen.", "tokens": ["Ge\u00b7bo\u00b7gen", "dr\u00e4ng\u00b7te", "in", "den", "Fins\u00b7ter\u00b7nis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Dort sucht' er selbstvergessen nach dem Brot;", "tokens": ["Dort", "sucht'", "er", "selbst\u00b7ver\u00b7ges\u00b7sen", "nach", "dem", "Brot", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Doch da begann's rings um ihn zu rumoren,", "tokens": ["Doch", "da", "be\u00b7gann's", "rings", "um", "ihn", "zu", "ru\u00b7mo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ADV", "APPR", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sie brachten mit den F\u00fc\u00dfen ihn in Not", "tokens": ["Sie", "brach\u00b7ten", "mit", "den", "F\u00fc\u00b7\u00dfen", "ihn", "in", "Not"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "PPER", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und schrien erbost: \u00bbWas, Kerl! hast du verloren?\u00ab", "tokens": ["Und", "schri\u00b7en", "er\u00b7bost", ":", "\u00bb", "Was", ",", "Kerl", "!", "hast", "du", "ver\u00b7lo\u00b7ren", "?", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "ADJD", "$.", "$(", "PWS", "$,", "NN", "$.", "VAFIN", "PPER", "VVPP", "$.", "$("], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.5": {"line.1": {"text": "Err\u00f6tend taucht' er aus dem dunklen Graus", "tokens": ["Er\u00b7r\u00f6\u00b7tend", "taucht'", "er", "aus", "dem", "dunk\u00b7len", "Graus"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVPP", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und barg das Br\u00f6tchen in des Tischtuchs Falten.", "tokens": ["Und", "barg", "das", "Br\u00f6t\u00b7chen", "in", "des", "Tischtuchs", "Fal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Er sann und sah sein ehrlich Vaterhaus", "tokens": ["Er", "sann", "und", "sah", "sein", "ehr\u00b7lich", "Va\u00b7ter\u00b7haus"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PPOSAT", "ADJD", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und einer edlen Mutter strenges Walten.", "tokens": ["Und", "ei\u00b7ner", "ed\u00b7len", "Mut\u00b7ter", "stren\u00b7ges", "Wal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Nach Jahren aber sa\u00df derselbe Mann", "tokens": ["Nach", "Jah\u00b7ren", "a\u00b7ber", "sa\u00df", "der\u00b7sel\u00b7be", "Mann"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "ADV", "VVFIN", "PDAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Bei Herrn und Damen an der Tafelrunde,", "tokens": ["Bei", "Herrn", "und", "Da\u00b7men", "an", "der", "Ta\u00b7fel\u00b7run\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wo Sonnenlicht das Silber \u00fcberspann", "tokens": ["Wo", "Son\u00b7nen\u00b7licht", "das", "Sil\u00b7ber", "\u00fc\u00b7bers\u00b7pann"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "ART", "NN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und in gew\u00e4hlten Worten floh die Stunde.", "tokens": ["Und", "in", "ge\u00b7w\u00e4hl\u00b7ten", "Wor\u00b7ten", "floh", "die", "Stun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Auch hier lag Brot, wei\u00df wie der Wirtin Hand,", "tokens": ["Auch", "hier", "lag", "Brot", ",", "wei\u00df", "wie", "der", "Wir\u00b7tin", "Hand", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "NN", "$,", "VVFIN", "KOKOM", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wohlschmeckend in dem Dufte guter Sitten;", "tokens": ["Wohl\u00b7schme\u00b7ckend", "in", "dem", "Duf\u00b7te", "gu\u00b7ter", "Sit\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Er selber hielt's nun fest und mit Verstand,", "tokens": ["Er", "sel\u00b7ber", "hielt's", "nun", "fest", "und", "mit", "Ver\u00b7stand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "ADV", "ADJD", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Doch einem Fr\u00e4ulein war ein St\u00fcck entglitten.", "tokens": ["Doch", "ei\u00b7nem", "Fr\u00e4u\u00b7lein", "war", "ein", "St\u00fcck", "ent\u00b7glit\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "\u00bbo lassen Sie es liegen!\u00ab sagt sie schnell;", "tokens": ["\u00bb", "o", "las\u00b7sen", "Sie", "es", "lie\u00b7gen", "!", "\u00ab", "sagt", "sie", "schnell", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["$(", "FM", "VVFIN", "PPER", "PPER", "VVFIN", "$.", "$(", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Zu sp\u00e4t, schon ist er untern Tisch gefahren", "tokens": ["Zu", "sp\u00e4t", ",", "schon", "ist", "er", "un\u00b7tern", "Tisch", "ge\u00b7fah\u00b7ren"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PTKA", "ADJD", "$,", "ADV", "VAFIN", "PPER", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und sp\u00e4ht und sucht, der treffliche Gesell,", "tokens": ["Und", "sp\u00e4ht", "und", "sucht", ",", "der", "treff\u00b7li\u00b7che", "Ge\u00b7sell", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wo kleine seidne F\u00fc\u00dfchen stehn zu Paaren!", "tokens": ["Wo", "klei\u00b7ne", "seid\u00b7ne", "F\u00fc\u00df\u00b7chen", "stehn", "zu", "Paa\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "ADJA", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Die Herren l\u00e4cheln, und die Damen ziehn", "tokens": ["Die", "Her\u00b7ren", "l\u00e4\u00b7cheln", ",", "und", "die", "Da\u00b7men", "ziehn"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "KON", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Sessel scheu zur\u00fcck vor dem Beginnen;", "tokens": ["Die", "Ses\u00b7sel", "scheu", "zu\u00b7r\u00fcck", "vor", "dem", "Be\u00b7gin\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "PTKVZ", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Er taucht empor und legt das Br\u00f6tchen hin,", "tokens": ["Er", "taucht", "em\u00b7por", "und", "legt", "das", "Br\u00f6t\u00b7chen", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "KON", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Err\u00f6tend hin auf das damastne Linnen.", "tokens": ["Er\u00b7r\u00f6\u00b7tend", "hin", "auf", "das", "da\u00b7mast\u00b7ne", "Lin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}}, "stanza.10": {"line.1": {"text": "\u00bbzu artig, Herr!\u00ab dankt' ihm das sch\u00f6ne Kind,", "tokens": ["\u00bb", "zu", "ar\u00b7tig", ",", "Herr", "!", "\u00ab", "dankt'", "ihm", "das", "sch\u00f6\u00b7ne", "Kind", ","], "token_info": ["punct", "word", "word", "punct", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PTKA", "ADJD", "$,", "NN", "$.", "$(", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Indem sie sp\u00f6ttisch l\u00e4chelnd sich verneigte;", "tokens": ["In\u00b7dem", "sie", "sp\u00f6t\u00b7tisch", "l\u00e4\u00b7chelnd", "sich", "ver\u00b7neig\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "ADJD", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Er aber sagte h\u00f6flich und gelind,", "tokens": ["Er", "a\u00b7ber", "sag\u00b7te", "h\u00f6f\u00b7lich", "und", "ge\u00b7lind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Indem er sich gar sittsamlich verbeugte:", "tokens": ["In\u00b7dem", "er", "sich", "gar", "sitt\u00b7sam\u00b7lich", "ver\u00b7beug\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.11": {"line.1": {"text": "\u00bbwohl einer Frau galt meine Artigkeit \u2013", "tokens": ["\u00bb", "wohl", "ei\u00b7ner", "Frau", "galt", "mei\u00b7ne", "Ar\u00b7tig\u00b7keit", "\u2013"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ART", "NN", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Euch aber diesmal nicht, verehrte Dame!", "tokens": ["Euch", "a\u00b7ber", "dies\u00b7mal", "nicht", ",", "ver\u00b7ehr\u00b7te", "Da\u00b7me", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "PTKNEG", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sie galt der Mutter, die vor langer Zeit", "tokens": ["Sie", "galt", "der", "Mut\u00b7ter", ",", "die", "vor", "lan\u00b7ger", "Zeit"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Entschlafen ist in Leid und bittrem Grame.\u00ab", "tokens": ["Ent\u00b7schla\u00b7fen", "ist", "in", "Leid", "und", "bitt\u00b7rem", "Gra\u00b7me", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVPP", "VAFIN", "APPR", "NN", "KON", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.12": {"line.1": {"text": "Die Schenke dr\u00f6hnt, und an dem langen Tisch", "tokens": ["Die", "Schen\u00b7ke", "dr\u00f6hnt", ",", "und", "an", "dem", "lan\u00b7gen", "Tisch"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ragt Kopf an Kopf verkommener Gesellen;", "tokens": ["Ragt", "Kopf", "an", "Kopf", "ver\u00b7kom\u00b7me\u00b7ner", "Ge\u00b7sel\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "APPR", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+---+-", "measure": "unknown.measure.tetra"}, "lin