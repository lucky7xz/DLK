{"dta.poem.176": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Da sie ihn aus ihrem Gesichte verbannete.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Die Lesbia die j\u00fcnst mit Anmuths-Blicken strahlte/", "tokens": ["Die", "Les\u00b7bia", "die", "j\u00fcnst", "mit", "An\u00b7muths\u00b7Bli\u00b7cken", "strahl\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADV", "APPR", "NN", "VVFIN", "$("], "meter": "--+-+-+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Und deren Freundlichkeit so wie die Sonne prahlte/", "tokens": ["Und", "de\u00b7ren", "Freund\u00b7lich\u00b7keit", "so", "wie", "die", "Son\u00b7ne", "prahl\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRELAT", "NN", "ADV", "KOKOM", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Verstellet sich anjetzt in einen Zorn-", "tokens": ["Ver\u00b7stel\u00b7let", "sich", "an\u00b7jetzt", "in", "ei\u00b7nen", "Zorn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "ADV", "APPR", "ART", "TRUNC"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Der \u00fcber meinen Haupt/ o grauses Wesen steht.", "tokens": ["Der", "\u00fc\u00b7ber", "mei\u00b7nen", "Haupt", "/", "o", "grau\u00b7ses", "We\u00b7sen", "steht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPOSAT", "NN", "$(", "FM", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die mich den s\u00fcssen Most gab in Citronen-Schaalen/", "tokens": ["Die", "mich", "den", "s\u00fcs\u00b7sen", "Most", "gab", "in", "Ci\u00b7tro\u00b7nen\u00b7Schaa\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "ADJA", "NN", "VVFIN", "APPR", "NN", "$("], "meter": "-+-+-++-+--+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Schenckt jetzo Wehrmuth ein mit Gall vermengte Quaalen/", "tokens": ["Schenckt", "jet\u00b7zo", "Wehr\u00b7muth", "ein", "mit", "Gall", "ver\u00b7meng\u00b7te", "Qua\u00b7a\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "NN", "ART", "APPR", "NE", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+--", "measure": "unknown.measure.hexa"}, "line.7": {"text": "Ich sa\u00df der Lieb im Schoo\u00df/ da ich dein Liebster war/", "tokens": ["Ich", "sa\u00df", "der", "Lieb", "im", "Schoo\u00df", "/", "da", "ich", "dein", "Liebs\u00b7ter", "war", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "$(", "KOUS", "PPER", "PPOSAT", "NN", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Nun aber ist mein Fall schon mehr denn allzu klar.", "tokens": ["Nun", "a\u00b7ber", "ist", "mein", "Fall", "schon", "mehr", "denn", "all\u00b7zu", "klar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PPOSAT", "NN", "ADV", "ADV", "ADV", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ich sterbe ", "tokens": ["Ich", "ster\u00b7be"], "token_info": ["word", "word"], "pos": ["PPER", "VVFIN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.10": {"text": "Ich mu\u00df nun tausend Angst und herben Schmertz geniessen:", "tokens": ["Ich", "mu\u00df", "nun", "tau\u00b7send", "Angst", "und", "her\u00b7ben", "Schmertz", "ge\u00b7nies\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "CARD", "NN", "KON", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Mein Trost der ist hinweg/ die Hoffnung f\u00e4llt nun ein", "tokens": ["Mein", "Trost", "der", "ist", "hin\u00b7weg", "/", "die", "Hoff\u00b7nung", "f\u00e4llt", "nun", "ein"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ART", "VAFIN", "ADV", "$(", "ART", "NN", "VVFIN", "ADV", "ART"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Denn meine ", "tokens": ["Denn", "mei\u00b7ne"], "token_info": ["word", "word"], "pos": ["KON", "PPOSAT"], "meter": "-+-", "measure": "amphibrach.single"}}}}}