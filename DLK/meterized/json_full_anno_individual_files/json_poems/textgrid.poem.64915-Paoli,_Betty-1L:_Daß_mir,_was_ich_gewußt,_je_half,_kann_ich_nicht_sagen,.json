{"textgrid.poem.64915": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Da\u00df mir, was ich gewu\u00dft, je half, kann ich nicht sagen,", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da\u00df mir, was ich gewu\u00dft, je half, kann ich nicht sagen,", "tokens": ["Da\u00df", "mir", ",", "was", "ich", "ge\u00b7wu\u00dft", ",", "je", "half", ",", "kann", "ich", "nicht", "sa\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "PWS", "PPER", "VVPP", "$,", "ADV", "VVFIN", "$,", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nur das, was ich gekonnt, hat rettend mich getragen.", "tokens": ["Nur", "das", ",", "was", "ich", "ge\u00b7konnt", ",", "hat", "ret\u00b7tend", "mich", "ge\u00b7tra\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDS", "$,", "PWS", "PPER", "ADJD", "$,", "VAFIN", "ADJD", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wohl zu beachten ist das Licht des Wissens auch,", "tokens": ["Wohl", "zu", "be\u00b7ach\u00b7ten", "ist", "das", "Licht", "des", "Wis\u00b7sens", "auch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKZU", "VVINF", "VAFIN", "ART", "NN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch \u00bbk\u00f6nnen\u00ab nur allein ist wahrer Gotteshauch.", "tokens": ["Doch", "\u00bb", "k\u00f6n\u00b7nen", "\u00ab", "nur", "al\u00b7lein", "ist", "wah\u00