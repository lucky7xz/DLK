{"textgrid.poem.34319": {"metadata": {"author": {"name": "Lenz, Jakob Michael Reinhold", "birth": "N.A.", "death": "N.A."}, "title": "A.", "genre": "verse", "period": "N.A.", "pub_year": 1775, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein schlechtgen\u00e4hrter Kandidat", "tokens": ["Ein", "schlecht\u00b7ge\u00b7n\u00e4hr\u00b7ter", "Kan\u00b7di\u00b7dat"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der oftmals einen Fehltritt that", "tokens": ["Der", "oft\u00b7mals", "ei\u00b7nen", "Fehl\u00b7tritt", "that"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und den verbotnen Liebestrieb", "tokens": ["Und", "den", "ver\u00b7bot\u00b7nen", "Lie\u00b7be\u00b7strieb"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In lauter Predigten verschrieb,", "tokens": ["In", "lau\u00b7ter", "Pre\u00b7dig\u00b7ten", "ver\u00b7schrieb", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+---+", "measure": "unknown.measure.tri"}, "line.5": {"text": "Kehrte einst bey einem Pfarrer ein", "tokens": ["Kehr\u00b7te", "einst", "bey", "ei\u00b7nem", "Pfar\u00b7rer", "ein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPR", "ART", "NN", "ART"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Den Sonntag sein Geh\u00fclf zu seyn.", "tokens": ["Den", "Sonn\u00b7tag", "sein", "Ge\u00b7h\u00fclf", "zu", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Der hat ein Kind, zwar still und bleich,", "tokens": ["Der", "hat", "ein", "Kind", ",", "zwar", "still", "und", "bleich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von Kummer krank, doch Engeln gleich. \u2013", "tokens": ["Von", "Kum\u00b7mer", "krank", ",", "doch", "En\u00b7geln", "gleich", ".", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "NN", "ADJD", "$,", "KON", "NN", "ADV", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie hielt im halberloschnen Blick", "tokens": ["Sie", "hielt", "im", "hal\u00b7ber\u00b7lo\u00b7schnen", "Blick"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Noch Flamen ohne Maa\u00df zur\u00fcck;", "tokens": ["Noch", "Fla\u00b7men", "oh\u00b7ne", "Maa\u00df", "zu\u00b7r\u00fcck", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "All itzt in Andacht eingeh\u00fcllt.", "tokens": ["All", "itzt", "in", "An\u00b7dacht", "ein\u00b7ge\u00b7h\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sch\u00f6n wie ein marmorn Heil'genbild. \u2013", "tokens": ["Sch\u00f6n", "wie", "ein", "mar\u00b7morn", "Heil'\u00b7gen\u00b7bild", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADJD", "KOKOM", "ART", "ADJA", "NN", "$.", "$("], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.7": {"text": "War nicht umsonst so still und schwach,", "tokens": ["War", "nicht", "um\u00b7sonst", "so", "still", "und", "schwach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "ADV", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Verla\u00dfne Liebe trug sie nach,", "tokens": ["Ver\u00b7la\u00df\u00b7ne", "Lie\u00b7be", "trug", "sie", "nach", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "In ihrer kleinen Kammer hoch", "tokens": ["In", "ih\u00b7rer", "klei\u00b7nen", "Kam\u00b7mer", "hoch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Sie stets an der Erinnerung sog;", "tokens": ["Sie", "stets", "an", "der", "E\u00b7rin\u00b7ne\u00b7rung", "sog", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-++--+--+", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "An ihrem Brodschrank an der Wand", "tokens": ["An", "ih\u00b7rem", "Brod\u00b7schrank", "an", "der", "Wand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Er immer immer vor ihr stand,", "tokens": ["Er", "im\u00b7mer", "im\u00b7mer", "vor", "ihr", "stand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Und wenn ein Schlaf sie \u00fcbernam,", "tokens": ["Und", "wenn", "ein", "Schlaf", "sie", "\u00fc\u00b7ber\u00b7nam", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Im Traum er immer wieder kam.", "tokens": ["Im", "Traum", "er", "im\u00b7mer", "wie\u00b7der", "kam", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "F\u00fcr ihn sie noch das H\u00e4rlein stutzt,", "tokens": ["F\u00fcr", "ihn", "sie", "noch", "das", "H\u00e4r\u00b7lein", "stutzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PPER", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sich wenn sie ganz allein ist putzt,", "tokens": ["Sich", "wenn", "sie", "ganz", "al\u00b7lein", "ist", "putzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "KOUS", "PPER", "ADV", "ADV", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "All ihre Sch\u00fcrzen anprobirt", "tokens": ["All", "ih\u00b7re", "Sch\u00fcr\u00b7zen", "an\u00b7pro\u00b7birt"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "PPOSAT", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und ihre sch\u00f6nen L\u00e4tzchen schn\u00fcrt,", "tokens": ["Und", "ih\u00b7re", "sch\u00f6\u00b7nen", "L\u00e4tz\u00b7chen", "schn\u00fcrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und vor dem Spiegel nur allein", "tokens": ["Und", "vor", "dem", "Spie\u00b7gel", "nur", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Verlangt, er soll ihr Schmeichler seyn.", "tokens": ["Ver\u00b7langt", ",", "er", "soll", "ihr", "Schmeich\u00b7ler", "seyn", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPER", "VMFIN", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Kam aber etwas fremds in's Haus,", "tokens": ["Kam", "a\u00b7ber", "et\u00b7was", "fremds", "in's", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADV", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "That sie sich schlecht und h\u00e4uslich aus.", "tokens": ["That", "sie", "sich", "schlecht", "und", "h\u00e4us\u00b7lich", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PRF", "ADJD", "KON", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Denn immer immer immer doch", "tokens": ["Denn", "im\u00b7mer", "im\u00b7mer", "im\u00b7mer", "doch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Schwebt ihr das Bild an W\u00e4nden noch", "tokens": ["Schwebt", "ihr", "das", "Bild", "an", "W\u00e4n\u00b7den", "noch"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPR", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Von einem Menschen, welcher kam", "tokens": ["Von", "ei\u00b7nem", "Men\u00b7schen", ",", "wel\u00b7cher", "kam"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und ihr als Kind das Herze nam.", "tokens": ["Und", "ihr", "als", "Kind", "das", "Her\u00b7ze", "nam", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "KOUS", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Fast ausgel\u00f6scht ist sein Gesicht,", "tokens": ["Fast", "aus\u00b7ge\u00b7l\u00f6scht", "ist", "sein", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Doch seiner Worte Kraft noch nicht", "tokens": ["Doch", "sei\u00b7ner", "Wor\u00b7te", "Kraft", "noch", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "NN", "ADV", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und jener Stunden Seligkeit", "tokens": ["Und", "je\u00b7ner", "Stun\u00b7den", "Se\u00b7lig\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PDAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und jener Tr\u00e4ume Wirklichkeit", "tokens": ["Und", "je\u00b7ner", "Tr\u00e4u\u00b7me", "Wirk\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PDAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Die angeboren jedermann", "tokens": ["Die", "an\u00b7ge\u00b7bo\u00b7ren", "je\u00b7der\u00b7mann"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "PIS"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Kein Mensch sich wirklich machen kann.", "tokens": ["Kein", "Mensch", "sich", "wirk\u00b7lich", "ma\u00b7chen", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PRF", "ADJD", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Ach M\u00e4nner M\u00e4nner seyd nicht stolz", "tokens": ["Ach", "M\u00e4n\u00b7ner", "M\u00e4n\u00b7ner", "seyd", "nicht", "stolz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "NN", "NN", "VAFIN", "PTKNEG", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Als w\u00e4rt nur ihr das gr\u00fcne Holz.", "tokens": ["Als", "w\u00e4rt", "nur", "ihr", "das", "gr\u00fc\u00b7ne", "Holz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "ADV", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Weiber G\u00fct' und Duldsamkeit", "tokens": ["Der", "Wei\u00b7ber", "G\u00fct'", "und", "Duld\u00b7sam\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ist grenzenlos wie Ewigkeit.", "tokens": ["Ist", "gren\u00b7zen\u00b7los", "wie", "E\u00b7wig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "KOKOM", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Ein schlechtgen\u00e4hrter Kandidat", "tokens": ["Ein", "schlecht\u00b7ge\u00b7n\u00e4hr\u00b7ter", "Kan\u00b7di\u00b7dat"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der oftmals einen Fehltritt that", "tokens": ["Der", "oft\u00b7mals", "ei\u00b7nen", "Fehl\u00b7tritt", "that"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "ia