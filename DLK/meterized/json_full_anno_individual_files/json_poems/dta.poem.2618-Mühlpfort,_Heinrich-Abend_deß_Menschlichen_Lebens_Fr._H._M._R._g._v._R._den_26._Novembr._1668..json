{"dta.poem.2618": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Abend de\u00df Menschlichen Lebens/  \n  Fr. H. M. R. g. v. R. den 26. Novembr.  1668.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Bey angetretner Nacht und finstren Abends-Schat\nten/", "tokens": ["Bey", "an\u00b7ge\u00b7tret\u00b7ner", "Nacht", "und", "finst\u00b7ren", "A\u00b7bends\u00b7Schat", "ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "ADJA", "NN", "FM", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da gleich der Sonnen-Rad durch Sch\u00fctz\u2019 und Stein-", "tokens": ["Da", "gleich", "der", "Son\u00b7nen\u00b7Rad", "durch", "Sch\u00fctz'", "und", "Stein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "NN", "APPR", "NN", "KON", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Soll ich den letzten Dienst der edlen Leich abstatten/", "tokens": ["Soll", "ich", "den", "letz\u00b7ten", "Dienst", "der", "ed\u00b7len", "Leich", "ab\u00b7stat\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die Ehr- und Lebens-satt auf ihrer Bahre steht.", "tokens": ["Die", "Ehr", "und", "Le\u00b7bens\u00b7satt", "auf", "ih\u00b7rer", "Bah\u00b7re", "steht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "TRUNC", "KON", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Unwiderruflich ists/ da\u00df sie die Nacht erreichet/", "tokens": ["Un\u00b7wi\u00b7der\u00b7ruf\u00b7lich", "ists", "/", "da\u00df", "sie", "die", "Nacht", "er\u00b7rei\u00b7chet", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "$(", "KOUS", "PPER", "ART", "NN", "VVFIN", "$("], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.6": {"text": "Die ein Verruchter fleucht/ ein Frommer aber hofft.", "tokens": ["Die", "ein", "Ver\u00b7ruch\u00b7ter", "fleucht", "/", "ein", "From\u00b7mer", "a\u00b7ber", "hofft", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVFIN", "$(", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ja weil de\u00df Lebens-Baum nur einem Tage gleichet/", "tokens": ["Ja", "weil", "de\u00df", "Le\u00b7bens\u00b7Baum", "nur", "ei\u00b7nem", "Ta\u00b7ge", "glei\u00b7chet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "KOUS", "ART", "NN", "ADV", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "K\u00f6mmt sie bey Demmerung de\u00df Alters zu der Grufft.", "tokens": ["K\u00f6mmt", "sie", "bey", "Dem\u00b7me\u00b7rung", "de\u00df", "Al\u00b7ters", "zu", "der", "Grufft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ach freylich ist ein Tag des gantzen Lebens-L\u00e4nge!", "tokens": ["Ach", "frey\u00b7lich", "ist", "ein", "Tag", "des", "gant\u00b7zen", "Le\u00b7bens\u00b7L\u00e4n\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "VAFIN", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Wie sich der Morgen-Schein mit Dampff und Nebel", "tokens": ["Wie", "sich", "der", "Mor\u00b7gen\u00b7Schein", "mit", "Dampff", "und", "Ne\u00b7bel"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PRF", "ART", "NN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "So sind wir auch befleckt durch vieler S\u00fcnden-Menge/", "tokens": ["So", "sind", "wir", "auch", "be\u00b7fleckt", "durch", "vie\u00b7ler", "S\u00fcn\u00b7den\u00b7Men\u00b7ge", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "VVPP", "APPR", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Die bald nach der Geburt mit Erb-Schuld dr\u00fcckt den", "tokens": ["Die", "bald", "nach", "der", "Ge\u00b7burt", "mit", "Er\u00b7bSchuld", "dr\u00fcckt", "den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "APPR", "ART", "NN", "APPR", "NN", "VVFIN", "ART"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Und wie der Sonnen-Liecht uns zu der Arbeit fodert/", "tokens": ["Und", "wie", "der", "Son\u00b7nen\u00b7Liecht", "uns", "zu", "der", "Ar\u00b7beit", "fo\u00b7dert", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "PPER", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "So mahnt die Tugend auch die Fr\u00fch-Jahr ernstlich an/", "tokens": ["So", "mahnt", "die", "Tu\u00b7gend", "auch", "die", "Fr\u00fch\u00b7Jahr", "ernst\u00b7lich", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "ART", "NN", "ADJD", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Da\u00df nicht in M\u00fcssiggang die erste Zeit verlodert/", "tokens": ["Da\u00df", "nicht", "in", "M\u00fcs\u00b7sig\u00b7gang", "die", "ers\u00b7te", "Zeit", "ver\u00b7lo\u00b7dert", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "APPR", "NN", "ART", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Wer hier nicht wachsam ist fehlt k\u00fcnfftig Ziel und Bahn.", "tokens": ["Wer", "hier", "nicht", "wach\u00b7sam", "ist", "fehlt", "k\u00fcnff\u00b7tig", "Ziel", "und", "Bahn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "PTKNEG", "ADJD", "VAFIN", "VVFIN", "ADJD", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Wenn nun des Himmels Hertz\u2019 und Auge h\u00f6her kommen/", "tokens": ["Wenn", "nun", "des", "Him\u00b7mels", "Hertz'", "und", "Au\u00b7ge", "h\u00f6\u00b7her", "kom\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "NE", "KON", "NN", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Stellt uns die sch\u00f6nste Pracht der Mittags-Circel dar.", "tokens": ["Stellt", "uns", "die", "sch\u00f6ns\u00b7te", "Pracht", "der", "Mit\u00b7tags\u00b7Cir\u00b7cel", "dar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "So auch wenn jetzt der Mensch in Jahren zugenommen/", "tokens": ["So", "auch", "wenn", "jetzt", "der", "Mensch", "in", "Jah\u00b7ren", "zu\u00b7ge\u00b7nom\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "KOUS", "ADV", "ART", "NN", "APPR", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Nimmt man der Ehre Glantz und Strahlen an ihm wahr.", "tokens": ["Nimmt", "man", "der", "Eh\u00b7re", "Glantz", "und", "Strah\u00b7len", "an", "ihm", "wahr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ART", "NN", "NN", "KON", "NN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Den ziert ein wichtig Ampt/ den des Geluckes-Blicke/", "tokens": ["Den", "ziert", "ein", "wich\u00b7tig", "Ampt", "/", "den", "des", "Ge\u00b7lu\u00b7ckes\u00b7Bli\u00b7cke", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "ADJD", "NN", "$(", "ART", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Der steht bey Kronen wol/ ein ander auf der See/", "tokens": ["Der", "steht", "bey", "Kro\u00b7nen", "wol", "/", "ein", "an\u00b7der", "auf", "der", "See", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "NN", "ADV", "$(", "ART", "ADJD", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Doch wie den Mittag tr\u00fcbt der Wolcken schwartze T\u00fccke/", "tokens": ["Doch", "wie", "den", "Mit\u00b7tag", "tr\u00fcbt", "der", "Wol\u00b7cken", "schwart\u00b7ze", "T\u00fc\u00b7cke", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "VVFIN", "ART", "NN", "VVFIN", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "So st\u00fcrtzt die Grossen auch ein unverhofftes Weh.", "tokens": ["So", "st\u00fcrtzt", "die", "Gros\u00b7sen", "auch", "ein", "un\u00b7ver\u00b7hoff\u00b7tes", "Weh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Und wenn man nun gedenckt des Liechtes zu geniessen/", "tokens": ["Und", "wenn", "man", "nun", "ge\u00b7denckt", "des", "Liech\u00b7tes", "zu", "ge\u00b7nies\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PIS", "ADV", "VVFIN", "ART", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Ach wie bald eilet nicht die Sonne Himmel ab!", "tokens": ["Ach", "wie", "bald", "ei\u00b7let", "nicht", "die", "Son\u00b7ne", "Him\u00b7mel", "ab", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ADV", "VVFIN", "PTKNEG", "ART", "NN", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Da sehn wir Liecht und Tag die Finstern\u00fc\u00df beschliessen/", "tokens": ["Da", "sehn", "wir", "Liecht", "und", "Tag", "die", "Fins\u00b7ter\u00b7n\u00fc\u00df", "be\u00b7schlies\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "KON", "NN", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Und unser gantzes Thun verf\u00e4llt in Sarg und Grab.", "tokens": ["Und", "un\u00b7ser", "gant\u00b7zes", "Thun", "ver\u00b7f\u00e4llt", "in", "Sarg", "und", "Grab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Des Lebens Abend hat uns heimlich \u00fcberschlichen/", "tokens": ["Des", "Le\u00b7bens", "A\u00b7bend", "hat", "uns", "heim\u00b7lich", "\u00fc\u00b7berschli\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "PPER", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.30": {"text": "Die Herbrig in der Welt ist voller Mord und Tod/", "tokens": ["Die", "Her\u00b7brig", "in", "der", "Welt", "ist", "vol\u00b7ler", "Mord", "und", "Tod", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VAFIN", "ADJA", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Gespenster werden uns im Finstern \u00fcberkriechen/", "tokens": ["Ge\u00b7spens\u00b7ter", "wer\u00b7den", "uns", "im", "Fins\u00b7tern", "\u00fc\u00b7ber\u00b7krie\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "APPRART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Wo suchen wir nun Trost und Rettung als bey ", "tokens": ["Wo", "su\u00b7chen", "wir", "nun", "Trost", "und", "Ret\u00b7tung", "als", "bey"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "NN", "KON", "NN", "KOKOM", "APPR"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.33": {"text": "Noch weiter/ wenn der HErr die Rechnung ab-will nehmen/", "tokens": ["Noch", "wei\u00b7ter", "/", "wenn", "der", "Herr", "die", "Rech\u00b7nung", "ab\u00b7will", "neh\u00b7men", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$(", "KOUS", "ART", "NN", "ART", "NN", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Stellt das Gewissen nicht uns tausend M\u00e4ngel aus?", "tokens": ["Stellt", "das", "Ge\u00b7wis\u00b7sen", "nicht", "uns", "tau\u00b7send", "M\u00e4n\u00b7gel", "aus", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKNEG", "PPER", "CARD", "NN", "PTKVZ", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.35": {"text": "Wenns zu dem Schlusse kommt/ so m\u00fcssen wir uns sch\u00e4men/", "tokens": ["Wenns", "zu", "dem", "Schlus\u00b7se", "kommt", "/", "so", "m\u00fcs\u00b7sen", "wir", "uns", "sch\u00e4\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "VVFIN", "$(", "ADV", "VMFIN", "PPER", "PRF", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Und jede Ziefer lehrt/ wie schlecht bestellt das Haus.", "tokens": ["Und", "je\u00b7de", "Zie\u00b7fer", "lehrt", "/", "wie", "schlecht", "be\u00b7stellt", "das", "Haus", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "$(", "PWAV", "ADJD", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Der Jugend Morgen ist in schn\u00f6der Lust verflogen/", "tokens": ["Der", "Ju\u00b7gend", "Mor\u00b7gen", "ist", "in", "schn\u00f6\u00b7der", "Lust", "ver\u00b7flo\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "APPR", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Des Lebens Mittag hat die Ehr-Sucht gantz verbrant.", "tokens": ["Des", "Le\u00b7bens", "Mit\u00b7tag", "hat", "die", "Ehr\u00b7Sucht", "gantz", "ver\u00b7brant", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+---+", "measure": "unknown.measure.penta"}, "line.39": {"text": "Den hat der d\u00fcrre Geitz so hefftig au\u00dfgesogen/", "tokens": ["Den", "hat", "der", "d\u00fcr\u00b7re", "Geitz", "so", "heff\u00b7tig", "au\u00df\u00b7ge\u00b7so\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "ADJA", "NN", "ADV", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Da\u00df er den Goldklos zwar doch niemals ", "tokens": ["Da\u00df", "er", "den", "Gold\u00b7klos", "zwar", "doch", "nie\u00b7mals"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "ADV", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.41": {"text": "Wenn nun sich allgemach Geist/ Krafft und Blut verliehren/", "tokens": ["Wenn", "nun", "sich", "all\u00b7ge\u00b7mach", "Geist", "/", "Krafft", "und", "Blut", "ver\u00b7lieh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PRF", "ADV", "NN", "$(", "NN", "KON", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-"