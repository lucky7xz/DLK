{"dta.poem.5619": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Vom Geist und C\u00f6rper", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Zuerst hat man die Welt nicht anders anzusehn,", "tokens": ["Zu\u00b7erst", "hat", "man", "die", "Welt", "nicht", "an\u00b7ders", "an\u00b7zu\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "ART", "NN", "PTKNEG", "ADV", "VVIZU", "$,"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Als Lettern, wovon wir den Inhalt nicht verstehn.", "tokens": ["Als", "Let\u00b7tern", ",", "wo\u00b7von", "wir", "den", "In\u00b7halt", "nicht", "ver\u00b7stehn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "$,", "PWAV", "PPER", "ART", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ein jeder Vorwurff ist mit Zahlen \u00fcberdeckt,", "tokens": ["Ein", "je\u00b7der", "Vor\u00b7wurff", "ist", "mit", "Zah\u00b7len", "\u00fc\u00b7berd\u00b7eckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VAFIN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Worinnen was geheimes steckt,", "tokens": ["Wo\u00b7rin\u00b7nen", "was", "ge\u00b7hei\u00b7mes", "steckt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PWS", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Das man ergr\u00fcbeln soll, das man durchdringen mu\u00df.", "tokens": ["Das", "man", "er\u00b7gr\u00fc\u00b7beln", "soll", ",", "das", "man", "durch\u00b7drin\u00b7gen", "mu\u00df", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "VVINF", "VMFIN", "$,", "PRELS", "PIS", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Es l\u00e4sst sich auch sobald kein Urtheil-Schlu\u00df,", "tokens": ["Es", "l\u00e4sst", "sich", "auch", "so\u00b7bald", "kein", "Urt\u00b7heil\u00b7Schlu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADV", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "In unbekannten Sachen", "tokens": ["In", "un\u00b7be\u00b7kann\u00b7ten", "Sa\u00b7chen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Unwiedersprechlich machen,", "tokens": ["Un\u00b7wie\u00b7der\u00b7sprech\u00b7lich", "ma\u00b7chen", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Man such\u2019 in unserm Witz zuerst den Schein der Wahrheit.", "tokens": ["Man", "such'", "in", "un\u00b7serm", "Witz", "zu\u00b7erst", "den", "Schein", "der", "Wahr\u00b7heit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "PPOSAT", "NN", "ADV", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Nur einfach sey die Lehr und voller Klarheit.", "tokens": ["Nur", "ein\u00b7fach", "sey", "die", "Lehr", "und", "vol\u00b7ler", "Klar\u00b7heit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "ART", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Es m\u00fcsse nichts darinn sich widersprechen;", "tokens": ["Es", "m\u00fcs\u00b7se", "nichts", "da\u00b7rinn", "sich", "wi\u00b7der\u00b7spre\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIS", "PAV", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Es mu\u00df befestigt seyn und recht verbunden,", "tokens": ["Es", "mu\u00df", "be\u00b7fes\u00b7tigt", "seyn", "und", "recht", "ver\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVPP", "VAINF", "KON", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "So durch Erfahrung, als Verstand:", "tokens": ["So", "durch", "Er\u00b7fah\u00b7rung", ",", "als", "Ver\u00b7stand", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "$,", "KOUS", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Was man gemuhtmasst hat, wird denn zur Festigkeit.", "tokens": ["Was", "man", "ge\u00b7muht\u00b7masst", "hat", ",", "wird", "denn", "zur", "Fes\u00b7tig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "VVPP", "VAFIN", "$,", "VAFIN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Der Geist der\u2019s also macht, ist dann mit recht erfreut,", "tokens": ["Der", "Geist", "der's", "al\u00b7so", "macht", ",", "ist", "dann", "mit", "recht", "er\u00b7freut", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIS", "ADV", "VVFIN", "$,", "VAFIN", "ADV", "APPR", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Da\u00df er die Wahrheit ausgefunden.", "tokens": ["Da\u00df", "er", "die", "Wahr\u00b7heit", "aus\u00b7ge\u00b7fun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}