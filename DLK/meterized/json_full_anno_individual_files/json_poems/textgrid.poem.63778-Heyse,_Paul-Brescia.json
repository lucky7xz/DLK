{"textgrid.poem.63778": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "Brescia", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie locken mich all deine Lieblichkeiten,", "tokens": ["Wie", "lo\u00b7cken", "mich", "all", "dei\u00b7ne", "Lieb\u00b7lich\u00b7kei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PIAT", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Du sch\u00f6nes Brescia! Nur noch einmal schauen", "tokens": ["Du", "sch\u00f6\u00b7nes", "Bre\u00b7scia", "!", "Nur", "noch", "ein\u00b7mal", "schau\u00b7en"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "ADJA", "NE", "$.", "ADV", "ADV", "ADV", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "M\u00f6cht' ich Morettos f\u00fcrstlich-holde Frauen", "tokens": ["M\u00f6cht'", "ich", "Mo\u00b7ret\u00b7tos", "f\u00fcrst\u00b7lich\u00b7hol\u00b7de", "Frau\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "NE", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und all die werte Kunst versunkner Zeiten.", "tokens": ["Und", "all", "die", "wer\u00b7te", "Kunst", "ver\u00b7sun\u00b7kner", "Zei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ART", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wie durch ein M\u00e4rchen glaubt' ich hinzuschreiten", "tokens": ["Wie", "durch", "ein", "M\u00e4r\u00b7chen", "glaubt'", "ich", "hin\u00b7zu\u00b7schrei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN", "VVFIN", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In totenstillen Gassen, an den grauen", "tokens": ["In", "to\u00b7tens\u00b7til\u00b7len", "Gas\u00b7sen", ",", "an", "den", "grau\u00b7en"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "APPR", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Pal\u00e4sten hin; nur das Geschrei der Pfauen", "tokens": ["Pa\u00b7l\u00e4s\u00b7ten", "hin", ";", "nur", "das", "Ge\u00b7schrei", "der", "Pfau\u00b7en"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "PTKVZ", "$.", "ADV", "ART", "NN", "ART", "NN"], "meter": "-+---+-+-+-", "measure": "dactylic.init"}, "line.4": {"text": "Drang \u00fcber Gartenmauern mir zur Seiten.", "tokens": ["Drang", "\u00fc\u00b7ber", "Gar\u00b7ten\u00b7mau\u00b7ern", "mir", "zur", "Sei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Doch wo die alten Tempeltr\u00fcmmer gr\u00fc\u00dfen", "tokens": ["Doch", "wo", "die", "al\u00b7ten", "Tem\u00b7pel\u00b7tr\u00fcm\u00b7mer", "gr\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Aus dunkler Feigen Laub, trat ich hinein", "tokens": ["Aus", "dunk\u00b7ler", "Fei\u00b7gen", "Laub", ",", "trat", "ich", "hin\u00b7ein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "NN", "$,", "VVFIN", "PPER", "PTKVZ"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und sah die sch\u00f6nste der Viktorien thron