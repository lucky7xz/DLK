{"dta.poem.17946": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "189.  Alles begehren ist nichts haben.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Mensch glaube di\u00df gewi\u00df/ hastu nach allm Begihr/", "tokens": ["Mensch", "glau\u00b7be", "di\u00df", "ge\u00b7wi\u00df", "/", "has\u00b7tu", "nach", "allm", "Be\u00b7gihr", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PDS", "ADV", "$(", "VAFIN", "APPR", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So bistu bettel arm und hast noch nichts in dir.", "tokens": ["So", "bis\u00b7tu", "bet\u00b7tel", "arm", "und", "hast", "noch", "nichts", "in", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "ADJD", "KON", "VAFIN", "ADV", "PIS", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}