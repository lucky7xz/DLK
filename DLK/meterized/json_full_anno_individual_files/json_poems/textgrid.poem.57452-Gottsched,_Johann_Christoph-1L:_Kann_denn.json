{"textgrid.poem.57452": {"metadata": {"author": {"name": "Gottsched, Johann Christoph", "birth": "N.A.", "death": "N.A."}, "title": "1L: Kann denn ", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kann denn ", "tokens": ["Kann", "denn"], "token_info": ["word", "word"], "pos": ["VMFIN", "ADV"], "meter": "+-", "measure": "trochaic.single"}, "line.2": {"text": "Auch in den gek\u00fcrzten Tagen,", "tokens": ["Auch", "in", "den", "ge\u00b7k\u00fcrz\u00b7ten", "Ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Mitten unter Frost und Schnee,", "tokens": ["Mit\u00b7ten", "un\u00b7ter", "Frost", "und", "Schnee", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "In verliebte Herzen schlagen?", "tokens": ["In", "ver\u00b7lieb\u00b7te", "Her\u00b7zen", "schla\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "F\u00fchlt denn auch, bey kalten L\u00fcften,", "tokens": ["F\u00fchlt", "denn", "auch", ",", "bey", "kal\u00b7ten", "L\u00fcf\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Der bereifte Theil der Welt,", "tokens": ["Der", "be\u00b7reif\u00b7te", "Theil", "der", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Um den kalten Norderbelt,", "tokens": ["Um", "den", "kal\u00b7ten", "Nor\u00b7der\u00b7belt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Was der ", "tokens": ["Was", "der"], "token_info": ["word", "word"], "pos": ["PWS", "ART"], "meter": "+-", "measure": "trochaic.single"}}, "stanza.2": {"line.1": {"text": "Ja, die starrende Natur", "tokens": ["Ja", ",", "die", "star\u00b7ren\u00b7de", "Na\u00b7tur"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["PTKANT", "$,", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Schl\u00e4ft in Auen, G\u00e4rten, Feldern;", "tokens": ["Schl\u00e4ft", "in", "Au\u00b7en", ",", "G\u00e4r\u00b7ten", ",", "Fel\u00b7dern", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "$,", "NN", "$,", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wer erblickt die mindste Spur", "tokens": ["Wer", "er\u00b7blickt", "die", "minds\u00b7te", "Spur"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "S\u00fc\u00dfer Regung in den W\u00e4ldern?", "tokens": ["S\u00fc\u00b7\u00dfer", "Re\u00b7gung", "in", "den", "W\u00e4l\u00b7dern", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Bey den Fischen, V\u00f6geln, Thieren", "tokens": ["Bey", "den", "Fi\u00b7schen", ",", "V\u00f6\u00b7geln", ",", "Thie\u00b7ren"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word"], "pos": ["APPR", "ART", "NN", "$,", "NN", "$,", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Scheinen alle Triebe todt:", "tokens": ["Schei\u00b7nen", "al\u00b7le", "Trie\u00b7be", "todt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Doch die\u00df m\u00e4chtige Geboth", "tokens": ["Doch", "die\u00df", "m\u00e4ch\u00b7ti\u00b7ge", "Ge\u00b7both"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PDS", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Kann nur nicht die Menschen r\u00fchren.", "tokens": ["Kann", "nur", "nicht", "die", "Men\u00b7schen", "r\u00fch\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PTKNEG", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Nur der Mensch, die kleine Welt,", "tokens": ["Nur", "der", "Mensch", ",", "die", "klei\u00b7ne", "Welt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Will der gro\u00dfen widerstreben;", "tokens": ["Will", "der", "gro\u00b7\u00dfen", "wi\u00b7der\u00b7stre\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "ADJA", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Weil er nichts von Regeln h\u00e4lt,", "tokens": ["Weil", "er", "nichts", "von", "Re\u00b7geln", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIS", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Will er stets in Freyheit leben.", "tokens": ["Will", "er", "stets", "in", "Frey\u00b7heit", "le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Er verlacht, mit muntern Sinnen,", "tokens": ["Er", "ver\u00b7lacht", ",", "mit", "mun\u00b7tern", "Sin\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "K\u00e4lte, Reif und Schnee und Frost;", "tokens": ["K\u00e4l\u00b7te", ",", "Reif", "und", "Schnee", "und", "Frost", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Will der Liebe G\u00f6tterkost", "tokens": ["Will", "der", "Lie\u00b7be", "G\u00f6t\u00b7ter\u00b7kost"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Auch im Winter lieb gewinnen.", "tokens": ["Auch", "im", "Win\u00b7ter", "lieb", "ge\u00b7win\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Kann auch kalte Herzen schmelzen;", "tokens": ["Kann", "auch", "kal\u00b7te", "Her\u00b7zen", "schmel\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn gleich Sonn und W\u00e4rme sich", "tokens": ["Wenn", "gleich", "Sonn", "und", "W\u00e4r\u00b7me", "sich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "NN", "KON", "NN", "PRF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Um den fernen S\u00fcdpol w\u00e4lzen;", "tokens": ["Um", "den", "fer\u00b7nen", "S\u00fcd\u00b7pol", "w\u00e4l\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn gleich Lunens Silberstralen,", "tokens": ["Wenn", "gleich", "Lu\u00b7nens", "Sil\u00b7ber\u00b7stra\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "NE", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Bey gestirnter Himmelspracht,", "tokens": ["Bey", "ge\u00b7stirn\u00b7ter", "Him\u00b7mel\u00b7spracht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Unsers Nordens l\u00e4ngste Nacht", "tokens": ["Un\u00b7sers", "Nor\u00b7dens", "l\u00e4ngs\u00b7te", "Nacht"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "NE", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Mit dem k\u00e4ltsten Glanze malen.", "tokens": ["Mit", "dem", "k\u00e4lts\u00b7ten", "Glan\u00b7ze", "ma\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Liebste Schwester, werthe Braut,", "tokens": ["Liebs\u00b7te", "Schwes\u00b7ter", ",", "wert\u00b7he", "Braut", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Dich hat ", "tokens": ["Dich", "hat"], "token_info": ["word", "word"], "pos": ["PPER", "VAFIN"], "meter": "--", "measure": "unknown.measure.zero"}, "line.3": {"text": "Ist auch dir ins Herz gedrungen.", "tokens": ["Ist", "auch", "dir", "ins", "Herz", "ge\u00b7drun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPER", "APPRART", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Deines Liebsten Ruhm und Gaben", "tokens": ["Dei\u00b7nes", "Liebs\u00b7ten", "Ruhm", "und", "Ga\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Haben dich so stark entbrannt,", "tokens": ["Ha\u00b7ben", "dich", "so", "stark", "ent\u00b7brannt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Da\u00df sie deinen Jungferstand", "tokens": ["Da\u00df", "sie", "dei\u00b7nen", "Jung\u00b7fers\u00b7tand"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Auch zuletzt geschmolzen haben.", "tokens": ["Auch", "zu\u00b7letzt", "ge\u00b7schmol\u00b7zen", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVPP", "VAINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Herbst und Sommer waren nicht", "tokens": ["Herbst", "und", "Som\u00b7mer", "wa\u00b7ren", "nicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "VAFIN", "PTKNEG"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "T\u00fcchtig, dich zu \u00fcberwinden;", "tokens": ["T\u00fcch\u00b7tig", ",", "dich", "zu", "\u00fc\u00b7berw\u00b7in\u00b7den", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PPER", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Auch kein warmes Fr\u00fchlingslicht", "tokens": ["Auch", "kein", "war\u00b7mes", "Fr\u00fch\u00b7lings\u00b7licht"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "PIAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Konnte deine Brust entz\u00fcnden.", "tokens": ["Konn\u00b7te", "dei\u00b7ne", "Brust", "ent\u00b7z\u00fcn\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Was nun keinem noch gelungen,", "tokens": ["Was", "nun", "kei\u00b7nem", "noch", "ge\u00b7lun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "PIS", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Kann dem Winter m\u00f6glich seyn;", "tokens": ["Kann", "dem", "Win\u00b7ter", "m\u00f6g\u00b7lich", "seyn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "ADJD", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Da dein Liebster nur allein", "tokens": ["Da", "dein", "Liebs\u00b7ter", "nur", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Deine keusche Brust bezwungen.", "tokens": ["Dei\u00b7ne", "keu\u00b7sche", "Brust", "be\u00b7zwun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Lachet dann bey eurer Glut,", "tokens": ["La\u00b7chet", "dann", "bey", "eu\u00b7rer", "Glut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn der Frost die Erde r\u00fchret;", "tokens": ["Wenn", "der", "Frost", "die", "Er\u00b7de", "r\u00fch\u00b7ret", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Zeigt, da\u00df euer hei\u00dfes Blut", "tokens": ["Zeigt", ",", "da\u00df", "eu\u00b7er", "hei\u00b7\u00dfes", "Blut"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "KOUS", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "St\u00fcndlich neuen Zunder sp\u00fcret.", "tokens": ["St\u00fcnd\u00b7lich", "neu\u00b7en", "Zun\u00b7der", "sp\u00fc\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wenn die Flocken alles decken,", "tokens": ["Wenn", "die", "Flo\u00b7cken", "al\u00b7les", "de\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PIS", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Seht es voller Flammen zu;", "tokens": ["Seht", "es", "vol\u00b7ler", "Flam\u00b7men", "zu", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und la\u00dft eure s\u00fc\u00dfe Ruh", "tokens": ["Und", "la\u00dft", "eu\u00b7re", "s\u00fc\u00b7\u00dfe", "Ruh"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVIMP", "PPOSAT", "ADJA", "NN"], "meter": "----+-+", "measure": "unknown.measure.di"}, "line.8": {"text": "Durch kein kaltes L\u00fcftchen schrecken.", "tokens": ["Durch", "kein", "kal\u00b7tes", "L\u00fcft\u00b7chen", "schre\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Eilt zu Bette, werthes Paar!", "tokens": ["Eilt", "zu", "Bet\u00b7te", ",", "wert\u00b7hes", "Paar", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "La\u00dft euch in der Lust nicht st\u00f6ren.", "tokens": ["La\u00dft", "euch", "in", "der", "Lust", "nicht", "st\u00f6\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "ART", "NN", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Eh noch die\u00df verj\u00fcngte Jahr", "tokens": ["Eh", "noch", "die\u00df", "ver\u00b7j\u00fcng\u00b7te", "Jahr"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "PDS", "VVFIN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Den verl\u00e4ngten Tag wird mehren;", "tokens": ["Den", "ver\u00b7l\u00e4ng\u00b7ten", "Tag", "wird", "meh\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Eh euch noch die fr\u00fchen Schatten,", "tokens": ["Eh", "euch", "noch", "die", "fr\u00fc\u00b7hen", "Schat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Zeitiger den Flor entziehn;", "tokens": ["Zei\u00b7ti\u00b7ger", "den", "Flor", "ent\u00b7ziehn", ";"], "token_info": ["word", "word", "wor