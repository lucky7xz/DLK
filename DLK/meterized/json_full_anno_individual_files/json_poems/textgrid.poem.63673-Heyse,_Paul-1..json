{"textgrid.poem.63673": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Stets liebt' ich's, wenn gepackt mein Koffer war,", "tokens": ["Stets", "liebt'", "ich's", ",", "wenn", "ge\u00b7packt", "mein", "Kof\u00b7fer", "war", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "$,", "KOUS", "VVPP", "PPOSAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Reise noch ein wenig aufzuschieben,", "tokens": ["Die", "Rei\u00b7se", "noch", "ein", "we\u00b7nig", "auf\u00b7zu\u00b7schie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ART", "PIS", "VVIZU", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Aus dem gewohnten Bett noch nicht vertrieben,", "tokens": ["Aus", "dem", "ge\u00b7wohn\u00b7ten", "Bett", "noch", "nicht", "ver\u00b7trie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ADV", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Doch der gewohnten Tagespflichten bar.", "tokens": ["Doch", "der", "ge\u00b7wohn\u00b7ten", "Ta\u00b7ges\u00b7pflich\u00b7ten", "bar", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ein Zwischenaktsgef\u00fchl, h\u00f6chst sonderbar,", "tokens": ["Ein", "Zwi\u00b7schen\u00b7akts\u00b7ge\u00b7f\u00fchl", ",", "h\u00f6chst", "son\u00b7der\u00b7bar", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wie wenn im Wartsaal man zur\u00fcckgeblieben,", "tokens": ["Wie", "wenn", "im", "Wart\u00b7saal", "man", "zu\u00b7r\u00fcck\u00b7ge\u00b7blie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOKOM", "KOUS", "APPRART", "NN", "PIS", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nicht v\u00f6llig h\u00fcben mehr und noch nicht dr\u00fcben,", "tokens": ["Nicht", "v\u00f6l\u00b7lig", "h\u00fc\u00b7ben", "mehr", "und", "noch", "nicht", "dr\u00fc\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "ADV", "ADV", "KON", "ADV", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und l\u00f6ste noch kein Fahrbillett sogar.", "tokens": ["Und", "l\u00f6s\u00b7te", "noch", "kein", "Fahr\u00b7bil\u00b7lett", "so\u00b7gar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PIAT", "NN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "So f\u00fchl' ich mich auch jetzt: noch nicht so eilig", "tokens": ["So", "f\u00fchl'", "ich", "mich", "auch", "jetzt", ":", "noch", "nicht", "so", "ei\u00b7lig"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ADV", "ADV", "$.", "ADV", "PTKNEG", "ADV", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gedr\u00e4ngt zur Fahrt nach jenem Land, woher", "tokens": ["Ge\u00b7dr\u00e4ngt", "zur", "Fahrt", "nach", "je\u00b7nem", "Land", ",", "wo\u00b7her"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["VVPP", "APPRART", "NN", "APPR", "PDAT", "NN", "$,", "PWAV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Kein Wandrer wiederkehrt, und gern verweil' ich", "tokens": ["Kein", "Wand\u00b7rer", "wie\u00b7der\u00b7kehrt", ",", "und", "gern", "ver\u00b7weil'", "ich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VVFIN", "$,", "KON", "ADV", "VVFIN", "PPER"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Im Wartsaal, gute Freunde ringsumher.", "tokens": ["Im", "Wart\u00b7saal", ",", "gu\u00b7te", "Freun\u00b7de", "rings\u00b7um\u00b7her", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mein Koffer steht gepackt \u2013 nur da\u00df er freilich", "tokens": ["Mein", "Kof\u00b7fer", "steht", "ge\u00b7packt", "\u2013", "nur", "da\u00df", "er", "frei\u00b7lich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "VVPP", "$(", "ADV", "KOUS", "PPER", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Zur\u00fcckbleibt, f\u00fcr die letzte Fahrt zu schwer!", "tokens": ["Zu\u00b7r\u00fcck\u00b7bleibt", ",", "f\u00fcr", "die", "letz\u00b7te", "Fahrt", "zu", "schwer", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "APPR", "ART", "ADJA", "NN", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Stets liebt' ich's, wenn gepackt mein Koffer war,", "tokens": ["Stets", "liebt'", "ich's", ",", "wenn", "ge\u00b7packt", "mein", "Kof\u00b7fer", "war", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "$,", "KOUS", "VVPP", "PPOSAT", "NN