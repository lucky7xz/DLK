{"textgrid.poem.47262": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Abschied", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr Berg' und o ihr Th\u00e4ler, hoch und tief,", "tokens": ["Ihr", "Ber\u00b7g'", "und", "o", "ihr", "Th\u00e4\u00b7ler", ",", "hoch", "und", "tief", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "FM", "PPOSAT", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "--+-+-+-+-+", "measure": "anapaest.init"}, "line.2": {"text": "Die ihr mir lange war't ein fremdes Land,", "tokens": ["Die", "ihr", "mir", "lan\u00b7ge", "wa\u00b7r't", "ein", "frem\u00b7des", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPER", "ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+--+----+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "In welchem nie mein Herz die Heimat fand,", "tokens": ["In", "wel\u00b7chem", "nie", "mein", "Herz", "die", "Hei\u00b7mat", "fand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "ADV", "PPOSAT", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die stets der Sehnsucht aus der Ferne rief!", "tokens": ["Die", "stets", "der", "Sehn\u00b7sucht", "aus", "der", "Fer\u00b7ne", "rief", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Da endlich nun die Sehnsucht hier entschlief,", "tokens": ["Da", "end\u00b7lich", "nun", "die", "Sehn\u00b7sucht", "hier", "ent\u00b7schlief", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Da mir die neue Heimat hier entstand,", "tokens": ["Da", "mir", "die", "neu\u00b7e", "Hei\u00b7mat", "hier", "ent\u00b7stand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Wo mich umketten wollte trautes Band;", "tokens": ["Wo", "mich", "um\u00b7ket\u00b7ten", "woll\u00b7te", "trau\u00b7tes", "Band", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVINF", "VMFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Euch reichen mu\u00df ich nun den Scheidebrief.", "tokens": ["Euch", "rei\u00b7chen", "mu\u00df", "ich", "nun", "den", "Schei\u00b7de\u00b7brief", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "VMFIN", "PPER", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Ihr Berg' und o ihr Th\u00e4ler, habt Ade!", "tokens": ["Ihr", "Ber\u00b7g'", "und", "o", "ihr", "Th\u00e4\u00b7ler", ",", "habt", "A\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "FM", "PPOSAT", "NN", "$,", "VAFIN", "NN", "$."], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Was ich hier fand, wo find' ich's wieder je?", "tokens": ["Was", "ich", "hier", "fand", ",", "wo", "find'", "ich's", "wie\u00b7der", "je", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "VVFIN", "$,", "PWAV", "VVFIN", "PIS", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Lebt wohl! ich sag' euch Lebewohl mit Weh.", "tokens": ["Lebt", "wohl", "!", "ich", "sag'", "euch", "Le\u00b7be\u00b7wohl", "mit", "Weh", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$.", "PPER", "VVFIN", "PPER", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Du sanftumb\u00fcschter Garten, s\u00fc\u00dfer Ort,", "tokens": ["Du", "sanf\u00b7tum\u00b7b\u00fcschter", "Gar\u00b7ten", ",", "s\u00fc\u00b7\u00dfer", "Ort", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Du hast ein Jahrlang mit vergeb'nen M\u00fch'n", "tokens": ["Du", "hast", "ein", "Jahr\u00b7lang", "mit", "ver\u00b7ge\u00b7b'\u00b7nen", "M\u00fch'n"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Dem aufgenommnen Fremdling wollen bl\u00fchn", "tokens": ["Dem", "auf\u00b7ge\u00b7nomm\u00b7nen", "Fremd\u00b7ling", "wol\u00b7len", "bl\u00fchn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VMFIN", "VVINF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und bliebst dem kranken Sinne stets verdorrt!", "tokens": ["Und", "bliebst", "dem", "kran\u00b7ken", "Sin\u00b7ne", "stets", "ver\u00b7dorrt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und nun, da pl\u00f6tzlich durch ein Zauberwort", "tokens": ["Und", "nun", ",", "da", "pl\u00f6tz\u00b7lich", "durch", "ein", "Zau\u00b7ber\u00b7wort"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "$,", "ADV", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "An allen Pl\u00e4tzen du mir wurdest gr\u00fcn,", "tokens": ["An", "al\u00b7len", "Pl\u00e4t\u00b7zen", "du", "mir", "wur\u00b7dest", "gr\u00fcn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "PPER", "PPER", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Mir alle deine Rosen Funken spr\u00fchn, \u2013", "tokens": ["Mir", "al\u00b7le", "dei\u00b7ne", "Ro\u00b7sen", "Fun\u00b7ken", "spr\u00fchn", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "PIAT", "PPOSAT", "NN", "NN", "VVFIN", "$,", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Das Schicksal ruft, und ich mu\u00df von dir fort.", "tokens": ["Das", "Schick\u00b7sal", "ruft", ",", "und", "ich", "mu\u00df", "von", "dir", "fort", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "KON", "PPER", "VMFIN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Wie nun zur Hand den Wanderstab ich nahm,", "tokens": ["Wie", "nun", "zur", "Hand", "den", "Wan\u00b7der\u00b7stab", "ich", "nahm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "APPRART", "NN", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Ist er so d\u00fcrr wie damals, da ich kam;", "tokens": ["Ist", "er", "so", "d\u00fcrr", "wie", "da\u00b7mals", ",", "da", "ich", "kam", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "KOKOM", "ADV", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Leb' wohl! dir sag' ich Lebewohl mit Gram.", "tokens": ["Leb'", "wohl", "!", "dir", "sag'", "ich", "Le\u00b7be\u00b7wohl", "mit", "Gram", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "$.", "PPER", "VVFIN", "PPER", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "O stiller See, bewegt vom Ruderklang", "tokens": ["O", "stil\u00b7ler", "See", ",", "be\u00b7wegt", "vom", "Ru\u00b7der\u00b7klang"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "ADJA", "NN", "$,", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Des Schwanes, der die feuchten Kreise zieht", "tokens": ["Des", "Schwa\u00b7nes", ",", "der", "die", "feuch\u00b7ten", "Krei\u00b7se", "zieht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Um seine Insel, wo in Schilf und Ried", "tokens": ["Um", "sei\u00b7ne", "In\u00b7sel", ",", "wo", "in", "Schilf", "und", "Ried"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUI", "PPOSAT", "NN", "$,", "PWAV", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Das Weib ihm sitzt und br\u00fctet! \u2013 o wie lang'", "tokens": ["Das", "Weib", "ihm", "sitzt", "und", "br\u00fc\u00b7tet", "!", "\u2013", "o", "wie", "lang'"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["ART", "NN", "PPER", "VVFIN", "KON", "VVFIN", "$.", "$(", "FM", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Belauscht', o Schwan, ich deinen Lebensgang,", "tokens": ["Be\u00b7lauscht'", ",", "o", "Schwan", ",", "ich", "dei\u00b7nen", "Le\u00b7bens\u00b7gang", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "FM", "NN", "$,", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und meine Thr\u00e4ne mehrte dein Gebiet.", "tokens": ["Und", "mei\u00b7ne", "Thr\u00e4\u00b7ne", "mehr\u00b7te", "dein", "Ge\u00b7biet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Ich sah im See ein Schattenbild, es flieht,", "tokens": ["Ich", "sah", "im", "See", "ein", "Schat\u00b7ten\u00b7bild", ",", "es", "flieht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ART", "NN", "$,", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Du schweigst und h\u00f6rest meinen Schwanensang.", "tokens": ["Du", "schweigst", "und", "h\u00f6\u00b7rest", "mei\u00b7nen", "Schwa\u00b7nen\u00b7sang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "O Wellen, die ihr meine Thr\u00e4nen seid,", "tokens": ["O", "Wel\u00b7len", ",", "die", "ihr", "mei\u00b7ne", "Thr\u00e4\u00b7nen", "seid", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PRELS", "PPER", "PPOSAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Einst Spiegel meines Gl\u00fccks und nun mein Neid,", "tokens": ["Einst", "Spie\u00b7gel", "mei\u00b7nes", "Gl\u00fccks", "und", "nun", "mein", "Neid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "PPOSAT", "NN", "KON", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Lebt wohl! ich sag' euch Lebewohl mit Leid.", "tokens": ["Lebt", "wohl", "!", "ich", "sag'", "euch", "Le\u00b7be\u00b7wohl", "mit", "Leid", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$.", "PPER", "VVFIN", "PPER", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "O Stadt mit allen H\u00e4usern Dach an Dach,", "tokens": ["O", "Stadt", "mit", "al\u00b7len", "H\u00e4u\u00b7sern", "Dach", "an", "Dach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "APPR", "PIAT", "NN", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die ihr als Gast mich aufgenommen habt,", "tokens": ["Die", "ihr", "als", "Gast", "mich", "auf\u00b7ge\u00b7nom\u00b7men", "habt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "KOUS", "NN", "PPER", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die ihr zuerst mir nur ein Obdach gabt", "tokens": ["Die", "ihr", "zu\u00b7erst", "mir", "nur", "ein", "Ob\u00b7dach", "gabt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ADV", "PPER", "ADV", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und keine Freud' im einsamen Gemach.", "tokens": ["Und", "kei\u00b7ne", "Freud'", "im", "ein\u00b7sa\u00b7men", "Ge\u00b7mach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Da nun aus euerem Gestein ein Bach", "tokens": ["Da", "nun", "aus", "eu\u00b7e\u00b7rem", "Ge\u00b7stein", "ein", "Bach"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "PPOSAT", "NN", "ART", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Des Lebens ist entsprungen, der mich labt, \u2013", "tokens": ["Des", "Le\u00b7bens", "ist", "ent\u00b7sprun\u00b7gen", ",", "der", "mich", "labt", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,", "PRELS", "PPER", "VVFIN", "$,", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Mu\u00df ich mich von euch wenden \u2013 o begrabt", "tokens": ["Mu\u00df", "ich", "mich", "von", "euch", "wen\u00b7den", "\u2013", "o", "be\u00b7grabt"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VMFIN", "PPER", "PRF", "APPR", "PPER", "VVINF", "$(", "FM", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "In eurer Giebel Rauch mein letztes Ach!", "tokens": ["In", "eu\u00b7rer", "Gie\u00b7bel", "Rauch", "mein", "letz\u00b7tes", "Ach", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Wie ich aus euch mich sehnte heimatw\u00e4rts,", "tokens": ["Wie", "ich", "aus", "euch", "mich", "sehn\u00b7te", "hei\u00b7mat\u00b7w\u00e4rts", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "PPER", "PPER", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "So wird nach euch sich sehnen nun mein Herz;", "tokens": ["So", "wird", "nach", "euch", "sich", "seh\u00b7nen", "nun", "mein", "Herz", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "PPER", "PRF", "ADV", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Lebt wohl! ihr seht mich von euch gehn mit Schmerz.", "tokens": ["Lebt", "wohl", "!", "ihr", "seht", "mich", "von", "euch", "gehn", "mit", "Schmerz", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$.", "PPER", "VVFIN", "PRF", "APPR", "PPER", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "O Berg und Thal, o Garten, See und Stadt,", "tokens": ["O", "Berg", "und", "Thal", ",", "o", "Gar\u00b7ten", ",", "See", "und", "Stadt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "KON", "NN", "$,", "FM", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ein Himmel mir, so weit als sich der Saum", "tokens": ["Ein", "Him\u00b7mel", "mir", ",", "so", "weit", "als", "sich", "der", "Saum"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PPER", "$,", "ADV", "ADJD", "KOKOM", "PRF", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Des Himmels dehnt! mir erst ein \u00f6der Raum,", "tokens": ["Des", "Him\u00b7mels", "dehnt", "!", "mir", "erst", "ein", "\u00f6\u00b7de