{"dta.poem.18118": {"metadata": {"author": {"name": "Bodmer, Johann Jacob", "birth": "N.A.", "death": "N.A."}, "title": "An den Abendstern.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-200905198432", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ach jetzund wollt ich gleich zu meiner Buhlschaft gehen,", "tokens": ["Ach", "je\u00b7tzund", "wollt", "ich", "gleich", "zu", "mei\u00b7ner", "Buhl\u00b7schaft", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "VMFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nun weicht die Sonne weg, und du wilt auch entstehen,", "tokens": ["Nun", "weicht", "die", "Son\u00b7ne", "weg", ",", "und", "du", "wilt", "auch", "ent\u00b7ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PTKVZ", "$,", "KON", "PPER", "VMFIN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Du sch\u00f6ner Abendstern, die sp\u00e4the Nacht bricht an,", "tokens": ["Du", "sch\u00f6\u00b7ner", "A\u00b7bends\u00b7tern", ",", "die", "sp\u00e4\u00b7the", "Nacht", "bricht", "an", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "So da\u00df ich heute nicht zu ihr gelangen kan.", "tokens": ["So", "da\u00df", "ich", "heu\u00b7te", "nicht", "zu", "ihr", "ge\u00b7lan\u00b7gen", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "ADV", "PTKNEG", "APPR", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}