{"dta.poem.5396": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Abschied vom Garten.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mein GOtt! du hast auf dieser Welt", "tokens": ["Mein", "Gott", "!", "du", "hast", "auf", "die\u00b7ser", "Welt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$.", "PPER", "VAFIN", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mir so viel herrliches geschencket,", "tokens": ["Mir", "so", "viel", "herr\u00b7li\u00b7ches", "ge\u00b7schen\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PIAT", "ADJA", "VVPP", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Da\u00df, wenn mein Geist es \u00fcberdencket,", "tokens": ["Da\u00df", ",", "wenn", "mein", "Geist", "es", "\u00fc\u00b7ber\u00b7den\u00b7cket", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "KOUS", "PPOSAT", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Es aller Gaben sich so gar nicht w\u00fcrdig h\u00e4lt.", "tokens": ["Es", "al\u00b7ler", "Ga\u00b7ben", "sich", "so", "gar", "nicht", "w\u00fcr\u00b7dig", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "NN", "PRF", "ADV", "ADV", "PTKNEG", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Es lallet mein ger\u00fchrter Sinn", "tokens": ["Es", "lal\u00b7let", "mein", "ge\u00b7r\u00fchr\u00b7ter", "Sinn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Voll Danck und Andacht: HErr! ich bin", "tokens": ["Voll", "Danck", "und", "An\u00b7dacht", ":", "Herr", "!", "ich", "bin"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["ADJD", "NN", "KON", "NN", "$.", "NN", "$.", "PPER", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Nicht w\u00fcrdig der Barmhertzigkeit,", "tokens": ["Nicht", "w\u00fcr\u00b7dig", "der", "Barm\u00b7hert\u00b7zig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Nicht w\u00fcrdig aller Treu und G\u00fcte,", "tokens": ["Nicht", "w\u00fcr\u00b7dig", "al\u00b7ler", "Treu", "und", "G\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "PIAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Die du an mir erzeigt die gantze Lebens-Zeit!", "tokens": ["Die", "du", "an", "mir", "er\u00b7zeigt", "die", "gant\u00b7ze", "Le\u00b7bens\u00b7Zeit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "So sprach ich j\u00fcngst, mit fr\u00f6lichem gem\u00fcthe,", "tokens": ["So", "sprach", "ich", "j\u00fcngst", ",", "mit", "fr\u00f6\u00b7li\u00b7chem", "ge\u00b7m\u00fc\u00b7the", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Als ich in meinem Garten ging,", "tokens": ["Als", "ich", "in", "mei\u00b7nem", "Gar\u00b7ten", "ging", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Und dessen Schmuck und Lag\u2019 an zu betrachten fing.", "tokens": ["Und", "des\u00b7sen", "Schmuck", "und", "Lag'", "an", "zu", "be\u00b7trach\u00b7ten", "fing", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRELAT", "NN", "KON", "NN", "PTKVZ", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Da\u00df alles hier so lieblich gr\u00fcnet,", "tokens": ["Da\u00df", "al\u00b7les", "hier", "so", "lieb\u00b7lich", "gr\u00fc\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Da\u00df alles uns zur Anmuth dienet,", "tokens": ["Da\u00df", "al\u00b7les", "uns", "zur", "An\u00b7muth", "die\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Davor mu\u00df ich, HErr! dir allein", "tokens": ["Da\u00b7vor", "mu\u00df", "ich", ",", "Herr", "!", "dir", "al\u00b7lein"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PAV", "VMFIN", "PPER", "$,", "NN", "$.", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "In froher Demuth danckbar seyn.", "tokens": ["In", "fro\u00b7her", "De\u00b7muth", "dan\u00b7ck\u00b7bar", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "VAINF", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.17": {"text": "Da\u00df du mir alles wollen g\u00f6nnen,", "tokens": ["Da\u00df", "du", "mir", "al\u00b7les", "wol\u00b7len", "g\u00f6n\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "PIS", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Zumahlen des Verstandes Kraft,", "tokens": ["Zu\u00b7mah\u00b7len", "des", "Ver\u00b7stan\u00b7des", "Kraft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Da\u00df ich es zierlich ordnen k\u00f6nnen,", "tokens": ["Da\u00df", "ich", "es", "zier\u00b7lich", "ord\u00b7nen", "k\u00f6n\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADJD", "VVINF", "VMINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Und so viel Witz und Wissenschaft,", "tokens": ["Und", "so", "viel", "Witz", "und", "Wis\u00b7sen\u00b7schaft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PIAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Es so gef\u00e4llig einzurichten,", "tokens": ["Es", "so", "ge\u00b7f\u00e4l\u00b7lig", "ein\u00b7zu\u00b7rich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJD", "VVIZU", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Davor erfordern meine Pflichten,", "tokens": ["Da\u00b7vor", "er\u00b7for\u00b7dern", "mei\u00b7ne", "Pflich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "In froher Ehrfurcht, dir allein", "tokens": ["In", "fro\u00b7her", "Ehr\u00b7furcht", ",", "dir", "al\u00b7lein"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Zu Ehren, froh und fromm zu seyn.", "tokens": ["Zu", "Eh\u00b7ren", ",", "froh", "und", "fromm", "zu", "seyn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "ADJD", "KON", "ADJD", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Herr, von aller dieser Sch\u00f6nheit, von der Farben", "tokens": ["Herr", ",", "von", "al\u00b7ler", "die\u00b7ser", "Sch\u00f6n\u00b7heit", ",", "von", "der", "Far\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "APPR", "PIAT", "PDAT", "NN", "$,", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Von dem sch\u00f6nen Licht und Schatten,", "tokens": ["Von", "dem", "sch\u00f6\u00b7nen", "Licht", "und", "Schat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Von der Bl\u00e4tter-eichen G\u00e4nge L\u00e4nge, Meng\u2019", "tokens": ["Von", "der", "Bl\u00e4t\u00b7ter\u00b7ei\u00b7chen", "G\u00e4n\u00b7ge", "L\u00e4n\u00b7ge", ",", "Meng'"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$,", "NE"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "Die, in fr\u00f6lichem Verband, alle hier sich lieblich gatten,", "tokens": ["Die", ",", "in", "fr\u00f6\u00b7li\u00b7chem", "Ver\u00b7band", ",", "al\u00b7le", "hier", "sich", "lieb\u00b7lich", "gat\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "APPR", "ADJA", "NN", "$,", "PIS", "ADV", "PRF", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}}, "stanza.3": {"line.1": {"text": "Ja wodurch, in Pracht und Ordnung, alles sich", "tokens": ["Ja", "wo\u00b7durch", ",", "in", "Pracht", "und", "Ord\u00b7nung", ",", "al\u00b7les", "sich"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PTKANT", "PWAV", "$,", "APPR", "NN", "KON", "NN", "$,", "PIS", "PRF"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "So, da\u00df nicht leicht sonder Anmuth es ein frembdes", "tokens": ["So", ",", "da\u00df", "nicht", "leicht", "son\u00b7der", "An\u00b7muth", "es", "ein", "fremb\u00b7des"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "KOUS", "PTKNEG", "ADJD", "KON", "NN", "PPER", "ART", "ADJA"], "meter": "----+-+-+-+-", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Bin ich billig gantz erstaunt: sonderlich wenn ich", "tokens": ["Bin", "ich", "bil\u00b7lig", "gantz", "er\u00b7staunt", ":", "son\u00b7der\u00b7lich", "wenn", "ich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADJD", "ADV", "ADJD", "$.", "ADV", "KOUS", "PPER"], "meter": "+-+-+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Und, woher es eigentlich seinen Ursprung hat? be-", "tokens": ["Und", ",", "wo\u00b7her", "es", "ei\u00b7gent\u00b7lich", "sei\u00b7nen", "Ur\u00b7sprung", "hat", "?", "be"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "$,", "PWAV", "PPER", "ADV", "PPOSAT", "NN", "VAFIN", "$.", "TRUNC"], "meter": "-+---+-+-+-+-", "measure": "dactylic.init"}}, "stanza.4": {"line.1": {"text": "Du selber hast die\u00df sch\u00f6ne St\u00fcck der Welt,", "tokens": ["Du", "sel\u00b7ber", "hast", "die\u00df", "sch\u00f6\u00b7ne", "St\u00fcck", "der", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "PDS", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Das allen, die es sehn, gef\u00e4llt,", "tokens": ["Das", "al\u00b7len", ",", "die", "es", "sehn", ",", "ge\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PDS", "PIS", "$,", "PRELS", "PPER", "VVINF", "$,", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Durch meine Hand, o GOtt, gezieret.", "tokens": ["Durch", "mei\u00b7ne", "Hand", ",", "o", "Gott", ",", "ge\u00b7zie\u00b7ret", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "FM", "NN", "$,", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Weswegen auch nur dir allein,", "tokens": ["Wes\u00b7we\u00b7gen", "auch", "nur", "dir", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADV", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Natur so wol, als Kunst und Wissenschaften dein,", "tokens": ["Na\u00b7tur", "so", "wol", ",", "als", "Kunst", "und", "Wis\u00b7sen\u00b7schaf\u00b7ten", "dein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADV", "$,", "KOUS", "NN", "KON", "NN", "PPOSAT", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Als die uns blos von dir geschencket seyn)", "tokens": ["Als", "die", "uns", "blos", "von", "dir", "ge\u00b7schen\u00b7cket", "seyn", ")"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "PPER", "ADV", "APPR", "PPER", "VVPP", "VAINF", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Lob, Ehre, Preis und Danck geb\u00fchret.", "tokens": ["Lob", ",", "Eh\u00b7re", ",", "Preis", "und", "Danck", "ge\u00b7b\u00fch\u00b7ret", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Mu\u00df ich nun gleich den sch\u00f6nen Ort,", "tokens": ["Mu\u00df", "ich", "nun", "gleich", "den", "sch\u00f6\u00b7nen", "Ort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Nach deinen F\u00fchrungen, hinfort,", "tokens": ["Nach", "dei\u00b7nen", "F\u00fch\u00b7run\u00b7gen", ",", "hin\u00b7fort", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "ADV", "$,"], "meter": "-+-+---+", "measure": "unknown.measure.tri"}, "line.10": {"text": "Und zwar auf lange Zeit, verlassen;", "tokens": ["Und", "zwar", "auf", "lan\u00b7ge", "Zeit", ",", "ver\u00b7las\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ADJA", "NN", "$,", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "So such\u2019 ich mich mit diesem Trost hiebey,", "tokens": ["So", "such'", "ich", "mich", "mit", "die\u00b7sem", "Trost", "hie\u00b7bey", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "PDAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Da\u00df es, wills GOtt, doch nicht vor immer sey;", "tokens": ["Da\u00df", "es", ",", "wills", "Gott", ",", "doch", "nicht", "vor", "im\u00b7mer", "sey", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "KOUS", "NN", "$,", "ADV", "PTKNEG", "APPR", "ADV", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "In dem Verlust zu fassen.", "tokens": ["In", "dem", "Ver\u00b7lust", "zu", "fas\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.14": {"text": "Wie leicht l\u00e4\u00dft es der Sch\u00f6pfer doch geschehn,", "tokens": ["Wie", "leicht", "l\u00e4\u00dft", "es", "der", "Sch\u00f6p\u00b7fer", "doch", "ge\u00b7schehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "PPER", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Da\u00df ich ihn fr\u00f6lich wieder sehn,", "tokens": ["Da\u00df", "ich", "ihn", "fr\u00f6\u00b7lich", "wie\u00b7der", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADJD", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Und sein aufs neu geniessen kann.", "tokens": ["Und", "sein", "aufs", "neu", "ge\u00b7nies\u00b7sen", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "APPRART", "ADJD", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Ich fleh ihn auch, wenn es sein Gnaden-Wille,", "tokens": ["Ich", "fleh", "ihn", "auch", ",", "wenn", "es", "sein", "Gna\u00b7den\u00b7Wil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$,", "KOUS", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Darum hiemit, in Demuth, an.", "tokens": ["Da\u00b7rum", "hie\u00b7mit", ",", "in", "De\u00b7muth", ",", "an", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PAV", "PAV", "$,", "APPR", "NN", "$,", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Will GOtt es aber nicht; wohlan,", "tokens": ["Will", "Gott", "es", "a\u00b7ber", "nicht", ";", "wo\u00b7hlan", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VMFIN", "NN", "PPER", "ADV", "PTKNEG", "$.", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "So halt ich ihm, nach meinen Pflichten stille,", "tokens": ["So", "halt", "ich", "ihm", ",", "nach", "mei\u00b7nen", "Pflich\u00b7ten", "stil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "$,", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Da GOttes Wahl auch billig meine Wahl,", "tokens": ["Da", "Got\u00b7tes", "Wahl", "auch", "bil\u00b7lig", "mei\u00b7ne", "Wahl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "NN", "ADV", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Und seh\u2019 des Gartens Pracht, mit seiner Anmuth F\u00fclle,", "tokens": ["Und", "seh'", "des", "Gar\u00b7tens", "Pracht", ",", "mit", "sei\u00b7ner", "An\u00b7muth", "F\u00fcl\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVF