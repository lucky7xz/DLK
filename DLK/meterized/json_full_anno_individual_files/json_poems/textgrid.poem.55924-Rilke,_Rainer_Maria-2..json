{"textgrid.poem.55924": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sehr viele Weite ist gemeint damit:", "tokens": ["Sehr", "vie\u00b7le", "Wei\u00b7te", "ist", "ge\u00b7meint", "da\u00b7mit", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "VAFIN", "VVPP", "PAV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "so wie mit den Kulissen einer Szene", "tokens": ["so", "wie", "mit", "den", "Ku\u00b7lis\u00b7sen", "ei\u00b7ner", "Sze\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+---+-+-+-", "measure": "dactylic.init"}, "line.3": {"text": "die Welt gemeint ist; und so wie durch jene", "tokens": ["die", "Welt", "ge\u00b7meint", "ist", ";", "und", "so", "wie", "durch", "je\u00b7ne"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVPP", "VAFIN", "$.", "KON", "ADV", "KOKOM", "APPR", "PDAT"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "der Held im Mantel seiner Handlung tritt: \u2013", "tokens": ["der", "Held", "im", "Man\u00b7tel", "sei\u00b7ner", "Hand\u00b7lung", "tritt", ":", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "PPOSAT", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "so tritt das Dunkel dieses Tores handelnd", "tokens": ["so", "tritt", "das", "Dun\u00b7kel", "die\u00b7ses", "To\u00b7res", "han\u00b7delnd"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos"