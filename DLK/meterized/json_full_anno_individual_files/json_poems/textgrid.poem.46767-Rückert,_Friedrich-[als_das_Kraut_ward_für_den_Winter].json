os": ["NN", "KON", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.29": {"text": "Werden sie das Kraut verw\u00fcrzen;", "tokens": ["Wer\u00b7den", "sie", "das", "Kraut", "ver\u00b7w\u00fcr\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.30": {"text": "Ihnen doch die Lust zu wehren,", "tokens": ["Ih\u00b7nen", "doch", "die", "Lust", "zu", "weh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.31": {"text": "Konnt' ich \u00fcbers Herz nicht bringen,", "tokens": ["Konnt'", "ich", "\u00fc\u00b7bers", "Herz", "nicht", "brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPRART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.32": {"text": "Weil sie gar zu zierlich streuten.", "tokens": ["Weil", "sie", "gar", "zu", "zier\u00b7lich", "streu\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PTKA", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.33": {"text": "Und nun ist es eingetroffen,", "tokens": ["Und", "nun", "ist", "es", "ein\u00b7ge\u00b7trof\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.34": {"text": "Schlimmer, als es war zu hoffen,", "tokens": ["Schlim\u00b7mer", ",", "als", "es", "war", "zu", "hof\u00b7fen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "VAFIN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.35": {"text": "K\u00fcmmel und Wachholderbeerchen,", "tokens": ["K\u00fcm\u00b7mel", "und", "Wach\u00b7hol\u00b7der\u00b7beer\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.36": {"text": "Ihr, gestreut von meinem P\u00e4rchen,", "tokens": ["Ihr", ",", "ge\u00b7streut", "von", "mei\u00b7nem", "P\u00e4r\u00b7chen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "VVPP", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.37": {"text": "Habet uns das Kraut verw\u00fcrzet,", "tokens": ["Ha\u00b7bet", "uns", "das", "Kraut", "ver\u00b7w\u00fcr\u00b7zet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.38": {"text": "Es verk\u00fcmmert und verbittert,", "tokens": ["Es", "ver\u00b7k\u00fcm\u00b7mert", "und", "ver\u00b7bit\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.39": {"text": "Da\u00df es ungenie\u00dfbar worden,", "tokens": ["Da\u00df", "es", "un\u00b7ge\u00b7nie\u00df\u00b7bar", "wor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VAPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.40": {"text": "Weil die beiden, die uns freuten,", "tokens": ["Weil", "die", "bei\u00b7den", ",", "die", "uns", "freu\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "PIAT", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.41": {"text": "Sich seitdem zum Flug gesch\u00fcrzet,", "tokens": ["Sich", "seit\u00b7dem", "zum", "Flug", "ge\u00b7sch\u00fcr\u00b7zet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "PAV", "APPRART", "NN", "VVPP", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.42": {"text": "Und in Trauer uns gest\u00fcrzet.", "tokens": ["Und", "in", "Trau\u00b7er", "uns", "ge\u00b7st\u00fcr\u00b7zet", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "PPER", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.43": {"text": "K\u00fcmmel und Wachholderbeerchen!", "tokens": ["K\u00fcm\u00b7mel", "und", "Wach\u00b7hol\u00b7der\u00b7beer\u00b7chen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.44": {"text": "Immer denk ich an das P\u00e4rchen,", "tokens": ["Im\u00b7mer", "denk", "ich", "an", "das", "P\u00e4r\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.45": {"text": "Das euch streute, das uns freute,", "tokens": ["Das", "euch", "streu\u00b7te", ",", "das", "uns", "freu\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "VVFIN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.46": {"text": "Und im Kummer freut mich's heute,", "tokens": ["Und", "im", "Kum\u00b7mer", "freut", "mich's", "heu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.47": {"text": "Da\u00df ich doch, als es euch streute,", "tokens": ["Da\u00df", "ich", "doch", ",", "als", "es", "euch", "streu\u00b7te", ","], "token_info": ["word", "word