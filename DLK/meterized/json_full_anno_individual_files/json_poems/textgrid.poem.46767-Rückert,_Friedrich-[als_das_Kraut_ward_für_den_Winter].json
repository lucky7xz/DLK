{"textgrid.poem.46767": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[als das Kraut ward f\u00fcr den Winter]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als das Kraut ward f\u00fcr den Winter", "tokens": ["Als", "das", "Kraut", "ward", "f\u00fcr", "den", "Win\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "VAFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Vom Krautschnitter eingeschnitten,", "tokens": ["Vom", "Krautsc\u00b7hnit\u00b7ter", "ein\u00b7ge\u00b7schnit\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sahen zu die beiden Kinder,", "tokens": ["Sa\u00b7hen", "zu", "die", "bei\u00b7den", "Kin\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sitzend auf dem Fenstertrittchen;", "tokens": ["Sit\u00b7zend", "auf", "dem", "Fens\u00b7ter\u00b7tritt\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Hatten eines um das andre", "tokens": ["Hat\u00b7ten", "ei\u00b7nes", "um", "das", "and\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ART", "APPR", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Hergelegt ein Aermchen jedes,", "tokens": ["Her\u00b7ge\u00b7legt", "ein", "A\u00b7erm\u00b7chen", "je\u00b7des", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PIAT", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.7": {"text": "Sie ihr linkes, er sein rechtes,", "tokens": ["Sie", "ihr", "lin\u00b7kes", ",", "er", "sein", "rech\u00b7tes", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "ADJA", "$,", "PPER", "PPOSAT", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Sie ihr linkes um den Bruder,", "tokens": ["Sie", "ihr", "lin\u00b7kes", "um", "den", "Bru\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "ADJA", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Er sein rechtes um die Schwester.", "tokens": ["Er", "sein", "rech\u00b7tes", "um", "die", "Schwes\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "ADJA", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Aber mit den freien Aermchen", "tokens": ["A\u00b7ber", "mit", "den", "frei\u00b7en", "A\u00b7erm\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.11": {"text": "Langten sie nach zwei Gef\u00e4\u00dfchen,", "tokens": ["Lang\u00b7ten", "sie", "nach", "zwei", "Ge\u00b7f\u00e4\u00df\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "CARD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Die beim gro\u00dfen Fasse standen,", "tokens": ["Die", "beim", "gro\u00b7\u00dfen", "Fas\u00b7se", "stan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Sie mit ihrem rechten H\u00e4ndchen,", "tokens": ["Sie", "mit", "ih\u00b7rem", "rech\u00b7ten", "H\u00e4nd\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Er mit seinem linken, griffen", "tokens": ["Er", "mit", "sei\u00b7nem", "lin\u00b7ken", ",", "grif\u00b7fen"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["PPER", "APPR", "PPOSAT", "ADJA", "$,", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "In den Vorrath der Gef\u00e4\u00dfe,", "tokens": ["In", "den", "Vor\u00b7rath", "der", "Ge\u00b7f\u00e4\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-++--+-", "measure": "trochaic.tetra.relaxed"}, "line.16": {"text": "K\u00fcmmel und Wachholderbeeren,", "tokens": ["K\u00fcm\u00b7mel", "und", "Wach\u00b7hol\u00b7der\u00b7bee\u00b7ren", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.17": {"text": "Die dem Kraut zur W\u00fcrze dienen.", "tokens": ["Die", "dem", "Kraut", "zur", "W\u00fcr\u00b7ze", "die\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Zusah ich mit L\u00e4cheln ihnen,", "tokens": ["Zu\u00b7sah", "ich", "mit", "L\u00e4\u00b7cheln", "ih\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "PPER", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.19": {"text": "Wie sie unverdrossen streuten,", "tokens": ["Wie", "sie", "un\u00b7ver\u00b7dros\u00b7sen", "streu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "K\u00fcmmel streut' er, sie Wachholder,", "tokens": ["K\u00fcm\u00b7mel", "streut'", "er", ",", "sie", "Wach\u00b7hol\u00b7der", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$,", "PPER", "NN", "$,"], "meter": "+-+-+++-", "measure": "unknown.measure.penta"}, "line.21": {"text": "Eifrig wie im Krieg begriffen,", "tokens": ["Eif\u00b7rig", "wie", "im", "Krieg", "be\u00b7grif\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "Mit dem Wettstreit zweier H\u00e4ndchen,", "tokens": ["Mit", "dem", "Wett\u00b7streit", "zwei\u00b7er", "H\u00e4nd\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.23": {"text": "Und die Eintracht ihrer beiden", "tokens": ["Und", "die", "Ein\u00b7tracht", "ih\u00b7rer", "bei\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "PPOSAT", "PIAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "Andern hielt den Leib umschlungen.", "tokens": ["An\u00b7dern", "hielt", "den", "Leib", "um\u00b7schlun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "VVFIN", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.25": {"text": "Sollt' ich mir die Wonne k\u00fcrzen", "tokens": ["Sollt'", "ich", "mir", "die", "Won\u00b7ne", "k\u00fcr\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PPER", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.26": {"text": "Dieses Anblicks? Zwar ich dachte,", "tokens": ["Die\u00b7ses", "An\u00b7blicks", "?", "Zwar", "ich", "dach\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "$.", "ADV", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.27": {"text": "Wenn so streuet zu mein P\u00e4rchen", "tokens": ["Wenn", "so", "streu\u00b7et", "zu", "mein", "P\u00e4r\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.28": {"text": "K\u00fcmmel und Wachholderbeerchen,", "tokens": ["K\u00fcm\u00b7mel", "und", "Wach\u00b7hol\u00b7der\u00b7beer\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.29": {"text": "Werden sie das Kraut verw\u00fcrzen;", "tokens": ["Wer\u00b7den", "sie", "das", "Kraut", "ver\u00b7w\u00fcr\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.30": {"text": "Ihnen doch die Lust zu wehren,", "tokens": ["Ih\u00b7nen", "doch", "die", "Lust", "zu", "weh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.31": {"text": "Konnt' ich \u00fcbers Herz nicht bringen,", "tokens": ["Konnt'", "ich", "\u00fc\u00b7bers", "Herz", "nicht", "brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPRART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.32": {"text": "Weil sie gar zu zierlich streuten.", "tokens": ["Weil", "sie", "gar", "zu", "zier\u00b7lich", "streu\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PTKA", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.33": {"text": "Und nun ist es eingetroffen,", "tokens": ["Und", "nun", "ist", "es", "ein\u00b7ge\u00b7trof\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.34": {"text": "Schlimmer, als es war zu hoffen,", "tokens": ["Schlim\u00b7mer", ",", "als", "es", "war", "zu", "hof\u00b7fen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "VAFIN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.35": {"text": "K\u00fcmmel und Wachholderbeerchen,", "tokens": ["K\u00fcm\u00b7mel", "und", "Wach\u00b7hol\u00b7der\u00b7beer\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.36": {"text": "Ihr, gestreut von meinem P\u00e4rchen,", "tokens": ["Ihr", ",", "ge\u00b7streut", "von", "mei\u00b7nem", "P\u00e4r\u00b7chen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "VVPP", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.37": {"text": "Habet uns das Kraut verw\u00fcrzet,", "tokens": ["Ha\u00b7bet", "uns", "das", "Kraut", "ver\u00b7w\u00fcr\u00b7zet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.38": {"text": "Es verk\u00fcmmert und verbittert,", "tokens": ["Es", "ver\u00b7k\u00fcm\u00b7mert", "und", "ver\u00b7bit\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.39": {"text": "Da\u00df es ungenie\u00dfbar worden,", "tokens": ["Da\u00df", "es", "un\u00b7ge\u00b7nie\u00df\u00b7bar", "wor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VAPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.40": {"text": "Weil die beiden, die uns freuten,", "tokens": ["Weil", "die", "bei\u00b7den", ",", "die", "uns", "freu\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "PIAT", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.41": {"text": "Sich seitdem zum Flug gesch\u00fcrzet,", "tokens": ["Sich", "seit\u00b7dem", "zum", "Flug", "ge\u00b7sch\u00fcr\u00b7zet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "PAV", "APPRART", "NN", "VVPP", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.42": {"text": "Und in Trauer uns gest\u00fcrzet.", "tokens": ["Und", "in", "Trau\u00b7er", "uns", "ge\u00b7st\u00fcr\u00b7zet", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "PPER", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.43": {"text": "K\u00fcmmel und Wachholderbeerchen!", "tokens": ["K\u00fcm\u00b7mel", "und", "Wach\u00b7hol\u00b7der\u00b7beer\u00b7chen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.44": {"text": "Immer denk ich an das P\u00e4rchen,", "tokens": ["Im\u00b7mer", "denk", "ich", "an", "das", "P\u00e4r\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.45": {"text": "Das euch streute, das uns freute,", "tokens": ["Das", "euch", "streu\u00b7te", ",", "das", "uns", "freu\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "VVFIN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.46": {"text": "Und im Kummer freut mich's heute,", "tokens": ["Und", "im", "Kum\u00b7mer", "freut", "mich's", "heu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.47": {"text": "Da\u00df ich doch, als es euch streute,", "tokens": ["Da\u00df", "ich", "doch", ",", "als", "es", "euch", "streu\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "$,", "KOUS", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.48": {"text": "Um die Freud' es nicht verk\u00fcrzet.", "tokens": ["Um", "die", "Freud'", "es", "nicht", "ver\u00b7k\u00fcr\u00b7zet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "PPER", "PTKNEG", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Als das Kraut ward f\u00fcr den Winter", "tokens": ["Als", "das", "Kraut", "ward", "f\u00fcr", "den", "Win\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "VAFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Vom Krautschnitter eingeschnitten,", "tokens": ["Vom", "Krautsc\u00b7hnit\u00b7ter", "ein\u00b7ge\u00b7schnit\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sahen zu die beiden Kinder,", "tokens": ["Sa\u00b7hen", "zu", "die", "bei\u00b7den", "Kin\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sitzend auf dem Fenstertrittchen;", "tokens": ["Sit\u00b7zend", "auf", "dem", "Fens\u00b7ter\u00b7tritt\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Hatten eines um das andre", "tokens": ["Hat\u00b7ten", "ei\u00b7nes", "um", "das", "and\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ART", "APPR", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Hergelegt ein Aermchen jedes,", "tokens": ["Her\u00b7ge\u00b7legt", "ein", "A\u00b7erm\u00b7chen", "je\u00b7des", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PIAT", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.7": {"text": "Sie ihr linkes, er sein rechtes,", "tokens": ["Sie", "ihr", "lin\u00b7kes", ",", "er", "sein", "rech\u00b7tes", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "ADJA", "$,", "PPER", "PPOSAT", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Sie ihr linkes um den Bruder,", "tokens": ["Sie", "ihr", "lin\u00b7kes", "um", "den", "Bru\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "ADJA", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Er sein rechtes um die Schwester.", "tokens": ["Er", "sein", "rech\u00b7tes", "um", "die", "Schwes\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "ADJA", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Aber mit den freien Aermchen", "tokens": ["A\u00b7ber", "mit", "den", "frei\u00b7en", "A\u00b7erm\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.11": {"text": "Langten sie nach zwei Gef\u00e4\u00dfchen,", "tokens": ["Lang\u00b7ten", "sie", "nach", "zwei", "Ge\u00b7f\u00e4\u00df\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "CARD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Die beim gro\u00dfen Fasse standen,", "tokens": ["Die", "beim", "gro\u00b7\u00dfen", "Fas\u00b7se", "stan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Sie mit ihrem rechten H\u00e4ndchen,", "tokens": ["Sie", "mit", "ih\u00b7rem", "rech\u00b7ten", "H\u00e4nd\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Er mit seinem linken, griffen", "tokens": ["Er", "mit", "sei\u00b7nem", "lin\u00b7ken", ",", "grif\u00b7fen"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["PPER", "APPR", "PPOSAT", "ADJA", "$,", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "In den Vorrath der Gef\u00e4\u00dfe,", "tokens": ["In", "den", "Vor\u00b7rath", "der", "Ge\u00b7f\u00e4\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-++--+-", "measure": "trochaic.tetra.relaxed"}, "line.16": {"text": "K\u00fcmmel und Wachholderbeeren,", "tokens": ["K\u00fcm\u00b7mel", "und", "Wach\u00b7hol\u00b7der\u00b7bee\u00b7ren", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.17": {"text": "Die dem Kraut zur W\u00fcrze dienen.", "tokens": ["Die", "dem", "Kraut", "zur", "W\u00fcr\u00b7ze", "die\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Zusah ich mit L\u00e4cheln ihnen,", "tokens": ["Zu\u00b7sah", "ich", "mit", "L\u00e4\u00b7cheln", "ih\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "PPER", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.19": {"text": "Wie sie unverdrossen streuten,", "tokens": ["Wie", "sie", "un\u00b7ver\u00b7dros\u00b7sen", "streu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "K\u00fcmmel streut' er, sie Wachholder,", "tokens": ["K\u00fcm\u00b7mel", "streut'", "er", ",", "sie", "Wach\u00b7hol\u00b7der", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$,", "PPER", "NN", "$,"], "meter": "+-+-+++-", "measure": "unknown.measure.penta"}, "line.21": {"text": "Eifrig wie im Krieg begriffen,", "tokens": ["Eif\u00b7rig", "wie", "im", "Krieg", "be\u00b7grif\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "Mit dem Wettstreit zweier H\u00e4ndchen,", "t