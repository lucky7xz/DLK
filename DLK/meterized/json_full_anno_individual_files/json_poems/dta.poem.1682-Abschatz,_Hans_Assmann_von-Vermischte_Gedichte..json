{"dta.poem.1682": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Vermischte  \n Gedichte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Nun werff ich ab das Joch der stillen Einsamkeit/ ", "tokens": ["Nun", "werff", "ich", "ab", "das", "Joch", "der", "stil\u00b7len", "Ein\u00b7sam\u00b7keit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und suche mehr vergn\u00fcgt zu schl\u00fcssen meine Zeit;", "tokens": ["Und", "su\u00b7che", "mehr", "ver\u00b7gn\u00fcgt", "zu", "schl\u00fcs\u00b7sen", "mei\u00b7ne", "Zeit", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "VVPP", "PTKZU", "VVINF", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der falschen Freyheit Schein", "tokens": ["Der", "fal\u00b7schen", "Frey\u00b7heit", "Schein"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Soll \u00fcber meine Sinnen/", "tokens": ["Soll", "\u00fc\u00b7ber", "mei\u00b7ne", "Sin\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die selber herrschen k\u00fcnnen/", "tokens": ["Die", "sel\u00b7ber", "herr\u00b7schen", "k\u00fcn\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVINF", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Nicht mehr Tyranne seyn.", "tokens": ["Nicht", "mehr", "Ty\u00b7ran\u00b7ne", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PIAT", "NN", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wer unverehligt lebt/ nennt sein Gem\u00fctte frey:", "tokens": ["Wer", "un\u00b7ve\u00b7reh\u00b7ligt", "lebt", "/", "nennt", "sein", "Ge\u00b7m\u00fct\u00b7te", "frey", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VVFIN", "$(", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+--++-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Urtheilet/ wer von uns doch mehr gebunden sey!", "tokens": ["Ur\u00b7thei\u00b7let", "/", "wer", "von", "uns", "doch", "mehr", "ge\u00b7bun\u00b7den", "sey", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "PWS", "APPR", "PPER", "ADV", "ADV", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mich hat mein freyer Schlu\u00df", "tokens": ["Mich", "hat", "mein", "frey\u00b7er", "Schlu\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Zu einer Wahl verbunden/", "tokens": ["Zu", "ei\u00b7ner", "Wahl", "ver\u00b7bun\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Um die er alle Stunden", "tokens": ["Um", "die", "er", "al\u00b7le", "Stun\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "PPER", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "In Sorgen stehen mu\u00df.", "tokens": ["In", "Sor\u00b7gen", "ste\u00b7hen", "mu\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Was er verlieren will/ hebt er mit Sorgen auff/", "tokens": ["Was", "er", "ver\u00b7lie\u00b7ren", "will", "/", "hebt", "er", "mit", "Sor\u00b7gen", "auff", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVINF", "VMFIN", "$(", "VVFIN", "PPER", "APPR", "NN", "APPR", "$("], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Erwartet offtermahls daf\u00fcr den schlechtsten Kauff:", "tokens": ["Er\u00b7war\u00b7tet", "off\u00b7ter\u00b7mahls", "da\u00b7f\u00fcr", "den", "schlechts\u00b7ten", "Kauff", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PAV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Er wei\u00df nicht was er kriegt/", "tokens": ["Er", "wei\u00df", "nicht", "was", "er", "kriegt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "PWS", "PPER", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Deuckt/ iedes sey das Seine;", "tokens": ["Deuckt", "/", "ie\u00b7des", "sey", "das", "Sei\u00b7ne", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "PIS", "VAFIN", "ART", "PPOSAT", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ich habe schon das Meine", "tokens": ["Ich", "ha\u00b7be", "schon", "das", "Mei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ART", "PPOSAT"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und bin damit vergn\u00fcgt.", "tokens": ["Und", "bin", "da\u00b7mit", "ver\u00b7gn\u00fcgt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PAV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Jedweder Gegenstand ist seiner Liebe Ziel/", "tokens": ["Jed\u00b7we\u00b7der", "Ge\u00b7gen\u00b7stand", "ist", "sei\u00b7ner", "Lie\u00b7be", "Ziel", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PPOSAT", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Er mu\u00df ein Sclave seyn/ wo er nicht herrschen will:", "tokens": ["Er", "mu\u00df", "ein", "Scla\u00b7ve", "seyn", "/", "wo", "er", "nicht", "herr\u00b7schen", "will", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "VAINF", "$(", "PWAV", "PPER", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich lebe wo ich lieb/", "tokens": ["Ich", "le\u00b7be", "wo", "ich", "lieb", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PWAV", "PPER", "ADJD", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und liebe wo ich lebe;", "tokens": ["Und", "lie\u00b7be", "wo", "ich", "le\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PWAV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Was ich empfang und gebe", "tokens": ["Was", "ich", "emp\u00b7fang", "und", "ge\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ist freyer Sinnen Trieb.", "tokens": ["Ist", "frey\u00b7er", "Sin\u00b7nen", "Trieb", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Die Augen aller Welt sind auff ihn zugericht;", "tokens": ["Die", "Au\u00b7gen", "al\u00b7ler", "Welt", "sind", "auff", "ihn", "zu\u00b7ge\u00b7richt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "NN", "VAFIN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Seht/ wie ihm jederman ein scharffes Urtheil spricht.", "tokens": ["Seht", "/", "wie", "ihm", "je\u00b7der\u00b7man", "ein", "scharf\u00b7fes", "Ur\u00b7theil", "spricht", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$(", "PWAV", "PPER", "PIS", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Bald lebet er zu frey/", "tokens": ["Bald", "le\u00b7bet", "er", "zu", "frey", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKA", "ADJD", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Bald will man \u00fcbel deuten/", "tokens": ["Bald", "will", "man", "\u00fc\u00b7bel", "deu\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIS", "ADJD", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Da\u00df er zu stoltz bey Leuten/", "tokens": ["Da\u00df", "er", "zu", "stoltz", "bey", "Leu\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKA", "ADJD", "APPR", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Zu bl\u00f6d und traurig sey.", "tokens": ["Zu", "bl\u00f6d", "und", "trau\u00b7rig", "sey", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "KON", "ADJD", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Was bringt ihm nicht f\u00fcr Furcht der hei\u00dfen Liebe Macht/", "tokens": ["Was", "bringt", "ihm", "nicht", "f\u00fcr", "Furcht", "der", "hei\u00b7\u00dfen", "Lie\u00b7be", "Macht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PTKNEG", "APPR", "NN", "ART", "ADJA", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn manch Lieb-reitzend Blick nach seiner Freyheit tracht/", "tokens": ["Wenn", "manch", "Lie\u00b7breit\u00b7zend", "Blick", "nach", "sei\u00b7ner", "Frey\u00b7heit", "tracht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wenn manche Venus ihn/", "tokens": ["Wenn", "man\u00b7che", "Ve\u00b7nus", "ihn", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "PPER", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "An der ihm nichts gelegen/", "tokens": ["An", "der", "ihm", "nichts", "ge\u00b7le\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PPER", "PIS", "VVPP", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Auff unbekandten Wegen", "tokens": ["Auff", "un\u00b7be\u00b7kand\u00b7ten", "We\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Will in ihr Netze ziehn!", "tokens": ["Will", "in", "ihr", "Net\u00b7ze", "ziehn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "L\u00f6scht wohl der Seelen Brand ein abgestohlner Ku\u00df/", "tokens": ["L\u00f6scht", "wohl", "der", "See\u00b7len", "Brand", "ein", "ab\u00b7ge\u00b7stohl\u00b7ner", "Ku\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "NN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn man f\u00fcr Augen sieht der andern Uberflu\u00df/", "tokens": ["Wenn", "man", "f\u00fcr", "Au\u00b7gen", "sieht", "der", "an\u00b7dern", "U\u00b7ber\u00b7flu\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "NN", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wenn andre nehmen ein", "tokens": ["Wenn", "and\u00b7re", "neh\u00b7men", "ein"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PIS", "VVFIN", "ART"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Mit ungez\u00e4hlten Zahlen/", "tokens": ["Mit", "un\u00b7ge\u00b7z\u00e4hl\u00b7ten", "Zah\u00b7len", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Was uns zu vielen mahlen", "tokens": ["Was", "uns", "zu", "vie\u00b7len", "mah\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "APPR", "PIS", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Mu\u00df abgeschlagen seyn?", "tokens": ["Mu\u00df", "ab\u00b7ge\u00b7schla\u00b7gen", "seyn", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "VVPP", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}