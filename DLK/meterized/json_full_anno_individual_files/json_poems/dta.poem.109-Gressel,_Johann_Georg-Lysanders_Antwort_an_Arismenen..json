okens": ["Durch", "die", "W\u00fcs\u00b7ten", "mu\u00df", "man", "ge\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VMFIN", "PIS", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.54": {"text": "Wenn man will in Canaan/", "tokens": ["Wenn", "man", "will", "in", "Ca\u00b7naan", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VMFIN", "APPR", "NE", "$("], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.55": {"text": "Bey den Br\u00fcsten stille stehen", "tokens": ["Bey", "den", "Br\u00fcs\u00b7ten", "stil\u00b7le", "ste\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.56": {"text": "Nur zum Schein vergn\u00fcgen kan.", "tokens": ["Nur", "zum", "Schein", "ver\u00b7gn\u00fc\u00b7gen", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.57": {"text": "Selbst die Seele kan es f\u00fchlen", "tokens": ["Selbst", "die", "See\u00b7le", "kan", "es", "f\u00fch\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "VMFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.58": {"text": "Was sie da vor Lust geneu\u00dft/", "tokens": ["Was", "sie", "da", "vor", "Lust", "ge\u00b7neu\u00dft", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "APPR", "NN", "VVPP", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.59": {"text": "Wenn sie kan daselbsten spielen/", "tokens": ["Wenn", "sie", "kan", "da\u00b7selbs\u00b7ten", "spie\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VM