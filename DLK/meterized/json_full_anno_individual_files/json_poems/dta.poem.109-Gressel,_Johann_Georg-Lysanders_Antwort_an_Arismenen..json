{"dta.poem.109": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Lysanders  Antwort an  Arismenen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Warum wird die Frucht des Lebens", "tokens": ["Wa\u00b7rum", "wird", "die", "Frucht", "des", "Le\u00b7bens"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sch\u00f6nster Engel mir versagt?", "tokens": ["Sch\u00f6ns\u00b7ter", "En\u00b7gel", "mir", "ver\u00b7sagt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Lieb\u2019 ich denn so gar vergebens?", "tokens": ["Lieb'", "ich", "denn", "so", "gar", "ver\u00b7ge\u00b7bens", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "ADV", "ADV", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Darf kein Griff nicht seyn gewagt?", "tokens": ["Darf", "kein", "Griff", "nicht", "seyn", "ge\u00b7wagt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIAT", "NN", "PTKNEG", "VAINF", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Heisset das schon \u00fcbertreten", "tokens": ["Heis\u00b7set", "das", "schon", "\u00fc\u00b7bert\u00b7re\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADV", "VVINF"], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.6": {"text": "Wenn man nur die Frucht angreifft?", "tokens": ["Wenn", "man", "nur", "die", "Frucht", "an\u00b7greifft", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Sollen denn Egyptens-Ketten", "tokens": ["Sol\u00b7len", "denn", "E\u00b7gyp\u00b7tens\u00b7Ket\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["VMFIN", "ADV", "NE"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Niemahls werden abgestreifft?", "tokens": ["Nie\u00b7mahls", "wer\u00b7den", "ab\u00b7ge\u00b7streifft", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Soll ein steter Sclave bleiben/", "tokens": ["Soll", "ein", "ste\u00b7ter", "Scla\u00b7ve", "blei\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Mein so sehr entbrannter Sinn?", "tokens": ["Mein", "so", "sehr", "ent\u00b7brann\u00b7ter", "Sinn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADV", "ADV", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Darf ich nicht die Finger treiben", "tokens": ["Darf", "ich", "nicht", "die", "Fin\u00b7ger", "trei\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PTKNEG", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "An das Land der L\u00fcste hin?", "tokens": ["An", "das", "Land", "der", "L\u00fcs\u00b7te", "hin", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "Kan man das wol dreiste nennen/", "tokens": ["Kan", "man", "das", "wol", "dreis\u00b7te", "nen\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "ART", "ADV", "ADJA", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Was die treue Hand ver\u00fcbt?", "tokens": ["Was", "die", "treu\u00b7e", "Hand", "ver\u00b7\u00fcbt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.15": {"text": "Die zum Opffer sich verbrennen", "tokens": ["Die", "zum", "Opf\u00b7fer", "sich", "ver\u00b7bren\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPRART", "NN", "PRF", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Hat man jederzeit geliebt.", "tokens": ["Hat", "man", "je\u00b7der\u00b7zeit", "ge\u00b7liebt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.17": {"text": "So kan auch mein Unternehmen/", "tokens": ["So", "kan", "auch", "mein", "Un\u00b7ter\u00b7neh\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ADV", "PPOSAT", "NN", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.18": {"text": "Gar kein Trieb der Geilheit seyn;", "tokens": ["Gar", "kein", "Trieb", "der", "Geil\u00b7heit", "seyn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "ART", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.19": {"text": "Denn zum Opffer sich bequehmen/", "tokens": ["Denn", "zum", "Opf\u00b7fer", "sich", "be\u00b7queh\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "PRF", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Nimmt nicht l\u00fcstern Geister ein.", "tokens": ["Nimmt", "nicht", "l\u00fcs\u00b7tern", "Geis\u00b7ter", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.21": {"text": "Ist es nicht des Schoosses Ehre?", "tokens": ["Ist", "es", "nicht", "des", "Schoos\u00b7ses", "Eh\u00b7re", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "Wenn sie kr\u00f6net meine Hand/", "tokens": ["Wenn", "sie", "kr\u00f6\u00b7net", "mei\u00b7ne", "Hand", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "PPOSAT", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.23": {"text": "Weil ich ihr gantz zu geh\u00f6re/", "tokens": ["Weil", "ich", "ihr", "gantz", "zu", "ge\u00b7h\u00f6\u00b7re", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "PTKZU", "VVFIN", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.24": {"text": "Gr\u00fcsse ich das sch\u00f6ne Land.", "tokens": ["Gr\u00fcs\u00b7se", "ich", "das", "sch\u00f6\u00b7ne", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.25": {"text": "Wo sind wol die zarten Wellen/", "tokens": ["Wo", "sind", "wol", "die", "zar\u00b7ten", "Wel\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ADV", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.26": {"text": "So des Lebens Perlen thaun/", "tokens": ["So", "des", "Le\u00b7bens", "Per\u00b7len", "thaun", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "NN", "VVINF", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.27": {"text": "Und der Wollust Lager-Stellen", "tokens": ["Und", "der", "Wol\u00b7lust", "La\u00b7ger\u00b7Stel\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.28": {"text": "Als in ihrer Schoo\u00df zu schaun?", "tokens": ["Als", "in", "ih\u00b7rer", "Schoo\u00df", "zu", "schaun", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.29": {"text": "Durch das Opffer treuer Finger", "tokens": ["Durch", "das", "Opf\u00b7fer", "treu\u00b7er", "Fin\u00b7ger"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.30": {"text": "Wird kein Heiligthum beschmitzt;", "tokens": ["Wird", "kein", "Hei\u00b7lig\u00b7thum", "be\u00b7schmitzt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.31": {"text": "So wird auch ihr Schoo\u00df nicht ringer", "tokens": ["So", "wird", "auch", "ihr", "Schoo\u00df", "nicht", "rin\u00b7ger"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "PPOSAT", "NN", "PTKNEG", "ADJD"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.32": {"text": "Wenn sie meine Hand besch\u00fctzt.", "tokens": ["Wenn", "sie", "mei\u00b7ne", "Hand", "be\u00b7sch\u00fctzt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.33": {"text": "Sch\u00fctzt den Eingang dieses Landes/", "tokens": ["Sch\u00fctzt", "den", "Ein\u00b7gang", "die\u00b7ses", "Lan\u00b7des", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PDAT", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.34": {"text": "Hat ihr Mund ja selbst gesagt;", "tokens": ["Hat", "ihr", "Mund", "ja", "selbst", "ge\u00b7sagt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ADV", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.35": {"text": "Ehrt man nicht die Pracht des Strandes/", "tokens": ["Ehrt", "man", "nicht", "die", "Pracht", "des", "Stran\u00b7des", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PTKNEG", "ART", "NN", "ART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.36": {"text": "Vor den man sein Leben wagt?", "tokens": ["Vor", "den", "man", "sein", "Le\u00b7ben", "wagt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PIS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+---+-+", "measure": "dactylic.init"}, "line.37": {"text": "S\u00fcndigen denn meine H\u00e4nde", "tokens": ["S\u00fcn\u00b7di\u00b7gen", "denn", "mei\u00b7ne", "H\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "KON", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.38": {"text": "Wenn sie ihr Gebote thun/", "tokens": ["Wenn", "sie", "ihr", "Ge\u00b7bo\u00b7te", "thun", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVINF", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.39": {"text": "Und als eigne Liebes-Pf\u00e4nde", "tokens": ["Und", "als", "eig\u00b7ne", "Lie\u00b7bes\u00b7P\u00b7f\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "KOUS", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.40": {"text": "Bey des Schoosses Eingang ruhn?", "tokens": ["Bey", "des", "Schoos\u00b7ses", "Ein\u00b7gang", "ruhn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.41": {"text": "Jsrael geht durch die W\u00fcsten", "tokens": ["Js\u00b7rael", "geht", "durch", "die", "W\u00fcs\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.42": {"text": "Aus der strengen Dienstbarkeit/", "tokens": ["Aus", "der", "stren\u00b7gen", "Dienst\u00b7bar\u00b7keit", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.43": {"text": "Solte mich denn nicht gel\u00fcsten", "tokens": ["Sol\u00b7te", "mich", "denn", "nicht", "ge\u00b7l\u00fcs\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ADV", "PTKNEG", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.44": {"text": "Auch zu seyn in Sicherheit?", "tokens": ["Auch", "zu", "seyn", "in", "Si\u00b7cher\u00b7heit", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKZU", "VAINF", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.45": {"text": "Ist nicht in den Rosen-Gr\u00fcnden", "tokens": ["Ist", "nicht", "in", "den", "Ro\u00b7sen\u00b7Gr\u00fcn\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PTKNEG", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.46": {"text": "Amors liebster Ruhe-Platz/", "tokens": ["A\u00