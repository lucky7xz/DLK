{"dta.poem.9075": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1898", "urn": "urn:nbn:de:kobv:b4-200905192685", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich liege noch im Bett und habe eben Kaffee getrunken.", "tokens": ["Ich", "lie\u00b7ge", "noch", "im", "Bett", "und", "ha\u00b7be", "e\u00b7ben", "Kaf\u00b7fee", "ge\u00b7trun\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "NN", "KON", "VAFIN", "ADV", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+--+-", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Das Feuer im Ofen knattert schon,", "tokens": ["Das", "Feu\u00b7er", "im", "O\u00b7fen", "knat\u00b7tert", "schon", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVFIN", "ADV", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "durchs Fenster,", "tokens": ["durchs", "Fens\u00b7ter", ","], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "das ganze St\u00fcbchen f\u00fcllend,", "tokens": ["das", "gan\u00b7ze", "St\u00fcb\u00b7chen", "f\u00fcl\u00b7lend", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Schneelicht.", "tokens": ["Schnee\u00b7licht", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}}, "stanza.2": {"line.1": {"text": "Ich lese.", "tokens": ["Ich", "le\u00b7se", "."], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVFIN", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.3": {"line.1": {"text": "Huysmans. L\u00e0 Bas.", "tokens": ["Huys\u00b7mans", ".", "L\u00e0", "Bas", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$.", "NE", "NE", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.4": {"line.1": {"text": ". . . Alors,", "tokens": [".", ".", ".", "A\u00b7lors", ","], "token_info": ["punct", "punct", "punct", "word", "punct"], "pos": ["$.", "$.", "$.", "NE", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.2": {"text": "en sa blanche splendeur,", "tokens": ["en", "sa", "blan\u00b7che", "splen\u00b7deur", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "l'\u00e2me du Moyen Age rayonna dans cette salle . . .", "tokens": ["l'\u00b7\u00e2\u00b7me", "du", "Mo\u00b7yen", "A\u00b7ge", "ra\u00b7yon\u00b7na", "dans", "cet\u00b7te", "sal\u00b7le", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["VVFIN", "PPER", "FM", "FM", "FM", "FM", "FM", "FM", "$.", "$.", "$."], "meter": "-+--+--+--+-+-+-", "measure": "amphibrach.tetra.plus"}}, "stanza.5": {"line.1": {"text": "Pl\u00f6tzlich,", "tokens": ["Pl\u00f6tz\u00b7lich", ","], "token_info": ["word", "punct"], "pos": ["ADJD", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.2": {"text": "irgendwo tiefer im Hause,", "tokens": ["ir\u00b7gend\u00b7wo", "tie\u00b7fer", "im", "Hau\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPRART", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "ein Kanarienvogel.", "tokens": ["ein", "Ka\u00b7na\u00b7ri\u00b7en\u00b7vo\u00b7gel", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "+-+-+--", "measure": "unknown.measure.tri"}}, "stanza.6": {"line.1": {"text": "Die sch\u00f6nsten L\u00e4ufe!", "tokens": ["Die", "sch\u00f6ns\u00b7ten", "L\u00e4u\u00b7fe", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.7": {"line.1": {"text": "Ich lasse das Buch sinken.", "tokens": ["Ich", "las\u00b7se", "das", "Buch", "sin\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Die Augen schliessen sich mir,", "tokens": ["Die", "Au\u00b7gen", "schlies\u00b7sen", "sich", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "PPER", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "ich liege wieder da, den Kopf in die Kissen \u2014 \u2014", "tokens": ["ich", "lie\u00b7ge", "wie\u00b7der", "da", ",", "den", "Kopf", "in", "die", "Kis\u00b7sen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "$,", "ART", "NN", "APPR", "ART", "NN", "$(", "$("], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}}}}}