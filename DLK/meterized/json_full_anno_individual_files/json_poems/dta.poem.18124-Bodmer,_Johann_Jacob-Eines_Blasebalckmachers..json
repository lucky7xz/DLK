{"dta.poem.18124": {"metadata": {"author": {"name": "Bodmer, Johann Jacob", "birth": "N.A.", "death": "N.A."}, "title": "Eines Blasebalckmachers.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-200905198432", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Olieber Mensch, dein Leben ja betrachte,", "tokens": ["O\u00b7lie\u00b7ber", "Mensch", ",", "dein", "Le\u00b7ben", "ja", "be\u00b7trach\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PPOSAT", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Hier liget, der die Blaseb\u00e4lge machte,", "tokens": ["Hier", "li\u00b7get", ",", "der", "die", "Bla\u00b7se\u00b7b\u00e4l\u00b7ge", "mach\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Jezt aber nun zulezt es doch sich findt,", "tokens": ["Jezt", "a\u00b7ber", "nun", "zu\u00b7lezt", "es", "doch", "sich", "findt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "ADV", "PPER", "ADV", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Dem Meister, schau, gebrist noch selber Wind.", "tokens": ["Dem", "Meis\u00b7ter", ",", "schau", ",", "ge\u00b7brist", "noch", "sel\u00b7ber", "Wind", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PTKVZ", "$,", "VVFIN", "ADV", "ADV", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}