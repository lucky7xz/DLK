{"dta.poem.20121": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Der Tower-Brand.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1851", "urn": "urn:nbn:de:kobv:b4-200905191321", "language": ["de:0.99"], "booktitle": "Fontane, Theodor: Gedichte. Berlin, 1851."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn\u2019s im Tower Nacht geworden, wenn die", "tokens": ["Wenn's", "im", "To\u00b7wer", "Nacht", "ge\u00b7wor\u00b7den", ",", "wenn", "die"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "APPRART", "NN", "NN", "VAPP", "$,", "KOUS", "ART"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "H\u00f6fe leer und stumm,", "tokens": ["H\u00f6\u00b7fe", "leer", "und", "stumm", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Gehn die Geister der Erschlagnen in den Corri-", "tokens": ["Gehn", "die", "Geis\u00b7ter", "der", "Er\u00b7schlag\u00b7nen", "in", "den", "Cor\u00b7ri"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "ART", "NN", "APPR", "ART", "TRUNC"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.4": {"text": "doren um,", "tokens": ["do\u00b7ren", "um", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "PTKVZ", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Durch die L\u00fcfte bebt Gefl\u00fcster klagend dann,", "tokens": ["Durch", "die", "L\u00fcf\u00b7te", "bebt", "Ge\u00b7fl\u00fcs\u00b7ter", "kla\u00b7gend", "dann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "NN", "VVPP", "ADV", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.6": {"text": "wie Herbsteswehn,", "tokens": ["wie", "Herbs\u00b7tes\u00b7wehn", ","], "token_info": ["word", "word", "punct"], "pos": ["PWAV", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Mancher hat im Mondenschimmer schon die", "tokens": ["Man\u00b7cher", "hat", "im", "Mon\u00b7den\u00b7schim\u00b7mer", "schon", "die"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VAFIN", "APPRART", "NN", "ADV", "ART"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Schatten schreiten sehn.", "tokens": ["Schat\u00b7ten", "schrei\u00b7ten", "sehn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "VVINF", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Vor dem Zug, im Purpurmantel, silberwei\u00df", "tokens": ["Vor", "dem", "Zug", ",", "im", "Pur\u00b7pur\u00b7man\u00b7tel", ",", "sil\u00b7ber\u00b7wei\u00df"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["APPR", "ART", "NN", "$,", "APPRART", "NN", "$,", "VVFIN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "von Bart umwallt,", "tokens": ["von", "Bart", "um\u00b7wallt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Schwebt des sechsten Heinrichs greise, gram-", "tokens": ["Schwebt", "des", "sechs\u00b7ten", "Hein\u00b7richs", "grei\u00b7se", ",", "gram"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN", "VVFIN", "$,", "TRUNC"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "verwitterte Gestalt,", "tokens": ["ver\u00b7wit\u00b7ter\u00b7te", "Ge\u00b7stalt", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Lady Gray dann, mit den S\u00f6hnen K\u00f6nig Edwards", "tokens": ["La\u00b7dy", "Gray", "dann", ",", "mit", "den", "S\u00f6h\u00b7nen", "K\u00f6\u00b7nig", "Ed\u00b7wards"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "ADV", "$,", "APPR", "ART", "NN", "NE", "NE"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.6": {"text": "an der Hand; \u2014 \u2014", "tokens": ["an", "der", "Hand", ";"], "token_info": ["word", "word", "word", "punct", "punct", "punct"], "pos": ["APPR", "ART", "NN", "$.", "$(", "$("], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Leise rauscht der Anna Bulen langes seidenes", "tokens": ["Lei\u00b7se", "rauscht", "der", "An\u00b7na", "Bu\u00b7len", "lan\u00b7ges", "sei\u00b7de\u00b7nes"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NE", "NE", "ADJA", "PPOSAT"], "meter": "+-+-+-+-+-+--", "measure": "unknown.measure.hexa"}, "line.8": {"text": "Gewand.", "tokens": ["Ge\u00b7wand", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "-+", "measure": "iambic.single"}}, "stanza.3": {"line.1": {"text": "Zahllos ist das Heer der Geister, das hinauf \u2014", "tokens": ["Zahl\u00b7los", "ist", "das", "Heer", "der", "Geis\u00b7ter", ",", "das", "hin\u00b7auf"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "ART", "NN", "$,", "PRELS", "ADV", "$("], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "hinunter schwebt,", "tokens": ["hin\u00b7un\u00b7ter", "schwebt", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Das da murmelt: \u201eFluch Dir Tower, dran das", "tokens": ["Das", "da", "mur\u00b7melt", ":", "\u201e", "Fluch", "Dir", "To\u00b7wer", ",", "dran", "das"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["PDS", "ADV", "VVFIN", "$.", "$(", "NN", "PPER", "NN", "$,", "PAV", "ART"], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Blut der Unschuld klebt;", "tokens": ["Blut", "der", "Un\u00b7schuld", "klebt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Schutt und Tr\u00fcmmer sollst Du werden!\u201c aber", "tokens": ["Schutt", "und", "Tr\u00fcm\u00b7mer", "sollst", "Du", "wer\u00b7den", "!", "\u201c", "a\u00b7ber"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "word"], "pos": ["ADJD", "KON", "NN", "VMFIN", "PPER", "VAINF", "$.", "$(", "ADV"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "machtlos ist ihr Fluch,", "tokens": ["macht\u00b7los", "ist", "ihr", "Fluch", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Ehern h\u00e4lt den Bau zusammen b\u00f6ser M\u00e4chte", "tokens": ["E\u00b7hern", "h\u00e4lt", "den", "Bau", "zu\u00b7sam\u00b7men", "b\u00f6\u00b7ser", "M\u00e4ch\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN", "ADV", "ADJA", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.8": {"text": "Zauberspruch.", "tokens": ["Zau\u00b7ber\u00b7spruch", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.4": {"line.1": {"text": "Wieder nachtet\u2019s, wieder ziehn sie durch die", "tokens": ["Wie\u00b7der", "nach\u00b7tet's", ",", "wie\u00b7der", "ziehn", "sie", "durch", "die"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "APPR", "ART"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.2": {"text": "R\u00e4ume still und weit,", "tokens": ["R\u00e4u\u00b7me", "still", "und", "weit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "KON", "ADJD", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Pl\u00f6tzlich stockt der Zug und schaart sich um ein", "tokens": ["Pl\u00f6tz\u00b7lich", "stockt", "der", "Zug", "und", "schaart", "sich", "um", "ein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "NN", "KON", "VVFIN", "PRF", "APPR", "ART"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "glimmend Tannenscheit,", "tokens": ["glim\u00b7mend", "Tan\u00b7nen\u00b7scheit", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Dann gesch\u00e4ftig, wie die Bienen, tragen Schnitz-", "tokens": ["Dann", "ge\u00b7sch\u00e4f\u00b7tig", ",", "wie", "die", "Bie\u00b7nen", ",", "tra\u00b7gen", "Schnitz"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ADJD", "$,", "PWAV", "ART", "NN", "$,", "VVFIN", "TRUNC"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.6": {"text": "werk sie herzu,", "tokens": ["werk", "sie", "her\u00b7zu", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Und zur hellen Flamme sch\u00fcren sie die matte", "tokens": ["Und", "zur", "hel\u00b7len", "Flam\u00b7me", "sch\u00fc\u00b7ren", "sie", "die", "mat\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "ADJA", "NN", "VVFIN", "PPER", "ART", "ADJA"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.8": {"text": "Gluth im Nu.", "tokens": ["Gluth", "im", "Nu", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "ADV", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.5": {"line.1": {"text": "Wie das prasselt, wie das flackert! einen spr\u00fchn-", "tokens": ["Wie", "das", "pras\u00b7selt", ",", "wie", "das", "fla\u00b7ckert", "!", "ei\u00b7nen", "spr\u00fchn"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "PDS", "VVFIN", "$,", "PWAV", "PDS", "VVFIN", "$.", "ART", "TRUNC"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "den Feuerbrand", "tokens": ["den", "Feu\u00b7er\u00b7brand"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Nehmen sie zum n\u00e4chtgen Umzug jetzt als Fackel", "tokens": ["Neh\u00b7men", "sie", "zum", "n\u00e4cht\u00b7gen", "Um\u00b7zug", "jetzt", "als", "Fa\u00b7ckel"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPRART", "ADJA", "NN", "ADV", "KOUS", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.4": {"text": "in die Hand,", "tokens": ["in", "die", "Hand", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Weithin wird die Saat der Funken in den Zim-", "tokens": ["Weit\u00b7hin", "wird", "die", "Saat", "der", "Fun\u00b7ken", "in", "den", "Zim"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "ART", "NN", "APPR", "ART", "TRUNC"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.6": {"text": "mern ausgestreut,", "tokens": ["mern", "aus\u00b7ge\u00b7streut", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVPP", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Flammen sollen draus erwachsen; hei, der Fluch", "tokens": ["Flam\u00b7men", "sol\u00b7len", "draus", "er\u00b7wach\u00b7sen", ";", "hei", ",", "der", "Fluch"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["NN", "VMFIN", "PAV", "VVINF", "$.", "PTKVZ", "$,", "ART", "NN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.8": {"text": "erf\u00fcllt sich heut!", "tokens": ["er\u00b7f\u00fcllt", "sich", "heut", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Alles schl\u00e4ft; doch auf vom Lager springt im Nu", "tokens": ["Al\u00b7les", "schl\u00e4ft", ";", "doch", "auf", "vom", "La\u00b7ger", "springt", "im", "Nu"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "$.", "ADV", "APPR", "APPRART", "NN", "VVFIN", "APPRART", "NN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "der rasche Sturm,", "tokens": ["der", "ra\u00b7sche", "Sturm", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Und er wirft sich in das Feuer, und das Feuer", "tokens": ["Und", "er", "wirft", "sich", "in", "das", "Feu\u00b7er", ",", "und", "das", "Feu\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PRF", "APPR", "ART", "NN", "$,", "KON", "ART", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.4": {"text": "in den Thurm,", "tokens": ["in", "den", "Thurm", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "An des Towers Felsenw\u00e4nde peitscht er schon", "tokens": ["An", "des", "To\u00b7wers", "Fel\u00b7sen\u00b7w\u00e4n\u00b7de", "peitscht", "er", "schon"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN", "VVFIN", "PPER", "ADV"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.6": {"text": "das Flammenmeer,", "tokens": ["das", "Flam\u00b7men\u00b7meer", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Und den Segen dr\u00fcber sprechend, wogt auf ihm", "tokens": ["Und", "den", "Se\u00b7gen", "dr\u00fc\u00b7ber", "spre\u00b7chend", ",", "wogt", "auf", "ihm"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ART", "NN", "PAV", "VVPP", "$,", "VVFIN", "APPR", "PPER"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.8": {"text": "das Geisterheer.", "tokens": ["das", "Geis\u00b7ter\u00b7heer", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.7": {"line.1": {"text": "Doch, als ob das Salz der Thr\u00e4nen feuerfest", "tokens": ["Doch", ",", "als", "ob", "das", "Salz", "der", "Thr\u00e4\u00b7nen", "feu\u00b7er\u00b7fest"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "$,", "KOKOM", "KOUS", "ART", "NN", "ART", "NN", "VVFIN"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "die W\u00e4nde macht,", "tokens": ["die", "W\u00e4n\u00b7de", "macht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Wie wenn Blut der beste M\u00f6rtel, den ein Mei-", "tokens": ["Wie", "wenn", "Blut", "der", "bes\u00b7te", "M\u00f6r\u00b7tel", ",", "den", "ein", "Mei"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOKOM", "KOUS", "NN", "ART", "ADJA", "NN", "$,", "PRELS", "ART", "TRUNC"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "ster je erdacht, \u2014", "tokens": ["ster", "je", "er\u00b7dacht", ","], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADV", "VVPP", "$,", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Seht, wie durstig auch die Flamme sich von Thurm", "tokens": ["Seht", ",", "wie", "durs\u00b7tig", "auch", "die", "Flam\u00b7me", "sich", "von", "Thurm"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "PWAV", "ADJD", "ADV", "ART", "NN", "PRF", "APPR", "NN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.6": {"text": "zu Thurme wirft,", "tokens": ["zu", "Thur\u00b7me", "wirft", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Hat sie doch, als w\u00e4ren\u2019s Becher, nur den In-", "tokens": ["Hat", "sie", "doch", ",", "als", "w\u00e4\u00b7ren's", "Be\u00b7cher", ",", "nur", "den", "In"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "$,", "KOUS", "ADJA", "NN", "$,", "ADV", "ART", "TRUNC"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.8": {"text": "halt ausgeschl\u00fcrft.", "tokens": ["halt", "aus\u00b7ge\u00b7schl\u00fcrft", "."], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.8": {"line.1": {"text": "Wieder, wenn es Nacht geworden, wenn\u2019s im", "tokens": ["Wie\u00b7der", ",", "wenn", "es", "Nacht", "ge\u00b7wor\u00b7den", ",", "wenn's", "im"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "$,", "KOUS", "PPER", "NN", "VAPP", "$,", "KOUS", "APPRART"], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Tower leer und stumm,", "tokens": ["To\u00b7wer", "leer", "und", "stumm", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Gehn die Geister der Erschlagnen in den Corri-", "tokens": ["Gehn", "die", "Geis\u00b7ter", "der", "Er\u00b7schlag\u00b7nen", "in", "den", "Cor\u00b7ri"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "ART", "NN", "APPR", "ART", "TRUNC"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.4": {"text": "doren um,", "tokens": ["do\u00b7ren", "um", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "PTKVZ", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Durch die L\u00fcfte bebt Gefl\u00fcster klagend dann,", "tokens": ["Durch", "die", "L\u00fcf\u00b7te", "bebt", "Ge\u00b7fl\u00fcs\u00b7ter", "kla\u00b7gend", "dann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "NN", "VVPP", "ADV", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.6": {"text": "wie Herbsteswehn,", "tokens": ["wie", "Herbs\u00b7tes\u00b7wehn", ","], "token_info": ["word", "word", "punct"], "pos": ["PWAV", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Mancher wird im Mondenschimmer noch die", "tokens": ["Man\u00b7cher", "wird", "im", "Mon\u00b7den\u00b7schim\u00b7mer", "noch", "die"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VAFIN", "APPRART", "NN", "ADV", "ART"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Schatten schreiten sehn.", "tokens": ["Schat\u00b7ten", "schrei\u00b7ten", "sehn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "VVINF", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}