{"dta.poem.2829": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "30.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Auswendig lernen sei, mein Sohn, dir eine Pflicht;", "tokens": ["Aus\u00b7wen\u00b7dig", "ler\u00b7nen", "sei", ",", "mein", "Sohn", ",", "dir", "ei\u00b7ne", "Pflicht", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VVINF", "VAFIN", "$,", "PPOSAT", "NN", "$,", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vers\u00e4ume nur dabei inwendig lernen nicht.", "tokens": ["Ver\u00b7s\u00e4u\u00b7me", "nur", "da\u00b7bei", "in\u00b7wen\u00b7dig", "ler\u00b7nen", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PAV", "ADJD", "VVFIN", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Auswendig ist gelernt, was dir vom Munde flie\u00dft,", "tokens": ["Aus\u00b7wen\u00b7dig", "ist", "ge\u00b7lernt", ",", "was", "dir", "vom", "Mun\u00b7de", "flie\u00dft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "VVPP", "$,", "PWS", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Inwendig, was im Sinn lebendig sich erschlie\u00dft.", "tokens": ["In\u00b7wen\u00b7dig", ",", "was", "im", "Sinn", "le\u00b7ben\u00b7dig", "sich", "er\u00b7schlie\u00dft", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PRELS", "APPRART", "NN", "ADJD", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure