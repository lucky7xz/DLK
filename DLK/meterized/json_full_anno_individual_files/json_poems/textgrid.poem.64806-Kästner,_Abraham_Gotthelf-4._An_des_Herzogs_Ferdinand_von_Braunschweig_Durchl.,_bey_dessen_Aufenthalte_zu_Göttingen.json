{"textgrid.poem.64806": {"metadata": {"author": {"name": "K\u00e4stner, Abraham Gotthelf", "birth": "N.A.", "death": "N.A."}, "title": "4. An des Herzogs Ferdinand von Braunschweig Durchl., bey dessen Aufenthalte zu G\u00f6ttingen", "genre": "verse", "period": "N.A.", "pub_year": 1759, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Herr!", "tokens": ["Herr", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}, "line.2": {"text": "Oefters hat man hier schon Deinen Ruhm geh\u00f6rt,", "tokens": ["Oef\u00b7ters", "hat", "man", "hier", "schon", "Dei\u00b7nen", "Ruhm", "ge\u00b7h\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "ADV", "ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "Nicht den die Zeitung schreibt, den ein Professor lehrt;", "tokens": ["Nicht", "den", "die", "Zei\u00b7tung", "schreibt", ",", "den", "ein", "Pro\u00b7fes\u00b7sor", "lehrt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "ART", "NN", "VVFIN", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Nein, voll Gef\u00fchl von Dir, voll Angst in allen Z\u00fcgen", "tokens": ["Nein", ",", "voll", "Ge\u00b7f\u00fchl", "von", "Dir", ",", "voll", "Angst", "in", "al\u00b7len", "Z\u00fc\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "ADJD", "NN", "APPR", "PPER", "$,", "ADJD", "NN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Erz\u00e4hlt ihn manches Heer, entflohn von Deinen Siegen.", "tokens": ["Er\u00b7z\u00e4hlt", "ihn", "man\u00b7ches", "Heer", ",", "ent\u00b7flohn", "von", "Dei\u00b7nen", "Sie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIAT", "NN", "$,", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Noch mehr hat uns von Dir der Gallier gesagt;", "tokens": ["Noch", "mehr", "hat", "uns", "von", "Dir", "der", "Gal\u00b7lier", "ge\u00b7sagt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "APPR", "PPER", "ART", "NN", "VVPP", "$."], "meter": "--+--+-+--+", "measure": "anapaest.di.plus"}, "line.7": {"text": "Gefangen, Dich geliebt, gewaffnet, nur gezagt.", "tokens": ["Ge\u00b7fan\u00b7gen", ",", "Dich", "ge\u00b7liebt", ",", "ge\u00b7waff\u00b7net", ",", "nur", "ge\u00b7zagt", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VVPP", "$,", "VVPP", "$,", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Doch f\u00fcr der Feinde Lob, das Macht und Huld erzwingen,", "tokens": ["Doch", "f\u00fcr", "der", "Fein\u00b7de", "Lob", ",", "das", "Macht", "und", "Huld", "er\u00b7zwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "NN", "$,", "ART", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "W\u00e4hlt froh dein g\u00fctig Herz den Dank, den B\u00fcrger bringen;", "tokens": ["W\u00e4hlt", "froh", "dein", "g\u00fc\u00b7tig", "Herz", "den", "Dank", ",", "den", "B\u00fcr\u00b7ger", "brin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPOSAT", "ADJD", "NN", "ART", "NN", "$,", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Genie\u00dft mit reiner Lust, von Mitleid unverg\u00e4llt,", "tokens": ["Ge\u00b7nie\u00dft", "mit", "rei\u00b7ner", "Lust", ",", "von", "Mit\u00b7leid", "un\u00b7ver\u00b7g\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$,", "APPR", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Das Gl\u00fcck Germaniens, durch Dich nun hergestellt;", "tokens": ["Das", "Gl\u00fcck", "Ger\u00b7ma\u00b7ni\u00b7ens", ",", "durch", "Dich", "nun", "her\u00b7ge\u00b7stellt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "APPR", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und merkt auf unsern Flei\u00df!", "tokens": ["Und", "merkt", "auf", "un\u00b7sern", "Flei\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "So hat Dein m\u00e4chtig Loben,", "tokens": ["So", "hat", "Dein", "m\u00e4ch\u00b7tig", "Lo\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "ADJD", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "O Herr, bey ", "tokens": ["O", "Herr", ",", "bey"], "token_info": ["word", "word", "punct", "word"], "pos": ["NE", "NN", "$,", "APPR"], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.3": {"line.1": {"text": "Herr!", "tokens": ["Herr", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure":