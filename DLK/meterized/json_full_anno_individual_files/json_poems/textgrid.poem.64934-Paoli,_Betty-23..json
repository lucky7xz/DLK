{"textgrid.poem.64934": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "23.", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ob l\u00e4ngst begraben sei, sie die mich hat geboren,", "tokens": ["Ob", "l\u00e4ngst", "be\u00b7gra\u00b7ben", "sei", ",", "sie", "die", "mich", "hat", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "VVPP", "VAFIN", "$,", "PPER", "ART", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Entr\u00fcckt nur ist sie mir, doch ewig unverloren;", "tokens": ["Ent\u00b7r\u00fcckt", "nur", "ist", "sie", "mir", ",", "doch", "e\u00b7wig", "un\u00b7ver\u00b7lo\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "VAFIN", "PPER", "PPER", "$,", "ADV", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Denn ewig lebet ja in mir der Liebe Geist,", "tokens": ["Denn", "e\u00b7wig", "le\u00b7bet", "ja", "in", "mir", "der", "Lie\u00b7be", "Geist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ADV", "APPR", "PPER", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der segnend ihrer denkt und fromm ger\u00fchrt sie preis't.", "tokens": ["Der", "seg\u00b7nend", "ih\u00b7rer", "denkt", "und", "fromm", "ge\u00b7r\u00fchrt", "sie", "preis'", "t."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["ART", "ADJD", "PPOSAT", "VVFIN", "KON", "ADJD", "VVPP", "PPER", "VVFIN", "NE"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Und nimmer, nimmer ist ganz aus dem Sein geschieden,", "tokens": ["Und", "nim\u00b7mer", ",", "nim\u00b7mer", "ist", "ganz", "aus", "dem", "Sein", "ge\u00b7schie\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "ADV", "VAFIN", "ADV", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+++-+-+-", "measure": "unknown.measure.septa"}, "line.2": {"text": "F\u00fcr den ein einzig Herz noch bebend schl\u00e4gt hienieden.", "tokens": ["F\u00fcr", "den", "ein", "ein\u00b7zig", "Herz", "noch", "be\u00b7bend", "schl\u00e4gt", "hien\u00b7ie\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "ADJD", "NN", "ADV", "ADJD", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Darum beirrt mich nicht des Erdentodes Schein:", "tokens": ["Da\u00b7rum", "be\u00b7irrt", "mich", "nicht", "des", "Er\u00b7den\u00b7to\u00b7des", "Schein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PTKNEG", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gestorben ist sie nicht, so lang noch Leben mein.", "tokens": ["Ge\u00b7stor\u00b7ben", "ist", "sie", "nicht", ",", "so", "lang", "noch", "Le\u00b7ben", "mein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "PTKNEG", "$,", "ADV", "ADJD", "ADV", "NN", "PPOSAT", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Ob l\u00e4ngst begraben sei, sie die mich hat geboren,", "tokens": ["Ob", "l\u00e4ngst", "be\u0