": ["PPER", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Jahrtausende sind ihm zu jung.", "tokens": ["Jahr\u00b7tau\u00b7sen\u00b7de", "sind", "ihm", "zu", "jung", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "PTKA", "ADJD", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.5": {"line.1": {"text": "Weit schreiten werden, welche lange sa\u00dfen", "tokens": ["Weit", "schrei\u00b7ten", "wer\u00b7den", ",", "wel\u00b7che", "lan\u00b7ge", "sa\u00b7\u00dfen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "VAINF", "$,", "PRELS", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "in ihrer tiefen D\u00e4mmerung.", "tokens": ["in", "ih\u00b7rer", "tie\u00b7fen", "D\u00e4m\u00b7me\u00b7rung", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}}, "stanza.6": {"line.1": {"text": "Das war in Tagen, da die Berge kamen:", "tokens": ["Das", "war", "in", "Ta\u00b7gen", ",", "da", "die", "Ber\u00b7ge", "ka\u00b7men", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "NN", "$,", "KOUS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "die B\u00e4ume b\u00e4umten sich, die noch nicht zahmen,", "tokens": ["die", "B\u00e4u\u00b7me", "b\u00e4um\u00b7ten", "sich", ",", "die", "noch", "nicht", "zah\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "$,", "PRELS", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "und rauschend in die R\u00fcstung s