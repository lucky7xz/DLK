{"textgrid.poem.55875": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das war in Tagen, da die Berge kamen:", "tokens": ["Das", "war", "in", "Ta\u00b7gen", ",", "da", "die", "Ber\u00b7ge", "ka\u00b7men", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "NN", "$,", "KOUS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "die B\u00e4ume b\u00e4umten sich, die noch nicht zahmen,", "tokens": ["die", "B\u00e4u\u00b7me", "b\u00e4um\u00b7ten", "sich", ",", "die", "noch", "nicht", "zah\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "$,", "PRELS", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "und rauschend in die R\u00fcstung stieg der Strom.", "tokens": ["und", "rau\u00b7schend", "in", "die", "R\u00fcs\u00b7tung", "stieg", "der", "Strom", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Zwei fremde Pilger riefen einen Namen,", "tokens": ["Zwei", "frem\u00b7de", "Pil\u00b7ger", "rie\u00b7fen", "ei\u00b7nen", "Na\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "ADJA", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "und aufgewacht aus seinem langen Lahmen", "tokens": ["und", "auf\u00b7ge\u00b7wacht", "aus", "sei\u00b7nem", "lan\u00b7gen", "Lah\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "war Ilija, der Riese von Murom.", "tokens": ["war", "I\u00b7li\u00b7ja", ",", "der", "Rie\u00b7se", "von", "Mu\u00b7rom", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NE", "$,", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Die alten Eltern brachen in den \u00c4ckern", "tokens": ["Die", "al\u00b7ten", "El\u00b7tern", "bra\u00b7chen", "in", "den", "\u00c4\u00b7ckern"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "an Steinen ab und an dem wilden Wuchs;", "tokens": ["an", "Stei\u00b7nen", "ab", "und", "an", "dem", "wil\u00b7den", "Wuchs", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKVZ", "KON", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "da kam der Sohn, ganz gro\u00df, von seinen Weckern", "tokens": ["da", "kam", "der", "Sohn", ",", "ganz", "gro\u00df", ",", "von", "sei\u00b7nen", "We\u00b7ckern"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "ADV", "ADJD", "$,", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "und zwang die Furchen in die Furcht des Pflugs.", "tokens": ["und", "zwang", "die", "Fur\u00b7chen", "in", "die", "Furcht", "des", "Pflugs", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Er hob die St\u00e4mme, die wie Streiter standen,", "tokens": ["Er", "hob", "die", "St\u00e4m\u00b7me", ",", "die", "wie", "Strei\u00b7ter", "stan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PRELS", "KOKOM", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "und lachte ihres wankenden Gewichts,", "tokens": ["und", "lach\u00b7te", "ih\u00b7res", "wan\u00b7ken\u00b7den", "Ge\u00b7wichts", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "und aufgest\u00f6rt wie schwarze Schlangen wanden", "tokens": ["und", "auf\u00b7ge\u00b7st\u00f6rt", "wie", "schwar\u00b7ze", "Schlan\u00b7gen", "wan\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "KOKOM", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "die Wurzeln, welche nur das Dunkel kannten,", "tokens": ["die", "Wur\u00b7zeln", ",", "wel\u00b7che", "nur", "das", "Dun\u00b7kel", "kann\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "sich in dem breiten Griff des Lichts.", "tokens": ["sich", "in", "dem", "brei\u00b7ten", "Griff", "des", "Lichts", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Es st\u00e4rkte sich im fr\u00fchen Tau die M\u00e4hre,", "tokens": ["Es", "st\u00e4rk\u00b7te", "sich", "im", "fr\u00fc\u00b7hen", "Tau", "die", "M\u00e4h\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPRART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "in deren Adern Kraft und Adel schlief;", "tokens": ["in", "de\u00b7ren", "A\u00b7dern", "Kraft", "und", "A\u00b7del", "schlief", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "sie reifte unter ihres Reiters Schwere,", "tokens": ["sie", "reif\u00b7te", "un\u00b7ter", "ih\u00b7res", "Rei\u00b7ters", "Schwe\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "ihr Wiehern war wie eine Stimme tief, \u2013", "tokens": ["ihr", "Wie\u00b7hern", "war", "wie", "ei\u00b7ne", "Stim\u00b7me", "tief", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "KOKOM", "ART", "NN", "ADJD", "$,", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "und beide f\u00fchlten, wie das Ungef\u00e4hre", "tokens": ["und", "bei\u00b7de", "f\u00fchl\u00b7ten", ",", "wie", "das", "Un\u00b7ge\u00b7f\u00e4h\u00b7re"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PIS", "VVFIN", "$,", "PWAV", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "sie mit verhei\u00dfenden Gefahren rief.", "tokens": ["sie", "mit", "ver\u00b7hei\u00b7\u00dfen\u00b7den", "Ge\u00b7fah\u00b7ren", "rief", "."], "token_info": ["word", "word", "word", "word", "wo