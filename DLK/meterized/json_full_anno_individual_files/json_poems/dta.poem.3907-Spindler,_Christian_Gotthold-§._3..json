{"dta.poem.3907": {"metadata": {"author": {"name": "Spindler, Christian Gotthold", "birth": "N.A.", "death": "N.A."}, "title": "\u00a7. 3.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-20581-9", "language": ["de:0.99"], "booktitle": "Spindler, Christian Gotthold: Unschuldige Jugend-Fr\u00fcchte. Leipzig, 1745."}, "poem": {"stanza.1": {"line.1": {"text": "Zeigt sie von ihrer Gunst ie manchmahl kurtze", "tokens": ["Zeigt", "sie", "von", "ih\u00b7rer", "Gunst", "ie", "manch\u00b7mahl", "kurt\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "ADV", "ADV", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Proben,", "tokens": ["Pro\u00b7ben", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Und solte sie an ihm bald di\u00df bald jenes loben:", "tokens": ["Und", "sol\u00b7te", "sie", "an", "ihm", "bald", "di\u00df", "bald", "je\u00b7nes", "lo\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "APPR", "PPER", "ADV", "PDS", "ADV", "PDS", "VVINF", "$."], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Er w\u00e4re an Person und Reden sehr geschickt,", "tokens": ["Er", "w\u00e4\u00b7re", "an", "Per\u00b7son", "und", "Re\u00b7den", "sehr", "ge\u00b7schickt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "KON", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So werden ihr daf\u00fcr die H\u00e4nde sanfft gedr\u00fcckt.", "tokens": ["So", "wer\u00b7den", "ihr", "da\u00b7f\u00fcr", "die", "H\u00e4n\u00b7de", "sanfft", "ge\u00b7dr\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PAV", "ART", "NN", "VVFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}