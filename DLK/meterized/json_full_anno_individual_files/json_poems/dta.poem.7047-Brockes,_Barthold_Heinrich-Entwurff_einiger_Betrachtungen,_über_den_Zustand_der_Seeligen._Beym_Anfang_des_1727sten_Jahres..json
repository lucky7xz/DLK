{"dta.poem.7047": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Entwurff einiger Betrachtungen,  \n \u00fcber den  \n  Zustand der Seeligen.  \n Beym Anfang des 1727sten Jahres.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Elis\u00e4ischen, so s\u00fc\u00df beschriebnen Anen,", "tokens": ["Die", "E\u00b7li\u00b7s\u00e4\u00b7i\u00b7schen", ",", "so", "s\u00fc\u00df", "be\u00b7schrieb\u00b7nen", "A\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die wir in Heydnischer Poeten Schrifften schauen,", "tokens": ["Die", "wir", "in", "Heyd\u00b7ni\u00b7scher", "Po\u00b7et\u00b7en", "Schriff\u00b7ten", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "ADJA", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sind w\u00fcrcklich lieblicher, sehn angenehmer aus.", "tokens": ["Sind", "w\u00fcrck\u00b7lich", "lieb\u00b7li\u00b7cher", ",", "sehn", "an\u00b7ge\u00b7neh\u00b7mer", "aus", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "ADJA", "$,", "VVFIN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ach! da\u00df man sich doch nicht nach GOTTes Wort bem\u00fcht,", "tokens": ["Ach", "!", "da\u00df", "man", "sich", "doch", "nicht", "nach", "GoT\u00b7Tes", "Wort", "be\u00b7m\u00fcht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "KOUS", "PIS", "PRF", "ADV", "PTKNEG", "APPR", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Von GOTTES Majest\u00e4t was w\u00fcrdigers zu dencken!", "tokens": ["Von", "GoT\u00b7TES", "Ma\u00b7jes\u00b7t\u00e4t", "was", "w\u00fcr\u00b7di\u00b7gers", "zu", "den\u00b7cken", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "PWS", "VAFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ich will dann itzt von jenem seel\u2019gen Wesen,", "tokens": ["Ich", "will", "dann", "itzt", "von", "je\u00b7nem", "seel'\u00b7gen", "We\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "So, was ich in der Schrifft, und sonst davon gelesen,", "tokens": ["So", ",", "was", "ich", "in", "der", "Schrifft", ",", "und", "sonst", "da\u00b7von", "ge\u00b7le\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWS", "PPER", "APPR", "ART", "NN", "$,", "KON", "ADV", "PAV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Als auch, so weit sich meine Kr\u00e4ffte strecken,", "tokens": ["Als", "auch", ",", "so", "weit", "sich", "mei\u00b7ne", "Kr\u00e4ff\u00b7te", "stre\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "$,", "ADV", "ADJD", "PRF", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "In froher Demuth euch entdecken.", "tokens": ["In", "fro\u00b7her", "De\u00b7muth", "euch", "ent\u00b7de\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPER", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.10": {"text": "Doch ist die Meinung nicht, da\u00df das, was ich erzehle", "tokens": ["Doch", "ist", "die", "Mei\u00b7nung", "nicht", ",", "da\u00df", "das", ",", "was", "ich", "er\u00b7zeh\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VAFIN", "ART", "NN", "PTKNEG", "$,", "KOUS", "PDS", "$,", "PWS", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Nothwendig so, und zwar, da\u00df nichts dran fehle,", "tokens": ["Noth\u00b7wen\u00b7dig", "so", ",", "und", "zwar", ",", "da\u00df", "nichts", "dran", "feh\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "$,", "KON", "ADV", "$,", "KOUS", "PIS", "PAV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Dereinsten sich begeben m\u00fcsse.", "tokens": ["De\u00b7reins\u00b7ten", "sich", "be\u00b7ge\u00b7ben", "m\u00fcs\u00b7se", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Ach nein! ich mache nur,", "tokens": ["Ach", "nein", "!", "ich", "ma\u00b7che", "nur", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PTKANT", "$.", "PPER", "VVFIN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.14": {"text": "Nach einigen Gesetzen der Natur,", "tokens": ["Nach", "ei\u00b7ni\u00b7gen", "Ge\u00b7set\u00b7zen", "der", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ART", "NN", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.15": {"text": "Und Richtschnur der Vernunfft, gantz unma\u00dfgeblich Schl\u00fcsse.", "tokens": ["Und", "Richt\u00b7schnur", "der", "Ver\u00b7nunfft", ",", "gantz", "un\u00b7ma\u00df\u00b7geb\u00b7lich", "Schl\u00fcs\u00b7se", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ART", "NN", "$,", "ADV", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Das Unerschaffne Licht, der Sch\u00f6pffer, kann und wird", "tokens": ["Das", "Un\u00b7er\u00b7schaff\u00b7ne", "Licht", ",", "der", "Sch\u00f6pf\u00b7fer", ",", "kann", "und", "wird"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "$,", "VMFIN", "KON", "VAFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Ja \u00fcberschwenglich mehr, als wir gedencken,", "tokens": ["Ja", "\u00fc\u00b7bersc\u00b7hweng\u00b7lich", "mehr", ",", "als", "wir", "ge\u00b7den\u00b7cken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADJD", "ADV", "$,", "KOUS", "PPER", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Den Seelen, die er liebt, den seel\u2019gen Frommen scheucken.", "tokens": ["Den", "See\u00b7len", ",", "die", "er", "liebt", ",", "den", "seel'\u00b7gen", "From\u00b7men", "scheu\u00b7cken", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Aufs wenigst\u2019 hoff\u2019 ich nicht, da\u00df ich geirrt,", "tokens": ["Aufs", "we\u00b7nigst'", "hoff'", "ich", "nicht", ",", "da\u00df", "ich", "ge\u00b7irrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "PIS", "VVFIN", "PPER", "PTKNEG", "$,", "KOUS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Wenn ich zugleich daraus dir eine Probe gebe,", "tokens": ["Wenn", "ich", "zu\u00b7gleich", "da\u00b7raus", "dir", "ei\u00b7ne", "Pro\u00b7be", "ge\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PAV", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Da\u00df unser Geist zwar schwach, jedoch so sehr verwirrt", "tokens": ["Da\u00df", "un\u00b7ser", "Geist", "zwar", "schwach", ",", "je\u00b7doch", "so", "sehr", "ver\u00b7wirrt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "ADJD", "$,", "ADV", "ADV", "ADV", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Am Jrdischen nicht klebe:", "tokens": ["Am", "Jr\u00b7di\u00b7schen", "nicht", "kle\u00b7be", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.23": {"text": "Nein, da\u00df dem Menschen, mehr, als er gedencket,", "tokens": ["Nein", ",", "da\u00df", "dem", "Men\u00b7schen", ",", "mehr", ",", "als", "er", "ge\u00b7den\u00b7cket", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "KOUS", "ART", "NN", "$,", "ADV", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "Zum dencken F\u00e4higkeit geschencket,", "tokens": ["Zum", "den\u00b7cken", "F\u00e4\u00b7hig\u00b7keit", "ge\u00b7schen\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Wodurch sich denn zugleich noch ein Beweis entdecket", "tokens": ["Wo\u00b7durch", "sich", "denn", "zu\u00b7gleich", "noch", "ein", "Be\u00b7weis", "ent\u00b7de\u00b7cket"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PRF", "ADV", "ADV", "ADV", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Von unsrer Seelen Daur, der in der Folge stecket.", "tokens": ["Von", "uns\u00b7rer", "See\u00b7len", "Daur", ",", "der", "in", "der", "Fol\u00b7ge", "ste\u00b7cket", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Auf denn! mein Geist! auf! auf! spann\u2019 alle deine Kr\u00e4ffte,", "tokens": ["Auf", "denn", "!", "mein", "Geist", "!", "auf", "!", "auf", "!", "spann'", "al\u00b7le", "dei\u00b7ne", "Kr\u00e4ff\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "$.", "PPOSAT", "NN", "$.", "PTKVZ", "$.", "PTKVZ", "$.", "VVFIN", "PIS", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Zu diesem n\u00fctzlichen Gesch\u00e4ffte,", "tokens": ["Zu", "die\u00b7sem", "n\u00fctz\u00b7li\u00b7chen", "Ge\u00b7sch\u00e4ff\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.29": {"text": "Mit Lust und Andacht an!", "tokens": ["Mit", "Lust", "und", "An\u00b7dacht", "an", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}