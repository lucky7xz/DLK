{"textgrid.poem.37494": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Der Katzenjammer am Neujahrsmorgen", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Am Morgen nach Silvester", "tokens": ["Am", "Mor\u00b7gen", "nach", "Sil\u00b7ves\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Schmerz in den Kniegelenken", "tokens": ["Schmerz", "in", "den", "Knie\u00b7ge\u00b7len\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.3": {"line.1": {"text": "Gesteigerte Sensibilit\u00e4t der Haarspitzen, vulgo Haarweh", "tokens": ["Ge\u00b7stei\u00b7ger\u00b7te", "Sen\u00b7si\u00b7bi\u00b7li\u00b7t\u00e4t", "der", "Haar\u00b7spit\u00b7zen", ",", "vul\u00b7go", "Haar\u00b7weh"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "ART", "NN", "$,", "NE", "NN"], "meter": "-+--+-+-+-++-+-+-", "measure": "iambic.octa.plus.relaxed"}}, "stanza.4": {"line.1": {"text": "Wiederkehrendes Bewu\u00dftsein", "tokens": ["Wie\u00b7der\u00b7keh\u00b7ren\u00b7des", "Be\u00b7wu\u00df\u00b7tsein"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Subjektive Farbenerscheinung in Gestalt beweglicher Flecken", "tokens": ["Sub\u00b7jek\u00b7ti\u00b7ve", "Far\u00b7be\u00b7ner\u00b7schei\u00b7nung", "in", "Ge\u00b7stalt", "be\u00b7weg\u00b7li\u00b7cher", "Fle\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "APPR", "NN", "ADJA", "NN"], "meter": "-+--+--+-+-+-+--+-", "measure": "amphibrach.tri.plus"}}, "stanza.6": {"line.1": {"text": "Wo hab' ich denn das heut' nacht erwischt?!", "tokens": ["Wo", "hab'", "ich", "denn", "das", "heut'", "nacht", "er\u00b7wischt", "?!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.7": {"line.1": {"text": "Ohha? \u2013 Noch immer ein bissel wackelig?!", "tokens": ["Oh\u00b7ha", "?", "\u2013", "Noch", "im\u00b7mer", "ein", "bis\u00b7sel", "wa\u00b7cke\u00b7lig", "?!"], "token_info": ["word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "$(", "ADV", "ADV", "ART", "ADV", "ADJD", "$."], "meter": "-+-+--+-+--", "measure": "iambic.tetra.relaxed"}}, "stanza.8": {"line.1": {"text": "Auch zuwider!!", "tokens": ["Auch", "zu\u00b7wi\u00b7der", "!!"], "token_info": ["word", "word", "punct"], "pos": ["ADV", "ADJD", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.9": {"line.1": {"text": "Wo im Dunkeln die Uhr hingelegt wurde", "tokens": ["Wo", "im", "Dun\u00b7keln", "die", "Uhr", "hin\u00b7ge\u00b7legt", "wur\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPRART", "NN", "ART", "NN", "VVPP", "VAFIN"], "meter": "--+--++--+-", "measure": "anapaest.di.plus"}}, "stanza.10": {"line.1": {"text": "O weh! Der neue Zylinder im Waschbecken.", "tokens": ["O", "weh", "!", "Der", "neu\u00b7e", "Zy\u00b7lin\u00b7der", "im", "Waschbe\u00b7cken", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$.", "ART", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.11": {"line.1": {"text": "Das Geld ist auch fort.", "tokens": ["Das", "Geld", "ist", "auch", "fort", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "PTKVZ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.12": {"line.1": {"text": "Doch finden sich noch drei Kreuzer im Stiefel.", "tokens": ["Doch", "fin\u00b7den", "sich", "noch", "drei", "Kreu\u00b7zer", "im", "Stie\u00b7fel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADV", "CARD", "NN", "APPRART", "NN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.13": {"line.1": {"text": "Ein Magenbitter", "tokens": ["Ein", "Ma\u00b7gen\u00b7bit\u00b7ter"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.14": {"line.1": {"text": "Brrr!", "tokens": ["Brrr", "!"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+", "measure": "single.up"}}, "stanza.15": {"line.1": {"text": "Nach dieser heilsamen Ersch\u00fctterung geht's ja soweit wieder ganz gut.", "tokens": ["Nach", "die\u00b7ser", "heil\u00b7sa\u00b7men", "Er\u00b7sch\u00fct\u00b7te\u00b7rung", "geht's", "ja", "so\u00b7weit", "wie\u00b7der", "ganz", "gut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "VVFIN", "ADV", "ADV", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+-+--++-+-+-+", "measure": "iambic.octa.plus.relaxed"}}, "stanza.16": {"line.1": {"text": "Am Morgen nach Silvester", "tokens": ["Am", "Mor\u00b7gen", "nach", "Sil\u00b7ves\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.17": {"line.1": {"text": "Schmerz in den Kniegelenken", "tokens": ["Schmerz", "in", "den", "Knie\u00b7ge\u00b7len\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.18": {"line.1": {"text": "Gesteigerte Sensibilit\u00e4t der Haarspitzen,