{"textgrid.poem.58710": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "4.", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihre Hoffart zeugt wider sie ins Angesicht. Hof. 7, 10", "tokens": ["Ih\u00b7re", "Hof\u00b7fart", "zeugt", "wi\u00b7der", "sie", "ins", "An\u00b7ge\u00b7sicht", ".", "Hof", ".", "7", ",", "10"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct", "number", "punct", "number"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "PPER", "APPRART", "NN", "$.", "NN", "$.", "CARD", "$,", "CARD"], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Der h\u00f6chsten Gotteslieb' ist nichts geh\u00e4ssiger,", "tokens": ["Der", "h\u00f6chs\u00b7ten", "Got\u00b7tes\u00b7lieb'", "ist", "nichts", "ge\u00b7h\u00e4s\u00b7si\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PIS", "ADJA", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als wenn man eiteln Hochmut heget;", "tokens": ["Als", "wenn", "man", "ei\u00b7teln", "Hoch\u00b7mut", "he\u00b7get", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PIS", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der Freund und die er liebt, bestreiten \u2013 sie und er \u2013", "tokens": ["Der", "Freund", "und", "die", "er", "liebt", ",", "be\u00b7strei\u00b7ten", "\u2013", "sie", "und", "er", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "PRELS", "PPER", "VVFIN", "$,", "VVFIN", "$(", "PPER", "KON", "PPER", "$("], "meter": "-+-+-+-+-++-", "measure": "unknown.measure.hexa"}, "line.4": {"text": "Den Stolz, bis er ist ganz erleget.", "tokens": ["Den", "Stolz", ",", "bis", "er", "ist", "ganz", "er\u00b7le\u00b7get", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "PPER", "VAFIN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Nein, du gef\u00e4llst Gott nie in der Erhabenheit,", "tokens": ["Nein", ",", "du", "ge\u00b7f\u00e4llst", "Gott", "nie", "in", "der", "Er\u00b7ha\u00b7ben\u00b7heit", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VVFIN", "NN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Demut ihn zur Seele lenket;", "tokens": ["Die", "De\u00b7mut", "ihn", "zur", "See\u00b7le", "len\u00b7ket", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Willst du empfinden rein der Liebe S\u00fc\u00dfigkeit,", "tokens": ["Willst", "du", "emp\u00b7fin\u00b7den", "rein", "der", "Lie\u00b7be", "S\u00fc\u00b7\u00dfig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVFIN", "ADJD", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dann bleibe klein, gebeugt, ersenket!", "tokens": ["Dann", "blei\u00b7be", "klein", ",", "ge\u00b7beugt", ",", "er\u00b7sen\u00b7ket", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "$,", "VVPP", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Es h\u