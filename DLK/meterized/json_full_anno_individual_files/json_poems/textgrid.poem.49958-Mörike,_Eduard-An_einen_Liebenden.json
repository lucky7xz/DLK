{"textgrid.poem.49958": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "An einen Liebenden", "genre": "verse", "period": "N.A.", "pub_year": 1853, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du klagst mir, Freund, da\u00df immer die Mutter noch", "tokens": ["Du", "klagst", "mir", ",", "Freund", ",", "da\u00df", "im\u00b7mer", "die", "Mut\u00b7ter", "noch"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$,", "NN", "$,", "KOUS", "ADV", "ART", "NN", "ADV"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Des sch\u00f6nen Kindes gleich unerbittlich sei.", "tokens": ["Des", "sch\u00f6\u00b7nen", "Kin\u00b7des", "gleich", "un\u00b7er\u00b7bitt\u00b7lich", "sei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "ADJD", "VAFIN", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Geduld! noch leben wir im J\u00e4nner,", "tokens": ["Ge\u00b7duld", "!", "noch", "le\u00b7ben", "wir", "im", "J\u00e4n\u00b7ner", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ADV", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Aber nicht stets wird der Eiswind schnauben.", "tokens": ["A\u00b7ber", "nicht", "stets", "wird", "der", "Eis\u00b7wind", "schnau\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ADV", "VAFIN", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Im Winkel, wo sich einsam des Daches Trauf", "tokens": ["Im", "Win\u00b7kel", ",", "wo", "sich", "ein\u00b7sam", "des", "D