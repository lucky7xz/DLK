{"textgrid.poem.67897": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "23. Herz und Auge", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer noch nicht die b\u00f6se Zwietracht", "tokens": ["Wer", "noch", "nicht", "die", "b\u00f6\u00b7se", "Zwiet\u00b7racht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "PTKNEG", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zwischen Herz und Auge kennt,", "tokens": ["Zwi\u00b7schen", "Herz", "und", "Au\u00b7ge", "kennt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wei\u00df noch nicht, warum so th\u00f6richt", "tokens": ["Wei\u00df", "noch", "nicht", ",", "wa\u00b7rum", "so", "th\u00f6\u00b7richt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ADV", "PTKNEG", "$,", "PWAV", "ADV", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Oft er weinet, oft er brennt.", "tokens": ["Oft", "er", "wei\u00b7net", ",", "oft", "er", "brennt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "$,", "ADV", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Klagend spricht das Herz zum Auge:", "tokens": ["Kla\u00b7gend", "spricht", "das", "Herz", "zum", "Au\u00b7ge", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Du bist Schuld an meiner Pein,", "tokens": ["Du", "bist", "Schu