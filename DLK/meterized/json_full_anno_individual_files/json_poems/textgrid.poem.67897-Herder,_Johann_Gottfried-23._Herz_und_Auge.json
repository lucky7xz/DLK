: ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ohne da\u00df du mich gesandt?", "tokens": ["Oh\u00b7ne", "da\u00df", "du", "mich", "ge\u00b7sandt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PPER", "PRF", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "War ich je des Feindes Freundin,", "tokens": ["War", "ich", "je", "des", "Fein\u00b7des", "Freun\u00b7din", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ohne Winke deiner Hand?", "tokens": ["Oh\u00b7ne", "Win\u00b7ke", "dei\u00b7ner", "Hand", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.16": {"line.1": {"text": "Schlo\u00df ich nicht, wo du befahlest,", "tokens": ["Schlo\u00df", "ich", "nicht", ",", "wo", "du", "be\u00b7fah\u00b7lest", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PTKNEG", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mich dem liebsten Raube zu?", "tokens": ["Mich", "dem", "liebs\u00b7ten", "Rau\u00b7be", "zu", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Lie\u00df ich nicht zu tausendmalen", "tokens": ["Lie\u00df", "ich", "nicht", "zu", "tau\u00b7send\u00b7ma\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKNEG", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dir und du mir nimmer Ruh?", "tokens": ["Dir", "und", "du", "mir", "nim\u00b7mer", "Ruh", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "KON", "PPER", "PPER", "ADV", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.17": {"line.1": {"text": "Aus dem Herzen keimt die S\u00fcnde;", "tokens": ["Aus", "dem", "Her\u00b7zen", "keimt", "die", "S\u00fcn\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Auge bringt sie nicht hinein,", "tokens": ["Au\u00b7ge", "bringt", "sie", "nicht", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PTKNEG", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Du vergiftest meine Blicke,", "tokens": ["Du", "ver\u00b7gif\u00b7test", "mei\u00b7ne", "Bli\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Du bist Schuld an deiner Pein.", "tokens": ["Du", "bist", "Schuld", "an", "dei\u00b7ner", "Pein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.18": {"line.1": {"text": "Also streiten sie und beide", "tokens": ["Al\u00b7so", "strei\u00b7ten", "sie", "und", "bei\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "KON", "PIS"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "S\u00fcndigen in ihrem Streit.", "tokens": ["S\u00fcn\u00b7di\u00b7gen", "in", "ih\u00b7rem", "Streit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Herz, du bist des B\u00f6sen Quelle", "tokens": ["Herz", ",", "du", "bist", "des", "B\u00f6\u00b7sen", "Quel\u00b7le"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PPER", "VAFIN", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Auge, die Gelegenheit.", "tokens": ["Au\u00b7ge", ",", "die", "Ge\u00b7le\u00b7gen\u00b7heit", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}