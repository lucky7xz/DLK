{"textgrid.poem.30262": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "76.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Eine F\u00fcrstin starbe noch in bester Jugend,", "tokens": ["Ei\u00b7ne", "F\u00fcrs\u00b7tin", "star\u00b7be", "noch", "in", "bes\u00b7ter", "Ju\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "War wie an dem Stande F\u00fcrstin auch an Tugend;", "tokens": ["War", "wie", "an", "dem", "Stan\u00b7de", "F\u00fcrs\u00b7tin", "auch", "an", "Tu\u00b7gend", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "KOKOM", "APPR", "ART", "ADJA", "NN", "ADV", "APPR", "NN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "Ieder, der sie kante, obs gleich nichts gegolten,", "tokens": ["Ie\u00b7der", ",", "der", "sie", "kan\u00b7te", ",", "obs", "gleich", "nichts", "ge\u00b7gol\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "PPER", "VVFIN", "$,", "KOUS", "ADV", "PIS", "VVPP", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.4": {"text": "Hat de\u00df Todes Toben, dennoch sehr gescholten.", "tokens": ["Hat", "de\u00df", "To\u00b7des", "To\u00b7ben", ",", "den\u00b7noch", "sehr", "ge\u00b7schol\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "NN", "$,", "ADV", "ADV", "VVPP", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.5": {"text": "Einer klagte hefftig, das die Thr\u00