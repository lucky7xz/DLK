{"dta.poem.13289": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Z w\u00f6lfftes  B uch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein grosser Welt-Monarch war dorten vorgestellet,", "tokens": ["Ein", "gros\u00b7ser", "Welt\u00b7Mon\u00b7a\u00b7rch", "war", "dor\u00b7ten", "vor\u00b7ge\u00b7stel\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV", "VVFIN", "$,"], "meter": "-+-++-+-+-+-+-", "measure": "unknown.measure.septa"}, "line.2": {"text": "Wie wann ein solcher F\u00fcrst die Kriegs-Entschliessung f\u00e4llet.", "tokens": ["Wie", "wann", "ein", "sol\u00b7cher", "F\u00fcrst", "die", "Kriegs\u00b7Ent\u00b7schlies\u00b7sung", "f\u00e4l\u00b7let", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PWAV", "ART", "PIAT", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "65Ein Feder-reicher Hut, ein goldnes Mantel-Kleid,", "tokens": ["Fe\u00b7der\u00b7rei\u00b7cher", "Hut", ",", "ein", "gold\u00b7nes", "Man\u00b7tel\u00b7Kleid", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "Erhob des F\u00fcrstens Pracht zur h\u00f6chsten Herrlichkeit;", "tokens": ["Er\u00b7hob", "des", "F\u00fcrs\u00b7tens", "Pracht", "zur", "h\u00f6chs\u00b7ten", "Herr\u00b7lich\u00b7keit", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Er stund an einem Tisch, den Purpur-Sammet zierte,", "tokens": ["Er", "stund", "an", "ei\u00b7nem", "Tisch", ",", "den", "Pur\u00b7pur\u00b7Sam\u00b7met", "zier\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wie wann sein Geist, was er vorher entschlo\u00df, vollf\u00fchrte.", "tokens": ["Wie", "wann", "sein", "Geist", ",", "was", "er", "vor\u00b7her", "ent\u00b7schlo\u00df", ",", "voll\u00b7f\u00fchr\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "PWAV", "PPOSAT", "NN", "$,", "PWS", "PPER", "ADV", "VVFIN", "$,", "VVFIN", "$."], "meter": "-+-+--++-+-+-", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "Jhm war ein solcher Schmuck von Kronen beygelegt,", "tokens": ["Jhm", "war", "ein", "sol\u00b7cher", "Schmuck", "von", "Kro\u00b7nen", "bey\u00b7ge\u00b7legt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "PIAT", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "70Wie sie der h\u00f6chste F\u00fcrst der deutschen F\u00fcrsten tr\u00e4gt.", "tokens": ["sie", "der", "h\u00f6chs\u00b7te", "F\u00fcrst", "der", "deut\u00b7schen", "F\u00fcrs\u00b7ten", "tr\u00e4gt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.9": {"text": "Mein F\u00fchrer sprach: da\u00df ich das Bild betrachten sollte,", "tokens": ["Mein", "F\u00fch\u00b7rer", "sprach", ":", "da\u00df", "ich", "das", "Bild", "be\u00b7trach\u00b7ten", "soll\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$.", "KOUS", "PPER", "ART", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Doch wu\u00dft\u2019 ich nicht was er dadurch mir sagen wollte.", "tokens": ["Doch", "wu\u00dft'", "ich", "nicht", "was", "er", "da\u00b7durch", "mir", "sa\u00b7gen", "woll\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "PWS", "PPER", "PAV", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}