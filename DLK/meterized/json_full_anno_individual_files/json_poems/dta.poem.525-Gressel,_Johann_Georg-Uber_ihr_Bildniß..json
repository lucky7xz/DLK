{"dta.poem.525": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Uber ihr Bildni\u00df.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Zucht/ Tugend/ Ehr und Pracht von dieser Tafel strahlen/", "tokens": ["Zucht", "/", "Tu\u00b7gend", "/", "Ehr", "und", "Pracht", "von", "die\u00b7ser", "Ta\u00b7fel", "strah\u00b7len", "/"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "NN", "$(", "NN", "KON", "NN", "APPR", "PDAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die ", "tokens": ["Die"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.3": {"text": "Die todten Augen sind erf\u00fcllt mit Liebes-Gluth/", "tokens": ["Die", "tod\u00b7ten", "Au\u00b7gen", "sind", "er\u00b7f\u00fcllt", "mit", "Lie\u00b7bes\u00b7Gluth", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "VVPP", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der Br\u00fcste ", "tokens": ["Der", "Br\u00fcs\u00b7te"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "Wenn aber ", "tokens": ["Wenn", "a\u00b7ber"], "token_info": ["word", "word"], "pos": ["KOUS", "ADV"], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "Wie aus den Augen Feur als einem ", "tokens": ["Wie", "aus", "den", "Au\u00b7gen", "Feur", "als", "ei\u00b7nem"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN", "NN", "KOKOM", "ART"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Wie ihre Brust ein Schnee der voller Flammen steckt/", "tokens": ["Wie", "ih\u00b7re", "Brust", "ein", "Schnee", "der", "vol\u00b7ler", "Flam\u00b7men", "steckt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So wird die Liebes-Gluth in Hertz und Sinn erweckt.", "tokens": ["So", "wird", "die", "Lie\u00b7bes\u00b7Gluth", "in", "Hertz", "und", "Sinn", "er\u00b7weckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "APPR", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}