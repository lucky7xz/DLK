{"dta.poem.20479": {"metadata": {"author": {"name": "Hofmann von Hofmannswaldau, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Verliebte Arien.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1695", "urn": "urn:nbn:de:kobv:b4-200905197751", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wische die \u00e4chzenden thr\u00e4nen", "tokens": ["Wi\u00b7sche", "die", "\u00e4ch\u00b7zen\u00b7den", "thr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Von den wangen ab/", "tokens": ["Von", "den", "wan\u00b7gen", "ab", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKVZ", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Wilstu dir ein grab", "tokens": ["Wils\u00b7tu", "dir", "ein", "grab"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ART", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Durch die schwachen seuffzer b\u00e4hnen?", "tokens": ["Durch", "die", "schwa\u00b7chen", "seuff\u00b7zer", "b\u00e4h\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "La\u00df mich wissen/", "tokens": ["La\u00df", "mich", "wis\u00b7sen", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VVINF", "$("], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Da\u00df das k\u00fcssen", "tokens": ["Da\u00df", "das", "k\u00fcs\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PDS", "VVINF"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Alle sch\u00e4den kan vers\u00fcssen.", "tokens": ["Al\u00b7le", "sch\u00e4\u00b7den", "kan", "ver\u00b7s\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VVINF", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Perlen geh\u00f6ren den wellen/", "tokens": ["Per\u00b7len", "ge\u00b7h\u00f6\u00b7ren", "den", "wel\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "VVINF", "$("], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Doch dein wangen-schnee", "tokens": ["Doch", "dein", "wan\u00b7gen\u00b7schnee"], "token_info": ["word", "word", "word"], "pos": ["KON", "PPOSAT", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Soll dem bunten klee", "tokens": ["Soll", "dem", "bun\u00b7ten", "klee"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "ART", "ADJA", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "In den wiesen sich gesellen.", "tokens": ["In", "den", "wie\u00b7sen", "sich", "ge\u00b7sel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "VVFIN", "PRF", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "La\u00df uns \u00fcben", "tokens": ["La\u00df", "uns", "\u00fc\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["VVIMP", "PPER", "VVINF"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "In den lieben/", "tokens": ["In", "den", "lie\u00b7ben", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "$("], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Warum wilstu dich betr\u00fcben?", "tokens": ["Wa\u00b7rum", "wils\u00b7tu", "dich", "be\u00b7tr\u00fc\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PRF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Trennet uns itzo das gl\u00fccke", "tokens": ["Tren\u00b7net", "uns", "it\u00b7zo", "das", "gl\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Durch den harten schlu\u00df/", "tokens": ["Durch", "den", "har\u00b7ten", "schlu\u00df", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Da\u00df ich scheiden mu\u00df.", "tokens": ["Da\u00df", "ich", "schei\u00b7den", "mu\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVINF", "VMFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Komm ich doch gewi\u00df zur\u00fccke:", "tokens": ["Komm", "ich", "doch", "ge\u00b7wi\u00df", "zu\u00b7r\u00fc\u00b7cke", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wenn die bienen", "tokens": ["Wenn", "die", "bie\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "ART", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "In dem gr\u00fcnen", "tokens": ["In", "dem", "gr\u00fc\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "ADJA"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Fliegen/ will ich dich bedienen.", "tokens": ["Flie\u00b7gen", "/", "will", "ich", "dich", "be\u00b7die\u00b7nen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "VMFIN", "PPER", "PRF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}