{"dta.poem.2469": {"metadata": {"author": {"name": "Neumark, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Anders.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1652", "urn": "urn:nbn:de:kobv:b4-20428-0", "language": ["de:0.99"], "booktitle": "Neumark, Georg: Poetisch- und Musikalisches Lustw\u00e4ldchen. Hamburg, 1652."}, "poem": {"stanza.1": {"line.1": {"text": "Ob schon der Wetzstein stumpf und ungeschikkt zum", "tokens": ["Ob", "schon", "der", "Wetz\u00b7stein", "stumpf", "und", "un\u00b7ge\u00b7schikkt", "zum"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "NN", "ADJD", "KON", "ADJD", "APPRART"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "schneiden/", "tokens": ["schnei\u00b7den", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "So wil er gleichwol nicht die St\u00fcmpf\u2019 am Messer leiden;", "tokens": ["So", "wil", "er", "gleich\u00b7wol", "nicht", "die", "St\u00fcmpf'", "am", "Mes\u00b7ser", "lei\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "PTKNEG", "ART", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So ist auch mancher Mensch/ der andre schleiffen wil.", "tokens": ["So", "ist", "auch", "man\u00b7cher", "Mensch", "/", "der", "and\u00b7re", "schleif\u00b7fen", "wil", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "PIAT", "NN", "$(", "ART", "PIS", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ein grober Ungeschikkt/ und tauget selbst nicht viel.", "tokens": ["Ein", "gro\u00b7ber", "Un\u00b7ge\u00b7schikkt", "/", "und", "tau\u00b7get", "selbst", "nicht", "viel", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "KON", "VVFIN", "ADV", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}