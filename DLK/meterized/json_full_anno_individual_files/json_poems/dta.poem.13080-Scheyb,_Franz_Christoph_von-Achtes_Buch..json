{"dta.poem.13080": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Achtes Buch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201eist in der Erde Rund ein Volck, ein Land, ein Staat,", "tokens": ["\u201e", "ist", "in", "der", "Er\u00b7de", "Rund", "ein", "Volck", ",", "ein", "Land", ",", "ein", "Staat", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "VAFIN", "APPR", "ART", "NN", "ADJD", "ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "\u201eder anders, als durch mich sein hohes Ansehn hat?", "tokens": ["\u201e", "der", "an\u00b7ders", ",", "als", "durch", "mich", "sein", "ho\u00b7hes", "An\u00b7sehn", "hat", "?"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "ADV", "$,", "KOUS", "APPR", "PPER", "PPOSAT", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "\u201eso bald die Noth befahl, vier Gabeln aufzurichten,", "tokens": ["\u201e", "so", "bald", "die", "Noth", "be\u00b7fahl", ",", "vier", "Ga\u00b7beln", "auf\u00b7zu\u00b7rich\u00b7ten", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ADV", "ART", "NN", "VVFIN", "$,", "CARD", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "\u201eund Stroh zum Dache war; so fieng ich an zu dichten,", "tokens": ["\u201e", "und", "Stroh", "zum", "Da\u00b7che", "war", ";", "so", "fi\u00b7eng", "ich", "an", "zu", "dich\u00b7ten", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "NN", "APPRART", "NN", "VAFIN", "$.", "ADV", "VVFIN", "PPER", "APPR", "APPR", "ADJA", "$,"], "meter": "-+-+----+-+-+-", "measure": "unknown.measure.penta"}, "line.5": {"text": "\u201ewie die Bequemlichkeit k\u00f6nnt beygef\u00fcget seyn;", "tokens": ["\u201e", "wie", "die", "Be\u00b7quem\u00b7lich\u00b7keit", "k\u00f6nnt", "bey\u00b7ge\u00b7f\u00fc\u00b7get", "seyn", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "ART", "NN", "VVFIN", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "110\u201dIch fande sie durch Kunst in Kalch, in Sand und Stein.", "tokens": ["\"", "Ich", "fan\u00b7de", "sie", "durch", "Kunst", "in", "Kalch", ",", "in", "Sand", "und", "Stein", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PPER", "APPR", "NN", "APPR", "NE", "$,", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "\u201emein Ma\u00df-Stab lehrte mich die Wohnung zu verbessern,", "tokens": ["\u201e", "mein", "Ma\u00df\u00b7Stab", "lehr\u00b7te", "mich", "die", "Woh\u00b7nung", "zu", "ver\u00b7bes\u00b7sern", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "VVFIN", "PRF", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "\u201eso war gleich das Geb\u00fcrg mit H\u00e4usern und mit Schl\u00f6ssern,", "tokens": ["\u201e", "so", "war", "gleich", "das", "Ge\u00b7b\u00fcrg", "mit", "H\u00e4u\u00b7sern", "und", "mit", "Schl\u00f6s\u00b7sern", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VAFIN", "ADV", "ART", "NN", "APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "\u201ehernach das \u00f6de Land, das Strauch und Wald verh\u00fcllt,", "tokens": ["\u201e", "her\u00b7nach", "das", "\u00f6\u00b7de", "Land", ",", "das", "Strauch", "und", "Wald", "ver\u00b7h\u00fcllt", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ART", "ADJA", "NN", "$,", "ART", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "\u201emit Z\u00e4unen, Dach und Fach, mit D\u00f6rfern angef\u00fcllt;", "tokens": ["\u201e", "mit", "Z\u00e4u\u00b7nen", ",", "Dach", "und", "Fach", ",", "mit", "D\u00f6r\u00b7fern", "an\u00b7ge\u00b7f\u00fcllt", ";"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "NN", "$,", "NN", "KON", "NN", "$,", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "115\u201dDa\u00df endlich aus der Nacht der Schrecken-vollen Schatten", "tokens": ["\"", "Da\u00df", "end\u00b7lich", "aus", "der", "Nacht", "der", "Schre\u00b7cken\u00b7vol\u00b7len", "Schat\u00b7ten"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "KOUS", "ADV", "APPR", "ART", "NN", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "\u201edie Menschen in den Tag der Stadt zusammen traten.", "tokens": ["\u201e", "die", "Men\u00b7schen", "in", "den", "Tag", "der", "Stadt", "zu\u00b7sam\u00b7men", "tra\u00b7ten", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], 