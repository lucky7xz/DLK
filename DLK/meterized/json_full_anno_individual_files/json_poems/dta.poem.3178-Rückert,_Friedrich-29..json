{"dta.poem.3178": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "29.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Heerde weidet und der Hirte weidet sie;", "tokens": ["Die", "Heer\u00b7de", "wei\u00b7det", "und", "der", "Hir\u00b7te", "wei\u00b7det", "sie", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "ART", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie eins ist Heerd' und Hirt, wer unterscheidet sie?", "tokens": ["Wie", "eins", "ist", "He\u00b7erd'", "und", "Hirt", ",", "wer", "un\u00b7ter\u00b7schei\u00b7det", "sie", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VAFIN", "NN", "KON", "NN", "$,", "PWS", "VVFIN", "PPER", "$."], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Er blickt, alsob er sie mit seinen Augen weide,", "tokens": ["Er", "blickt", ",", "al\u00b7sob", "er", "sie", "mit", "sei\u00b7nen", "Au\u00b7gen", "wei\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und da\u00df sie weiden, das ist seine Augenweide.", "tokens": ["Und", "da\u00df", "sie", "wei\u00b7den", ",", "das", "ist", "sei\u00b7ne", "Au\u00b7gen\u00b7wei\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVINF", "$,", "PDS", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Die stille H\u00fcrde dort steht am bekannten Ort,", "tokens": ["Die", "stil\u00b7le", "H\u00fcr\u00b7de", "dort", "steht", "am", "be\u00b7kann\u00b7ten", "Ort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da ist des Hirten Herd, und seiner Horden Hort.", "tokens": ["Da", "ist", "des", "Hir\u00b7ten", "Herd", ",", "und", "sei\u00b7ner", "Hor\u00b7den", "Hort", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "NN", "$,", "KON", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Dann wird er scheren sie im Sommer, wenn sie wollen;", "tokens": ["Dann", "wird", "er", "sche\u00b7ren", "sie", "im", "Som\u00b7mer", ",", "wenn", "sie", "wol\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVFIN", "PPER", "APPRART", "NN", "$,", "KOUS", "PPER", "VMFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und ihm bescheren sie die \u00fcberfl\u00fcss'gen Wollen.", "tokens": ["Und", "ihm", "be\u00b7sche\u00b7ren", "sie", "die", "\u00fc\u00b7ber\u00b7fl\u00fcss'\u00b7gen", "Wol\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Wie eines Wehr und Werth dem andern so gew\u00e4hrt,", "tokens": ["Wie", "ei\u00b7nes", "Wehr", "und", "Werth", "dem", "an\u00b7dern", "so", "ge\u00b7w\u00e4hrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "KON", "NN", "ART", "ADJA", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "O wenn ihr, Herr und Heer, wie Hirt und Heerde w\u00e4rt!", "tokens": ["O", "wenn", "ihr", ",", "Herr", "und", "Heer", ",", "wie", "Hirt", "und", "Heer\u00b7de", "w\u00e4rt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOUS", "PPER", "$,", "NN", "KON", "NN", "$,", "PWAV", "NN", "KON", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}