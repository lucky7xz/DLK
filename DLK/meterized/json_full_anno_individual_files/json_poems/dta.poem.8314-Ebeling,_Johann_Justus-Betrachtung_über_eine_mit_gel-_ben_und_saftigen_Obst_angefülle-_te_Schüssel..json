{"dta.poem.8314": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "Betrachtung \u00fcber eine mit gel-  \n ben und saftigen Obst angef\u00fclle-  \n te Sch\u00fcssel.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198782", "language": ["de:0.85", "af:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "O! mein Sch\u00f6pfer wie erquikkend,", "tokens": ["O", "!", "mein", "Sch\u00f6p\u00b7fer", "wie", "er\u00b7quik\u00b7kend", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PPOSAT", "NN", "KOKOM", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und wie lieblich ja entz\u00fckkend", "tokens": ["Und", "wie", "lieb\u00b7lich", "ja", "ent\u00b7z\u00fck\u00b7kend"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ADJD", "ADV", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ist die Baumfrucht anzusehn,", "tokens": ["Ist", "die", "Baum\u00b7frucht", "an\u00b7zu\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "VVIZU", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Die so roth und sprenglicht sch\u00f6n,", "tokens": ["Die", "so", "roth", "und", "spreng\u00b7licht", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "KON", "VVFIN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Als wenn, wie in silbern Schaalen,", "tokens": ["Als", "wenn", ",", "wie", "in", "sil\u00b7bern", "Schaa\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "$,", "PWAV", "APPR", "ADJA", "NN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.6": {"text": "G\u00fcldne Aepfel herrlich strahlen!", "tokens": ["G\u00fcld\u00b7ne", "A\u00b7e\u00b7pfel", "herr\u00b7lich", "strah\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJD", "VVINF", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Solche Sch\u00f6nheit l\u00e4st du steigen;", "tokens": ["Sol\u00b7che", "Sch\u00f6n\u00b7heit", "l\u00e4st", "du", "stei\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Aus den troknen Stamm und Zweigen,", "tokens": ["Aus", "den", "trok\u00b7nen", "Stamm", "und", "Zwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "KON", "NN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.3": {"text": "Wie bist du den Menschen hold,", "tokens": ["Wie", "bist", "du", "den", "Men\u00b7schen", "hold", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ART", "NN", "ADJD", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.4": {"text": "Denen du ein esbar Gold", "tokens": ["De\u00b7nen", "du", "ein", "es\u00b7bar", "Gold"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "PPER", "ART", "ADJD", "NN"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.5": {"text": "Wundernsw\u00fcrdig zubereitest,", "tokens": ["Wun\u00b7derns\u00b7w\u00fcr\u00b7dig", "zu\u00b7be\u00b7rei\u00b7test", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und durch zarte R\u00f6hren leitest.", "tokens": ["Und", "durch", "zar\u00b7te", "R\u00f6h\u00b7ren", "lei\u00b7test", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Weil daraus die giergen Augen,", "tokens": ["Weil", "da\u00b7raus", "die", "gier\u00b7gen", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PAV", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "So viel s\u00fcsse Anmuth saugen:", "tokens": ["So", "viel", "s\u00fcs\u00b7se", "An\u00b7muth", "sau\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "So zieht der vergn\u00fcgte Blik,", "tokens": ["So", "zieht", "der", "ver\u00b7gn\u00fcg\u00b7te", "Blik", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Gleichsam meinen Mund zur\u00fck,", "tokens": ["Gleich\u00b7sam", "mei\u00b7nen", "Mund", "zu\u00b7r\u00fck", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und behindert im Verzehren,", "tokens": ["Und", "be\u00b7hin\u00b7dert", "im", "Ver\u00b7zeh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Diese Sch\u00f6nheit zu zerst\u00f6hren.", "tokens": ["Die\u00b7se", "Sch\u00f6n\u00b7heit", "zu", "zer\u00b7st\u00f6h\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Doch was lieblich anzusehen,", "tokens": ["Doch", "was", "lieb\u00b7lich", "an\u00b7zu\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADJD", "VVIZU", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bringt nicht allzeit solche Wehen,", "tokens": ["Bringt", "nicht", "all\u00b7zeit", "sol\u00b7che", "We\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ADV", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Als die Frucht im Paradies,", "tokens": ["Als", "die", "Frucht", "im", "Pa\u00b7ra\u00b7dies", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Die der Probe-Baum dort wies:", "tokens": ["Die", "der", "Pro\u00b7be\u00b7Baum", "dort", "wies", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Diese soll man ja geniessen,", "tokens": ["Die\u00b7se", "soll", "man", "ja", "ge\u00b7nies\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PIS", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Weil du sie uns l\u00e4ssest spriessen.", "tokens": ["Weil", "du", "sie", "uns", "l\u00e4s\u00b7sest", "spries\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "PPER", "VVFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Zwiefach sch\u00f6n sind diese Fr\u00fcchte,", "tokens": ["Zwie\u00b7fach", "sch\u00f6n", "sind", "die\u00b7se", "Fr\u00fcch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie sie laben das Gesichte;", "tokens": ["Wie", "sie", "la\u00b7ben", "das", "Ge\u00b7sich\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "So labt auch bei jedem Bis,", "tokens": ["So", "labt", "auch", "bei", "je\u00b7dem", "Bis", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "APPR", "PIAT", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Jhr Saft welcher Honig s\u00fc\u00df,", "tokens": ["Ihr", "Saft", "wel\u00b7cher", "Ho\u00b7nig", "s\u00fc\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PWAT", "NN", "ADJD", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Da wir in begiergen Schmekken,", "tokens": ["Da", "wir", "in", "be\u00b7gier\u00b7gen", "Schmek\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sch\u00f6pfer! deine G\u00fct entdekken.", "tokens": ["Sch\u00f6p\u00b7fer", "!", "dei\u00b7ne", "G\u00fct", "ent\u00b7dek\u00b7ken", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Ach! wie reitzend, frisch und k\u00fchle,", "tokens": ["Ach", "!", "wie", "reit\u00b7zend", ",", "frisch", "und", "k\u00fch\u00b7le", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PWAV", "VVPP", "$,", "ADJD", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schmeichelt uns des Gaums Gef\u00fchle,", "tokens": ["Schmei\u00b7chelt", "uns", "des", "Gaums", "Ge\u00b7f\u00fch\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn dadurch der Saft sich gie\u00dft,", "tokens": ["Wenn", "da\u00b7durch", "der", "Saft", "sich", "gie\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PAV", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "----+-+", "measure": "unknown.measure.di"}, "line.4": {"text": "Der aus dieser Baumfrucht fliest,", "tokens": ["Der", "aus", "die\u00b7ser", "Baum\u00b7frucht", "fliest", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PDAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Durch die Kehle lieblich quillet,", "tokens": ["Durch", "die", "Keh\u00b7le", "lieb\u00b7lich", "quil\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wenn sie unsern Mund gef\u00fcllet.", "tokens": ["Wenn", "sie", "un\u00b7sern", "Mund", "ge\u00b7f\u00fcl\u00b7let", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Diese die uns aufgetischet,", "tokens": ["Die\u00b7se", "die", "uns", "auf\u00b7ge\u00b7ti\u00b7schet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+---+---", "measure": "dactylic.init"}, "line.2": {"text": "Sind wie Saur und S\u00fc\u00df vermischet,", "tokens": ["Sind", "wie", "Saur", "und", "S\u00fc\u00df", "ver\u00b7mi\u00b7schet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "KOKOM", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Jhre herbe S\u00e4urlichkeit", "tokens": ["Ih\u00b7re", "her\u00b7be", "S\u00e4ur\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ist mit Zukker \u00fcberstreut:", "tokens": ["Ist", "mit", "Zuk\u00b7ker", "\u00fc\u00b7bers\u00b7treut", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Damit wir an deinen Gaben,", "tokens": ["Da\u00b7mit", "wir", "an", "dei\u00b7nen", "Ga\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.6": {"text": "Nach Gefalln uns k\u00f6nnen laben.", "tokens": ["Nach", "Ge\u00b7falln", "uns", "k\u00f6n\u00b7nen", "la\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Wenn ich an der Herbstgeschenke", "tokens": ["Wenn", "ich", "an", "der", "Herbst\u00b7ge\u00b7schen\u00b7ke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ursprung, recht zur\u00fckke denke,", "tokens": ["Ur\u00b7sprung", ",", "recht", "zu\u00b7r\u00fck\u00b7ke", "den\u00b7ke", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "PTKVZ", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie sie zu der Reif gedein,", "tokens": ["Wie", "sie", "zu", "der", "Reif", "ge\u00b7dein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Mu\u00df ich mich von neuen freun,", "tokens": ["Mu\u00df", "ich", "mich", "von", "neu\u00b7en", "freun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "APPR", "ADJA", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Da wir warlich Wunder essen,", "tokens": ["Da", "wir", "war\u00b7lich", "Wun\u00b7der", "es\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Die nicht gnugsam zu ermessen.", "tokens": ["Die", "nicht", "gnug\u00b7sam", "zu", "er\u00b7mes\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "ADJD", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Was der Saft der Erde bringet,", "tokens": ["Was", "der", "Saft", "der", "Er\u00b7de", "brin\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der durch Baum und Zweige dringet,", "tokens": ["Der", "durch", "Baum", "und", "Zwei\u00b7ge", "drin\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NE", "KON", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sezt sich in den Bl\u00fcthen an", "tokens": ["Sezt", "sich", "in", "den", "Bl\u00fc\u00b7then", "an"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "APPR", "ART", "NN", "APPR"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Draus dies Obst entstehen kan,", "tokens": ["Draus", "dies", "Obst", "ent\u00b7ste\u00b7hen", "kan", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PDS", "NN", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wenn durch ", "tokens": ["Wenn", "durch"], "token_info": ["word", "word"], "pos": ["KOUS", "APPR"], "meter": "-+", "measure": "iambic.single"}, "line.6": {"text": "Es zur Zeitigung erhalten.", "tokens": ["Es", "zur", "Zei\u00b7ti\u00b7gung", "er\u00b7hal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Was so durch den Stamm gebrochen,", "tokens": ["Was", "so", "durch", "den", "Stamm", "ge\u00b7bro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mu\u00df die Sonne gleichsam kochen,", "tokens": ["Mu\u00df", "die", "Son\u00b7ne", "gleich\u00b7sam", "ko\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Bis der Saft so durchger\u00fchrt,", "tokens": ["Bis", "der", "Saft", "so", "durch\u00b7ge\u00b7r\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sich die Bitterkeit verliehrt,", "tokens": ["Sich", "die", "Bit\u00b7ter\u00b7keit", "ver\u00b7liehrt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Die verduftet durch das Brennen", "tokens": ["Die", "ver\u00b7duf\u00b7tet", "durch", "das", "Bren\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Da wir sie denn essen k\u00f6nnen.", "tokens": ["Da", "wir", "sie", "denn", "es\u00b7sen", "k\u00f6n\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "VVINF", "VMINF", "$."], "meter": "----+-+-", "measure": "unknown.measure.di"}}, "stanza.11": {"line.1": {"text": "Dr\u00fckket uns dies GOttes G\u00fcte", "tokens": ["Dr\u00fck\u00b7ket", "uns", "dies", "Got\u00b7tes", "G\u00fc\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PDS", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nicht so gleich in das Gem\u00fcthe,", "tokens": ["Nicht", "so", "gleich", "in", "das", "Ge\u00b7m\u00fc\u00b7the", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Die so wunderbahrlich macht,", "tokens": ["Die", "so", "wun\u00b7der\u00b7bahr\u00b7lich", "macht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Was uns in die Augen lacht", "tokens": ["Was", "uns", "in", "die", "Au\u00b7gen", "lacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "APPR", "ART", "NN", "VVFIN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Was uns unsre Zung erfreuet,", "tokens": ["Was", "uns", "uns\u00b7re", "Zung", "er\u00b7freu\u00b7et", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.6": {"text": "Was im Magen uns gedeiet?", "tokens": ["Was", "im", "Ma\u00b7gen", "uns", "ge\u00b7dei\u00b7et", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPRART", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Menschen! wenn ihr diese Fr\u00fcchte,", "tokens": ["Men\u00b7schen", "!", "wenn", "ihr", "die\u00b7se", "Fr\u00fcch\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "KOUS", "PPER", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Diese lieblichen Gerichte,", "tokens": ["Die\u00b7se", "lieb\u00b7li\u00b7chen", "Ge\u00b7rich\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Die der Sch\u00f6pfer euch beschert,", "tokens": ["Die", "der", "Sch\u00f6p\u00b7fer", "euch", "be\u00b7schert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Bei vergn\u00fcgter Lust verzehrt:", "tokens": ["Bei", "ver\u00b7gn\u00fcg\u00b7ter", "Lust", "ver\u00b7zehrt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "So erkennt zu seinem Preise,", "tokens": ["So", "er\u00b7kennt", "zu", "sei\u00b7nem", "Prei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wie er m\u00e4chtig, g\u00fctig, weise.", "tokens": ["Wie", "er", "m\u00e4ch\u00b7tig", ",", "g\u00fc\u00b7tig", ",", "wei\u00b7se", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "$,", "ADJD", "$,", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Dieses k\u00f6nt ihr achtsam schmekken,", "tokens": ["Die\u00b7ses", "k\u00f6nt", "ihr", "acht\u00b7sam", "schmek\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und dadurch den Trieb erwekken,", "tokens": ["Und", "da\u00b7durch", "den", "Trieb", "er\u00b7wek\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "ART", "NN", "VVINF", "$,"], "meter": "----+-+-", "measure": "unknown.measure.di"}, "line.3": {"text": "Einer regen Dankbarkeit,", "tokens": ["Ei\u00b7ner", "re\u00b7gen", "Dank\u00b7bar\u00b7keit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Die ihr daf\u00fcr schuldig seid,", "tokens": ["Die", "ihr", "da\u00b7f\u00fcr", "schul\u00b7dig", "seid", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PAV", "ADJD", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Dem, der zum vergn\u00fcgten Leben,", "tokens": ["Dem", ",", "der", "zum", "ver\u00b7gn\u00fcg\u00b7ten", "Le\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PRELS", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Euch so manche Frucht gegeben.", "tokens": ["Euch", "so", "man\u00b7che", "Frucht", "ge\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PIAT", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "Wer das Obst vergn\u00fcgt geniesset,", "tokens": ["Wer", "das", "Obst", "ver\u00b7gn\u00fcgt", "ge\u00b7nies\u00b7set", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VVPP", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df der Mund von Safte fliesset,", "tokens": ["Da\u00df", "der", "Mund", "von", "Saf\u00b7te", "flies\u00b7set", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber nicht dabei gedenkt,", "tokens": ["A\u00b7ber", "nicht", "da\u00b7bei", "ge\u00b7denkt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "PAV", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wer dasselbe ihm geschenkt,", "tokens": ["Wer", "das\u00b7sel\u00b7be", "ihm", "ge\u00b7schenkt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PDAT", "PPER", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ist nicht werth die s\u00fcssen Gaben", "tokens": ["Ist", "nicht", "werth", "die", "s\u00fcs\u00b7sen", "Ga\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PTKNEG", "ADJD", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Die der Sch\u00f6pfer giebt, zu haben.", "tokens": ["Die", "der", "Sch\u00f6p\u00b7fer", "giebt", ",", "zu", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVFIN", "$,", "PTKZU", "VAINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.15": {"line.1": {"text": "Wer an Fr\u00fcchten sich erg\u00f6zzet,", "tokens": ["Wer", "an", "Fr\u00fcch\u00b7ten", "sich", "er\u00b7g\u00f6z\u00b7zet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "NN", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und ", "tokens": ["Und"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Und damit den Magen f\u00fcllt,", "tokens": ["Und", "da\u00b7mit", "den", "Ma\u00b7gen", "f\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Des Geschmakkes Reizung stillt,", "tokens": ["Des", "Ge\u00b7schmak\u00b7kes", "Rei\u00b7zung", "stillt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Jsset wie die Thiere pflegen,", "tokens": ["Js\u00b7set", "wie", "die", "Thie\u00b7re", "pfle\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Die den Geber nicht erwegen.", "tokens": ["Die", "den", "Ge\u00b7ber", "nicht", "er\u00b7we\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}