{"dta.poem.929": {"metadata": {"author": {"name": "George, Stefan", "birth": "N.A.", "death": "N.A."}, "title": "N ACH   D ER   L ESE", "genre": "Lyrik", "period": "N.A.", "pub_year": "1897", "urn": "urn:nbn:de:kobv:b4-200905191530", "language": ["de:0.99"], "booktitle": "George, Stefan: Das Jahr der Seele. Berlin, 1897."}, "poem": {"stanza.1": {"line.1": {"text": "Komm in den totgesagten park und schau:               ", "tokens": ["Komm", "in", "den", "tot\u00b7ge\u00b7sag\u00b7ten", "park", "und", "schau", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "KON", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der schimmer ferner l&#228;chelnder gestade               ", "tokens": ["Der", "schim\u00b7mer", "fer\u00b7ner", "l", "&#228;", "cheln\u00b7der", "ge\u00b7sta\u00b7de"], "token_info": ["word", "word", "word", "word", "XML_entity", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der reinen wolken unverhofftes blau               ", "tokens": ["Der", "rei\u00b7nen", "wol\u00b7ken", "un\u00b7ver\u00b7hoff\u00b7tes", "blau"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Erhellt die weiher und die bunten pfade             ", "tokens": ["Er\u00b7hellt", "die", "wei\u00b7her", "und", "die", "bun\u00b7ten", "pfa\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "KON", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Dort nimm das tiefe gelb das weiche grau               ", "tokens": ["Dort", "nimm", "das", "tie\u00b7fe", "gelb", "das", "wei\u00b7che", "grau"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVIMP", "ART", "ADJA", "ADJD", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Von birken und von buchs &#183; der wind ist lau               ", "tokens": ["Von", "bir\u00b7ken", "und", "von", "buchs", "&#183;", "der", "wind", "ist", "lau"], "token_info": ["word", "word", "word", "word", "word", "XML_entity", "word", "word", "word", "word"], "pos": ["APPR", "VVINF", "KON", "APPR", "NE", "$(", "ART", "NN", "VAFIN", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die sp&#228;ten rosen welkten noch nicht ganz               ", "tokens": ["Die", "sp", "&#228;", "ten", "ro\u00b7sen", "welk\u00b7ten", "noch", "nicht", "ganz"], "token_info": ["word", "word", "XML_entity", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "FM", "FM", "FM", "FM", "FM", "ADV", "PTKNEG", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Erlese k&#252;sse sie und flicht den kranz             ", "tokens": ["Er\u00b7le\u00b7se", "k", "&#252;", "sse", "sie", "und", "flicht", "den", "kranz"], "token_info": ["word", "word", "XML_entity", "word", "word", "word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "$(", "VVFIN", "PPER", "KON", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Vergiss auch diese letzen astern nicht               ", "tokens": ["Ver\u00b7giss", "auch", "die\u00b7se", "let\u00b7zen", "as\u00b7tern", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "PDAT", "ADJA", "NN", "PTKNEG"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Den purpur um die ranken wilder reben               ", "tokens": ["Den", "pur\u00b7pur", "um", "die", "ran\u00b7ken", "wil\u00b7der", "re\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und auch was &#252;brig blieb von gr&#252;nem leben               ", "tokens": ["Und", "auch", "was", "&#252;", "brig", "blieb", "von", "gr", "&#252;", "nem", "le\u00b7ben"], "token_info": ["word", "word", "word", "XML_entity", "word", "word", "word", "word", "XML_entity", "word", "word"], "pos": ["KON", "ADV", "PWS", "$(", "ADJD", "VVFIN", "APPR", "NE", "$(", "ADV", "VVINF"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Verwinde leicht im herbstlichen gesicht.             ", "tokens": ["Ver\u00b7win\u00b7de", "leicht", "im", "herbst\u00b7li\u00b7chen", "ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPRART", "ADJA", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}