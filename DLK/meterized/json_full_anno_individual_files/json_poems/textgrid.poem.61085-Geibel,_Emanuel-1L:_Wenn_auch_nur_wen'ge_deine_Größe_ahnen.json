{"textgrid.poem.61085": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wenn auch nur wen'ge deine Gr\u00f6\u00dfe ahnen", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn auch nur wen'ge deine Gr\u00f6\u00dfe ahnen", "tokens": ["Wenn", "auch", "nur", "wen'\u00b7ge", "dei\u00b7ne", "Gr\u00f6\u00b7\u00dfe", "ah\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ADV", "VVFIN", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von jenem Volk, f\u00fcr das du hast gesungen,", "tokens": ["Von", "je\u00b7nem", "Volk", ",", "f\u00fcr", "das", "du", "hast", "ge\u00b7sun\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,", "APPR", "PRELS", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "F\u00fcr das du hast gefochten und gerungen,", "tokens": ["F\u00fcr", "das", "du", "hast", "ge\u00b7foch\u00b7ten", "und", "ge\u00b7run\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "VAFIN", "VVPP", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Voran ihm wandelnd auf der Sch\u00f6nheit Bahnen:", "tokens": ["Vo\u00b7ran", "ihm", "wan\u00b7delnd", "auf", "der", "Sch\u00f6n\u00b7heit", "Bah\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVPP", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Doch sammelt schon im Schatten deiner Fahnen", "tokens": ["Doch", "sam\u00b7melt", "schon", "im", "Schat\u00b7ten", "dei\u00b7ner", "Fah\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "APPRART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein H\u00e4uflein sich von edlem Mut durchdrungen,", "tokens": ["Ein", "H\u00e4uf\u00b7lein", "sich", "von", "ed\u00b7lem", "Mut", "durch\u00b7drun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und ob dein eigner Feldruf auch verklungen,", "tokens": ["Und", "ob", "dein", "eig\u00b7ner", "Feld\u00b7ruf", "auch", "ver\u00b7klun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPOSAT", "ADJA", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wir schlagen fort die Schlacht f\u00fcr deine Manen.", "tokens": ["Wir", "schla\u00b7gen", "fort", "die", "Schlacht", "f\u00fcr", "dei\u00b7ne", "Ma\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wir sind die Schar, die nie von Schrecken bleiche,", "tokens": ["Wir", "sind", "die", "Schar", ",", "die", "nie", "von", "Schre\u00b7cken", "blei\u00b7che", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PRELS", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die mitten durch des Feinds gesenkte Speere", "tokens": ["Die", "mit\u00b7ten", "durch", "des", "Feinds", "ge\u00b7senk\u00b7te", "Spee\