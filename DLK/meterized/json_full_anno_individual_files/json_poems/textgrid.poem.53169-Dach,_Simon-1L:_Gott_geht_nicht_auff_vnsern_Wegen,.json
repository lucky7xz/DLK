{"textgrid.poem.53169": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "1L: Gott geht nicht auff vnsern Wegen,", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gott geht nicht auff vnsern Wegen,", "tokens": ["Gott", "geht", "nicht", "auff", "vn\u00b7sern", "We\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PTKNEG", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Seiner ist nicht vnser Rath,", "tokens": ["Sei\u00b7ner", "ist", "nicht", "vn\u00b7ser", "Rath", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VAFIN", "PTKNEG", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie der Himmel ist entlegen", "tokens": ["Wie", "der", "Him\u00b7mel", "ist", "ent\u00b7le\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "VAFIN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Von der finstern Erden stat,", "tokens": ["Von", "der", "fins\u00b7tern", "Er\u00b7den", "stat", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Also hoch auch f\u00e4hrt sein Sinn", "tokens": ["Al\u00b7so", "hoch", "auch", "f\u00e4hrt", "sein", "Sinn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "ADV", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Vber vnsern Vorsatz hin.", "tokens": ["Vber", "vn\u00b7sern", "Vor\u00b7satz", "hin", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "H\u00e4tt es, Braut, bey dir gestanden,", "tokens": ["H\u00e4tt", "es", ",", "Braut", ",", "bey", "dir", "ge\u00b7stan\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "NN", "$,", "APPR", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Du best\u00fcndest warlich nicht", "tokens": ["Du", "be\u00b7st\u00fcn\u00b7dest", "war\u00b7lich", "nicht"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "PTKNEG"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wiederumb in Liebes-Banden,", "tokens": ["Wie\u00b7de\u00b7rumb", "in", "Lie\u00b7bes\u00b7Ban\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Denn du woltest Trew vnd Pflicht", "tokens": ["Denn", "du", "wol\u00b7test", "Trew", "vnd", "Pflicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VMFIN", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Vnserm wehrten Roberthin", "tokens": ["Vn\u00b7serm", "wehr\u00b7ten", "Ro\u00b7bert\u00b7hin"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Nach dem Tod' auch nicht entziehn.", "tokens": ["Nach", "dem", "Tod'", "auch", "nicht", "ent\u00b7ziehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Dritthalb Jahre sind vergangen,", "tokens": ["Dritt\u00b7halb", "Jah\u00b7re", "sind", "ver\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Seit durchaus dich keine Gunst,", "tokens": ["Seit", "durc\u00b7haus", "dich", "kei\u00b7ne", "Gunst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "PPER", "PIAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Keine Liebe k\u00f6nnen fangen", "tokens": ["Kei\u00b7ne", "Lie\u00b7be", "k\u00f6n\u00b7nen", "fan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "NN", "VMFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ohn allein des H\u00f6chsten Brunst,", "tokens": ["Ohn", "al\u00b7lein", "des", "H\u00f6chs\u00b7ten", "Brunst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Welchem du mit aller Macht", "tokens": ["Wel\u00b7chem", "du", "mit", "al\u00b7ler", "Macht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "APPR", "PIAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Hast gedienet Tag vnd Nacht.", "tokens": ["Hast", "ge\u00b7die\u00b7net", "Tag", "vnd", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Niemand hat bi\u00dfher vernommen", "tokens": ["Nie\u00b7mand", "hat", "bi\u00df\u00b7her", "ver\u00b7nom\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["PIS", "VAFIN", "ADV", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dich zu einer Frewde gehn,", "tokens": ["Dich", "zu", "ei\u00b7ner", "Frew\u00b7de", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn ich nur bin zu dir kommen,", "tokens": ["Wenn", "ich", "nur", "bin", "zu", "dir", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VAFIN", "APPR", "PPER", "VVINF", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Sah' ich dich betr\u00fcbet stehn,", "tokens": ["Sah'", "ich", "dich", "be\u00b7tr\u00fc\u00b7bet", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "VVFIN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Es vertrieben dir die Zeit", "tokens": ["Es", "ver\u00b7trie\u00b7ben", "dir", "die", "Zeit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Andacht, Still vnd Einsamkeit.", "tokens": ["An\u00b7dacht", ",", "Still", "vnd", "Ein\u00b7sam\u00b7keit", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Br\u00e4utgam, Gott hat angesehen,", "tokens": ["Br\u00e4ut\u00b7gam", ",", "Gott", "hat", "an\u00b7ge\u00b7se\u00b7hen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie du dich bi\u00dfher gekr\u00e4nckt,", "tokens": ["Wie", "du", "dich", "bi\u00df\u00b7her", "ge\u00b7kr\u00e4nckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "ADV", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hat auff dein Gebeht vnd flehen", "tokens": ["Hat", "auff", "dein", "Ge\u00b7beht", "vnd", "fle\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "PPOSAT", "NN", "KON", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ihr den harten Sinn gelenckt,", "tokens": ["Ihr", "den", "har\u00b7ten", "Sinn", "ge\u00b7lenckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Da\u00df sie sich mit Hertz vnd Hand", "tokens": ["Da\u00df", "sie", "sich", "mit", "Hertz", "vnd", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Endlich noch zu dir gewand.", "tokens": ["End\u00b7lich", "noch", "zu", "dir", "ge\u00b7wand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Diese sch\u00f6ne Zucht-Geberden,", "tokens": ["Die\u00b7se", "sch\u00f6\u00b7ne", "Zucht\u00b7Ge\u00b7ber\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dieser thewren Vnschuld Muth,", "tokens": ["Die\u00b7ser", "thew\u00b7ren", "Vn\u00b7schuld", "Muth", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Der Verstand sol dein nun werden,", "tokens": ["Der", "Ver\u00b7stand", "sol", "dein", "nun", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPOSAT", "ADV", "VAINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sieh, was Gottes Rath nicht thut:", "tokens": ["Sieh", ",", "was", "Got\u00b7tes", "Rath", "nicht", "thut", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "NN", "NN", "PTKNEG", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Roberthinen Gl\u00fcck vnd Zier", "tokens": ["Ro\u00b7bert\u00b7hi\u00b7nen", "Gl\u00fcck", "vnd", "Zier"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Wird nach seinem Tode dir.", "tokens": ["Wird", "nach", "sei\u00b7nem", "To\u00b7de", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPOSAT", "NN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Die der L\u00f6bnicht h\u00e4lt erkohren,", "tokens": ["Die", "der", "L\u00f6b\u00b7nicht", "h\u00e4lt", "er\u00b7koh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der sich warlich sehr erfrewt,", "tokens": ["Der", "sich", "war\u00b7lich", "sehr", "er\u00b7frewt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "ADV", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df er Sie nicht hat verlohren,", "tokens": ["Da\u00df", "er", "Sie", "nicht", "hat", "ver\u00b7loh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "PTKNEG", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "H\u00e4tt ein ander Sie gefreyt,", "tokens": ["H\u00e4tt", "ein", "an\u00b7der", "Sie", "ge\u00b7freyt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJD", "PPER", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Die den Ruhm der Keuschheit tr\u00e4gt,", "tokens": ["Die", "den", "Ruhm", "der", "Keuschheit", "tr\u00e4gt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "Wird dir jetzund beygelegt.", "tokens": ["Wird", "dir", "je\u00b7tzund", "bey\u00b7ge\u00b7legt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "O mit was erfrewten Sinnen", "tokens": ["O", "mit", "was", "er\u00b7frew\u00b7ten", "Sin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "APPR", "PRELS", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wirst du an dein Ampt nun gehn,", "tokens": ["Wirst", "du", "an", "dein", "Ampt", "nun", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "PPOSAT", "NN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wirst viel Seelen Gott gewinnen", "tokens": ["Wirst", "viel", "See\u00b7len", "Gott", "ge\u00b7win\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PIAT", "NN", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Vnd f\u00fcr seiner Herde stehn", "tokens": ["Vnd", "f\u00fcr", "sei\u00b7ner", "Her\u00b7de", "stehn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN", "VVINF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Aller Trew vnd Vnschuld voll,", "tokens": ["Al\u00b7ler", "Trew", "vnd", "Vn\u00b7schuld", "voll", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Als ein Seelen-W\u00e4chter sol!", "tokens": ["Als", "ein", "See\u00b7len\u00b7W\u00e4ch\u00b7ter", "sol", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Sol ich grosse W\u00fcnsch' hie fassen?", "tokens": ["Sol", "ich", "gros\u00b7se", "W\u00fcn\u00b7sch'", "hie", "fas\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADJA", "NN", "ADV", "VVINF", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Zwar es w\u00e4re meine Pflicht,", "tokens": ["Zwar", "es", "w\u00e4\u00b7re", "mei\u00b7ne", "Pflicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Gott wird euch vorhin nicht lassen,", "tokens": ["Gott", "wird", "euch", "vor\u00b7hin", "nicht", "las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Solcher Heyraht fehlt es nicht:", "tokens": ["Sol\u00b7cher", "Hey\u00b7raht", "fehlt", "es", "nicht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Die Braut bringt durch jeden Trit,", "tokens": ["Die", "Braut", "bringt", "durch", "je\u00b7den", "Trit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Br\u00e4utgam, tausent Segen mit.", "tokens": ["Br\u00e4ut\u00b7gam", ",", "tau\u00b7sent", "Se\u00b7gen", "mit", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Gott geht nicht auff vnsern Wegen,", "tokens": ["Gott", "geht", "nicht", "auff", "vn\u00b7sern", "We\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PTKNEG", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Seiner ist nicht vnser Rath,", "tokens": ["Sei\u00b7ner", "ist", "nicht", "vn\u00b7ser", "Rath", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VAFIN", "PTKNEG", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie der Himmel ist entlegen", "tokens": ["Wie", "der", "Him\u00b7mel", "ist", "ent\u00b7le\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "VAFIN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Von der finstern Erden stat,", "tokens": ["Von", "der", "fins\u00b7tern", "Er\u00b7den", "stat", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Also hoch auch f\u00e4hrt sein Sinn", "tokens": ["Al\u00b7so", "hoch", "auch", "f\u00e4hrt", "sein", "Sinn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "ADV", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.te