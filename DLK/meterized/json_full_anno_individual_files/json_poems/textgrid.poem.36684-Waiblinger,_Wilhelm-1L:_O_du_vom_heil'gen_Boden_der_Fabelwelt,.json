{"textgrid.poem.36684": {"metadata": {"author": {"name": "Waiblinger, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: O du vom heil'gen Boden der Fabelwelt,", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O du vom heil'gen Boden der Fabelwelt,", "tokens": ["O", "du", "vom", "heil'\u00b7gen", "Bo\u00b7den", "der", "Fa\u00b7bel\u00b7welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "APPRART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Vom Fr\u00fchlingsgarten meines Hesperiens,", "tokens": ["Vom", "Fr\u00fch\u00b7lings\u00b7gar\u00b7ten", "mei\u00b7nes", "Hes\u00b7pe\u00b7ri\u00b7ens", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Von meiner Sehnsucht Grab und Wiege", "tokens": ["Von", "mei\u00b7ner", "Sehn\u00b7sucht", "Grab", "und", "Wie\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "S\u00fc\u00dfestes, theuerstes Schattenpl\u00e4tzchen!", "tokens": ["S\u00fc\u00b7\u00dfes\u00b7tes", ",", "theu\u00b7ers\u00b7tes", "Schat\u00b7ten\u00b7pl\u00e4tz\u00b7chen", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Wohin die Schwermuth fl\u00fcchtet, die Ewige", "tokens": ["Wo\u00b7hin", "die", "Schwer\u00b7muth", "fl\u00fcch\u00b7tet", ",", "die", "E\u00b7wi\u00b7ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "ART", "NN", "VVFIN", "$,", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Verlassend, die Jahrtausende nicht gelehrt", "tokens": ["Ver\u00b7las\u00b7send", ",", "die", "Jahr\u00b7tau\u00b7sen\u00b7de", "nicht", "ge\u00b7lehrt"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVPP", "$,", "ART", "NN", "PTKNEG", "VVPP"], "meter": "-+---+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ihr stolzes Herrscherhaupt zu b\u00fccken,", "tokens": ["Ihr", "stol\u00b7zes", "Herr\u00b7scher\u00b7haupt", "zu", "b\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Roma, die Einsame, wie die Eine!", "tokens": ["Ro\u00b7ma", ",", "die", "Ein\u00b7sa\u00b7me", ",", "wie", "die", "Ei\u00b7ne", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "NN", "$,", "PWAV", "ART", "NN", "$."], "meter": "++-+--+-+-", "measure": "trochaic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Denn also ist des Menschen Gem\u00fcth. Der Geist,", "tokens": ["Denn", "al\u00b7so", "ist", "des", "Men\u00b7schen", "Ge\u00b7m\u00fcth", ".", "Der", "Geist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "ART", "NN", "NN", "$.", "ART", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Der einsam lastet \u00fcber den Tr\u00fcmmern all',", "tokens": ["Der", "ein\u00b7sam", "las\u00b7tet", "\u00fc\u00b7ber", "den", "Tr\u00fcm\u00b7mern", "all'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "APPR", "ART", "NN", "PIS", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Oft dr\u00fcckt er, und ein starkes Herz z\u00fcrnt,", "tokens": ["Oft", "dr\u00fcckt", "er", ",", "und", "ein", "star\u00b7kes", "Herz", "z\u00fcrnt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KON", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn sich die Hehre vor ihm entfaltet,", "tokens": ["Wenn", "sich", "die", "Heh\u00b7re", "vor", "ihm", "ent\u00b7fal\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Einst ach so gro\u00dfer Thaten und G\u00f6tter voll,", "tokens": ["Einst", "ach", "so", "gro\u00b7\u00dfer", "Tha\u00b7ten", "und", "G\u00f6t\u00b7ter", "voll", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "ADJA", "NN", "KON", "NN", "ADJD", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Noch ohne Lorbeer gl\u00fchet der Genius,", "tokens": ["Noch", "oh\u00b7ne", "Lor\u00b7beer", "gl\u00fc\u00b7het", "der", "Ge\u00b7nius", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und Scham ihm, gleich der Purpurbl\u00fcthe", "tokens": ["Und", "Scham", "ihm", ",", "gleich", "der", "Pur\u00b7pur\u00b7bl\u00fct\u00b7he"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "NN", "PPER", "$,", "ADV", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "K\u00fcnftiger Fr\u00fcchte, die Wange r\u00f6thet.", "tokens": ["K\u00fcnf\u00b7ti\u00b7ger", "Fr\u00fcch\u00b7te", ",", "die", "Wan\u00b7ge", "r\u00f6\u00b7thet", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ART", "NN", "VVFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.5": {"line.1": {"text": "Zu schwach auch ist er. Immer im Tempel selbst", "tokens": ["Zu", "schwach", "auch", "ist", "er", ".", "Im\u00b7mer", "im", "Tem\u00b7pel", "selbst"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKA", "ADJD", "ADV", "VAFIN", "PPER", "$.", "ADV", "APPRART", "NN", "ADV"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Verharrt die Andacht nicht. Der Olympier", "tokens": ["Ver\u00b7harrt", "die", "An\u00b7dacht", "nicht", ".", "Der", "O\u00b7lym\u00b7pier"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "ART", "NN", "PTKNEG", "$.", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Legt oft den Donnerkeil zur Seite,", "tokens": ["Legt", "oft", "den", "Don\u00b7ner\u00b7keil", "zur", "Sei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ueber den schlummernden Kronos l\u00e4chelt", "tokens": ["Ue\u00b7ber", "den", "schlum\u00b7mern\u00b7den", "Kro\u00b7nos", "l\u00e4\u00b7chelt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.6": {"line.1": {"text": "Die Charis. Eilig flieg' ich zu dir alsdann", "tokens": ["Die", "Cha\u00b7ris", ".", "Ei\u00b7lig", "flieg'", "ich", "zu", "dir", "als\u00b7dann"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$.", "ADJD", "VVFIN", "PPER", "APPR", "PPER", "ADV"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "In k\u00fchles Dunkel, wo den Erinn'rungen", "tokens": ["In", "k\u00fch\u00b7les", "Dun\u00b7kel", ",", "wo", "den", "Er\u00b7inn'\u00b7run\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "PWAV", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der fernen Vorwelt noch zum Denkmal", "tokens": ["Der", "fer\u00b7nen", "Vor\u00b7welt", "noch", "zum", "Denk\u00b7mal"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Epheugeh\u00e4nge dem Fels entsinken.", "tokens": ["E\u00b7pheu\u00b7ge\u00b7h\u00e4n\u00b7ge", "dem", "Fels", "ent\u00b7sin\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVINF", "$."], "meter": "---+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.7": {"line.1": {"text": "Da st\u00f6rt sie nicht in m\u00e4chtiger Wirklichkeit", "tokens": ["Da", "st\u00f6rt", "sie", "nicht", "in", "m\u00e4ch\u00b7ti\u00b7ger", "Wirk\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PTKNEG", "APPR", "ADJA", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Die Ruhmsucht auf: sie wehet dem L\u00fcftchen gleich,", "tokens": ["Die", "Ruhm\u00b7sucht", "auf", ":", "sie", "we\u00b7het", "dem", "L\u00fcft\u00b7chen", "gleich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$.", "PPER", "VVFIN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Sie d\u00e4mmert, wie die Abendr\u00f6the,", "tokens": ["Sie", "d\u00e4m\u00b7mert", ",", "wie", "die", "A\u00b7bend\u00b7r\u00f6\u00b7the", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Duftet, wie Rosen, ums Angesicht mir.", "tokens": ["Duf\u00b7tet", ",", "wie", "Ro\u00b7sen", ",", "ums", "An\u00b7ge\u00b7sicht", "mir", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "NN", "$,", "APPRART", "NN", "PPER", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.8": {"line.1": {"text": "So ist's dem K\u00fchnen, der aus der wogenden", "tokens": ["So", "ist's", "dem", "K\u00fch\u00b7nen", ",", "der", "aus", "der", "wo\u00b7gen\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "$,", "PRELS", "APPR", "ART", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Urnacht des Meeres schwindelnd hervorgetaucht,", "tokens": ["Ur\u00b7nacht", "des", "Mee\u00b7res", "schwin\u00b7delnd", "her\u00b7vor\u00b7ge\u00b7taucht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Noch taumelnd von den Wundern allen,", "tokens": ["Noch", "tau\u00b7melnd", "von", "den", "Wun\u00b7dern", "al\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "ART", "NN", "PIAT", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die er gesehen im ew'gen Abgrund.", "tokens": ["Die", "er", "ge\u00b7se\u00b7hen", "im", "ew'\u00b7gen", "Ab\u00b7grund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVPP", "APPRART", "ADJA", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.9": {"line.1": {"text": "Wie mir's ist, wenn ich deiner Gewalt entfloh,", "tokens": ["Wie", "mir's", "ist", ",", "wenn", "ich", "dei\u00b7ner", "Ge\u00b7walt", "ent\u00b7floh", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "VAFIN", "$,", "KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.2": {"text": "O Rom, und dennoch h\u00e4ngt mein Gedank' an dir,", "tokens": ["O", "Rom", ",", "und", "den\u00b7noch", "h\u00e4ngt", "mein", "Ge\u00b7dank'", "an", "dir", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "KON", "ADV", "VVFIN", "PPOSAT", "NN", "APPR", "PPER", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Herakles du der Weltgeschichte,", "tokens": ["Her\u00b7ak\u00b7les", "du", "der", "Welt\u00b7ge\u00b7schich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nur da\u00df ich ihn in der Wiege denke.", "tokens": ["Nur", "da\u00df", "ich", "ihn", "in", "der", "Wie\u00b7ge", "den\u00b7ke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.10": {"line.1": {"text": "Denn wo die Fluth so selig durch Fr\u00fchlingslaub", "tokens": ["Denn", "wo", "die", "Fluth", "so", "se\u00b7lig", "durch", "Fr\u00fch\u00b7lings\u00b7laub"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "NN", "ADV", "ADJD", "APPR", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Vorblinkend, dort am felsigen Ufer spielt,", "tokens": ["Vor\u00b7blin\u00b7kend", ",", "dort", "am", "fel\u00b7si\u00b7gen", "U\u00b7fer", "spielt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "ADV", "APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Stand ja die Mutter Alba, die ihr", "tokens": ["Stand", "ja", "die", "Mut\u00b7ter", "Al\u00b7ba", ",", "die", "ihr"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "NN", "NN", "$,", "PRELS", "PPER"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Leben geopfert dem Zorn der Tochter.", "tokens": ["Le\u00b7ben", "ge\u00b7o\u00b7pfert", "dem", "Zorn", "der", "Toch\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.11": {"line.1": {"text": "Doch nicht die Schlachten, nicht die zerst\u00f6renden", "tokens": ["Doch", "nicht", "die", "Schlach\u00b7ten", ",", "nicht", "die", "zer\u00b7st\u00f6\u00b7ren\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PTKNEG", "ART", "NN", "$,", "PTKNEG", "ART", "ADJA"], "meter": "-+-+--+-+--", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Streitkr\u00e4fte, nicht des Kriegesget\u00fcmmels denkt", "tokens": ["Streit\u00b7kr\u00e4f\u00b7te", ",", "nicht", "des", "Krie\u00b7ges\u00b7ge\u00b7t\u00fcm\u00b7mels", "denkt"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "PTKNEG", "ART", "NN", "VVFIN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Mein Geist; es jubeln hier und singen", "tokens": ["Mein", "Geist", ";", "es", "ju\u00b7beln", "hier", "und", "sin\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$.", "PPER", "VVFIN", "ADV", "KON", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Liebliche V\u00f6gel zur\u00fcck die G\u00f6tter,", "tokens": ["Lieb\u00b7li\u00b7che", "V\u00f6\u00b7gel", "zu\u00b7r\u00fcck", "die", "G\u00f6t\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKVZ", "ART", "NN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.12": {"line.1": {"text": "Die alten, die zur Heimath das Seegestad,", "tokens": ["Die", "al\u00b7ten", ",", "die", "zur", "Hei\u00b7math", "das", "See\u00b7ge\u00b7stad", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "PRELS", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Der Grotte Dunkel, und dies erquickliche,", "tokens": ["Der", "Grot\u00b7te", "Dun\u00b7kel", ",", "und", "dies", "er\u00b7quick\u00b7li\u00b7che", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "KON", "PDS", "ADJA", "$,"], "meter": "-+-+--+-+--", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Dies ew'ge Gr\u00fcn gew\u00e4hlt, und heimlich", "tokens": ["Dies", "ew'\u00b7ge", "Gr\u00fcn", "ge\u00b7w\u00e4hlt", ",", "und", "heim\u00b7lich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PDS", "ADJA", "NN", "VVPP", "$,", "KON", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Noch ihr unsterbliches Leben f\u00fchren.", "tokens": ["Noch", "ihr", "uns\u00b7terb\u00b7li\u00b7ches", "Le\u00b7ben", "f\u00fch\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "+--+--+-+-", "measur