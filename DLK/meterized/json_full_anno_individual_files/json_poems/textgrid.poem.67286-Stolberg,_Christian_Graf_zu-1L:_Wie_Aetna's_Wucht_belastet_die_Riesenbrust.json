{"textgrid.poem.67286": {"metadata": {"author": {"name": "Stolberg, Christian Graf zu", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie Aetna's Wucht belastet die Riesenbrust", "genre": "verse", "period": "N.A.", "pub_year": 1784, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie Aetna's Wucht belastet die Riesenbrust", "tokens": ["Wie", "A\u00b7et\u00b7na's", "Wucht", "be\u00b7las\u00b7tet", "die", "Rie\u00b7sen\u00b7brust"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "NE", "NN", "VVFIN", "ART", "NN"], "meter": "-+--+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Des Typhon \u2013 zuckt er, dumpf das Gebirg' erkracht", "tokens": ["Des", "Ty\u00b7phon", "\u2013", "zuckt", "er", ",", "dumpf", "das", "Ge\u00b7bir\u00b7g'", "er\u00b7kracht"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$(", "VVFIN", "PPER", "$,", "ADJD", "ART", "NN", "VVFIN"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Mit Kluft und Hainen; st\u00f6hnt er, Wolken", "tokens": ["Mit", "Kluft", "und", "Hai\u00b7nen", ";", "st\u00f6hnt", "er", ",", "Wol\u00b7ken"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["APPR", "NN", "KON", "NN", "$.", "VVFIN", "PPER", "$,", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wirbeln empor sich mit Asch' und Flammen \u2013", "tokens": ["Wir\u00b7beln", "em\u00b7por", "sich", "mit", "Asch'", "und", "Flam\u00b7men", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "PRF", "APPR", "NE", "KON", "NN", "$("], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "So lag des Grames Bild auf der Seele mir", "tokens": ["So", "lag", "des", "Gra\u00b7mes", "Bild", "auf", "der", "See\u00b7le", "mir"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "APPR", "ART", "NN", "PPER"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "In jener schwarzen Stunde des Strafgerichts,", "tokens": ["In", "je\u00b7ner", "schwar\u00b7zen", "Stun\u00b7de", "des", "Straf\u00b7ge\u00b7richts", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Die ausgo\u00df ihres Zornes Schaalen", "tokens": ["Die", "aus\u00b7go\u00df", "ih\u00b7res", "Zor\u00b7nes", "Schaa\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ueber den Busen des Vaterlandes.", "tokens": ["Ue\u00b7ber", "den", "Bu\u00b7sen", "des", "Va\u00b7ter\u00b7lan\u00b7des", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Nun kr\u00e4nze deine Locken, Germania,", "tokens": ["Nun", "kr\u00e4n\u00b7ze", "dei\u00b7ne", "Lo\u00b7cken", ",", "Ger\u00b7ma\u00b7nia", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$,", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Dein Haupt erhebe hoch und dein Aug' umher,", "tokens": ["Dein", "Haupt", "er\u00b7he\u00b7be", "hoch", "und", "dein", "Aug'", "um\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJD", "KON", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Dein gro\u00dfes, blaues Auge! Welch ein", "tokens": ["Dein", "gro\u00b7\u00dfes", ",", "blau\u00b7es", "Au\u00b7ge", "!", "Welch", "ein"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "ADJA", "$,", "ADJA", "NN", "$.", "PIAT", "ART"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Morgen verscheuchte die Nacht des Drangsals!", "tokens": ["Mor\u00b7gen", "ver\u00b7scheuch\u00b7te", "die", "Nacht", "des", "Drangs\u00b7als", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.4": {"line.1": {"text": "Ihr Vortrab schw\u00e4rmte l\u00e4ngst in der D\u00e4mmrung Graun,", "tokens": ["Ihr", "Vor\u00b7trab", "schw\u00e4rm\u00b7te", "l\u00e4ngst", "in", "der", "D\u00e4mm\u00b7rung", "Graun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "APPR", "ART", "NN", "NE", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ein t\u00e4uschendes Gewimmel von Geisterchen", "tokens": ["Ein", "t\u00e4u\u00b7schen\u00b7des", "Ge\u00b7wim\u00b7mel", "von", "Geis\u00b7ter\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "NN"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Des Irrsals, Schwindels, gleich umflatternd", "tokens": ["Des", "Irr\u00b7sals", ",", "Schwin\u00b7dels", ",", "gleich", "um\u00b7flat\u00b7ternd"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "NN", "$,", "ADV", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Tempel und Thron und des Schreiblers Lampe.", "tokens": ["Tem\u00b7pel", "und", "Thron", "und", "des", "Schreib\u00b7lers", "Lam\u00b7pe", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "KON", "ART", "NN", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.5": {"line.1": {"text": "Schlau ihren Apfel hatt' in die V\u00f6lkerschaar", "tokens": ["Schlau", "ih\u00b7ren", "Ap\u00b7fel", "hatt'", "in", "die", "V\u00f6l\u00b7ker\u00b7schaar"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "PPOSAT", "NN", "VAFIN", "APPR", "ART", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Des Einen Urstamms Eris-Tisiphone", "tokens": ["Des", "Ei\u00b7nen", "Ur\u00b7stamms", "E\u00b7ris\u00b7Ti\u00b7si\u00b7pho\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ART", "NN", "NE"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Geworfen, und der Zwietracht Saaten", "tokens": ["Ge\u00b7wor\u00b7fen", ",", "und", "der", "Zwiet\u00b7racht", "Saa\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "KON", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ernteten Jene, die nun verst\u00e4ubt sind,", "tokens": ["Ern\u00b7te\u00b7ten", "Je\u00b7ne", ",", "die", "nun", "ver\u00b7st\u00e4ubt", "sind", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "PRELS", "ADV", "VVPP", "VAFIN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.6": {"line.1": {"text": "Wie Sand des Heerwegs! Siehe, wie starren dort", "tokens": ["Wie", "Sand", "des", "Heer\u00b7wegs", "!", "Sie\u00b7he", ",", "wie", "star\u00b7ren", "dort"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "NN", "ART", "NN", "$.", "VVIMP", "$,", "PWAV", "VVFIN", "ADV"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Gefild' und Str\u00f6me, wo sich die Hord' ergo\u00df", "tokens": ["Ge\u00b7fild'", "und", "Str\u00f6\u00b7me", ",", "wo", "sich", "die", "Hord'", "er\u00b7go\u00df"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "$,", "PWAV", "PRF", "ART", "NN", "VVFIN"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "In Ro\u00dfbachs Flucht, von Leichen, Waffen", "tokens": ["In", "Ro\u00df\u00b7bachs", "Flucht", ",", "von", "Lei\u00b7chen", ",", "Waf\u00b7fen"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["APPR", "NE", "NN", "$,", "APPR", "NN", "$,", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Fernhin geschleudert und Geier-Fahnen!", "tokens": ["Fern\u00b7hin", "ge\u00b7schleu\u00b7dert", "und", "Gei\u00b7er\u00b7Fah\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "KON", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.7": {"line.1": {"text": "Ja Geier sind es! Nenne nicht Adler sie,", "tokens": ["Ja", "Gei\u00b7er", "sind", "es", "!", "Nen\u00b7ne", "nicht", "Ad\u00b7ler", "sie", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "NN", "VAFIN", "PPER", "$.", "VVFIN", "PTKNEG", "NN", "PPER", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Du deutsche Zunge! Geier! und Hornisse,", "tokens": ["Du", "deut\u00b7sche", "Zun\u00b7ge", "!", "Gei\u00b7er", "!", "und", "Hor\u00b7nis\u00b7se", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$.", "NN", "$.", "KON", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Nicht Bienen sind's, die nun den Prunkschild \u2013", "tokens": ["Nicht", "Bie\u00b7nen", "sin\u00b7d's", ",", "die", "nun", "den", "Prunk\u00b7schild", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "VAFIN", "$,", "PRELS", "ADV", "ART", "NN", "$("], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Bl\u00fchender Lilien einst \u2013 umschwirren.", "tokens": ["Bl\u00fc\u00b7hen\u00b7der", "Li\u00b7li\u00b7en", "einst", "\u2013", "um\u00b7schwir\u00b7ren", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "$(", "VVINF", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}}, "stanza.8": {"line.1": {"text": "Gab Moskow's Schlitten Fl\u00fcgel dem Fliehenden?", "tokens": ["Gab", "Mos\u00b7kow's", "Schlit\u00b7ten", "Fl\u00fc\u00b7gel", "dem", "Flie\u00b7hen\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ha Xerxes Nachen! \u2013 als Er im Huy dem Heer", "tokens": ["Ha", "Xe\u00b7rxes", "Na\u00b7chen", "!", "\u2013", "als", "Er", "im", "Huy", "dem", "Heer"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "NN", "$.", "$(", "KOUS", "PPER", "APPRART", "NE", "ART", "NN"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Den R\u00fccken kehrte, Held und Klepper", "tokens": ["Den", "R\u00fc\u00b7cken", "kehr\u00b7te", ",", "Held", "und", "Klep\u00b7per"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Reichend in Angst vor des Treibers Gei\u00dfel!", "tokens": ["Rei\u00b7chend", "in", "Angst", "vor", "des", "Trei\u00b7bers", "Gei\u00b7\u00dfel", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "APPR", "ART", "NN", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.9": {"line.1": {"text": "Die Rach' erkohr Ihn! Unter des Corsen Fu\u00df", "tokens": ["Die", "Rach'", "er\u00b7kohr", "Ihn", "!", "Un\u00b7ter", "des", "Cor\u00b7sen", "Fu\u00df"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "$.", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Gestampfet, solltest b\u00fc\u00dfen du, Gallia,", "tokens": ["Ge\u00b7stamp\u00b7fet", ",", "soll\u00b7test", "b\u00fc\u00b7\u00dfen", "du", ",", "Gal\u00b7lia", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "ADV", "VVFIN", "PPER", "$,", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Das Blut der Bessern, die zum Schmaus des", "tokens": ["Das", "Blut", "der", "Bes\u00b7sern", ",", "die", "zum", "Schmaus", "des"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "$,", "PRELS", "APPRART", "NN", "ART"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Thronenden P\u00f6bels dein Mordstahl w\u00fcrgte.", "tokens": ["Thro\u00b7nen\u00b7den", "P\u00f6\u00b7bels", "dein", "Mord\u00b7stahl", "w\u00fcrg\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.10": {"line.1": {"text": "Verduftet war die W\u00fcrze des M\u00f6rderspiels,", "tokens": ["Ver\u00b7duf\u00b7tet", "war", "die", "W\u00fcr\u00b7ze", "des", "M\u00f6r\u00b7der\u00b7spiels", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Da schwoll empor Er selber die lebende \u2013", "tokens": ["Da", "schwoll", "em\u00b7por", "Er", "sel\u00b7ber", "die", "le\u00b7ben\u00b7de", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PTKVZ", "PPER", "ADV", "ART", "ADJA", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Verzeih mir's, Muse! \u2013 Guillotine,", "tokens": ["Ver\u00b7zeih", "mir's", ",", "Mu\u00b7se", "!", "\u2013", "Guil\u00b7lo\u00b7ti\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "punct", "punct", "word", "punct"], "pos": ["VVIMP", "NE", "$,", "NN", "$.", "$(", "NE", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Schleppend zur Schlachtbank auf Heerschaar Heerschaar;", "tokens": ["Schlep\u00b7pend", "zur", "Schlacht\u00b7bank", "auf", "Heer\u00b7schaar", "Heer\u00b7schaar", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "APPR", "NN", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.11": {"line.1": {"text": "Bartloser F\u00e4ntchen Schw\u00e4rme, wie Abendhauch", "tokens": ["Bart\u00b7lo\u00b7ser", "F\u00e4nt\u00b7chen", "Schw\u00e4r\u00b7me", ",", "wie", "A\u00b7bend\u00b7hauch"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "NN", "$,", "PWAV", "NN"], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Die M\u00fcckenwolke, jagend zum Acheron,", "tokens": ["Die", "M\u00fc\u00b7cken\u00b7wol\u00b7ke", ",", "ja\u00b7gend", "zum", "A\u00b7che\u00b7ron", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Von Heerd und Pflug, gleich S\u00fcdpols Wilden,", "tokens": ["Von", "Heerd", "und", "Pflug", ",", "gleich", "S\u00fcd\u00b7pols", "Wil\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,", "ADV", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "F\u00e4llend den Baum um die Frucht zu naschen.", "tokens": ["F\u00e4l\u00b7lend", "den", "Baum", "um", "die", "Frucht", "zu", "na\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.12": {"line.1": {"text": "Bist deutsch nun, Vater Rhein! doch erz\u00fcrne nicht,", "tokens": ["Bist", "deutsch", "nun", ",", "Va\u00b7ter", "Rhein", "!", "doch", "er\u00b7z\u00fcr\u00b7ne", "nicht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "ADV", "$,", "NN", "NE", "$.", "ADV", "VVFIN", "PTKNEG", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Wenn ich den Wonnebecher bei'm Kaiserfest,", "tokens": ["Wenn", "ich", "den", "Won\u00b7ne\u00b7be\u00b7cher", "bei'm", "Kai\u00b7ser\u00b7fest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Das unserm ", "tokens": ["Das", "un\u00b7serm"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Krone noch einmal die Schl\u00e4fen g\u00fcrtet,", "tokens": ["Kro\u00b7ne", "noch", "ein\u00b7mal", "die", "Schl\u00e4\u00b7fen", "g\u00fcr\u00b7tet", ","], "token_info": ["word", "