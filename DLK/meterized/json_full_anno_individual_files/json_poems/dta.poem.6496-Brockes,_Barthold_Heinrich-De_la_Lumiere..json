{"dta.poem.6496": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "De la Lumiere.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Echo spricht im Wald uns nach, wenn man sie fragt.", "tokens": ["Die", "E\u00b7cho", "spricht", "im", "Wald", "uns", "nach", ",", "wenn", "man", "sie", "fragt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "PPER", "PTKVZ", "$,", "KOUS", "PIS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So bald der Lufft unsichtbar Wallen", "tokens": ["So", "bald", "der", "Lufft", "un\u00b7sicht\u00b7bar", "Wal\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "NN", "ADJD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "An einen Vorwurff st\u00f6sst, und, im Zur\u00fccke-Prallen,", "tokens": ["An", "ei\u00b7nen", "Vor\u00b7wurff", "st\u00f6sst", ",", "und", ",", "im", "Zu\u00b7r\u00fcck\u00b7e\u00b7Pral\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$,", "KON", "$,", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die W\u00f6rter wiederbringt, die man vorher gesagt.", "tokens": ["Die", "W\u00f6r\u00b7ter", "wie\u00b7der\u00b7bringt", ",", "die", "man", "vor\u00b7her", "ge\u00b7sagt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PRELS", "PIS", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}