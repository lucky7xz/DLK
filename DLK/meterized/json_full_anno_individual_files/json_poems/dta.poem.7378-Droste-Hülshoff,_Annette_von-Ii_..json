{"dta.poem.7378": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Ii .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-20090519994", "language": ["de:0.99"], "booktitle": "Droste-H\u00fclshoff, Annette von: Gedichte. Stuttgart u. a., 1844."}, "poem": {"stanza.1": {"line.1": {"text": "Wie rollt in den Gassen das Marktgebraus!", "tokens": ["Wie", "rollt", "in", "den", "Gas\u00b7sen", "das", "Markt\u00b7ge\u00b7braus", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Welch ein Get\u00fcmmel, Geblitze!", "tokens": ["Welch", "ein", "Ge\u00b7t\u00fcm\u00b7mel", ",", "Ge\u00b7blit\u00b7ze", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PIAT", "ART", "NN", "$,", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Hanswurst schaut \u00fcber die Bude hinaus,", "tokens": ["Hans\u00b7wurst", "schaut", "\u00fc\u00b7ber", "die", "Bu\u00b7de", "hin\u00b7aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "APZR", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und winkt mit der klingelnden M\u00fctze;", "tokens": ["Und", "winkt", "mit", "der", "klin\u00b7geln\u00b7den", "M\u00fct\u00b7ze", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Karossen rasseln, der Trinker jucht,", "tokens": ["Ka\u00b7ros\u00b7sen", "ras\u00b7seln", ",", "der", "Trin\u00b7ker", "jucht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Und M\u00e4dchen schrein im Gedr\u00e4nge,", "tokens": ["Und", "M\u00e4d\u00b7chen", "schrein", "im", "Ge\u00b7dr\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Drehorgeln pfeifen, der K\u00e4rrner flucht,", "tokens": ["Dre\u00b7hor\u00b7geln", "pfei\u00b7fen", ",", "der", "K\u00e4rr\u00b7ner", "flucht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "O Babels w\u00fcrdige Kl\u00e4nge!", "tokens": ["O", "Ba\u00b7bels", "w\u00fcr\u00b7di\u00b7ge", "Kl\u00e4n\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Da tritt ein Weib aus der Ladenth\u00fcr,", "tokens": ["Da", "tritt", "ein", "Weib", "aus", "der", "La\u00b7dent\u00b7h\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Eine schlichte Frau von den Fl\u00fchen,", "tokens": ["Ei\u00b7ne", "schlich\u00b7te", "Frau", "von", "den", "Fl\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Die stie\u00df an den klingelnden Harlekin schier,", "tokens": ["Die", "stie\u00df", "an", "den", "klin\u00b7geln\u00b7den", "Har\u00b7le\u00b7kin", "schier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "ART", "ADJA", "NN", "ADJD", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Und hat nicht gelacht noch geschrien.", "tokens": ["Und", "hat", "nicht", "ge\u00b7lacht", "noch", "ge\u00b7schri\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PTKNEG", "VVPP", "ADV", "VVPP", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Ihr mattes Auge sucht auf dem Grund,", "tokens": ["Ihr", "mat\u00b7tes", "Au\u00b7ge", "sucht", "auf", "dem", "Grund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Als habe sie Etwas verloren,", "tokens": ["Als", "ha\u00b7be", "sie", "Et\u00b7was", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.7": {"text": "Und hinter ihr trabt ein zottiger Hund,", "tokens": ["Und", "hin\u00b7ter", "ihr", "trabt", "ein", "zot\u00b7ti\u00b7ger", "Hund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Verdutzt, mit h\u00e4ngenden Ohren.", "tokens": ["Ver\u00b7dutzt", ",", "mit", "h\u00e4n\u00b7gen\u00b7den", "Oh\u00b7ren", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "APPR", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "\u201ezur\u00fcck, Verwegne! siehst du denn nicht", "tokens": ["\u201e", "zu\u00b7r\u00fcck", ",", "Ver\u00b7weg\u00b7ne", "!", "siehst", "du", "denn", "nicht"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "PTKVZ", "$,", "NN", "$.", "VVFIN", "PPER", "ADV", "PTKNEG"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Den Wagen, die schnaubenden Braunen?\u201c", "tokens": ["Den", "Wa\u00b7gen", ",", "die", "schnau\u00b7ben\u00b7den", "Brau\u00b7nen", "?", "\u201c"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Schon dampfen die N\u00fcstern ihr am Gesicht,", "tokens": ["Schon", "damp\u00b7fen", "die", "N\u00fcs\u00b7tern", "ihr", "am", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPER", "APPRART", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Da f\u00e4hrt sie zur\u00fcck mit Staunen,", "tokens": ["Da", "f\u00e4hrt", "sie", "zu\u00b7r\u00fcck", "mit", "Stau\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "APPR", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Und ist noch \u00fcber die Rinne grad", "tokens": ["Und", "ist", "noch", "\u00fc\u00b7ber", "die", "Rin\u00b7ne", "grad"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "ADV", "APPR", "ART", "NN", "ADV"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Mit raschem Sprunge gewichen,", "tokens": ["Mit", "ra\u00b7schem", "Sprun\u00b7ge", "ge\u00b7wi\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Als an die Sch\u00fcrze das klirrende Rad", "tokens": ["Als", "an", "die", "Sch\u00fcr\u00b7ze", "das", "klir\u00b7ren\u00b7de", "Rad"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "In wirbelndem Schwunge gestrichen.", "tokens": ["In", "wir\u00b7beln\u00b7dem", "Schwun\u00b7ge", "ge\u00b7stri\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.4": {"line.1": {"text": "Noch ein Moment, \u2014 sie taumelt, erbleicht,", "tokens": ["Noch", "ein", "Mo\u00b7ment", ",", "sie", "tau\u00b7melt", ",", "er\u00b7bleicht", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "$(", "PPER", "VVFIN", "$,", "VVPP", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Und dann ein pl\u00f6tzlich Ergl\u00fchen,", "tokens": ["Und", "dann", "ein", "pl\u00f6tz\u00b7lich", "Er\u00b7gl\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "ADJD", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "O schau, wie durch das Gew\u00fchl sie keucht,", "tokens": ["O", "schau", ",", "wie", "durch", "das", "Ge\u00b7w\u00fchl", "sie", "keucht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$,", "PWAV", "APPR", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Mit Armen und H\u00e4nden und Knieen!", "tokens": ["Mit", "Ar\u00b7men", "und", "H\u00e4n\u00b7den", "und", "Kni\u00b7e\u00b7en", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "KON", "NN", "$."], "meter": "-+--+--+--", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Sie rudert, sie windet sich, \u2014 Sto\u00df auf Sto\u00df,", "tokens": ["Sie", "ru\u00b7dert", ",", "sie", "win\u00b7det", "sich", ",", "Sto\u00df", "auf", "Sto\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "PRF", "$,", "$(", "NN", "APPR", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Scheltworte und Fl\u00fcche wie Schlossen \u2014", "tokens": ["Schelt\u00b7wor\u00b7te", "und", "Fl\u00fc\u00b7che", "wie", "Schlos\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "KOKOM", "NN", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.7": {"text": "Das F\u00fcrtuch rei\u00dft, dann flattert es los,", "tokens": ["Das", "F\u00fcr\u00b7tuch", "rei\u00dft", ",", "dann", "flat\u00b7tert", "es", "los", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.8": {"text": "Und ist in die Rinne geflossen.", "tokens": ["Und", "ist", "in", "die", "Rin\u00b7ne", "ge\u00b7flos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.5": {"line.1": {"text": "Nun steht sie vor einem stattlichen Haus,", "tokens": ["Nun", "steht", "sie", "vor", "ei\u00b7nem", "statt\u00b7li\u00b7chen", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ohne Schuh, besudelt mit Kothe;", "tokens": ["Oh\u00b7ne", "Schuh", ",", "be\u00b7su\u00b7delt", "mit", "Ko\u00b7the", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "VVFIN", "APPR", "NE", "$."], "meter": "+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Dort h\u00e4lt die Karosse, dort schnauben aus", "tokens": ["Dort", "h\u00e4lt", "die", "Ka\u00b7ros\u00b7se", ",", "dort", "schnau\u00b7ben", "aus"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "ADV", "VVFIN", "PTKVZ"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die Braunen und rauchen wie Schlote.", "tokens": ["Die", "Brau\u00b7nen", "und", "rau\u00b7chen", "wie", "Schlo\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "VVFIN", "KOKOM", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Der Schlag ist offen, und eben sieht", "tokens": ["Der", "Schlag", "ist", "of\u00b7fen", ",", "und", "e\u00b7ben", "sieht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "KON", "ADV", "VVFIN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Sie im Portale verschwinden", "tokens": ["Sie", "im", "Por\u00b7ta\u00b7le", "ver\u00b7schwin\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "APPRART", "NN", "VVINF"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.7": {"text": "Eines Kleides Falte, die purpurn gl\u00fcht,", "tokens": ["Ei\u00b7nes", "Klei\u00b7des", "Fal\u00b7te", ",", "die", "pur\u00b7purn", "gl\u00fcht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ADJD", "VVFIN", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.8": {"text": "Und den Schleyer, segelnd in Winden.", "tokens": ["Und", "den", "Schle\u00b7yer", ",", "se\u00b7gelnd", "in", "Win\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "VVPP", "APPR", "NN", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}}, "stanza.6": {"line.1": {"text": "\u201each\u201c fl\u00fcstert Gertrude, \u201ewas hab ich gemacht,", "tokens": ["\u201e", "ach", "\u201c", "fl\u00fcs\u00b7tert", "Ger\u00b7tru\u00b7de", ",", "\u201e", "was", "hab", "ich", "ge\u00b7macht", ","], "token_info": ["punct", "word", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "XY", "$(", "VVFIN", "NN", "$,", "$(", "PWS", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+--+-++--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ich bin wohl verr\u00fcckt geworden!", "tokens": ["Ich", "bin", "wohl", "ver\u00b7r\u00fcckt", "ge\u00b7wor\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "VAPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Kein Trost bei Tag, keine Ruh bei Nacht,", "tokens": ["Kein", "Trost", "bei", "Tag", ",", "kei\u00b7ne", "Ruh", "bei", "Nacht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "NN", "$,", "PIAT", "NN", "APPR", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Das kann die Sinne schon morden.\u201c", "tokens": ["Das", "kann", "die", "Sin\u00b7ne", "schon", "mor\u00b7den", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "VMFIN", "ART", "NN", "ADV", "VVINF", "$.", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Da poltert es schreiend die Stiegen hinab,", "tokens": ["Da", "pol\u00b7tert", "es", "schrei\u00b7end", "die", "Stie\u00b7gen", "hin\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "ART", "NN", "PTKVZ", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Ein Fu\u00dftritt aus dem Portale,", "tokens": ["Ein", "Fu\u00df\u00b7tritt", "aus", "dem", "Por\u00b7ta\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Und wimmernd rollt von der Rampe herab", "tokens": ["Und", "wim\u00b7mernd", "rollt", "von", "der", "Ram\u00b7pe", "her\u00b7ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "APPR", "ART", "NN", "ADV"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Ihr Hund, der zottige, fahle.", "tokens": ["Ihr", "Hund", ",", "der", "zot\u00b7ti\u00b7ge", ",", "fah\u00b7le", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ART", "ADJA", "$,", "VVFIN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.7": {"line.1": {"text": "\u201eja\u201c seufzt Gertrude, \u201enun ist es klar,", "tokens": ["\u201e", "ja", "\u201c", "seufzt", "Ger\u00b7tru\u00b7de", ",", "\u201e", "nun", "ist", "es", "klar", ","], "token_info": ["punct", "word", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PTKANT", "$(", "VVFIN", "NN", "$,", "$(", "ADV", "VAFIN", "PPER", "ADJD", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ich bin eine Irre leider!\u201c", "tokens": ["Ich", "bin", "ei\u00b7ne", "Ir\u00b7re", "lei\u00b7der", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADV", "$.", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ergl\u00fchend streicht sie zur\u00fcck ihr Haar,", "tokens": ["Er\u00b7gl\u00fc\u00b7hend", "streicht", "sie", "zu\u00b7r\u00fcck", "ihr", "Haar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "PPER", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und ordnet die staubigen Kleider.", "tokens": ["Und", "ord\u00b7net", "die", "stau\u00b7bi\u00b7gen", "Klei\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "\u201ewie sah ich so deutlich ihr liebes Gesicht,", "tokens": ["\u201e", "wie", "sah", "ich", "so", "deut\u00b7lich", "ihr", "lie\u00b7bes", "Ge\u00b7sicht", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "VVFIN", "PPER", "ADV", "ADJD", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "So deutlich am Schlage doch ragen!", "tokens": ["So", "deut\u00b7lich", "am", "Schla\u00b7ge", "doch", "ra\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPRART", "NN", "ADV", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.7": {"text": "Allein in Ewigkeit h\u00e4tte ", "tokens": ["Al\u00b7lein", "in", "E\u00b7wig\u00b7keit", "h\u00e4t\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "NN", "VAFIN"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.8": {"text": "Den armen Fidel geschlagen.\u201c", "tokens": ["Den", "ar\u00b7men", "Fi\u0