{"dta.poem.21344": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Caput XI.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-30602-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "In unserem Vaterland herrschten jetzt", "tokens": ["In", "un\u00b7se\u00b7rem", "Va\u00b7ter\u00b7land", "herrschten", "jetzt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ADV"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Nur r\u00f6mische Sprache und Sitten,", "tokens": ["Nur", "r\u00f6\u00b7mi\u00b7sche", "Spra\u00b7che", "und", "Sit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Vestalen g\u00e4b\u2019 es in M\u00fcnchen sogar,", "tokens": ["Ves\u00b7ta\u00b7len", "g\u00e4b'", "es", "in", "M\u00fcn\u00b7chen", "so\u00b7gar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPR", "NE", "ADV", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die Schwaben hie\u00dfen Quiriten!", "tokens": ["Die", "Schwa\u00b7ben", "hie\u00b7\u00dfen", "Qui\u00b7ri\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Der Hengstenberg w\u00e4r\u2019 ein Haruspex", "tokens": ["Der", "Hengs\u00b7ten\u00b7berg", "w\u00e4r'", "ein", "Ha\u00b7rus\u00b7pex"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NE", "VAFIN", "ART", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und gr\u00fcbelte in den Ged\u00e4rmen", "tokens": ["Und", "gr\u00fc\u00b7bel\u00b7te", "in", "den", "Ge\u00b7d\u00e4r\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Von Ochsen. Neander w\u00e4r\u2019 ein Augur,", "tokens": ["Von", "Och\u00b7sen", ".", "Ne\u00b7an\u00b7der", "w\u00e4r'", "ein", "Au\u00b7gur", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$.", "NE", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und schaute nach V\u00f6gelschw\u00e4rmen.", "tokens": ["Und", "schau\u00b7te", "nach", "V\u00f6\u00b7gel\u00b7schw\u00e4r\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Birch-Pfeifer s\u00f6ffe Terpentin,", "tokens": ["Birch\u00b7Pfei\u00b7fer", "s\u00f6f\u00b7fe", "Ter\u00b7pen\u00b7tin", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie einst die r\u00f6mischen Damen.", "tokens": ["Wie", "einst", "die", "r\u00f6\u00b7mi\u00b7schen", "Da\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "(man sagt, da\u00df sie dadurch den Urin", "tokens": ["(", "man", "sagt", ",", "da\u00df", "sie", "da\u00b7durch", "den", "U\u00b7rin"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["$(", "PIS", "VVFIN", "$,", "KOUS", "PPER", "PAV", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Besonders wohlriechend bekamen.)", "tokens": ["Be\u00b7son\u00b7ders", "wohl\u00b7rie\u00b7chend", "be\u00b7ka\u00b7men", ".", ")"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "$.", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.4": {"line.1": {"text": "Der Raumer w\u00e4re kein deutscher Lump,", "tokens": ["Der", "Rau\u00b7mer", "w\u00e4\u00b7re", "kein", "deut\u00b7scher", "Lump", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Er w\u00e4re ein r\u00f6m\u2019scher Lumpazius.", "tokens": ["Er", "w\u00e4\u00b7re", "ein", "r\u00f6m'\u00b7scher", "Lum\u00b7pa\u00b7zius", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NE", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Der Freiligrath dichtete ohne Reim,", "tokens": ["Der", "Frei\u00b7li\u00b7gra\u00b7th", "dich\u00b7te\u00b7te", "oh\u00b7ne", "Reim", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Wie weiland Flaccus Horazius.", "tokens": ["Wie", "wei\u00b7land", "Flac\u00b7cus", "Ho\u00b7ra\u00b7zi\u00b7us", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "NE", "NE", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}}}}