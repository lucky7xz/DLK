{"dta.poem.3923": {"metadata": {"author": {"name": "Spindler, Christian Gotthold", "birth": "N.A.", "death": "N.A."}, "title": "\u00a7. 1.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-20581-9", "language": ["de:0.99"], "booktitle": "Spindler, Christian Gotthold: Unschuldige Jugend-Fr\u00fcchte. Leipzig, 1745."}, "poem": {"stanza.1": {"line.1": {"text": "Ein M\u00e4dgen, ob es fchon gleich noch so spr\u00f6te thut,", "tokens": ["Ein", "M\u00e4d\u00b7gen", ",", "ob", "es", "fchon", "gleich", "noch", "so", "spr\u00f6\u00b7te", "thut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "PPER", "ADV", "ADV", "ADV", "ADV", "VVFIN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ist doch der L\u00f6ffeley von Grund der Seelen gut,", "tokens": ["Ist", "doch", "der", "L\u00f6f\u00b7fe\u00b7ley", "von", "Grund", "der", "See\u00b7len", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "APPR", "NN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und wer sich daran kehrt, wenn sie ihm widerstreben,", "tokens": ["Und", "wer", "sich", "da\u00b7ran", "kehrt", ",", "wenn", "sie", "ihm", "wi\u00b7der\u00b7stre\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PRF", "PAV", "VVFIN", "$,", "KOUS", "PPER", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So sprechen sie wohl selbst: der Mensch wei\u00df nicht", "tokens": ["So", "spre\u00b7chen", "sie", "wohl", "selbst", ":", "der", "Mensch", "wei\u00df", "nicht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "$.", "ART", "NN", "VVFIN", "PTKNEG"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "zu leben/", "tokens": ["zu", "le\u00b7ben", "/"], "token_info": ["word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$("], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "Er sieht so h\u00f6ltzern aus, lernt doch den groben Knoll,", "tokens": ["Er", "sieht", "so", "h\u00f6lt\u00b7zern", "aus", ",", "lernt", "doch", "den", "gro\u00b7ben", "Knoll", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "PTKVZ", "$,", "VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wie man in Compagnie mit Jungfern leben soll.", "tokens": ["Wie", "man", "in", "Com\u00b7pag\u00b7nie", "mit", "Jung\u00b7fern", "le\u00b7ben", "soll", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "APPR", "NN", "APPR", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}