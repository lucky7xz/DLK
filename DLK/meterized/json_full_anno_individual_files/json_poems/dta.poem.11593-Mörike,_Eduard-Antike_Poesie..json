{"dta.poem.11593": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Antike Poesie.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905193969", "language": ["de:0.99"], "booktitle": "M\u00f6rike, Eduard: Gedichte. Stuttgart, 1838."}, "poem": {"stanza.1": {"line.1": {"text": "Nur kaum ber\u00fchrt vom ersten Sonnenstrahle;", "tokens": ["Nur", "kaum", "be\u00b7r\u00fchrt", "vom", "ers\u00b7ten", "Son\u00b7nen\u00b7strah\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVPP", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Schau! jetzo stehen hoch mit einem Male", "tokens": ["Schau", "!", "jet\u00b7zo", "ste\u00b7hen", "hoch", "mit", "ei\u00b7nem", "Ma\u00b7le"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$.", "ADV", "VVFIN", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Gipfel dort in Morgenr\u00f6the-Brunst.", "tokens": ["Die", "Gip\u00b7fel", "dort", "in", "Mor\u00b7gen\u00b7r\u00f6\u00b7the\u00b7Brunst", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Doch unten spricht von holder Musen Gunst", "tokens": ["Doch", "un\u00b7ten", "spricht", "von", "hol\u00b7der", "Mu\u00b7sen", "Gunst"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "APPR", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der heil'ge Quell im dunkelgr\u00fcnen Thale:", "tokens": ["Der", "heil'\u00b7ge", "Quell", "im", "dun\u00b7kel\u00b7gr\u00fc\u00b7nen", "Tha\u00b7le", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wer aber sch\u00f6pft mit reiner Opferschale,", "tokens": ["Wer", "a\u00b7ber", "sch\u00f6pft", "mit", "rei\u00b7ner", "Op\u00b7fer\u00b7scha\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wie einst, den \u00e4chten Thau der alten Kunst?", "tokens": ["Wie", "einst", ",", "den", "\u00e4ch\u00b7ten", "Thau", "der", "al\u00b7ten", "Kunst", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "$,", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wie? soll ich endlich keinen Meister sehn?", "tokens": ["Wie", "?", "soll", "ich", "end\u00b7lich", "kei\u00b7nen", "Meis\u00b7ter", "sehn", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "VMFIN", "PPER", "ADV", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Will keiner mehr den alten Lorbeer pfl\u00fccken?", "tokens": ["Will", "kei\u00b7ner", "mehr", "den", "al\u00b7ten", "Lor\u00b7beer", "pfl\u00fc\u00b7cken", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "ADV", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "\u2014 Da sah ich Iphigeniens Dichter stehn:", "tokens": ["Da", "sah", "ich", "Ip\u00b7hi\u00b7ge\u00b7ni\u00b7ens", "Dich\u00b7ter", "stehn", ":"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPER", "NE", "NN", "VVINF", "$."], "meter": "+-+--+-+--+", "measure": "trochaic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Er ist's, an dessen Blick sich diese H\u00f6hn", "tokens": ["Er", "ist's", ",", "an", "des\u00b7sen", "Blick", "sich", "die\u00b7se", "H\u00f6hn"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "$,", "APPR", "PRELAT", "NN", "PRF", "PDAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "So zauberhaft, so sonnewarm erquicken.", "tokens": ["So", "zau\u00b7ber\u00b7haft", ",", "so", "son\u00b7ne\u00b7warm", "er\u00b7qui\u00b7cken", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Er geht, und frostig rauhe L\u00fcfte weh'n.", "tokens": ["Er", "geht", ",", "und", "fros\u00b7tig", "rau\u00b7he", "L\u00fcf\u00b7te", "weh'", "n."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["PPER", "VVFIN", "$,", "KON", "ADJD", "ADJA", "NN", "VVFIN", "NE"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}