{"dta.poem.19698": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Wilhelm Tell .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519168", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wilhelm bin ich der Telle,               ", "tokens": ["Wil\u00b7helm", "bin", "ich", "der", "Tel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Von Heldenmuth und Blut,", "tokens": ["Von", "Hel\u00b7den\u00b7muth", "und", "Blut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mit meinem G'scho\u00df und Pfeile", "tokens": ["Mit", "mei\u00b7nem", "G'\u00b7scho\u00df", "und", "Pfei\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "KON", "NN"], "meter": "-+-++-+-", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Hab ich die Freiheit gut", "tokens": ["Hab", "ich", "die", "Frei\u00b7heit", "gut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "ART", "NN", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Dem Vaterland erworben,", "tokens": ["Dem", "Va\u00b7ter\u00b7land", "er\u00b7wor\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Vertrieben Tyranney,", "tokens": ["Ver\u00b7trie\u00b7ben", "Ty\u00b7ran\u00b7ney", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "NE", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Einen festen Bund geschworen", "tokens": ["Ei\u00b7nen", "fes\u00b7ten", "Bund", "ge\u00b7schwo\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Haben unsre Gesellen drey.", "tokens": ["Ha\u00b7ben", "uns\u00b7re", "Ge\u00b7sel\u00b7len", "drey", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "CARD", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.2": {"line.1": {"text": "Uri, Schweiz und Unterwald,", "tokens": ["U\u00b7ri", ",", "Schweiz", "und", "Un\u00b7ter\u00b7wald", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NE", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Befreiet von dem Reich,", "tokens": ["Be\u00b7frei\u00b7et", "von", "dem", "Reich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Litten gro\u00dfen Zwang und Gewalt", "tokens": ["Lit\u00b7ten", "gro\u00b7\u00dfen", "Zwang", "und", "Ge\u00b7walt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADJA", "NN", "KON", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Von V\u00f6gten unbillig.", "tokens": ["Von", "V\u00f6g\u00b7ten", "un\u00b7bil\u00b7lig", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Kein Landmann durft nicht sprechen,", "tokens": ["Kein", "Land\u00b7mann", "durft", "nicht", "spre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Dies ist mein eigen Gut,", "tokens": ["Dies", "ist", "mein", "ei\u00b7gen", "Gut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Man nahm ihm also freche", "tokens": ["Man", "nahm", "ihm", "al\u00b7so", "fre\u00b7che"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "PPER", "ADV", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die Ochsen von dem Pflug.", "tokens": ["Die", "Och\u00b7sen", "von", "dem", "Pflug", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Dem der sich wollte r\u00e4chen,", "tokens": ["Dem", "der", "sich", "woll\u00b7te", "r\u00e4\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PRELS", "PRF", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und stellen in die Wehr,", "tokens": ["Und", "stel\u00b7len", "in", "die", "Wehr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Th\u00e4t man die Augen ausstechen,", "tokens": ["Th\u00e4t", "man", "die", "Au\u00b7gen", "aus\u00b7ste\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ART", "NN", "VVIZU", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Und h\u00f6rte Bosheit mehr.", "tokens": ["Und", "h\u00f6r\u00b7te", "Bos\u00b7heit", "mehr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Zu Altdorf bei der Linden", "tokens": ["Zu", "Alt\u00b7dorf", "bei", "der", "Lin\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "APPR", "ART", "NE"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der Vogt steckt auf sein Hut,", "tokens": ["Der", "Vogt", "steckt", "auf", "sein", "Hut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Er sprach, den will ich finden,", "tokens": ["Er", "sprach", ",", "den", "will", "ich", "fin\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ART", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Der ihm kein Ehr' anthut.", "tokens": ["Der", "ihm", "kein", "Ehr'", "an\u00b7thut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PIAT", "NN", "VVFIN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}}}}