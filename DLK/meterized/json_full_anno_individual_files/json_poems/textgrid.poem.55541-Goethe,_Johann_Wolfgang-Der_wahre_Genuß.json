{"textgrid.poem.55541": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Der wahre Genu\u00df", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Umsonst, da\u00df du, ein Herz zu lenken,", "tokens": ["Um\u00b7sonst", ",", "da\u00df", "du", ",", "ein", "Herz", "zu", "len\u00b7ken", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PPER", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Des M\u00e4dchens Scho\u00df mit Golde f\u00fcllst.", "tokens": ["Des", "M\u00e4d\u00b7chens", "Scho\u00df", "mit", "Gol\u00b7de", "f\u00fcllst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "O F\u00fcrst, la\u00df dir die Wollust schenken,", "tokens": ["O", "F\u00fcrst", ",", "la\u00df", "dir", "die", "Wol\u00b7lust", "schen\u00b7ken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "VVIMP", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn du sie wahr empfinden willst.", "tokens": ["Wenn", "du", "sie", "wahr", "emp\u00b7fin\u00b7den", "willst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADJD", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Gold kauft die Zunge ganzer Haufen,", "tokens": ["Gold", "kauft", "die", "Zun\u00b7ge", "gan\u00b7zer", "Hau\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Kein einzig Herz erwirbt es dir;", "tokens": ["Kein", "ein\u00b7zig", "Herz", "er\u00b7wirbt", "es", "dir", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJD", "NN", "VVFIN", "PPER", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Doch willst du eine Tugend kaufen,", "tokens": ["Doch", "willst", "du", "ei\u00b7ne", "Tu\u00b7gend", "kau\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "So geh und gib dein Herz daf\u00fcr.", "tokens": ["So", "geh", "und", "gib", "dein", "Herz", "da\u00b7f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KON", "VVIMP", "PPOSAT", "NN", "PAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Was ist die Lust, die in den Armen", "tokens": ["Was", "ist", "die", "Lust", ",", "die", "in", "den", "Ar\u00b7men"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "ART", "NN", "$,", "PRELS", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Buhlerin die Wollust schafft?", "tokens": ["Der", "Buh\u00b7le\u00b7rin", "die", "Wol\u00b7lust", "schafft", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du w\u00e4rst ein Vorwurf zum Erbarmen,", "tokens": ["Du", "w\u00e4rst", "ein", "Vor\u00b7wurf", "zum", "Er\u00b7bar\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Tor, w\u00e4rst du nicht lasterhaft.", "tokens": ["Ein", "Tor", ",", "w\u00e4rst", "du", "nicht", "las\u00b7ter\u00b7haft", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VAFIN", "PPER", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sie k\u00fcsset dich aus feilem Triebe,", "tokens": ["Sie", "k\u00fcs\u00b7set", "dich", "aus", "fei\u00b7lem", "Trie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und Glut nach Gold f\u00fcllt ihr Gesicht.", "tokens": ["Und", "Glut", "nach", "Gold", "f\u00fcllt", "ihr", "Ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ungl\u00fccklicher! Du f\u00fchlst nicht Liebe,", "tokens": ["Un\u00b7gl\u00fcck\u00b7li\u00b7cher", "!", "Du", "f\u00fchlst", "nicht", "Lie\u00b7be", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPER", "VVFIN", "PTKNEG", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Sogar die Wollust f\u00fchlst du nicht.", "tokens": ["So\u00b7gar", "die", "Wol\u00b7lust", "f\u00fchlst", "du", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Sei ohne Tugend, doch verliere", "tokens": ["Sei", "oh\u00b7ne", "Tu\u00b7gend", ",", "doch", "ver\u00b7lie\u00b7re"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VAFIN", "APPR", "NN", "$,", "ADV", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Den Vorzug eines Menschen nie!", "tokens": ["Den", "Vor\u00b7zug", "ei\u00b7nes", "Men\u00b7schen", "nie", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Denn Wollust f\u00fchlen alle Tiere,", "tokens": ["Denn", "Wol\u00b7lust", "f\u00fch\u00b7len", "al\u00b7le", "Tie\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Mensch allein verfeinert sie.", "tokens": ["Der", "Mensch", "al\u00b7lein", "ver\u00b7fei\u00b7nert", "sie", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "La\u00df dich die Lehren nicht verdrie\u00dfen,", "tokens": ["La\u00df", "dich", "die", "Leh\u00b7ren", "nicht", "ver\u00b7drie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Sie hindern dich nicht am Genu\u00df,", "tokens": ["Sie", "hin\u00b7dern", "dich", "nicht", "am", "Ge\u00b7nu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "APPRART", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.7": {"text": "Sie lehren dich, wie man genie\u00dfen", "tokens": ["Sie", "leh\u00b7ren", "dich", ",", "wie", "man", "ge\u00b7nie\u00b7\u00dfen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PWAV", "PIS", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und Wollust w\u00fcrdig f\u00fchlen mu\u00df.", "tokens": ["Und", "Wol\u00b7lust", "w\u00fcr\u00b7dig", "f\u00fch\u00b7len", "mu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADJD", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Soll dich kein heilig Band umgeben,", "tokens": ["Soll", "dich", "kein", "hei\u00b7lig", "Band", "um\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PIAT", "ADJD", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "O J\u00fcngling, schr\u00e4nke selbst dich ein.", "tokens": ["O", "J\u00fcng\u00b7ling", ",", "schr\u00e4n\u00b7ke", "selbst", "dich", "ein", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "VVFIN", "ADV", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Man kann in wahrer Freiheit leben", "tokens": ["Man", "kann", "in", "wah\u00b7rer", "Frei\u00b7heit", "le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VMFIN", "APPR", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und doch nicht ungebunden sein.", "tokens": ["Und", "doch", "nicht", "un\u00b7ge\u00b7bun\u00b7den", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PTKNEG", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "La\u00df nur f\u00fcr ", "tokens": ["La\u00df", "nur", "f\u00fcr"], "token_info": ["word", "word", "word"], "pos": ["VVIMP", "ADV", "APPR"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Und ist ihr Herz von Liebe voll,", "tokens": ["Und", "ist", "ihr", "Herz", "von", "Lie\u00b7be", "voll", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPOSAT", "NN", "APPR", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "So la\u00df die Z\u00e4rtlichkeit dich binden,", "tokens": ["So", "la\u00df", "die", "Z\u00e4rt\u00b7lich\u00b7keit", "dich", "bin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Wenn dich die Pflicht nicht binden soll.", "tokens": ["Wenn", "dich", "die", "Pflicht", "nicht", "bin\u00b7den", "soll", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Empfinde, J\u00fcngling, und dann w\u00e4hle", "tokens": ["Emp\u00b7fin\u00b7de", ",", "J\u00fcng\u00b7ling", ",", "und", "dann", "w\u00e4h\u00b7le"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "KON", "ADV", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein M\u00e4dchen dir, sie w\u00e4hle dich,", "tokens": ["Ein", "M\u00e4d\u00b7chen", "dir", ",", "sie", "w\u00e4h\u00b7le", "dich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "$,", "PPER", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Von K\u00f6rper sch\u00f6n und sch\u00f6n von Seele,", "tokens": ["Von", "K\u00f6r\u00b7per", "sch\u00f6n", "und", "sch\u00f6n", "von", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "KON", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und dann bist du begl\u00fcckt wie ich!", "tokens": ["Und", "dann", "bist", "du", "be\u00b7gl\u00fcckt", "wie", "ich", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "VVFIN", "KOKOM", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich, der ich diese Kunst verstehe,", "tokens": ["Ich", ",", "der", "ich", "die\u00b7se", "Kunst", "ver\u00b7ste\u00b7he", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "PPER", "PDAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ich habe mir ein Kind gew\u00e4hlt,", "tokens": ["Ich", "ha\u00b7be", "mir", "ein", "Kind", "ge\u00b7w\u00e4hlt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Da\u00df uns zum Gl\u00fcck der sch\u00f6nsten Ehe", "tokens": ["Da\u00df", "uns", "zum", "Gl\u00fcck", "der", "sch\u00f6ns\u00b7ten", "E\u00b7he"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPRART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Allein des Priesters Segen fehlt.", "tokens": ["Al\u00b7lein", "des", "Pries\u00b7ters", "Se\u00b7gen", "fehlt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "F\u00fcr nichts besorgt als meine Freude,", "tokens": ["F\u00fcr", "nichts", "be\u00b7sorgt", "als", "mei\u00b7ne", "Freu\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "ADJD", "KOKOM", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "F\u00fcr mich nur sch\u00f6n zu sein bem\u00fcht,", "tokens": ["F\u00fcr", "mich", "nur", "sch\u00f6n", "zu", "sein", "be\u00b7m\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "ADJD", "PTKZU", "VAINF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Woll\u00fcstig nur an meiner Seite", "tokens": ["Wol\u00b7l\u00fcs\u00b7tig", "nur", "an", "mei\u00b7ner", "Sei\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "ADV", "APPR", "PPOSAT", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Und sittsam, wenn die Welt sie sieht.", "tokens": ["Und", "sitt\u00b7sam", ",", "wenn", "die", "Welt", "sie", "sieht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "KOUS", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da\u00df unsrer Glut die Zeit nicht schade,", "tokens": ["Da\u00df", "uns\u00b7rer", "Glut", "die", "Zeit", "nicht", "scha\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ART", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "R\u00e4umt sie kein Recht aus Schwachheit ein,", "tokens": ["R\u00e4umt", "sie", "kein", "Recht", "aus", "Schwach\u00b7heit", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIAT", "NN", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und ihre Gunst bleibt immer Gnade,", "tokens": ["Und", "ih\u00b7re", "Gunst", "bleibt", "im\u00b7mer", "Gna\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ADV", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und ich mu\u00df immer dankbar sein.", "tokens": ["Und", "ich", "mu\u00df", "im\u00b7mer", "dank\u00b7bar", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "ADV", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Ich bin gen\u00fcgsam und genie\u00dfe", "tokens": ["Ich", "bin", "ge\u00b7n\u00fcg\u00b7sam", "und", "ge\u00b7nie\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADJD", "KON", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Schon da, wenn sie mir z\u00e4rtlich lacht,", "tokens": ["Schon", "da", ",", "wenn", "sie", "mir", "z\u00e4rt\u00b7lich", "lacht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "KOUS", "PPER", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn sie beim Tisch des Liebsten F\u00fc\u00dfe", "tokens": ["Wenn", "sie", "beim", "Tisch", "des", "Liebs\u00b7ten", "F\u00fc\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPRART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zum Schemel ihrer F\u00fc\u00dfe macht,", "tokens": ["Zum", "Sche\u00b7mel", "ih\u00b7rer", "F\u00fc\u00b7\u00dfe", "macht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Den Apfel, den sie angebissen,", "tokens": ["Den", "Ap\u00b7fel", ",", "den", "sie", "an\u00b7ge\u00b7bis\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Das Glas, woraus sie trank, mir reicht", "tokens": ["Das", "Glas", ",", "wo\u00b7raus", "sie", "trank", ",", "mir", "reicht"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "PWAV", "PPER", "VVFIN", "$,", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und mir bei halb geraubten K\u00fcssen", "tokens": ["Und", "mir", "bei", "halb", "ge\u00b7raub\u00b7ten", "K\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "APPR", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Den sonst verdeckten Busen zeigt.", "tokens": ["Den", "sonst", "ver\u00b7deck\u00b7ten", "Bu\u00b7sen", "zeigt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Wenn in gesellschaftlicher Stunde", "tokens": ["Wenn", "in", "ge\u00b7sell\u00b7schaft\u00b7li\u00b7cher", "Stun\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sie einst mit mir von Liebe spricht,", "tokens": ["Sie", "einst", "mit", "mir", "von", "Lie\u00b7be", "spricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "W\u00fcnsch ich nur Worte von dem Munde,", "tokens": ["W\u00fcnsch", "ich", "nur", "Wor\u00b7te", "von", "dem", "Mun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nur Worte, K\u00fcsse w\u00fcnsch ich nicht.", "tokens": ["Nur", "Wor\u00b7te", ",", "K\u00fcs\u00b7se", "w\u00fcnsch", "ich", "nicht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "$,", "NN", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Welch ein Verstand, der sie beseelet,", "tokens": ["Welch", "ein", "Ver\u00b7stand", ",", "der", "sie", "be\u00b7see\u00b7let", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Mit immer neuem Reiz umgibt!", "tokens": ["Mit", "im\u00b7mer", "neu\u00b7em", "Reiz", "um\u00b7gibt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Sie ist vollkommen, und sie fehlet", "tokens": ["Sie", "ist", "voll\u00b7kom\u00b7men", ",", "und", "sie", "feh\u00b7let"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADJD", "$,", "KON", "PPER", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Darin allein, da\u00df sie mich liebt.", "tokens": ["Da\u00b7rin", "al\u00b7lein", ",", "da\u00df", "sie", "mich", "liebt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "$,", "KOUS", "PPER", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Die Ehrfurcht wirft mich ihr zu F\u00fc\u00dfen,", "tokens": ["Die", "Ehr\u00b7furcht", "wirft", "mich", "ihr", "zu", "F\u00fc\