{"textgrid.poem.42364": {"metadata": {"author": {"name": "Lachmann, Hedwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Sonne steht schon tief. Wir scheiden bald.", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Sonne steht schon tief. Wir scheiden bald.", "tokens": ["Die", "Son\u00b7ne", "steht", "schon", "tief", ".", "Wir", "schei\u00b7den", "bald", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADJD", "$.", "PPER", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Leis spr\u00fcht der Regen. Horch! Die Meise klagt.", "tokens": ["Leis", "spr\u00fcht", "der", "Re\u00b7gen", ".", "Horch", "!", "Die", "Mei\u00b7se", "klagt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$.", "NE", "$.", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wie dunkel und verschwiegen ist der Wald!", "tokens": ["Wie", "dun\u00b7kel", "und", "ver\u00b7schwie\u00b7gen", "ist", "der", "Wald", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "KON", "VVPP", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Du hast das tiefste Wort mir nicht gesagt. \u2013", "tokens": ["Du", "hast", "das", "tiefs\u00b7te", "Wort", "mir", "nicht", "ge\u00b7sagt", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "PPER", "PTKNEG", "VVPP", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Zwei helle Birken an der Waldeswand.", "tokens": ["Zwei", "hel\u00b7le", "Bir\u00b7ken", "an", "der", "Wal\u00b7des\u00b7wand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ein Spinngewebe zwischen beiden, sieh!", "tokens": ["Ein", "Spinn\u00b7ge\u00b7we\u00b7be", "zwi\u00b7schen", "bei\u00b7den", ",", "sieh", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "APPR", "PIAT", "$,", "VVIMP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wie ist es zart von Stamm zu Stamm gespannt!", "tokens": ["Wie", "ist", "es", "zart", "von", "Stamm", "zu", "Stamm", "ge\u00b7spannt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADJD", "APPR", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Was uns zu tiefst bewegt, wir sagen's nie. \u2013", "tokens": ["Was", "uns", "zu", "tiefst", "be\u00b7wegt", ",", "wir", "sa\u00b7gen's", "nie", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "PPER", "PTKA", "ADJD", "VVFIN", "$,", "PPER", "VVFIN", "ADV", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "F\u00fchlst du den Hauch? Ein Zittern auf dem Grund", "tokens": ["F\u00fchlst", "du", "den", "Hauch", "?", "Ein", "Zit\u00b7tern", "auf", "dem", "Grund"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "$.", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Des Sees. Die glatte Oberfl\u00e4che bebt.", "tokens": ["Des", "Sees", ".", "Die", "glat\u00b7te", "O\u00b7berf\u00b7l\u00e4\u00b7che", "bebt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wie Schatten weht es auch um unsern Mund \u2013", "tokens": ["Wie", "Schat\u00b7ten", "weht", "es", "auch", "um", "un\u00b7sern", "Mund", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wir haben wahrhaft nur im Traum gelebt. \u2013", "tokens": ["Wir", "ha\u00b7ben", "wahr\u00b7haft", "nur", "im", "Traum", "ge\u00b7lebt", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "APPRART", "NN", "VVPP", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Die Sonne steht schon tief. Wir scheiden bald.", "tokens": ["Die", "Son\u00b7ne", "steht", "schon", "tief", ".", "Wir", "schei\u00b7den", "bald", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADJD", "$.", "PPER", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Leis spr\u00fcht der Regen. Horch! Die Meise klagt.", "tokens": ["Leis", "spr\u00fcht", "der", "Re\u00b7gen", ".", "Horch", "!", "Die", "Mei\u00b7se", "klagt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$.", "NE", "$.", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wie dunkel und verschwiegen ist der Wald!", "tokens": ["Wie", "dun\u00b7kel", "und", "ver\u00b7schwie\u00b7gen", "ist", "der