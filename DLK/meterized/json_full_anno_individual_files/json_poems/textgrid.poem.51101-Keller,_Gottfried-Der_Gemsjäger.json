{"textgrid.poem.51101": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Der Gemsj\u00e4ger", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Er kam, ein alter J\u00e4gersmann,", "tokens": ["Er", "kam", ",", "ein", "al\u00b7ter", "J\u00e4\u00b7gers\u00b7mann", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Herab an unsrer Str\u00f6me Flut,", "tokens": ["Her\u00b7ab", "an", "uns\u00b7rer", "Str\u00f6\u00b7me", "Flut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er hatte kurze Hosen an", "tokens": ["Er", "hat\u00b7te", "kur\u00b7ze", "Ho\u00b7sen", "an"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADJA", "NN", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und trug 'nen spitzen J\u00e4gerhut.", "tokens": ["Und", "trug", "'nen", "spit\u00b7zen", "J\u00e4\u00b7ger\u00b7hut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "VVFIN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Er ging so ernst, er sah so schlicht,", "tokens": ["Er", "ging", "so", "ernst", ",", "er", "sah", "so", "schlicht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "$,", "PPER", "VVFIN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie seiner Joppe graues Tuch;", "tokens": ["Wie", "sei\u00b7ner", "Jop\u00b7pe", "