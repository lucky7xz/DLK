{"dta.poem.12930": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Als er seine Liebste vor eine sch\u00f6-  \n nere fahren lie\u00df.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Jndem ein edelstein", "tokens": ["In\u00b7dem", "ein", "e\u00b7del\u00b7stein"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Durch seinen s\u00fc\u00dfen wunder-schein", "tokens": ["Durch", "sei\u00b7nen", "s\u00fc\u00b7\u00dfen", "wun\u00b7der\u00b7schein"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bem\u00fchet ist mein aug\u2019 an sich zu hefften,", "tokens": ["Be\u00b7m\u00fc\u00b7het", "ist", "mein", "aug'", "an", "sich", "zu", "heff\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "VAFIN", "PPOSAT", "NN", "APPR", "PRF", "PTKZU", "VVINF", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "So tritt ein andrer vor, der mit noch st\u00e4rckern kr\u00e4fften", "tokens": ["So", "tritt", "ein", "an\u00b7drer", "vor", ",", "der", "mit", "noch", "st\u00e4r\u00b7ckern", "kr\u00e4ff\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "PTKVZ", "$,", "PRELS", "APPR", "ADV", "VVINF", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Demselben seinen glantz, und mir das hertz entzieht.", "tokens": ["Dem\u00b7sel\u00b7ben", "sei\u00b7nen", "glantz", ",", "und", "mir", "das", "hertz", "ent\u00b7zieht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPOSAT", "NN", "$,", "KON", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ach, Amor! wilst du mir einmal zu dienste leben,", "tokens": ["Ach", ",", "A\u00b7mor", "!", "wilst", "du", "mir", "ein\u00b7mal", "zu", "diens\u00b7te", "le\u00b7ben", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "NE", "$.", "VMFIN", "PPER", "PPER", "ADV", "APPR", "ADJA", "VVINF", "$,"], "meter": "-+-+-++--+-+-", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "So mache, da\u00df er bald in mich versetzet sey;", "tokens": ["So", "ma\u00b7che", ",", "da\u00df", "er", "bald", "in", "mich", "ver\u00b7set\u00b7zet", "sey", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "KOUS", "PPER", "ADV", "APPR", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Das gold ist meine treu,", "tokens": ["Das", "gold", "ist", "mei\u00b7ne", "treu", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Und wenn das feuer nicht in deiner werckstatt gl\u00fcht,", "tokens": ["Und", "wenn", "das", "feu\u00b7er", "nicht", "in", "dei\u00b7ner", "wercks\u00b7tatt", "gl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "PTKNEG", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Kan meine liebe schon hierzu die glut hergeben.", "tokens": ["Kan", "mei\u00b7ne", "lie\u00b7be", "schon", "hier\u00b7zu", "die", "glut", "her\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "VVFIN", "ADV", "PAV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}