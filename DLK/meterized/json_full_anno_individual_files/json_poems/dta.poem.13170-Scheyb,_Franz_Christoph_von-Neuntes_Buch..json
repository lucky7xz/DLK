{"dta.poem.13170": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Neuntes Buch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Prinz, der j\u00fcngste Prinz war aller Freuden Kron;", "tokens": ["Der", "Prinz", ",", "der", "j\u00fcngs\u00b7te", "Prinz", "war", "al\u00b7ler", "Freu\u00b7den", "Kron", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "NN", "VAFIN", "PIAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "O aller Tugenden Wunsch, Zeugnis, Pfand und Sohn!", "tokens": ["O", "al\u00b7ler", "Tu\u00b7gen\u00b7den", "Wunsch", ",", "Zeug\u00b7nis", ",", "Pfand", "und", "Sohn", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PIAT", "NN", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+---+-+-+", "measure": "unknown.measure.penta"}, "line.3": {"text": "Sie trug ihn auf dem Arm, liebkoste seinem Blicke,", "tokens": ["Sie", "trug", "ihn", "auf", "dem", "Arm", ",", "lieb\u00b7kos\u00b7te", "sei\u00b7nem", "Bli\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "$,", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "380Und wies, da\u00df ihr Gem\u00fcth sich nur in ihm erquicke.", "tokens": ["wies", ",", "da\u00df", "ihr", "Ge\u00b7m\u00fcth", "sich", "nur", "in", "ihm", "er\u00b7qui\u00b7cke", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "PPOSAT", "NN", "PRF", "ADV", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.5": {"text": "Wer sollte nicht hieraus die frohe Folge ziehn:", "tokens": ["Wer", "soll\u00b7te", "nicht", "hier\u00b7aus", "die", "fro\u00b7he", "Fol\u00b7ge", "ziehn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PTKNEG", "PAV", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Da\u00df Heil und Wohl des Throns in den Geb\u00e4rden bl\u00fchn?", "tokens": ["Da\u00df", "Heil", "und", "Wohl", "des", "Throns", "in", "den", "Ge\u00b7b\u00e4r\u00b7den", "bl\u00fchn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "ADV", "ART", "NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Da\u00df nichts annehmlichers, gl\u00fcckseeligers zu finden,", "tokens": ["Da\u00df", "nichts", "an\u00b7nehm\u00b7li\u00b7chers", ",", "gl\u00fcck\u00b7see\u00b7li\u00b7gers", "zu", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "$,", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Als sehn, ein solches Paar in Liebe sich verbinden.", "tokens": ["Als", "sehn", ",", "ein", "sol\u00b7ches", "Paar", "in", "Lie\u00b7be", "sich", "ver\u00b7bin\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVINF", "$,", "ART", "PIAT", "NN", "APPR", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "385Ein Kind, aus dessen Aug ein Strahl der Hoffnung blizt,", "tokens": ["Kind", ",", "aus", "des\u00b7sen", "Aug", "ein", "Strahl", "der", "Hoff\u00b7nung", "blizt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "PRELAT", "NN", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.10": {"text": "Da\u00df es, wann es erw\u00e4chst, den Thron der Eltern st\u00fczt,", "tokens": ["Da\u00df", "es", ",", "wann", "es", "er\u00b7w\u00e4chst", ",", "den", "Thron", "der", "El\u00b7tern", "st\u00fczt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "PWAV", "PPER", "VVFIN", "$,", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "(o wahre Lust des Volcks!) schm\u00fcckt und belebt die L\u00e4nder!", "tokens": ["(", "o", "wah\u00b7re", "Lust", "des", "Volcks", "!", ")", "schm\u00fcckt", "und", "be\u00b7lebt", "die", "L\u00e4n\u00b7der", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM", "ADJA", "NN", "ART", "NN", "$.", "$(", "VVFIN", "KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Es \u00fcbertrifft den Schaz der werthsten Friedens-Pf\u00e4nder.", "tokens": ["Es", "\u00fc\u00b7bert\u00b7rifft", "den", "Schaz", "der", "werths\u00b7ten", "Frie\u00b7dens\u00b7P\u00b7f\u00e4n\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}