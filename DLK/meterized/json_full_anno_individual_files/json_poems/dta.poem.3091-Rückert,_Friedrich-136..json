{"dta.poem.3091": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "136.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du kannst denselben Sinn in viele Bilder senken,", "tokens": ["Du", "kannst", "den\u00b7sel\u00b7ben", "Sinn", "in", "vie\u00b7le", "Bil\u00b7der", "sen\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PDAT", "NN", "APPR", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und kannst im selben Bild gar viele Sinne denken.", "tokens": ["Und", "kannst", "im", "sel\u00b7ben", "Bild", "gar", "vie\u00b7le", "Sin\u00b7ne", "den\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "APPRART", "ADJA", "NN", "ADV", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Denn der Gedanke mu\u00df sich in viel H\u00fcllen kleiden,", "tokens": ["Denn", "der", "Ge\u00b7dan\u00b7ke", "mu\u00df", "sich", "in", "viel", "H\u00fcl\u00b7len", "klei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VMFIN", "PRF", "APPR", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df er sich lerne von sich selber unterscheiden.", "tokens": ["Da\u00df", "er", "sich", "ler\u00b7ne", "von", "sich", "sel\u00b7ber", "un\u00b7ter\u00b7schei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVFIN", "APPR", "PRF", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Und viel Gedanken sind in Einem Glanz erbrannt,", "tokens": ["Und", "viel", "Ge\u00b7dan\u00b7ken", "sind", "in", "Ei\u00b7nem", "Glanz", "er\u00b7brannt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VAFIN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wo die verschiedenen als Eines sich erkannt.", "tokens": ["Wo", "die", "ver\u00b7schie\u00b7de\u00b7nen", "als", "Ei\u00b7nes", "sich", "er\u00b7kannt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "KOUS", "PIS", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}