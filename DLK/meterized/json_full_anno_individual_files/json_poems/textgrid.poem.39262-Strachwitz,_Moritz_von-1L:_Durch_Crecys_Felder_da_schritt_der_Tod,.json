{"textgrid.poem.39262": {"metadata": {"author": {"name": "Strachwitz, Moritz von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Durch Crecys Felder da schritt der Tod,", "genre": "verse", "period": "N.A.", "pub_year": 1834, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Durch Crecys Felder da schritt der Tod,", "tokens": ["Durch", "Cre\u00b7cys", "Fel\u00b7der", "da", "schritt", "der", "Tod", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Da hielt er blutige Mahd,", "tokens": ["Da", "hielt", "er", "blu\u00b7ti\u00b7ge", "Mahd", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Da neigte welkend die Lilie sich", "tokens": ["Da", "neig\u00b7te", "wel\u00b7kend", "die", "Li\u00b7lie", "sich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADJD", "ART", "NN", "PRF"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ins purpurne Wellenbad.", "tokens": ["Ins", "pur\u00b7pur\u00b7ne", "Wel\u00b7len\u00b7bad", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Wie rang im Schaume der starke Leu", "tokens": ["Wie", "rang", "im", "Schau\u00b7me", "der", "star\u00b7ke", "Leu"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "APPRART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Im markigen Kampfeszorn,", "tokens": ["Im", "mar\u00b7ki\u00b7gen", "Kamp\u00b7fes\u00b7zorn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wie schwang die Sense der schwarze Prinz", "tokens": ["Wie", "schwang", "die", "Sen\u00b7se", "der", "schwar\u00b7ze", "Prinz"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Durchs fr\u00e4nkische Ritterkorn.", "tokens": ["Durchs", "fr\u00e4n\u00b7ki\u00b7sche", "Rit\u00b7ter\u00b7korn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Wie wogt so golden die Locke hell", "tokens": ["Wie", "wogt", "so", "gol\u00b7den", "die", "Lo\u00b7cke", "hell"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "ADV", "ADJD", "ART", "NN", "ADJD"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Aus m\u00e4chtigen Helmessaum,", "tokens": ["Aus", "m\u00e4ch\u00b7ti\u00b7gen", "Hel\u00b7mes\u00b7saum", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wie zieht am Bogen die Sense schnell", "tokens": ["Wie", "zieht", "am", "Bo\u00b7gen", "die", "Sen\u00b7se", "schnell"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "APPRART", "NN", "ART", "NN", "ADJD"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Im triefenden Todesschaum.", "tokens": ["Im", "trie\u00b7fen\u00b7den", "To\u00b7des\u00b7schaum", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Da ward zum Purpur das Panzerhemd", "tokens": ["Da", "ward", "zum", "Pur\u00b7pur", "das", "Pan\u00b7zer\u00b7hemd"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPRART", "NN", "ART", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "In sch\u00e4umender Farben Glut,", "tokens": ["In", "sch\u00e4u\u00b7men\u00b7der", "Far\u00b7ben", "Glut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Da dampfte kochend der Helmbusch rot", "tokens": ["Da", "dampf\u00b7te", "ko\u00b7chend", "der", "Helm\u00b7busch", "rot"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADJD", "ART", "NN", "ADJD"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Vom tr\u00e4ufelnden Frankenblut.", "tokens": ["Vom", "tr\u00e4u\u00b7feln\u00b7den", "Fran\u00b7ken\u00b7blut", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Wie tobst Du so m\u00e4chtig! Du junger Leu!", "tokens": ["Wie", "tobst", "Du", "so", "m\u00e4ch\u00b7tig", "!", "Du", "jun\u00b7ger", "Leu", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "ADJD", "$.", "PPER", "ADJA", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Halt an Dein st\u00f6hnendes Ro\u00df,", "tokens": ["Halt", "an", "Dein", "st\u00f6h\u00b7nen\u00b7des", "Ro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Willst erfechten Du Dir das Spornes Gold", "tokens": ["Willst", "er\u00b7fech\u00b7ten", "Du", "Dir", "das", "Spor\u00b7nes", "Gold"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "PPER", "ART", "ADJA", "NN"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Als des Todes Waffengeno\u00df?", "tokens": ["Als", "des", "To\u00b7des", "Waf\u00b7fen\u00b7ge\u00b7no\u00df", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "NN", "$."], "meter": "--+-+--+", "measure": "iambic.tri.chol"}}, "stanza.6": {"line.1": {"text": "Sieh hin! wie sie sinken, die Ritter Dein", "tokens": ["Sieh", "hin", "!", "wie", "sie", "sin\u00b7ken", ",", "die", "Rit\u00b7ter", "Dein"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "PTKVZ", "$.", "PWAV", "PPER", "VVINF", "$,", "ART", "NN", "PPOSAT"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Um Dich in blutigem Kreis,", "tokens": ["Um", "Dich", "in", "blu\u00b7ti\u00b7gem", "Kreis", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PRF", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Schau hin! wie dampfet von Englands Blut", "tokens": ["Schau", "hin", "!", "wie", "damp\u00b7fet", "von", "En\u00b7glands", "Blut"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "PTKVZ", "$.", "PWAV", "VVFIN", "APPR", "NE", "NN"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Die fr\u00e4nkische Erde hei\u00df!", "tokens": ["Die", "fr\u00e4n\u00b7ki\u00b7sche", "Er\u00b7de", "hei\u00df", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.7": {"line.1": {"text": "Schau hin, wie den Boden der Bannerherr", "tokens": ["Schau", "hin", ",", "wie", "den", "Bo\u00b7den", "der", "Ban\u00b7ner\u00b7herr"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "PTKVZ", "$,", "PWAV", "ART", "NN", "ART", "NN"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Mit st\u00f6hnender Lippe pre\u00dft!", "tokens": ["Mit", "st\u00f6h\u00b7nen\u00b7der", "Lip\u00b7pe", "pre\u00dft", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ha, wahre Dich, Leu, in des Stromes Macht,", "tokens": ["Ha", ",", "wah\u00b7re", "Dich", ",", "Leu", ",", "in", "des", "Stro\u00b7mes", "Macht", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VVFIN", "PPER", "$,", "NE", "$,", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+---+-+", "measure": "zehnsilber"}, "line.4": {"text": "Da\u00df die Kraft Dich nimmer verl\u00e4\u00dft.", "tokens": ["Da\u00df", "die", "Kraft", "Dich", "nim\u00b7mer", "ver\u00b7l\u00e4\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "ADV", "VVFIN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.8": {"line.1": {"text": "Da st\u00fcrzte getroffen des Prinzen Tier", "tokens": ["Da", "st\u00fcrz\u00b7te", "ge\u00b7trof\u00b7fen", "des", "Prin\u00b7zen", "Tier"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "VVPP", "ART", "NN", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "In m\u00e4chtigen Todeswucht,", "tokens": ["In", "m\u00e4ch\u00b7ti\u00b7gen", "To\u00b7des\u00b7wucht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Da scho\u00df im Bogen der Heldenstrom", "tokens": ["Da", "scho\u00df", "im", "Bo\u00b7gen", "der", "Hel\u00b7den\u00b7strom"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "ART", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Aus klaffender Helmesbucht.", "tokens": ["Aus", "klaf\u00b7fen\u00b7der", "Hel\u00b7mes\u00b7bucht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.9": {"line.1": {"text": "Und \u00fcber ihn hin des Franken Tier", "tokens": ["Und", "\u00fc\u00b7ber", "ihn", "hin", "des", "Fran\u00b7ken", "Tier"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPER", "ADV", "ART", "NN", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Mit stampfendem Hufesrand,", "tokens": ["Mit", "stamp\u00b7fen\u00b7dem", "Hu\u00b7fes\u00b7rand", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Da lag in grimmiger Todesnot", "tokens": ["Da", "lag", "in", "grim\u00b7mi\u00b7ger", "To\u00b7des\u00b7not"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die Blume von Engelland!", "tokens": ["Die", "Blu\u00b7me", "von", "En\u00b7gel\u00b7land", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.10": {"line.1": {"text": "Da ringt er gewaltig, da hebt er sich hoch,", "tokens": ["Da", "ringt", "er", "ge\u00b7wal\u00b7tig", ",", "da", "hebt", "er", "sich", "hoch", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "$,", "ADV", "VVFIN", "PPER", "PRF", "ADJD", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Zum Banner die Linke greift,", "tokens": ["Zum", "Ban\u00b7ner", "die", "Lin\u00b7ke", "greift", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Da l\u00e4\u00dft er sausen die Klinge gut,", "tokens": ["Da", "l\u00e4\u00dft", "er", "sau\u00b7sen", "die", "Klin\u00b7ge", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wie prasselnder Hagel streift.", "tokens": ["Wie", "pras\u00b7seln\u00b7der", "Ha\u00b7gel", "streift", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.11": {"line.1": {"text": "Da tritt er stolz auf des Rosses Leib,", "tokens": ["Da", "tritt", "er", "stolz", "auf", "des", "Ros\u00b7ses", "Leib", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Des blutigen Helmes bar,", "tokens": ["Des", "blu\u00b7ti\u00b7gen", "Hel\u00b7mes", "bar", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Da sch\u00fcttelt er wild in des L\u00f6wen Zorn", "tokens": ["Da", "sch\u00fct\u00b7telt", "er", "wild", "in", "des", "L\u00f6\u00b7wen", "Zorn"], "toke