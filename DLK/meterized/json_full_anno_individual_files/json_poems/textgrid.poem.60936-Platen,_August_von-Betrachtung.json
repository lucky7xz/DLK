{"textgrid.poem.60936": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "Betrachtung", "genre": "verse", "period": "N.A.", "pub_year": 1815, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sch\u00f6n ist's, unter den Br\u00fccken hindurch in der l\u00e4nglichen Gondel", "tokens": ["Sch\u00f6n", "ist's", ",", "un\u00b7ter", "den", "Br\u00fc\u00b7cken", "hin\u00b7durch", "in", "der", "l\u00e4ng\u00b7li\u00b7chen", "Gon\u00b7del"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "$,", "APPR", "ART", "NN", "PAV", "APPR", "ART", "ADJA", "NN"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Schweben, und auch sch\u00f6n ist's, schweifend am Ufer umher,", "tokens": ["Schwe\u00b7ben", ",", "und", "auch", "sch\u00f6n", "ist's", ",", "schwei\u00b7fend", "am", "U\u00b7fer", "um\u00b7her", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "ADV", "ADJD", "VAFIN", "$,", "ADJD", "APPRART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Deine Geschichte zu lesen in deinen Troph\u00e4n, o Venedig!", "tokens": ["Dei\u00b7ne", "Ge\u00b7schich\u00b7te", "zu", "le\u00b7sen", "in", "dei\u00b7nen", "Tro\u00b7ph\u00e4n", ",", "o", "Ve\u00b7ne\u00b7dig", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKZU", "VVINF", "APPR", "PPOSAT", "NN", "$,", "FM", "NE", "$."], "meter": "+--+--+--+-++-+--", "measure": "dactylic.tri.plus"}, "line.4": {"text": "Jene Geschichte der einst m\u00e4chtigen Seerepublik,", "tokens": ["Je\u00b7ne", "Ge\u00b7schich\u00b7te", "der", "einst", "m\u00e4ch\u00b7ti\u00b7gen", "See\u00b7re\u00b7pub\u00b7lik", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "ART", "ADV", "ADJA", "NN", "$,"], "meter": "+--+-+-+--+--+", "measure": "iambic.hexa.invert"}, "line.5": {"text": "Die, dreizehn Jahrhunderte durch, sich erh\u00e4lt und bereichert,", "tokens": ["Die", ",", "drei\u00b7zehn", "Jahr\u00b7hun\u00b7der\u00b7te", "durch", ",", "sich", "er\u00b7h\u00e4lt", "und", "be\u00b7rei\u00b7chert", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "CARD", "NN", "PTKVZ", "$,", "PRF", "VVFIN", "KON", "VVPP", "$,"], "meter": "-+--+--+--+--+-", "measure": "amphibrach.penta.plus"}, "line.6": {"text": "Bis sie zuletzt umst\u00fcrzt jener titanische Mann.", "tokens": ["Bis", "sie", "zu\u00b7letzt", "um\u00b7st\u00fcrzt", "je\u00b7ner", "ti\u00b7ta\u00b7ni\u00b7sche", "Mann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVFIN", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+---+--+", "measure": "iambic.penta.chol"}, "line.7": {"text": "Der, da der Freiheit kurzer Moment den Talenten Entwicklung", "tokens": ["Der", ",", "da", "der", "Frei\u00b7heit", "kur\u00b7zer", "Mo\u00b7ment", "den", "Ta\u00b7len\u00b7ten", "Ent\u00b7wick\u00b7lung"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "$,", "KOUS", "ART", "NN", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+--+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "G\u00f6nnte, sich rasch vordr\u00e4ngt als der Talente Talent,", "tokens": ["G\u00f6nn\u00b7te", ",", "sich", "rasch", "vor\u00b7dr\u00e4ngt", "als", "der", "Ta\u00b7len\u00b7te", "Ta\u00b7lent", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRF", "ADJD", "VVFIN", "KOKOM", "ART", "NN", "NN", "$,"], "meter": "+--+-++-+--+-", "measure": "iambic.hexa.invert"}, "line.9": {"text": "Zepter entwindet und Zepter verteilt. Ihm fielst du, Venedig;", "tokens": ["Zep\u00b7ter", "ent\u00b7win\u00b7det", "und", "Zep\u00b7ter", "ver\u00b7teilt", ".", "Ihm", "fielst", "du", ",", "Ve\u00b7ne\u00b7dig", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "VVPP", "KON", "NN", "VVPP", "$.", "PPER", "VVFIN", "PPER", "$,", "NE", "$."], "meter": "+--+--+--+-+-+-+", "measure": "dactylic.tri.plus"}, "line.10": {"text": "Aber er fiel bald selbst unter die R\u00e4der des Gl\u00fccks!", "tokens": ["A\u00b7ber", "er", "fiel", "bald", "selbst", "un\u00b7ter", "die", "R\u00e4\u00b7der", "des", "Gl\u00fccks", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "ADV", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}}, "stanza.2": {"line.1": {"text": "Sch\u00f6n ist's, unter den Br\u00fccken hindurch in der l\u00e4nglichen Gondel", "tokens": ["Sch\u00f6n", "ist's", ",", "un\u00b7ter", "den", "Br\u00fc\u00b7cken", "hin\u00b7durch", "in", "der", "l\u00e4ng\u00b7li\u00b7chen", "Gon\u00b7del"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "$,", "APPR", "ART", "NN", "PAV", "APPR", "ART", "ADJA", "NN"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Schweben, und auch sch\u00f6n ist's, schweifend am Ufer umher,", "tokens": ["Schwe\u00b7ben", ",", "und", "auch", "sch\u00f6n", "ist's", ",", "schwei\u00b7fend", "am", "U\u00b7fer", "um\u00b7her", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "ADV", "ADJD", "VAFIN", "$,", "ADJD", "APPRART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+--+--+", "measure