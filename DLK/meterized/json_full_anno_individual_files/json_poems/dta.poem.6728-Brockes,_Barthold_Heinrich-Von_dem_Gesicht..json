{"dta.poem.6728": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von dem Gesicht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.85", "af:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Dis sind die frevelhafften Lehren,", "tokens": ["Dis", "sind", "die", "fre\u00b7vel\u00b7haff\u00b7ten", "Leh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "So wir in Schulen h\u00f6ren.", "tokens": ["So", "wir", "in", "Schu\u00b7len", "h\u00f6\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Man schmiedet uns Gespenster, nach Gefallen", "tokens": ["Man", "schmie\u00b7det", "uns", "Ge\u00b7spens\u00b7ter", ",", "nach", "Ge\u00b7fal\u00b7len"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PIS", "VVFIN", "PPER", "NN", "$,", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und Bilder, welche schwebend wallen,", "tokens": ["Und", "Bil\u00b7der", ",", "wel\u00b7che", "schwe\u00b7bend", "wal\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "PRELS", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die ihrem Urbild \u00e4hnlich seyn,", "tokens": ["Die", "ih\u00b7rem", "Ur\u00b7bild", "\u00e4hn\u00b7lich", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die mitten in der Lufft sich unsern Augen", "tokens": ["Die", "mit\u00b7ten", "in", "der", "Lufft", "sich", "un\u00b7sern", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "APPR", "ART", "NN", "PRF", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Zu n\u00e4hern taugen.", "tokens": ["Zu", "n\u00e4\u00b7hern", "tau\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "Ein Grillen-Heer, erdichte Bilderlein", "tokens": ["Ein", "Gril\u00b7len\u00b7Heer", ",", "er\u00b7dich\u00b7te", "Bil\u00b7der\u00b7lein"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Von C\u00f6rpern, blo\u00df im Schein,", "tokens": ["Von", "C\u00f6r\u00b7pern", ",", "blo\u00df", "im", "Schein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Von Form und Farben Ausz\u00fcg\u2019, die im Reisen", "tokens": ["Von", "Form", "und", "Far\u00b7ben", "Aus\u00b7z\u00fcg'", ",", "die", "im", "Rei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "NN", "$,", "PRELS", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Von ihren Vorwurff sich so klug, so treu erweisen:", "tokens": ["Von", "ih\u00b7ren", "Vor\u00b7wurff", "sich", "so", "klug", ",", "so", "treu", "er\u00b7wei\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PRF", "ADV", "ADJD", "$,", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Sie h\u00f6ren all\u2019, in ihrem gantzen Lauff,", "tokens": ["Sie", "h\u00f6\u00b7ren", "all'", ",", "in", "ih\u00b7rem", "gant\u00b7zen", "Lauff", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "$,", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Sich zu verkleinern niemals auf,", "tokens": ["Sich", "zu", "ver\u00b7klei\u00b7nern", "nie\u00b7mals", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "PTKZU", "VVINF", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Bis sie in unser Auge gehn,", "tokens": ["Bis", "sie", "in", "un\u00b7ser", "Au\u00b7ge", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Wodurch denn ins Gehirn, von Vorwurff, der sie schickt,", "tokens": ["Wo\u00b7durch", "denn", "ins", "Ge\u00b7hirn", ",", "von", "Vor\u00b7wurff", ",", "der", "sie", "schickt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "APPRART", "NN", "$,", "APPR", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Sich eine Schilderey und Bildni\u00df dr\u00fcckt.", "tokens": ["Sich", "ei\u00b7ne", "Schil\u00b7de\u00b7rey", "und", "Bild\u00b7ni\u00df", "dr\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}