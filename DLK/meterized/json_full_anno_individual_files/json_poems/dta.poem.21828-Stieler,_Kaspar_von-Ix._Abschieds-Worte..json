{"dta.poem.21828": {"metadata": {"author": {"name": "Stieler, Kaspar von", "birth": "N.A.", "death": "N.A."}, "title": "Ix.  \n  Abschieds-Worte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1660", "urn": "urn:nbn:de:kobv:b4-20586-2", "language": ["de:0.99"], "booktitle": "Filidor der Dorfferer [i. e. Stieler, Kaspar von]: Die Geharnschte Venus. Hamburg, 1660."}, "poem": {"stanza.1": {"line.1": {"text": "Telesill' h\u00f6r' auff zu weinen!", "tokens": ["Te\u00b7le\u00b7sill'", "h\u00f6r'", "auff", "zu", "wei\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "worzu soll der Tr\u00e4hnen See?", "tokens": ["wor\u00b7zu", "soll", "der", "Tr\u00e4h\u00b7nen", "See", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "weinstu doch als Niobe", "tokens": ["weins\u00b7tu", "doch", "als", "Ni\u00b7o\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "KOUS", "NE"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "eh bevor sie ward zusteinen/", "tokens": ["eh", "be\u00b7vor", "sie", "ward", "zu\u00b7stei\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "VAFIN", "ADV", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "wie die arme Briseis tahte", "tokens": ["wie", "die", "ar\u00b7me", "Bri\u00b7seis", "tah\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "da sie au\u00df der Freyheit trate.", "tokens": ["da", "sie", "au\u00df", "der", "Frey\u00b7heit", "tra\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Zwar mein Stern rufft mich von hinnen/", "tokens": ["Zwar", "mein", "Stern", "rufft", "mich", "von", "hin\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVFIN", "PRF", "APPR", "ADV", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "iezt zieht man die Segel auff/", "tokens": ["iezt", "zieht", "man", "die", "Se\u00b7gel", "auff", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ART", "NN", "APPR", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "iezt geh ich den Schmerzen-Lauff", "tokens": ["iezt", "geh", "ich", "den", "Schmer\u00b7zen\u00b7Lauff"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "denn die Parzen kl\u00e4glich spinnen:", "tokens": ["denn", "die", "Par\u00b7zen", "kl\u00e4g\u00b7lich", "spin\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "weil sie Leiden und Verdriessen", "tokens": ["weil", "sie", "Lei\u00b7den", "und", "Ver\u00b7dries\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "diese Reise drehen m\u00fcssen.", "tokens": ["die\u00b7se", "Rei\u00b7se", "dre\u00b7hen", "m\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VVINF", "VMINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Dennoch sind wir nicht getrennet.", "tokens": ["Den\u00b7noch", "sind", "wir", "nicht", "ge\u00b7tren\u00b7net", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PTKNEG", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Filidor ist allzeit dein/", "tokens": ["Fi\u00b7li\u00b7dor", "ist", "all\u00b7zeit", "dein", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ADV", "PPOSAT", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "wo man keine Sonne kennet/", "tokens": ["wo", "man", "kei\u00b7ne", "Son\u00b7ne", "ken\u00b7net", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "PIAT", "NN", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "deine Fakkel macht ihn lichte", "tokens": ["dei\u00b7ne", "Fak\u00b7kel", "macht", "ihn", "lich\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "bey dem schwarzen Nacht-gesichte.", "tokens": ["bey", "dem", "schwar\u00b7zen", "Nacht\u00b7ge\u00b7sich\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Bey dem g\u00fcnstigem S\u00fcd-westen", "tokens": ["Bey", "dem", "g\u00fcns\u00b7ti\u00b7gem", "S\u00fcd\u00b7wes\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+--++-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "dein verbleib\u2019 ich f\u00fcr und f\u00fcr.", "tokens": ["dein", "ver\u00b7bleib'", "ich", "f\u00fcr", "und", "f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VVFIN", "PPER", "APPR", "KON", "APPR", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Gib mein Schiff den Felsen-\u00e4sten/", "tokens": ["Gib", "mein", "Schiff", "den", "Fel\u00b7sen\u00b7\u00e4s\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "ART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Zesyrs-Bruder wo ich liege.", "tokens": ["Ze\u00b7syr\u00b7sBru\u00b7der", "wo", "ich", "lie\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PWAV", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "und mein liebstes Kind betriege!", "tokens": ["und", "mein", "liebs\u00b7tes", "Kind", "be\u00b7trie\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}