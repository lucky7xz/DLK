{"textgrid.poem.49207": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "7. An die Bienen", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr Honigv\u00f6gelein, die ihr von den Violen", "tokens": ["Ihr", "Ho\u00b7nig\u00b7v\u00f6\u00b7ge\u00b7lein", ",", "die", "ihr", "von", "den", "Vi\u00b7o\u00b7len"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und Rosen abgemeyt den wunders\u00fcssen Safft,", "tokens": ["Und", "Ro\u00b7sen", "ab\u00b7ge\u00b7meyt", "den", "wun\u00b7der\u00b7s\u00fcs\u00b7sen", "Safft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVPP", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die ihr dem gr\u00fcnen Klee entzogen seine Krafft,", "tokens": ["Die", "ihr", "dem", "gr\u00fc\u00b7nen", "Klee", "ent\u00b7zo\u00b7gen", "sei\u00b7ne", "Krafft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die ihr das sch\u00f6ne Feld so offt und viel bestohlen,", "tokens": ["Die", "ihr", "das", "sch\u00f6\u00b7ne", "Feld", "so", "offt", "und", "viel", "be\u00b7stoh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "ADJA", "NN", "ADV", "ADV", "KON", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ihr Feldeinwohnerin, was wollet ihr doch holen", "tokens": ["Ihr", "Feld\u00b7e\u00b7in\u00b7woh\u00b7ne\u00b7rin", ",", "was", "wol\u00b7let", "ihr", "doch", "ho\u00b7len"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PWS", "VMFIN", "PPER", "ADV", "VVINF"], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Da\u00df, so euch noch zur Zeit hat wenig Nutz geschafft,", "tokens": ["Da\u00df", ",", "so", "euch", "noch", "zur", "Zeit", "hat", "we\u00b7nig", "Nutz", "ge\u00b7schafft", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "ADV", "PPER", "ADV", "APPRART", "NN", "VAFIN", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Weil ihr mit Dienstbarkeit de\u00df Menschen seyd behafft,", "tokens": ["Weil", "ihr", "mit", "Dienst\u00b7bar\u00b7keit", "de\u00df", "Men\u00b7schen", "seyd", "be\u00b7hafft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und ihnen mehrentheils das Honig m\u00fcsset zollen?", "tokens": ["Und", "ih\u00b7nen", "meh\u00b7ren\u00b7theils", "das", "Ho\u00b7nig", "m\u00fcs\u00b7set", "zol\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "ART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Kompt, kompt zu meinem Lieb', auff ihren Rosenmund,", "tokens": ["Kompt", ",", "kompt", "zu", "mei\u00b7nem", "Lieb'", ",", "auff", "ih\u00b7ren", "Ro\u00b7sen\u00b7mund", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "APPR", "PPOSAT", "NN", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der mir mein kranckes Hertz hat inniglich verwundt,", "tokens": ["Der", "mir", "mein", "kran\u00b7ckes", "Hertz", "hat", "in\u00b7nig\u00b7lich", "ver\u00b7wundt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPOSAT", "ADJA", "NN", "VAFIN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da solt ihr Himmelspeis' auch \u00fcberfl\u00fcssig brechen;", "tokens": ["Da", "solt", "ihr", "Him\u00b7mel\u00b7speis'", "auch", "\u00fc\u00b7berf\u00b7l\u00fcs\u00b7sig", "bre\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPOSAT", "NN", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Wann aber jemand sie wil setzen in Gefahr,", "tokens": ["Wann", "a\u00b7ber", "je\u00b7mand", "sie", "wil", "set\u00b7zen", "in", "Ge\u00b7fahr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PIS", "PPER", "VMFIN", "VVINF", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und ihr ein Leyd anthun, dem solt du starcke Schar", "tokens": ["Und", "ihr", "ein", "Leyd", "an\u00b7thun", ",", "dem", "solt", "du", "star\u00b7cke", "Schar"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "ART", "NN", "VVINF", "$,", "PDS", "VMFIN", "PPER", "VVFIN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "F\u00fcr Honig Galle seyn, und ihn zu Todte stechen.", "tokens": ["F\u00fcr", "Ho\u00b7nig", "Gal\u00b7le", "seyn", ",", "und", "ihn", "zu", "Tod\u00b7te", "ste\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "VAINF", "$,", "KON", "PPER", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Ihr Honigv\u00f6gelein, die ihr von den Violen", "tokens": ["Ihr", "Ho\u00b7nig\u00b7v\u00f6\u00b7ge\u00b7lein", ",", "die", "ihr", "von", "den", "Vi\u00b7o\u00b7len"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und Rosen abgemeyt den wunders\u00fcssen Safft,", "tokens": ["Und", "Ro\u00b7sen", "ab\u00b7ge\u00b7meyt", "den", "wun\u00b7der\u00b7s\u00fcs\u00b7sen", "Safft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVPP", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die ihr dem gr\u00fcnen Klee entzogen seine Krafft,", "tokens": ["Die", "ihr", "dem", "gr\u00fc\u00b7nen", "Klee", "ent\u00b7zo\u00b7gen", "sei\u00b7ne", "Krafft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die ihr das sch\u00f6ne Feld so offt und viel bestohlen,", "tokens": ["Die", "ihr", "das", "sch\u00f6\u00b7ne", "Feld", "so", "offt", "und", "viel", "be\u00b7stoh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "ADJA", "NN", "ADV", "ADV", "KON", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "al