{"dta.poem.11076": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "An die Schlesische Melpomene.  \n  B. N.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du forderst, sch\u00f6nste! mich zu reim und versen aus.", "tokens": ["Du", "for\u00b7derst", ",", "sch\u00f6ns\u00b7te", "!", "mich", "zu", "reim", "und", "ver\u00b7sen", "aus", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "$.", "PPER", "PTKA", "ADJD", "KON", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was aber soll ich schreiben?", "tokens": ["Was", "a\u00b7ber", "soll", "ich", "schrei\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wenn man an musen schreibt, und mit g\u00f6ttinnen spricht,", "tokens": ["Wenn", "man", "an", "mu\u00b7sen", "schreibt", ",", "und", "mit", "g\u00f6t\u00b7tin\u00b7nen", "spricht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "NN", "VVFIN", "$,", "KON", "APPR", "ADJA", "VVFIN", "$,"], "meter": "--+--+-+---+", "measure": "anapaest.di.plus"}, "line.4": {"text": "So schickt sich, wie du weist, kein falscher firni\u00df nicht;", "tokens": ["So", "schickt", "sich", ",", "wie", "du", "weist", ",", "kein", "fal\u00b7scher", "fir\u00b7ni\u00df", "nicht", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "$,", "PWAV", "PPER", "VVFIN", "$,", "PIAT", "ADJA", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wer aber hertzlich spricht, den mu\u00df die liebe treiben.", "tokens": ["Wer", "a\u00b7ber", "hertz\u00b7lich", "spricht", ",", "den", "mu\u00df", "die", "lie\u00b7be", "trei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADJD", "VVFIN", "$,", "ART", "VMFIN", "ART", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Du weist, Melpomene! da\u00df ich dir folgen mu\u00df,", "tokens": ["Du", "weist", ",", "Mel\u00b7po\u00b7me\u00b7ne", "!", "da\u00df", "ich", "dir", "fol\u00b7gen", "mu\u00df", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "NE", "$.", "KOUS", "PPER", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.7": {"text": "Drum mache selbst den schlu\u00df,", "tokens": ["Drum", "ma\u00b7che", "selbst", "den", "schlu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Ob meine feder soll falsch oder redlich sprechen;", "tokens": ["Ob", "mei\u00b7ne", "fe\u00b7der", "soll", "falsch", "o\u00b7der", "red\u00b7lich", "spre\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VMFIN", "ADJD", "KON", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Glaub aber dieses nur, wofern ich falsch mu\u00df seyn,", "tokens": ["Glaub", "a\u00b7ber", "die\u00b7ses", "nur", ",", "wo\u00b7fern", "ich", "falsch", "mu\u00df", "seyn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PDAT", "ADV", "$,", "KOUS", "PPER", "ADJD", "VMFIN", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "So thu ich es bey dir wahrhafftig auf den schein,", "tokens": ["So", "thu", "ich", "es", "bey", "dir", "wahr\u00b7haff\u00b7tig", "auf", "den", "schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "APPR", "PPER", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.11": {"text": "Jm hertzen werd ich doch nicht das gesetze brechen.", "tokens": ["Jm", "hert\u00b7zen", "werd", "ich", "doch", "nicht", "das", "ge\u00b7set\u00b7ze", "bre\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PPER", "ADV", "PTKNEG", "ART", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}