{"dta.poem.13102": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Achtes Buch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201eder K\u00fcnste Wohlgeschmack, der guten Sitten Werth,", "tokens": ["\u201e", "der", "K\u00fcns\u00b7te", "Wohl\u00b7ge\u00b7schmack", ",", "der", "gu\u00b7ten", "Sit\u00b7ten", "Werth", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "370\u201dWard diesem wilden Volck durch meine Kunst gelehrt.", "tokens": ["\"", "Ward", "die\u00b7sem", "wil\u00b7den", "Volck", "durch", "mei\u00b7ne", "Kunst", "ge\u00b7lehrt", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "PDAT", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "\u201eich schmeichelte der Wuth, womit es anfangs tobte,", "tokens": ["\u201e", "ich", "schmei\u00b7chel\u00b7te", "der", "Wuth", ",", "wo\u00b7mit", "es", "an\u00b7fangs", "tob\u00b7te", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "ART", "NN", "$,", "PWAV", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "\u201ebi\u00df es bes\u00e4nftiget mein kluges Singen lobte,", "tokens": ["\u201e", "bi\u00df", "es", "be\u00b7s\u00e4nf\u00b7ti\u00b7get", "mein", "klu\u00b7ges", "Sin\u00b7gen", "lob\u00b7te", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.5": {"text": "\u201eund allgemach das Herz der B\u00fcgsamkeit ergab,", "tokens": ["\u201e", "und", "all\u00b7ge\u00b7mach", "das", "Herz", "der", "B\u00fcg\u00b7sam\u00b7keit", "er\u00b7gab", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "ADV", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "\u201enach der ich den Gesang gestimmt, begeistert hab.", "tokens": ["\u201e", "nach", "der", "ich", "den", "Ge\u00b7sang", "ge\u00b7stimmt", ",", "be\u00b7geis\u00b7tert", "hab", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "APPR", "PRELS", "PPER", "ART", "NN", "VVPP", "$,", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "375\u201dDas ist der Urbeginn der Menschen Wissenheiten,", "tokens": ["\"", "Das", "ist", "der", "Ur\u00b7be\u00b7ginn", "der", "Men\u00b7schen", "Wis\u00b7sen\u00b7hei\u00b7ten", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PDS", "VAFIN", "ART", "NN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "\u201edie man von Volck zu Volck getrachtet auszubreiten.", "tokens": ["\u201e", "die", "man", "von", "Volck", "zu", "Volck", "ge\u00b7trach\u00b7tet", "aus\u00b7zu\u00b7brei\u00b7ten", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PRELS", "PIS", "APPR", "NN", "APPR", "NN", "VVPP", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}