{"textgrid.poem.37110": {"metadata": {"author": {"name": "H\u00f6lty, Ludwig Christoph Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Petrarchische Bettlerode", "genre": "verse", "period": "N.A.", "pub_year": 1762, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn mit leisen Hutfilzs\u00f6ckchen", "tokens": ["Wenn", "mit", "lei\u00b7sen", "Hut\u00b7filz\u00b7s\u00f6ck\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Meine braune Trutschel geht,", "tokens": ["Mei\u00b7ne", "brau\u00b7ne", "Trut\u00b7schel", "geht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ihr rothes B\u00fcffelr\u00f6ckchen", "tokens": ["Und", "ihr", "ro\u00b7thes", "B\u00fcf\u00b7fel\u00b7r\u00f6\u00b7ck\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN"], "meter": "--+-+-+--", "measure": "anapaest.init"}, "line.4": {"text": "Um die dicken Schinken weht,", "tokens": ["Um", "die", "di\u00b7cken", "Schin\u00b7ken", "weht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "\u00dcber Z\u00e4une, Steg und Br\u00fccken,", "tokens": ["\u00dc\u00b7ber", "Z\u00e4u\u00b7ne", ",", "Steg", "und", "Br\u00fc\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Jeden ausgeschlagnen Tag,", "tokens": ["Je\u00b7den", "aus\u00b7ge\u00b7schlag\u00b7nen", "Tag", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Humpl' ich dann auf beiden Kr\u00fccken", "tokens": ["Hum\u00b7pl'", "ich", "dann", "auf", "bei\u00b7den", "Kr\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "PPER", "ADV", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ihr mit Sack und Packe nach.", "tokens": ["Ihr", "mit", "Sack", "und", "Pa\u00b7cke", "nach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "W\u00e4r ich nur ein Dorn der Hecke,", "tokens": ["W\u00e4r", "ich", "nur", "ein", "Dorn", "der", "He\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Welche schlau ihr R\u00f6ckchen ritzt!", "tokens": ["Wel\u00b7che", "schlau", "ihr", "R\u00f6ck\u00b7chen", "ritzt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nur ein Tr\u00f6pfchen von dem Drecke,", "tokens": ["Nur", "ein", "Tr\u00f6pf\u00b7chen", "von", "dem", "Dre\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Der an ihre Waden spritzt!", "tokens": ["Der", "an", "ih\u00b7re", "Wa\u00b7den", "spritzt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "W\u00e4r ich nur das Flederm\u00e4uschen,", "tokens": ["W\u00e4r", "ich", "nur", "das", "Fle\u00b7der\u00b7m\u00e4us\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Das um ihre M\u00fctze schwirrt!", "tokens": ["Das", "um", "ih\u00b7re", "M\u00fct\u00b7ze", "schwirrt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Nur das kleine Silberl\u00e4uschen,", "tokens": ["Nur", "das", "klei\u00b7ne", "Sil\u00b7ber\u00b7l\u00e4u\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Das von Ohr zu Ohr ihr irrt!", "tokens": ["Das", "von", "Ohr", "zu", "Ohr", "ihr", "irrt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "NN", "APPR", "NN", "PPER", "VVFIN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}, "stanza.3": {"line.1": {"text": "W\u00fc\u00dft ich h\u00fcbsche Liebesst\u00fcckchen,", "tokens": ["W\u00fc\u00dft", "ich", "h\u00fcb\u00b7sche", "Lie\u00b7bess\u00b7t\u00fcck\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lustig, wie des Kukuks Schall;", "tokens": ["Lus\u00b7tig", ",", "wie", "des", "Ku\u00b7kuks", "Schall", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ach! Dann h\u00f6rte mich mein Fieckchen", "tokens": ["Ach", "!", "Dann", "h\u00f6r\u00b7te", "mich", "mein", "Fieck\u00b7chen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "ADV", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Abends an des Amtmanns Stall!", "tokens": ["A\u00b7bends", "an", "des", "Amt\u00b7manns", "Stall", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Schmauchten mich nur ihre Lippen", "tokens": ["Schmauch\u00b7ten", "mich", "nur", "ih\u00b7re", "Lip\u00b7pen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Als ein Paffchen Krolltoback!", "tokens": ["Als", "ein", "Paf\u00b7fchen", "Kroll\u00b7to\u00b7back", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Oder dr\u00fcckt' an ihre Rippen", "tokens": ["O\u00b7der", "dr\u00fcckt'", "an", "ih\u00b7re", "Rip\u00b7pen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Sie mich als den Dudelsack!", "tokens": ["Sie", "mich", "als", "den", "Du\u00b7del\u00b7sack", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PRF", "KOUS", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "K\u00f6nnt' ich als ein Kamm ihr dienen,", "tokens": ["K\u00f6nnt'", "ich", "als", "ein", "Kamm", "ihr", "die\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "KOUS", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn sie hinterm Zaun sich k\u00e4mmt!", "tokens": ["Wenn", "sie", "hin\u00b7term", "Zaun", "sich", "k\u00e4mmt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "PRF", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "K\u00f6nnt' ich an dem Teiche gr\u00fcnen,", "tokens": ["K\u00f6nnt'", "ich", "an", "dem", "Tei\u00b7che", "gr\u00fc\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wo sie ihre Glieder schwemmt!", "tokens": ["Wo", "sie", "ih\u00b7re", "Glie\u00b7der", "schwemmt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "W\u00e4r' ich doch auf Veltens Diele,", "tokens": ["W\u00e4r'", "ich", "doch", "auf", "Vel\u00b7tens", "Die\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "APPR", "NE", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Schatz, f\u00fcr dich ein B\u00fcndel Stroh!", "tokens": ["Schatz", ",", "f\u00fcr", "dich", "ein", "B\u00fcn\u00b7del", "Stroh", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "PPER", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Nagt' ich, ach! mit s\u00fc\u00dfem Spiele", "tokens": ["Nagt'", "ich", ",", "ach", "!", "mit", "s\u00fc\u00b7\u00dfem", "Spie\u00b7le"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "ITJ", "$.", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Dir dein Leder, als ein Floh!", "tokens": ["Dir", "dein", "Le\u00b7der", ",", "als", "ein", "Floh", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "$,", "KOUS", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "W\u00fcrde doch von Niklas Mutter,", "tokens": ["W\u00fcr\u00b7de", "doch", "von", "Nik\u00b7las", "Mut\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "NE", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Durch den alten Teufelstext", "tokens": ["Durch", "den", "al\u00b7ten", "Teu\u00b7fels\u00b7text"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ein St\u00fccklein Hexenbutter,", "tokens": ["Und", "ein", "St\u00fcck\u00b7lein", "He\u00b7xen\u00b7but\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dir ein Traum von mir gehext!", "tokens": ["Dir", "ein", "Traum", "von", "mir", "ge\u00b7hext", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "APPR", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Schmunzelnd in dem Schlafe, dr\u00fccke,", "tokens": ["Schmun\u00b7zelnd", "in", "dem", "Schla\u00b7fe", ",", "dr\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Fest mein Bild mit einem Schmaz!", "tokens": ["Fest", "mein", "Bild", "mit", "ei\u00b7nem", "Schmaz", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Morgens trabst bey meiner Kr\u00fccke", "tokens": ["Mor\u00b7gens", "trabst", "bey", "mei\u00b7ner", "Kr\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Du einher, und bist mein Schatz!", "tokens": ["Du", "ein\u00b7her", ",", "und", "bist", "mein", "Schatz", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKVZ", "$,", "KON", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Wenn mit leisen Hutfilzs\u00f6ckchen", "tokens": ["Wenn", "mit", "lei\u00b7sen", "Hut\u00b7filz\u00b7s\u00f6ck\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Meine braune Trutschel geht,", "tokens": ["Mei\u00b7ne", "brau\u00b7ne", "Trut\u00b7schel", "geht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ihr rothes B\u00fcffelr\u00f6ckchen", "tokens": ["Und", "ihr", "ro\u00b7thes", "B\u00fcf\u00b7fel\u00b7r\u00f6\u00b7ck\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN"], "meter": "--+-+-+--", "measure": "anapaest.init"}, "line.4": {"text": "Um die dicken Schinken weht,", "tokens": ["Um", "die", "di\u00b7cken", "Schin\u00b7ken", "weht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "\u00dcber Z\u00e4une, Steg und Br\u0