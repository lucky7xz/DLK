ct"], "pos": ["KON", "VVFIN", "$,", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Sogleich auf Paphos Myrthenh\u00fcgel,", "tokens": ["Sog\u00b7leich", "auf", "Pa\u00b7phos", "Myr\u00b7then\u00b7h\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NE", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Wo Amor unter Rosen lauscht.", "tokens": ["Wo", "A\u00b7mor", "un\u00b7ter", "Ro\u00b7sen", "lauscht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Hier sah ihn ein gelehrter Rabe,", "tokens": ["Hier", "sah", "ihn", "ein", "ge\u00b7lehr\u00b7ter", "Ra\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Der in betrachtungsvoller Ruh", "tokens": ["Der", "in", "be\u00b7trach\u00b7tungs\u00b7vol\u00b7ler", "Ruh"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Zehn Jahre schon in einem Grabe", "tokens": ["Zehn", "Jah\u00b7re", "schon", "in", "ei\u00b7nem", "Gra\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["CARD", "NN", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Sein Wesen trieb, und rief ihm zu:", "tokens": ["Sein", "We\u00b7sen", "trieb", ",", "und", "rief", "ihm", "zu", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,", "KON", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"tex