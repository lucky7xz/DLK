{"dta.poem.10825": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "Ueberschrift an die Vorraths-  \n kammer und den Geldkasten ei-  \n nes Geizigen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198774", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Dies ist die Sacristei, wo man den\nMammon ehrt,", "tokens": ["Dies", "ist", "die", "Sa\u00b7cris\u00b7tei", ",", "wo", "man", "den", "Mam\u00b7mon", "ehrt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "PWAV", "PIS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Dienst besteht darin, da\u00df man", "tokens": ["Der", "Dienst", "be\u00b7steht", "da\u00b7rin", ",", "da\u00df", "man"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PAV", "$,", "KOUS", "PIS"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Kasten der bedekt den G\u00f6zzen so verhohlen;", "tokens": ["Der", "Kas\u00b7ten", "der", "be\u00b7dekt", "den", "G\u00f6z\u00b7zen", "so", "ver\u00b7hoh\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "VVFIN", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Weil er von Dieben wird, sonst leicht hinweg ge-", "tokens": ["Weil", "er", "von", "Die\u00b7ben", "wird", ",", "sonst", "leicht", "hin\u00b7weg", "ge"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PDAT", "VAFIN", "$,", "ADV", "ADJD", "ADV", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Der Priester der ihn dient ist bange Tag und Nacht,", "tokens": ["Der", "Pries\u00b7ter", "der", "ihn", "dient", "ist", "ban\u00b7ge", "Tag", "und", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "PPER", "VVFIN", "VAFIN", "ADV", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Vor seines G\u00f6zzen Wohl; er ist darauf bedacht,", "tokens": ["Vor", "sei\u00b7nes", "G\u00f6z\u00b7zen", "Wohl", ";", "er", "ist", "da\u00b7rauf", "be\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "$.", "PPER", "VAFIN", "PAV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wie er stets seiner Gr\u00f6s und Herrlichkeit vermehre:", "tokens": ["Wie", "er", "stets", "sei\u00b7ner", "Gr\u00f6s", "und", "Herr\u00b7lich\u00b7keit", "ver\u00b7meh\u00b7re", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "PPOSAT", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Darin bestehet nur die Furcht, der Dienst, die Ehre,", "tokens": ["Da\u00b7rin", "be\u00b7ste\u00b7het", "nur", "die", "Furcht", ",", "der", "Dienst", ",", "die", "Eh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ADV", "ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Die er den Mammon giebt; er ehret ihn allein,", "tokens": ["Die", "er", "den", "Mam\u00b7mon", "giebt", ";", "er", "eh\u00b7ret", "ihn", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "VVFIN", "$.", "PPER", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und l\u00e4sset niemand sonst in seinen Tempel ein;", "tokens": ["Und", "l\u00e4s\u00b7set", "nie\u00b7mand", "sonst", "in", "sei\u00b7nen", "Tem\u00b7pel", "ein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "ADV", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Er sagt zu jederman: Wer meinen G\u00f6zzen liebet,", "tokens": ["Er", "sagt", "zu", "je\u00b7der\u00b7man", ":", "Wer", "mei\u00b7nen", "G\u00f6z\u00b7zen", "lie\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PIS", "$.", "PWS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Der ist mein gr\u00f6\u00dfter Feind, der mich stets qu\u00e4lt, be-", "tokens": ["Der", "ist", "mein", "gr\u00f6\u00df\u00b7ter", "Feind", ",", "der", "mich", "stets", "qu\u00e4lt", ",", "be"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["PDS", "VAFIN", "PPOSAT", "ADJA", "NN", "$,", "PRELS", "PPER", "ADV", "VVFIN", "$,", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}