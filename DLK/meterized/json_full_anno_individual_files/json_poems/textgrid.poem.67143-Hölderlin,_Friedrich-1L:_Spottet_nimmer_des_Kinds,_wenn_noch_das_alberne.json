{"textgrid.poem.67143": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Spottet nimmer des Kinds, wenn noch das alberne", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Spottet nimmer des Kinds, wenn noch das alberne", "tokens": ["Spot\u00b7tet", "nim\u00b7mer", "des", "Kinds", ",", "wenn", "noch", "das", "al\u00b7ber\u00b7ne"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "NN", "$,", "KOUS", "ADV", "ART", "NN"], "meter": "+-+--+-+-+--", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Auf dem Rosse von Holz herrlich und viel sich d\u00fcnkt,", "tokens": ["Auf", "dem", "Ros\u00b7se", "von", "Holz", "herr\u00b7lich", "und", "viel", "sich", "d\u00fcnkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "NN", "ADJD", "KON", "ADV", "PRF", "VVFIN", "$,"], "meter": "--+--+---+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "O ihr Guten! auch wir sind", "tokens": ["O", "ihr", "Gu\u00b7ten", "!", "auch", "wir", "sind"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "PPOSAT", "NN", "$.", "ADV", "PPER", "VAFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Tatenarm und gedankenvoll!", "tokens": ["Ta\u00b7ten\u00b7arm", "und", "ge\u00b7dan\u00b7ken\u00b7voll", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.2": {"line.1": {"text": "Aber kommt, wie der Strahl aus dem Gew\u00f6lke kommt,", "tokens": ["A\u00b7ber", "kommt", ",", "wie", "der", "Strahl", "aus", "dem", "Ge\u00b7w\u00f6l\u00b7ke", "kommt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PWAV", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Aus Gedanken vielleicht, geistig und reif die Tat?", "tokens": ["Aus", "Ge\u00b7dan\u00b7ken", "viel\u00b7leicht", ",", "geis\u00b7tig", "und", "reif", "die", "Tat", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "$,", "ADJD", "KON", "ADJD", "ART", "NN", "$."], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Folgt die Frucht, wie des Haines", "tokens": ["Folgt", "die", "Frucht", ",", "wie", "des", "Hai\u00b7nes"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "$,", "PWAV", "ART", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Dunklem Blatte, der stillen Schrift?", "tokens": ["Dunk\u00b7lem", "Blat\u00b7te", ",", "der", "stil\u00b7len", "Schrift", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.3": {"line.1": {"text": "Und das Schweigen im Volk, ist es die Feier schon", "tokens": ["Und", "das", "Schwei\u00b7gen", "im", "Volk", ",", "ist", "es", "die", "Fei\u00b7er", "schon"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPRART", "NN", "$,", "VAFIN", "PPER", "ART", "NN", "ADV"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Vor dem Feste? die Furcht, welche den Gott ansagt?", "tokens": ["Vor", "dem", "Fes\u00b7te", "?", "die", "Furcht", ",", "wel\u00b7che", "den", "Gott", "an\u00b7sagt", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$.", "ART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "--+--++--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "O dann nimmt mich, ihr Lieben!", "tokens": ["O", "dann", "nimmt", "mich", ",", "ihr", "Lie\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "ADV", "VVFIN", "PPER", "$,", "PPOSAT", "ADJA", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Da\u00df ich b\u00fc\u00dfe die L\u00e4sterung.", "tokens": ["Da\u00df", "ich", "b\u00fc\u00b7\u00dfe", "die", "L\u00e4s\u00b7te\u00b7rung", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.4": {"line.1": {"text": "Schon zu lange, zu lang irr ich, dem Laien gleich,", "tokens": ["Schon", "zu", "lan\u00b7ge", ",", "zu", "lang", "irr", "ich", ",", "dem", "Lai\u00b7en", "gleich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PTKA", "ADV", "$,", "PTKA", "ADJD", "NN", "PPER", "$,", "ART", "NN", "ADV", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "In des bildenden Geists werdender Werkstatt hier,", "tokens": ["In", "des", "bil\u00b7den\u00b7den", "Geists", "wer\u00b7den\u00b7der", "Werk\u00b7statt", "hier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ADJA", "NN", "ADV", "$,"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Nur was bl\u00fchet, erkenn ich,", "tokens": ["Nur", "was", "bl\u00fc\u00b7het", ",", "er\u00b7kenn", "ich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PWS", "VVFIN", "$,", "KOUS", "PPER", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Was er sinnet, erkenn ich nicht.", "tokens": ["Was", "er", "sin\u00b7net", ",", "er\u00b7kenn", "ich", "nicht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "KOUS", "PPER", "PTKNEG", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.5": {"line.1": {"text": "Und zu ahnen ist s\u00fc\u00df, aber ein Leiden auch,", "tokens": ["Und", "zu", "ah\u00b7nen", "ist", "s\u00fc\u00df", ",", "a\u00b7ber", "ein", "Lei\u00b7den", "auch", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKZU", "VVINF", "VAFIN", "ADJD", "$,", "ADV", "ART", "NN", "ADV", "$,"], "meter": "--+--+---+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Und schon Jahre genug leb ich in sterblicher", "tokens": ["Und", "schon", "Jah\u00b7re", "ge\u00b7nug", "leb", "ich", "in", "sterb\u00b7li\u00b7cher"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "NN", "ADV", "VVFIN", "PPER", "APPR", "ADJA"], "meter": "--+--+-+-+--", "measure": "anapaest.di.plus"}, "line.3": {"text": "Unverst\u00e4ndiger Liebe", "tokens": ["Un\u00b7ver\u00b7st\u00e4n\u00b7di\u00b7ger", "Lie\u00b7be"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Zweifelnd, immer bewegt vor ihm,", "tokens": ["Zwei\u00b7felnd", ",", "im\u00b7mer", "be\u00b7wegt", "vor", "ihm", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "ADV", "VVFIN", "APPR", "PPER", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.6": {"line.1": {"text": "Der das stetige Werk immer aus liebender", "tokens": ["Der", "das", "ste\u00b7ti\u00b7ge", "Werk", "im\u00b7mer", "aus", "lie\u00b7ben\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ART", "ADJA", "NN", "ADV", "APPR", "ADJA"], "meter": "+-+---+--+--", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Seele n\u00e4her mir bringt, l\u00e4chelnd dem Sterblichen,", "tokens": ["See\u00b7le", "n\u00e4\u00b7her", "mir", "bringt", ",", "l\u00e4\u00b7chelnd", "dem", "Sterb\u00b7li\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PPER", "VVFIN", "$,", "ADJD", "ART", "NN", "$,"], "meter": "+-+---+--+--", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Wo ich zage, des Lebens", "tokens": ["Wo", "ich", "za\u00b7ge", ",", "des", "Le\u00b7bens"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "PPER", "VVFIN", "$,", "ART", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Reine Tiefe zu Reife bringt.", "tokens": ["Rei\u00b7ne", "Tie\u00b7fe", "zu", "Rei\u00b7fe", "bringt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.7": {"line.1": {"text": "Sch\u00f6pferischer, o wann, Genius unsers Volks,", "tokens": ["Sch\u00f6p\u00b7fe\u00b7ri\u00b7scher", ",", "o", "wann", ",", "Ge\u00b7nius", "un\u00b7sers", "Volks", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "FM", "PWAV", "$,", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Wann erscheinest du ganz, Seele des Vaterlands,", "tokens": ["Wann", "er\u00b7schei\u00b7nest", "du", "ganz", ",", "See\u00b7le", "des", "Va\u00b7ter\u00b7lands", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "$,", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Da\u00df ich tiefer mich beuge,", "tokens": ["Da\u00df", "ich", "tie\u00b7fer", "mich", "beu\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "PPER", "VVFIN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Da\u00df die leiseste Saite selbst", "tokens": ["Da\u00df", "die", "lei\u00b7ses\u00b7te", "Sai\u00b7te", "selbst"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "ADV"], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.8": {"line.1": {"text": "Mir verstumme vor dir, da\u00df ich besch\u00e4mt", "tokens": ["Mir", "ver\u00b7stum\u00b7me", "vor", "dir", ",", "da\u00df", "ich", "be\u00b7sch\u00e4mt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "$,", "KOUS", "PPER", "VVFIN"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Eine Blume der Nacht, himmlischer Tag, vor dir", "tokens": ["Ei\u00b7ne", "Blu\u00b7me", "der", "Nacht", ",", "himm\u00b7li\u00b7scher", "Tag", ",", "vor", "dir"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "$,", "ADJA", "NN", "$,", "APPR", "PPER"], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.3": {"text": "Enden m\u00f6ge mit Freuden,", "tokens": ["En\u00b7den", "m\u00f6\u00b7ge", "mit", "Freu\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "APPR", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Wenn sie alle, mit denen ich", "tokens": ["Wenn", "sie", "al\u00b7le", ",", "mit", "de\u00b7nen", "ich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "PIS", "$,", "APPR", "PRELS", "PPER"], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.9": {"line.1": {"text": "Vormals trauerte, wenn unsere St\u00e4dte nun", "tokens": ["Vor\u00b7mals", "trau\u00b7er\u00b7te", ",", "wenn", "un\u00b7se\u00b7re", "St\u00e4d\u00b7te", "nun"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$,", "KOUS", "PPOSAT", "NN", "ADV"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Hell und offen und wach, reineren Feuers voll", "tokens": ["Hell", "und", "of\u00b7fen", "und", "wach", ",", "rei\u00b7ne\u00b7ren", "Feu\u00b7ers", "voll"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "KON", "ADJD", "KON", "ADJD", "$,", "ADJA", "NN", "ADJD"], "meter": "+-+--+---+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Und die Berge des deutschen", "tokens": ["Und", "die", "Ber\u00b7ge", "des", "deut\u00b7schen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ART", "ADJA"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Landes Berge der Musen sind,", "tokens": ["Lan\u00b7des", "Ber\u00b7ge", "der", "Mu\u00b7sen", "sind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "ART", "NN", "VAFIN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.10": {"line.1": {"text": "Wie die herrlichen einst, Pindos und Helikon,", "tokens": ["Wie", "die", "herr\u00b7li\u00b7chen", "einst", ",", "Pin\u00b7dos", "und", "He\u00b7li\u00b7kon", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "ADV", "$,", "NE", "KON", "NN", "$,"], "meter": "--+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und Parnassos, und rings unter des Vaterlands", "tokens": ["Und", "Par\u00b7nas\u00b7sos", ",", "und", "rings", "un\u00b7ter", "des", "Va\u00b7ter\u00b7lands"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "$,", "KON", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Goldnem Himmel die freie,", "tokens": ["Gold\u00b7nem", "Him\u00b7mel", "die", "frei\u00b7e", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "ADJA", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Klare, geistige Freude gl\u00e4nzt.", "tokens": ["Kla\u00b7re", ",", "geis\u00b7ti\u00b7ge", "Freu\u00b7de", "gl\u00e4nzt", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.11": {"line.1": {"text": "Wohl ist enge begrenzt unsere Lebenszeit,", "tokens": ["Wohl", "ist", "en\u00b7ge", "be\u00b7grenzt", "un\u00b7se\u00b7re", "Le\u00b7bens\u00b7zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADJA", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "--+--+---+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Unserer Jahre Zahl sehen und z\u00e4hlen wir,", "tokens": ["Un\u00b7se\u00b7rer", "Jah\u00b7re", "Zahl", "se\u00b7hen", "und", "z\u00e4h\u00b7len", "wir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "VVINF", "KON", "VVFIN", "PPER", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Doch die Jahre der V\u00f6lker,", "tokens": ["Doch", "die", "Jah\u00b7re", "der", "V\u00f6l\u00b7ker", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Sah ein sterbliches Auge sie?", "tokens": ["Sah", "ein", "sterb\u00b7li\u00b7ches", "Au\u00b7ge", "sie", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "PPER", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.12": {"line.1": {"text": "Wenn die Seele dir auch \u00fcber die eigne Zeit", "tokens": ["Wenn", "die", "See\u00b7le", "dir", "auch", "\u00fc\u00b7ber", "die", "eig\u00b7ne", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "PPER", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Sich, die sehnende, schwingt, trauernd verweilest du", "tokens": ["Sich", ",", "die", "seh\u00b7nen\u00b7de", ",", "schwingt", ",", "trau\u00b7ernd", "ver\u00b7wei\u00b7lest", "du"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PRF", "$,", "PRELS", "VVFIN", "$,", "VVFIN", "$,", "ADJD", "VVFIN", "PPER"], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.3": {"text": "Dann am kalten Gestade", "tokens": ["Dann", "am", "kal\u00b7ten", "Ge\u00b7sta\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPRART", "ADJA", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Bei den Deinen und kennst sie nie,", "tokens": ["Bei", "den", "Dei\u00b7nen", "und", "kennst", "sie", "nie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KON", "VVFIN", "PPER", "ADV", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.13": {"line.1": {"text": "Und die K\u00fcnftigen auch, sie, die Verhei\u00dfenen,", "tokens": ["Und", "die", "K\u00fcnf\u00b7ti\u00b7gen", "auch", ",", "sie", ",", "die", "Ver\u00b7hei\u00b7\u00dfe\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "$,", "PPER", "$,", "ART", "NN", "$,"], "meter": "--+----+-+--", "measure": "anapaest.init"}, "line.2": {"text": "Wo, wo si