{"dta.poem.20541": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Widmung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1848", "urn": "urn:nbn:de:kobv:b4-25129-1", "language": ["de:0.99"], "booktitle": "[N. N.]: Erinnerungen eines freiwilligen reitenden J\u00e4gers aus den Kriegsjahren 1813\u20131815. Berlin, 1848."}, "poem": {"stanza.1": {"line.1": {"text": "Vor Dir, o Gott, der Erde Herr, der Himmel,", "tokens": ["Vor", "Dir", ",", "o", "Gott", ",", "der", "Er\u00b7de", "Herr", ",", "der", "Him\u00b7mel", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "FM", "NN", "$,", "ART", "NN", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In eigner Schwachheit ich hier flehend steh\u2019.", "tokens": ["In", "eig\u00b7ner", "Schwach\u00b7heit", "ich", "hier", "fle\u00b7hend", "steh'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPER", "ADV", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "O Herr, zu Dir ich aus dem Erdgewimmel", "tokens": ["O", "Herr", ",", "zu", "Dir", "ich", "aus", "dem", "Erd\u00b7ge\u00b7wim\u00b7mel"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "NN", "$,", "APPR", "PPER", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Vertrauend, glaubend, hoffend aufw\u00e4rts seh\u2019.", "tokens": ["Ver\u00b7trau\u00b7end", ",", "glau\u00b7bend", ",", "hof\u00b7fend", "auf\u00b7w\u00e4rts", "seh'", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "ADJD", "$,", "ADJD", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Gew\u00e4hr\u2019 mir Beistand in erlaubten Dingen,", "tokens": ["Ge\u00b7w\u00e4hr'", "mir", "Bei\u00b7stand", "in", "er\u00b7laub\u00b7ten", "Din\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Lass\u2019, was ich m\u00f6chte, mich mit Kraft vollbringen!", "tokens": ["Lass'", ",", "was", "ich", "m\u00f6ch\u00b7te", ",", "mich", "mit", "Kraft", "voll\u00b7brin\u00b7gen", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWS", "PPER", "VMFIN", "$,", "PRF", "APPR", "NN", "VVINF", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}, "stanza.2": {"line.1": {"text": "Schon oft hatt\u2019 ich und sogar fest beschlossen,", "tokens": ["Schon", "oft", "hatt'", "ich", "und", "so\u00b7gar", "fest", "be\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "KON", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zu schildern Einzelnheiten aus der Zeit,", "tokens": ["Zu", "schil\u00b7dern", "Ein\u00b7zeln\u00b7hei\u00b7ten", "aus", "der", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die wir zusammen, edle Kampfgenossen", "tokens": ["Die", "wir", "zu\u00b7sam\u00b7men", ",", "ed\u00b7le", "Kampf\u00b7ge\u00b7nos\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "PPER", "PTKVZ", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Verlebt in jugendlicher Heiterkeit.", "tokens": ["Ver\u00b7lebt", "in", "ju\u00b7gend\u00b7li\u00b7cher", "Hei\u00b7ter\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Indessen ich mi\u00dftraute eignem Geiste,", "tokens": ["In\u00b7des\u00b7sen", "ich", "mi\u00df\u00b7trau\u00b7te", "eig\u00b7nem", "Geis\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und auch mit Recht, da\u00df er dies wirklich leiste.", "tokens": ["Und", "auch", "mit", "Recht", ",", "da\u00df", "er", "dies", "wirk\u00b7lich", "leis\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "NN", "$,", "KOUS", "PPER", "PDS", "ADJD", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Doch immer weiter zieht in Wolkenfernen", "tokens": ["Doch", "im\u00b7mer", "wei\u00b7ter", "zieht", "in", "Wol\u00b7ken\u00b7fer\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das einst durchlauf\u2019ne sch\u00f6ne J\u00e4gerland!", "tokens": ["Das", "einst", "durch\u00b7lauf'\u00b7ne", "sch\u00f6\u00b7ne", "J\u00e4\u00b7ger\u00b7land", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Manch braver Freund weilt lange schon auf Sternen,", "tokens": ["Manch", "bra\u00b7ver", "Freund", "weilt", "lan\u00b7ge", "schon", "auf", "Ster\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVFIN", "ADV", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dort, wo Sein Geist die wahre Heimath fand.", "tokens": ["Dort", ",", "wo", "Sein", "Geist", "die", "wah\u00b7re", "Hei\u00b7math", "fand", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "PPOSAT", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Verfasser drum gieb auf Dein albern Tr\u00e4umen!", "tokens": ["Ver\u00b7fas\u00b7ser", "drum", "gieb", "auf", "Dein", "al\u00b7bern", "Tr\u00e4u\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PAV", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Kleb\u2019 Vers an Vers! \u2014 Sollst, darfst nicht l\u00e4nger s\u00e4umen!", "tokens": ["Kleb'", "Vers", "an", "Vers", "!", "Sollst", ",", "darfst", "nicht", "l\u00e4n\u00b7ger", "s\u00e4u\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPR", "NN", "$.", "$(", "VMFIN", "$,", "VMFIN", "PTKNEG", "ADJD", "VVINF", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Nach Leib\u2019s Verwesung folgt auf\u2019s Neu\u2019 Belebung,", "tokens": ["Nach", "Leib's", "Ver\u00b7we\u00b7sung", "folgt", "auf's", "Neu'", "Be\u00b7le\u00b7bung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "VVFIN", "APPRART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Unsterblichkeit, Dir geht voraus der Tod!", "tokens": ["U\u00b7nsterb\u00b7lich\u00b7keit", ",", "Dir", "geht", "vo\u00b7raus", "der", "Tod", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "++-+-+-+-+", "measure": "iambic.penta.spondeus"}, "line.3": {"text": "Der Schmach von Preu\u00dfen folgte die Erhebung,", "tokens": ["Der", "Schmach", "von", "Preu\u00b7\u00dfen", "folg\u00b7te", "die", "Er\u00b7he\u00b7bung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Nach Nacht erschien der Freiheit Morgenroth.", "tokens": ["Nach", "Nacht", "er\u00b7schien", "der", "Frei\u00b7heit", "Mor\u00b7gen\u00b7roth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Jena und Austerlitz in tr\u00fcben Tagen,", "tokens": ["Je\u00b7na", "und", "Aus\u00b7ter\u00b7litz", "in", "tr\u00fc\u00b7ben", "Ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Sie waren Grund, da\u00df Leipz\u2019ger Schlacht geschlagen.", "tokens": ["Sie", "wa\u00b7ren", "Grund", ",", "da\u00df", "Leip\u00b7z'\u00b7ger", "Schlacht", "ge\u00b7schla\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "$,", "KOUS", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.5": {"line.1": {"text": "Kam\u2019raden denn, die Jhr seit langen Jahren", "tokens": ["Kam'\u00b7ra\u00b7den", "denn", ",", "die", "Ihr", "seit", "lan\u00b7gen", "Jah\u00b7ren"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "$,", "PRELS", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So freudig eilt zu dem Erinn\u2019rungs-Mahl.", "tokens": ["So", "freu\u00b7dig", "eilt", "zu", "dem", "Er\u00b7inn'\u00b7rungs\u00b7Mahl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Nicht Haar allein nur, h\u00f6rt! nein, auch die Schaaren", "tokens": ["Nicht", "Haar", "al\u00b7lein", "nur", ",", "h\u00f6rt", "!", "nein", ",", "auch", "die", "Schaa\u00b7ren"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PTKNEG", "NN", "ADV", "ADV", "$,", "VVFIN", "$.", "PTKANT", "$,", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der alten K\u00e4mpfer nehmen ab an Zahl.", "tokens": ["Der", "al\u00b7ten", "K\u00e4mp\u00b7fer", "neh\u00b7men", "ab", "an", "Zahl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PTKVZ", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Freiwill\u2019ge J\u00e4ger, leset hier die Zeilen,", "tokens": ["Frei\u00b7will'\u00b7ge", "J\u00e4\u00b7ger", ",", "le\u00b7set", "hier", "die", "Zei\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.6": {"text": "Denkt unsres Kampfes oft, nicht nur zuweilen!", "tokens": ["Denkt", "uns\u00b7res", "Kamp\u00b7fes", "oft", ",", "nicht", "nur", "zu\u00b7wei\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADV", "$,", "PTKNEG", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}