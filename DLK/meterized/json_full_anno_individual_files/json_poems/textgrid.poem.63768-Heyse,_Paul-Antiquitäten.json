{"textgrid.poem.63768": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "Antiquit\u00e4ten", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Etruskervasen, Urnen, Opferschalen,", "tokens": ["E\u00b7trus\u00b7ker\u00b7va\u00b7sen", ",", "Ur\u00b7nen", ",", "Op\u00b7fer\u00b7scha\u00b7len", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Amphoren, sch\u00f6n bemalt, mit m\u00e4cht'gem Bauch,", "tokens": ["Am\u00b7pho\u00b7ren", ",", "sch\u00f6n", "be\u00b7malt", ",", "mit", "m\u00e4cht'\u00b7gem", "Bauch", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "VVFIN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Pompejis L\u00e4mpchen, noch geschw\u00e4rzt vom Rauch,", "tokens": ["Pom\u00b7pe\u00b7jis", "L\u00e4mp\u00b7chen", ",", "noch", "ge\u00b7schw\u00e4rzt", "vom", "Rauch", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "ADV", "VVPP", "APPRART", "NN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.4": {"text": "Und Ring' und M\u00fcnzen, Spangen und Sandalen \u2013", "tokens": ["Und", "Ring'", "und", "M\u00fcn\u00b7zen", ",", "Span\u00b7gen", "und", "San\u00b7da\u00b7len", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "$,", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Was nur verschonten Goten und Vandalen,", "tokens": ["Was", "nur", "ver\u00b7schon\u00b7ten", "Go\u00b7ten", "und", "Van\u00b7da\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+--++-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Damit wir lernten alter Zeiten Brauch,", "tokens": ["Da\u00b7mit", "wir", "lern\u00b7ten", "al\u00b7ter", "Zei\u00b7ten", "Brauch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Hier liegt's geh\u00e4uft, und mit der Ehrfurcht Hauch", "tokens": ["Hier", "liegt's", "ge\u00b7h\u00e4uft", ",", "und", "mit", "der", "Ehr\u00b7furcht", "Hauch"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "VVPP", "$,", "KON", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Beschleichen sacht dich der Begierde Qualen.", "tokens": ["Be\u00b7schlei\u00b7chen", "sacht", "dich", "der", "Be\u00b7gier\u00b7de", "Qua\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Doch tr\u00f6ste dich, wenn dir die Reisekasse", "tokens": ["Doch", "tr\u00f6s\u00b7te", "dich", ",", "wenn", "dir", "die", "Rei\u00b7se\u00b7kas\u00b7se"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "$,", "KOUS", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Entsagung auferlegt zu deiner Pein", "tokens": ["Ent\u00b7sa\u00b7gung", "auf\u00b7er\u00b7legt", "zu", "dei\u00b7ner", "Pein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVPP", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Bei all den teuren Sch\u00e4tzen dieser Bude.", "tokens": ["Bei", "all", "den", "teu\u00b7ren", "Sch\u00e4t\u00b7zen", "die\u00b7ser", "Bu\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ART", "ADJA", "NN", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Man fabriziert hier Altertum in Masse;", "tokens": ["Man", "fab\u00b7ri\u00b7ziert", "hier", "Al\u00b7ter\u00b7tum", "in", "Mas\u00b7se", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Echt ist und alt der H\u00e4ndler nur allein,", "tokens": ["Echt", "ist", "und", "alt", "der", "H\u00e4nd\u00b7ler", "nur", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "KON", "ADJD", "ART", "NN", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ein echter alter Fuchs und Ghettojude.", "tokens": ["Ein", "ech\u00b7ter", "al\u00b7ter", "Fuchs", "und", "Ghet\u00b7to\u00b7ju\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NE", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Etruskervasen, Urnen, Opferschalen,", "tokens": ["E\u00b7trus\u00b7ker\u00b7va\u00b7sen", ",", "Ur\u00b7nen", ",", "Op\u00b7fer\u00b7scha\u00b7len", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Amphoren, sch\u00f6n bemalt, mit m\u00e4cht'gem Bauch,", "tokens": ["Am\u00b7pho\u00b7ren", ",", "sch\u00f6n", "be\u00b7malt", ",", "mit", "m\u00e4cht'\u00b7gem",