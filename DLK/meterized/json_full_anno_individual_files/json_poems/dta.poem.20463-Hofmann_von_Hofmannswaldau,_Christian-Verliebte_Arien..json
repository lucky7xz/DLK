{"dta.poem.20463": {"metadata": {"author": {"name": "Hofmann von Hofmannswaldau, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Verliebte Arien.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1695", "urn": "urn:nbn:de:kobv:b4-200905197751", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie lange soll noch meine pein/", "tokens": ["Wie", "lan\u00b7ge", "soll", "noch", "mei\u00b7ne", "pein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VMFIN", "ADV", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Durch dich/ o grausame Caliste/", "tokens": ["Durch", "dich", "/", "o", "grau\u00b7sa\u00b7me", "Ca\u00b7lis\u00b7te", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$(", "FM", "FM", "FM", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "In der verzweifflungs \u00f6den w\u00fcste", "tokens": ["In", "der", "ver\u00b7zweif\u00b7flungs", "\u00f6\u00b7den", "w\u00fcs\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein abgematter pilgrim seyn?", "tokens": ["Ein", "ab\u00b7ge\u00b7mat\u00b7ter", "pil\u00b7grim", "seyn", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die zeit verlieret jahr um jahr/", "tokens": ["Die", "zeit", "ver\u00b7lie\u00b7ret", "jahr", "um", "jahr", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPR", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df ich nach meinem tod wallfahrte/", "tokens": ["Da\u00df", "ich", "nach", "mei\u00b7nem", "tod", "wall\u00b7fahr\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und auff die letztere gefahr/", "tokens": ["Und", "auff", "die", "letz\u00b7te\u00b7re", "ge\u00b7fahr", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Als bote/ den du schickst/ auff deine botschafft warte.", "tokens": ["Als", "bo\u00b7te", "/", "den", "du", "schickst", "/", "auff", "dei\u00b7ne", "bot\u00b7schafft", "war\u00b7te", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "$(", "ART", "PPER", "VVFIN", "$(", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Zwar klag ichs nicht der h\u00f6hnschen welt/", "tokens": ["Zwar", "klag", "ichs", "nicht", "der", "h\u00f6hn\u00b7schen", "welt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PTKNEG", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich kleide mich mit stillem kummer:", "tokens": ["Ich", "klei\u00b7de", "mich", "mit", "stil\u00b7lem", "kum\u00b7mer", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Doch glaube da\u00df ein ieder schlummer", "tokens": ["Doch", "glau\u00b7be", "da\u00df", "ein", "ie\u00b7der", "schlum\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "KOUS", "ART", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mir deinen zorn f\u00fcr augen stellt.", "tokens": ["Mir", "dei\u00b7nen", "zorn", "f\u00fcr", "au\u00b7gen", "stellt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Lacht gleich die lippe manches mahl/", "tokens": ["Lacht", "gleich", "die", "lip\u00b7pe", "man\u00b7ches", "mahl", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "VVFIN", "PIS", "ADV", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Nur fr\u00f6lich vor der welt zuscheinen;", "tokens": ["Nur", "fr\u00f6\u00b7lich", "vor", "der", "welt", "zu\u00b7schei\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Ist doch das hertz ein trauer-saal/", "tokens": ["Ist", "doch", "das", "hertz", "ein", "trau\u00b7e\u00b7rsaal", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wo die gedancken mich als leiche schon beweinen.", "tokens": ["Wo", "die", "ge\u00b7dan\u00b7cken", "mich", "als", "lei\u00b7che", "schon", "be\u00b7wei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PPER", "KOUS", "ADJA", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Mein gantzes leben streicht dahin", "tokens": ["Mein", "gant\u00b7zes", "le\u00b7ben", "streicht", "da\u00b7hin"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "VVINF", "VVFIN", "PAV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In meynung bald nicht mehr zu leben:", "tokens": ["In", "mey\u00b7nung", "bald", "nicht", "mehr", "zu", "le\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "PTKNEG", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und was mir einen trost soll geben/", "tokens": ["Und", "was", "mir", "ei\u00b7nen", "trost", "soll", "ge\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "ART", "NN", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Spricht: da\u00df ich noch mehr w\u00fcrdig bin.", "tokens": ["Spricht", ":", "da\u00df", "ich", "noch", "mehr", "w\u00fcr\u00b7dig", "bin", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "KOUS", "PPER", "ADV", "ADV", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich sterbe t\u00e4glich ohne todt/", "tokens": ["Ich", "ster\u00b7be", "t\u00e4g\u00b7lich", "oh\u00b7ne", "todt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der kalte schwei\u00df auff meinen wangen", "tokens": ["Der", "kal\u00b7te", "schwei\u00df", "auff", "mei\u00b7nen", "wan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Ist zwar ein vorbot dieser noth.", "tokens": ["Ist", "zwar", "ein", "vor\u00b7bot", "die\u00b7ser", "noth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "VVFIN", "PDAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Nur da\u00df den letzten sto\u00df ich noch nicht kan empfangen.", "tokens": ["Nur", "da\u00df", "den", "letz\u00b7ten", "sto\u00df", "ich", "noch", "nicht", "kan", "emp\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "ART", "ADJA", "VVFIN", "PPER", "ADV", "PTKNEG", "VMFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Ich scheu mich f\u00fcr dem tode nicht/", "tokens": ["Ich", "scheu", "mich", "f\u00fcr", "dem", "to\u00b7de", "nicht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "NN", "PTKNEG", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nur daurt es mich dich zu verlassen/", "tokens": ["Nur", "daurt", "es", "mich", "dich", "zu", "ver\u00b7las\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "PPER", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und durch das traurige verblassen", "tokens": ["Und", "durch", "das", "trau\u00b7ri\u00b7ge", "ver\u00b7blas\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zu meiden deiner augen licht.", "tokens": ["Zu", "mei\u00b7den", "dei\u00b7ner", "au\u00b7gen", "licht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mein leben lieb ich/ weil du lebst/", "tokens": ["Mein", "le\u00b7ben", "lieb", "ich", "/", "weil", "du", "lebst", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VVFIN", "ADJD", "PPER", "$(", "KOUS", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df ich in solchem dich kan lieben/", "tokens": ["Da\u00df", "ich", "in", "sol\u00b7chem", "dich", "kan", "lie\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PIAT", "PPER", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Denn weil du meinen leib begr\u00e4bst/", "tokens": ["Denn", "weil", "du", "mei\u00b7nen", "leib", "be\u00b7gr\u00e4bst", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ist weder lust noch schertz der aschen \u00fcberblieben.", "tokens": ["Ist", "we\u00b7der", "lust", "noch", "schertz", "der", "asc\u00b7hen", "\u00fc\u00b7berb\u00b7lie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "KON", "VVFIN", "ADV", "ADJD", "ART", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Caliste sey nicht fel\u00df und stein/", "tokens": ["Ca\u00b7lis\u00b7te", "sey", "nicht", "fel\u00df", "und", "stein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PTKNEG", "ADJD", "KON", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Soll ich ein leben schon verderben?", "tokens": ["Soll", "ich", "ein", "le\u00b7ben", "schon", "ver\u00b7der\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKVZ", "VVFIN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Was mach ich/ wann ich werde sterben?", "tokens": ["Was", "mach", "ich", "/", "wann", "ich", "wer\u00b7de", "ster\u00b7ben", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$(", "PWAV", "PPER", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ists nicht genug dann todt zu seyn?", "tokens": ["Ists", "nicht", "ge\u00b7nug", "dann", "todt", "zu", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "ADV", "ADV", "ADJD", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Zweymahl zu sterben ist zu viel/", "tokens": ["Zwey\u00b7mahl", "zu", "ster\u00b7ben", "ist", "zu", "viel", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKZU", "VVINF", "VAFIN", "PTKA", "PIS", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und zwar dich ewig zu verlieren/", "tokens": ["Und", "zwar", "dich", "e\u00b7wig", "zu", "ver\u00b7lie\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPER", "ADJD", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Ich fehl lebendig meinem ziel/", "tokens": ["Ich", "fehl", "le\u00b7ben\u00b7dig", "mei\u00b7nem", "ziel", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ADJD", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und in dem tode kan ich gar dich nicht ber\u00fchren.", "tokens": ["Und", "in", "dem", "to\u00b7de", "kan", "ich", "gar", "dich", "nicht", "be\u00b7r\u00fch\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VMFIN", "PPER", "ADV", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Hastu ein hertz von fleisch und blut/", "tokens": ["Has\u00b7tu", "ein", "hertz", "von", "fleisch", "und", "blut", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "APPR", "ADJD", "KON", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So hastu/ als ein mensch/ empfinden/", "tokens": ["So", "has\u00b7tu", "/", "als", "ein", "mensch", "/", "emp\u00b7fin\u00b7den", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VAFIN", "$(", "KOUS", "ART", "NN", "$(", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Du straffst zu hart so kleine s\u00fcnden/", "tokens": ["Du", "straffst", "zu", "hart", "so", "klei\u00b7ne", "s\u00fcn\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKA", "ADJD", "ADV", "ADJA", "ADJA", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da doch dein zorn was h\u00f6hers thut/", "tokens": ["Da", "doch", "dein", "zorn", "was", "h\u00f6\u00b7hers", "thut", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPOSAT", "NN", "PWS", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der himmel der dir gn\u00e4dig ist/", "tokens": ["Der", "him\u00b7mel", "der", "dir", "gn\u00e4\u00b7dig", "ist", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "PPER", "ADJD", "VAFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Heist dich nicht unbarmhertzig bleiben:", "tokens": ["Heist", "dich", "nicht", "un\u00b7barm\u00b7hert\u00b7zig", "blei\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und weil du selbst ein s\u00fcnder bist/", "tokens": ["Und", "weil", "du", "selbst", "ein", "s\u00fcn\u00b7der", "bist", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "ART", "ADJA", "VAFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Mu\u00df keinen \u00fcbermuth dein unmuth mit mir treiben.", "tokens": ["Mu\u00df", "kei\u00b7nen", "\u00fc\u00b7ber\u00b7muth", "dein", "un\u00b7muth", "mit", "mir", "trei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIAT", "APPR", "PPOSAT", "NN", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Doch ist mein tod bey dir gemacht/", "tokens": ["Doch", "ist", "mein", "tod", "bey", "dir", "ge\u00b7macht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPOSAT", "NN", "APPR", "PPER", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wohlan! so schick ich mich zum ende/", "tokens": ["Wo\u00b7hlan", "!", "so", "schick", "ich", "mich", "zum", "en\u00b7de", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "ADV", "VVFIN", "PPER", "PRF", "APPRART", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und spreche/ da\u00df Calistens h\u00e4nde", "tokens": ["Und", "spre\u00b7che", "/", "da\u00df", "Ca\u00b7lis\u00b7tens", "h\u00e4n\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "$(", "KOUS", "NE", "AD