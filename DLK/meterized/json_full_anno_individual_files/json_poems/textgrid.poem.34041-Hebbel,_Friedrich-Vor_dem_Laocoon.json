{"textgrid.poem.34041": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Vor dem Laocoon", "genre": "verse", "period": "N.A.", "pub_year": 1838, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Michel Angelo hie\u00df als Wunder der Kunst dich willkommen,", "tokens": ["Mi\u00b7chel", "An\u00b7ge\u00b7lo", "hie\u00df", "als", "Wun\u00b7der", "der", "Kunst", "dich", "will\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "KOUS", "NN", "ART", "NN", "PPER", "ADJD", "$,"], "meter": "+-+--+-+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Weil du als Gegengewicht gegen den sch\u00f6nen Apoll,", "tokens": ["Weil", "du", "als", "Ge\u00b7gen\u00b7ge\u00b7wicht", "ge\u00b7gen", "den", "sch\u00f6\u00b7nen", "A\u00b7poll", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "KOUS", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Der den Raphael trug und ihn verneinte, ihm dientest;", "tokens": ["Der", "den", "Ra\u00b7phael", "trug", "und", "ihn", "ver\u00b7nein\u00b7te", ",", "ihm", "dien\u00b7test", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ART", "NE", "VVFIN", "KON", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "$."], "meter": "--+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Mancher sprach es ihm nach, aber er sagte zu viel.", "tokens": ["Man\u00b7cher", "sprach", "es", "ihm", "nach", ",", "a\u00b7ber", "er", "sag\u00b7te", "zu", "viel", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "PPER", "PTKVZ", "$,", "KON", "PPER", "VVFIN", "APPR", "PIS", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.5": {"text": "Was die Wahrheit vermag, das zeigst du deutlich, o Gruppe,", "tokens": ["Was", "die", "Wahr\u00b7heit", "ver\u00b7mag", ",", "das", "zeigst", "du", "deut\u00b7lich", ",", "o", "Grup\u00b7pe", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VVFIN", "$,", "PDS", "VVFIN", "PPER", "ADJD", "$,", "FM", "NN", "$,"], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "Deutlicher zeigst du jedoch, da\u00df sie nicht Alles vermag!", "tokens": ["Deut\u00b7li\u00b7cher", "zeigst", "du", "je\u00b7doch", ",", "da\u00df", "sie", "nicht", "Al\u00b7les", "ver\u00b7mag", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ADV", "$,", "KOUS", "PPER", "PTKNEG", "PIS", "VVFIN", "$."], "meter": "+--+-+-+-++--+", "measure": "iambic.septa.invert"}}, "stanza.2": {"line.1": {"text": "Michel Angelo hie\u00df als Wunder der Kunst dich willkommen,", "tokens": ["Mi\u00b7chel", "An\u00b7ge\u00b7lo", "hie\u00df", "als", "Wun\u00b7der", "der", "Kunst", "dich", "will\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "KOUS", "NN", "ART", "NN", "PPER", "ADJD", "$,"], "meter": "+-+--+-+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Weil du als Gegengewicht gegen den sch\u00f6nen Apoll,", "tokens": ["Weil", "du", "als", "Ge\u00b7gen\u00b7ge\u00b7wicht", "ge\u00b7gen", "den", "sch\u00f6\u00b7nen", "A\u00b7poll", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "KOUS", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-+-+-", "measure": "i