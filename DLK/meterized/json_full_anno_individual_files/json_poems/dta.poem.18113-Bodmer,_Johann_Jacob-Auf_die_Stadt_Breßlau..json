{"dta.poem.18113": {"metadata": {"author": {"name": "Bodmer, Johann Jacob", "birth": "N.A.", "death": "N.A."}, "title": "Auf die Stadt Bre\u00dflau.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-200905198432", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Als Themis aus der Welt zu ziehn ihr vorgenommen,", "tokens": ["Als", "The\u00b7mis", "aus", "der", "Welt", "zu", "ziehn", "ihr", "vor\u00b7ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "APPR", "ART", "NN", "PTKZU", "VVINF", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Soll unterweges sie auch seyn nach Bre\u00dflau kommen,", "tokens": ["Soll", "un\u00b7ter\u00b7we\u00b7ges", "sie", "auch", "seyn", "nach", "Bre\u00df\u00b7lau", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PPER", "ADV", "PPOSAT", "APPR", "NE", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und weil sie hat vermeynt, sie sey nun allbereit", "tokens": ["Und", "weil", "sie", "hat", "ver\u00b7meynt", ",", "sie", "sey", "nun", "all\u00b7be\u00b7reit"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "VAFIN", "VVPP", "$,", "PPER", "VAFIN", "ADV", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Jm Himmel, ist sie da noch bi\u00df auf diese Zeit.", "tokens": ["Jm", "Him\u00b7mel", ",", "ist", "sie", "da", "noch", "bi\u00df", "auf", "die\u00b7se", "Zeit", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "VAFIN", "PPER", "ADV", "ADV", "ADV", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}