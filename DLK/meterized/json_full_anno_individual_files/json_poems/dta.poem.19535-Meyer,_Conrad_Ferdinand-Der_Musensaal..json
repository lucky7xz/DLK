{"dta.poem.19535": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Der Musensaal.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-200905193933", "language": ["de:0.99"], "booktitle": "Meyer, Conrad Ferdinand: Gedichte. Leipzig, 1882."}, "poem": {"stanza.1": {"line.1": {"text": "J\u00fcngst trug ein Traum auf dunkler Schwinge mich", "tokens": ["J\u00fcngst", "trug", "ein", "Traum", "auf", "dunk\u00b7ler", "Schwin\u00b7ge", "mich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Nach Rom der ew'gen Stadt. Den Vatican", "tokens": ["Nach", "Rom", "der", "ew'\u00b7gen", "Stadt", ".", "Den", "Va\u00b7ti\u00b7can"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "NE", "ART", "ADJA", "NN", "$.", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Betrat ich. Ich betrat den Musensaal", "tokens": ["Be\u00b7trat", "ich", ".", "Ich", "be\u00b7trat", "den", "Mu\u00b7sen\u00b7saal"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$.", "PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Verwundert, denn er war ein andrer heut,", "tokens": ["Ver\u00b7wun\u00b7dert", ",", "denn", "er", "war", "ein", "an\u00b7drer", "heut", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "KON", "PPER", "VAFIN", "ART", "ADJA", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Als ich geschaut mit jungen Augen ihn,", "tokens": ["Als", "ich", "ge\u00b7schaut", "mit", "jun\u00b7gen", "Au\u00b7gen", "ihn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "APPR", "ADJA", "NN", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Da Pio Nono h\u00f6chster Priester war.", "tokens": ["Da", "Pio", "No\u00b7no", "h\u00f6chs\u00b7ter", "Pries\u00b7ter", "war", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "NE", "ADJA", "NN", "VAFIN", "$."], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.7": {"text": "Verschwunden aus dem edeln Octogon,", "tokens": ["Ver\u00b7schwun\u00b7den", "aus", "dem", "e\u00b7deln", "Oc\u00b7to\u00b7gon", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Dem kuppelhellen, war der Musaget,", "tokens": ["Dem", "kup\u00b7pel\u00b7hel\u00b7len", ",", "war", "der", "Mu\u00b7sa\u00b7get", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Apollo, der die Cither zierlich schlug,", "tokens": ["A\u00b7pol\u00b7lo", ",", "der", "die", "Cit\u00b7her", "zier\u00b7lich", "schlug", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.10": {"text": "Voranzugehn dem Chor tanzmeisterlich.", "tokens": ["Vor\u00b7an\u00b7zu\u00b7gehn", "dem", "Chor", "tanz\u00b7meis\u00b7ter\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Die Neune sa\u00dfen oder standen nicht", "tokens": ["Die", "Neu\u00b7ne", "sa\u00b7\u00dfen", "o\u00b7der", "stan\u00b7den", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "PTKNEG"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Umher vertheilt in sch\u00f6nen Stellungen \u2014", "tokens": ["Um\u00b7her", "ver\u00b7theilt", "in", "sch\u00f6\u00b7nen", "Stel\u00b7lun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-++-", "measure": "unknown.measure.penta"}, "line.13": {"text": "In wilder Gruppe schritten eilig sie,", "tokens": ["In", "wil\u00b7der", "Grup\u00b7pe", "schrit\u00b7ten", "ei\u00b7lig", "sie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ADJD", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Wie Schnitterinnen, die auf blachem Feld", "tokens": ["Wie", "Schnit\u00b7te\u00b7rin\u00b7nen", ",", "die", "auf", "bla\u00b7chem", "Feld"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Ein leuchtendes Gewitter \u00fcberrascht:", "tokens": ["Ein", "leuch\u00b7ten\u00b7des", "Ge\u00b7wit\u00b7ter", "\u00fc\u00b7berr\u00b7ascht", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Voran die blutige Melpomene,", "tokens": ["Vo\u00b7ran", "die", "blu\u00b7ti\u00b7ge", "Mel\u00b7po\u00b7me\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.17": {"text": "Die an den S\u00f6hnen r\u00e4cht der Vater Schuld.", "tokens": ["Die", "an", "den", "S\u00f6h\u00b7nen", "r\u00e4cht", "der", "Va\u00b7ter", "Schuld", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Sie tr\u00e4gt das Schwert und auch den Kranz von Wein.", "tokens": ["Sie", "tr\u00e4gt", "das", "Schwert", "und", "auch", "den", "Kranz", "von", "Wein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "KON", "ADV", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "\u201eein Reich\u201c, so jubelt sie, \u201ezerst\u00f6r' ich jetzt!", "tokens": ["\u201e", "ein", "Reich", "\u201c", ",", "so", "ju\u00b7belt", "sie", ",", "\u201e", "zer\u00b7st\u00f6r'", "ich", "jetzt", "!"], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "$(", "$,", "ADV", "VVFIN", "PPER", "$,", "$(", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Das Feuer knistert unter seinem Thron!", "tokens": ["Das", "Feu\u00b7er", "knis\u00b7tert", "un\u00b7ter", "sei\u00b7nem", "Thron", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Die nordische Barbarin pre\u00dft den Fu\u00df,", "tokens": ["Die", "nor\u00b7di\u00b7sche", "Bar\u00b7ba\u00b7rin", "pre\u00dft", "den", "Fu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.22": {"text": "Den plumpen, auf den Nacken eines Weibs,", "tokens": ["Den", "plum\u00b7pen", ",", "auf", "den", "Na\u00b7cken", "ei\u00b7nes", "Weibs", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "Das schmerzenreicher blickt als Niobe \u2014", "tokens": ["Das", "schmer\u00b7zen\u00b7rei\u00b7cher", "blickt", "als", "Ni\u00b7o\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "KOKOM", "NE", "$("], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.24": {"text": "Sklavin, empor! Zerbrich die Fessel! Wirf", "tokens": ["Skla\u00b7vin", ",", "em\u00b7por", "!", "Zer\u00b7brich", "die", "Fes\u00b7sel", "!", "Wirf"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["NN", "$,", "PTKVZ", "$.", "ADJD", "ART", "NN", "$.", "NE"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "Die grinsende Barbarin in den Staub! ...\u201c", "tokens": ["Die", "grin\u00b7sen\u00b7de", "Bar\u00b7ba\u00b7rin", "in", "den", "Staub", "!", "...", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "$.", "$(", "$("], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.26": {"text": "So jauchzt die blutige Melpomene \u2014", "tokens": ["So", "jauchzt", "die", "blu\u00b7ti\u00b7ge", "Mel\u00b7po\u00b7me\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.27": {"text": "Wer schreitet, schlicht gewandet, neben ihr?", "tokens": ["Wer", "schrei\u00b7tet", ",", "schlicht", "ge\u00b7wan\u00b7det", ",", "ne\u00b7ben", "ihr", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "ADJD", "VVPP", "$,", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.28": {"text": "Kalliope, die keusch und kindlich blickt,", "tokens": ["Kal\u00b7li\u00b7o\u00b7pe", ",", "die", "keusch", "und", "kind\u00b7lich", "blickt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "ADJD", "KON", "ADJD", "VVFIN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.29": {"text": "Die den erblindeten Homer gef\u00fchrt,", "tokens": ["Die", "den", "er\u00b7blin\u00b7de\u00b7ten", "Ho\u00b7mer", "ge\u00b7f\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.30": {"text": "Die tapfre Helden liebt und Schildgetos", "tokens": ["Die", "tapf\u00b7re", "Hel\u00b7den", "liebt", "und", "Schild\u00b7ge\u00b7tos"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.31": {"text": "Und Rossgestampf und dann abseits der Schlacht", "tokens": ["Und", "Ross\u00b7ge\u00b7stampf", "und", "dann", "ab\u00b7seits", "der", "Schlacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "KON", "ADV", "ADV", "ART", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.32": {"text": "In jugendzartem Busen Loose w\u00e4gt \u2014", "tokens": ["In", "ju\u00b7gen\u00b7dzar\u00b7tem", "Bu\u00b7sen", "Loo\u00b7se", "w\u00e4gt"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NE", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.33": {"text": "Mit beiden Armen in die Ferne gr\u00fc\u00dft", "tokens": ["Mit", "bei\u00b7den", "Ar\u00b7men", "in", "die", "Fer\u00b7ne", "gr\u00fc\u00dft"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.34": {"text": "Sie jetzt: \u201eBehelmte! Blonde Herzogin!", "tokens": ["Sie", "jetzt", ":", "\u201e", "Be\u00b7helm\u00b7te", "!", "Blon\u00b7de", "Her\u00b7zo\u00b7gin", "!"], "token_info": ["word", "word", "punct", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADV", "$.", "$(", "VVFIN", "$.", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.35": {"text": "Ins rauhe Heerhorn st\u00f6\u00dfest du mit Macht!", "tokens": ["Ins", "rau\u00b7he", "Heer\u00b7horn", "st\u00f6\u00b7\u00dfest", "du", "mit", "Macht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.36": {"text": "Erzklirrend springen dir die S\u00f6hne auf!", "tokens": ["Erz\u00b7klir\u00b7rend", "sprin\u00b7gen", "dir", "die", "S\u00f6h\u00b7ne", "auf", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.37": {"text": "Die V\u00f6lker richtest und beherrschest du,", "tokens": ["Die", "V\u00f6l\u00b7ker", "rich\u00b7test", "und", "be\u00b7herr\u00b7schest", "du", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.38": {"text": "Gerechte Herrin, beilgewalt'ge Frau!\u201c", "tokens": ["Ge\u00b7rech\u00b7te", "Her\u00b7rin", ",", "beil\u00b7ge\u00b7walt'\u00b7ge", "Frau", "!", "\u201c"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.39": {"text": "Weithallend redet jetzt ein m\u00e4chtig Paar,", "tokens": ["Weit\u00b7hal\u00b7lend", "re\u00b7det", "jetzt", "ein", "m\u00e4ch\u00b7tig", "Paar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ADV", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.40": {"text": "Terpsichore und Polyhymnia:", "tokens": ["Ter\u00b7psi\u00b7cho\u00b7re", "und", "Po\u00b7ly\u00b7hym\u00b7nia", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "KON", "NE", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.41": {"text": "\u201eder Tag ist fern und er erf\u00fcllt sich doch:", "tokens": ["\u201e", "der", "Tag", "ist", "fern", "und", "er", "er\u00b7f\u00fcllt", "sich", "doch", ":"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VAFIN", "ADJD", "KON", "PPER", "VVFIN", "PRF", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.42": {"text": "Die V\u00f6lker schreiten ", "tokens": ["Die", "V\u00f6l\u00b7ker", "schrei\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.43": {"text": "Sich an den H\u00e4nden haltend, frei gesellt,", "tokens": ["Sich", "an", "den", "H\u00e4n\u00b7den", "hal\u00b7tend", ",", "frei", "ge\u00b7sellt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "VVPP", "$,", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.44": {"text": "Vieltausendstimmig dr\u00f6hnt der Chorgesang!\u201c", "tokens": ["Viel\u00b7tau\u00b7sends\u00b7tim\u00b7mig", "dr\u00f6hnt", "der", "Chor\u00b7ge\u00b7sang", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.45": {"text": "\u2014 \u201eDann weicht das Leid! Nicht alles, aber doch", "tokens": ["\u201e", "Dann", "weicht", "das", "Leid", "!", "Nicht", "al\u00b7les", ",", "a\u00b7ber", "doch"], "token_info": ["punct", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["$(", "$(", "ADV", "VVFIN", "ART", "NN", "$.", "PTKNEG", "PIS", "$,", "ADV", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.46": {"text": "Das meiste Leid!\u201c Euterpe fl\u00f6tet es,", "tokens": ["Das", "meis\u00b7te", "Leid", "!", "\u201c", "Eu\u00b7ter\u00b7pe", "fl\u00f6\u00b7tet", "es", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "$(", "NE", "VVFIN", "PPER", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.47": {"text": "Das liebliche Gesch\u00f6pf, die Schmeichlerin!", "tokens": ["Das", "lieb\u00b7li\u00b7che", "Ge\u00b7sch\u00f6pf", ",", "die", "Schmeich\u00b7le\u00b7rin", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.48": {"text": "\u2014 \u201eDann f\u00fcllt\u201c, Erato lacht's mit bl\u00fch'ndem Mund,", "tokens": ["\u201e", "Dann", "f\u00fcllt", "\u201c", ",", "E\u00b7ra\u00b7to", "lacht's", "mit", "bl\u00fch'n\u00b7dem", "Mund", ","], "token_info": ["punct", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "$(", "ADV", "VVFIN", "$(", "$,", "NE", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.49": {"text": "Die sch\u00f6ne Schelmin, die das Liebeslied,", "tokens": ["Die", "sch\u00f6\u00b7ne", "Schel\u00b7min", ",", "die", "das", "Lie\u00b7bes\u00b7lied", ","], "token_info": ["word", "wo