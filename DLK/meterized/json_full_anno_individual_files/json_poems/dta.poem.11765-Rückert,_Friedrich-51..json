{"dta.poem.11765": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "51.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "In einem Hause wohnt' ein armes Hausgesind,", "tokens": ["In", "ei\u00b7nem", "Hau\u00b7se", "wohnt'", "ein", "ar\u00b7mes", "Haus\u00b7ge\u00b7sind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das H\u00fcndlein und der Knecht, der Vater und das Kind.", "tokens": ["Das", "H\u00fcnd\u00b7lein", "und", "der", "Knecht", ",", "der", "Va\u00b7ter", "und", "das", "Kind", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Der Herr des Lebens kam zu schaun der Menschen Noth,", "tokens": ["Der", "Herr", "des", "Le\u00b7bens", "kam", "zu", "schaun", "der", "Men\u00b7schen", "Noth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "PTKZU", "VVINF", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als Bettler pr\u00fcft' er sie und forderte ein Brot.", "tokens": ["Als", "Bett\u00b7ler", "pr\u00fcft'", "er", "sie", "und", "for\u00b7der\u00b7te", "ein", "Brot", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVFIN", "PPER", "PPER", "KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Der Herr sprach: Gib ihm eins! der Knecht sprach: dir ist kund,", "tokens": ["Der", "Herr", "sprach", ":", "Gib", "ihm", "eins", "!", "der", "Knecht", "sprach", ":", "dir", "ist", "kund", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "VVIMP", "PPER", "PIS", "$.", "ART", "NN", "VVFIN", "$.", "PPER", "VAFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vier Brote sind im Haus, je eins f\u00fcr einen Mund.", "tokens": ["Vier", "Bro\u00b7te", "sind", "im", "Haus", ",", "je", "eins", "f\u00fcr", "ei\u00b7nen", "Mund", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VAFIN", "APPRART", "NN", "$,", "ADV", "PIS", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Der Herr sprach: Gib ihm, das gespart war meinem Mund,", "tokens": ["Der", "Herr", "sprach", ":", "Gib", "ihm", ",", "das", "ge\u00b7spart", "war", "mei\u00b7nem", "Mund", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "VVIMP", "PPER", "$,", "PRELS", "VVPP", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und aufbewahrt sei das f\u00fcr dich, f\u00fcr Kind und Hund.", "tokens": ["Und", "auf\u00b7be\u00b7wahrt", "sei", "das", "f\u00fcr", "dich", ",", "f\u00fcr", "Kind", "und", "Hund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "VAFIN", "PDS", "APPR", "PPER", "$,", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Der Knecht mit Z\u00f6gern gabs; er nahm's und kam zur\u00fcck,", "tokens": ["Der", "Knecht", "mit", "Z\u00f6\u00b7gern", "gabs", ";", "er", "nahm's", "und", "kam", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VVFIN", "$.", "PPER", "NE", "KON", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein zweites fordert' er. \u201eGib ihm ein zweites St\u00fcck.", "tokens": ["Ein", "zwei\u00b7tes", "for\u00b7dert'", "er", ".", "\u201e", "Gib", "ihm", "ein", "zwei\u00b7tes", "St\u00fcck", "."], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "PPER", "$.", "$(", "VVIMP", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.6": {"line.1": {"text": "Recht mu\u00df dem Diener seyn, was seinem Herrn ist recht;", "tokens": ["Recht", "mu\u00df", "dem", "Die\u00b7ner", "seyn", ",", "was", "sei\u00b7nem", "Herrn", "ist", "recht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "ART", "NN", "VAINF", "$,", "PRELS", "PPOSAT", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "La\u00df das f\u00fcr Kind und Hund, und gib ihm deins, mein Knecht.\u201c", "tokens": ["La\u00df", "das", "f\u00fcr", "Kind", "und", "Hund", ",", "und", "gib", "ihm", "deins", ",", "mein", "Knecht", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["VVIMP", "PDS", "APPR", "NN", "KON", "NN", "$,", "KON", "VVIMP", "PPER", "NE", "$,", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Der Knecht mit Freuden gabs; er nahm's und kam zur\u00fcck,", "tokens": ["Der", "Knecht", "mit", "Freu\u00b7den", "gabs", ";", "er", "nahm's", "und", "kam", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VVFIN", "$.", "PPER", "NE", "KON", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein drittes fordert' er. \u201eGib ihm das dritte St\u00fcck.", "tokens": ["Ein", "drit\u00b7tes", "for\u00b7dert'", "er", ".", "\u201e", "Gib", "ihm", "das", "drit\u00b7te", "St\u00fcck", "."], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "PPER", "$.", "$(", "VVIMP", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.8": {"line.1": {"text": "Da\u00df es Enthaltsamkeit von seinem Vater lerne,", "tokens": ["Da\u00df", "es", "Ent\u00b7halt\u00b7sam\u00b7keit", "von", "sei\u00b7nem", "Va\u00b7ter", "ler\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gib hin des Kindes St\u00fcck!\u201c Der Diener gabs nicht gerne.", "tokens": ["Gib", "hin", "des", "Kin\u00b7des", "St\u00fcck", "!", "\u201c", "Der", "Die\u00b7ner", "gabs", "nicht", "ger\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "ART", "NN", "NN", "$.", "$(", "ART", "NN", "VVFIN", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Das Kindlein lacht' und gabs; er nahm's und kam zur\u00fcck,", "tokens": ["Das", "Kin\u00b7dlein", "lacht'", "und", "gabs", ";", "er", "nahm's", "und", "kam", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "$.", "PPER", "NE", "KON", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein viertes fordert' er. \u201eSo gib das letzte St\u00fcck!", "tokens": ["Ein", "vier\u00b7tes", "for\u00b7dert'", "er", ".", "\u201e", "So", "gib", "das", "letz\u00b7te", "St\u00fcck", "!"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "PPER", "$.", "$(", "ADV", "VVIMP", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.10": {"line.1": {"text": "Hab' ichs dem Knecht, dem Kind und meinem eignen Munde", "tokens": ["Hab'", "ichs", "dem", "Knecht", ",", "dem", "Kind", "und", "mei\u00b7nem", "eig\u00b7nen", "Mun\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PIS", "ART", "NN", "$,", "ART", "NN", "KON", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Entzogen, darf ichs wol entziehn auch meinem Hunde.\u201c", "tokens": ["Ent\u00b7zo\u00b7gen", ",", "darf", "ichs", "wol", "ent\u00b7ziehn", "auch", "mei\u00b7nem", "Hun\u00b7de", ".", "\u201c"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "$,", "VMFIN", "PIS", "ADV", "VVFIN", "ADV", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.11": {"line.1": {"text": "Geduldig gabs der Knecht; er nahm's und kam nicht wieder,", "tokens": ["Ge\u00b7dul\u00b7dig", "gabs", "der", "Knecht", ";", "er", "nahm's", "und", "kam", "nicht", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$.", "PPER", "NE", "KON", "VVFIN", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch drau\u00dfen in der Luft rauscht' es wie Lenzgefieder.", "tokens": ["Doch", "drau\u00b7\u00dfen", "in", "der", "Luft", "rauscht'", "es", "wie", "Lenz\u00b7ge\u00b7fie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ART", "NN", "VVFIN", "PPER", "KOKOM", "NE", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.12": {"line.1": {"text": "Ein goldner Regen flo\u00df herab vom Himmelsraum,", "tokens": ["Ein", "gold\u00b7ner", "Re\u00b7gen", "flo\u00df", "her\u00b7ab", "vom", "Him\u00b7mels\u00b7raum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wo er die Flur bego\u00df, da wuchs empor ein Baum.", "tokens": ["Wo", "er", "die", "Flur", "be\u00b7go\u00df", ",", "da", "wuchs", "em\u00b7por", "ein", "Baum", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "VVFIN", "$,", "ADV", "VVFIN", "PTKVZ", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.13": {"line.1": {"text": "Der Herr des Lebens sa\u00df im Wipfelzelt und sprach", "tokens": ["Der", "Herr", "des", "Le\u00b7bens", "sa\u00df", "im", "Wip\u00b7fel\u00b7zelt", "und", "sprach"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "APPRART", "NN", "KON", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit sanftem Rauschen: Gern gabt ihr, was euch gebrach.", "tokens": ["Mit", "sanf\u00b7tem", "Rau\u00b7schen", ":", "Gern", "gabt", "ihr", ",", "was", "euch", "ge\u00b7brach", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$.", "ADV", "VVFIN", "PPER", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-++--+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.14": {"line.1": {"text": "Drum soll des Lebens Brot hinfort euch nie gebrechen,", "tokens": ["Drum", "soll", "des", "Le\u00b7bens", "Brot", "hin\u00b7fort", "euch", "nie", "ge\u00b7bre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "ART", "NN", "NN", "ADV", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und gern gebt allen es, die meinen Namen sprechen.", "tokens": ["Und", "gern", "gebt", "al\u00b7len", "es", ",", "die", "mei\u00b7nen", "Na\u00b7men", "spre\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PIS", "PPER", "$,", "PRELS", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.15": {"line.1": {"text": "Ihr sollt den Acker drum nicht pfl\u00fcgen oder hacken,", "tokens": ["Ihr", "sollt", "den", "A\u00b7cker", "drum", "nicht", "pfl\u00fc\u00b7gen", "o\u00b7der", "ha\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "PAV", "PTKNEG", "VVINF", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "S\u00e4'n, schneiden oder m\u00e4hn, dann dreschen, mahlen, backen.", "tokens": ["S\u00e4'n", ",", "schnei\u00b7den", "o\u00b7der", "m\u00e4hn", ",", "dann", "dre\u00b7schen", ",", "mah\u00b7len", ",", "ba\u00b7cken", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "ADJA", "KON", "VVINF", "$,", "ADV", "VVINF", "$,", "VVINF", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.16": {"line.1": {"text": "Von selbst ein mehl'ger Kern, gebacken und gew\u00fcrzt,", "tokens": ["Von", "selbst", "ein", "mehl'\u00b7ger", "Kern", ",", "ge\u00b7ba\u00b7cken", "und", "ge\u00b7w\u00fcrzt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ART", "PIAT", "NN", "$,", "VVPP", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "W\u00e4chst euch das Brot am Baum, in Fruchtgestalt gesch\u00fcrzt.", "tokens": ["W\u00e4chst", "euch", "das", "Brot", "am", "Baum", ",", "in", "Frucht\u00b7ge\u00b7stalt", "ge\u00b7sch\u00fcrzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPRART", "NN", "$,", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.17": {"line.1": {"text": "Vier Brote tr\u00e4gt der Baum, und jedes f\u00fcllt im Raum", "tokens": ["Vier", "Bro\u00b7te", "tr\u00e4gt", "der", "Baum", ",", "und", "je\u00b7des", "f\u00fcllt", "im", "Raum"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["CARD", "NN", "VVFIN", "ART", "NN", "$,", "KON", "PIAT", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Des Jahres seinen Mund; das ist der Brotfruchtbaum.", "tokens": ["Des", "Jah\u00b7res", "sei\u00b7nen", "Mund", ";", "das", "ist", "der", "Brot\u00b7frucht\u00b7baum", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$.", "PDS", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}