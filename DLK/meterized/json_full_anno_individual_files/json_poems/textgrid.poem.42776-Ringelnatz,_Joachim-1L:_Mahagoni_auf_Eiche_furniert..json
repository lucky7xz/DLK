{"textgrid.poem.42776": {"metadata": {"author": {"name": "Ringelnatz, Joachim", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mahagoni auf Eiche furniert.", "genre": "verse", "period": "N.A.", "pub_year": 1908, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mahagoni auf Eiche furniert.", "tokens": ["Ma\u00b7ha\u00b7go\u00b7ni", "auf", "Ei\u00b7che", "fur\u00b7niert", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "NN", "VVFIN", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Deckel sauber scharniert.", "tokens": ["De\u00b7ckel", "sau\u00b7ber", "schar\u00b7niert", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJD", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Alle Bretter gefedert, gespundet.", "tokens": ["Al\u00b7le", "Bret\u00b7ter", "ge\u00b7fe\u00b7dert", ",", "ge\u00b7spun\u00b7det", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PIAT", "NN", "VVPP", "$,", "VVPP", "$."], "meter": "--+---+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Die Ecken fein weich gerundet.", "tokens": ["Die", "E\u00b7cken", "fein", "weich", "ge\u00b7run\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "ADJD", "VVPP", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Die Seitenw\u00e4nde mit tief geschnitzten", "tokens": ["Die", "Sei\u00b7ten\u00b7w\u00e4n\u00b7de", "mit", "tief", "ge\u00b7schnitz\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ADJD", "VVPP"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Weintrauben und Schellfischen geziert.", "tokens": ["Wein\u00b7trau\u00b7ben", "und", "Schell\u00b7fi\u00b7schen", "ge\u00b7ziert", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVPP", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Das war bei Weber in Osnabr\u00fcck", "tokens": ["Das", "war", "bei", "We\u00b7ber", "in", "Os\u00b7na\u00b7br\u00fcck"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "APPR", "NE", "APPR", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Mein Gesellenst\u00fcck.", "tokens": ["Mein", "Ge\u00b7sel\u00b7len\u00b7st\u00fcck", "."], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Selbst Wasmann und Peter sagten 1910:", "tokens": ["Selbst", "Was\u00b7mann", "und", "Pe\u00b7ter", "sag\u00b7ten", "19\u00b710", ":"], "token_info": ["word", "word", "word", "word", "word", "number", "punct"], "pos": ["ADV", "NN", "KON", "NE", "VVFIN", "CARD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Solch einen Sarg h\u00e4tten sie noch nie gesehn.", "tokens": ["Solch", "ei\u00b7nen", "Sarg", "h\u00e4t\u00b7ten", "sie", "noch", "nie", "ge\u00b7sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ART", "NN", "VAFIN", "PPER", "ADV", "ADV", "VVPP", "$."], "meter": "-+-++-+-+-+", "measure": "unknown.measure.hexa"}}, "stanza.3": {"line.1": {"text": "Ohne mich r\u00fchmen. Das soll einer machen.", "tokens": ["Oh\u00b7ne", "mich", "r\u00fch\u00b7men", ".", "Das", "soll", "ei\u00b7ner", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVINF", "$.", "PDS", "VMFIN", "PIS", "VVINF", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Und dabei alles selber gemacht.", "tokens": ["Und", "da\u00b7bei", "al\u00b7les", "sel\u00b7ber", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "PIS", "ADV", "VVPP", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Die Griffe kupfergeschmiedete Drachen,", "tokens": ["Die", "Grif\u00b7fe", "kup\u00b7fer\u00b7ge\u00b7schmie\u00b7de\u00b7te", "Dra\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die F\u00fc\u00dfe gedrechselt (((Acht, sacht, Pracht, lacht, gedacht))),", "tokens": ["Die", "F\u00fc\u00b7\u00dfe", "ge\u00b7drech\u00b7selt", "(", "(", "(", "Acht", ",", "sacht", ",", "Pracht", ",", "lacht", ",", "ge\u00b7dacht", ")", ")", ")", ","], "token_info": ["word", "word", "word", "punct", "punct", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "punct", "punct", "punct"], "pos": ["ART", "NN", "VVPP", "$(", "$(", "$(", "CARD", "$,", "VVFIN", "$,", "NN", "$,", "VVFIN", "$,", "VVPP", "$(", "$(", "$(", "$,"], "meter": "-+--+-+-+--+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Auf den Deckel in Rundschrift fein s\u00e4uberlich", "tokens": ["Auf", "den", "De\u00b7ckel", "in", "Rund\u00b7schrift", "fein", "s\u00e4u\u00b7ber\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "NN", "ADJD", "ADJD"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.6": {"text": "Eingebrannt: \u00bbSarg f\u00fcr Frau (Doppelpunkt Strich)\u00ab.", "tokens": ["Ein\u00b7ge\u00b7brannt", ":", "\u00bb", "Sarg", "f\u00fcr", "Frau", "(", "Dop\u00b7pel\u00b7punkt", "Strich", ")", "\u00ab", "."], "token_info": ["word", "punct", "punct", "word", "word", "word", "punct", "word", "word", "punct", "punct", "punct"], "pos": ["ADJD", "$.", "$(", "NN", "APPR", "NN", "$(", "NN", "NE", "$(", "$(", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.7": {"text": "Inwendig ein ro\u00dfhaargepolstertes Bett,", "tokens": ["In\u00b7wen\u00b7dig", "ein", "ro\u00df\u00b7haar\u00b7ge\u00b7pols\u00b7ter\u00b7tes", "Bett", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.8": {"text": "Rosa P\u00fcnktchen auf Gelb-Violett.", "tokens": ["Ro\u00b7sa", "P\u00fcnkt\u00b7chen", "auf", "Gelb\u00b7Vi\u00b7o\u00b7lett", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.9": {"text": "Ich habe manchmal des Studiums wegen", "tokens": ["Ich", "ha\u00b7be", "manch\u00b7mal", "des", "Stu\u00b7di\u00b7ums", "we\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ART", "NN", "APPR"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "24 Stunden darin gelegen.", "tokens": ["Stun\u00b7den", "da\u00b7rin", "ge\u00b7le\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PAV", "VVPP", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.11": {"text": "Da war ein durch sch\u00f6ne Bilder verdecktes", "tokens": ["Da", "war", "ein", "durch", "sch\u00f6\u00b7ne", "Bil\u00b7der", "ver\u00b7deck\u00b7tes"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "APPR", "ADJA", "NN", "ADJA"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "Speiseregal zur linken Hand,", "tokens": ["Spei\u00b7se\u00b7re\u00b7gal", "zur", "lin\u00b7ken", "Hand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "ADJA", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.13": {"text": "Wo Camembert, Zwieback und Butter stand", "tokens": ["Wo", "Ca\u00b7mem\u00b7bert", ",", "Zwie\u00b7back", "und", "But\u00b7ter", "stand"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "NE", "$,", "NN", "KON", "NN", "VVFIN"], "meter": "-+--++-+-+", "measure": "iambic.penta.relaxed"}, "line.14": {"text": "Und Trockengem\u00fcse und Eingewecktes. \u2013", "tokens": ["Und", "Tro\u00b7cken\u00b7ge\u00b7m\u00fc\u00b7se", "und", "Ein\u00b7ge\u00b7weck\u00b7tes", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "NN", "KON", "NN", "$.", "$("], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}}, "stanza.4": {"line.1": {"text": "Auf den leisesten Druck mit der Zehe im Schlaf", "tokens": ["Auf", "den", "lei\u00b7ses\u00b7ten", "Druck", "mit", "der", "Ze\u00b7he", "im", "Schlaf"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "APPR", "ART", "NN", "APPRART", "NN"], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.2": {"text": "L\u00f6ste sich zu Fu\u00dfende ein Kinematograph", "tokens": ["L\u00f6s\u00b7te", "sich", "zu", "Fu\u00b7\u00dfen\u00b7de", "ein", "Ki\u00b7ne\u00b7ma\u00b7to\u00b7graph"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "APPR", "NN", "ART", "NN"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.3": {"text": "Und zeigte abwechselnd \u00bbBrudermord\u00ab", "tokens": ["Und", "zeig\u00b7te", "ab\u00b7wech\u00b7selnd", "\u00bb", "Bru\u00b7der\u00b7mord", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "$(", "NN", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und \u00bbTorpedoangriff an Steuerbord\u00ab.", "tokens": ["Und", "\u00bb", "Tor\u00b7pe\u00b7do\u00b7an\u00b7griff", "an", "Steu\u00b7er\u00b7bord", "\u00ab", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["KON", "$(", "NN", "APPR", "NN", "$(", "$."], "meter": "-+---+-+-+", "measure": "dactylic.init"}, "line.5": {"text": "Alle zwei Stunden von selbst automatisch", "tokens": ["Al\u00b7le", "zwei", "Stun\u00b7den", "von", "selbst", "au\u00b7to\u00b7ma\u00b7tisch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "CARD", "NN", "APPR", "ADV", "ADJD"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.6": {"text": "Spielte ein Grammophon ganz zart:", "tokens": ["Spiel\u00b7te", "ein", "Gram\u00b7mo\u00b7phon", "ganz", "zart", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "ADJD", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.7": {"text": "\u00bbich bin der Doktor Eisenbarth.\u00ab", "tokens": ["\u00bb", "ich", "bin", "der", "Dok\u00b7tor", "Ei\u00b7sen\u00b7barth", ".", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VAFIN", "ART", "NN", "NE", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Au\u00dferdem roch es dort sehr sympathisch", "tokens": ["Au\u00b7\u00dfer\u00b7dem", "roch", "es", "dort", "sehr", "sym\u00b7pa\u00b7thisch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "ADV", "ADV", "ADJD"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Nach Moschus, Kampfer und kalter K\u00fcche.", "tokens": ["Nach", "Mo\u00b7schus", ",", "Kamp\u00b7fer", "und", "kal\u00b7ter", "K\u00fc\u00b7che", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "NN", "KON", "ADJA", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.3": {"text": "Von wegen die Leichenger\u00fcche.", "tokens": ["Von", "we\u00b7gen", "die", "Lei\u00b7chen\u00b7ge\u00b7r\u00fc\u00b7che", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Und dann die Technik und das Komfort:", "tokens": ["Und", "dann", "die", "Tech\u00b7nik", "und", "das", "Kom\u00b7fort", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Kalender, das Telefon rechts am Ohr,", "tokens": ["Ka\u00b7len\u00b7der", ",", "das", "Te\u00b7le\u00b7fon", "rechts", "am", "Ohr", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.6": {"text": "Gl\u00fchbirnen und Klingeln. Ein tolles Gewirr.", "tokens": ["Gl\u00fch\u00b7bir\u00b7nen", "und", "Klin\u00b7geln", ".", "Ein", "tol\u00b7les", "Ge\u00b7wirr", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$.", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.7": {"text": "Auch ein kleines, versilbertes Nachtgeschirr. \u2013", "tokens": ["Auch", "ein", "klei\u00b7nes", ",", "ver\u00b7sil\u00b7ber\u00b7tes", "Nacht\u00b7ge\u00b7schirr", ".", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["ADV", "ART", "ADJA", "$,", "ADJA", "NN", "$.", "$("], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.8": {"text": "Und Wasserstandglas und Thermometer.", "tokens": ["Und", "Was\u00b7ser\u00b7stand\u00b7glas", "und", "Ther\u00b7mo\u00b7me\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Kurz herrlich! herrlich! \u2013 Wasmann und Peter", "tokens": ["Kurz", "herr\u00b7lich", "!", "herr\u00b7lich", "!", "\u2013", "Was\u00b7mann", "und", "Pe\u00b7ter"], "token_info": ["word", "word", "punct", "word", "punct", "punct", "word", "word", "word"], "pos": ["ADJD", "ADJD", "$.", "ADJD", "$.", "$(", "NN", "KON", "NE"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "H\u00e4tten mir glattweg f\u00fcnftausend Mark", "tokens": ["H\u00e4t\u00b7ten", "mir", "glatt\u00b7weg", "f\u00fcnf\u00b7tau\u00b7send", "Mark"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "CARD", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.11": {"text": "Und doppelt soviel gezahlt f\u00fcr den Sarg.", "tokens": ["Und", "dop\u00b7pelt", "so\u00b7viel", "ge\u00b7zahlt", "f\u00fcr", "den", "Sarg", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "VVPP", "APPR", "ART", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "Und das war damals ein Geld, wenn man's denkt.", "tokens": ["Und", "das", "war", "da\u00b7mals", "ein", "Geld", ",", "wenn", "man's", "denkt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "ADV", "ART", "NN", "$,", "KOUS", "PIS", "VVFIN", "$."], "meter": "-+-+-++--+", "measure": "iambic.penta.chol"}}, "stanza.6": {"line.1": {"text": "Aber ich h\u00e4nge nicht so am Golde. \u2013", "tokens": ["A\u00b7ber", "ich", "h\u00e4n\u00b7ge", "nicht", "so", "am", "Gol\u00b7de", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPER", "VVFIN", "PTKNEG", "ADV", "APPRART", "NN", "$.", "$("], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Und so hab ich ihn dann meiner Tante Isolde", "tokens": ["Und", "so", "hab", "ich", "ihn", "dann", "mei\u00b7ner", "Tan\u00b7te", "I\u00b7sol\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "PPER", "PPER", "ADV", "PPOSAT", "NN", "NE"], "meter": "+-+-+-+-+--+-", "measure": "hexameter"}, "line.3": {"text": "Zum 70. Geburtstag geschenkt.", "tokens": ["Zum", "70.", "Ge\u00b7burts\u00b7tag", "ge\u00b7schenkt", "."], "token_info": ["word", "ordinal", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVPP", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}}, "stanza.7": {"line.1": {"text": "Mahagoni auf Eiche furniert.", "tokens": ["Ma\u00b7ha\u00b7go\u00b7ni", "auf", "Ei\u00b7che", "fur\u00b7niert", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "NN", "VVFIN", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Deckel sauber scharniert.", "tokens": ["De\u00b7ckel", "sau\u00b7ber", "schar\u00b7niert", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJD", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Alle Bretter gefedert, gespundet.", "tokens": ["Al\u00b7le", "Bret\u00b7ter", "ge\u00b7fe\u00b7dert", ",", "ge\u00b7spun\u00b7det", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PIAT", "NN", "VVPP", "$,", "VVPP", "$."], "meter": "--+---+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Die Ecken fein weich gerundet.", "tokens": ["Die", "E\u00b7cken", "fein", "weich", "ge\u00b7run\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "ADJD", "VVPP", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Die Seitenw\u00e4nde mit tief geschnitzten", "tokens": ["Die", "Sei\u00b7ten\u00b7w\u00e4n\u00b7de", "mit", "tief", "ge\u00b7schnitz\u00b7ten"], "token_info": ["word", "word", "word", "word"