{"dta.poem.7918": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Lied eines deutschen S\u00e4ngers .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.71", "nl:0.28"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "Ich sang in vor\u2019gen Tagen", "tokens": ["Ich", "sang", "in", "vor'\u00b7gen", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Lieder mancherlei,", "tokens": ["Der", "Lie\u00b7der", "man\u00b7cher\u00b7lei", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PIS", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Von alten, frommen Sagen,", "tokens": ["Von", "al\u00b7ten", ",", "from\u00b7men", "Sa\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Von Minne, Wein und Mai.", "tokens": ["Von", "Min\u00b7ne", ",", "Wein", "und", "Mai", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Nun ist es ausgesungen,", "tokens": ["Nun", "ist", "es", "aus\u00b7ge\u00b7sun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Es d\u00fcnkt mir Alles Tand:", "tokens": ["Es", "d\u00fcnkt", "mir", "Al\u00b7les", "Tand", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Der Heerschild ist erklungen,", "tokens": ["Der", "Heer\u00b7schild", "ist", "er\u00b7klun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Der Ruf: f\u00fcr\u2019s Vaterland!", "tokens": ["Der", "Ruf", ":", "f\u00fcr's", "Va\u00b7ter\u00b7land", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Man sagt wohl von den Katten:", "tokens": ["Man", "sagt", "wohl", "von", "den", "Kat\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sie legten Erzring\u2019 an,", "tokens": ["Sie", "leg\u00b7ten", "Erz\u00b7ring'", "an", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Bis sie gel\u00f6st sich hatten", "tokens": ["Bis", "sie", "ge\u00b7l\u00f6st", "sich", "hat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "PRF", "VAFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mit einem erschlagnen Mann.", "tokens": ["Mit", "ei\u00b7nem", "er\u00b7schlag\u00b7nen", "Mann", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Ich schlag\u2019 den Geist in Bande", "tokens": ["Ich", "schlag'", "den", "Geist", "in", "Ban\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und werf\u2019 an den Mund ein Schlo\u00df,", "tokens": ["Und", "wer\u00b7f'", "an", "den", "Mund", "ein", "Schlo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Bis ich dem Vaterlande", "tokens": ["Bis", "ich", "dem", "Va\u00b7ter\u00b7lan\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Gedient als Schwerdtgeno\u00df.", "tokens": ["Ge\u00b7dient", "als", "Schwerdt\u00b7ge\u00b7no\u00df", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und bin ich nicht geboren", "tokens": ["Und", "bin", "ich", "nicht", "ge\u00b7bo\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "PTKNEG", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zu hohem Heldenthum,", "tokens": ["Zu", "ho\u00b7hem", "Hel\u00b7den\u00b7thum", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ist mir das Lied erkoren", "tokens": ["Ist", "mir", "das", "Lied", "er\u00b7ko\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zu Lust und schlichtem Ruhm:", "tokens": ["Zu", "Lust", "und", "schlich\u00b7tem", "Ruhm", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Doch m\u00f6cht\u2019 ich Eins erringen", "tokens": ["Doch", "m\u00f6cht'", "ich", "Eins", "er\u00b7rin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "In diesem heil\u2019gen Krieg:", "tokens": ["In", "die\u00b7sem", "heil'\u00b7gen", "Krieg", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Das edle Recht, zu singen", "tokens": ["Das", "ed\u00b7le", "Recht", ",", "zu", "sin\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Des deutschen Volkes Sieg.", "tokens": ["Des", "deut\u00b7schen", "Vol\u00b7kes", "Sieg", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}