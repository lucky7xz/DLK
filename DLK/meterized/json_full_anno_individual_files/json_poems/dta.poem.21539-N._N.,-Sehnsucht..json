{"dta.poem.21539": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Sehnsucht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Mich fa\u00dfte der Sehnsucht Fieber,", "tokens": ["Mich", "fa\u00df\u00b7te", "der", "Sehn\u00b7sucht", "Fie\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Ich hebe mein Haupt vom Pf\u00fchl \u2014", "tokens": ["Ich", "he\u00b7be", "mein", "Haupt", "vom", "Pf\u00fchl"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPRART", "NN", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Es geht durch die stille Kammer", "tokens": ["Es", "geht", "durch", "die", "stil\u00b7le", "Kam\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Der Sommernacht Odem schw\u00fcl \u2014", "tokens": ["Der", "Som\u00b7mer\u00b7nacht", "O\u00b7dem", "schw\u00fcl"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "ADJD", "$("], "meter": "-+-++-+", "measure": "unknown.measure.tetra"}, "line.5": {"text": "Mir ist, als m\u00fc\u00dftest du kommen,", "tokens": ["Mir", "ist", ",", "als", "m\u00fc\u00df\u00b7test", "du", "kom\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "KOUS", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Du, die mir die Seele genommen", "tokens": ["Du", ",", "die", "mir", "die", "See\u00b7le", "ge\u00b7nom\u00b7men"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "$,", "PRELS", "PPER", "ART", "NN", "VVPP"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.7": {"text": "Und die mir das Herz berauscht,", "tokens": ["Und", "die", "mir", "das", "Herz", "be\u00b7rauscht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.8": {"text": "Mich fa\u00dfte der Sehnsucht Fieber,", "tokens": ["Mich", "fa\u00df\u00b7te", "der", "Sehn\u00b7sucht", "Fie\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "Ich hebe mein Haupt vom Pf\u00fchl.", "tokens": ["Ich", "he\u00b7be", "mein", "Haupt", "vom", "Pf\u00fchl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPRART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Ich starre in\u2019s tiefe Dunkel", "tokens": ["Ich", "star\u00b7re", "in's", "tie\u00b7fe", "Dun\u00b7kel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Mit Augen, gluthentfacht,", "tokens": ["Mit", "Au\u00b7gen", ",", "glu\u00b7thent\u00b7facht", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mir ist es, als m\u00fc\u00dfte mir wallen", "tokens": ["Mir", "ist", "es", ",", "als", "m\u00fc\u00df\u00b7te", "mir", "wal\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "$,", "KOUS", "VMFIN", "PPER", "VVINF"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Deiner Locken braundunkele Nacht", "tokens": ["Dei\u00b7ner", "Lo\u00b7cken", "braun\u00b7dun\u00b7ke\u00b7le", "Nacht"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "NN"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.5": {"text": "Um meine brennenden Wangen,", "tokens": ["Um", "mei\u00b7ne", "bren\u00b7nen\u00b7den", "Wan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Als m\u00fc\u00dfte mich weich umfangen", "tokens": ["Als", "m\u00fc\u00df\u00b7te", "mich", "weich", "um\u00b7fan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "VMFIN", "PRF", "ADJD", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Dein lilienwei\u00dfer Arm;", "tokens": ["Dein", "li\u00b7li\u00b7en\u00b7wei\u00b7\u00dfer", "Arm", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Mich fa\u00dfte der Sehnsucht Fieber,", "tokens": ["Mich", "fa\u00df\u00b7te", "der", "Sehn\u00b7sucht", "Fie\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "Ich starre hinaus in die Nacht.", "tokens": ["Ich", "star\u00b7re", "hin\u00b7aus", "in", "die", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ich breite nach Dir die Arme,", "tokens": ["Ich", "brei\u00b7te", "nach", "Dir", "die", "Ar\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "ART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Als wollt\u2019 ich Dich an mich zieh\u2019n,", "tokens": ["Als", "wollt'", "ich", "Dich", "an", "mich", "zieh'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VMFIN", "PPER", "PRF", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Mir ist es, als ob ich m\u00fc\u00dfte", "tokens": ["Mir", "ist", "es", ",", "als", "ob", "ich", "m\u00fc\u00df\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "$,", "KOKOM", "KOUS", "PPER", "VMFIN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Zu deinen F\u00fc\u00dfen knie\u2019n,", "tokens": ["Zu", "dei\u00b7nen", "F\u00fc\u00b7\u00dfen", "knie'n", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Als m\u00fc\u00dftest im Arm du mir liegen,", "tokens": ["Als", "m\u00fc\u00df\u00b7test", "im", "Arm", "du", "mir", "lie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VMFIN", "APPRART", "NN", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Und wonnig sich an mich schmiegen", "tokens": ["Und", "won\u00b7nig", "sich", "an", "mich", "schmie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "PRF", "APPR", "PPER", "VVFIN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Dein liebes M\u00e4dchengesicht.", "tokens": ["Dein", "lie\u00b7bes", "M\u00e4d\u00b7chen\u00b7ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.8": {"text": "Mich fa\u00dfte der Sehnsucht Fieber,", "tokens": ["Mich", "fa\u00df\u00b7te", "der", "Sehn\u00b7sucht", "Fie\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "Die Arme breit ich nach dir.", "tokens": ["Die", "Ar\u00b7me", "breit", "ich", "nach", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "PPER", "APPR", "PPER", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.4": {"line.1": {"text": "Da pl\u00f6tzlich erbebt meine Seele,", "tokens": ["Da", "pl\u00f6tz\u00b7lich", "er\u00b7bebt", "mei\u00b7ne", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Mein Schrei durchzittert die Luft:", "tokens": ["Mein", "Schrei", "durch\u00b7zit\u00b7tert", "die", "Luft", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Und Weinende seh\u2019 ich wallen,", "tokens": ["Und", "Wei\u00b7nen\u00b7de", "seh'", "ich", "wal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und \u00f6ffnen sich eine Gruft,", "tokens": ["Und", "\u00f6ff\u00b7nen", "sich", "ei\u00b7ne", "Gruft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Seh senken ich darin nieder \u2026", "tokens": ["Seh", "sen\u00b7ken", "ich", "da\u00b7rin", "nie\u00b7der", "\u2026"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "PAV", "PTKVZ", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wild press\u2019 ich die Stirn auf\u2019s Pf\u00fchl!", "tokens": ["Wild", "press'", "ich", "die", "Stirn", "auf's", "Pf\u00fchl", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Mich sch\u00fcttelt der Sehnsucht Fieber", "tokens": ["Mich", "sch\u00fct\u00b7telt", "der", "Sehn\u00b7sucht", "Fie\u00b7ber"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Nach dir, o Todte, nach dir.", "tokens": ["Nach", "dir", ",", "o", "Tod\u00b7te", ",", "nach", "dir", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "FM", "NN", "$,", "APPR", "PPER", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}}}}