{"textgrid.poem.63086": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "Winterlandschaft", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das H\u00fcgelland wogt wie ein wei\u00dfes Meer im Schnee,", "tokens": ["Das", "H\u00fc\u00b7gel\u00b7land", "wogt", "wie", "ein", "wei\u00b7\u00dfes", "Meer", "im", "Schnee", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KOKOM", "ART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vom Himmel nieder wuchten violette", "tokens": ["Vom", "Him\u00b7mel", "nie\u00b7der", "wuch\u00b7ten", "vi\u00b7o\u00b7let\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "PTKVZ", "VVFIN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Schneewolken, eine dichtverschlungne Kette,", "tokens": ["Schnee\u00b7wol\u00b7ken", ",", "ei\u00b7ne", "dicht\u00b7ver\u00b7schlung\u00b7ne", "Ket\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}, "line.4": {"text": "Die in der Luft an roten \u00d6fen h\u00e4ngt \u2013", "tokens": ["Die", "in", "der", "Luft", "an", "ro\u00b7ten", "\u00d6\u00b7fen", "h\u00e4ngt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Die Sonne brannte sie \u2013", "tokens": ["Die", "Son\u00b7ne", "brann\u00b7te", "sie", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Am Horizonte aber w\u00f6lbt sich aus der wei\u00dfen Flur ein Berg,", "tokens": ["Am", "Ho\u00b7ri\u00b7zon\u00b7te", "a\u00b7ber", "w\u00f6lbt", "sich", "aus", "der", "wei\u00b7\u00dfen", "Flur", "ein", "Berg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "VVFIN", "PRF", "APPR", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.7": {"text": "Mit Tann bestanden, schwarz gekappt,", "tokens": ["Mit", "Tann", "be\u00b7stan\u00b7den", ",", "schwarz", "ge\u00b7kappt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ein ungeheurer Igel, der den Schneefall", "tokens": ["Ein", "un\u00b7ge\u00b7heu\u00b7rer", "I\u00b7gel", ",", "der", "den", "Schnee\u00b7fall"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Von seinen Borsten schl\u00e4frig sch\u00fcttelte.", "tokens": ["Von", "sei\u00b7nen", "Bors\u00b7ten", "schl\u00e4f\u00b7rig", "sch\u00fct\u00b7tel\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Das H\u00fcgelland wogt wie ein wei\u00dfes Meer im Schnee,", "tokens": ["Das", "H\u00fc\u00b7gel\u00b7land", "wogt", "wie", "ein", "wei\u00b7\u00dfes", "Meer", "im", "Schnee", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KOKOM", "ART", "ADJA", "NN", "APPRART", "NN",