{"textgrid.poem.36263": {"metadata": {"author": {"name": "Schlegel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Einsam blieb die Mutter auf der Erde,", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Einsam blieb die Mutter auf der Erde,", "tokens": ["Ein\u00b7sam", "blieb", "die", "Mut\u00b7ter", "auf", "der", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Einsam steht die Mutter nun im Leben.", "tokens": ["Ein\u00b7sam", "steht", "die", "Mut\u00b7ter", "nun", "im", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "ADV", "APPRART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Bleich die Wang', und bleicher noch im Herzen,", "tokens": ["Bleich", "die", "Wang'", ",", "und", "blei\u00b7cher", "noch", "im", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "$,", "KON", "ADJD", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Lebt sie schweigend und will schweigend enden;", "tokens": ["Lebt", "sie", "schwei\u00b7gend", "und", "will", "schwei\u00b7gend", "en\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "KON", "VMFIN", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Denn nur einmal weinte sie von Herzen.", "tokens": ["Denn", "nur", "ein\u00b7mal", "wein\u00b7te", "sie", "von", "Her\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.6": {"text": "Als sie weinte, ward das Dunkel helle,", "tokens": ["Als", "sie", "wein\u00b7te", ",", "ward", "das", "Dun\u00b7kel", "hel\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "VAFIN", "ART", "NN", "ADJA", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Von des Knaben Schimmer sie geblendet.", "tokens": ["Von", "des", "Kna\u00b7ben", "Schim\u00b7mer", "sie", "ge\u00b7blen\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "PPER", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Ihren Knaben sah sie l\u00e4chelnd schweben,", "tokens": ["Ih\u00b7ren", "Kna\u00b7ben", "sah", "sie", "l\u00e4\u00b7chelnd", "schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "Andre Kinder schwebten um ihn ferne.", "tokens": ["And\u00b7re", "Kin\u00b7der", "schweb\u00b7ten", "um", "ihn", "fer\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "APPR", "PPER", "ADV", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "T\u00e4ndelnd sa\u00df er an der alten Stelle,", "tokens": ["T\u00e4n\u00b7delnd", "sa\u00df", "er", "an", "der", "al\u00b7ten", "Stel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.11": {"text": "Zu dem Spielzeug, das er kannte, redend:", "tokens": ["Zu", "dem", "Spiel\u00b7zeug", ",", "das", "er", "kann\u00b7te", ",", "re\u00b7dend", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "O wie lieb' ich die Sachen,", "tokens": ["O", "wie", "lieb'", "ich", "die", "Sa\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die mit mir spielen!", "tokens": ["Die", "mit", "mir", "spie\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPER", "VVINF", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "O wie bunt sind die Kinder,", "tokens": ["O", "wie", "bunt", "sind", "die", "Kin\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "ADJD", "VAFIN", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Die mit mir fliegen!", "tokens": ["Die", "mit", "mir", "flie\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPER", "VVINF", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "Sie scheinen mich zu h\u00fcten,", "tokens": ["Sie", "schei\u00b7nen", "mich", "zu", "h\u00fc\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und geben S\u00fc\u00dfes.", "tokens": ["Und", "ge\u00b7ben", "S\u00fc\u00b7\u00dfes", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Ich sehe, da\u00df ich gl\u00e4nze,", "tokens": ["Ich", "se\u00b7he", ",", "da\u00df", "ich", "gl\u00e4n\u00b7ze", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und habe Fl\u00fcgel.", "tokens": ["Und", "ha\u00b7be", "Fl\u00fc\u00b7gel", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Mit den Worten war der Schein verschwunden.", "tokens": ["Mit", "den", "Wor\u00b7ten", "war", "der", "Schein", "ver\u00b7schwun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "In dem Glanz verga\u00df sich selbst die Mutter;", "tokens": ["In", "dem", "Glanz", "ver\u00b7ga\u00df", "sich", "selbst", "die", "Mut\u00b7ter", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PRF", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Doch der Schrecken fa\u00dfte sie im Dunkel,", "tokens": ["Doch", "der", "Schre\u00b7cken", "fa\u00df\u00b7te", "sie", "im", "Dun\u00b7kel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Da\u00df sie wohl auf ewig hingesunken,", "tokens": ["Da\u00df", "sie", "wohl", "auf", "e\u00b7wig", "hin\u00b7ge\u00b7sun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Wenn nicht neues Licht der Nacht entsprungen.", "tokens": ["Wenn", "nicht", "neu\u00b7es", "Licht", "der", "Nacht", "ent\u00b7sprun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "ADJA", "NN", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Ernsthaft winkt das M\u00e4dchen tief im Grunde,", "tokens": ["Ernst\u00b7haft", "winkt", "das", "M\u00e4d\u00b7chen", "tief", "im", "Grun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "ADJD", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Bittet aus der Ferne nimmer ruhend,", "tokens": ["Bit\u00b7tet", "aus", "der", "Fer\u00b7ne", "nim\u00b7mer", "ru\u00b7hend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Klagend flie\u00dft Gesang vom s\u00fc\u00dfen Munde.", "tokens": ["Kla\u00b7gend", "flie\u00dft", "Ge\u00b7sang", "vom", "s\u00fc\u00b7\u00dfen", "Mun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Ich bitte um die Mutter,", "tokens": ["Ich", "bit\u00b7te", "um", "die", "Mut\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ihr gebt mir wei\u00dfe Rosen;", "tokens": ["Ihr", "gebt", "mir", "wei\u00b7\u00dfe", "Ro\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich frage nach der Mutter,", "tokens": ["Ich", "fra\u00b7ge", "nach", "der", "Mut\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ihr zeigt des Himmels Bogen.", "tokens": ["Ihr", "zeigt", "des", "Him\u00b7mels", "Bo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Ich war so gern auf Erden", "tokens": ["Ich", "war", "so", "gern", "auf", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und liebte recht die Sonne;", "tokens": ["Und", "lieb\u00b7te", "recht", "die", "Son\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Nun bin ich leiser Schatten,", "tokens": ["Nun", "bin", "ich", "lei\u00b7ser", "Schat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sie winken mir nach oben.", "tokens": ["Sie", "win\u00b7ken", "mir", "nach", "o\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Ich bin ein banges M\u00e4dchen,", "tokens": ["Ich", "bin", "ein", "ban\u00b7ges", "M\u00e4d\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Liebe fr\u00fch entzogen;", "tokens": ["Der", "Lie\u00b7be", "fr\u00fch", "ent\u00b7zo\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich bin noch kindisch bl\u00f6de:", "tokens": ["Ich", "bin", "noch", "kin\u00b7disch", "bl\u00f6\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Was soll ich schon am Throne?", "tokens": ["Was", "soll", "ich", "schon", "am", "Thro\u00b7ne", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Du hast mich auch verlassen,", "tokens": ["Du", "hast", "mich", "auch", "ver\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sonst w\u00e4rst du schon gekommen.", "tokens": ["Sonst", "w\u00e4rst", "du", "schon", "ge\u00b7kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "O komm' zu deinem Kinde,", "tokens": ["O", "komm'", "zu", "dei\u00b7nem", "Kin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das ungern dir entflohen!", "tokens": ["Das", "un\u00b7gern", "dir", "ent\u00b7flo\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "PPER", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Als das s\u00fc\u00dfe M\u00e4dchen sang die Klagen,", "tokens": ["Als", "das", "s\u00fc\u00b7\u00dfe", "M\u00e4d\u00b7chen", "sang", "die", "Kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "War's als ob die Schmerzen alle brachen,", "tokens": ["Wa\u00b7r's", "als", "ob", "die", "Schmer\u00b7zen", "al\u00b7le", "bra\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "KOKOM", "KOUS", "ART", "NN", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Aufgel\u00f6st in Tr\u00e4nen mich zu baden,", "tokens": ["Auf\u00b7ge\u00b7l\u00f6st", "in", "Tr\u00e4\u00b7nen", "mich", "zu", "ba\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Die nun ewig einsam und verlassen,", "tokens": ["Die", "nun", "e\u00b7wig", "ein\u00b7sam", "und", "ver\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "ADJD", "KON", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "An das M\u00e4dchen denkt und an den Knaben.", "tokens": ["An", "das", "M\u00e4d\u00b7chen", "denkt", "und", "an", "den", "Kna\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "KON", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.10": {"line.1": {"text": "Einsam blieb die Mutter auf der Erde,", "tokens": ["Ein\u00b7sam", "blieb", "die", "Mut\u00b7ter", "auf", "der", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Einsam steht die Mutter nun im Leben.", "tokens": ["Ein\u00b7sam", "steht", "die", "Mut\u00b7ter", "nun", "im", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "ADV", "APPRART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Bleich die Wang', und bleicher noch im Herzen,", "tokens": ["Bleich", "die", "Wang'", ",", "und", "blei\u00b7cher", "noch", "im", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "$,", "KON", "ADJD", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Lebt sie schweigend und will schweigend enden;", "tokens": ["Lebt", "sie", "schwei\u00b7gend", "und", "will", "schwei\u00b7gend", "en\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "KON", "VMFIN", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Denn nur einmal weinte sie von Herzen.", "tokens": ["Denn", "nur", "ein\u00b7mal", "wein\u00b7te", "sie", "von", "Her\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.6": {"text": "Als sie weinte, ward das Dunkel helle,", "tokens": ["Als", "sie", "wein\u00b7te", ",", "ward", "da