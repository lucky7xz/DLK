{"textgrid.poem.60332": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Das Wiedersehen", "genre": "verse", "period": "N.A.", "pub_year": 1835, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du heimatliches Tal,", "tokens": ["Du", "hei\u00b7mat\u00b7li\u00b7ches", "Tal", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Mir wird so wohl und wehe,", "tokens": ["Mir", "wird", "so", "wohl", "und", "we\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "KON", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df ich dich nun einmal,", "tokens": ["Da\u00df", "ich", "dich", "nun", "ein\u00b7mal", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ersehntes! wiedersehe.", "tokens": ["Er\u00b7sehn\u00b7tes", "!", "wie\u00b7der\u00b7se\u00b7he", "."], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$.", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Weinberg, sei mir gegr\u00fc\u00dft!", "tokens": ["Wein\u00b7berg", ",", "sei", "mir", "ge\u00b7gr\u00fc\u00dft", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VAFIN", "PPER", "VVPP", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Noch gr\u00fcnen deine Reben,", "tokens": ["Noch", "gr\u00fc\u00b7nen", "dei\u00b7ne", "Re\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Womit du oft vers\u00fc\u00dft", "tokens": ["Wo\u00b7mit", "du", "oft", "ver\u00b7s\u00fc\u00dft"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADV", "VVPP"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ein herbes Menschenleben;", "tokens": ["Ein", "her\u00b7bes", "Men\u00b7schen\u00b7le\u00b7ben", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Viel Herbste schwanden dir,", "tokens": ["Viel", "Herbs\u00b7te", "schwan\u00b7den", "dir", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Die deine Trauben reiften,", "tokens": ["Die", "dei\u00b7ne", "Trau\u00b7ben", "reif\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und die vom Herzen mir", "tokens": ["Und", "die", "vom", "Her\u00b7zen", "mir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "APPRART", "NN", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "So manche Hoffnung streiften.", "tokens": ["So", "man\u00b7che", "Hoff\u00b7nung", "streif\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Noch kenn ich jeden Baum,", "tokens": ["Noch", "kenn", "ich", "je\u00b7den", "Baum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Wo ich vor so viel Jahren", "tokens": ["Wo", "ich", "vor", "so", "viel", "Jah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPR", "ADV", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Gehegt den Jugendtraum,", "tokens": ["Ge\u00b7hegt", "den", "Ju\u00b7gend\u00b7traum", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Der scheu dahingefahren.", "tokens": ["Der", "scheu", "da\u00b7hin\u00b7ge\u00b7fah\u00b7ren", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Noch kenn ich jedes Haus;", "tokens": ["Noch", "kenn", "ich", "je\u00b7des", "Haus", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Doch andre Menschen schreiten", "tokens": ["Doch", "and\u00b7re", "Men\u00b7schen", "schrei\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Gesch\u00e4ftig ein und aus,", "tokens": ["Ge\u00b7sch\u00e4f\u00b7tig", "ein", "und", "aus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Als wie zu meinen Zeiten.", "tokens": ["Als", "wie", "zu", "mei\u00b7nen", "Zei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Ich frage dort und hier", "tokens": ["Ich", "fra\u00b7ge", "dort", "und", "hier"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "KON", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Nach einem Freund mit Zagen", "tokens": ["Nach", "ei\u00b7nem", "Freund", "mit", "Za\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und Furcht, ich k\u00f6nnte schier", "tokens": ["Und", "Furcht", ",", "ich", "k\u00f6nn\u00b7te", "schier"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "NN", "$,", "PPER", "VMFIN", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Nach einem Toten fragen.", "tokens": ["Nach", "ei\u00b7nem", "To\u00b7ten", "fra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Es ist nur noch der Ort,", "tokens": ["Es", "ist", "nur", "noch", "der", "Ort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Wo wir gefreut uns haben,", "tokens": ["Wo", "wir", "ge\u00b7freut", "uns", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Lieben all sind fort,", "tokens": ["Die", "Lie\u00b7ben", "all", "sind", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "PIAT", "VAFIN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Verreiset und begraben.", "tokens": ["Ver\u00b7rei\u00b7set", "und", "be\u00b7gra\u00b7ben", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Drum bleib ich hier nicht lang,", "tokens": ["Drum", "bleib", "ich", "hier", "nicht", "lang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ADV", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Mich f\u00fchlend zu verlassen,", "tokens": ["Mich", "f\u00fch\u00b7lend", "zu", "ver\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVPP", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und tu auch keinen Gang", "tokens": ["Und", "tu", "auch", "kei\u00b7nen", "Gang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "NE", "ADV", "PIAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Bei Tag mehr durch die Stra\u00dfen.", "tokens": ["Bei", "Tag", "mehr", "durch", "die", "Stra\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Erst wenn es worden Nacht", "tokens": ["Erst", "wenn", "es", "wor\u00b7den", "Nacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "PPER", "VAPP", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Und schl\u00e4ft des Tags Gebrause,", "tokens": ["Und", "schl\u00e4ft", "des", "Tags", "Ge\u00b7brau\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Schleich ich heran mich sacht", "tokens": ["Schleich", "ich", "he\u00b7ran", "mich", "sacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "PPER", "PTKVZ", "PPER", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Zu manchem Freundeshause.", "tokens": ["Zu", "man\u00b7chem", "Freun\u00b7des\u00b7hau\u00b7se", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Die s\u00fc\u00dfe Tr\u00e4umerei", "tokens": ["Die", "s\u00fc\u00b7\u00dfe", "Tr\u00e4u\u00b7me\u00b7rei"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Such ich dann festzuhalten,", "tokens": ["Such", "ich", "dann", "fest\u00b7zu\u00b7hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Als ob doch alles sei", "tokens": ["Als", "ob", "doch", "al\u00b7les", "sei"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "ADV", "PIS", "VAFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Geblieben hier beim alten.", "tokens": ["Ge\u00b7blie\u00b7ben", "hier", "beim", "al\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPRART", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Zum Fenster dann empor", "tokens": ["Zum", "Fens\u00b7ter", "dann", "em\u00b7por"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADV", "PTKVZ"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Blick ich und lausch und gr\u00fc\u00dfe,", "tokens": ["Blick", "ich", "und", "lausch", "und", "gr\u00fc\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "KON", "ADJD", "KON", "VVFIN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Ob mich, den ich verlor,", "tokens": ["Ob", "mich", ",", "den", "ich", "ver\u00b7lor", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Der Freund erblicken m\u00fcsse;", "tokens": ["Der", "Freund", "er\u00b7bli\u00b7cken", "m\u00fcs\u00b7se", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Ich lausch und scheide nicht,", "tokens": ["Ich", "lausch", "und", "schei\u00b7de", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "KON", "VVFIN", "PTKNEG", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Bis ich zu schauen meine", "tokens": ["Bis", "ich", "zu", "schau\u00b7en", "mei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PTKZU", "VVINF", "PPOSAT"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sein liebes Angesicht", "tokens": ["Sein", "lie\u00b7bes", "An\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Im wirren Mondenscheine.", "tokens": ["Im", "wir\u00b7ren", "Mon\u00b7den\u00b7schei\u00b7ne", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Du heimatliches Tal,", "tokens": ["Du", "hei\u00b7mat\u00b7li\u00b7ches", "Tal", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Mir wird so wohl und wehe,", "tokens": ["Mir", "wird", "so", "wohl", "und", "we\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "KON", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df ich dich nun einmal,", "tokens": ["Da\u00df", "ich", "dich", "nun", "ein\u00b7mal", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ersehntes! wiedersehe.", "tokens": ["Er\u00b7sehn\u00b7tes", "!", "wie\u00b7der\u00b7se\u00b7he", "."], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$.", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Weinberg, sei mir gegr\u00fc\u00dft!", "tokens": ["Wein\u00b7berg", ",", "sei", "mir", "ge\u00b7gr\u00fc\u00dft", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VAFIN", "PPER", "VVPP", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Noch gr\u00fcnen deine Reben,", "tokens": ["Noch", "gr\u00fc\u00b7nen", "dei\u00b7ne", "Re\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Womit du oft vers\u00fc\u00dft", "tokens": ["Wo\u00b7mit", "du", "oft", "ver\u00b7s\u00fc\u00dft"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADV", "VVPP"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ein herbes Menschenleben;", "tokens": ["Ein", "her\u00b7bes", "Men\u00b7schen\u00b7le\u00b7ben", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "Viel Herbste schwanden dir,", "tokens": ["Viel", "Herbs\u00b7te", "schwan\u00b7den", "dir", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Die deine Trauben reiften,", "tokens": ["Die", "dei\u00b7ne", "Trau\u00b7ben", "reif\u00b7te