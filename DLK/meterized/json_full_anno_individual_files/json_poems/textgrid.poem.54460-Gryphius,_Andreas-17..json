{"textgrid.poem.54460": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "17.", "genre": "verse", "period": "N.A.", "pub_year": 1640, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Des strengen richters buch/ das buch so voll von s\u00fcnden", "tokens": ["Des", "stren\u00b7gen", "rich\u00b7ters", "buch", "/", "das", "buch", "so", "voll", "von", "s\u00fcn\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN", "$(", "ART", "NN", "ADV", "ADJD", "APPR", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Voll vbermachter schuld in dem viel tausend pfund", "tokens": ["Voll", "vber\u00b7mach\u00b7ter", "schuld", "in", "dem", "viel", "tau\u00b7send", "pfund"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "ADJD", "ADJD", "APPR", "ART", "PIAT", "CARD", "NN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "Ein jeder seele hatt/ mitt welcher alle stund", "tokens": ["Ein", "je\u00b7der", "see\u00b7le", "hatt", "/", "mitt", "wel\u00b7cher", "al\u00b7le", "stund"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "PIAT", "NN", "VAFIN", "$(", "APPR", "PRELS", "PIAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der hochergrimbte Gott/ sich rechtlich ab will finden/", "tokens": ["Der", "ho\u00b7cher\u00b7grimb\u00b7te", "Gott", "/", "sich", "recht\u00b7lich", "ab", "will", "fin\u00b7den", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "PRF", "ADJD", "PTKVZ", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"te