{"textgrid.poem.47348": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Das bittere Kraut", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Scheiden und Meiden, du bittres Kraut!", "tokens": ["O", "Schei\u00b7den", "und", "Mei\u00b7den", ",", "du", "bitt\u00b7res", "Kraut", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "KON", "NN", "$,", "PPER", "ADJA", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Wer hat dich zuerst im Garten gebaut?", "tokens": ["Wer", "hat", "dich", "zu\u00b7erst", "im", "Gar\u00b7ten", "ge\u00b7baut", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "ADV", "APPRART", "NN", "VVPP", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Konnt' er nichts Besseres ziehen?", "tokens": ["Konnt'", "er", "nichts", "Bes\u00b7se\u00b7res", "zie\u00b7hen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PIS", "PIS", "VVINF", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Er hat dich mit seinen Augen betaut,", "tokens": ["Er", "hat", "dich", "mit", "sei\u00b7nen", "Au\u00b7gen", "be\u00b7taut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PRF", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Davon bist du gediehen.", "tokens": ["Da\u00b7von", "bist", "du", "ge\u00b7die\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "VVPP", "$."], "meter": "-++--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2"