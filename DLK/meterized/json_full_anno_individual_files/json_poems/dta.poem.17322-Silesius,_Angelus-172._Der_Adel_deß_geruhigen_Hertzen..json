{"dta.poem.17322": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "172.  Der Adel de\u00df geruhigen Hertzen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Mein Hertze wenns GOtt ruht/ ists\u2019 Braut Bett seines", "tokens": ["Mein", "Hert\u00b7ze", "wenns", "Gott", "ruht", "/", "ist\u00b7s'", "Braut", "Bett", "sei\u00b7nes"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "VVFIN", "KOUS", "NN", "VVFIN", "$(", "VAFIN", "NN", "NN", "PPOSAT"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Sohns:", "tokens": ["Sohns", ":"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Wanns dann sein Geist bewegt/ die s\u00e4nffte Solomons.", "tokens": ["Wanns", "dann", "sein", "Geist", "be\u00b7wegt", "/", "die", "s\u00e4nff\u00b7te", "So\u00b7lo\u00b7mons", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPOSAT", "NN", "VVFIN", "$(", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}