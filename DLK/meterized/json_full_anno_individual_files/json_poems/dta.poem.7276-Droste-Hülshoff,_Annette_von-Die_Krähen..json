{"dta.poem.7276": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Die Kr\u00e4hen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-20090519994", "language": ["de:0.99"], "booktitle": "Droste-H\u00fclshoff, Annette von: Gedichte. Stuttgart u. a., 1844."}, "poem": {"stanza.1": {"line.1": {"text": "Hei\u00df, hei\u00df der Sonnenbrand", "tokens": ["Hei\u00df", ",", "hei\u00df", "der", "Son\u00b7nen\u00b7brand"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "ADJD", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Dr\u00fcckt vom Zenith herunter,", "tokens": ["Dr\u00fcckt", "vom", "Ze\u00b7nith", "her\u00b7un\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Weit, weit der gelbe Sand", "tokens": ["Weit", ",", "weit", "der", "gel\u00b7be", "Sand"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "$,", "ADJD", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Zieht sein Gest\u00e4ube drunter;", "tokens": ["Zieht", "sein", "Ge\u00b7st\u00e4u\u00b7be", "drun\u00b7ter", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PAV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Nur wie ein gr\u00fcner Strich", "tokens": ["Nur", "wie", "ein", "gr\u00fc\u00b7ner", "Strich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Am Horizont die F\u00f6hren;", "tokens": ["Am", "Ho\u00b7ri\u00b7zont", "die", "F\u00f6h\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Mich d\u00fcnkt, man m\u00fc\u00dft' es h\u00f6ren,", "tokens": ["Mich", "d\u00fcnkt", ",", "man", "m\u00fc\u00dft'", "es", "h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PIS", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Wenn nur ein Kranker schlich.", "tokens": ["Wenn", "nur", "ein", "Kran\u00b7ker", "schlich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Der blasse Aether siecht,", "tokens": ["Der", "blas\u00b7se", "A\u00b7e\u00b7ther", "siecht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Ein Ruhen rings, ein Schweigen,", "tokens": ["Ein", "Ru\u00b7hen", "rings", ",", "ein", "Schwei\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Dem matt das Ohr erliegt;", "tokens": ["Dem", "matt", "das", "Ohr", "er\u00b7liegt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Nur an der D\u00fcne steigen", "tokens": ["Nur", "an", "der", "D\u00fc\u00b7ne", "stei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Zwei Fichten, d\u00fcrr, ergraut \u2014", "tokens": ["Zwei", "Fich\u00b7ten", ",", "d\u00fcrr", ",", "er\u00b7graut"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["CARD", "NN", "$,", "ADJD", "$,", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Wie Trauernde am Grabe \u2014", "tokens": ["Wie", "Trau\u00b7ern\u00b7de", "am", "Gra\u00b7be"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPRART", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Wo einsam sich ein Rabe", "tokens": ["Wo", "ein\u00b7sam", "sich", "ein", "Ra\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "PRF", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die rupp'gen Federn kraut.", "tokens": ["Die", "rupp'\u00b7gen", "Fe\u00b7dern", "kraut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Da zieht's in Westen schwer", "tokens": ["Da", "zieht's", "in", "Wes\u00b7ten", "schwer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "NN", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Wie eine Wetterwolke,", "tokens": ["Wie", "ei\u00b7ne", "Wet\u00b7ter\u00b7wol\u00b7ke", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Kreis't um die F\u00f6hren her", "tokens": ["Kreis't", "um", "die", "F\u00f6h\u00b7ren", "her"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "APZR"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "Und f\u00e4llt am Haidekolke;", "tokens": ["Und", "f\u00e4llt", "am", "Hai\u00b7de\u00b7kol\u00b7ke", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und wieder steigt es dann,", "tokens": ["Und", "wie\u00b7der", "steigt", "es", "dann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Es flattert und es \u00e4chzet,", "tokens": ["Es", "flat\u00b7tert", "und", "es", "\u00e4ch\u00b7zet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und immer n\u00e4her kr\u00e4chzet", "tokens": ["Und", "im\u00b7mer", "n\u00e4\u00b7her", "kr\u00e4ch\u00b7zet"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "ADJD", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Das Galgenvolk heran.", "tokens": ["Das", "Gal\u00b7gen\u00b7volk", "he\u00b7ran", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Recht, wo der Sand sich d\u00e4mmt,", "tokens": ["Recht", ",", "wo", "der", "Sand", "sich", "d\u00e4mmt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "Da lagert es am H\u00fcgel;", "tokens": ["Da", "la\u00b7gert", "es", "am", "H\u00fc\u00b7gel", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Es badet sich und schwemmt,", "tokens": ["Es", "ba\u00b7det", "sich", "und", "schwemmt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "KON", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "St\u00e4ubt Asche durch die Fl\u00fcgel", "tokens": ["St\u00e4ubt", "A\u00b7sche", "durch", "die", "Fl\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Bis jede Feder grau;", "tokens": ["Bis", "je\u00b7de", "Fe\u00b7der", "grau", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Dann rasten sie im Bade,", "tokens": ["Dann", "ras\u00b7ten", "sie", "im", "Ba\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und horchen der Suade", "tokens": ["Und", "hor\u00b7chen", "der", "Sua\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Der alten Kr\u00e4henfrau,", "tokens": ["Der", "al\u00b7ten", "Kr\u00e4\u00b7hen\u00b7frau", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Die sich im Sande reckt,", "tokens": ["Die", "sich", "im", "San\u00b7de", "reckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Das Bein lang ausgeschossen,", "tokens": ["Das", "Bein", "lang", "aus\u00b7ge\u00b7schos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ihr eines Aug' gefleckt,", "tokens": ["Ihr", "ei\u00b7nes", "Aug'", "ge\u00b7fleckt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Das andre ist geschlossen;", "tokens": ["Das", "and\u00b7re", "ist", "ge\u00b7schlos\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Zweihundert Jahr und mehr", "tokens": ["Zwei\u00b7hun\u00b7dert", "Jahr", "und", "mehr"], "token_info": ["word", "word", "word", "word"], "pos": ["CARD", "NN", "KON", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Gehetzt mit allen Hunden,", "tokens": ["Ge\u00b7hetzt", "mit", "al\u00b7len", "Hun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Schnarrt sie nun ihre Kunden", "tokens": ["Schnarrt", "sie", "nun", "ih\u00b7re", "Kun\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Dem jungen Volke her:", "tokens": ["Dem", "jun\u00b7gen", "Vol\u00b7ke", "her", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "\u201eja, ritterlich und k\u00fchn all sein Gebahr!", "tokens": ["\u201e", "ja", ",", "rit\u00b7ter\u00b7lich", "und", "k\u00fchn", "all", "sein", "Ge\u00b7bahr", "!"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PTKANT", "$,", "ADJD", "KON", "ADJD", "PIAT", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wenn er so herstolzirte vor der Schaar,", "tokens": ["Wenn", "er", "so", "her\u00b7stol\u00b7zir\u00b7te", "vor", "der", "Schaar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und lie\u00df sein b\u00e4umend Ro\u00df so drehn und schwenken,", "tokens": ["Und", "lie\u00df", "sein", "b\u00e4u\u00b7mend", "Ro\u00df", "so", "drehn", "und", "schwen\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "ADJD", "NN", "ADV", "CARD", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da mu\u00dft ich immer an Sanct G\u00f6rgen denken,", "tokens": ["Da", "mu\u00dft", "ich", "im\u00b7mer", "an", "Sanct", "G\u00f6r\u00b7gen", "den\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "APPR", "VVFIN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Den Wettermann, der \u2014 als am Schlot ich sa\u00df,", "tokens": ["Den", "Wet\u00b7ter\u00b7mann", ",", "der", "als", "am", "Schlot", "ich", "sa\u00df", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "$(", "KOKOM", "APPRART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Lie\u00df mir die Sonne auf den R\u00fccken brennen \u2014", "tokens": ["Lie\u00df", "mir", "die", "Son\u00b7ne", "auf", "den", "R\u00fc\u00b7cken", "bren\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Vom Wind getrillt mich schlug so hart, da\u00df ba\u00df", "tokens": ["Vom", "Wind", "ge\u00b7trillt", "mich", "schlug", "so", "hart", ",", "da\u00df", "ba\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPRART", "NN", "VVPP", "PPER", "VVFIN", "ADV", "ADJD", "$,", "KOUS", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Ich es dem alten Raben m\u00f6chte g\u00f6nnen,", "tokens": ["Ich", "es", "dem", "al\u00b7ten", "Ra\u00b7ben", "m\u00f6ch\u00b7te", "g\u00f6n\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPER", "ART", "ADJA", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Der dort von seiner Hopfenstange schaut,", "tokens": ["Der", "dort", "von", "sei\u00b7ner", "Hop\u00b7fen\u00b7stan\u00b7ge", "schaut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Als sey ein Baum er und wir andern Kraut! \u2014", "tokens": ["Als", "sey", "ein", "Baum", "er", "und", "wir", "an\u00b7dern", "Kraut", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "VAFIN", "ART", "NN", "PPER", "KON", "PPER", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "\u201ek\u00fchn war der Halberstadt, das ist gewi\u00df!", "tokens": ["\u201e", "k\u00fchn", "war", "der", "Hal\u00b7ber\u00b7stadt", ",", "das", "ist", "ge\u00b7wi\u00df", "!"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "VAFIN", "ART", "NN", "$,", "PDS", "VAFIN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wenn er die Braue zog, die Lippe bi\u00df,", "tokens": ["Wenn", "er", "die", "Brau\u00b7e", "zog", ",", "die", "Lip\u00b7pe", "bi\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$,", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dann standen seine Landsknecht' auf den F\u00fc\u00dfen", "tokens": ["Dann", "stan\u00b7den", "sei\u00b7ne", "Lands\u00b7knecht'", "auf", "den", "F\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wie Speere, solche Blicke konnt er schie\u00dfen.", "tokens": ["Wie", "Spee\u00b7re", ",", "sol\u00b7che", "Bli\u00b7cke", "konnt", "er", "schie\u00b7\u00dfen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "$,", "PIAT", "NN", "VMFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Einst brach sein Schwert; er ri\u00df die Kuppel los,", "tokens": ["Einst", "brach", "sein", "Schwert", ";", "er", "ri\u00df", "die", "Kup\u00b7pel", "los", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$.", "PPER", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Stie\u00df mit der Scheide einen Mann vom Pferde.", "tokens": ["Stie\u00df", "mit", "der", "Schei\u00b7de", "ei\u00b7nen", "Mann", "vom", "Pfer\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ART", "NN", "APPRART", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.7": {"text": "Ich war nur immer froh, da\u00df fl\u00fcgellos,", "tokens": ["Ich", "