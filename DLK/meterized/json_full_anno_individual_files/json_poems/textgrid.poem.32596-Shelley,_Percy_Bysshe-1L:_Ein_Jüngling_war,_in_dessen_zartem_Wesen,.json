{"textgrid.poem.32596": {"metadata": {"author": {"name": "Shelley, Percy Bysshe", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein J\u00fcngling war, in dessen zartem Wesen,", "genre": "verse", "period": "N.A.", "pub_year": 1816, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein J\u00fcngling war, in dessen zartem Wesen,", "tokens": ["Ein", "J\u00fcng\u00b7ling", "war", ",", "in", "des\u00b7sen", "zar\u00b7tem", "We\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$,", "APPR", "PRELAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie Licht und Wind in einer duft'gen Wolke,", "tokens": ["Wie", "Licht", "und", "Wind", "in", "ei\u00b7ner", "duft'\u00b7gen", "Wol\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die vor des blauen Mittags Gluth zergeht,", "tokens": ["Die", "vor", "des", "blau\u00b7en", "Mit\u00b7tags", "Gluth", "zer\u00b7geht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "ADJA", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der Genius sich mit dem Tode stritt.", "tokens": ["Der", "Ge\u00b7nius", "sich", "mit", "dem", "To\u00b7de", "stritt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Niemand vermag die s\u00fc\u00dfe Lust zu ahnen,", "tokens": ["Nie\u00b7mand", "ver\u00b7mag", "die", "s\u00fc\u00b7\u00dfe", "Lust", "zu", "ah\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die seinen Odem, gleich dem Zauberbann", "tokens": ["Die", "sei\u00b7nen", "O\u00b7dem", ",", "gleich", "dem", "Zau\u00b7ber\u00b7bann"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "PPOSAT", "NN", "$,", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Der stillen Sommerluft, verstummen machte,", "tokens": ["Der", "stil\u00b7len", "Som\u00b7mer\u00b7luft", ",", "ver\u00b7stum\u00b7men", "mach\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Als er mit der Geliebten, welche damals", "tokens": ["Als", "er", "mit", "der", "Ge\u00b7lieb\u00b7ten", ",", "wel\u00b7che", "da\u00b7mals"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "$,", "PRELS", "ADV"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.9": {"text": "Die Schrankenlosigkeit vereinten Seins", "tokens": ["Die", "Schran\u00b7ken\u00b7lo\u00b7sig\u00b7keit", "ver\u00b7ein\u00b7ten", "Seins"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Zuerst gekostet, durch ein Feld gewandelt,", "tokens": ["Zu\u00b7erst", "ge\u00b7kos\u00b7tet", ",", "durch", "ein", "Feld", "ge\u00b7wan\u00b7delt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "$,", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Das, gegen Ost von einem Hain beschattet,", "tokens": ["Das", ",", "ge\u00b7gen", "Ost", "von", "ei\u00b7nem", "Hain", "be\u00b7schat\u00b7tet", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "APPR", "NN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Dem Himmel gegen Westen offen lag.", "tokens": ["Dem", "Him\u00b7mel", "ge\u00b7gen", "Wes\u00b7ten", "of\u00b7fen", "lag", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Dort war die Sonne jetzt hinabgesunken,", "tokens": ["Dort", "war", "die", "Son\u00b7ne", "jetzt", "hin\u00b7ab\u00b7ge\u00b7sun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Doch Streifen Golds ums\u00e4umten noch die Wolken,", "tokens": ["Doch", "Strei\u00b7fen", "Golds", "um\u00b7s\u00e4um\u00b7ten", "noch", "die", "Wol\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "NN", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Der weiten Grasesebne Spitzen, und", "tokens": ["Der", "wei\u00b7ten", "Gra\u00b7se\u00b7seb\u00b7ne", "Spit\u00b7zen", ",", "und"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ART", "ADJA", "NN", "NN", "$,", "KON"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Des alten L\u00f6wenzahnes grauen Bart,", "tokens": ["Des", "al\u00b7ten", "L\u00f6\u00b7wen\u00b7zah\u00b7nes", "grau\u00b7en", "Bart", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Und lagen auf dem dichten, braunen Wald,", "tokens": ["Und", "la\u00b7gen", "auf", "dem", "dich\u00b7ten", ",", "brau\u00b7nen", "Wald", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Vereinigt mit des Zwielichts D\u00e4mmerschatten.", "tokens": ["Ver\u00b7ei\u00b7nigt", "mit", "des", "Zwie\u00b7lichts", "D\u00e4m\u00b7mer\u00b7schat\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Im Ost hob langsam sich des Vollmonds Scheibe", "tokens": ["Im", "Ost", "hob", "lang\u00b7sam", "sich", "des", "Voll\u00b7monds", "Schei\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "ADJD", "PRF", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Zwischen der B\u00e4ume St\u00e4mmen hell empor,", "tokens": ["Zwi\u00b7schen", "der", "B\u00e4u\u00b7me", "St\u00e4m\u00b7men", "hell", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Und droben schaarten sich die bleichen Sterne. \u2013", "tokens": ["Und", "dro\u00b7ben", "schaar\u00b7ten", "sich", "die", "blei\u00b7chen", "Ster\u00b7ne", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADV", "VVFIN", "PRF", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "\u00bbist es nicht seltsam, Isabella\u00ab, sprach", "tokens": ["\u00bb", "ist", "es", "nicht", "selt\u00b7sam", ",", "I\u00b7sa\u00b7bel\u00b7la", "\u00ab", ",", "sprach"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "punct", "punct", "word"], "pos": ["$(", "VAFIN", "PPER", "PTKNEG", "ADJD", "$,", "NE", "$(", "$,", "VVFIN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.23": {"text": "Der J\u00fcngling, \u00bbda\u00df ich nie die Sonne sah?", "tokens": ["Der", "J\u00fcng\u00b7ling", ",", "\u00bb", "da\u00df", "ich", "nie", "die", "Son\u00b7ne", "sah", "?"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "$(", "KOUS", "PPER", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "Wir wollen morgen wieder hieher wandeln,", "tokens": ["Wir", "wol\u00b7len", "mor\u00b7gen", "wie\u00b7der", "hie\u00b7her", "wan\u00b7deln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "PAV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.25": {"text": "Dann sollst du sie mit mir einmal beschaun.\u00ab", "tokens": ["Dann", "sollst", "du", "sie", "mit", "mir", "ein\u00b7mal", "be\u00b7schaun", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPER", "APPR", "PPER", "ADV", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.26": {"text": "Der J\u00fcngling und das M\u00e4dchen lagen Beide", "tokens": ["Der", "J\u00fcng\u00b7ling", "und", "das", "M\u00e4d\u00b7chen", "la\u00b7gen", "Bei\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NN", "VVFIN", "PIS"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.27": {"text": "Vereint in Lieb' und Schlummer diese Nacht \u2013", "tokens": ["Ver\u00b7eint", "in", "Lieb'", "und", "Schlum\u00b7mer", "die\u00b7se", "Nacht", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "KON", "NN", "PDAT", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.28": {"text": "Doch als der Morgen kam, da fand das M\u00e4dchen", "tokens": ["Doch", "als", "der", "Mor\u00b7gen", "kam", ",", "da", "fand", "das", "M\u00e4d\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "VVFIN", "$,", "ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.29": {"text": "Den Freund, den hei\u00dfgeliebten, todt und kalt.", "tokens": ["Den", "Freund", ",", "den", "hei\u00df\u00b7ge\u00b7lieb\u00b7ten", ",", "todt", "und", "kalt", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.30": {"text": "Glaubt nicht, da\u00df Gott in seiner Gnade so", "tokens": ["Glaubt", "nicht", ",", "da\u00df", "Gott", "in", "sei\u00b7ner", "Gna\u00b7de", "so"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PTKNEG", "$,", "KOUS", "NN", "APPR", "PPOSAT", "NN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.31": {"text": "Ihn heimgef\u00fchrt. Das M\u00e4dchen starb nicht, ward", "tokens": ["Ihn", "heim\u00b7ge\u00b7f\u00fchrt", ".", "Das", "M\u00e4d\u00b7chen", "starb", "nicht", ",", "ward"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "$.", "ART", "NN", "VVFIN", "PTKNEG", "$,", "VAFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.32": {"text": "Wahnsinnig nicht, \u2013 sie lebte lange Jahre.", "tokens": ["Wahn\u00b7sin\u00b7nig", "nicht", ",", "\u2013", "sie", "leb\u00b7te", "lan\u00b7ge", "Jah\u00b7re", "."], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "$,", "$(", "PPER", "VVFIN", "ADV", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.33": {"text": "Zwar mein' ich, ihre Sanftmuth und Geduld,", "tokens": ["Zwar", "mein'", "ich", ",", "ih\u00b7re", "Sanft\u00b7muth", "und", "Ge\u00b7duld", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.34": {"text": "Ihr traurig L\u00e4cheln, und da\u00df sie nicht starb,", "tokens": ["Ihr", "trau\u00b7rig", "L\u00e4\u00b7cheln", ",", "und", "da\u00df", "sie", "nicht", "starb", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "NN", "$,", "KON", "KOUS", "PPER", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.35": {"text": "Nein, weiter lebt', um ihren greisen Vater", "tokens": ["Nein", ",", "wei\u00b7ter", "lebt'", ",", "um", "ih\u00b7ren", "grei\u00b7sen", "Va\u00b7ter"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "ADV", "VVFIN", "$,", "KOUI", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.36": {"text": "Zu pflegen, waren eine Art von Wahnsinn,", "tokens": ["Zu", "pfle\u00b7gen", ",", "wa\u00b7ren", "ei\u00b7ne", "Art", "von", "Wahn\u00b7sinn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "VAFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.37": {"text": "Wenn Wahnsinn anders sein hei\u00dft, als die Welt.", "tokens": ["Wenn", "Wahn\u00b7sinn", "an\u00b7ders", "sein", "hei\u00dft", ",", "als", "die", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADV", "VAINF", "VVFIN", "$,", "KOUS", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.38": {"text": "Denn sie zu sehn nur, war, als ob man lese", "tokens": ["Denn", "sie", "zu", "sehn", "nur", ",", "war", ",", "als", "ob", "man", "le\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPER", "PTKZU", "VVINF", "ADV", "$,", "VAFIN", "$,", "KOKOM", "KOUS", "PIS", "VVFIN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.39": {"text": "Ein Lied, das ein geweihter Dichter schuf,", "tokens": ["Ein", "Lied", ",", "das", "ein", "ge\u00b7weih\u00b7ter", "Dich\u00b7ter", "schuf", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.40": {"text": "Das harte Herzen l\u00f6st in linde Wehmuth.", "tokens": ["Das", "har\u00b7te", "Her\u00b7zen", "l\u00f6st", "in", "lin\u00b7de", "Weh\u00b7muth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.41": {"text": "Von Thr\u00e4nen war die Wimper weggesengt,", "tokens": ["Von", "Thr\u00e4\u00b7nen", "war", "die", "Wim\u00b7per", "weg\u00b7ge\u00b7sengt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.42": {"text": "Und Lipp' und Wange wie der Tod so bleich,", "tokens": ["Und", "Lipp'", "und", "Wan\u00b7ge", "wie", "der", "Tod", "so", "bleich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "KON", "NN", "KOKOM", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.43": {"text": "Die H\u00e4nde mager, da\u00df durch die Gelenke", "tokens": ["Die", "H\u00e4n\u00b7de", "ma\u00b7ger", ",", "da\u00df", "durch", "die", "Ge\u00b7len\u00b7ke"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "$,", "KOUS", "APPR", "ART", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.44": {"text": "Und Adern schier des Tages r\u00f6thlich Licht", "tokens": ["Und", "A\u00b7dern", "schier", "des", "Ta\u00b7ges", "r\u00f6th\u00b7lich", "Licht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "ADJD", "ART", "NN", "ADJD", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.45": {"text": "Durchschien. Das Grab von deinem todten Ich,", "tokens": ["Durch\u00b7schien", ".", "Das", "Grab", "von", "dei\u00b7nem", "tod\u00b7ten", "Ich", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "ART", "NN", "APPR", "PPOSAT", "ADJA", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.46": {"text": "Das Ein unsteter Geist bei Nacht und Tag", "tokens": ["Das", "Ein", "un\u00b7ste\u00b7ter", "Geist", "bei", "Nacht", "und", "Tag"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "ART", "ADJA", "NN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.47": {"text": "Bewohnt, ist Alles, du verlornes Kind,", "tokens": ["Be\u00b7wohnt", ",", "ist", "Al\u00b7les", ",", "du", "ver\u00b7lor\u00b7nes", "Kind", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "