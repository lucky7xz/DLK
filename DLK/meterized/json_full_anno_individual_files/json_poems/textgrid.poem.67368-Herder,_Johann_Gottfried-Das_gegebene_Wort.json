{"textgrid.poem.67368": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Das gegebene Wort", "genre": "verse", "period": "N.A.", "pub_year": 1789, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Von einem m\u00e4chtigen Seer\u00e4uber war", "tokens": ["Von", "ei\u00b7nem", "m\u00e4ch\u00b7ti\u00b7gen", "See\u00b7r\u00e4u\u00b7ber", "war"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VAFIN"], "meter": "-+-+--++-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Heemskerk, ein Prediger, mit vielen Andern", "tokens": ["Heems\u00b7kerk", ",", "ein", "Pre\u00b7di\u00b7ger", ",", "mit", "vie\u00b7len", "An\u00b7dern"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "ART", "NN", "$,", "APPR", "PIAT", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Gefangen und von ihm zum Fort geschickt,", "tokens": ["Ge\u00b7fan\u00b7gen", "und", "von", "ihm", "zum", "Fort", "ge\u00b7schickt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "APPR", "PPER", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der Insel Uebergabe zu bewirken;", "tokens": ["Der", "In\u00b7sel", "Ue\u00b7berg\u00b7a\u00b7be", "zu", "be\u00b7wir\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Wo nicht, und kehrte er nicht wieder, so", "tokens": ["Wo", "nicht", ",", "und", "kehr\u00b7te", "er", "nicht", "wie\u00b7der", ",", "so"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["PWAV", "PTKNEG", "$,", "KON", "VVFIN", "PPER", "PTKNEG", "ADV", "$,", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Erwarte seine Mitgefangnen alle", "tokens": ["Er\u00b7war\u00b7te", "sei\u00b7ne", "Mit\u00b7ge\u00b7fang\u00b7nen", "al\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "PIAT"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Ein b\u00f6ser Tod. Heemskerk, ein Regulus,", "tokens": ["Ein", "b\u00f6\u00b7ser", "Tod", ".", "Heems\u00b7kerk", ",", "ein", "Re\u00b7gu\u00b7lus", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Ging in das Fort und sprach mit allen Kr\u00e4ften,", "tokens": ["Ging", "in", "das", "Fort", "und", "sprach", "mit", "al\u00b7len", "Kr\u00e4f\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "KON", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Nicht zu ergeben sich, und kehrte dann", "tokens": ["Nicht", "zu", "er\u00b7ge\u00b7ben", "sich", ",", "und", "kehr\u00b7te", "dann"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKNEG", "PTKZU", "VVFIN", "PRF", "$,", "KON", "VVFIN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "(da halfen seiner Mutter, seiner Kinder", "tokens": ["(", "da", "hal\u00b7fen", "sei\u00b7ner", "Mut\u00b7ter", ",", "sei\u00b7ner", "Kin\u00b7der"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["$(", "ADV", "VVFIN", "PPOSAT", "NN", "$,", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Und seines Weibes Thr\u00e4nen nicht), er kehrte", "tokens": ["Und", "sei\u00b7nes", "Wei\u00b7bes", "Thr\u00e4\u00b7nen", "nicht", ")", ",", "er", "kehr\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "NN", "PTKNEG", "$(", "$,", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Zur\u00fcck in die Gefangenschaft. \u00bbIch mu\u00df", "tokens": ["Zu\u00b7r\u00fcck", "in", "die", "Ge\u00b7fan\u00b7gen\u00b7schaft", ".", "\u00bb", "Ich", "mu\u00df"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "$.", "$(", "PPER", "VMFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Mein Wort erf\u00fcllen,\u00ab sprach er. \u00bbNiemand soll", "tokens": ["Mein", "Wort", "er\u00b7f\u00fcl\u00b7len", ",", "\u00ab", "sprach", "er", ".", "\u00bb", "Nie\u00b7mand", "soll"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "punct", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "VVINF", "$,", "$(", "VVFIN", "PPER", "$.", "$(", "PIS", "VMFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Von Heemskerk sagen, da\u00df er, sich zu retten,", "tokens": ["Von", "Heems\u00b7kerk", "sa\u00b7gen", ",", "da\u00df", "er", ",", "sich", "zu", "ret\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "$,", "KOUS", "PPER", "$,", "PRF", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Nur ", "tokens": ["Nur"], "token_info": ["word"], "pos": ["ADV"], "meter": "+", "measure": "single.up"}, "line.16": {"text": "Er ging; die Flotte kam und rettet' Alle.", "tokens": ["Er", "ging", ";", "die", "Flot\u00b7te", "kam", "und", "ret\u00b7tet'", "Al\u00b7le", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"]