{"textgrid.poem.56992": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Der Lattenzaun", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es war einmal ein Lattenzaun,", "tokens": ["Es", "war", "ein\u00b7mal", "ein", "Lat\u00b7ten\u00b7zaun", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "mit Zwischenraum, hindurchzuschaun.", "tokens": ["mit", "Zwi\u00b7schen\u00b7raum", ",", "hin\u00b7durch\u00b7zu\u00b7schaun", "."], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "$,", "VVIZU", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ein Architekt, der dieses sah,", "tokens": ["Ein", "Ar\u00b7chi\u00b7tekt", ",", "der", "die\u00b7ses", "sah", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PDS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "stand eines Abends pl\u00f6tzlich da \u2013", "tokens": ["stand", "ei\u00b7nes", "A\u00b7bends", "pl\u00f6tz\u00b7lich", "da", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJD", "ADV", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "und nahm den Zwischenraum heraus", "tokens": ["und", "nahm", "den", "Zwi\u00b7schen\u00b7raum", "he\u00b7raus"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "und baute draus ein gro\u00dfes Haus.", "tokens": ["und", "bau\u00b7te", "draus", "ein", "gro\u00b7\u00dfes", "Haus", "."], "t