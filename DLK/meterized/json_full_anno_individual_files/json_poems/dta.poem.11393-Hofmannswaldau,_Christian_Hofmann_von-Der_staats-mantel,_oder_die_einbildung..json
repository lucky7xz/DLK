{"dta.poem.11393": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Der staats-mantel, oder die einbildung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es ist ein wort bey den gelehrten,", "tokens": ["Es", "ist", "ein", "wort", "bey", "den", "ge\u00b7lehr\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "ART", "ADJA", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Das heisset man ", "tokens": ["Das", "heis\u00b7set", "man"], "token_info": ["word", "word", "word"], "pos": ["PDS", "VVFIN", "PIS"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Als solches meine freunde h\u00f6rten,", "tokens": ["Als", "sol\u00b7ches", "mei\u00b7ne", "freun\u00b7de", "h\u00f6r\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So fragten sie: Was heist denn so?", "tokens": ["So", "frag\u00b7ten", "sie", ":", "Was", "heist", "denn", "so", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$.", "PWS", "VVFIN", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich sprach: Ich wills euch k\u00fcrtzlich sagen,", "tokens": ["Ich", "sprach", ":", "Ich", "wills", "euch", "k\u00fcrtz\u00b7lich", "sa\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PPER", "VMFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Es heist ein mantel auf latein,", "tokens": ["Es", "heist", "ein", "man\u00b7tel", "auf", "la\u00b7tein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Den alle solche leute tragen,", "tokens": ["Den", "al\u00b7le", "sol\u00b7che", "leu\u00b7te", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Die in der welt was groses seyn.", "tokens": ["Die", "in", "der", "welt", "was", "gro\u00b7ses", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "PWS", "ADJA", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Den mantel trugen schon die alten,", "tokens": ["Den", "man\u00b7tel", "tru\u00b7gen", "schon", "die", "al\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Denn dieses war ihr ehren-kleid:", "tokens": ["Denn", "die\u00b7ses", "war", "ihr", "eh\u00b7ren\u00b7kleid", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die nach-welt hat die tracht behalten,", "tokens": ["Die", "nach\u00b7welt", "hat", "die", "tracht", "be\u00b7hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+---+-+-", "measure": "dactylic.init"}, "line.4": {"text": "Und diese w\u00e4hrt bi\u00df diese zeit.", "tokens": ["Und", "die\u00b7se", "w\u00e4hrt", "bi\u00df", "die\u00b7se", "zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der mantel decket alle m\u00e4ngel,", "tokens": ["Der", "man\u00b7tel", "de\u00b7cket", "al\u00b7le", "m\u00e4n\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und weil sich mancher drein verh\u00fcllt!", "tokens": ["Und", "weil", "sich", "man\u00b7cher", "drein", "ver\u00b7h\u00fcllt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PRF", "PIS", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "So kommt es, da\u00df gar offt ein pengel", "tokens": ["So", "kommt", "es", ",", "da\u00df", "gar", "offt", "ein", "pen\u00b7gel"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "ADV", "ADV", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "So viel, als ein gelehrter, gilt.", "tokens": ["So", "viel", ",", "als", "ein", "ge\u00b7lehr\u00b7ter", ",", "gilt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADV", "$,", "KOUS", "ART", "ADJA", "$,", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Wie mancher gilt mit seinen lehren", "tokens": ["Wie", "man\u00b7cher", "gilt", "mit", "sei\u00b7nen", "leh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PIS", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So viel, als ein propheten-kind:", "tokens": ["So", "viel", ",", "als", "ein", "pro\u00b7phe\u00b7ten\u00b7kind", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "KOUS", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er l\u00e4sset keine worte h\u00f6ren,", "tokens": ["Er", "l\u00e4s\u00b7set", "kei\u00b7ne", "wor\u00b7te", "h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die nicht, als wie orackel sind:", "tokens": ["Die", "nicht", ",", "als", "wie", "o\u00b7rac\u00b7kel", "sind", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "$,", "KOUS", "KOKOM", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Er ist, wie dort der hohe priester:", "tokens": ["Er", "ist", ",", "wie", "dort", "der", "ho\u00b7he", "pries\u00b7ter", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "PWAV", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die welt erschallt vor seinem ruhm;", "tokens": ["Die", "welt", "er\u00b7schallt", "vor", "sei\u00b7nem", "ruhm", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und meritirt kaum einen k\u00fcster:", "tokens": ["Und", "me\u00b7ri\u00b7tirt", "kaum", "ei\u00b7nen", "k\u00fcs\u00b7ter", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "ADJA", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Das macht, er hat den mantel um.", "tokens": ["Das", "macht", ",", "er", "hat", "den", "man\u00b7tel", "um", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "PPER", "VAFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ein ander, der das recht verstehet,", "tokens": ["Ein", "an\u00b7der", ",", "der", "das", "recht", "ver\u00b7ste\u00b7het", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "$,", "PRELS", "ART", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der tritt, als wie ein gott, herein,", "tokens": ["Der", "tritt", ",", "als", "wie", "ein", "gott", ",", "her\u00b7ein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "KOUS", "KOKOM", "ART", "NN", "$,", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und wenn man nur von weitem gehet,", "tokens": ["Und", "wenn", "man", "nur", "von", "wei\u00b7tem", "ge\u00b7het", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PIS", "ADV", "APPR", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So mu\u00df der hut herunter seyn;", "tokens": ["So", "mu\u00df", "der", "hut", "her\u00b7un\u00b7ter", "seyn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "VVFIN", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ach! solte man den mantel rauben,", "tokens": ["Ach", "!", "sol\u00b7te", "man", "den", "man\u00b7tel", "rau\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VMFIN", "PIS", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der seine schande decken mu\u00df,", "tokens": ["Der", "sei\u00b7ne", "schan\u00b7de", "de\u00b7cken", "mu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJA", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "So w\u00fcrde man wahrhafftig glauben,", "tokens": ["So", "w\u00fcr\u00b7de", "man", "wahr\u00b7haff\u00b7tig", "glau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Er w\u00e4re kaum ", "tokens": ["Er", "w\u00e4\u00b7re", "kaum"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV"], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Manch medicus lebt sehr bequeme,", "tokens": ["Manch", "me\u00b7di\u00b7cus", "lebt", "sehr", "be\u00b7que\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "ADV", "ADJA", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Und macht die kasten ziemlich schwer:", "tokens": ["Und", "macht", "die", "kas\u00b7ten", "ziem\u00b7lich", "schwer", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Man d\u00e4chte, pest und hunger k\u00e4me,", "tokens": ["Man", "d\u00e4ch\u00b7te", ",", "pest", "und", "hun\u00b7ger", "k\u00e4\u00b7me", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "VVFIN", "KON", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wann dieser nicht zugegen w\u00e4r;", "tokens": ["Wann", "die\u00b7ser", "nicht", "zu\u00b7ge\u00b7gen", "w\u00e4r", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PDS", "PTKNEG", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Doch \u00f6ffters trifft man in den messen", "tokens": ["Doch", "\u00f6ff\u00b7ters", "trifft", "man", "in", "den", "mes\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PIS", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wohl einen land-betr\u00fcger an,", "tokens": ["Wohl", "ei\u00b7nen", "lan\u00b7dbe\u00b7tr\u00fc\u00b7ger", "an", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der hat so viel, als er vergessen:", "tokens": ["Der", "hat", "so", "viel", ",", "als", "er", "ver\u00b7ges\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ADV", "$,", "KOUS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Drum sehet, was der mantel kan.", "tokens": ["Drum", "se\u00b7het", ",", "was", "der", "man\u00b7tel", "kan", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "$,", "PRELS", "ART", "NN", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Wohlan! bey so gestalten sachen", "tokens": ["Wo\u00b7hlan", "!", "bey", "so", "ge\u00b7stal\u00b7ten", "sa\u00b7chen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$.", "APPR", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Schm\u00e4hlt auf die neue kleidung nicht:", "tokens": ["Schm\u00e4hlt", "auf", "die", "neu\u00b7e", "klei\u00b7dung", "nicht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es l\u00e4st kein mensch ein neues machen,", "tokens": ["Es", "l\u00e4st", "kein", "mensch", "ein", "neu\u00b7es", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "ART", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Weil es das alte noch verricht;", "tokens": ["Weil", "es", "das", "al\u00b7te", "noch", "ver\u00b7richt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wir sind den v\u00e4tern nachgeschlagen,", "tokens": ["Wir", "sind", "den", "v\u00e4\u00b7tern", "nach\u00b7ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die kinder machens eben so:", "tokens": ["Die", "kin\u00b7der", "ma\u00b7chens", "e\u00b7ben", "so", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Den mantel, den wir alle tragen,", "tokens": ["Den", "man\u00b7tel", ",", "den", "wir", "al\u00b7le", "tra\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "PIS", "VVINF", "$,"], "meter": "-+---+-+-", "measure": "dactylic.init"}, "line.8": {"text": "Der heist und bleibt ", "tokens": ["Der", "heist", "und", "bleibt"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}}}}}