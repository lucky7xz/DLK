{"dta.poem.8462": {"metadata": {"author": {"name": "H\u00f6lty, Ludwig Christoph Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "An die Nachtigall.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1783", "urn": "urn:nbn:de:kobv:b4-200905192568", "language": ["de:0.99"], "booktitle": "H\u00f6lty, Ludwig Christoph Heinrich: Gedichte. Hamburg, 1783."}, "poem": {"stanza.1": {"line.1": {"text": "Tonreichen Schall", "tokens": ["Ton\u00b7rei\u00b7chen", "Schall"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "++-+", "measure": "iambic.di"}, "line.2": {"text": "Vom Bl\u00fctenast des Apfelbaums hernieder,", "tokens": ["Vom", "Bl\u00fc\u00b7ten\u00b7ast", "des", "Ap\u00b7fel\u00b7baums", "her\u00b7nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "O Nachtigall.", "tokens": ["O", "Nach\u00b7ti\u00b7gall", "."], "token_info": ["word", "word", "punct"], "pos": ["NE", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Du t\u00f6nest mir mit deiner s\u00fcssen Kehle", "tokens": ["Du", "t\u00f6\u00b7nest", "mir", "mit", "dei\u00b7ner", "s\u00fcs\u00b7sen", "Keh\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Die Liebe wach;", "tokens": ["Die", "Lie\u00b7be", "wach", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Denn schon durchbebt die Tiefen meiner Seele", "tokens": ["Denn", "schon", "durch\u00b7bebt", "die", "Tie\u00b7fen", "mei\u00b7ner", "See\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Dein schmelzend Ach.", "tokens": ["Dein", "schmel\u00b7zend", "Ach", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "ITJ", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Dann flieht der Schlaf von neuem dieses Lager,", "tokens": ["Dann", "flieht", "der", "Schlaf", "von", "neu\u00b7em", "die\u00b7ses", "La\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ADJA", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich starre dann,", "tokens": ["Ich", "star\u00b7re", "dann", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Mit nassem Blick, und todtenbleich und hager,", "tokens": ["Mit", "nas\u00b7sem", "Blick", ",", "und", "tod\u00b7ten\u00b7bleich", "und", "ha\u00b7ger", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "KON", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den Himmel an.", "tokens": ["Den", "Him\u00b7mel", "an", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Fleuch, Nachtigall, in gr\u00fcne Finsternisse,", "tokens": ["Fleuch", ",", "Nach\u00b7ti\u00b7gall", ",", "in", "gr\u00fc\u00b7ne", "Fins\u00b7ter\u00b7nis\u00b7se", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ins Haingestr\u00e4uch,", "tokens": ["Ins", "Hain\u00b7ge\u00b7str\u00e4uch", ","], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Und spend' im Nest der treuen Gattin K\u00fcsse;", "tokens": ["Und", "spend'", "im", "Nest", "der", "treu\u00b7en", "Gat\u00b7tin", "K\u00fcs\u00b7se", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Entfleuch, entfleuch!", "tokens": ["Ent\u00b7fleuch", ",", "ent\u00b7fleuch", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "VVIMP", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}