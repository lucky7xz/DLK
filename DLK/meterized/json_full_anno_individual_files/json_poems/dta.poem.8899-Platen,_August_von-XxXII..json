{"dta.poem.8899": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "XxXII.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Mag's um die Buden am Rialto flittern:", "tokens": ["Mag's", "um", "die", "Bu\u00b7den", "am", "Ri\u00b7al\u00b7to", "flit\u00b7tern", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Um nicht den Geist im Tande zu zersplittern,", "tokens": ["Um", "nicht", "den", "Geist", "im", "Tan\u00b7de", "zu", "zer\u00b7split\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PTKNEG", "ART", "NN", "APPRART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Such' ich die Stille, die den Tag besieget.", "tokens": ["Such'", "ich", "die", "Stil\u00b7le", ",", "die", "den", "Tag", "be\u00b7sie\u00b7get", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Dann blick' ich oft, an Br\u00fccken angeschmieget,", "tokens": ["Dann", "blick'", "ich", "oft", ",", "an", "Br\u00fc\u00b7cken", "an\u00b7ge\u00b7schmie\u00b7get", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In \u00f6de Wellen, die nur leise zittern,", "tokens": ["In", "\u00f6\u00b7de", "Wel\u00b7len", ",", "die", "nur", "lei\u00b7se", "zit\u00b7tern", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "PRELS", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wo \u00fcber Mauern, welche halb verwittern,", "tokens": ["Wo", "\u00fc\u00b7ber", "Mau\u00b7ern", ",", "wel\u00b7che", "halb", "ver\u00b7wit\u00b7tern", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "NN", "$,", "PRELS", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein wilder Lorbeerbusch die Zweige bieget.", "tokens": ["Ein", "wil\u00b7der", "Lor\u00b7beer\u00b7busch", "die", "Zwei\u00b7ge", "bie\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und wann ich, stehend auf versteinten Pf\u00e4hlen,", "tokens": ["Und", "wann", "ich", ",", "ste\u00b7hend", "auf", "ver\u00b7stein\u00b7ten", "Pf\u00e4h\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "$,", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den Blick hinaus in's dunkle Meer verliere,", "tokens": ["Den", "Blick", "hin\u00b7aus", "in's", "dunk\u00b7le", "Meer", "ver\u00b7lie\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APZR", "APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dem f\u00fcrder keine Dogen sich verm\u00e4hlen:", "tokens": ["Dem", "f\u00fcr\u00b7der", "kei\u00b7ne", "Do\u00b7gen", "sich", "ver\u00b7m\u00e4h\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "PIAT", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Dann st\u00f6rt mich kaum im schweigenden Reviere,", "tokens": ["Dann", "st\u00f6rt", "mich", "kaum", "im", "schwei\u00b7gen\u00b7den", "Re\u00b7vie\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Herschallend aus entlegenen Kan\u00e4len,", "tokens": ["Her\u00b7schal\u00b7lend", "aus", "ent\u00b7le\u00b7ge\u00b7nen", "Ka\u00b7n\u00e4\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Von Zeit zu Zeit ein Ruf der Gondoliere.", "tokens": ["Von", "Zeit", "zu", "Zeit", "ein", "Ruf", "der", "Gon\u00b7do\u00b7lie\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}