{"textgrid.poem.40767": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Im Tuileriengarten", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im Tuileriengarten", "tokens": ["Im", "Tu\u00b7i\u00b7le\u00b7ri\u00b7en\u00b7gar\u00b7ten"], "token_info": ["word", "word"], "pos": ["APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bl\u00fcht ein Kastanienbaum;", "tokens": ["Bl\u00fcht", "ein", "Kas\u00b7ta\u00b7ni\u00b7en\u00b7baum", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Die Br\u00fcder aller Arten", "tokens": ["Die", "Br\u00fc\u00b7der", "al\u00b7ler", "Ar\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Umf\u00e4ngt noch Wintertraum.", "tokens": ["Um\u00b7f\u00e4ngt", "noch", "Win\u00b7ter\u00b7traum", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Eh' ihre Knospen sprangen,", "tokens": ["Eh'", "ih\u00b7re", "Knos\u00b7pen", "spran\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Rauscht seine Bl\u00e4tterkron';", "tokens": ["Rauscht", "sei\u00b7ne", "Bl\u00e4t\u00b7ter\u00b7kron'", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Eh' sie mit Laub behangen,", "tokens": ["Eh'", "sie", "mit", "Laub", "be\u00b7han\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Prangt er in Bl\u00fcthen schon.", "tokens": ["Prangt", "er", "in", "Bl\u00fc\u00b7then", "schon", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "ADV", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.3": {"line.1": {"text": "So tr\u00e4gt der Auserkorne", "tokens": ["So", "tr\u00e4gt", "der", "Au\u00b7ser\u00b7kor\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das Lenzpanier voran,", "tokens": ["Das", "Lenz\u00b7pa\u00b7nier", "vo\u00b7ran", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df er zur Folge sporne", "tokens": ["Da\u00df", "er", "zur", "Fol\u00b7ge", "spor\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Den gr\u00fcnen Heeresbann.", "tokens": ["Den", "gr\u00fc\u00b7nen", "Hee\u00b7res\u00b7bann", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ich lehnt' einst an dem Baume", "tokens": ["Ich", "lehnt'", "einst", "an", "dem", "Bau\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der mir zu Herzen sprach,", "tokens": ["Der", "mir", "zu", "Her\u00b7zen", "sprach", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und sann im Schattenraume", "tokens": ["Und", "sann", "im", "Schat\u00b7ten\u00b7rau\u00b7me"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dem Bl\u00fcthenr\u00e4thsel nach.", "tokens": ["Dem", "Bl\u00fc\u00b7then\u00b7r\u00e4th\u00b7sel", "nach", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Mich wollt's der Geister mahnen,", "tokens": ["Mich", "wollt's", "der", "Geis\u00b7ter", "mah\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die schon zum Licht erwacht,", "tokens": ["Die", "schon", "zum", "Licht", "er\u00b7wacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Als auf der Menschheit Bahnen", "tokens": ["Als", "auf", "der", "Menschheit", "Bah\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Noch lag des Wahnes Nacht;", "tokens": ["Noch", "lag", "des", "Wah\u00b7nes", "Nacht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Ich dachte der Erkornen,", "tokens": ["Ich", "dach\u00b7te", "der", "Er\u00b7kor\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In denen l\u00e4ngst gebl\u00fcht", "tokens": ["In", "de\u00b7nen", "l\u00e4ngst", "ge\u00b7bl\u00fcht"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PRELS", "ADV", "VVPP"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Was jetzt uns Sp\u00e4tgebornen", "tokens": ["Was", "jetzt", "uns", "Sp\u00e4t\u00b7ge\u00b7bor\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "ADV", "PPER", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nachlenzet im Gem\u00fcth. \u2013", "tokens": ["Nach\u00b7len\u00b7zet", "im", "Ge\u00b7m\u00fcth", ".", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "APPRART", "NN", "$.", "$("], "meter": "+----+", "measure": "dactylic.init"}}, "stanza.7": {"line.1": {"text": "Da schritt mit seinem Sohne", "tokens": ["Da", "schritt", "mit", "sei\u00b7nem", "Soh\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Des Wegs ein Edelmann,", "tokens": ["Des", "Wegs", "ein", "E\u00b7del\u00b7mann", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sah still zur Wipfelkrone", "tokens": ["Sah", "still", "zur", "Wip\u00b7fel\u00b7kro\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und sprach zum Jungen dann:", "tokens": ["Und", "sprach", "zum", "Jun\u00b7gen", "dann", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "\u00bbhut ab! Ein Denkmal ragen", "tokens": ["\u00bb", "hut", "ab", "!", "Ein", "Denk\u00b7mal", "ra\u00b7gen"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "VVFIN", "PTKVZ", "$.", "ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Siehst du der Schreckensnacht,", "tokens": ["Siehst", "du", "der", "Schre\u00b7cken\u00b7snacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da Meuter hier erschlagen", "tokens": ["Da", "Meu\u00b7ter", "hier", "er\u00b7schla\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "NN", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die treu'ste K\u00f6nigswacht.", "tokens": ["Die", "treu'\u00b7ste", "K\u00f6\u00b7nigs\u00b7wacht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Weil von so edlen Leichen", "tokens": ["Weil", "von", "so", "ed\u00b7len", "Lei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ADV", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ged\u00fcngt der heil'ge Baum,", "tokens": ["Ge\u00b7d\u00fcngt", "der", "heil'\u00b7ge", "Baum", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mu\u00df er vor Seinesgleichen", "tokens": ["Mu\u00df", "er", "vor", "Sei\u00b7nes\u00b7glei\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der erste bl\u00fchn im Raum.\u00ab", "tokens": ["Der", "ers\u00b7te", "bl\u00fchn", "im", "Raum", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Ihm folgten Wandrerschaaren", "tokens": ["Ihm", "folg\u00b7ten", "Wand\u00b7rer\u00b7schaa\u00b7ren"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In Blousenhemden nach;", "tokens": ["In", "Blou\u00b7sen\u00b7hem\u00b7den", "nach", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ein Werkmann hoch in Jahren", "tokens": ["Ein", "Werk\u00b7mann", "hoch", "in", "Jah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zu den Genossen sprach:", "tokens": ["Zu", "den", "Ge\u00b7nos\u00b7sen", "sprach", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "\u00bbhier haben sie verblutet", "tokens": ["\u00bb", "hier", "ha\u00b7ben", "sie", "ver\u00b7blu\u00b7tet"], "token_info": ["punct", "word", "word", "word", "word"], "pos": ["$(", "ADV", "VAFIN", "PPER", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mit Schergen im Gefecht,", "tokens": ["Mit", "Scher\u00b7gen", "im", "Ge\u00b7fecht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die M\u00e4nner freigemuthet,", "tokens": ["Die", "M\u00e4n\u00b7ner", "frei\u00b7ge\u00b7mu\u00b7thet", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "F\u00fcr ihres Volkes Recht.", "tokens": ["F\u00fcr", "ih\u00b7res", "Vol\u00b7kes", "Recht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Von solchem Thau begossen", "tokens": ["Von", "sol\u00b7chem", "Thau", "be\u00b7gos\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wird fruchtbar jeder Grund,", "tokens": ["Wird", "frucht\u00b7bar", "je\u00b7der", "Grund", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Drum mu\u00df der Baum auch sprossen", "tokens": ["Drum", "mu\u00df", "der", "Baum", "auch", "spros\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VMFIN", "ART", "NN", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der erste weit im Rund.\u00ab \u2013", "tokens": ["Der", "ers\u00b7te", "weit", "im", "Rund", ".", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ART", "ADJA", "ADJD", "APPRART", "NN", "$.", "$(", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Ich horchte ihren Reden", "tokens": ["Ich", "horch\u00b7te", "ih\u00b7ren", "Re\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und sah das Widerspiel,", "tokens": ["Und", "sah", "das", "Wi\u00b7der\u00b7spiel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Als in die alten Fehden", "tokens": ["Als", "in", "die", "al\u00b7ten", "Feh\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die junge Bl\u00fcthe fiel.", "tokens": ["Die", "jun\u00b7ge", "Bl\u00fc\u00b7the", "fiel", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Sie w\u00e4hnen jede Ader", "tokens": ["Sie", "w\u00e4h\u00b7nen", "je\u00b7de", "A\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Des Baumes \u00fcbervoll", "tokens": ["Des", "Bau\u00b7mes", "\u00fc\u00b7ber\u00b7voll"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Getr\u00e4nkt mit ihrem Hader,", "tokens": ["Ge\u00b7tr\u00e4nkt", "mit", "ih\u00b7rem", "Ha\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mit ihrem Zwist und Groll;", "tokens": ["Mit", "ih\u00b7rem", "Zwist", "und", "Groll", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "Doch er, \u2013 o mildes Tauschen! \u2013", "tokens": ["Doch", "er", ",", "\u2013", "o", "mil\u00b7des", "Tau\u00b7schen", "!", "\u2013"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPER", "$,", "$(", "FM", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Er l\u00e4\u00dft ihr z\u00fcrnend Weh", "tokens": ["Er", "l\u00e4\u00dft", "ihr", "z\u00fcr\u00b7nend", "Weh"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "VVPP", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Im Bl\u00e4tterkranz verrauschen,", "tokens": ["Im", "Bl\u00e4t\u00b7ter\u00b7kranz", "ver\u00b7rau\u00b7schen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Verwehn im Bl\u00fcthenschnee.", "tokens": ["Ver\u00b7wehn", "im", "Bl\u00fc\u00b7then\u00b7schnee", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.16": {"line.1": {"text": "Verrausche und verwehe", "tokens": ["Ver\u00b7rau\u00b7sche", "und", "ver\u00b7we\u00b7he"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So unser Leid und Streit!", "tokens": ["So", "un\u00b7ser", "Leid", "und", "Streit", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Den Bl\u00fcthenkranz nur sehe", "tokens": ["Den", "Bl\u00fc\u00b7then\u00b7kranz", "nur", "se\u00b7he"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Davon die Enkelzeit.", "tokens": ["Da\u00b7von", "die", "En\u00b7kel\u00b7zeit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.17": {"line.1": {"text": "Im Tuileriengarten", "tokens": ["Im", "Tu\u00b7i\u00b7le\u00b7ri\u00b7en\u00b7gar\u00b7ten"], "token_info": ["word", "word"], "pos": ["APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bl\u00fcht ein Kastanienbaum;", "tokens": ["Bl\u00fcht", "ein", "Kas\u00b7ta\u00b7ni\u00b7en\u00b7baum", ";"], "toke