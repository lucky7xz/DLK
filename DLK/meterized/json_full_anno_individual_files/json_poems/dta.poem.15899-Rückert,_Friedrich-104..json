{"dta.poem.15899": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "104.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Gegen den J\u00fcnger nimmt vertrauliche Geberden", "tokens": ["Ge\u00b7gen", "den", "J\u00fcn\u00b7ger", "nimmt", "ver\u00b7trau\u00b7li\u00b7che", "Ge\u00b7ber\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "ADJA", "NN"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Kein Meister, ohne gleich daf\u00fcr bestraft zu werden.", "tokens": ["Kein", "Meis\u00b7ter", ",", "oh\u00b7ne", "gleich", "da\u00b7f\u00fcr", "be\u00b7straft", "zu", "wer\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "KOUI", "ADV", "PAV", "ADJD", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Da\u00df du herunterstiegst zu ihm, wird er vergessen,", "tokens": ["Da\u00df", "du", "her\u00b7un\u00b7ter\u00b7stiegst", "zu", "ihm", ",", "wird", "er", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "APPR", "PPER", "$,", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und mit dir Haupt an Haupt auf ebnem Feld sich messen.", "tokens": ["Und", "mit", "dir", "Haupt", "an", "Haupt", "auf", "eb\u00b7nem", "Feld", "sich", "mes\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "NN", "APPR", "NN", "APPR", "ADJA", "NN", "PRF", "VVINF", "$."], "meter