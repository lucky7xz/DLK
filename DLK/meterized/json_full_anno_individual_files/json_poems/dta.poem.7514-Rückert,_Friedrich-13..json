{"dta.poem.7514": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "13.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Stell dich in Reih und Glied, das Ganze zu verst\u00e4rken,", "tokens": ["Stell", "dich", "in", "Reih", "und", "Glied", ",", "das", "Gan\u00b7ze", "zu", "ver\u00b7st\u00e4r\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "APPR", "NN", "KON", "NN", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mag auch, wer 's Ganze sieht, dich nicht darin bemerken.", "tokens": ["Mag", "auch", ",", "wer", "'s", "Gan\u00b7ze", "sieht", ",", "dich", "nicht", "da\u00b7rin", "be\u00b7mer\u00b7ken", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "$,", "PWS", "PPER", "NN", "VVFIN", "$,", "PPER", "PTKNEG", "PAV", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}}, "stanza.2": {"line.1": {"text": "Mag auch, wer 's Ganze sieht, dich nicht darin bemerken;", "tokens": ["Mag", "auch", ",", "wer", "'s", "Gan\u00b7ze", "sieht", ",", "dich", "nicht", "da\u00b7rin", "be\u00b7mer\u00b7ken", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "$,", "PWS", "PPER", "NN", "VVFIN", "$,", "PPER", "PTKNEG", "PAV", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.2": {"text": "Das Ganze wirkt, und du bist drin mit deinen Werken.", "tokens": ["Das", "Gan\u00b7ze", "wirkt", ",", "und", "du", "bist", "drin", "mit", "dei\u00b7nen", "Wer\u00b7ken", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "KON", "PPER", "VAFIN", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Stell dich in Reih und Glied, und schaare dich den Schaaren;", "tokens": ["Stell", "dich", "in", "Reih", "und", "Glied", ",", "und", "schaa\u00b7re", "dich", "den", "Schaa\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "APPR", "NN", "KON", "NN", "$,", "KON", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und theilst du nicht den Ruhm, so theilst du die Gefahren.", "tokens": ["Und", "theilst", "du", "nicht", "den", "Ruhm", ",", "so", "theilst", "du", "die", "Ge\u00b7fah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "ART", "NN", "$,", "ADV", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Wird nicht der Musterer den Einzelmann gewahren,", "tokens": ["Wird", "nicht", "der", "Mus\u00b7te\u00b7rer", "den", "Ein\u00b7zel\u00b7mann", "ge\u00b7wah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit Lust doch wird er sehn vollz\u00e4hlig seine Schaaren.", "tokens": ["Mit", "Lust", "doch", "wird", "er", "sehn", "voll\u00b7z\u00e4h\u00b7lig", "sei\u00b7ne", "Schaa\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "VAFIN", "PPER", "VVFIN", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Damit im Lanzenwald nicht fehlet eine Lanze,", "tokens": ["Da\u00b7mit", "im", "Lan\u00b7zen\u00b7wald", "nicht", "feh\u00b7let", "ei\u00b7ne", "Lan\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "APPRART", "NN", "PTKNEG", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Heb deine fein, und sei gefa\u00dft auf jede Schanze.", "tokens": ["Heb", "dei\u00b7ne", "fein", ",", "und", "sei", "ge\u00b7fa\u00dft", "auf", "je\u00b7de", "Schan\u00b7ze", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "ADJD", "$,", "KON", "VAFIN", "VVPP", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Sei nur ein Blatt im Kranz, ein Ring im Ringeltanze,", "tokens": ["Sei", "nur", "ein", "Blatt", "im", "Kranz", ",", "ein", "Ring", "im", "Rin\u00b7gel\u00b7tan\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "APPRART", "NN", "$,", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "F\u00fchl' dich im Ganzen ganz, und ewig wie das Ganze!", "tokens": ["F\u00fchl'", "dich", "im", "Gan\u00b7zen", "ganz", ",", "und", "e\u00b7wig", "wie", "das", "Gan\u00b7ze", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPRART", "NN", "ADV", "$,", "KON", "ADJD", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}