{"textgrid.poem.60531": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Guter Rat", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Willst du auf die Ferne wirken,", "tokens": ["Willst", "du", "auf", "die", "Fer\u00b7ne", "wir\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und dein Leben ihnen zeigen,", "tokens": ["Und", "dein", "Le\u00b7ben", "ih\u00b7nen", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Mu\u00dft du vor den Nahen schweigen,", "tokens": ["Mu\u00dft", "du", "vor", "den", "Na\u00b7hen", "schwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die belauschend dich umzirken,", "tokens": ["Die", "be\u00b7lau\u00b7schend", "dich", "um\u00b7zir\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PPER", "VVIZU", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Die, wenn sie ein Wort erpassen,", "tokens": ["Die", ",", "wenn", "sie", "ein", "Wort", "er\u00b7pas\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "KOUS", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Gleich entstellt es drucken lassen.", "tokens": ["Gleich", "ent\u00b7stellt", "es", "dru\u00b7cken", "las\u00b7sen", ".