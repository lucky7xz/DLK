{"dta.poem.5423": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "J rdisches  \n  V ergn\u00fcgen  \n in  \n  GOTT .  \n  F \u00fcnfter  T heil.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein fr\u00f6liches: GOtt Lob! allein", "tokens": ["Ein", "fr\u00f6\u00b7li\u00b7ches", ":", "Gott", "Lob", "!", "al\u00b7lein"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ART", "ADJA", "$.", "NN", "NN", "$.", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wird meine gantze Gabe seyn.", "tokens": ["Wird", "mei\u00b7ne", "gant\u00b7ze", "Ga\u00b7be", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein solches freuden-reiches Lallen,", "tokens": ["Ein", "sol\u00b7ches", "freu\u00b7den\u00b7rei\u00b7ches", "Lal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Gewirckt durch danck-begier\u2019ge Triebe,", "tokens": ["Ge\u00b7wirckt", "durch", "dan\u00b7ck\u00b7be\u00b7gier'\u00b7ge", "Trie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Wird, uns zum Nutz, o ew\u2019ge Liebe,", "tokens": ["Wird", ",", "uns", "zum", "Nutz", ",", "o", "ew'\u00b7ge", "Lie\u00b7be", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "PPER", "APPRART", "NN", "$,", "FM", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Allein aus Liebe, dir gefallen.", "tokens": ["Al\u00b7lein", "aus", "Lie\u00b7be", ",", "dir", "ge\u00b7fal\u00b7len", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "$,", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}