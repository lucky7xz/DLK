{"dta.poem.2943": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "142.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Lust der Welt ist durch das Christenthum verdorben;", "tokens": ["Die", "Lust", "der", "Welt", "ist", "durch", "das", "Chris\u00b7ten\u00b7thum", "ver\u00b7dor\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VAFIN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wir alle sind am Kreuz, an dem Er hieng, gestorben.", "tokens": ["Wir", "al\u00b7le", "sind", "am", "Kreuz", ",", "an", "dem", "Er", "hieng", ",", "ge\u00b7stor\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "PIS", "VAFIN", "APPRART", "NN", "$,", "APPR", "PRELS", "PPER", "VVFIN", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Und soll die Lust der Welt nie wieder sich geb\u00e4ren?", "tokens": ["Und", "soll", "die", "Lust", "der", "Welt", "nie", "wie\u00b7der", "sich", "ge\u00b7b\u00e4\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "NN", "ART", "NN", "ADV", "ADV", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ja, der sie \u00fcberwand, der wird sie auch verkl\u00e4ren.", "tokens": ["Ja", ",", "der", "sie", "\u00fc\u00b7ber\u00b7wand", ",", "der", "wird", "sie", "auch", "ver\u00b7kl\u00e4\u00b7ren", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PRELS", "PPER", "VVFIN", "$,", "PRELS", "VAFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Neu wird die Rose bl\u00fchn am Ziel der Dornenbahn.", "tokens": ["Neu", "wird", "die", "Ro\u00b7se", "bl\u00fchn", "am", "Ziel", "der", "Dor\u00b7nen\u00b7bahn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "VVFIN", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Erf\u00fcllt das Christenthum! so ist es abgethan.", "tokens": ["Er\u00b7f\u00fcllt", "das", "Chris\u00b7ten\u00b7thum", "!", "so", "ist", "es", "ab\u00b7ge\u00b7than", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$.", "ADV", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Einsetzen werden dann das Fleisch in seine Rechte", "tokens": ["Ein\u00b7set\u00b7zen", "wer\u00b7den", "dann", "das", "Fleisch", "in", "sei\u00b7ne", "Rech\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "ADV", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Des Geistes Freie, nicht, wie jetzt, der S\u00fcnde Knechte.", "tokens": ["Des", "Geis\u00b7tes", "Frei\u00b7e", ",", "nicht", ",", "wie", "jetzt", ",", "der", "S\u00fcn\u00b7de", "Knech\u00b7te", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "PTKNEG", "$,", "PWAV", "ADV", "$,", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}