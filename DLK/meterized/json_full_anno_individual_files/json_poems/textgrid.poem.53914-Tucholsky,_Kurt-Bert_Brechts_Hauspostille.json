, "der", "Hei\u00b7mat", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Voran die Musik mit Tschindrara", "tokens": ["Vo\u00b7ran", "die", "Mu\u00b7sik", "mit", "Tschin\u00b7dra\u00b7ra"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "APPR", "NE"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Spielt einen flotten Marsch.", "tokens": ["Spielt", "ei\u00b7nen", "flot\u00b7ten", "Marsch", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und der Soldat, so wie ers gelernt", "tokens": ["Und", "der", "Sol\u00b7dat", ",", "so", "wie", "ers", "ge\u00b7lernt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "$,", "ADV", "KOKOM", "PIS", "VVPP"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Schmei\u00dft seine Beine vom Arsch.", "tokens": ["Schmei\u00dft", "sei\u00b7ne", "Bei\u00b7ne", "vom", "Arsch", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPRART", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.6": {"line.1": {"text": "Die Katzen und die Hunde schrein", "tokens": ["Die", "Kat\u00b7zen", "und", "die", "Hun\u00b7de", "schrein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Ratzen im Feld pfeifen w\u00fcst:", "tokens": ["Die", "Rat\u00b7zen", "im", "Feld", "pfei\u00b7fen", "w\u00fcst", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN",