{"textgrid.poem.48492": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "13. Von den Blumen", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Rosen Milch und Blut mu\u00df ihren Wangen weichen,", "tokens": ["Der", "Ro\u00b7sen", "Milch", "und", "Blut", "mu\u00df", "ih\u00b7ren", "Wan\u00b7gen", "wei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "NN", "VMFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "kein rotes Negelein mag ihrem Munde gleichen.", "tokens": ["kein", "ro\u00b7tes", "Ne\u00b7ge\u00b7lein", "mag", "ih\u00b7rem", "Mun\u00b7de", "glei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VMFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Saffranblume stirbt f\u00fcr ihrer Haare Zier,", "tokens": ["Die", "Saf\u00b7fran\u00b7blu\u00b7me", "stirbt", "f\u00fcr", "ih\u00b7rer", "Haa\u00b7re", "Zier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Vergi\u00dfmeinnicht vergi\u00dft auch seiner selbst f\u00fcr ihr.", "tokens": ["Ver\u00b7gi\u00df\u00b7mein\u00b7nicht", "ver\u00b7gi\u00dft", "auch", "sei\u00b7ner", "selbst", "f\u00fcr", "ihr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "PPOSAT", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Narzissen sind wol wei\u00df, doch nicht f\u00fcr ihren H\u00e4nden,", "tokens": ["Nar\u00b7zis\u00b7sen", "sind", "wol", "wei\u00df", ",", "doch", "nicht", "f\u00fcr", "ih\u00b7ren", "H\u00e4n\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "VVFIN", "$,", "ADV", "PTKNEG", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "die Veilgen werden blas, wenn sie sich nach ihr wenden.", "tokens": ["die", "Veil\u00b7gen", "wer\u00b7den", "blas", ",", "wenn", "sie", "sich", "nach", "ihr", "wen\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAINF", "VVFIN", "$,", "KOUS", "PPER", "PRF", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ihr Hals ist heller noch als alle Lilgen sein,", "tokens": ["Ihr", "Hals", "ist", "hel\u00b7ler", "noch", "als", "al\u00b7le", "Lil\u00b7gen", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "ADV", "KOKOM", "PIAT", "NN", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "und ihre Brust sticht hin der Anemonen Schein.", "tokens": ["und", "ih\u00b7re", "Brust", "sticht", "hin", "der", "A\u00b7ne\u00b7mo\u00b7nen", "Schein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ADV", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ihr s\u00fc\u00dfer Odem reucht wie starke Bisemblumen,", "tokens": ["Ihr", "s\u00fc\u00b7\u00dfer", "O\u00b7dem", "reucht", "wie", "star\u00b7ke", "Bi\u00b7sem\u00b7blu\u00b7men", ","], "toke