{"textgrid.poem.59467": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Der verirrte J\u00e4ger", "genre": "verse", "period": "N.A.", "pub_year": 1812, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbich hab gesehn ein Hirschlein schlank", "tokens": ["\u00bb", "ich", "hab", "ge\u00b7sehn", "ein", "Hirsc\u00b7hlein", "schlank"], "token_info": ["punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "PPER", "VAFIN", "VVPP", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im Waldesgrunde stehn,", "tokens": ["Im", "Wal\u00b7des\u00b7grun\u00b7de", "stehn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nun ist mir drau\u00dfen weh und bang,", "tokens": ["Nun", "ist", "mir", "drau\u00b7\u00dfen", "weh", "und", "bang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "PTKVZ", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mu\u00df ewig nach ihm gehn.", "tokens": ["Mu\u00df", "e\u00b7wig", "nach", "ihm", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADJD", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Frischauf, ihr Waldgesellen mein!", "tokens": ["Frisc\u00b7hauf", ",", "ihr", "Wald\u00b7ge\u00b7sel\u00b7len", "mein", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPOSAT", "NN", "PPOSAT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ins Horn, ins Horn frischauf!", "tokens": ["Ins", "Horn", ",", "ins", "Horn", "frisc\u00b7hauf", "!"], "token_info": ["word", "word"