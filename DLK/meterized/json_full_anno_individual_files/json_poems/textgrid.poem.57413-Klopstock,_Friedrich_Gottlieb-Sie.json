{"textgrid.poem.57413": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "Sie", "genre": "verse", "period": "N.A.", "pub_year": 1797, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Freude, wem gleichst du? Umsonst streb ich zu w\u00e4hlen. Du bist", "tokens": ["Freu\u00b7de", ",", "wem", "gleichst", "du", "?", "Um\u00b7sonst", "streb", "ich", "zu", "w\u00e4h\u00b7len", ".", "Du", "bist"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "$,", "PWS", "VVFIN", "PPER", "$.", "ADV", "VVFIN", "PPER", "PTKZU", "VVINF", "$.", "PPER", "VAFIN"], "meter": "+-+-+-+-+-+--+", "measure": "iambic.septa.chol"}, "line.2": {"text": "Allem, was sch\u00f6ner ist, gleich, allem, das hoch", "tokens": ["Al\u00b7lem", ",", "was", "sch\u00f6\u00b7ner", "ist", ",", "gleich", ",", "al\u00b7lem", ",", "das", "hoch"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word"], "pos": ["PIS", "$,", "PWS", "ADJA", "VAFIN", "$,", "ADV", "$,", "PIS", "$,", "PRELS", "ADJD"], "meter": "---+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Sich erhebet, allem, was ganz", "tokens": ["Sich", "er\u00b7he\u00b7bet", ",", "al\u00b7lem", ",", "was", "ganz"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PRF", "VVFIN", "$,", "PIS", "$,", "PRELS", "ADV"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "R\u00fchret das Herz.", "tokens": ["R\u00fch\u00b7ret", "das", "Herz", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "O, sie kennen dich nicht! Wissen sie, da\u00df du nicht kommst,", "tokens": ["O", ",", "sie", "ken\u00b7nen", "dich", "nicht", "!", "Wis\u00b7sen", "sie", ",", "da\u00df", "du", "nicht", "kommst", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "VVFIN", "PPER", "PTKNEG", "$.", "VVFIN", "PPER", "$,", "KOUS", "PPER", "PTKNEG", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.2": {"text": "Wenn sie dir rufen? da\u00df du, Freieste du,", "tokens": ["Wenn", "sie", "dir", "ru\u00b7fen", "?", "da\u00df", "du", ",", "Frei\u00b7es\u00b7te", "du", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVINF", "$.", "KOUS", "PPER", "$,", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Sie, wenn zu zwingen sie w\u00e4hnen, verlachst,", "tokens": ["Sie", ",", "wenn", "zu", "zwin\u00b7gen", "sie", "w\u00e4h\u00b7nen", ",", "ver\u00b7lachst", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "$,", "KOUS", "PTKZU", "VVINF", "PPER", "VVINF", "$,", "VVFIN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Fliehend verlachst?", "tokens": ["Flie\u00b7hend", "ver\u00b7lachst", "?"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "Freieste, aber du bist F\u00fchlenden, Redlichen hold,", "tokens": ["Frei\u00b7es\u00b7te", ",", "a\u00b7ber", "du", "bist", "F\u00fch\u00b7len\u00b7den", ",", "Red\u00b7li\u00b7chen", "hold", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "PPER", "VAFIN", "NN", "$,", "NN", "ADJD", "$,"], "meter": "+--+-+-+--+--+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "L\u00e4chelst ihnen. Du labst dann wie der West,", "tokens": ["L\u00e4\u00b7chelst", "ih\u00b7nen", ".", "Du", "labst", "dann", "wie", "der", "West", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$.", "PPER", "VVFIN", "ADV", "KOKOM", "ART", "NN", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Bl\u00fchest wie Rosen, welche mit Moos", "tokens": ["Bl\u00fc\u00b7hest", "wie", "Ro\u00b7sen", ",", "wel\u00b7che", "mit", "Moos"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "KOKOM", "NN", "$,", "PRELS", "APPR", "NN"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "G\u00fcrten ihr Blatt,", "tokens": ["G\u00fcr\u00b7ten", "ihr", "Blatt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.4": {"line.1": {"text": "Gl\u00fchst von der Lerche Glut, hebt sie gen Himmel sich, weinst,", "tokens": ["Gl\u00fchst", "von", "der", "Ler\u00b7che", "Glut", ",", "hebt", "sie", "gen", "Him\u00b7mel", "sich", ",", "weinst", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "NN", "$,", "VVFIN", "PPER", "APPR", "NN", "PRF", "$,", "VVFIN", "$,"], "meter": "+--+-+-+-+--+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Wie die gekr\u00e4nzete Braut, wie, wenn den Sohn,", "tokens": ["Wie", "die", "ge\u00b7kr\u00e4n\u00b7ze\u00b7te", "Braut", ",", "wie", ",", "wenn", "den", "Sohn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "$,", "PWAV", "$,", "KOUS", "ART", "NN", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Junge Mutter nunmehr, sie umarmt,", "tokens": ["Jun\u00b7ge", "Mut\u00b7ter", "nun\u00b7mehr", ",", "sie", "um\u00b7armt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "$,", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Dr\u00fcckt an ihr Herz.", "tokens": ["Dr\u00fcckt", "an", "ihr", "Herz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Aber du weinest auch, wenn mit der Wehmut du dich", "tokens": ["A\u00b7ber", "du", "wei\u00b7nest", "auch", ",", "wenn", "mit", "der", "Weh\u00b7mut", "du", "dich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ADV", "$,", "KOUS", "APPR", "ART", "NN", "PPER", "PRF"], "meter": "+--+-+-+-+--+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Einst und der Tr\u00f6stung. Besucht oft sie, ihr drei,", "tokens": ["Einst", "und", "der", "Tr\u00f6s\u00b7tung", ".", "Be\u00b7sucht", "oft", "sie", ",", "ihr", "drei", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "KON", "ART", "NN", "$.", "NN", "ADV", "PPER", "$,", "PPOSAT", "CARD", "$,"], "meter": "---+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Denen ihr liebe Gespielinnen seid,", "tokens": ["De\u00b7nen", "ihr", "lie\u00b7be", "Ge\u00b7spie\u00b7lin\u00b7nen", "seid", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "ADJA", "NN", "VAFIN", "$,"], "meter": "+--+---+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Grazien seid!", "tokens": ["Gra\u00b7zi\u00b7en", "seid", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VAFIN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.6": {"line.1": {"text": "F