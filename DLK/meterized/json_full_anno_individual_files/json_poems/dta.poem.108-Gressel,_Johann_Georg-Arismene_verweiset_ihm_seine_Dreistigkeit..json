{"dta.poem.108": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Arismene  verweiset ihm seine Dreistigkeit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Nicht zu dreiste mein Lysander!", "tokens": ["Nicht", "zu", "dreis\u00b7te", "mein", "Ly\u00b7san\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PTKZU", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "K\u00fc\u00dft die Br\u00fcste/ so euch frey/", "tokens": ["K\u00fc\u00dft", "die", "Br\u00fcs\u00b7te", "/", "so", "euch", "frey", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$(", "ADV", "PPER", "ADJD", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bleibt bey diesen/ la\u00dft das Ander/", "tokens": ["Bleibt", "bey", "die\u00b7sen", "/", "la\u00dft", "das", "An\u00b7der", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PDAT", "$(", "VVIMP", "ART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dencket/ da\u00df es heilig sey.", "tokens": ["Den\u00b7cket", "/", "da\u00df", "es", "hei\u00b7lig", "sey", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$(", "KOUS", "PPER", "ADJD", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "La\u00dft die geilen Griffe bleiben/", "tokens": ["La\u00dft", "die", "gei\u00b7len", "Grif\u00b7fe", "blei\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Kr\u00f6net lieber meine Brust/", "tokens": ["Kr\u00f6\u00b7net", "lie\u00b7ber", "mei\u00b7ne", "Brust", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und last eure Finger treiben", "tokens": ["Und", "last", "eu\u00b7re", "Fin\u00b7ger", "trei\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "In dem Marmor-Meer der Lust.", "tokens": ["In", "dem", "Mar\u00b7mor\u00b7Meer", "der", "Lust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "K\u00fchlet eure heisse Finger", "tokens": ["K\u00fch\u00b7let", "eu\u00b7re", "heis\u00b7se", "Fin\u00b7ger"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "In des Busens zarten Schnee/", "tokens": ["In", "des", "Bu\u00b7sens", "zar\u00b7ten", "Schnee", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Er wird dadurch nicht geringer", "tokens": ["Er", "wird", "da\u00b7durch", "nicht", "ge\u00b7rin\u00b7ger"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PAV", "PTKNEG", "ADJD"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.12": {"text": "Sondern quillet in die H\u00f6h.", "tokens": ["Son\u00b7dern", "quil\u00b7let", "in", "die", "H\u00f6h", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "Spielet in den zarten Wellen/", "tokens": ["Spie\u00b7let", "in", "den", "zar\u00b7ten", "Wel\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "K\u00fchlt euch in dem Perlen-Thau/", "tokens": ["K\u00fchlt", "euch", "in", "dem", "Per\u00b7len\u00b7\u00b7T\u00b7hau", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "$("], "meter": "+---+--+", "measure": "iambic.tri.chol"}, "line.15": {"text": "K\u00fc\u00dft der Wollust Lager-Stellen", "tokens": ["K\u00fc\u00dft", "der", "Wol\u00b7lust", "La\u00b7ger\u00b7Stel\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Auf der Br\u00fcste Rosen-Au.", "tokens": ["Auf", "der", "Br\u00fcs\u00b7te", "Ro\u00b7sen\u00b7Au", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.17": {"text": "Schaut Corallen auf ", "tokens": ["Schaut", "Co\u00b7ral\u00b7len", "auf"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "NN", "APPR"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.18": {"text": "Und den T\u00fcrcks in Milch gesetzt;", "tokens": ["Und", "den", "T\u00fcrcks", "in", "Milch", "ge\u00b7setzt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.19": {"text": "Schmeckt die Anmuth/ so die Liljen", "tokens": ["Schmeckt", "die", "An\u00b7muth", "/", "so", "die", "Lil\u00b7jen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "$(", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Meiner Br\u00fcste angenetzt.", "tokens": ["Mei\u00b7ner", "Br\u00fcs\u00b7te", "an\u00b7ge\u00b7netzt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.21": {"text": "Sch\u00fctzt den Eingang dieses Landes/", "tokens": ["Sch\u00fctzt", "den", "Ein\u00b7gang", "die\u00b7ses", "Lan\u00b7des", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PDAT", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "Wo Rubin auf Perlen gl\u00fcht/", "tokens": ["Wo", "Ru\u00b7bin", "auf", "Per\u00b7len", "gl\u00fcht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "APPR", "NN", "VVFIN", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.23": {"text": "Ehrt die Gegend dieses Strandes/", "tokens": ["Ehrt", "die", "Ge\u00b7gend", "die\u00b7ses", "Stran\u00b7des", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PDAT", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "So den Geist mit Lust versteht.", "tokens": ["So", "den", "Geist", "mit", "Lust", "ver\u00b7steht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.25": {"text": "Wadet in dem Marmor-Meere/", "tokens": ["Wa\u00b7det", "in", "dem", "Mar\u00b7mor\u00b7Mee\u00b7re", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.26": {"text": "Dessen Wellen milchern sind/", "tokens": ["Des\u00b7sen", "Wel\u00b7len", "mil\u00b7chern", "sind", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "ADJD", "VAFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.27": {"text": "Was ich allen hier verwehre/", "tokens": ["Was", "ich", "al\u00b7len", "hier", "ver\u00b7weh\u00b7re", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PIS", "ADV", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.28": {"text": "Wird euch nur allein geg\u00fcnnt.", "tokens": ["Wird", "euch", "nur", "al\u00b7lein", "ge\u00b7g\u00fcnnt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.29": {"text": "F\u00fcget euren Mund und Lippen", "tokens": ["F\u00fc\u00b7get", "eu\u00b7ren", "Mund", "und", "Lip\u00b7pen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.30": {"text": "Diesen Himmels-Fr\u00fcchten bey/", "tokens": ["Die\u00b7sen", "Him\u00b7mels\u00b7Fr\u00fcch\u00b7ten", "bey", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "NN", "APPR", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.31": {"text": "Suchet von den Perlen-Klippen", "tokens": ["Su\u00b7chet", "von", "den", "Per\u00b7len\u00b7Klip\u00b7pen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.32": {"text": "Etwas/ so euch Zucker sey.", "tokens": ["Et\u00b7was", "/", "so", "euch", "Zu\u00b7cker", "sey", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "ADV", "PPER", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.33": {"text": "Sauget als die zarten Bienen", "tokens": ["Sau\u00b7get", "als", "die", "zar\u00b7ten", "Bie\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "KOKOM", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.34": {"text": "Honig aus dem Tausend-Sch\u00f6n", "tokens": ["Ho\u00b7nig", "aus", "dem", "Tau\u00b7sen\u00b7dSch\u00f6n"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.35": {"text": "Jener bl\u00fchenden ", "tokens": ["Je\u00b7ner", "bl\u00fc\u00b7hen\u00b7den"], "token_info": ["word", "word"], "pos": ["PDAT", "ADJA"], "meter": "+-+--", "measure": "unknown.measure.di"}, "line.36": {"text": "Die auf meinen H\u00fcgeln stehn.", "tokens": ["Die", "auf", "mei\u00b7nen", "H\u00fc\u00b7geln", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.37": {"text": "Kr\u00f6net/ k\u00fcsset diese Ballen/", "tokens": ["Kr\u00f6\u00b7net", "/", "k\u00fcs\u00b7set", "die\u00b7se", "Bal\u00b7len", "/"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$(", "VVFIN", "PDAT", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.38": {"text": "So die Anmuth T\u00f6chter nennt/", "tokens": ["So", "die", "An\u00b7muth", "T\u00f6ch\u00b7ter", "nennt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "NN", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.39": {"text": "La\u00dft euch ihre Pracht gefallen/", "tokens": ["La\u00dft", "euch", "ih\u00b7re", "Pracht", "ge\u00b7fal\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPOSAT", "NN", "VVPP", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.40": {"text": "Davon sich die Lust nie trennt.", "tokens": ["Da\u00b7von", "sich", "die", "Lust", "nie", "trennt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PRF", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.41": {"text": "Wartet diese Lust-Granaten/", "tokens": ["War\u00b7tet", "die\u00b7se", "Lust\u00b7Gra\u00b7na\u00b7ten", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PDAT", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.42": {"text": "Lacht die ", "tokens": ["Lacht", "die"], "token_info": ["word", "word"], "pos": ["VVFIN", "ART"], "meter": "+-", "measure": "trochaic.single"}, "line.43": {"text": "Wie ihr Schmuck so wohl gerahten/", "tokens": ["Wie", "ihr", "Schmuck", "so", "wohl", "ge\u00b7rah\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "ADV", "ADV", "VVPP", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.44": {"text": "Da\u00df er Todten helffen kan.", "tokens": ["Da\u00df", "er", "Tod\u00b7ten", "helf\u00b7fen", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.45": {"text": "Bleibt von dem verbohtnen Lande/", "tokens": ["Bleibt", "von", "dem", "ver\u00b7boht\u00b7nen", "Lan\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.46": {"text": "In dasselbe kommt ihr nicht/", "tokens": ["In", "das\u00b7sel\u00b7be", "kommt", "ihr", "nicht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "VVFIN", "PPER", "PTKNEG", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.47": {"text": "Wohnet bey dem sch\u00f6nen Strande", "tokens": ["Woh\u