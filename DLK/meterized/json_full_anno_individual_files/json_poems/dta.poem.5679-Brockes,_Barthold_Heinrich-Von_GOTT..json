{"dta.poem.5679": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von GOTT.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nein, nein, es fusset sich von GOTT der Unterricht", "tokens": ["Nein", ",", "nein", ",", "es", "fus\u00b7set", "sich", "von", "GoTT", "der", "Un\u00b7ter\u00b7richt"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "PTKANT", "$,", "PPER", "VVFIN", "PRF", "APPR", "NE", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Auf menschlichen Betr\u00fcgereyen nicht.", "tokens": ["Auf", "menschli\u00b7chen", "Be\u00b7tr\u00fc\u00b7ge\u00b7re\u00b7yen", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKNEG", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Die Gr\u00fcnde sind wahrhafftig. Es entstehen,", "tokens": ["Die", "Gr\u00fcn\u00b7de", "sind", "wahr\u00b7haff\u00b7tig", ".", "Es", "ent\u00b7ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$.", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Aus einem Nichts, nie w\u00fcrckliche Jdeen.", "tokens": ["Aus", "ei\u00b7nem", "Nichts", ",", "nie", "w\u00fcrck\u00b7li\u00b7che", "Jdeen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Wir k\u00f6nnen von dem ", "tokens": ["Wir", "k\u00f6n\u00b7nen", "von", "dem"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "ART"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "In seinem Urstand selbst, der ewig war, die Schrifften", "tokens": ["In", "sei\u00b7nem", "Ur\u00b7stand", "selbst", ",", "der", "e\u00b7wig", "war", ",", "die", "Schriff\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "$,", "PRELS", "ADJD", "VAFIN", "$,", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Unwidersprechlich lesen.", "tokens": ["Un\u00b7wi\u00b7der\u00b7sprech\u00b7lich", "le\u00b7sen", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Nur GOTT allein", "tokens": ["Nur", "GoTT", "al\u00b7lein"], "token_info": ["word", "word", "word"], "pos": ["ADV", "NE", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "K\u00f6nnt\u2019 uns von ", "tokens": ["K\u00f6nnt'", "uns", "von"], "token_info": ["word", "word", "word"], "pos": ["VMFIN", "PPER", "APPR"], "meter": "+-+", "measure": "trochaic.di"}, "line.10": {"text": "Die Nachricht selber g\u00f6nnen.", "tokens": ["Die", "Nach\u00b7richt", "sel\u00b7ber", "g\u00f6n\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}