{"dta.poem.15666": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "99.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Welch Ungl\u00fcck, weder recht zu wachen noch zu tr\u00e4umen,", "tokens": ["Welch", "Un\u00b7gl\u00fcck", ",", "we\u00b7der", "recht", "zu", "wa\u00b7chen", "noch", "zu", "tr\u00e4u\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "KON", "ADJD", "PTKZU", "VVINF", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Auf Erden nicht zu Haus noch auch in Himmelsr\u00e4umen.", "tokens": ["Auf", "Er\u00b7den", "nicht", "zu", "Haus", "noch", "auch", "in", "Him\u00b7mels\u00b7r\u00e4u\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKNEG", "APPR", "NN", "ADV", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Im Schlaf zu wachen und zu wandeln, kann dir taugen", "tokens": ["Im", "Schlaf", "zu", "wa\u00b7chen", "und", "zu", "wan\u00b7deln", ",", "kann", "dir", "tau\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "PTKZU", "VVINF", "KON", "PTKZU", "VVFIN", "$,", "VMFIN", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sowenig als ein Schlaf mit halbwach offnen Augen.", "tokens": ["So\u00b7we\u00b7nig", "als", "ein", "Schlaf", "mit", "halb\u00b7wach", "off\u00b7nen", "Au\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "ART", "NN", "APPR", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-++--+-", "measure": "iambic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "Abwechselnd m\u00fcssen Schlaf und Wachen sich erfrischen,", "tokens": ["Ab\u00b7wech\u00b7selnd", "m\u00fcs\u00b7sen", "Schlaf", "und", "Wa\u00b7chen", "sich", "er\u00b7fri\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "KON", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nicht lassen sich die zwei wie Wein und Wasser mischen.", "tokens": ["Nicht", "las\u00b7sen", "sich", "die", "zwei", "wie", "Wein", "und", "Was\u00b7ser", "mi\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "PRF", "ART", "CARD", "KOKOM", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Nicht gatten k\u00f6nnen sich die zwei wie Licht und Schatten,", "tokens": ["Nicht", "gat\u00b7ten", "k\u00f6n\u00b7nen", "sich", "die", "zwei", "wie", "Licht", "und", "Schat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVINF", "VMFIN", "PRF", "ART", "CARD", "KOKOM", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ohn' unerquicklich eins am andern zu ermatten.", "tokens": ["Ohn'", "un\u00b7er\u00b7quick\u00b7lich", "eins", "am", "an\u00b7dern", "zu", "er\u00b7mat\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PIS", "APPRART", "PIS", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Die D\u00e4mmerung ist sch\u00f6n, doch nur als Uebergang,", "tokens": ["Die", "D\u00e4m\u00b7me\u00b7rung", "ist", "sch\u00f6n", ",", "doch", "nur", "als", "Ue\u00b7ber\u00b7gang", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "ADV", "ADV", "KOUS", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ob aus ihr Sternennacht, ob Sonnentag entsprang.", "tokens": ["Ob", "aus", "ihr", "Ster\u00b7nen\u00b7nacht", ",", "ob", "Son\u00b7nen\u00b7tag", "ent\u00b7sprang", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PPOSAT", "NN", "$,", "KOUS", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "So zwischen Wachen auch und zwischen Schlafen liegt", "tokens": ["So", "zwi\u00b7schen", "Wa\u00b7chen", "auch", "und", "zwi\u00b7schen", "Schla\u00b7fen", "liegt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "NN", "ADV", "KON", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein sch\u00f6ner Augenblick, sch\u00f6n weil er schnell entfliegt;", "tokens": ["Ein", "sch\u00f6\u00b7ner", "Au\u00b7gen\u00b7blick", ",", "sch\u00f6n", "weil", "er", "schnell", "ent\u00b7fliegt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADJD", "KOUS", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Wo Seele B\u00fcrgerin sich f\u00fchlet zweier Welten,", "tokens": ["Wo", "See\u00b7le", "B\u00fcr\u00b7ge\u00b7rin", "sich", "f\u00fch\u00b7let", "zwei\u00b7er", "Wel\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "NN", "PRF", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und in dem Augenblick vergleicht, was beide gelten.", "tokens": ["Und", "in", "dem", "Au\u00b7gen\u00b7blick", "ver\u00b7gleicht", ",", "was", "bei\u00b7de", "gel\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "$,", "PRELS", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}