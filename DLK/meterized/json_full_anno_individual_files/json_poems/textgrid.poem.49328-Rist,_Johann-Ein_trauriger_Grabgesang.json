": "Zur jeden Frist,", "tokens": ["Zur", "je\u00b7den", "Frist", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "PIAT", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.33": {"text": "Der dieses recht bedenket,", "tokens": ["Der", "die\u00b7ses", "recht", "be\u00b7den\u00b7ket", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PDAT", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.34": {"text": "Wie der Herr der Herlichkeit", "tokens": ["Wie", "der", "Herr", "der", "Her\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.35": {"text": "Wird ins Grab versenket.", "tokens": ["Wird", "ins", "Grab", "ver\u00b7sen\u00b7ket", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.36": {"text": "O Jesu, du", "tokens": ["O", "Je\u00b7su", ",", "du"], "token_info": ["word", "word", "punct", "word"], "pos": ["NE", "NE", "$,", "PPER"], "meter": "+-+-", "measure": "trochaic.di"}, "line.37": {"text": "Mein' H\u00fclf und Ruh',", "tokens": ["Mein'", "H\u00fclf", "und", "Ruh'", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.38": {"text": "Ich bitte dich mit Thr\u00e4nen:", "tokens": ["Ich", "bit\u00b7te", "dich", "mit", "Thr\u00e4\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.39": {"text": "Hilf, da\u00df ich mich bis ins Grab", "tokens": ["Hilf", ",", "da\u00df", "ich", "mich", "bis", "ins", "Grab"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "KOUS", 