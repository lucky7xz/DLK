{"dta.poem.9839": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Kunst gehet \u00fcber kleider-pracht.  \n S. D.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mein kind/ dich m\u00fcssen leute lieben/", "tokens": ["Mein", "kind", "/", "dich", "m\u00fcs\u00b7sen", "leu\u00b7te", "lie\u00b7ben", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "PPER", "VMFIN", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vor welchen ich ein schatten bin;", "tokens": ["Vor", "wel\u00b7chen", "ich", "ein", "schat\u00b7ten", "bin", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "PPER", "ART", "ADJA", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Drum wundert mich es/ da\u00df dein sinn", "tokens": ["Drum", "wun\u00b7dert", "mich", "es", "/", "da\u00df", "dein", "sinn"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "PPER", "$(", "KOUS", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zu meiner einfalt wird getrieben:", "tokens": ["Zu", "mei\u00b7ner", "ein\u00b7falt", "wird", "ge\u00b7trie\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Es pfleget ietzt ja zu geschehn/", "tokens": ["Es", "pfle\u00b7get", "ietzt", "ja", "zu", "ge\u00b7schehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df alle nur auff hochzeit sehn.", "tokens": ["Da\u00df", "al\u00b7le", "nur", "auff", "hoch\u00b7zeit", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich wei\u00df mich so nicht auszuputzen/", "tokens": ["Ich", "wei\u00df", "mich", "so", "nicht", "aus\u00b7zu\u00b7put\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie itzt die geile jugend thut/", "tokens": ["Wie", "itzt", "die", "gei\u00b7le", "ju\u00b7gend", "thut", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und die ihr v\u00e4terliches gut", "tokens": ["Und", "die", "ihr", "v\u00e4\u00b7ter\u00b7li\u00b7ches", "gut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "PPOSAT", "ADJA", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Jm halben jahr offt gantz verstutzen;", "tokens": ["Jm", "hal\u00b7ben", "jahr", "offt", "gantz", "ver\u00b7stut\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Was hoch und \u00fcber stands-geb\u00fchr/", "tokens": ["Was", "hoch", "und", "\u00fc\u00b7ber", "stands\u00b7ge\u00b7b\u00fchr", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "KON", "APPR", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da eckelt meiner seelen f\u00fcr.", "tokens": ["Da", "ec\u00b7kelt", "mei\u00b7ner", "see\u00b7len", "f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "APPR", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Wie schlecht ich auch herein mag gehen/", "tokens": ["Wie", "schlecht", "ich", "auch", "her\u00b7ein", "mag", "ge\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "PTKVZ", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So sch\u00e4mest du dennoch/ mein licht/", "tokens": ["So", "sch\u00e4\u00b7mest", "du", "den\u00b7noch", "/", "mein", "licht", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$(", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dich nimmer meiner liebe nicht;", "tokens": ["Dich", "nim\u00b7mer", "mei\u00b7ner", "lie\u00b7be", "nicht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PPOSAT", "VVFIN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Du darffst es offentlich gestehen", "tokens": ["Du", "darffst", "es", "of\u00b7fent\u00b7lich", "ge\u00b7ste\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und sagst/ durch keines zwang und trieb:", "tokens": ["Und", "sagst", "/", "durch", "kei\u00b7nes", "zwang", "und", "trieb", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$(", "APPR", "PIS", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ja ja/ mein kind/ ich hab euch lieb.", "tokens": ["Ja", "ja", "/", "mein", "kind", "/", "ich", "hab", "euch", "lieb", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PTKANT", "$(", "PPOSAT", "NN", "$(", "PPER", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ich hab es Venus wissen lassen/", "tokens": ["Ich", "hab", "es", "Ve\u00b7nus", "wis\u00b7sen", "las\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "NN", "VVINF", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sie hat es Amorn kund gethan/", "tokens": ["Sie", "hat", "es", "A\u00b7morn", "kund", "ge\u00b7than", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "NE", "PTKVZ", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die haben ihre lust daran/", "tokens": ["Die", "ha\u00b7ben", "ih\u00b7re", "lust", "da\u00b7ran", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "NN", "PAV", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und lieben dich auch bester massen/", "tokens": ["Und", "lie\u00b7ben", "dich", "auch", "bes\u00b7ter", "mas\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Da\u00df du/ o frommer seelen lust/", "tokens": ["Da\u00df", "du", "/", "o", "from\u00b7mer", "see\u00b7len", "lust", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$(", "FM", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So treu und redlich bey mir thust.", "tokens": ["So", "treu", "und", "red\u00b7lich", "bey", "mir", "thust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Gehabt euch wohl ihr stoltzen pfauen!", "tokens": ["Ge\u00b7habt", "euch", "wohl", "ihr", "stolt\u00b7zen", "pfau\u00b7en", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ich kenn und liebe wenig gold/", "tokens": ["Ich", "kenn", "und", "lie\u00b7be", "we\u00b7nig", "gold", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PIAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und dennoch ist mir treu und hold", "tokens": ["Und", "den\u00b7noch", "ist", "mir", "treu", "und", "hold"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die zier und crone der jungfrauen/", "tokens": ["Die", "zier", "und", "cro\u00b7ne", "der", "jung\u00b7frau\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "ART", "ADJA", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die mehr auff ein ber\u00fchmtes lied", "tokens": ["Die", "mehr", "auff", "ein", "be\u00b7r\u00fchm\u00b7tes", "lied"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Als auff verg\u00fcldte kleider sieht.", "tokens": ["Als", "auff", "ver\u00b7g\u00fcld\u00b7te", "klei\u00b7der", "sieht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}