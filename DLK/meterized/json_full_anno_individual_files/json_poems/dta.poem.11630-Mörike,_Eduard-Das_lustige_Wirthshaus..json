{"dta.poem.11630": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Das lustige Wirthshaus.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905193969", "language": ["de:0.99"], "booktitle": "M\u00f6rike, Eduard: Gedichte. Stuttgart, 1838."}, "poem": {"stanza.1": {"line.1": {"text": "Denn ich und des Sch\u00e4fers sein lustiger Franz,", "tokens": ["Denn", "ich", "und", "des", "Sch\u00e4\u00b7fers", "sein", "lus\u00b7ti\u00b7ger", "Franz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "KON", "ART", "NN", "PPOSAT", "ADJA", "NE", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Wir ziehn wie die V\u00f6gel so frei,", "tokens": ["Wir", "ziehn", "wie", "die", "V\u00f6\u00b7gel", "so", "frei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Ich spiele die Zither, das Hackbrett zum Tanz,", "tokens": ["Ich", "spie\u00b7le", "die", "Zi\u00b7ther", ",", "das", "Hack\u00b7brett", "zum", "Tanz", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Mein Liebster, der spielt die Schalmei.", "tokens": ["Mein", "Liebs\u00b7ter", ",", "der", "spielt", "die", "Schal\u00b7mei", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "VVFIN", "ART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}}}}