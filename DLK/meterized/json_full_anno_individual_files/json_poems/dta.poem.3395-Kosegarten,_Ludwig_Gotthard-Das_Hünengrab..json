{"dta.poem.3395": {"metadata": {"author": {"name": "Kosegarten, Ludwig Gotthard", "birth": "N.A.", "death": "N.A."}, "title": "Das H\u00fcnengrab.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1798", "urn": "urn:nbn:de:kobv:b4-200905193292", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Nacht ist heilig und hehr.", "tokens": ["Die", "Nacht", "ist", "hei\u00b7lig", "und", "hehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "D\u00e4mmernd und schauernd und ahnend ist die", "tokens": ["D\u00e4m\u00b7mernd", "und", "schau\u00b7ernd", "und", "ah\u00b7nend", "ist", "die"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVPP", "KON", "ADJD", "KON", "ADJD", "VAFIN", "ART"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.3": {"text": "Nacht.", "tokens": ["Nacht", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Im Mondlicht woget die d\u00fcstere Waldung,", "tokens": ["Im", "Mond\u00b7licht", "wo\u00b7get", "die", "d\u00fcs\u00b7te\u00b7re", "Wal\u00b7dung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Im Mondlicht die Saat den H\u00fcgel hinan.", "tokens": ["Im", "Mond\u00b7licht", "die", "Saat", "den", "H\u00fc\u00b7gel", "hi\u00b7nan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Wie die Unken l\u00e4uten im Teich!", "tokens": ["Wie", "die", "Un\u00b7ken", "l\u00e4u\u00b7ten", "im", "Teich", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Wie die Nachtigallen den Busch durchfl\u00f6ten!", "tokens": ["Wie", "die", "Nach\u00b7ti\u00b7gal\u00b7len", "den", "Busch", "durch\u00b7fl\u00f6\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.3": {"text": "Liebliche K\u00fchle durchwehet die L\u00fcfte.", "tokens": ["Lieb\u00b7li\u00b7che", "K\u00fch\u00b7le", "durch\u00b7we\u00b7het", "die", "L\u00fcf\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.4": {"text": "Die Maynacht ist duftig und thauig", "tokens": ["Die", "May\u00b7nacht", "ist", "duf\u00b7tig", "und", "thau\u00b7ig"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADJD", "KON", "ADJD"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "und hehr!", "tokens": ["und", "hehr", "!"], "token_info": ["word", "word", "punct"], "pos": ["KON", "ADJD", "$."], "meter": "-+", "measure": "iambic.single"}}, "stanza.3": {"line.1": {"text": "Schweigt Nachtigallen! Unken schweigt!", "tokens": ["Schweigt", "Nach\u00b7ti\u00b7gal\u00b7len", "!", "Un\u00b7ken", "schweigt", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "NN", "$.", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Schauererinnerung umflistert mich.", "tokens": ["Schau\u00b7e\u00b7re\u00b7rin\u00b7ne\u00b7rung", "um\u00b7flis\u00b7tert", "mich", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Zwischen vier bemooseten Steinen,", "tokens": ["Zwi\u00b7schen", "vier", "be\u00b7moo\u00b7se\u00b7ten", "Stei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "ADJA", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Unter drey rauschenden Eichen sitz' ich", "tokens": ["Un\u00b7ter", "drey", "rau\u00b7schen\u00b7den", "Ei\u00b7chen", "sitz'", "ich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "CARD", "ADJA", "NN", "VVFIN", "PPER"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.5": {"text": "hier.", "tokens": ["hier", "."], "token_info": ["word", "punct"], "pos": ["ADV", "$."], "meter": "-", "measure": "single.down"}}, "stanza.4": {"line.1": {"text": "\u00dcber die vier moosbewachsnen Steine,", "tokens": ["\u00dc\u00b7ber", "die", "vier", "moos\u00b7be\u00b7wachs\u00b7nen", "Stei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "CARD", "ADJA", "NN", "$,"], "meter": "+--++-+-+-", "measure": "dactylic.init"}, "line.2": {"text": "\u00dcber die drey rauschenden Eichen Fried' und Ruh!", "tokens": ["\u00dc\u00b7ber", "die", "drey", "rau\u00b7schen\u00b7den", "Ei\u00b7chen", "Fried'", "und", "Ruh", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "CARD", "ADJA", "NN", "NE", "KON", "NN", "$."], "meter": "+---+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Die ihr schlummert drunten, Helden, Herr-", "tokens": ["Die", "ihr", "schlum\u00b7mert", "drun\u00b7ten", ",", "Hel\u00b7den", ",", "Herr"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["ART", "PPER", "VVFIN", "ADV", "$,", "NN", "$,", "TRUNC"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "liche,", "tokens": ["li\u00b7che", ","], "token_info": ["word", "punct"], "pos": ["ADJA", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Schlummert sanft, die ihr sanket in der", "tokens": ["Schlum\u00b7mert", "sanft", ",", "die", "ihr", "san\u00b7ket", "in", "der"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "$,", "PRELS", "PPER", "VVFIN", "APPR", "ART"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "blutigen Schlacht!", "tokens": ["blu\u00b7ti\u00b7gen", "Schlacht", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.5": {"line.1": {"text": "Die Schlucht br\u00fcllte,", "tokens": ["Die", "Schlucht", "br\u00fcll\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-++-", "measure": "unknown.measure.di"}, "line.2": {"text": "Der Wald brauste,", "tokens": ["Der", "Wald", "braus\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+--", "measure": "dactylic.init"}, "line.3": {"text": "Das Meer tosete dumpf auf,", "tokens": ["Das", "Meer", "to\u00b7se\u00b7te", "dumpf", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Als die Herrlichen fielen.", "tokens": ["Als", "die", "Herr\u00b7li\u00b7chen", "fie\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.6": {"line.1": {"text": "Sie fielen. Die Feinde frohlockten.", "tokens": ["Sie", "fie\u00b7len", ".", "Die", "Fein\u00b7de", "froh\u00b7lock\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "ART", "NN", "VVFIN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Verlassen weinten die Br\u00e4ute.", "tokens": ["Ver\u00b7las\u00b7sen", "wein\u00b7ten", "die", "Br\u00e4u\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Die Barden klagten. Die \u00dcbrigbliebnen", "tokens": ["Die", "Bar\u00b7den", "klag\u00b7ten", ".", "Die", "\u00dcb\u00b7rig\u00b7blieb\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$.", "ART", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Th\u00fcrmten das ehrekr\u00f6nende Mahl.", "tokens": ["Th\u00fcrm\u00b7ten", "das", "eh\u00b7re\u00b7kr\u00f6\u00b7nen\u00b7de", "Mahl", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}}, "stanza.7": {"line.1": {"text": "Schlaft sanft, ihr Edelgefallnen.", "tokens": ["Schlaft", "sanft", ",", "ihr", "E\u00b7del\u00b7ge\u00b7fall\u00b7nen", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ADJD", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Schlaft sanft im Ringe der Steine.", "tokens": ["Schlaft", "sanft", "im", "Rin\u00b7ge", "der", "Stei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Schlaft sanft \u2014 oder steigt herauf", "tokens": ["Schlaft", "sanft", "o\u00b7der", "steigt", "her\u00b7auf"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ADJD", "$(", "KON", "VVFIN", "PTKVZ"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Mit der benarbeten Stirn, mit dem blut-", "tokens": ["Mit", "der", "be\u00b7nar\u00b7be\u00b7ten", "Stirn", ",", "mit", "dem", "blut"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "APPR", "ART", "TRUNC"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "bestr\u00f6meten Busen.", "tokens": ["be\u00b7str\u00f6\u00b7me\u00b7ten", "Bu\u00b7sen", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.8": {"line.1": {"text": "Steigt herauf, und reicht mir die Hand", "tokens": ["Steigt", "her\u00b7auf", ",", "und", "reicht", "mir", "die", "Hand"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PTKVZ", "$,", "KON", "VVFIN", "PPER", "ART", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Voll Schwielen f\u00fcr die Freyheit, die ich liebe,", "tokens": ["Voll", "Schwie\u00b7len", "f\u00fcr", "die", "Frey\u00b7heit", ",", "die", "ich", "lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "APPR", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "wie ihr;", "tokens": ["wie", "ihr", ";"], "token_info": ["word", "word", "punct"], "pos": ["PWAV", "PPER", "$."], "meter": "++", "measure": "spondeus"}, "line.4": {"text": "Ich, eurer Enkel einer,", "tokens": ["Ich", ",", "eu\u00b7rer", "En\u00b7kel", "ei\u00b7ner", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PPOSAT", "NN", "ART", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der Sp\u00e4teren, der Schw\u00e4cheren einer!", "tokens": ["Der", "Sp\u00e4\u00b7te\u00b7ren", ",", "der", "Schw\u00e4\u00b7che\u00b7ren", "ei\u00b7ner", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "ART", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.9": {"line.1": {"text": "Die Heldenzeiten sind vor\u00fcber,", "tokens": ["Die", "Hel\u00b7den\u00b7zei\u00b7ten", "sind", "vor\u00b7\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vertreten die Spuren der Ahnentugend,", "tokens": ["Ver\u00b7tre\u00b7ten", "die", "Spu\u00b7ren", "der", "Ah\u00b7nen\u00b7tu\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Verst\u00fcrme der Freyheit Donnerrufe,", "tokens": ["Ver\u00b7st\u00fcr\u00b7me", "der", "Frey\u00b7heit", "Don\u00b7ner\u00b7ru\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Verbr\u00fcllt die Schlachten f\u00fcr das Vater-", "tokens": ["Ver\u00b7br\u00fcllt", "die", "Schlach\u00b7ten", "f\u00fcr", "das", "Va\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "land.", "tokens": ["land", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "-", "measure": "single.down"}}, "stanza.10": {"line.1": {"text": "Knechtschaft umklirrt", "tokens": ["Knecht\u00b7schaft", "um\u00b7klirrt"], "token_info": ["word", "word"], "pos": ["NN", "VVFIN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Die S\u00f6hne der Freyen", "tokens": ["Die", "S\u00f6h\u00b7ne", "der", "Frey\u00b7en"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Striemen der Despotengeissel", "tokens": ["Strie\u00b7men", "der", "Des\u00b7po\u00b7ten\u00b7geis\u00b7sel"], "token_info": ["word", "word", "word"], "pos": ["NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Brandmalen den R\u00fccken der Heldens\u00f6hne.", "tokens": ["Brand\u00b7ma\u00b7len", "den", "R\u00fc\u00b7cken", "der", "Hel\u00b7den\u00b7s\u00f6h\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ART", "NN", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}}, "stanza.11": {"line.1": {"text": "Wo ist Biedersitte?", "tokens": ["Wo", "ist", "Bie\u00b7der\u00b7sit\u00b7te", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Wo sind M\u00e4dchenbl\u00f6de und J\u00fcnglingsscham?", "tokens": ["Wo", "sind", "M\u00e4d\u00b7chen\u00b7bl\u00f6\u00b7de", "und", "J\u00fcng\u00b7lings\u00b7scham", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "NN", "KON", "NN", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Begraben unter dem tausendj\u00e4hrigen Stein;", "tokens": ["Be\u00b7gra\u00b7ben", "un\u00b7ter", "dem", "tau\u00b7send\u00b7j\u00e4h\u00b7ri\u00b7gen", "Stein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-+--+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Begraben, oder Laut eines M\u00e4hrleins.", "tokens": ["Be\u00b7gra\u00b7ben", ",", "o\u00b7der", "Laut", "ei\u00b7nes", "M\u00e4hr\u00b7leins", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "APPR", "ART", "NN", "$."], "meter": "-+-+-+--++", "measure": "iambic.penta.relaxed"}}, "stanza.12": {"line.1": {"text": "Reiche mir, Braga, die Harfe! Reiche mir, Wodan,", "tokens": ["Rei\u00b7che", "mir", ",", "Bra\u00b7ga", ",", "die", "Har\u00b7fe", "!", "Rei\u00b7che", "mir", ",", "Wo\u00b7dan", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NE", "PPER", "$,", "NE", "$,", "ART", "NN", "$.", "NE", "PPER", "$,", "PWAV", "$,"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.2": {"text": " das Schwert!", "tokens": ["das", "Schwert", "!"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Ich f\u00fchle flammen in mir der Ahnen Tugend.", "tokens": ["Ich", "f\u00fch\u00b7le", "flam\u00b7men", "in", "mir", "der", "Ah\u00b7nen", "Tu\u00b7gend", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "VVFIN", "APPR", "PPER", "ART", "NN", "NN", "$."], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Bey den Edelgefallnen,", "tokens": ["Bey", "den", "E\u00b7del\u00b7ge\u00b7fall\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.5": {"text": "Bey der Eiche und dem Mahlstein, ich", "tokens": ["Bey", "der", "Ei\u00b7che", "und", "dem", "Mahl\u00b7stein", ",", "ich"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["APPR", "ART", "NN", "KON", "ART", "NN", "$,", "PPER"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "schw\u00f6re der Ahnentugend.", "tokens": ["schw\u00f6\u00b7re", "der", "Ah\u00b7nen\u00b7tu\u00b7gend", "."], "token_info": ["word", "word", "word", "punct"], "pos":