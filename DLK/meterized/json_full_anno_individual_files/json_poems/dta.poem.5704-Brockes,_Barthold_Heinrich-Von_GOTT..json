{"dta.poem.5704": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von GOTT.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Man hat euch ja gezeigt: er k\u00f6nne sich nicht regen.", "tokens": ["Man", "hat", "euch", "ja", "ge\u00b7zeigt", ":", "er", "k\u00f6n\u00b7ne", "sich", "nicht", "re\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PPER", "ADV", "VVPP", "$.", "PPER", "VMFIN", "PRF", "PTKNEG", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein Wesen, so kein Leib, m\u00fcss\u2019 ihn allein bewegen:", "tokens": ["Ein", "We\u00b7sen", ",", "so", "kein", "Leib", ",", "m\u00fcss'", "ihn", "al\u00b7lein", "be\u00b7we\u00b7gen", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "PIAT", "NN", "$,", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da\u00df alle Ding\u2019 aus einem Geist entstehn,", "tokens": ["Da\u00df", "al\u00b7le", "Ding'", "aus", "ei\u00b7nem", "Geist", "ent\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der einzig, und allein", "tokens": ["Der", "ein\u00b7zig", ",", "und", "al\u00b7lein"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "ADJD", "$,", "KON", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Nohtwendig m\u00fcsse seyn,", "tokens": ["Noht\u00b7wen\u00b7dig", "m\u00fcs\u00b7se", "seyn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "VMFIN", "VAINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Der alle C\u00f6rper lenckt; der alle Geister klug", "tokens": ["Der", "al\u00b7le", "C\u00f6r\u00b7per", "lenckt", ";", "der", "al\u00b7le", "Geis\u00b7ter", "klug"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "PIAT", "NN", "VVFIN", "$.", "ART", "PIAT", "NN", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und weise macht. Man wei\u00df: Er sey. Das ist genug.", "tokens": ["Und", "wei\u00b7se", "macht", ".", "Man", "wei\u00df", ":", "Er", "sey", ".", "Das", "ist", "ge\u00b7nug", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "$.", "PIS", "VVFIN", "$.", "PPER", "VAFIN", "$.", "PDS", "VAFIN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Will jemand, da\u00df wir ihm, wie GOTT w\u00fcrckt, sagen sollen?", "tokens": ["Will", "je\u00b7mand", ",", "da\u00df", "wir", "ihm", ",", "wie", "GoTT", "w\u00fcrckt", ",", "sa\u00b7gen", "sol\u00b7len", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "$,", "KOUS", "PPER", "PPER", "$,", "PWAV", "NE", "VVFIN", "$,", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Des ", "tokens": ["Des"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.10": {"text": "Durch wessen Willen denckt ein Mensch doch, da\u00df die Welt", "tokens": ["Durch", "wes\u00b7sen", "Wil\u00b7len", "denckt", "ein", "Mensch", "doch", ",", "da\u00df", "die", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "NN", "ADV", "$,", "KOUS", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Zuerst entstanden sey, was setzt man ihr", "tokens": ["Zu\u00b7erst", "ent\u00b7stan\u00b7den", "sey", ",", "was", "setzt", "man", "ihr"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVPP", "VAFIN", "$,", "PWS", "VVFIN", "PIS", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Doch sonst f\u00fcr einen Sch\u00f6pffer f\u00fcr,", "tokens": ["Doch", "sonst", "f\u00fcr", "ei\u00b7nen", "Sch\u00f6pf\u00b7fer", "f\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ART", "NN", "APPR", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Als einen GOTT, der alles in sich h\u00e4lt,", "tokens": ["Als", "ei\u00b7nen", "GoTT", ",", "der", "al\u00b7les", "in", "sich", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,", "PRELS", "PIS", "APPR", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Vor welchem nichts je war, den nichts umschr\u00e4ncken kan.", "tokens": ["Vor", "wel\u00b7chem", "nichts", "je", "war", ",", "den", "nichts", "um\u00b7schr\u00e4n\u00b7cken", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PIS", "ADV", "VAFIN", "$,", "PRELS", "PIS", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Sieht man solch einen GOTT f\u00fcr einen GOTT nicht an;", "tokens": ["Sieht", "man", "solch", "ei\u00b7nen", "GoTT", "f\u00fcr", "ei\u00b7nen", "GoTT", "nicht", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PIAT", "ART", "NN", "APPR", "ART", "NN", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "So hat ein Nichts aus Nichts hervor gebracht", "tokens": ["So", "hat", "ein", "Nichts", "aus", "Nichts", "her\u00b7vor", "ge\u00b7bracht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "PIS", "APPR", "PIS", "PTKVZ", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Der Elementen Stoff, draus alles ist gemacht.", "tokens": ["Der", "E\u00b7le\u00b7men\u00b7ten", "Stoff", ",", "draus", "al\u00b7les", "ist", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PAV", "PIS", "VAFIN", "VVPP", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.18": {"text": "Nur was unendlich ist, allein,", "tokens": ["Nur", "was", "un\u00b7end\u00b7lich", "ist", ",", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "PWS", "ADJD", "VAFIN", "$,", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Kan in sich selber alles finden.", "tokens": ["Kan", "in", "sich", "sel\u00b7ber", "al\u00b7les", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "PRF", "ADV", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Gedenckt man ausser Jhm zu fassen, zu ergr\u00fcnden", "tokens": ["Ge\u00b7denckt", "man", "aus\u00b7ser", "Jhm", "zu", "fas\u00b7sen", ",", "zu", "er\u00b7gr\u00fcn\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PIS", "APPR", "PPER", "PTKZU", "VVINF", "$,", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Woher der Stoff doch kan gekommen seyn?", "tokens": ["Wo\u00b7her", "der", "Stoff", "doch", "kan", "ge\u00b7kom\u00b7men", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADV", "VMFIN", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Er will; gleich sieht Er Seinen Willen", "tokens": ["Er", "will", ";", "gleich", "sieht", "Er", "Sei\u00b7nen", "Wil\u00b7len"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "$.", "ADV", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Den Augenblick erf\u00fcllen.", "tokens": ["Den", "Au\u00b7gen\u00b7blick", "er\u00b7f\u00fcl\u00b7len", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.24": {"text": "Man kan von allen, was wir schauen,", "tokens": ["Man", "kan", "von", "al\u00b7len", ",", "was", "wir", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "APPR", "PIAT", "$,", "PRELS", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Nichts als auf diesem Grunde bauen.", "tokens": ["Nichts", "als", "auf", "die\u00b7sem", "Grun\u00b7de", "bau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "KOKOM", "APPR", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}