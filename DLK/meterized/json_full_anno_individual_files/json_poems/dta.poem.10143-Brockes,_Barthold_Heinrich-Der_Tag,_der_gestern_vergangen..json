{"dta.poem.10143": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der Tag, der gestern vergangen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20086-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Gestern ist nicht heute mehr: es ist weg, es ist dahin.", "tokens": ["Ge\u00b7stern", "ist", "nicht", "heu\u00b7te", "mehr", ":", "es", "ist", "weg", ",", "es", "ist", "da\u00b7hin", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PTKNEG", "ADV", "ADV", "$.", "PPER", "VAFIN", "ADV", "$,", "PPER", "VAFIN", "PAV", "$."], "meter": "-+--+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Es versp\u00fchrt, empfindet, f\u00fchlet, sieht und h\u00f6ret unser", "tokens": ["Es", "ver\u00b7sp\u00fchrt", ",", "emp\u00b7fin\u00b7det", ",", "f\u00fch\u00b7let", ",", "sieht", "und", "h\u00f6\u00b7ret", "un\u00b7ser"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "KON", "VVFIN", "PPOSAT"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.3": {"text": "Sinn", "tokens": ["Sinn"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Nichts von seiner Gegenwart. ", "tokens": ["Nichts", "von", "sei\u00b7ner", "Ge\u00b7gen\u00b7wart", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "schrey,", "tokens": ["schrey", ","], "token_info": ["word", "punct"], "pos": ["ADJD", "$,"], "meter": "+", "measure": "single.up"}, "line.6": {"text": "Das im Augenblick verschwindet, auch verschwunden und", "tokens": ["Das", "im", "Au\u00b7gen\u00b7blick", "ver\u00b7schwin\u00b7det", ",", "auch", "ver\u00b7schwun\u00b7den", "und"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "APPRART", "NN", "VVPP", "$,", "ADV", "VVPP", "KON"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.7": {"text": "vorbey.", "tokens": ["vor\u00b7bey", "."], "token_info": ["word", "punct"], "pos": ["PTKVZ", "$."], "meter": "++", "measure": "spondeus"}, "line.8": {"text": "Alles gestrige Vergn\u00fcgen, Lachen, Fr\u00f6hlichkeit und Schertz", "tokens": ["Al\u00b7les", "ge\u00b7stri\u00b7ge", "Ver\u00b7gn\u00fc\u00b7gen", ",", "La\u00b7chen", ",", "Fr\u00f6h\u00b7lich\u00b7keit", "und", "Schertz"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PIAT", "ADJA", "NN", "$,", "NN", "$,", "NN", "KON", "NN"], "meter": "+--+--+-+-+-+-+", "measure": "dactylic.di.plus"}, "line.9": {"text": "Ist nunmehr ein leeres Nichts. Aber auch ein bittrer", "tokens": ["Ist", "nun\u00b7mehr", "ein", "lee\u00b7res", "Nichts", ".", "A\u00b7ber", "auch", "ein", "bit\u00b7trer"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "NN", "$.", "KON", "ADV", "ART", "ADJA"], "meter": "-+--+-++-+-+-", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "Schmertz,", "tokens": ["Schmertz", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+", "measure": "single.up"}, "line.11": {"text": "Der uns ", "tokens": ["Der", "uns"], "token_info": ["word", "word"], "pos": ["ART", "PPER"], "meter": "++", "measure": "spondeus"}, "line.12": {"text": "durchw\u00fchlet,", "tokens": ["durch\u00b7w\u00fch\u00b7let", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.13": {"text": "Hat mit ", "tokens": ["Hat", "mit"], "token_info": ["word", "word"], "pos": ["VAFIN", "APPR"], "meter": "+-", "measure": "trochaic.single"}, "line.14": {"text": "Eines Reichen fr\u00f6hlichs ", "tokens": ["Ei\u00b7nes", "Rei\u00b7chen", "fr\u00f6h\u00b7lichs"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "ADJA"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.15": {"text": "gen,", "tokens": ["gen", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "-", "measure": "single.down"}, "line.16": {"text": "Und des Armen elend ", "tokens": ["Und", "des", "Ar\u00b7men", "e\u00b7lend"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJD"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.17": {"text": "gen.", "tokens": ["gen", "."], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "-", "measure": "single.down"}, "line.18": {"text": "Beides bringt besondern Trost. Denn die kurtze Daur der", "tokens": ["Bei\u00b7des", "bringt", "be\u00b7son\u00b7dern", "Trost", ".", "Denn", "die", "kurt\u00b7ze", "Daur", "der"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "ADJA", "NN", "$.", "KON", "ART", "ADJA", "NN", "ART"], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.19": {"text": "Freuden", "tokens": ["Freu\u00b7den"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.20": {"text": "Tr\u00f6stet alle, die nicht gl\u00fccklich: Und, die Pein und Schmer-", "tokens": ["Tr\u00f6s\u00b7tet", "al\u00b7le", ",", "die", "nicht", "gl\u00fcck\u00b7lich", ":", "Und", ",", "die", "Pein", "und", "Schmer"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "$,", "PRELS", "PTKNEG", "ADJD", "$.", "KON", "$,", "ART", "NN", "KON", "TRUNC"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.21": {"text": "tzen leiden,", "tokens": ["tzen", "lei\u00b7den", ","], "token_info": ["word", "word", "punct"], "pos": ["VVINF", "VVINF", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.22": {"text": "Werden ungemein gest\u00e4rckt, wenn sie dieses \u00fcberlegen,", "tokens": ["Wer\u00b7den", "un\u00b7ge\u00b7mein", "ge\u00b7st\u00e4rckt", ",", "wenn", "sie", "die\u00b7ses", "\u00fc\u00b7berl\u00b7e\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "VVPP", "$,", "KOUS", "PPER", "PDS", "VVINF", "$,"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.23": {"text": "Und die unleugbare Wahrheit dieser Lehre wol erwegen:", "tokens": ["Und", "die", "un\u00b7leug\u00b7ba\u00b7re", "Wahr\u00b7heit", "die\u00b7ser", "Leh\u00b7re", "wol", "er\u00b7we\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "PDAT", "NN", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.24": {"text": "Indem du ", "tokens": ["In\u00b7dem", "du"], "token_info": ["word", "word"], "pos": ["KOUS", "PPER"], "meter": "+-+", "measure": "trochaic.di"}, "line.25": {"text": "Mehr f\u00fchlen kannst, noch darfst ertragen;", "tokens": ["Mehr", "f\u00fch\u00b7len", "kannst", ",", "noch", "darfst", "er\u00b7tra\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VVINF", "VMFIN", "$,", "ADV", "PAV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "So mindre Kummer und Verdru\u00df,", "tokens": ["So", "mind\u00b7re", "Kum\u00b7mer", "und", "Ver\u00b7dru\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Und kr\u00e4ncke dich nicht mehr so sehr auf Erden.", "tokens": ["Und", "kr\u00e4n\u00b7cke", "dich", "nicht", "mehr", "so", "sehr", "auf", "Er\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "ADV", "ADV", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.28": {"text": "Es wird, mit ungehemmten Flu\u00df,", "tokens": ["Es", "wird", ",", "mit", "un\u00b7ge\u00b7hemm\u00b7ten", "Flu\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Ein iedes ", "tokens": ["Ein", "ie\u00b7des"], "token_info": ["word", "word"], "pos": ["ART", "PIAT"], "meter": "-+-", "measure": "amphibrach.single"}}}}}