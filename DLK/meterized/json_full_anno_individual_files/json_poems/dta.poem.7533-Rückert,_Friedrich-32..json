{"dta.poem.7533": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "32.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Vater mit dem Sohn ist \u00fcber Feld gegangen;", "tokens": ["Der", "Va\u00b7ter", "mit", "dem", "Sohn", "ist", "\u00fc\u00b7ber", "Feld", "ge\u00b7gan\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VAFIN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sie k\u00f6nnen nachtverirrt die Heimat nicht erlangen.", "tokens": ["Sie", "k\u00f6n\u00b7nen", "nacht\u00b7ver\u00b7irrt", "die", "Hei\u00b7mat", "nicht", "er\u00b7lan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVFIN", "ART", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Nach jedem Felsen blickt der Sohn, nach jedem Baum,", "tokens": ["Nach", "je\u00b7dem", "Fel\u00b7sen", "blickt", "der", "Sohn", ",", "nach", "je\u00b7dem", "Baum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "ART", "NN", "$,", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wegweiser ihm zu seyn im weglos dunklen Raum.", "tokens": ["Weg\u00b7wei\u00b7ser", "ihm", "zu", "seyn", "im", "weg\u00b7los", "dunk\u00b7len", "Raum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PTKZU", "VAINF", "APPRART", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Der Vater aber blickt indessen nach den Sternen,", "tokens": ["Der", "Va\u00b7ter", "a\u00b7ber", "blickt", "in\u00b7des\u00b7sen", "nach", "den", "Ster\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Alsob der Erde Weg er woll' am Himmel lernen.", "tokens": ["Al\u00b7sob", "der", "Er\u00b7de", "Weg", "er", "woll'", "am", "Him\u00b7mel", "ler\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "NN", "PPER", "VMFIN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Die Felsen blieben stumm, die B\u00e4ume sagten nichts,", "tokens": ["Die", "Fel\u00b7sen", "blie\u00b7ben", "stumm", ",", "die", "B\u00e4u\u00b7me", "sag\u00b7ten", "nichts", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$,", "ART", "NN", "VVFIN", "PIS", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Sterne deuteten mit einem Streifen Lichts.", "tokens": ["Die", "Ster\u00b7ne", "deu\u00b7te\u00b7ten", "mit", "ei\u00b7nem", "Strei\u00b7fen", "Lichts", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Zur Heimat deuten sie; wohl dem, der traut den Sternen!", "tokens": ["Zur", "Hei\u00b7mat", "deu\u00b7ten", "sie", ";", "wohl", "dem", ",", "der", "traut", "den", "Ster\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "$.", "ADV", "ART", "$,", "PRELS", "VVFIN", "ART", "NN", "$."], "meter": "-+-+--++-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Den Weg der Erde kann man nur am Himmel lernen.", "tokens": ["Den", "Weg", "der", "Er\u00b7de", "kann", "man", "nur", "am", "Him\u00b7mel", "ler\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VMFIN", "PIS", "ADV", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}