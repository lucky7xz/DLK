{"dta.poem.21601": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Andacht .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Vor dem Bild der Mutter Gottes sah ich Dich, Geliebte, beten,", "tokens": ["Vor", "dem", "Bild", "der", "Mut\u00b7ter", "Got\u00b7tes", "sah", "ich", "Dich", ",", "Ge\u00b7lieb\u00b7te", ",", "be\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "NN", "VVFIN", "PPER", "PPER", "$,", "NN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Und es trieb mein volles Herz mich, leis an Dich heranzutreten,", "tokens": ["Und", "es", "trieb", "mein", "vol\u00b7les", "Herz", "mich", ",", "leis", "an", "Dich", "her\u00b7an\u00b7zu\u00b7tre\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "PPER", "$,", "ADJD", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Und ich blickte Dir in\u2019s Auge, jenes Auge zauberklar,", "tokens": ["Und", "ich", "blick\u00b7te", "Dir", "in's", "Au\u00b7ge", ",", "je\u00b7nes", "Au\u00b7ge", "zau\u00b7ber\u00b7klar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "APPRART", "NN", "$,", "PDAT", "NN", "ADJD", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Das umflort vom keuschen Glanze hei\u00dfer Andachtsthr\u00e4nen war.", "tokens": ["Das", "um\u00b7flort", "vom", "keu\u00b7schen", "Glan\u00b7ze", "hei\u00b7\u00dfer", "An\u00b7dachtst\u00b7hr\u00e4\u00b7nen", "war", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPRART", "ADJA", "NN", "ADJA", "NN", "VAFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.2": {"line.1": {"text": "O Geliebte, dieses Auge, diese jugendfrischen Wangen,", "tokens": ["O", "Ge\u00b7lieb\u00b7te", ",", "die\u00b7ses", "Au\u00b7ge", ",", "die\u00b7se", "ju\u00b7gend\u00b7fri\u00b7schen", "Wan\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PDAT", "NN", "$,", "PDAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "La\u00df sie nicht an jener Heilgen, la\u00df an mir sie gl\u00fchend hangen!", "tokens": ["La\u00df", "sie", "nicht", "an", "je\u00b7ner", "Heil\u00b7gen", ",", "la\u00df", "an", "mir", "sie", "gl\u00fc\u00b7hend", "han\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKNEG", "APPR", "PDAT", "NN", "$,", "VVIMP", "APPR", "PPER", "PPER", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Denn ich f\u00fchl es, da\u00df die Gottheit nicht zur Andacht Dich erschuf,", "tokens": ["Denn", "ich", "f\u00fchl", "es", ",", "da\u00df", "die", "Got\u00b7theit", "nicht", "zur", "An\u00b7dacht", "Dich", "er\u00b7schuf", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "$,", "KOUS", "ART", "NN", "PTKNEG", "APPRART", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Da\u00df der Feuerdienst der Liebe, sch\u00f6nes M\u00e4dchen, Dein Beruf.", "tokens": ["Da\u00df", "der", "Feu\u00b7er\u00b7dienst", "der", "Lie\u00b7be", ",", "sch\u00f6\u00b7nes", "M\u00e4d\u00b7chen", ",", "Dein", "Be\u00b7ruf", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "NN", "$,", "ADJA", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}}}}