{"dta.poem.11849": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "26.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie dir auf n\u00e4cht'ger Fahrt die n\u00e4chste Reih der B\u00e4ume", "tokens": ["Wie", "dir", "auf", "n\u00e4cht'\u00b7ger", "Fahrt", "die", "n\u00e4chs\u00b7te", "Reih", "der", "B\u00e4u\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPR", "ADJA", "NN", "ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Am Weg vor\u00fcber eilt, als w\u00e4ren's deine Tr\u00e4ume,", "tokens": ["Am", "Weg", "vor\u00b7\u00fc\u00b7ber", "eilt", ",", "als", "w\u00e4\u00b7ren's", "dei\u00b7ne", "Tr\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "VVFIN", "$,", "KOKOM", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Dahinter langsamer dahin die Bergflur schreitet,", "tokens": ["Da\u00b7hin\u00b7ter", "lang\u00b7sa\u00b7mer", "da\u00b7hin", "die", "Berg\u00b7flur", "schrei\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADJD", "PAV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und hinter ihr der Mond nur deinen Lauf begleitet;", "tokens": ["Und", "hin\u00b7ter", "ihr", "der", "Mond", "nur", "dei\u00b7nen", "Lauf", "be\u00b7glei\u00b7tet", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "ART", "NN", "ADV", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "So fliehn am schnellsten auch auf deines Lebens Fahrt", "tokens": ["So", "fliehn", "am", "schnells\u00b7ten", "auch", "auf", "dei\u00b7nes", "Le\u00b7bens", "Fahrt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPRART", "ADJA", "ADV", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dir die Erscheinungen vorbei der Gegenwart,", "tokens": ["Dir", "die", "Er\u00b7schei\u00b7nun\u00b7gen", "vor\u00b7bei", "der", "Ge\u00b7gen\u00b7wart", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Langsam die gr\u00f6\u00dferen Gestalten ferner Zeiten,", "tokens": ["Lang\u00b7sam", "die", "gr\u00f6\u00b7\u00dfe\u00b7ren", "Ge\u00b7stal\u00b7ten", "fer\u00b7ner", "Zei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Und nur die Ewigkeit bleibt ewig dir zur Seiten.", "tokens": ["Und", "nur", "die", "E\u00b7wig\u00b7keit", "bleibt", "e\u00b7wig", "dir", "zur", "Sei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VVFIN", "ADJD", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}