{"dta.poem.5347": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Ro\u00df-K\u00e4fer.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Indem ich j\u00fcngst im gr\u00fcnen Klee", "tokens": ["In\u00b7dem", "ich", "j\u00fcngst", "im", "gr\u00fc\u00b7nen", "Klee"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Wiesen Schmuck mit tausend Lust beseh,", "tokens": ["Der", "Wie\u00b7sen", "Schmuck", "mit", "tau\u00b7send", "Lust", "be\u00b7seh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPR", "CARD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Werd\u2019 ich von ungefehr gewahr,", "tokens": ["Werd'", "ich", "von", "un\u00b7ge\u00b7fehr", "ge\u00b7wahr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie eine blaue K\u00e4fer-Schaar", "tokens": ["Wie", "ei\u00b7ne", "blau\u00b7e", "K\u00e4\u00b7fer\u00b7Schaar"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "In halb-gedorrtem Pferde-Mist", "tokens": ["In", "halb\u00b7ge\u00b7dorr\u00b7tem", "Pfer\u00b7de\u00b7Mist"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sich aufh\u00e4lt und besch\u00e4ftigt ist,", "tokens": ["Sich", "auf\u00b7h\u00e4lt", "und", "be\u00b7sch\u00e4f\u00b7tigt", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "KON", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "(ohn an der Erden Pracht und Sch\u00e4tzen,", "tokens": ["(", "ohn", "an", "der", "Er\u00b7den", "Pracht", "und", "Sch\u00e4t\u00b7zen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUI", "APPR", "ART", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Mit welchem sie umringt, sich zu ergetzen", "tokens": ["Mit", "wel\u00b7chem", "sie", "um\u00b7ringt", ",", "sich", "zu", "er\u00b7get\u00b7zen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PRELS", "PPER", "VVPP", "$,", "PRF", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Und einiges Vergn\u00fcgen draus zu f\u00fchlen)", "tokens": ["Und", "ei\u00b7ni\u00b7ges", "Ver\u00b7gn\u00fc\u00b7gen", "draus", "zu", "f\u00fch\u00b7len", ")"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "PAV", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "In ihrem Wuft vergn\u00fcgt, best\u00e4ndig fort zu w\u00fchlen.", "tokens": ["In", "ih\u00b7rem", "Wuft", "ver\u00b7gn\u00fcgt", ",", "be\u00b7st\u00e4n\u00b7dig", "fort", "zu", "w\u00fch\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$,", "ADJD", "PTKVZ", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Ich sahe die\u00df zuerst nicht sonder Eckel an,", "tokens": ["Ich", "sa\u00b7he", "die\u00df", "zu\u00b7erst", "nicht", "son\u00b7der", "E\u00b7ckel", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PDS", "ADV", "PTKNEG", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Bi\u00df ich mich \u00fcberwand", "tokens": ["Bi\u00df", "ich", "mich", "\u00fc\u00b7ber\u00b7wand"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "Und eine kurtze Zeit bey ihnen stille stand;", "tokens": ["Und", "ei\u00b7ne", "kurt\u00b7ze", "Zeit", "bey", "ih\u00b7nen", "stil\u00b7le", "stand", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "APPR", "PPER", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Da ich auf ihr Betrieb, mit ernstem Dencken, sann.", "tokens": ["Da", "ich", "auf", "ihr", "Be\u00b7trieb", ",", "mit", "erns\u00b7tem", "Den\u00b7cken", ",", "sann", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "$,", "APPR", "ADJA", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Es scheint, ich solte mich fast der Vergleichung sch\u00e4men,", "tokens": ["Es", "scheint", ",", "ich", "sol\u00b7te", "mich", "fast", "der", "Ver\u00b7glei\u00b7chung", "sch\u00e4\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VMFIN", "PPER", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.16": {"text": "Fiel mir zu Anfang bey,", "tokens": ["Fiel", "mir", "zu", "An\u00b7fang", "bey", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "PTKVZ", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.17": {"text": "Von dieser Bruht ein Beyspiel herzunehmen,", "tokens": ["Von", "die\u00b7ser", "Bruht", "ein", "Bey\u00b7spiel", "her\u00b7zu\u00b7neh\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Als ob in ihr und uns was gleiches sey;", "tokens": ["Als", "ob", "in", "ihr", "und", "uns", "was", "glei\u00b7ches", "sey", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "APPR", "PPER", "KON", "PPER", "PWS", "PIS", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Allein,", "tokens": ["Al\u00b7lein", ","], "token_info": ["word", "punct"], "pos": ["ADV", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.20": {"text": "Fiel mir, beym fernern Dencken ein:", "tokens": ["Fiel", "mir", ",", "beym", "fer\u00b7nern", "Den\u00b7cken", "ein", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "APPRART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.21": {"text": "Es ist ja dennoch wahr. Warum soll ichs nicht sagen?", "tokens": ["Es", "ist", "ja", "den\u00b7noch", "wahr", ".", "Wa\u00b7rum", "soll", "ichs", "nicht", "sa\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ADJD", "$.", "PWAV", "VMFIN", "PIS", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Vielleicht vermag des Beyspiels Scheuslichkeit", "tokens": ["Viel\u00b7leicht", "ver\u00b7mag", "des", "Bey\u00b7spiels", "Scheus\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "Zur Lehr\u2019 und Besserung, was beyzutragen.", "tokens": ["Zur", "Lehr'", "und", "Bes\u00b7se\u00b7rung", ",", "was", "bey\u00b7zu\u00b7tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "NN", "$,", "PRELS", "VVIZU", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "Wenn ich den geitzigen Chrysander,", "tokens": ["Wenn", "ich", "den", "geit\u00b7zi\u00b7gen", "Chry\u00b7san\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Sammt seines Gleichen, bey einander", "tokens": ["Sammt", "sei\u00b7nes", "Glei\u00b7chen", ",", "bey", "ein\u00b7an\u00b7der"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "APPR", "PRF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Mit nichts, als irdschem Koth, besch\u00e4ftigt seh,", "tokens": ["Mit", "nichts", ",", "als", "ird\u00b7schem", "Koth", ",", "be\u00b7sch\u00e4f\u00b7tigt", "seh", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PIS", "$,", "KOUS", "ADJA", "NN", "$,", "VVPP", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.27": {"text": "An welchem sie mit Leib und Seele hangen,", "tokens": ["An", "wel\u00b7chem", "sie", "mit", "Leib", "und", "See\u00b7le", "han\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "APPR", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.28": {"text": "Nichts anders suchen, nichts verlangen,", "tokens": ["Nichts", "an\u00b7ders", "su\u00b7chen", ",", "nichts", "ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIS", "ADV", "VVINF", "$,", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.29": {"text": "Den edlen Geist mit allen seinen Kr\u00e4ften,", "tokens": ["Den", "ed\u00b7len", "Geist", "mit", "al\u00b7len", "sei\u00b7nen", "Kr\u00e4f\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PIAT", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.30": {"text": "Auf nichts, als Gold und Reichthum, heften,", "tokens": ["Auf", "nichts", ",", "als", "Gold", "und", "Reicht\u00b7hum", ",", "hef\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PIS", "$,", "KOUS", "NN", "KON", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}