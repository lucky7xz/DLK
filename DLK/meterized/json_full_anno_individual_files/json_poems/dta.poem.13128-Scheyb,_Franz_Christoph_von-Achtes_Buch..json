{"dta.poem.13128": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Achtes Buch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201eso lang der Gnaden-Quell der Allmacht es erlaubt,", "tokens": ["\u201e", "so", "lang", "der", "Gna\u00b7den\u00b7Quell", "der", "All\u00b7macht", "es", "er\u00b7laubt", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ADJD", "ART", "NN", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "\u201eso lang ist nichts das mich von meinem Amt beraubt.", "tokens": ["\u201e", "so", "lang", "ist", "nichts", "das", "mich", "von", "mei\u00b7nem", "Amt", "be\u00b7raubt", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ADJD", "VAFIN", "PIS", "ART", "PPER", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "645\u201dWas aber meiner Macht in der Natur verborgen,", "tokens": ["\"", "Was", "a\u00b7ber", "mei\u00b7ner", "Macht", "in", "der", "Na\u00b7tur", "ver\u00b7bor\u00b7gen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "ADV", "PPOSAT", "NN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "\u201eum dieses lassen wir den h\u00f6chsten Arzt nur sorgen.", "tokens": ["\u201e", "um", "die\u00b7ses", "las\u00b7sen", "wir", "den", "h\u00f6chs\u00b7ten", "Arzt", "nur", "sor\u00b7gen", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "PDAT", "VVFIN", "PPER", "ART", "ADJA", "NN", "ADV", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "\u201esonst bleib ich im Besiz, und stehe nimmer ab;", "tokens": ["\u201e", "sonst", "bleib", "ich", "im", "Be\u00b7siz", ",", "und", "ste\u00b7he", "nim\u00b7mer", "ab", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPER", "APPRART", "NN", "$,", "KON", "VVFIN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "\u201ejhr kennet, was vor Kraft ihr meine Tugend gab.", "tokens": ["\u201e", "jhr", "ken\u00b7net", ",", "was", "vor", "Kraft", "ihr", "mei\u00b7ne", "Tu\u00b7gend", "gab", "."], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "$,", "PRELS", "APPR", "NN", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "\u201ewann manche von dem Krei\u00df am sichersten geschlafen,", "tokens": ["\u201e", "wann", "man\u00b7che", "von", "dem", "Krei\u00df", "am", "si\u00b7chers\u00b7ten", "ge\u00b7schla\u00b7fen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "PIS", "APPR", "ART", "NN", "APPRART", "ADJA", "VVPP", "$,"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "650\u201dDa fand ich jederzeit am mehresten zu schaffen.", "tokens": ["\"", "Da", "fand", "ich", "je\u00b7der\u00b7zeit", "am", "meh\u00b7res\u00b7ten", "zu", "schaf\u00b7fen", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPER", "ADV", "APPRART", "PIS", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "\u201estets war ich um das Herz der K\u00f6niginn besorgt;", "tokens": ["\u201e", "stets", "war", "ich", "um", "das", "Herz", "der", "K\u00f6\u00b7ni\u00b7ginn", "be\u00b7sorgt", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VAFIN", "PPER", "APPR", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "\u201ewie manche Stund hab ich, ihr beyzustehn, geborgt:", "tokens": ["\u201e", "wie", "man\u00b7che", "Stund", "hab", "ich", ",", "ihr", "bey\u00b7zu\u00b7stehn", ",", "ge\u00b7borgt", ":"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["$(", "PWAV", "PIAT", "NN", "VAFIN", "PPER", "$,", "PPER", "VVINF", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "\u201edamit wann ihr Gem\u00fcth ein Trauren \u00fcberw\u00e4nde,", "tokens": ["\u201e", "da\u00b7mit", "wann", "ihr", "Ge\u00b7m\u00fcth", "ein", "Trau\u00b7ren", "\u00fc\u00b7ber\u00b7w\u00e4n\u00b7de", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PAV", "PWAV", "PPOSAT", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "\u201ees weder Angst noch Schmerz, noch Bitterkeit empf\u00e4nde.", "tokens": ["\u201e", "es", "we\u00b7der", "Angst", "noch", "Schmerz", ",", "noch", "Bit\u00b7ter\u00b7keit", "em\u00b7pf\u00e4n\u00b7de", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "KON", "NN", "ADV", "NN", "$,", "ADV", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}