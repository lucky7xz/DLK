{"dta.poem.9650": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Uber einen/ der noch im traur-jahre wieder  \n heyrathet/ und die neugeheyrathete gleichfalls in  \n demselben jahre verliehret.  \n \u2020 \u2020 \u2020", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der kaum die erste frau/ so gut sie war/ bedaurt/", "tokens": ["Der", "kaum", "die", "ers\u00b7te", "frau", "/", "so", "gut", "sie", "war", "/", "be\u00b7daurt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADV", "ART", "ADJA", "NN", "$(", "ADV", "ADJD", "PPER", "VAFIN", "$(", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Will um der andern todt vor hertzeleid sich t\u00f6dten.", "tokens": ["Will", "um", "der", "an\u00b7dern", "todt", "vor", "hert\u00b7ze\u00b7leid", "sich", "t\u00f6d\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "ART", "ADJA", "ADJD", "APPR", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Alleine h\u00e4tt\u2019 er nur die erste recht betraurt/", "tokens": ["Al\u00b7lei\u00b7ne", "h\u00e4tt'", "er", "nur", "die", "ers\u00b7te", "recht", "be\u00b7traurt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So h\u00e4tt\u2019 er dieser traur anietzo nicht von n\u00f6then.", "tokens": ["So", "h\u00e4tt'", "er", "die\u00b7ser", "traur", "an\u00b7iet\u00b7zo", "nicht", "von", "n\u00f6\u00b7then", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PDS", "VVFIN", "ADV", "PTKNEG", "APPR", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}