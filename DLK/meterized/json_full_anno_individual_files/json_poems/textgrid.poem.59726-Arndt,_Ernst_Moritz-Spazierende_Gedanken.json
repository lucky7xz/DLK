trochaic.penta.relaxed"}, "line.6": {"text": "Rappell bald zum Abmarsch geblasen,", "tokens": ["Rap\u00b7pell", "bald", "zum", "Ab\u00b7marsch", "ge\u00b7bla\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "APPRART", "NN", "VVPP", "$,"], "meter": "----+--+-", "measure": "iambic.di.relaxed"}, "line.7": {"text": "Und wird ihm auf Erden sein letztes Quartier", "tokens": ["Und", "wird", "ihm", "auf", "Er\u00b7den", "sein", "letz\u00b7tes", "Quar\u00b7tier"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "APPR", "NN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Gebettet bald unter dem Rasen.", "tokens": ["Ge\u00b7bet\u00b7tet", "bald", "un\u00b7ter", "dem", "Ra\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.8": {"line.1": {"text": "Ei Fabel! Was fabl' ich das Alte mir vor,", "tokens": ["Ei", "Fa\u00b7bel", "!", "Was", "fa\u00b7bl'", "ich", "das", "Al\u00b7te", "mir", "vor", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$.", "PWS", "VVFIN", "PPER", "ART", "NN", "PPER", "PTKVZ", "$,"], "meter": "-+--+---+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die Kluft zwischen Himmel und Erde?", "tokens": ["Die", "Kluft", "zwi\u00b7schen", "Him\u00b7mel", "und", "Er\u00b7de", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Weitauf steht der Welten unendliches Tor,", "tokens": ["Weit\u00b7auf", "steht", "der", "Wel\u00b7ten", "un\u00b7end\u00b7li\u00b7ches", "Tor", ","], "token_info": ["word", "word", "word", "w