{"textgrid.poem.59726": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "Spazierende Gedanken", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schau' ich wandelnd die pr\u00e4chtigen H\u00e4user mir an,", "tokens": ["Schau'", "ich", "wan\u00b7delnd", "die", "pr\u00e4ch\u00b7ti\u00b7gen", "H\u00e4u\u00b7ser", "mir", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VVPP", "ART", "ADJA", "NN", "PPER", "PTKVZ", "$,"], "meter": "+-+--+--+--+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Wird's mir schier, als m\u00f6cht' ich noch bauen,", "tokens": ["Wird's", "mir", "schier", ",", "als", "m\u00f6cht'", "ich", "noch", "bau\u00b7en", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "$,", "KOUS", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Und sollte doch ein so steinalter Mann", "tokens": ["Und", "soll\u00b7te", "doch", "ein", "so", "stei\u00b7nal\u00b7ter", "Mann"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "ADV", "ART", "ADV", "ADJA", "NN"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Himmelauf nur und himmelein schauen;", "tokens": ["Him\u00b7me\u00b7lauf", "nur", "und", "him\u00b7mel\u00b7ein", "schau\u00b7en", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "KON", "NN", "VVINF", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Wird doch seinem fl\u00fcchtigen Bleiben allhier", "tokens": ["Wird", "doch", "sei\u00b7nem", "fl\u00fcch\u00b7ti\u00b7gen", "Blei\u00b7ben", "all\u00b7hier"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "PPOSAT", "ADJA", "NN", "ADV"], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "Rappell bald zum Abmarsch geblasen,", "tokens": ["Rap\u00b7pell", "bald", "zum", "Ab\u00b7marsch", "ge\u00b7bla\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "APPRART", "NN", "VVPP", "$,"], "meter": "----+--+-", "measure": "iambic.di.relaxed"}, "line.7": {"text": "Und wird ihm auf Erden sein letztes Quartier", "tokens": ["Und", "wird", "ihm", "auf", "Er\u00b7den", "sein", "letz\u00b7tes", "Quar\u00b7tier"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "APPR", "NN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Gebettet bald unter dem Rasen.", "tokens": ["Ge\u00b7bet\u00b7tet", "bald", "un\u00b7ter", "dem", "Ra\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.2": {"line.1": {"text": "Ei Fabel! Was fabl' ich das Alte mir vor,", "tokens": ["Ei", "Fa\u00b7bel", "!", "Was", "fa\u00b7bl'", "ich", "das", "Al\u00b7te", "mir", "vor", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$.", "PWS", "VVFIN", "PPER", "ART", "NN", "PPER", "PTKVZ", "$,"], "meter": "-+--+---+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die Kluft zwischen Himmel und Erde?", "tokens": ["Die", "Kluft", "zwi\u00b7schen", "Him\u00b7mel", "und", "Er\u00b7de", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Weitauf steht der Welten unendliches Tor,", "tokens": ["Weit\u00b7auf", "steht", "der", "Wel\u00b7ten", "un\u00b7end\u00b7li\u00b7ches", "Tor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Wo ich Kleiner schon durchschl\u00fcpfen werde:", "tokens": ["Wo", "ich", "Klei\u00b7ner", "schon", "durch\u00b7schl\u00fcp\u00b7fen", "wer\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "NN", "ADV", "VVINF", "VAFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Sankt Peter mit aller Kardinalpolizei,", "tokens": ["Sankt", "Pe\u00b7ter", "mit", "al\u00b7ler", "Kar\u00b7di\u00b7nal\u00b7po\u00b7li\u00b7zei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "APPR", "PIAT", "NN", "$,"], "meter": "-+--+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Mit all ihrer schrecklichen Presse", "tokens": ["Mit", "all", "ih\u00b7rer", "schreck\u00b7li\u00b7chen", "Pres\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "PPOSAT", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.7": {"text": "Schaut meinen Pa\u00df an und rufet: \u00bbPassiere nur frei!", "tokens": ["Schaut", "mei\u00b7nen", "Pa\u00df", "an", "und", "ru\u00b7fet", ":", "\u00bb", "Pas\u00b7sie\u00b7re", "nur", "frei", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PTKVZ", "KON", "VVFIN", "$.", "$(", "NN", "ADV", "ADJD", "$."], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Dein Pa\u00df ist der beste der P\u00e4sse.", "tokens": ["Dein", "Pa\u00df", "ist", "der", "bes\u00b7te", "der", "P\u00e4s\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "ADJA", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.3": {"line.1": {"text": "Dir flammet im Herzen der g\u00f6ttliche Mut,", "tokens": ["Dir", "flam\u00b7met", "im", "Her\u00b7zen", "der", "g\u00f6tt\u00b7li\u00b7che", "Mut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Dir flammen im Kopfe die Blitze,", "tokens": ["Dir", "flam\u00b7men", "im", "Kop\u00b7fe", "die", "Blit\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "F\u00fcr solche sind Himmel und Erde gleich gut,", "tokens": ["F\u00fcr", "sol\u00b7che", "sind", "Him\u00b7mel", "und", "Er\u00b7de", "gleich", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "VAFIN", "NN", "KON", "NN", "ADV", "ADJD", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Sie bauen nicht bleibende Sitze.", "tokens": ["Sie", "bau\u00b7en", "nicht", "blei\u00b7ben\u00b7de", "Sit\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Frei durch denn! Und wolltest du wieder heraus,", "tokens": ["Frei", "durch", "denn", "!", "Und", "woll\u00b7test", "du", "wie\u00b7der", "he\u00b7raus", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ADV", "$.", "KON", "VMFIN", "PPER", "ADV", "PTKVZ", "$,"], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "Bei dem Tor sind unz\u00e4hlige Pforten:", "tokens": ["Bei", "dem", "Tor", "sind", "un\u00b7z\u00e4h\u00b7li\u00b7ge", "Pfor\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "ADJA", "NN", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.7": {"text": "Soweit Licht scheint, bauen G\u00f6tter und Geister ihr Haus,", "tokens": ["So\u00b7weit", "Licht", "scheint", ",", "bau\u00b7en", "G\u00f6t\u00b7ter", "und", "Geis\u00b7ter", "ihr", "Haus", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVFIN", "$,", "ADJA", "NN", "KON", "NN", "PPOSAT", "NN", "$,"], "meter": "-++-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Ihnen t\u00f6nt's nicht von St\u00e4tten und Orten.\u00ab", "tokens": ["Ih\u00b7nen", "t\u00f6nt's", "nicht", "von", "St\u00e4t\u00b7ten", "und", "Or\u00b7ten", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "APPR", "NN", "KON", "NN", "$.", "$("], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "So pilgr' ich und finde mich leidlich zurecht \u2013", "tokens": ["So", "pil\u00b7gr'", "ich", "und", "fin\u00b7de", "mich", "leid\u00b7lich", "zu\u00b7recht", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "KON", "VVFIN", "PPER", "ADJD", "PTKVZ", "$("], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Das \u00fcbrige wisse Sankt Peter \u2013", "tokens": ["Das", "\u00fcb\u00b7ri\u00b7ge", "wis\u00b7se", "Sankt", "Pe\u00b7ter", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "NE", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "So schrei' ich \u00fcber Erden- und Himmelgeflecht", "tokens": ["So", "schrei'", "ich", "\u00fc\u00b7ber", "Er\u00b7den", "und", "Him\u00b7mel\u00b7ge\u00b7flecht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "TRUNC", "KON", "NN"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Nicht kl\u00e4glich Mordio! und Zeter!", "tokens": ["Nicht", "kl\u00e4g\u00b7lich", "Mor\u00b7dio", "!", "und", "Ze\u00b7ter", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "NN", "$.", "KON", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Denn der's geflochten, das wei\u00df ich, der wird seinerzeit", "tokens": ["Denn", "der's", "ge\u00b7floch\u00b7ten", ",", "das", "wei\u00df", "ich", ",", "der", "wird", "sei\u00b7ner\u00b7zeit"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PIS", "VVPP", "$,", "PDS", "VVFIN", "PPER", "$,", "PRELS", "VAFIN", "NN"], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Alle F\u00e4den aufs sch\u00f6nste entwirren:", "tokens": ["Al\u00b7le", "F\u00e4\u00b7den", "aufs", "sch\u00f6ns\u00b7te", "ent\u00b7wir\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPRART", "ADJA", "VVINF", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.7": {"text": "Ihm trau' ich, drum lass' ich zu wild und zu weit", "tokens": ["Ihm", "trau'", "ich", ",", "drum", "lass'", "ich", "zu", "wild", "und", "zu", "weit"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PAV", "VVFIN", "PPER", "PTKA", "ADJD", "KON", "PTKA", "ADJD"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.8": {"text": "Die Gedanken mein Hirn nicht umschwirren.", "tokens": ["Die", "Ge\u00b7dan\u00b7ken", "mein", "Hirn", "nicht", "um\u00b7schwir\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "PTKNEG", "VVINF", "$."], "meter": "--+-++--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Nein, kein Jung und kein Alt und kein Dort und kein Hier!", "tokens": ["Nein", ",", "kein", "Jung", "und", "kein", "Alt", "und", "kein", "Dort", "und", "kein", "Hier", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PIAT", "NN", "KON", "PIAT", "NN", "KON", "PIAT", "NN", "KON", "PIAT", "ADV", "$."], "meter": "+-+--+--+--+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Weg, Gedanken, ihr grauen und falben!", "tokens": ["Weg", ",", "Ge\u00b7dan\u00b7ken", ",", "ihr", "grau\u00b7en", "und", "fal\u00b7ben", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "PPOSAT", "ADJA", "KON", "VVINF", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Bei dem \u00c4ltsten, er hei\u00dft ", "tokens": ["Bei", "dem", "\u00c4lts\u00b7ten", ",", "er", "hei\u00dft"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "PPER", "VVFIN"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Bei dem \u00c4ltsten der Tage, da nehm' ich den Sitz \u2013", "tokens": ["Bei", "dem", "\u00c4lts\u00b7ten", "der", "Ta\u00b7ge", ",", "da", "nehm'", "ich", "den", "Sitz", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,", "ADV", "VVFIN", "PPER", "ART", "NN", "$("], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.5": {"text": "Er blies auch durch mich seinen Odem.", "tokens": ["Er", "blies", "auch", "durch", "mich", "sei\u00b7nen", "O\u00b7dem", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Auf mit Fl\u00fcgeln, mein Geistchen! Und funkle wie Blitz!", "tokens": ["Auf", "mit", "Fl\u00fc\u00b7geln", ",", "mein", "Geist\u00b7chen", "!", "Und", "funk\u00b7le", "wie", "Blitz", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "NN", "$,", "PPOSAT", "NN", "$.", "KON", "VVFIN", "KOKOM", "NN", "$."], "meter": "+-+--+--+--+", "measure": "trochaic.penta.relaxed"}, "line.7": {"text": "Blitze Leben aus Kaltem und Totem!", "tokens": ["Blit\u00b7ze", "Le\u00b7ben", "aus", "Kal\u00b7tem", "und", "To\u00b7tem", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "Sei mutig! Dem K\u00fchnen verwelket kein Kranz,", "tokens": ["Sei", "mu\u00b7tig", "!", "Dem", "K\u00fch\u00b7nen", "ver\u00b7wel\u00b7ket", "kein", "Kranz", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "$.", "ART", "NN", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Ein ewiger Lenz ist sein Eigen;", "tokens": ["Ein", "e\u00b7wi\u00b7ger", "Lenz", "ist", "sein", "Ei\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Tanze mit in der Welten unsterblichem Glanz", "tokens": ["Tan\u00b7ze", "mit", "in", "der", "Wel\u00b7ten", "uns\u00b7terb\u00b7li\u00b7chem", "Glanz"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+--+--+--+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Der Wonne unsterblichen Reigen.", "tokens": ["Der", "Won\u00b7ne", "uns\u00b7terb\u00b7li\u00b7chen", "Rei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Sei mutig! Und gleich wird das engste Revier,", "tokens": ["Sei", "mu\u00b7tig", "!", "Und", "gleich", "wird", "das", "engs\u00b7te", "Re\u00b7vier", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "$.", "KON", "ADV", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wo du weilest, der weiteste Himmel,", "tokens": ["Wo", "du", "wei\u00b7lest", ",", "der", "wei\u00b7tes\u00b7te", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.7": {"text": "Das Unten und Oben, das Dort und das Hier", "tokens": ["Das", "Un\u00b7ten", "und", "O\u00b7ben", ",", "das", "Dort", "und", "das", "Hier"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "NN", "$,", "PRELS", "ADV", "KON", "ART", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.8": {"text": "Verschwimmt in der Wonne Gewimmel.", "tokens": ["Ver\u00b7schwimmt", "in", "der", "Won\u00b7ne", "Ge\u00b7wim\u00b7mel", "."], "token_info": ["word", "word", "word", "word", "w