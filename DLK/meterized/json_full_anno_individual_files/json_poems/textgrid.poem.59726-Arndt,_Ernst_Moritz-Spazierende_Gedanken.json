hibrach.tri"}}, "stanza.9": {"line.1": {"text": "Dir flammet im Herzen der g\u00f6ttliche Mut,", "tokens": ["Dir", "flam\u00b7met", "im", "Her\u00b7zen", "der", "g\u00f6tt\u00b7li\u00b7che", "Mut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Dir flammen im Kopfe die Blitze,", "tokens": ["Dir", "flam\u00b7men", "im", "Kop\u00b7fe", "die", "Blit\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "F\u00fcr solche sind Himmel und Erde gleich gut,", "tokens": ["F\u00fcr", "sol\u00b7che", "sind", "Him\u00b7mel", "und", "Er\u00b7de", "gleich", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "VAFIN", "NN", "KON", "NN", "ADV", "ADJD", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Sie bauen nicht bleibende Sitze.", "tokens": ["Sie", "bau\u00b7en", "nicht", "blei\u00b7ben\u00b7de", "Sit\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Frei durch denn! Und wolltest du wieder heraus,", "tokens": ["Frei", "durch", "denn", "!", "Und", "woll\u00b7test", "du", "wie\u00b7der", "he\u00b7raus", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ADV", "$.", "KON", "VMFIN", "PPER", "ADV", "PTKVZ", "$,"], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "Bei dem Tor sind unz\u00e4hlige Pforten:", "tokens": ["Bei", "dem", "Tor", "sind", "un\u00b7z\u00e4h\u00b