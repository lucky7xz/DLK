rch", "mich", "sei\u00b7nen", "O\u00b7dem", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Auf mit Fl\u00fcgeln, mein Geistchen! Und funkle wie Blitz!", "tokens": ["Auf", "mit", "Fl\u00fc\u00b7geln", ",", "mein", "Geist\u00b7chen", "!", "Und", "funk\u00b7le", "wie", "Blitz", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "NN", "$,", "PPOSAT", "NN", "$.", "KON", "VVFIN", "KOKOM", "NN", "$."], "meter": "+-+--+--+--+", "measure": "trochaic.penta.relaxed"}, "line.7": {"text": "Blitze Leben aus Kaltem und Totem!", "tokens": ["Blit\u00b7ze", "Le\u00b7ben", "aus", "Kal\u00b7tem", "und", "To\u00b7tem", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.12": {"line.1": {"text": "Sei mutig! Dem K\u00fchnen verwelket kein Kranz,", "tokens": ["Sei", "mu\u00b7tig", "!", "Dem", "K\u00fch\u00b7nen", "ver\u00b7wel\u00b7ket", "kein", "Kranz", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "$.", "ART", "NN", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Ein ewiger Lenz ist sein Eigen;", "tokens": ["Ein", "e\u00b7wi\u00b7ger", "Lenz", "ist", "sein", "Ei\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Tanze mit in der Welten unsterblichem Glanz", "tokens": ["Tan\u00b7ze", "mit", "in", "der", "Wel\u00b7ten", "uns\u00b7terb\u00b7li\u00b7chem", "Glanz"], "token_i