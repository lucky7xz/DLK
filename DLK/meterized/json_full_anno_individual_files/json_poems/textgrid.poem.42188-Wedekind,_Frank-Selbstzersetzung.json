{"textgrid.poem.42188": {"metadata": {"author": {"name": "Wedekind, Frank", "birth": "N.A.", "death": "N.A."}, "title": "Selbstzersetzung", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hochheil'ge Gebete, die fromm ich gelernt,", "tokens": ["Hoch\u00b7heil'\u00b7ge", "Ge\u00b7be\u00b7te", ",", "die", "fromm", "ich", "ge\u00b7lernt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PRELS", "ADJD", "PPER", "VVPP", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ich stellte sie frech an den Pranger;", "tokens": ["Ich", "stell\u00b7te", "sie", "frech", "an", "den", "Pran\u00b7ger", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Mein kindlicher Himmel, so herrlich besternt,", "tokens": ["Mein", "kind\u00b7li\u00b7cher", "Him\u00b7mel", ",", "so", "herr\u00b7lich", "bes\u00b7ternt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Ward w\u00fcsten Gelagen zum Anger.", "tokens": ["Ward", "w\u00fcs\u00b7ten", "Ge\u00b7la\u00b7gen", "zum", "An\u00b7ger", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVFIN", "NN", "APPRART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.2": {"line.1": {"text": "Ich schalt meinen Gott einen schl\u00e4frigen Wicht;", "tokens": ["Ich", "schalt", "mei\u00b7nen", "Gott", "ei\u00b7nen", "schl\u00e4f\u00b7ri\u00b7gen", "Wicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Ich schlug ihm begeistert den Stempel", "tokens": ["Ich", "schlug", "ihm", "be\u00b7geis\u00b7tert", "den", "Stem\u00b7pel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "VVFIN", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Heillosen Betrugs ins vergr\u00e4mte Gesicht", "tokens": ["Heil\u00b7lo\u00b7sen", "Be\u00b7trugs", "ins", "ver\u00b7gr\u00e4m\u00b7te", "Ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "APPRART", "ADJA", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Und wies ihn hinaus aus dem Tempel.", "tokens": ["Und", "wies", "ihn", "hin\u00b7aus", "aus", "dem", "Tem\u00b7pel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APZR", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Da stand ich allein im erleuchteten Haus", "tokens": ["Da", "stand", "ich", "al\u00b7lein", "im", "er\u00b7leuch\u00b7te\u00b7ten", "Haus"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Und lie\u00df mir die Seele zerw\u00fchlen", "tokens": ["Und", "lie\u00df", "mir", "die", "See\u00b7le", "zer\u00b7w\u00fch\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "VVINF"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Von grausiger Wonne, von wonnigem Graus:", "tokens": ["Von", "grau\u00b7si\u00b7ger", "Won\u00b7ne", ",", "von", "won\u00b7ni\u00b7gem", "Graus