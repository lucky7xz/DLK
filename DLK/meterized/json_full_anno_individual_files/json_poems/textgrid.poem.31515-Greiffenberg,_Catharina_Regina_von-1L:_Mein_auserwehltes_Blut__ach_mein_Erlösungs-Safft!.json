{"textgrid.poem.31515": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mein auserwehltes Blut / ach mein Erl\u00f6sungs-Safft!", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein auserwehltes Blut / ach mein Erl\u00f6sungs-Safft!", "tokens": ["Mein", "au\u00b7ser\u00b7wehl\u00b7tes", "Blut", "/", "ach", "mein", "Er\u00b7l\u00f6\u00b7sungs\u00b7\u00b7S\u00b7afft", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$(", "XY", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.2": {"text": "wo sind ich Herz / wo Wort / zu lieben und zu loben", "tokens": ["wo", "sind", "ich", "Herz", "/", "wo", "Wort", "/", "zu", "lie\u00b7ben", "und", "zu", "lo\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "PPER", "NN", "$(", "PWAV", "NN", "$(", "PTKZU", "VVINF", "KON", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "solch unerh\u00f6rter Treu Herzschmerzlich hohe Proben /", "tokens": ["solch", "un\u00b7er\u00b7h\u00f6r\u00b7ter", "Treu", "Herz\u00b7schmerz\u00b7lich", "ho\u00b7he", "Pro\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "ADJD", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "wann mir vom Himmel nicht wird Preisungs-Krafft verschafft.", "tokens": ["wann", "mir", "vom", "Him\u00b7mel", "nicht", "wird", "Prei\u00b7sungs\u00b7Krafft", "ver\u00b7schafft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "AP