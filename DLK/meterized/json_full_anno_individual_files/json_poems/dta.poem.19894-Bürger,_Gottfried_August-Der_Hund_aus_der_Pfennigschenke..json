{"dta.poem.19894": {"metadata": {"author": {"name": "B\u00fcrger, Gottfried August", "birth": "N.A.", "death": "N.A."}, "title": "Der                \n  Hund aus der Pfennigschenke.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1778", "urn": "urn:nbn:de:kobv:b4-20090519672", "language": ["de:0.99"], "booktitle": "B\u00fcrger, Gottfried August: Gedichte. G\u00f6ttingen, 1778."}, "poem": {"stanza.1": {"line.1": {"text": "Es ging, was ernstes zu bestellen,               ", "tokens": ["Es", "ging", ",", "was", "erns\u00b7tes", "zu", "be\u00b7stel\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWS", "ADJA", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Wandrer seinen stillen Gang,", "tokens": ["Ein", "Wand\u00b7rer", "sei\u00b7nen", "stil\u00b7len", "Gang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Als auf ihn los ein Hund, mit Bellen,", "tokens": ["Als", "auf", "ihn", "los", "ein", "Hund", ",", "mit", "Bel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PPER", "ADV", "ART", "NN", "$,", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und Rasseln vieler Halsbandschellen,", "tokens": ["Und", "Ras\u00b7seln", "vie\u00b7ler", "Hals\u00b7band\u00b7schel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Aus einer Pfennigschenke sprang.", "tokens": ["Aus", "ei\u00b7ner", "Pfen\u00b7nig\u00b7schen\u00b7ke", "sprang", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Er, ohne Stok und Stein zu heben,", "tokens": ["Er", ",", "oh\u00b7ne", "Stok", "und", "Stein", "zu", "he\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "KOUI", "NN", "KON", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Noch sonst sich mit ihm abzugeben,", "tokens": ["Noch", "sonst", "sich", "mit", "ihm", "ab\u00b7zu\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PRF", "APPR", "PPER", "VVIZU", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Hub ruhig weiter Fus und Stab,", "tokens": ["Hub", "ru\u00b7hig", "wei\u00b7ter", "Fus", "und", "Stab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ADV", "NE", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Und Klifklaf lies vom L\u00e4rmen ab.", "tokens": ["Und", "Klif\u00b7klaf", "lies", "vom", "L\u00e4r\u00b7men", "ab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Des Wegs kam auch, mit Rohr und Degen,", "tokens": ["Des", "Wegs", "kam", "auch", ",", "mit", "Rohr", "und", "De\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "$,", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Flink, wolgemut, kek und verwegen,", "tokens": ["Flink", ",", "wol\u00b7ge\u00b7mut", ",", "kek", "und", "ver\u00b7we\u00b7gen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VMFIN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Herrchen Krauskopf herspaziert.", "tokens": ["Ein", "Herr\u00b7chen", "Kraus\u00b7kopf", "her\u00b7spa\u00b7ziert", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Klifklaf sezt an; und hochtuschirt", "tokens": ["Klif\u00b7klaf", "sezt", "an", ";", "und", "hoch\u00b7tu\u00b7schirt"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "ADV", "PTKVZ", "$.", "KON", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "H\u00e4lt von dem Hunde sich das Herrchen.", "tokens": ["H\u00e4lt", "von", "dem", "Hun\u00b7de", "sich", "das", "Herr\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "PRF", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und Herrchen Krauskopf ist ein N\u00e4rrchen,", "tokens": ["Und", "Herr\u00b7chen", "Kraus\u00b7kopf", "ist", "ein", "N\u00e4rr\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "F\u00e4ngt mit dem Klaffer H\u00e4ndel an,", "tokens": ["F\u00e4ngt", "mit", "dem", "Klaf\u00b7fer", "H\u00e4n\u00b7del", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "NN", "PTKVZ", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.8": {"text": "Greift fix nach Steinen in die Runde,", "tokens": ["Greift", "fix", "nach", "Stei\u00b7nen", "in", "die", "Run\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "APPR", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Und schleudert, was es schleudern kan,", "tokens": ["Und", "schleu\u00b7dert", ",", "was", "es", "schleu\u00b7dern", "kan", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PWS", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Und flucht und pr\u00fcgelt nach dem Hunde.", "tokens": ["Und", "flucht", "und", "pr\u00fc\u00b7gelt", "nach", "dem", "Hun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Der K\u00f6ter knirscht in jeden Stein,", "tokens": ["Der", "K\u00f6\u00b7ter", "knirscht", "in", "je\u00b7den", "Stein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Zerrt bald an meines Herrchens Stocke,", "tokens": ["Zerrt", "bald", "an", "mei\u00b7nes", "Herr\u00b7chens", "Sto\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Bald an dem Degen, bald am Rocke,", "tokens": ["Bald", "an", "dem", "De\u00b7gen", ",", "bald", "am", "Ro\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Beist endlich gar ihm in das Bein,", "tokens": ["Beist", "end\u00b7lich", "gar", "ihm", "in", "das", "Bein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Und belt so w\u00fctig, da\u00df mit Haufen", "tokens": ["Und", "belt", "so", "w\u00fc\u00b7tig", ",", "da\u00df", "mit", "Hau\u00b7fen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "ADJD", "$,", "KOUS", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Die Nachbarn alle, gros und klein,", "tokens": ["Die", "Nach\u00b7barn", "al\u00b7le", ",", "gros", "und", "klein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIS", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Zu Fenstern und zu Th\u00fcren laufen.", "tokens": ["Zu", "Fens\u00b7tern", "und", "zu", "Th\u00fc\u00b7ren", "lau\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Die Buben klatschen und juchhein,", "tokens": ["Die", "Bu\u00b7ben", "klat\u00b7schen", "und", "juch\u00b7hein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "KON", "PTKANT", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Und hezen gar noch oben drein.", "tokens": ["Und", "he\u00b7zen", "gar", "noch", "o\u00b7ben", "drein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADV", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Nun fing sich\u2019s Herrchen an zu sch\u00e4men,", "tokens": ["Nun", "fing", "sich's", "Herr\u00b7chen", "an", "zu", "sch\u00e4\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "VVFIN", "PTKVZ", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Umsonst so sehr sich abzum\u00fchn.", "tokens": ["Um\u00b7sonst", "so", "sehr", "sich", "ab\u00b7zu\u00b7m\u00fchn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "PRF", "VVIZU", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Es muste sachtchen sich bequemen,", "tokens": ["Es", "mus\u00b7te", "sacht\u00b7chen", "sich", "be\u00b7que\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVFIN", "PRF", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Um dem Halloh sich zu entziehn,", "tokens": ["Um", "dem", "Hal\u00b7loh", "sich", "zu", "ent\u00b7ziehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "PRF", "PTKZU", "VVINF", "$,"], "meter": "--++-+-+", "measure": "anapaest.init"}, "line.24": {"text": "Wol f\u00fcrbas seinen Weg zu nehmen,", "tokens": ["Wol", "f\u00fcr\u00b7bas", "sei\u00b7nen", "Weg", "zu", "neh\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Und einzustecken Hohn und Schmach.", "tokens": ["Und", "ein\u00b7zu\u00b7ste\u00b7cken", "Hohn", "und", "Schmach", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Denn alle Strassenbuben gaften,", "tokens": ["Denn", "al\u00b7le", "Stras\u00b7sen\u00b7bu\u00b7ben", "gaf\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.27": {"text": "Und alle Klafkonsorten klaften", "tokens": ["Und", "al\u00b7le", "Klaf\u00b7kon\u00b7sor\u00b7ten", "klaf\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "Noch weit zum Dorf hinaus ihm nach.", "tokens": ["Noch", "weit", "zum", "Dorf", "hin\u00b7aus", "ihm", "nach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPRART", "NN", "APZR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Dies Fabelchen f\u00fchrt Gold im Munde:", "tokens": ["Dies", "Fa\u00b7bel\u00b7chen", "f\u00fchrt", "Gold", "im", "Mun\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "VVFIN", "NN", "APPRART", "NN", "$."], "meter": "-+---+-+-", "measure": "dactylic.init"}, "line.2": {"text": "Weicht aus dem Rezensentenhunde!", "tokens": ["Weicht", "aus", "dem", "Re\u00b7zen\u00b7sen\u00b7ten\u00b7hun\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}}}}}