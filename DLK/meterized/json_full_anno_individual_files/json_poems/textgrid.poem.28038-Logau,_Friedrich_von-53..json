{"textgrid.poem.28038": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "53.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein rinderner Verstand und k\u00e4lberne Geberden,", "tokens": ["Ein", "rin\u00b7der\u00b7ner", "Ver\u00b7stand", "und", "k\u00e4l\u00b7ber\u00b7ne", "Ge\u00b7ber\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-----+-+-----", "measure": "unknown.measure.di"}, "line.2": {"text": "Dabey ein w\u00f6lffisch Sinn sind br\u00e4uchlich ietzt auff Erden.", "tokens": ["Da\u00b7bey", "ein", "w\u00f6lf\u00b7fisch", "Sinn", "sind", "br\u00e4uch\u00b7lich", "ietzt", "auff", "Er\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "ADJD", "NN", "VAFIN", "ADJD", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Das Rind versteht sich nicht, dann nur auff Stroh und Gras;", "tokens": ["Das", "Rind", "ver\u00b7steht", "sich", "nicht", ",", "dann", "nur", "auff", "Stroh", "und", "Gras", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "PTKNEG", "$,", "ADV", "ADV", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ein Mensch laufft, rennt und schwitzt blo\u00df um den vollen Fra\u00df.", "tokens": ["Ein", "Mensch", "laufft", ",", "rennt", "und", "schwitzt", "blo\u00df", "um", "den", "vol\u00b7len", "Fra\u00df", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN"