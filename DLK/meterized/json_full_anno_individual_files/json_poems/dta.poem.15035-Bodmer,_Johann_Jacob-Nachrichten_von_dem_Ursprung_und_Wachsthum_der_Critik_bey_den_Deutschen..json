{"dta.poem.15035": {"metadata": {"author": {"name": "Bodmer, Johann Jacob", "birth": "N.A.", "death": "N.A."}, "title": "Nachrichten von dem Ursprung und  \n Wachsthum der Critik bey den  \n Deutschen.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1741", "urn": "urn:nbn:de:kobv:b4-200905198526", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die in ein Spinngeweb das Bild der Dichtkunst pr\u00e4gt,", "tokens": ["Die", "in", "ein", "Spinn\u00b7ge\u00b7web", "das", "Bild", "der", "Dicht\u00b7kunst", "pr\u00e4gt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Marmor und Albast aus Brust und H\u00e4nden haut,", "tokens": ["Die", "Mar\u00b7mor", "und", "Al\u00b7bast", "aus", "Brust", "und", "H\u00e4n\u00b7den", "haut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Edelstein und Stern aus ihrer Feder sprizt,", "tokens": ["Die", "E\u00b7del\u00b7stein", "und", "Stern", "aus", "ih\u00b7rer", "Fe\u00b7der", "sprizt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die in dem Aug Achat, in Thr\u00e4nen Perlen findt,", "tokens": ["Die", "in", "dem", "Aug", "A\u00b7chat", ",", "in", "Thr\u00e4\u00b7nen", "Per\u00b7len", "findt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "NN", "$,", "APPR", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und aus den Disteln Zeug der Lust zum Schlafrock spinnt,", "tokens": ["Und", "aus", "den", "Dis\u00b7teln", "Zeug", "der", "Lust", "zum", "Schla\u00b7frock", "spinnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "NN", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}