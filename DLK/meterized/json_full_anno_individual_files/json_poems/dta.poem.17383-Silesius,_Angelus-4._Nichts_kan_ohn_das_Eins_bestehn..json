{"dta.poem.17383": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "4.  Nichts kan ohn das Eins bestehn.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Wie all\u2019 und jede zahln ohns eines nicht bestehn;", "tokens": ["Wie", "all'", "und", "je\u00b7de", "zahln", "ohns", "ei\u00b7nes", "nicht", "be\u00b7stehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "KON", "PIAT", "NN", "APPR", "ART", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So m\u00fcssen die Gesch\u00f6pff ohn GOtt das Einsvergehn.", "tokens": ["So", "m\u00fcs\u00b7sen", "die", "Ge\u00b7sch\u00f6pff", "ohn", "Gott", "das", "Eins\u00b7ver\u00b7gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "APPR", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}