{"textgrid.poem.36117": {"metadata": {"author": {"name": "Gloger, Georg", "birth": "N.A.", "death": "N.A."}, "title": "14. So \u00fcberwunden werden ist r\u00fchmlich", "genre": "verse", "period": "N.A.", "pub_year": 1617, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da\u00df ", "tokens": ["Da\u00df"], "token_info": ["word"], "pos": ["KOUS"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "Als K\u00f6nig dir allein, das hat er ihm gethan,", "tokens": ["Als", "K\u00f6\u00b7nig", "dir", "al\u00b7lein", ",", "das", "hat", "er", "ihm", "ge\u00b7than", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPER", "ADV", "$,", "PDS", "VAFIN", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Damit sein hohes Lob nicht etwa m\u00f6cht' erbleichen,", "tokens": ["Da\u00b7mit", "sein", "ho\u00b7hes", "Lob", "nicht", "et\u00b7wa", "m\u00f6cht'", "er\u00b7blei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPOSAT", "ADJA", "NN", "PTKNEG", "ADV", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wenn einem Niedrigern er sich macht' vnterthan.", "tokens": ["Wenn", "ei\u00b7nem", "Nied\u00b7ri\u00b7gern", "er", "sich", "macht'", "vn\u00b7ter\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "PRF", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So, ob er schon erliegt, kan doch mit Rhum er sagen,", "tokens": ["So", ",", "ob", "er", "schon", "er\u00b7liegt", ",", "kan", "doch", "mit", "Rhum", "er", "sa\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PPER", "ADV", "VVPP", "$,", "VMFIN", "ADV", "APPR", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Da\u00df nur ein K\u00f6nig hab' ihn k\u00f6nnen niederschlagen.", "tokens": ["Da\u00df", "nur", "ein", "K\u00f6\u00b7nig", "hab'", "ihn", "k\u00f6n\u00b7nen", "nie\u00b7der\u00b7schla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "VAFIN", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Da\u00df ", "tokens": ["Da\u00df"], "token_info": ["word"], "pos": ["KOUS"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "Als K\u00f6nig dir allein, das hat er ihm gethan,", "tokens": ["Als", "K\u00f6\u00b7nig", "dir", "al\u00b7lein", ",", "das", "hat", "er", "ihm", "ge\u00b7than", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPER", "ADV", "$,", "PDS", "VAFIN", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Damit sein hohes Lob nicht etwa m\u00f6cht' erbleichen,", "tokens": ["Da\u00b7mit", "sein", "ho\u00b7hes", "Lob", "nicht", "et\u00b7wa", "m\u00f6cht'", "er\u00b7blei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPOSAT", "ADJA", "NN", "PTKNEG", "ADV", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wenn einem Niedrigern er sich macht' vnterthan.", "tokens": ["Wenn", "ei\u00b7nem", "Nied\u00b7ri\u00b7gern", "er", "sich", "macht'", "vn\u00b7ter\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"