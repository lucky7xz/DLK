{"textgrid.poem.45883": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "[am Rheine schweb' ich her und hin]", "genre": "verse", "period": "N.A.", "pub_year": 1802, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Am Rheine schweb' ich her und hin", "tokens": ["Am", "Rhei\u00b7ne", "schweb'", "ich", "her", "und", "hin"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NE", "VVFIN", "PPER", "PTKVZ", "KON", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und such' den Fr\u00fchling auf", "tokens": ["Und", "such'", "den", "Fr\u00fch\u00b7ling", "auf"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So schwer mein Herz, so leicht mein Sinn", "tokens": ["So", "schwer", "mein", "Herz", ",", "so", "leicht", "mein", "Sinn"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "PPOSAT", "NN", "$,", "ADV", "ADJD", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wer wiegt sie beide auf.", "tokens": ["Wer", "wiegt", "sie", "bei\u00b7de", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PIS", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die Berge dr\u00e4ngen sich heran,", "tokens": ["Die", "Ber\u00b7ge", "dr\u00e4n\u00b7gen", "sich", "he\u00b7ran", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und lauschen meinem Sang,", "tokens": ["Und", "lau\u00b7schen", "mei\u00b7nem", "Sang", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sirenen schwimmen um den Kahn,", "tokens": ["Si\u00b7re\u00b7nen", "schwim\u00b7men", "um", "den", "Kahn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Mir folget Echoklang.", "tokens": ["Mir", "fol\u00b7get", "E\u00b7cho\u00b7klang", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "O halle nicht, du Widerhall,", "tokens": ["O", "hal\u00b7le", "nicht", ",", "du", "Wi\u00b7der\u00b7hall", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PTKNEG", "$,", "PPER", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "O Berge kehrt zur\u00fcck,", "tokens": ["O", "Ber\u00b7ge", "kehrt", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Gefangen liegt so eng und bang", "tokens": ["Ge\u00b7fan\u00b7gen", "liegt", "so", "eng", "und", "bang"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ADV", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Im Herzen Liebesgl\u00fcck.", "tokens": ["Im", "Her\u00b7zen", "Lie\u00b7bes\u00b7gl\u00fcck", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Sirenen tauchet in die Flut,", "tokens": ["Si\u00b7re\u00b7nen", "tau\u00b7chet", "in", "die", "Flut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Mich f\u00e4ngt nicht Lust nicht Spiel,", "tokens": ["Mich", "f\u00e4ngt", "nicht", "Lust", "nicht", "Spiel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "NN", "PTKNEG", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Aus Wassers K\u00fchle trink' ich Glut,", "tokens": ["Aus", "Was\u00b7sers", "K\u00fch\u00b7le", "trink'", "ich", "Glut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "VVFIN", "PPER", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und ringe froh zum Ziel.", "tokens": ["Und", "rin\u00b7ge", "froh", "zum", "Ziel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "O w\u00e4hnend Lieben, Liebeswahn,", "tokens": ["O", "w\u00e4h\u00b7nend", "Lie\u00b7ben", ",", "Lie\u00b7bes\u00b7wahn", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "APPR", "ADJA", "$,", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Allm\u00e4chtiger Magnet,", "tokens": ["All\u00b7m\u00e4ch\u00b7ti\u00b7ger", "Mag\u00b7net", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Versto\u00dfe nicht des S\u00e4ngers Kahn,", "tokens": ["Ver\u00b7sto\u00b7\u00dfe", "nicht", "des", "S\u00e4n\u00b7gers", "Kahn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der stets nach S\u00fcden geht.", "tokens": ["Der", "stets", "nach", "S\u00fc\u00b7den", "geht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "O Liebesziel so nah so fern,", "tokens": ["O", "Lie\u00b7bes\u00b7ziel", "so", "nah", "so", "fern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ADV", "ADJD", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich hole dich noch ein,", "tokens": ["Ich", "ho\u00b7le", "dich", "noch", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Frommen f\u00fchrt der Morgenstern,", "tokens": ["Die", "From\u00b7men", "f\u00fchrt", "der", "Mor\u00b7gens\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.4": {"text": "Ja all zum Krippelein.", "tokens": ["Ja", "all", "zum", "Krip\u00b7pe\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PIAT", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Geweihtes Kind erl\u00f6se mich,", "tokens": ["Ge\u00b7weih\u00b7tes", "Kind", "er\u00b7l\u00f6\u00b7se", "mich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gieb meine Freude los,", "tokens": ["Gieb", "mei\u00b7ne", "Freu\u00b7de", "los", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "S\u00fc\u00df Bl\u00fcmlein ich erkenne dich,", "tokens": ["S\u00fc\u00df", "Bl\u00fcm\u00b7lein", "ich", "er\u00b7ken\u00b7ne", "dich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "PPER", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Du bl\u00fchest mir mein Los,", "tokens": ["Du", "bl\u00fc\u00b7hest", "mir", "mein", "Los", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "In Fr\u00fchlingsauen sah mein Traum", "tokens": ["In", "Fr\u00fch\u00b7lin\u00b7gsau\u00b7en", "sah", "mein", "Traum"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dich Glockenbl\u00fcmlein stehn,", "tokens": ["Dich", "Glo\u00b7cken\u00b7bl\u00fcm\u00b7lein", "stehn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "NN", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Vom blauen Kelch zum goldnen Saum", "tokens": ["Vom", "blau\u00b7en", "Kelch", "zum", "gold\u00b7nen", "Saum"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hab' ich zu viel gesehn,", "tokens": ["Hab'", "ich", "zu", "viel", "ge\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PTKA", "PIS", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Du blauer Liebeskelch in dich,", "tokens": ["Du", "blau\u00b7er", "Lie\u00b7bes\u00b7kelch", "in", "dich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sank all mein Fr\u00fchling hin,", "tokens": ["Sank", "all", "mein", "Fr\u00fch\u00b7ling", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Vergifte mich, umd\u00fcfte mich,", "tokens": ["Ver\u00b7gif\u00b7te", "mich", ",", "um\u00b7d\u00fcf\u00b7te", "mich", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Weil ich dein eigen bin.", "tokens": ["Weil", "ich", "dein", "ei\u00b7gen", "bin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "ADJD", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Und schlie\u00dfest du den Kelch mir zu", "tokens": ["Und", "schlie\u00b7\u00dfest", "du", "den", "Kelch", "mir", "zu"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "PPER", "PTKZU"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie Blumen abends tun,", "tokens": ["Wie", "Blu\u00b7men", "a\u00b7bends", "tun", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So lasse mich die letzte Ruh',", "tokens": ["So", "las\u00b7se", "mich", "die", "letz\u00b7te", "Ruh'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zu deinen F\u00fc\u00dfen ruhn.", "tokens": ["Zu", "dei\u00b7nen", "F\u00fc\u00b7\u00dfen", "ruhn", "."], "token_info": ["word", "word", "word", "word", "punct"], "