{"textgrid.poem.61225": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Lied des M\u00e4dchens", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "La\u00df schlafen mich und tr\u00e4umen,", "tokens": ["La\u00df", "schla\u00b7fen", "mich", "und", "tr\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "VVFIN", "PPER", "KON", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Was hab' ich zu vers\u00e4umen", "tokens": ["Was", "hab'", "ich", "zu", "ver\u00b7s\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "PPER", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "In dieser Einsamkeit!", "tokens": ["In", "die\u00b7ser", "Ein\u00b7sam\u00b7keit", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Der Reif bedeckt den Garten,", "tokens": ["Der", "Reif", "be\u00b7deckt", "den", "Gar\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Mein Dasein ist ein Warten", "tokens": ["Mein", "Da\u00b7sein", "ist", "ein", "War\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "NN"], "meter": "---+-+-", "measure": "unknown.measure.di"}, "line.6": {"text": "Auf Liebe nur und Lenzeszeit.", "tokens": ["Auf", "Lie\u00b7be", "nur", "und", "Len\u00b7zes\u00b7zeit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Es kommt im Fr\u00fchlingsglanze", "tokens": ["Es", "kommt", "im", "Fr\u00fch\u00b7lings\u00b7glan\u00b7ze"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "F\u00fcr jede kleine Pflanze", "tokens": ["F\u00fcr", "je\u00b7de", "klei\u00b7ne", "Pflan\u00b7ze"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Einmal der Bl\u00fctentag.", "tokens": ["Ein\u00b7mal", "der", "Bl\u00fc\u00b7ten\u00b7tag", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "So wird der Tag auch kommen,", "tokens": ["So", "wird", "der", "Tag", "auch", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Da diesem Frost entnommen", "tokens": ["Da", "die\u00b7sem", "Frost", "ent\u00b7nom\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PDAT", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Mein Herz in Wonnen bl\u00fchen mag.", "tokens": ["Mein", "Herz", "in", "Won\u00b7nen", "bl\u00fc\u00b7hen", "mag."], "token_info": ["word", "word", "word", "word", "word", "abbreviation"], "pos": ["PPOSAT", "NN", "APPR", "NN", "VVFIN", "NE"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Doch bis mir das gegeben,", "tokens": ["Doch", "bis", "mir", "das", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PDS", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Deucht mir nur halb mein Leben", "tokens": ["Deucht", "m