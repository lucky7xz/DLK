{"textgrid.poem.28108": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "23.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich bin von Hertzen feind den runden Samarittern,", "tokens": ["Ich", "bin", "von", "Hert\u00b7zen", "feind", "den", "run\u00b7den", "Sa\u00b7ma\u00b7rit\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die ietzund warm, ietzt kalt, ietzt klar, ietzt tr\u00fcbe wittern.", "tokens": ["Die", "ie\u00b7tzund", "warm", ",", "ietzt", "kalt", ",", "ietzt", "klar", ",", "ietzt", "tr\u00fc\u00b7be", "wit\u00b7tern", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "$,", "ADV", "ADJD", "$,", "ADV", "ADJD", "$,", "ADV", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich bin von Hertzen feind dem Philistiner-Stamm,", "tokens": ["Ich", "bin", "von", "Hert\u00b7zen", "feind", "dem", "Phi\u00b7lis\u00b7ti\u00b7ner\u00b7Stamm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "NN", "ART", "NN", "$,"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Der ihm, wo Recht gebrach, das Schwerdt zu H\u00e4nden nam.", "tokens": ["Der", "ihm", ",", "wo", "Recht", "ge\u00b7brach", ",", "das", "Schwerdt", "zu", "H\u00e4n\u00b7den", "nam", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "$,", "PWAV", "NN", "VVFIN", "$,", "ART", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Am gr\u00e4msten bin ich noch den tollen Sichemiten,", "tokens": ["Am", "gr\u00e4ms\u00b7ten", "bin", "ich", "noch", "den", "tol\u00b7len", "Si\u00b7che\u00b7mi\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "VAFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die sicher in dem Sinn und frevlisch sind an Siten.", "tokens": ["Die", "si\u00b7cher", "in", "dem", "Sinn", "und", "frev\u00b7lisch", "sind", "an", "Si\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "ART", "NN", "KON", "ADJD", "VAFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ich bin von Hertzen feind den runden Samarittern,", "tokens": ["Ich", "bin", "von", "Hert\u00b7zen", "feind", "den", "run\u00b7den", "Sa\u00b7ma\u00b7rit\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die ietzund warm, ietzt kalt, ietzt klar, ietzt tr\u00fcbe wittern.", "tokens": ["Die", "ie\u00b7tzund", "warm", ",", "ietzt", "kalt", ",", "ietzt", "klar", ",", "ietzt", "tr\u00fc\u00b7be", "wit\u00b7tern", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "$,", "ADV", "ADJD", "$,", "ADV", "ADJD", "$,", "ADV", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"tex