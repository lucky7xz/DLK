{"dta.poem.543": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Uber einen Alten/ der ein junges M\u00e4dgen  \n heyrahtet.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "So gehts/ we&#241; ein Alter wil bey den jungen M\u00e4dgens liegen/", "tokens": ["So", "gehts", "/", "we", "&#241;", "ein", "Al\u00b7ter", "wil", "bey", "den", "jun\u00b7gen", "M\u00e4d\u00b7gens", "lie\u00b7gen", "/"], "token_info": ["word", "word", "punct", "word", "XML_entity", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$(", "NE", "$(", "ART", "NN", "VMFIN", "APPR", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+--+--+-+-+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Da\u00df er zu dem Lohne mu\u00df H\u00f6rner auf die Scheitel kriegen", "tokens": ["Da\u00df", "er", "zu", "dem", "Loh\u00b7ne", "mu\u00df", "H\u00f6r\u00b7ner", "auf", "die", "Schei\u00b7tel", "krie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "VMFIN", "NN", "APPR", "ART", "NN", "VVINF"], "meter": "----+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Denn weil er der jungen Frauen nicht das Leder gerben kan/", "tokens": ["Denn", "weil", "er", "der", "jun\u00b7gen", "Frau\u00b7en", "nicht", "das", "Le\u00b7der", "ger\u00b7ben", "kan", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ART", "ADJA", "NN", "PTKNEG", "ART", "NN", "VVINF", "VMFIN", "$("], "meter": "-+--+-+-+-+-+--", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "So nimmt sie zum Zeitvertreib ander gute Tr\u00f6sters an.", "tokens": ["So", "nimmt", "sie", "zum", "Zeit\u00b7ver\u00b7treib", "an\u00b7der", "gu\u00b7te", "Tr\u00f6s\u00b7ters", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN", "ADJD", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+--+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}}}}