{"textgrid.poem.55545": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Das Gl\u00fcck", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du hast uns oft im Traum gesehen", "tokens": ["Du", "hast", "uns", "oft", "im", "Traum", "ge\u00b7se\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "APPRART", "NN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zusammen zum Altare gehen,", "tokens": ["Zu\u00b7sam\u00b7men", "zum", "Al\u00b7ta\u00b7re", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und dich als Frau und mich als Mann.", "tokens": ["Und", "dich", "als", "Frau", "und", "mich", "als", "Mann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "KOUS", "NN", "KON", "PPER", "KOUS", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Oft nahm ich wachend deinem Munde", "tokens": ["Oft", "nahm", "ich", "wa\u00b7chend", "dei\u00b7nem", "Mun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "In einer unbewachten Stunde,", "tokens": ["In", "ei\u00b7ner", "un\u00b7be\u00b7wach\u00b7ten", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Soviel man K\u00fcsse nehmen kann.", "tokens": ["So\u00b7viel", "man", "K\u00fcs\u00b7se", "neh\u00b7men", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "PIS", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Das reinste Gl\u00fcck, das wir empfunden,", "tokens": ["Das", "reins\u00b7te", "Gl\u00fcck", ",", "das", "wir", "emp\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Wollust mancher reichen Stunden", "tokens": ["Die", "Wol\u00b7lust", "man\u00b7cher", "rei\u00b7chen", "Stun\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Floh wie die Zeit mit dem Genu\u00df.", "tokens": ["Floh", "wie", "die", "Zeit", "mit", "dem", "Ge\u00b7nu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Was hilft es mir, da\u00df ich genie\u00dfe?", "tokens": ["Was", "hilft", "es", "mir", ",", "da\u00df", "ich", "ge\u00b7nie\u00b7\u00dfe", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PPER", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wie Tr\u00e4ume fliehn die w\u00e4rmsten K\u00fcsse,", "tokens": ["Wie", "Tr\u00e4u\u00b7me", "fliehn", "die", "w\u00e4rms\u00b7ten", "K\u00fcs\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und alle Freude wie ein Ku\u00df.", "tokens": ["Und", "al\u00