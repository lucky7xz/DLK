{"dta.poem.11767": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "53.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Von menschlichem Geschlecht verlassen stand ein Haus,", "tokens": ["Von", "menschli\u00b7chem", "Ge\u00b7schlecht", "ver\u00b7las\u00b7sen", "stand", "ein", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Vertrieben waren sie daraus von Ratt' und Maus.", "tokens": ["Ver\u00b7trie\u00b7ben", "wa\u00b7ren", "sie", "da\u00b7raus", "von", "Ratt'", "und", "Maus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "PAV", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Da richteten sich ein die M\u00e4use und die Ratten,", "tokens": ["Da", "rich\u00b7te\u00b7ten", "sich", "ein", "die", "M\u00e4u\u00b7se", "und", "die", "Rat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und machten Alles fein, wie sie's am liebsten hatten.", "tokens": ["Und", "mach\u00b7ten", "Al\u00b7les", "fein", ",", "wie", "sie's", "am", "liebs\u00b7ten", "hat\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "ADJD", "$,", "PWAV", "PIS", "PTKA", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Sie sa\u00dfen lange Zeit und f\u00fchlten stark ihr Recht,", "tokens": ["Sie", "sa\u00b7\u00dfen", "lan\u00b7ge", "Zeit", "und", "f\u00fchl\u00b7ten", "stark", "ihr", "Recht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "KON", "VVFIN", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da dr\u00e4ngte sich herein ein anderes Geschlecht.", "tokens": ["Da", "dr\u00e4ng\u00b7te", "sich", "her\u00b7ein", "ein", "an\u00b7de\u00b7res", "Ge\u00b7schlecht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "APZR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Im Sparrwerk nistete sich ein Volk von Eulen;", "tokens": ["Im", "Sparr\u00b7werk", "nis\u00b7te\u00b7te", "sich", "ein", "Volk", "von", "Eu\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PRF", "ART", "NN", "APPR", "NE", "$."], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Mit Pfeifen klagts die Maus dem Schicksal, wie sie heulen.", "tokens": ["Mit", "Pfei\u00b7fen", "klagts", "die", "Maus", "dem", "Schick\u00b7sal", ",", "wie", "sie", "heu\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "ART", "NN", "$,", "PWAV", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Das Heulen war ein Vorspiel nur zum Trauerspiel,", "tokens": ["Das", "Heu\u00b7len", "war", "ein", "Vor\u00b7spiel", "nur", "zum", "Trau\u00b7er\u00b7spiel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bald fra\u00df der Eulenchor die Maus mit Stumpf und Stiel.", "tokens": ["Bald", "fra\u00df", "der", "Eu\u00b7len\u00b7chor", "die", "Maus", "mit", "Stumpf", "und", "Stiel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Behaglich hausten nun im alten Schlo\u00df die Eulen,", "tokens": ["Be\u00b7hag\u00b7lich", "haus\u00b7ten", "nun", "im", "al\u00b7ten", "Schlo\u00df", "die", "Eu\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ADV", "APPRART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da kamen Menschen her und setzten neu die S\u00e4ulen.", "tokens": ["Da", "ka\u00b7men", "Men\u00b7schen", "her", "und", "setz\u00b7ten", "neu", "die", "S\u00e4u\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "PTKVZ", "KON", "VVFIN", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Die Eulen sahen sich aus dem Besitz gesetzt,", "tokens": ["Die", "Eu\u00b7len", "sa\u00b7hen", "sich", "aus", "dem", "Be\u00b7sitz", "ge\u00b7setzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und klagen j\u00e4mmerlich, es sei ihr Recht verletzt.", "tokens": ["Und", "kla\u00b7gen", "j\u00e4m\u00b7mer\u00b7lich", ",", "es", "sei", "ihr", "Recht", "ver\u00b7letzt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "$,", "PPER", "VAFIN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Der Mensch macht sich nichts draus, und wohnt in seinem Haus,", "tokens": ["Der", "Mensch", "macht", "sich", "nichts", "draus", ",", "und", "wohnt", "in", "sei\u00b7nem", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "PIS", "PAV", "$,", "KON", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+---+-+-+-+", "measure": "dactylic.init"}, "line.2": {"text": "Bis wieder ihn daraus wird treiben Ratt' und Maus.", "tokens": ["Bis", "wie\u00b7der", "ihn", "da\u00b7raus", "wird", "trei\u00b7ben", "Ratt'", "und", "Maus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "PPER", "PAV", "VAFIN", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}