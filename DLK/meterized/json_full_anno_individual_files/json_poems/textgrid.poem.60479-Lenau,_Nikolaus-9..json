{"textgrid.poem.60479": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "9.", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Rings ein Verstummen, ein Entf\u00e4rben;", "tokens": ["Rings", "ein", "Ver\u00b7stum\u00b7men", ",", "ein", "Ent\u00b7f\u00e4r\u00b7ben", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie sanft den Wald die L\u00fcfte streicheln,", "tokens": ["Wie", "sanft", "den", "Wald", "die", "L\u00fcf\u00b7te", "strei\u00b7cheln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sein welkes Laub ihm abzuschmeicheln;", "tokens": ["Sein", "wel\u00b7kes", "Laub", "ihm", "ab\u00b7zu\u00b7schmei\u00b7cheln", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PPER", "VVIZU", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ich liebe dieses milde Sterben.", "tokens": ["Ich", "lie\u00b7be", "die\u00b7ses", "mil\u00b7de", "Ster\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Von hinnen geht die stille Reise,", "tokens": ["Von", "hin\u00b7nen", "geht", "die", "stil\u00b7le", "Rei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Zeit der Liebe ist verklungen,", "tokens": ["Die", "Zeit", "der", "Lie\u00b7be", "ist", "ver\u00b7klun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die V\u00f6gel haben ausgesungen,", "tokens": ["Die", "V\u00f6\u00b7gel", "ha\u00b7ben", "aus\u00b7ge\u00b7sun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und d\u00fcrre Bl\u00e4tter sinken leise.", "tokens": ["Und", "d\u00fcr\u00b7re", "Bl\u00e4t\u00b7ter", "sin\u00b7ken", "lei\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Die V\u00f6gel zogen nach dem S\u00fcden", "tokens": ["Die", "V\u00f6\u00b7gel", "zo\u00b7gen", "nach", "dem", "S\u00fc\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Aus dem Verfall des Laubes tauchen", "tokens": ["Aus", "dem", "Ver\u00b7fall", "des", "Lau\u00b7bes", "tau\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die Nester, die nicht Schutz mehr brauchen,", "tokens": ["Die", "Nes\u00b7ter", ",", "die", "nicht", "Schutz", "mehr", "brau\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PTKNEG", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Bl\u00e4tter fallen stets, die m\u00fcden.", "tokens"