{"dta.poem.990": {"metadata": {"author": {"name": "George, Stefan", "birth": "N.A.", "death": "N.A."}, "title": "L. K.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1897", "urn": "urn:nbn:de:kobv:b4-200905191530", "language": ["de:0.99"], "booktitle": "George, Stefan: Das Jahr der Seele. Berlin, 1897."}, "poem": {"stanza.1": {"line.1": {"text": "Doch unser aller heimat bleibt das licht                 ", "tokens": ["Doch", "un\u00b7ser", "al\u00b7ler", "hei\u00b7mat", "bleibt", "das", "licht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "PIAT", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Zu dem wir kehren auf verschlungnen stegen &#183;                 ", "tokens": ["Zu", "dem", "wir", "keh\u00b7ren", "auf", "ver\u00b7schlung\u00b7nen", "ste\u00b7gen", "&#183;"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "XML_entity"], "pos": ["APPR", "PRELS", "PPER", "VVFIN", "APPR", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Magst du dich einig nennen mit den recken                 ", "tokens": ["Magst", "du", "dich", "ei\u00b7nig", "nen\u00b7nen", "mit", "den", "re\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PRF", "ADJD", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und trotzigen gewalten bracher ebnen &#8212;                 ", "tokens": ["Und", "trot\u00b7zi\u00b7gen", "ge\u00b7wal\u00b7ten", "bra\u00b7cher", "eb\u00b7nen", "&#8212;"], "token_info": ["word", "word", "word", "word", "word", "XML_entity"], "pos": ["KON", "ADJA", "ADJA", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Sagt nicht bei jedem treffen die umschlingung                 ", "tokens": ["Sagt", "nicht", "bei", "je\u00b7dem", "tref\u00b7fen", "die", "um\u00b7schlin\u00b7gung"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PTKNEG", "APPR", "PIS", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und dass ich oft dich suche wie du viel                 ", "tokens": ["Und", "dass", "ich", "oft", "dich", "su\u00b7che", "wie", "du", "viel"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "ADV", "PPER", "VVFIN", "KOKOM", "PPER", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "In mir erregst und mir geh\u00f6rst? verr&#228;t nicht                 ", "tokens": ["In", "mir", "er\u00b7regst", "und", "mir", "ge\u00b7h\u00f6rst", "?", "verr", "&#228;", "t", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "XML_entity", "word", "word"], "pos": ["APPR", "PPER", "VVFIN", "KON", "PPER", "VVPP", "$.", "NN", "$(", "VVFIN", "PTKNEG"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Dass du mich fliehst wie sehr ich in dir bin?               ", "tokens": ["Dass", "du", "mich", "fliehst", "wie", "sehr", "ich", "in", "dir", "bin", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVFIN", "KOKOM", "ADV", "PPER", "APPR", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}