{"dta.poem.19447": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Schillers Bestattung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-200905193933", "language": ["de:0.99"], "booktitle": "Meyer, Conrad Ferdinand: Gedichte. Leipzig, 1882."}, "poem": {"stanza.1": {"line.1": {"text": "Ein \u00e4rmlich d\u00fcster brennend Fackelpaar, das Sturm", "tokens": ["Ein", "\u00e4rm\u00b7lich", "d\u00fcs\u00b7ter", "bren\u00b7nend", "Fa\u00b7ckel\u00b7paar", ",", "das", "Sturm"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJD", "ADJD", "VVPP", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und Regen jeden Augenblick zu l\u00f6schen droht.", "tokens": ["Und", "Re\u00b7gen", "je\u00b7den", "Au\u00b7gen\u00b7blick", "zu", "l\u00f6\u00b7schen", "droht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PIAT", "NN", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ein flatternd Bahrtuch. Ein gemeiner Tannensarg", "tokens": ["Ein", "flat\u00b7ternd", "Bahr\u00b7tuch", ".", "Ein", "ge\u00b7mei\u00b7ner", "Tan\u00b7nen\u00b7sarg"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJD", "NN", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mit keinem Kranz, dem kargsten nicht, und kein Geleit!", "tokens": ["Mit", "kei\u00b7nem", "Kranz", ",", "dem", "kargs\u00b7ten", "nicht", ",", "und", "kein", "Ge\u00b7leit", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "PRELS", "VVFIN", "PTKNEG", "$,", "KON", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Als br\u00e4chte eilig einen Frevel man zu Grab.", "tokens": ["Als", "br\u00e4ch\u00b7te", "ei\u00b7lig", "ei\u00b7nen", "Fre\u00b7vel", "man", "zu", "Grab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "ADJD", "ART", "NN", "PIS", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die Tr\u00e4ger hasteten. Ein Unbekannter nur,", "tokens": ["Die", "Tr\u00e4\u00b7ger", "has\u00b7te\u00b7ten", ".", "Ein", "Un\u00b7be\u00b7kann\u00b7ter", "nur", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Von eines weiten Mantels k\u00fchnem Schwung umweht,", "tokens": ["Von", "ei\u00b7nes", "wei\u00b7ten", "Man\u00b7tels", "k\u00fch\u00b7nem", "Schwung", "um\u00b7weht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Schritt dieser Bahre nach. Der Menschheit Genius war's.", "tokens": ["Schritt", "die\u00b7ser", "Bah\u00b7re", "nach", ".", "Der", "Menschheit", "Ge\u00b7nius", "wa\u00b7r'", "s."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "abbreviation"], "pos": ["NN", "PDAT", "NN", "PTKVZ", "$.", "ART", "NN", "NE", "VAFIN", "NE"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}