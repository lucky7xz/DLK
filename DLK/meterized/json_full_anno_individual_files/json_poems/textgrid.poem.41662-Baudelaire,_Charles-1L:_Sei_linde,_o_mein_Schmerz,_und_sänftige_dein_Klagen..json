{"textgrid.poem.41662": {"metadata": {"author": {"name": "Baudelaire, Charles", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sei linde, o mein Schmerz, und s\u00e4nftige dein Klagen.", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sei linde, o mein Schmerz, und s\u00e4nftige dein Klagen.", "tokens": ["Sei", "lin\u00b7de", ",", "o", "mein", "Schmerz", ",", "und", "s\u00e4nf\u00b7ti\u00b7ge", "dein", "Kla\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "$,", "FM", "PPOSAT", "NN", "$,", "KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den Abend riefest du, schau seine Wiederkehr:", "tokens": ["Den", "A\u00b7bend", "rie\u00b7fest", "du", ",", "schau", "sei\u00b7ne", "Wie\u00b7der\u00b7kehr", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Aus seinem tr\u00fcben Dunst siehst du die Stadt entragen,", "tokens": ["Aus", "sei\u00b7nem", "tr\u00fc\u00b7ben", "Dunst", "siehst", "du", "die", "Stadt", "ent\u00b7ra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Den einen ist er s\u00fc\u00df, den andern sorgenschwer.", "tokens": ["Den", "ei\u00b7nen", "ist", "er", "s\u00fc\u00df", ",", "den", "an\u00b7dern", "sor\u00b7gen\u00b7schwer", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "