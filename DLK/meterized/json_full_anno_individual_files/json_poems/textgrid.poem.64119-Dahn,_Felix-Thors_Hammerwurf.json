{"textgrid.poem.64119": {"metadata": {"author": {"name": "Dahn, Felix", "birth": "N.A.", "death": "N.A."}, "title": "Thors Hammerwurf", "genre": "verse", "period": "N.A.", "pub_year": 1873, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Thor stand am Mitternachtende der Welt,", "tokens": ["Thor", "stand", "am", "Mit\u00b7ter\u00b7nach\u00b7ten\u00b7de", "der", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Streitaxt warf er, die schwere:", "tokens": ["Die", "Strei\u00b7taxt", "warf", "er", ",", "die", "schwe\u00b7re", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,", "ART", "ADJA", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "\u00bbso weit der sausende Hammer f\u00e4llt,", "tokens": ["\u00bb", "so", "weit", "der", "sau\u00b7sen\u00b7de", "Ham\u00b7mer", "f\u00e4llt", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ADJD", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Sind mein das Land und die Meere!\u00ab \u2013", "tokens": ["Sind", "mein", "das", "Land", "und", "die", "Mee\u00b7re", "!", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["VAFIN", "PPOSAT", "ART", "NN", "KON", "ART", "NN", "$.", "$(", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Und es flog der Hammer aus seiner Hand,", "tokens": ["Und", "es", "flog", "der", "Ham\u00b7mer", "aus", "sei\u00b7ner", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word",