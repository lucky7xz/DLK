{"dta.poem.15689": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "22.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die H\u00f6flichkeit, o Sohn, ist so vom Hof benannt,", "tokens": ["Die", "H\u00f6f\u00b7lich\u00b7keit", ",", "o", "Sohn", ",", "ist", "so", "vom", "Hof", "be\u00b7nannt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "FM", "NN", "$,", "VAFIN", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und f\u00fcr der Wahrheit Schul' ist nicht der Hof bekannt.", "tokens": ["Und", "f\u00fcr", "der", "Wahr\u00b7heit", "Schul'", "ist", "nicht", "der", "Hof", "be\u00b7kannt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "NN", "VAFIN", "PTKNEG", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die H\u00f6flichkeit hat nie, gib auf dich selbst nur Acht,", "tokens": ["Die", "H\u00f6f\u00b7lich\u00b7keit", "hat", "nie", ",", "gib", "auf", "dich", "selbst", "nur", "Acht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "$,", "VVIMP", "APPR", "PPER", "ADV", "ADV", "CARD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein v\u00f6llig wahres Wort, o Sohn, hervorgebracht.", "tokens": ["Ein", "v\u00f6l\u00b7lig", "wah\u00b7res", "Wort", ",", "o", "Sohn", ",", "her\u00b7vor\u00b7ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "$,", "FM", "NN", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Unwahres spricht sie nicht, doch wei\u00df sie einzukleiden", "tokens": ["Un\u00b7wah\u00b7res", "spricht", "sie", "nicht", ",", "doch", "wei\u00df", "sie", "ein\u00b7zu\u00b7klei\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "PTKNEG", "$,", "ADV", "VVFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den Stolz der Wahrheit so, da\u00df er sieht aus bescheiden.", "tokens": ["Den", "Stolz", "der", "Wahr\u00b7heit", "so", ",", "da\u00df", "er", "sieht", "aus", "be\u00b7schei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "ADV", "$,", "KOUS", "PPER", "VVFIN", "APPR", "ADJA", "$."], "meter": "-+-+-+---+-+-", "measure": "unknown.measure.penta"}}}}}