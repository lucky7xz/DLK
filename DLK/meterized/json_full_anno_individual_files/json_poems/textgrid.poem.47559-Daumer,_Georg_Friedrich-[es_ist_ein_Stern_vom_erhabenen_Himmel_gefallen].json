{"textgrid.poem.47559": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[es ist ein Stern vom erhabenen Himmel gefallen]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es ist ein Stern vom erhabenen Himmel gefallen,", "tokens": ["Es", "ist", "ein", "Stern", "vom", "er\u00b7ha\u00b7be\u00b7nen", "Him\u00b7mel", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPRART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+--+--+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Herab in's irdische tolle Get\u00fcmmel gefallen.", "tokens": ["Her\u00b7ab", "in's", "ir\u00b7di\u00b7sche", "tol\u00b7le", "Ge\u00b7t\u00fcm\u00b7mel", "ge\u00b7fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+--+--+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Da sah er umher die Kr\u00e4uter und Blumen der Wiese;", "tokens": ["Da", "sah", "er", "um\u00b7her", "die", "Kr\u00e4u\u00b7ter", "und", "Blu\u00b7men", "der", "Wie\u00b7se", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "ART", "NN", "KON", "NN", "ART", "NN", "$."], "meter": "-+--+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ihm hat das lustige, bunte Gewimmel gefallen.", "tokens": ["Ihm", "hat", "das", "lus\u00b7ti\u00b7ge", ",", "bun\u00b7te", "Ge\u00b7wim\u00b7mel", "ge\u00b7fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "$,", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+--+--+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Er h\u00f6rte die Gl\u00f6ckchen am Halse der Herde l\u00e4uten;", "tokens": ["Er", "h\u00f6r\u00b7te", "die", "Gl\u00f6ck\u00b7chen", "am", "Hal\u00b7se", "der", "Her\u00b7de", "l\u00e4u\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+--+--+--+-+-", "measure": "amphibrach.tetra.plus"}, "line.2": {"text": "Ihm hat das klingende, kleine Gebimmel gefallen.", "tokens": ["Ihm", "hat", "das", "klin\u00b7gen\u00b7de", ",", "klei\u00b7ne", "Ge\u00b7bim\u00b7mel", "ge\u00b7fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "$,", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+--+--+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Er sah, wie ein Ro\u00df leicht \u00fcber die Heide dahinflog;", "tokens": ["Er", "sah", ",", "wie", "ein", "Ro\u00df", "leicht", "\u00fc\u00b7ber", "die", "Hei\u00b7de", "da\u00b7hin\u00b7flog", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "ART", "NN", "ADJD", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+--+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Ihm hat der herrliche, fliegende Schimmel gefallen.", "tokens": ["Ihm", "hat", "der", "herr\u00b7li\u00b7che", ",", "flie\u00b7gen\u00b7de", "Schim\u00b7mel", "ge\u00b7fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "$,", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+--+--+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.5": {"line.1": {"text": "Ihm haben die traulichen H\u00fctten, die laubigen