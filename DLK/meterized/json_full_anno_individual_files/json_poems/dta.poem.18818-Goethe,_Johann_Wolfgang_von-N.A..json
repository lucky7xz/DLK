{"dta.poem.18818": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang von", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1819", "urn": "urn:nbn:de:kobv:b4-200905191692", "language": ["de:0.99"], "booktitle": "Goethe, Johann Wolfgang von: West-\u00f6stlicher Divan. Stuttgart, 1819."}, "poem": {"stanza.1": {"line.1": {"text": "Ob der Koran von Ewigkeit sey?", "tokens": ["Ob", "der", "Ko\u00b7ran", "von", "E\u00b7wig\u00b7keit", "sey", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPR", "NN", "VAFIN", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Darnach frag\u2019 ich nicht!", "tokens": ["Dar\u00b7nach", "frag'", "ich", "nicht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Ob der Koran geschaffen sey?", "tokens": ["Ob", "der", "Ko\u00b7ran", "ge\u00b7schaf\u00b7fen", "sey", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das weiss ich nicht!", "tokens": ["Das", "weiss", "ich", "nicht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Dass er das Buch der B\u00fccher sey", "tokens": ["Dass", "er", "das", "Buch", "der", "B\u00fc\u00b7cher", "sey"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "ART", "NN", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Glaub\u2019 ich aus Mosleminen-Pflicht.", "tokens": ["Glaub'", "ich", "aus", "Mos\u00b7le\u00b7mi\u00b7nen\u00b7Pflicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Dass aber der Wein von Ewigkeit sey", "tokens": ["Dass", "a\u00b7ber", "der", "Wein", "von", "E\u00b7wig\u00b7keit", "sey"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "NN", "APPR", "NN", "VAFIN"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Daran zweifl\u2019 ich nicht.", "tokens": ["Da\u00b7ran", "zweifl'", "ich", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "--+-+", "measure": "anapaest.init"}, "line.9": {"text": "Oder dass er vor den Engeln geschaffen sey", "tokens": ["O\u00b7der", "dass", "er", "vor", "den", "En\u00b7geln", "ge\u00b7schaf\u00b7fen", "sey"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "APPR", "ART", "NN", "VVPP", "VAFIN"], "meter": "--+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "Ist vielleicht auch kein Gedicht.", "tokens": ["Ist", "viel\u00b7leicht", "auch", "kein", "Ge\u00b7dicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Der Trinkende, wie es auch immer sey,", "tokens": ["Der", "Trin\u00b7ken\u00b7de", ",", "wie", "es", "auch", "im\u00b7mer", "sey", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "PPER", "ADV", "ADV", "VAFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.12": {"text": "Blickt Gott frischer ins Angesicht.", "tokens": ["Blickt", "Gott", "fri\u00b7scher", "ins", "An\u00b7ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}