{"textgrid.poem.42936": {"metadata": {"author": {"name": "Ringelnatz, Joachim", "birth": "N.A.", "death": "N.A."}, "title": "Frucht-Zucht-Frucht", "genre": "verse", "period": "N.A.", "pub_year": 1908, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bananen, Melonen, Ananas \u2013 \u2013.", "tokens": ["Ba\u00b7na\u00b7nen", ",", "Me\u00b7lo\u00b7nen", ",", "A\u00b7na\u00b7nas", "\u2013", "\u2013", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct", "punct", "punct"], "pos": ["NN", "$,", "NN", "$,", "NE", "$(", "$(", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Alle Fr\u00fcchte haben etwas \u2013", "tokens": ["Al\u00b7le", "Fr\u00fcch\u00b7te", "ha\u00b7ben", "et\u00b7was", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "ADV", "$("], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.3": {"text": "Frei gesagt: Unanst\u00e4ndiges,", "tokens": ["Frei", "ge\u00b7sagt", ":", "Un\u00b7an\u00b7st\u00e4n\u00b7di\u00b7ges", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADJD", "VVPP", "$.", "ADJA", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Etwas Nudit\u00e4tes an sich.", "tokens": ["Et\u00b7was", "Nu\u00b7di\u00b7t\u00e4\u00b7tes", "an", "sich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "APPR", "PRF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Dar\u00fcber freue ich mich.", "tokens": ["Da\u00b7r\u00fc\u00b7ber", "freu\u00b7e", "ich", "mich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PRF", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "Denn das ist etwas Unb\u00e4ndiges.", "tokens": ["Denn", "das", "ist", "et\u00b7was", "Un\u00b7b\u00e4n\u00b7di\u00b7ges", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "ADV", "ADJA", "$."], "meter": "-+-+-+---", "measure": "unknown.measure.tri"}, "line.7": {"text": "Instinktiv oder auch bewu\u00dft", "tokens": ["Ins\u00b7tink\u00b7tiv", "o\u00b7der", "auch", "be\u00b7wu\u00dft"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "KON", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Haben wir alle daran unsre Lust.", "tokens": ["Ha\u00b7ben", "wir", "al\u00b7le", "da\u00b7ran", "uns\u00b7re", "Lust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PIS", "PAV", "PPOSAT", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}, "stanza.2": {"line.1": {"text": "Aber die dar\u00fcber erschreckt sind,", "tokens": ["A\u00b7ber", "die", "da\u00b7r\u00fc\u00b7ber", "er\u00b7schreckt", "sind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PAV", "VVPP", "VAFIN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Sich entr\u00fcsten und jemand verklagen,", "tokens": ["Sich", "ent\u00b7r\u00fcs\u00b7ten", "und", "je\u00b7mand", "ver\u00b7kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "KON", "PIS", "VVINF", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Denen wollen wir andere sagen,", "tokens": ["De\u00b7nen", "wol\u00b7len", "wir", "an\u00b7de\u00b7re", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "PIS", "VVINF", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Da\u00df wir schon lang nicht mehr a.A. geleckt sind.", "tokens": ["Da\u00df", "wir", "schon", "lang", "nicht", "mehr", "a.", "A.", "ge\u00b7leckt", "sind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "abbreviation", "abbreviation", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "PTKNEG", "ADV", "APPRART", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und das mu\u00df \u2013 wenn auch nur theoretisch \u2013", "tokens": ["Und", "das", "mu\u00df", "\u2013", "wenn", "auch", "nur", "the\u00b7o\u00b7re\u00b7tisch", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VMFIN", "$(", "KOUS", "ADV", "ADV", "ADJD", "$("], "meter": "-+-+-+-++-", "measure": "unknown.measure.penta"}, "line.6": {"text": "Immer mal wieder auf Erden geschehn.", "tokens": ["Im\u00b7mer", "mal", "wie\u00b7der", "auf", "Er\u00b7den", "ge\u00b7schehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "APPR", "NN", "VVPP", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.7": {"text": "Sonst werden wir Mehlbrei und hyper\u00e4sthetisch", "tokens": ["Sonst", "wer\u00b7den", "wir", "Mehl\u00b7brei", "und", "hy\u00b7per\u00b7\u00e4s\u00b7the\u00b7tisch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "NN", "KON", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und werden rot, wenn wir Pfirsiche sehn.", "tokens": ["Und", "wer\u00b7den", "rot", ",", "wenn", "wir", "Pfir\u00b7si\u00b7che", "sehn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADJD", "$,", "KOUS", "PPER", "NN", "VVINF", "$."], "meter": "-+-+-++--+", "measure": "iambic.penta.chol"}}, "stanza.3": {"line.1": {"text": "Bananen, Melonen, Ananas \u2013 \u2013.", "tokens": ["Ba\u00b7na\u00b7nen", ",", "Me\u00b7lo\u00b7nen", ",", "A\u00b7na\u00b7nas", "\u2013", "\u2013", "."], "token_info": ["word", "punct", "word", "punct", "word