{"textgrid.poem.33640": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "Die Schiffersfrau", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wir sahn dem Schiff am Ufer nach,", "tokens": ["Wir", "sahn", "dem", "Schiff", "am", "U\u00b7fer", "nach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bis Wind die Segel fingen,", "tokens": ["Bis", "Wind", "die", "Se\u00b7gel", "fin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Bis \u00fcber die See das Dunkel brach", "tokens": ["Bis", "\u00fc\u00b7ber", "die", "See", "das", "Dun\u00b7kel", "brach"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "APPR", "ART", "NN", "ART", "NN", "VVFIN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und die Augen \u00fcbergingen,", "tokens": ["Und", "die", "Au\u00b7gen", "\u00fc\u00b7ber\u00b7gin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Dann kehrten wir heim, allein und zerstreut,", "tokens": ["Dann", "kehr\u00b7ten", "wir", "heim", ",", "al\u00b7lein", "und", "zer\u00b7streut", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$,", "ADV", "KON", "VVPP", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Wir Frauen und T\u00f6chter der Schifferleut'.", "t