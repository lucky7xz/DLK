{"textgrid.poem.24121": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die roten Tulpenflammen sind vergl\u00fcht;", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die roten Tulpenflammen sind vergl\u00fcht;", "tokens": ["Die", "ro\u00b7ten", "Tul\u00b7pen\u00b7flam\u00b7men", "sind", "ver\u00b7gl\u00fcht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Maiglocken wachen auf; der Flieder bl\u00fcht;", "tokens": ["Mai\u00b7glo\u00b7cken", "wa\u00b7chen", "auf", ";", "der", "Flie\u00b7der", "bl\u00fcht", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PTKVZ", "$.", "ART", "NN", "VVFIN", "$."], "meter": "++-+-+-+-+", "measure": "iambic.penta.spondeus"}, "line.3": {"text": "Die Eiche, die so lange sich besann,", "tokens": ["Die", "Ei\u00b7che", ",", "die", "so", "lan\u00b7ge", "sich", "be\u00b7sann", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "ADV", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Steht nun in Laub; es steckt die Kerzen an,", "tokens": ["Steht", "nun", "in", "Laub", ";", "es", "steckt", "die", "Ker\u00b7zen", "an", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "NN", "$.", "PPER", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Die gr\u00fcnen Kerzen, \u00fcbertrieft von Saft,", "tokens": ["Die", "gr\u00fc\u00b7nen", "Ker\u00b7zen", ",", "\u00fc\u00b7bert\u00b7rieft", "v