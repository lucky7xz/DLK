{"textgrid.poem.46711": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[wenn die Sonne lustanregend scheinet]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn die Sonne lustanregend scheinet,", "tokens": ["Wenn", "die", "Son\u00b7ne", "lus\u00b7tan\u00b7re\u00b7gend", "schei\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Seufz' ich wie aus dunkler Haft:", "tokens": ["Seuf\u00b7z'", "ich", "wie", "aus", "dunk\u00b7ler", "Haft", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KOKOM", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ach, die Wonne, die dein Glutblick meinet,", "tokens": ["Ach", ",", "die", "Won\u00b7ne", ",", "die", "dein", "Glut\u00b7blick", "mei\u00b7net", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Da\u00df dazu mir fehlt die Kraft!", "tokens": ["Da\u00df", "da\u00b7zu", "mir", "fehlt", "die", "Kraft", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PAV", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}, "stanza.2": {"line.1": {"text": "Nicht ein Etwas, mir erst jetzt genommen,", "tokens": ["Nicht", "ein", "Et\u00b7was", ",", "mir", "erst", "jetzt", "ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "ADV", "$,", "PPER", "ADV", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Und dem Tode zugez\u00e4hlt,", "tokens": ["Und", "dem", "To\u00b7de", "zu\u00b7ge\u00b7z\u00e4hlt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Zum Bewu\u00dftsein mir nur jetzt gekommen,", "tokens": ["Zum", "Be\u00b7wu\u00df\u00b7tsein", "mir", "nur", "jetzt", "ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "ADV", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Da\u00df es immer mir gefehlt.", "tokens": ["Da\u00df", "es", "im\u00b7mer", "mir", "ge\u00b7fehlt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Sonne, du bist wie im Anbeginne", "tokens": ["Son\u00b7ne", ",", "du", "bist", "wie", "im", "An\u00b7be\u00b7gin\u00b7ne"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PPER", "VAFIN", "KOKOM", "APPRART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Frischgeaugt und freudenjung;", "tokens": ["Frischge\u00b7augt", "und", "freu\u00b7den\u00b7jung", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und kein Herz kann's mit erkranktem Sinne", "tokens": ["Und", "kein", "Herz", "kann's", "mit", "er\u00b7krank\u00b7tem", "Sin\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "VMFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Nachthun Deinem Freude