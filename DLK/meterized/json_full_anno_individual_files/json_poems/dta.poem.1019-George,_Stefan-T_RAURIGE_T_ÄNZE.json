{"dta.poem.1019": {"metadata": {"author": {"name": "George, Stefan", "birth": "N.A.", "death": "N.A."}, "title": "T RAURIGE   T \u00c4NZE", "genre": "Lyrik", "period": "N.A.", "pub_year": "1897", "urn": "urn:nbn:de:kobv:b4-200905191530", "language": ["de:0.99"], "booktitle": "George, Stefan: Das Jahr der Seele. Berlin, 1897."}, "poem": {"stanza.1": {"line.1": {"text": "Wie in der gruft die alte               ", "tokens": ["Wie", "in", "der", "gruft", "die", "al\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "VVFIN", "ART", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Lebendige ampel gl&#252;ht               ", "tokens": ["Le\u00b7ben\u00b7di\u00b7ge", "am\u00b7pel", "gl", "&#252;", "ht"], "token_info": ["word", "word", "word", "XML_entity", "word"], "pos": ["FM.la", "FM.la", "FM.la", "$(", "VVFIN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Wie ihr karfunkel spr&#252;ht               ", "tokens": ["Wie", "ihr", "kar\u00b7fun\u00b7kel", "spr", "&#252;", "ht"], "token_info": ["word", "word", "word", "word", "XML_entity", "word"], "pos": ["PWAV", "PPER", "ADJD", "VVFIN", "$(", "VVFIN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Um schauernde basalte!             ", "tokens": ["Um", "schau\u00b7ern\u00b7de", "ba\u00b7sal\u00b7te", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUI", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Vom runden fenster droben               ", "tokens": ["Vom", "run\u00b7den", "fens\u00b7ter", "dro\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Entfliesst der ganze glanz               ", "tokens": ["Ent\u00b7fliesst", "der", "gan\u00b7ze", "glanz"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Von feuriger monstranz               ", "tokens": ["Von", "feu\u00b7ri\u00b7ger", "mons\u00b7tranz"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Mit goldumreiften globen             ", "tokens": ["Mit", "gold\u00b7um\u00b7reif\u00b7ten", "glo\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und einem weissen lamme &#8212;               ", "tokens": ["Und", "ei\u00b7nem", "weis\u00b7sen", "lam\u00b7me", "&#8212;"], "token_info": ["word", "word", "word", "word", "XML_entity"], "pos": ["KON", "ART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und wenn die ampel gl&#252;ht               ", "tokens": ["Und", "wenn", "die", "am\u00b7pel", "gl", "&#252;", "ht"], "token_info": ["word", "word", "word", "word", "word", "XML_entity", "word"], "pos": ["KON", "KOUS", "ART", "NN", "NE", "$(", "VVFIN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und wenn ihr kleinod spr&#252;ht               ", "tokens": ["Und", "wenn", "ihr", "klei\u00b7nod", "spr", "&#252;", "ht"], "token_info": ["word", "word", "word", "word", "word", "XML_entity", "word"], "pos": ["KON", "KOUS", "PPOSAT", "NN", "VVFIN", "$(", "VVFIN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Ist es von eigner flamme?             ", "tokens": ["Ist", "es", "von", "eig\u00b7ner", "flam\u00b7me", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}