{"dta.poem.7154": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Betrachtung des Schlafs,  \n als  \n eine G\u00f6ttliche Wohlthat,  \n bey dem 1728sten Jahres-Wechsel.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie f\u00fcll\u2019test Du das Land mit Fr\u00fcchten, Wein und Korn", "tokens": ["Wie", "f\u00fcll'\u00b7test", "Du", "das", "Land", "mit", "Fr\u00fcch\u00b7ten", ",", "Wein", "und", "Korn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "ART", "NN", "APPR", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit Obst: mit Wild so gar;", "tokens": ["Mit", "Obst", ":", "mit", "Wild", "so", "gar", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$.", "APPR", "NE", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df auch das fette Jahr,", "tokens": ["Da\u00df", "auch", "das", "fet\u00b7te", "Jahr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Das drey und zwanzigste noch kaum so fruchtbar war.", "tokens": ["Das", "drey", "und", "zwan\u00b7zigs\u00b7te", "noch", "kaum", "so", "frucht\u00b7bar", "war", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "CARD", "KON", "VVFIN", "ADV", "ADV", "ADV", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Es hat an Trauben ja das Holtz von einer Reben", "tokens": ["Es", "hat", "an", "Trau\u00b7ben", "ja", "das", "Holtz", "von", "ei\u00b7ner", "Re\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "NN", "ADV", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Vierhundert Sechszehn mir gegeben:", "tokens": ["Vier\u00b7hun\u00b7dert", "Sechs\u00b7zehn", "mir", "ge\u00b7ge\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "CARD", "PPER", "VVPP", "$."], "meter": "++-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Ja allenthalben hat der Weinstock viel gebracht,", "tokens": ["Ja", "al\u00b7len\u00b7thal\u00b7ben", "hat", "der", "Wein\u00b7stock", "viel", "ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADV", "VAFIN", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So da\u00df man gar in Hamburg Wein gemacht.", "tokens": ["So", "da\u00df", "man", "gar", "in", "Ham\u00b7burg", "Wein", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PIS", "ADV", "APPR", "NE", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Wie wolfeil war das Fleisch, sowol was zahm, als wild:", "tokens": ["Wie", "wol\u00b7feil", "war", "das", "Fleisch", ",", "so\u00b7wol", "was", "zahm", ",", "als", "wild", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "ART", "NN", "$,", "KON", "PWS", "VVFIN", "$,", "KOUS", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Man konnte Ochsen-Fleisch um wenig Pfennig\u2019 heuer,", "tokens": ["Man", "konn\u00b7te", "Och\u00b7sen\u00b7Fleisch", "um", "we\u00b7nig", "Pfen\u00b7nig'", "heu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "NN", "APPR", "PIAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Und ein Pfund Schweine-Fleisch um eben so viel kauffen,", "tokens": ["Und", "ein", "Pfund", "Schwei\u00b7ne\u00b7Fleisch", "um", "e\u00b7ben", "so", "viel", "kauf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "APPR", "ADV", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "War mit Gefl\u00fcgel nicht die Lufft so sehr erf\u00fcllt?", "tokens": ["War", "mit", "Ge\u00b7fl\u00fc\u00b7gel", "nicht", "die", "Lufft", "so", "sehr", "er\u00b7f\u00fcllt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "NN", "PTKNEG", "ART", "NN", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Von Srammets-V\u00f6geln galt ein Hauffen", "tokens": ["Von", "Sram\u00b7mets\u00b7V\u00f6\u00b7geln", "galt", "ein", "Hauf\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Von zwantzig St\u00fcck, zwey Groschen und zwey Dreyer.", "tokens": ["Von", "zwant\u00b7zig", "St\u00fcck", ",", "zwey", "Gro\u00b7schen", "und", "zwey", "Drey\u00b7er", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "$,", "CARD", "NN", "KON", "CARD", "NE", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}