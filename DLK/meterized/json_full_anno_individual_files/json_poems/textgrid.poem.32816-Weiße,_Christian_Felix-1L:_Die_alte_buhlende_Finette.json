{"textgrid.poem.32816": {"metadata": {"author": {"name": "Wei\u00dfe, Christian Felix", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die alte buhlende Finette", "genre": "verse", "period": "N.A.", "pub_year": 1765, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die alte buhlende Finette", "tokens": ["Die", "al\u00b7te", "buh\u00b7len\u00b7de", "Fi\u00b7net\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN"], "meter": "-+-+---+-", "measure": "unknown.measure.tri"}, "line.2": {"text": "Fragt einst, ich war allein mit ihr;", "tokens": ["Fragt", "einst", ",", "ich", "war", "al\u00b7lein", "mit", "ihr", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "PPER", "VAFIN", "ADV", "APPR", "PPOSAT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ob ich ihr nichts zu sagen h\u00e4tte?", "tokens": ["Ob", "ich", "ihr", "nichts", "zu", "sa\u00b7gen", "h\u00e4t\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "PIS", "PTKZU", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "\u00bbich? Nichts \u2013 \u2013 als \u2013 \u2013 Sie gefallen mir.\u00ab", "tokens": ["\u00bb", "ich", "?", "Nichts", "\u2013", "\u2013", "als", "\u2013", "\u2013", "Sie", "ge\u00b7fal\u00b7len", "mir", ".", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "punct", "punct", "word", "punct", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "$.", "PIS", "$(", "$(", "KOUS", "$(", "$(", "PPER", "VVPP", "PPER", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sie seufzt und sinkt aufs Ruhebette;", "tokens": ["Sie", "seufzt", "und", "sinkt", "aufs", "Ru\u00b7he\u00b7bet\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Im Ernst, seufzt sie, sie glauben das?", "tokens": ["Im", "Ernst", ",", "seufzt", "sie", ",", "sie", "glau\u00b7ben", "das", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "VVFIN", "PPER", "$,", "PPER", "VVFIN", "PDS", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ich Thor! da\u00df ich so albern redte!", "tokens": ["Ich", "Thor", "!", "da\u00df", "ich", "so", "al\u00b7bern", "red\u00b7te", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$.", "KOUS", "PPER", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Versteht die N\u00e4rrin keinen Spas?", "tokens": ["Ver\u00b7steht", "die", "N\u00e4r\u00b7rin", "kei\u00b7nen", "Spas", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Allein aus dichtbewachsnen Hecken", "tokens": ["Al\u00b7lein", "aus", "dicht\u00b7be\u00b7wachs\u00b7nen", "He\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Guckt lauschend Doris, ruft mich an,", "tokens": ["Guckt", "lau\u00b7schend", "Do\u00b7ris", ",", "ruft", "mich", "an", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "NE", "$,", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und eilt, sich wieder zu verstecken,", "tokens": ["Und", "eilt", ",", "sich", "wie\u00b7der", "zu", "ver\u0