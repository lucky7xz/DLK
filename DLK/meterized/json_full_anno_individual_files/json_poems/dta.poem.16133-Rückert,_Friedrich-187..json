{"dta.poem.16133": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "187.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Salamander sprach zu einem Schmetterlinge,", "tokens": ["Der", "Sa\u00b7la\u00b7man\u00b7der", "sprach", "zu", "ei\u00b7nem", "Schmet\u00b7ter\u00b7lin\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als er am Feuer ihn versengen sah die Schwinge:", "tokens": ["Als", "er", "am", "Feu\u00b7er", "ihn", "ver\u00b7sen\u00b7gen", "sah", "die", "Schwin\u00b7ge", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "PPER", "VVINF", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wie bist du doch gewebt aus gar so leichten Stoffen!", "tokens": ["Wie", "bist", "du", "doch", "ge\u00b7webt", "aus", "gar", "so", "leich\u00b7ten", "Stof\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "VVPP", "APPR", "ADV", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mich hat in dieser Glut kein Unfall noch betroffen.", "tokens": ["Mich", "hat", "in", "die\u00b7ser", "Glut", "kein", "Un\u00b7fall", "noch", "be\u00b7trof\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PDAT", "NN", "PIAT", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Mein Blut macht um mich her die gl\u00fchen Kohlen k\u00fchl,", "tokens": ["Mein", "Blut", "macht", "um", "mich", "her", "die", "gl\u00fc\u00b7hen", "Koh\u00b7len", "k\u00fchl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "PRF", "APZR", "ART", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und recht behaglich ist mirs auf dem Rosenpf\u00fchl.", "tokens": ["Und", "recht", "be\u00b7hag\u00b7lich", "ist", "mirs", "auf", "dem", "Ro\u00b7sen\u00b7pf\u00fchl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "VAFIN", "NE", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Du r\u00fchrest nur daran und gehest auf in Flammen;", "tokens": ["Du", "r\u00fch\u00b7rest", "nur", "da\u00b7ran", "und", "ge\u00b7hest", "auf", "in", "Flam\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PAV", "KON", "VVFIN", "APPR", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie kommt dein Ungemach und mein Gemach zusammen?", "tokens": ["Wie", "kommt", "dein", "Un\u00b7ge\u00b7mach", "und", "mein", "Ge\u00b7mach", "zu\u00b7sam\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Kann Tod und Leben so von gleicher Weide stammen?", "tokens": ["Kann", "Tod", "und", "Le\u00b7ben", "so", "von", "glei\u00b7cher", "Wei\u00b7de", "stam\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "KON", "NN", "ADV", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Da sprach der Schmetterling zum Salamander sterbend:", "tokens": ["Da", "sprach", "der", "Schmet\u00b7ter\u00b7ling", "zum", "Sa\u00b7la\u00b7man\u00b7der", "ster\u00b7bend", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So ist, was den erquickt, dem anderen verderbend.", "tokens": ["So", "ist", ",", "was", "den", "er\u00b7quickt", ",", "dem", "an\u00b7de\u00b7ren", "ver\u00b7der\u00b7bend", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "$,", "PRELS", "ART", "VVPP", "$,", "PRELS", "PIS", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Vielleicht beneidet wer dich um dein z\u00e4hes Leben,", "tokens": ["Viel\u00b7leicht", "be\u00b7nei\u00b7det", "wer", "dich", "um", "dein", "z\u00e4\u00b7hes", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "PWS", "PPER", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Die Liebe aber liebt das ihre aufzugeben. \u2014", "tokens": ["Die", "Lie\u00b7be", "a\u00b7ber", "liebt", "das", "ih\u00b7re", "auf\u00b7zu\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "ART", "PPOSAT", "VVIZU", "$.", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Mein Herz, vergleichest du die beiden mit einander,", "tokens": ["Mein", "Herz", ",", "ver\u00b7glei\u00b7chest", "du", "die", "bei\u00b7den", "mit", "ein\u00b7an\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "PPER", "ART", "PIAT", "APPR", "PRF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du ziehst den Schmetterling wol vor dem Salamander.", "tokens": ["Du", "ziehst", "den", "Schmet\u00b7ter\u00b7ling", "wol", "vor", "dem", "Sa\u00b7la\u00b7man\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}