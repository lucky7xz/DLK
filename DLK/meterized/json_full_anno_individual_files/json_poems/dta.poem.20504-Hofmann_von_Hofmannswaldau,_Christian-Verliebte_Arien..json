{"dta.poem.20504": {"metadata": {"author": {"name": "Hofmann von Hofmannswaldau, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Verliebte Arien.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1695", "urn": "urn:nbn:de:kobv:b4-200905197751", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was qv\u00e4lstu mich Luci'gen in dem schlaffe/", "tokens": ["Was", "qv\u00e4ls\u00b7tu", "mich", "Lu\u00b7ci'\u00b7gen", "in", "dem", "schlaf\u00b7fe", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "NN", "APPR", "ART", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df ich vor dir nicht sicher ruhen kan?", "tokens": ["Da\u00df", "ich", "vor", "dir", "nicht", "si\u00b7cher", "ru\u00b7hen", "kan", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "PTKNEG", "ADJD", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sind etwan mir die tr\u00e4ume nur zur straffe?", "tokens": ["Sind", "et\u00b7wan", "mir", "die", "tr\u00e4u\u00b7me", "nur", "zur", "straf\u00b7fe", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPER", "ART", "VVFIN", "ADV", "APPRART", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dieweil ich dir nicht wachend gnug gethan?", "tokens": ["Die\u00b7weil", "ich", "dir", "nicht", "wa\u00b7chend", "gnug", "ge\u00b7than", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "PTKNEG", "ADJD", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wie offte mu\u00df ich doch im schlaff empfinden", "tokens": ["Wie", "off\u00b7te", "mu\u00df", "ich", "doch", "im", "schlaff", "emp\u00b7fin\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "VMFIN", "PPER", "ADV", "APPRART", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Was ich mich nie darff wachend unterwinden.", "tokens": ["Was", "ich", "mich", "nie", "darff", "wa\u00b7chend", "un\u00b7ter\u00b7win\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PRF", "ADV", "VMFIN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ich schliesse kaum die matten augen-lieder/", "tokens": ["Ich", "schlies\u00b7se", "kaum", "die", "mat\u00b7ten", "au\u00b7gen\u00b7lie\u00b7der", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So sehe ich mein kind schon vor mir stehn/", "tokens": ["So", "se\u00b7he", "ich", "mein", "kind", "schon", "vor", "mir", "stehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "ADV", "APPR", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da deucht mir gleich als wenn wir hin und wieder/", "tokens": ["Da", "deucht", "mir", "gleich", "als", "wenn", "wir", "hin", "und", "wie\u00b7der", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "KOKOM", "KOUS", "PPER", "PTKVZ", "KON", "ADV", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Durch welt und feld verliebt spatzieren gehn;", "tokens": ["Durch", "welt", "und", "feld", "ver\u00b7liebt", "spat\u00b7zie\u00b7ren", "gehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVPP", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Es k\u00fchlen mich die s\u00fcssen phantaseyen/", "tokens": ["Es", "k\u00fch\u00b7len", "mich", "die", "s\u00fcs\u00b7sen", "phan\u00b7ta\u00b7se\u00b7yen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "ADJA", "VVINF", "$("], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Da\u00df ich mich mu\u00df im schlaffe recht erfreuen?", "tokens": ["Da\u00df", "ich", "mich", "mu\u00df", "im", "schlaf\u00b7fe", "recht", "er\u00b7freu\u00b7en", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VMFIN", "APPRART", "VVFIN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Da darff ich dich zu tausend mahlen k\u00fcssen/", "tokens": ["Da", "darff", "ich", "dich", "zu", "tau\u00b7send", "mah\u00b7len", "k\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "APPR", "CARD", "VVINF", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mir ist verg\u00f6nnt die alabaster brust/", "tokens": ["Mir", "ist", "ver\u00b7g\u00f6nnt", "die", "a\u00b7la\u00b7bas\u00b7ter", "brust", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ich darff auch wohl im traum ein solches wissen/", "tokens": ["Ich", "darff", "auch", "wohl", "im", "traum", "ein", "sol\u00b7ches", "wis\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "APPRART", "NN", "ART", "PIS", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "So wachend mir noch g\u00e4ntzlich unbewust/", "tokens": ["So", "wa\u00b7chend", "mir", "noch", "g\u00e4ntz\u00b7lich", "un\u00b7be\u00b7wust", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "ADV", "ADJD", "ADJD", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wenn aber ich bin wieder auffgewachet/", "tokens": ["Wenn", "a\u00b7ber", "ich", "bin", "wie\u00b7der", "auff\u00b7ge\u00b7wa\u00b7chet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPER", "VAFIN", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "So wird die lust mir erst zur qvaal gemachet.", "tokens": ["So", "wird", "die", "lust", "mir", "erst", "zur", "qvaal", "ge\u00b7ma\u00b7chet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "PPER", "ADV", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "So mu\u00df ich denn des tages \u00fcber b\u00fcssen/", "tokens": ["So", "mu\u00df", "ich", "denn", "des", "ta\u00b7ges", "\u00fc\u00b7ber", "b\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ART", "NN", "APPR", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Was mich zuvor des nachtes hat erfreut/", "tokens": ["Was", "mich", "zu\u00b7vor", "des", "nach\u00b7tes", "hat", "er\u00b7freut", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "ART", "ADJA", "VAFIN", "ADJD", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und wei\u00df mein leid mir denn nicht zu vers\u00fcssen/", "tokens": ["Und", "wei\u00df", "mein", "leid", "mir", "denn", "nicht", "zu", "ver\u00b7s\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "ADJD", "PPER", "ADV", "PTKNEG", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Als wenn sich einst das liebe gl\u00fcck erbeut/", "tokens": ["Als", "wenn", "sich", "einst", "das", "lie\u00b7be", "gl\u00fcck", "er\u00b7beut", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PRF", "ADV", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Da\u00df ich dir kan die s\u00fcssen tr\u00e4um erzehlen/", "tokens": ["Da\u00df", "ich", "dir", "kan", "die", "s\u00fcs\u00b7sen", "tr\u00e4um", "er\u00b7zeh\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VMFIN", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und wie ich mich so sehr um dich mu\u00df qv\u00e4len.", "tokens": ["Und", "wie", "ich", "mich", "so", "sehr", "um", "dich", "mu\u00df", "qv\u00e4\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "PRF", "ADV", "ADV", "APPR", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Darun/ mein kind/ verschone mich mit tr\u00e4umen/", "tokens": ["Da\u00b7run", "/", "mein", "kind", "/", "ver\u00b7scho\u00b7ne", "mich", "mit", "tr\u00e4u\u00b7men", "/"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$(", "PPOSAT", "NN", "$(", "VVFIN", "PRF", "APPR", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ist deine gunst mir ja wohl zugethan/", "tokens": ["Ist", "dei\u00b7ne", "gunst", "mir", "ja", "wohl", "zu\u00b7ge\u00b7than", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "PPER", "ADV", "ADV", "ADJD", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "So werd ich nie die liebe zeit vers\u00e4umen/", "tokens": ["So", "werd", "ich", "nie", "die", "lie\u00b7be", "zeit", "ver\u00b7s\u00e4u\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wie ich dich in der that bedienen kan/", "tokens": ["Wie", "ich", "dich", "in", "der", "that", "be\u00b7die\u00b7nen", "kan", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "APPR", "ART", "VVFIN", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Viel lieber will ich wachend einmahl k\u00fcssen/", "tokens": ["Viel", "lie\u00b7ber", "will", "ich", "wa\u00b7chend", "ein\u00b7mahl", "k\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VMFIN", "PPER", "ADJD", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Als tausend mahl die lust im traum geniessen.", "tokens": ["Als", "tau\u00b7send", "mahl", "die", "lust", "im", "traum", "ge\u00b7nies\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "CARD", "ADV", "ART", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}