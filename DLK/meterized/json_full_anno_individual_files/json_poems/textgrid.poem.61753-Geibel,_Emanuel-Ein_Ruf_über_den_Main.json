_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "$,", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sein stolzes Herz beschied", "tokens": ["Sein", "stol\u00b7zes", "Herz", "be\u00b7schied"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und gro\u00dfen Sinns die Krone,", "tokens": ["Und", "gro\u00b7\u00dfen", "Sinns", "die", "Kro\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Darnach er selbst begehrt,", "tokens": ["Dar\u00b7nach", "er", "selbst", "be\u00b7gehrt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Des Nordens starkem Sohne", "tokens": ["Des", "Nor\u00b7dens", "star\u00b7kem", "Soh\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Darbot am Vogelherd.", "tokens": ["Dar\u00b7bot", "am", "Vo\u00b7gel\u00b7herd", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.16": {"line.1": {"text": "O la\u00dft sein Bild euch mahnen", "tokens": ["O", "la\u00dft", "sein", "Bild", "euch", "mah\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPOSAT", "NN", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und zieht aus S\u00fcd und West,", "tokens": ["Und", "zieht", "aus", "S\u00fcd", "und", "West", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Zieht hin mit euren Fahnen", "tokens": ["Zieht", "hin", "mit", "eu\u00b7ren", "Fah\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zum sch\u00f6nsten S\u00fchnungsfest", "tokens": ["Zum", "sch\u00f6ns\u00b7ten", "S\u00fch\u00b7nungs\u00b7fest"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und bringt, die uns verloren,", "tokens": ["Und", "bringt", ",", "die", "uns", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Doch nie vergessen war,", "tokens": ["Doch", "nie", "ver\u00b7ges\u00b7sen", "war", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Dem Haupt, das Gott erkoren,", "tokens": ["Dem", "Haupt", ",", "das", "Gott", "er\u00b7ko\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die Kaiserkrone dar!", "tokens": ["Die", "Kai\u00b7ser\u00b7kro\u00b7ne", "dar", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}