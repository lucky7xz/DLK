{"textgrid.poem.61753": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Ein Ruf \u00fcber den Main", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun steht das Haus gegr\u00fcndet", "tokens": ["Nun", "steht", "das", "Haus", "ge\u00b7gr\u00fcn\u00b7det"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und prangt im Fr\u00fchrotschein,", "tokens": ["Und", "prangt", "im", "Fr\u00fch\u00b7rot\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nun ist das Wort verk\u00fcndet:", "tokens": ["Nun", "ist", "das", "Wort", "ver\u00b7k\u00fcn\u00b7det", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Kommt her und tretet ein!", "tokens": ["Kommt", "her", "und", "tre\u00b7tet", "ein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "KON", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Kein Fremdling soll euch hindern,", "tokens": ["Kein", "Fremd\u00b7ling", "soll", "euch", "hin\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Kein Machtspruch fern und nah,", "tokens": ["Kein", "Machts\u00b7pruch", "fern", "und", "nah", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Nach allen ihren Kindern", "tokens": ["Nach", "al\u00b7len", "ih\u00b7ren", "Kin\u00b7dern"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PIAT", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Verlangt Germania.", "tokens": ["Ver\u00b7langt", "Ger\u00b7ma\u00b7nia", "."], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "NE", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Ihr sollt nicht l\u00e4nger tragen", "tokens": ["Ihr", "sollt", "nicht", "l\u00e4n\u00b7ger", "tra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PTKNEG", "ADJD", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Waisen schwarz Gewand,", "tokens": ["Der", "Wai\u00b7sen", "schwarz", "Ge\u00b7wand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ihr sollt nicht f\u00fcrder fragen:", "tokens": ["Ihr", "sollt", "nicht", "f\u00fcr\u00b7der", "fra\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wo ist das Vaterland?", "tokens": ["Wo", "ist", "das", "Va\u00b7ter\u00b7land", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Den Hort euch zu gewinnen,", "tokens": ["Den", "Hort", "euch", "zu", "ge\u00b7win\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der j\u00fcngst ein Traum noch war,", "tokens": ["Der", "j\u00fcngst", "ein", "Traum", "noch", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "ADV", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Reicht nur in treuen Sinnen", "tokens": ["Reicht", "nur", "in", "treu\u00b7en", "Sin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die Hand den Br\u00fcdern dar!", "tokens": ["Die", "Hand", "den", "Br\u00fc\u00b7dern", "dar", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ihr raschen Alemannen,", "tokens": ["Ihr", "ra\u00b7schen", "A\u00b7le\u00b7man\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Gl\u00fcckauf! Mit Jubelton", "tokens": ["Gl\u00fcc\u00b7kauf", "!", "Mit", "Ju\u00b7bel\u00b7ton"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$.", "APPR", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Aus eures Schwarzwalds Tannen", "tokens": ["Aus", "eu\u00b7res", "Schwarz\u00b7walds", "Tan\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Antwortend gr\u00fc\u00dft ihr schon.", "tokens": ["Ant\u00b7wor\u00b7tend", "gr\u00fc\u00dft", "ihr", "schon", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "PPER", "ADV", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Ihr habt die heil'ge Lohe", "tokens": ["Ihr", "habt", "die", "heil'\u00b7ge", "Lo\u00b7he"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der Freiheit stets gen\u00e4hrt,", "tokens": ["Der", "Frei\u00b7heit", "stets", "ge\u00b7n\u00e4hrt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Nun sch\u00fcrt getreu die hohe", "tokens": ["Nun", "sch\u00fcrt", "ge\u00b7treu", "die", "ho\u00b7he"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADJD", "ART", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Auf gr\u00f6\u00dferm Opferherd!", "tokens": ["Auf", "gr\u00f6\u00b7\u00dferm", "Op\u00b7fer\u00b7herd", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Was s\u00e4umt ihr ernsten Schwaben,", "tokens": ["Was", "s\u00e4umt", "ihr", "erns\u00b7ten", "Schwa\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Vork\u00e4mpfer einst im Reich?", "tokens": ["Vor\u00b7k\u00e4mp\u00b7fer", "einst", "im", "Reich", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPRART", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Wohl ist an Geist und Gaben", "tokens": ["Wohl", "ist", "an", "Geist", "und", "Ga\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Kein Stamm dem euren gleich;", "tokens": ["Kein", "Stamm", "dem", "eu\u00b7ren", "gleich", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ART", "PPOSAT", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "O la\u00dft den Schatz nicht rosten,", "tokens": ["O", "la\u00dft", "den", "Schatz", "nicht", "ros\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ihr sollt auch \u00fcberm Main,", "tokens": ["Ihr", "sollt", "auch", "\u00fc\u00b7berm", "Main", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wo Lichtgedanken spro\u00dften,", "tokens": ["Wo", "Licht\u00b7ge\u00b7dan\u00b7ken", "spro\u00df\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die Bannertr\u00e4ger sein.", "tokens": ["Die", "Ban\u00b7ner\u00b7tr\u00e4\u00b7ger", "sein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Ihr l\u00f6wenherz'gen Bayern,", "tokens": ["Ihr", "l\u00f6\u00b7wen\u00b7her\u00b7z'\u00b7gen", "Bay\u00b7ern", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Ihr Franken klug und k\u00fchn,", "tokens": ["Ihr", "Fran\u00b7ken", "klug", "und", "k\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie lange wollt ihr feiern,", "tokens": ["Wie", "lan\u00b7ge", "wollt", "ihr", "fei\u00b7ern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wo Deutschlands Ehren bl\u00fchn?", "tokens": ["Wo", "Deutschlands", "Eh\u00b7ren", "bl\u00fchn", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "NN", "VVINF", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Den Arm, erprobt im Schlagen,", "tokens": ["Den", "Arm", ",", "er\u00b7probt", "im", "Schla\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Den Blick voll Weltverstand,", "tokens": ["Den", "Blick", "voll", "Welt\u00b7ver\u00b7stand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wollt ihr sie tr\u00e4g versagen", "tokens": ["Wollt", "ihr", "sie", "tr\u00e4g", "ver\u00b7sa\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PPER", "ADJD", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Dem gro\u00dfen Vaterland?", "tokens": ["Dem", "gro\u00b7\u00dfen", "Va\u00b7ter\u00b7land", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Empor! Ihr hofft vergebens,", "tokens": ["Em\u00b7por", "!", "Ihr", "hofft", "ver\u00b7ge\u00b7bens", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PPER", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein Volk im Volk zu sein,", "tokens": ["Ein", "Volk", "im", "Volk", "zu", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "PTKZU", "VAINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Schon rei\u00dft der Strom des Lebens", "tokens": ["Schon", "rei\u00dft", "der", "Strom", "des", "Le\u00b7bens"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die dumpfen Schranken ein.", "tokens": ["Die", "dum\u00b7pfen", "Schran\u00b7ken", "ein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Vertraut euch seinen Wogen", "tokens": ["Ver\u00b7traut", "euch", "sei\u00b7nen", "Wo\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und sucht ein besser Heil!", "tokens": ["Und", "sucht", "ein", "bes\u00b7ser", "Heil", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Allm\u00e4chtig angezogen", "tokens": ["All\u00b7m\u00e4ch\u00b7tig", "an\u00b7ge\u00b7zo\u00b7gen"], "token_info": ["word", "word"], "pos": ["ADJD", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Zum Ganzen strebt der Teil.", "tokens": ["Zum", "Gan\u00b7zen", "strebt", "der", "Teil", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Wohl habt ihr's oft vernommen,", "tokens": ["Wohl", "habt", "ih\u00b7r's", "oft", "ver\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Vom Eberhard das Lied,", "tokens": ["Vom", "E\u00b7ber\u00b7hard", "das", "Lied", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NE", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie er, dem Reich zum Frommen,", "tokens": ["Wie", "er", ",", "dem", "Reich", "zum", "From\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "$,", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sein stolzes Herz beschied", "tokens": ["Sein", "stol\u00b7zes", "Herz", "be\u00b7schied"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und gro\u00dfen Sinns die Krone,", "tokens": ["Und", "gro\u00b7\u00dfen", "Sinns", "die", "Kro\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Darnach er selbst begehrt,", "tokens": ["Dar\u00b7nach", "er", "selbst", "be\u00b7gehrt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Des Nordens starkem Sohne", "tokens": ["Des", "Nor\u00b7dens", "star\u00b7kem", "Soh\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Darbot am Vogelherd.", "tokens": ["Dar\u00b7bot", "am", "Vo\u00b7gel\u00b7herd", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "O la\u00dft sein Bild euch mahnen", "tokens": ["O", "la\u00dft", "sein", "Bild", "euch", "mah\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPOSAT", "NN", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und zieht aus S\u00fcd und West,", "tokens": ["Und", "zieht", "aus", "S\u00fcd", "und", "West", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Zieht hin mit euren Fahnen", "tokens": ["Zieht", "hin", "mit", "eu\u00b7ren", "Fah\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zum sch\u00f6nsten S\u00fchnungsfest", "tokens": ["Zum", "sch\u00f6ns\u00b7ten", "S\u00fch\u00b7nungs\u00b7fest"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und bringt, die uns verloren,", "tokens": ["Und", "bringt", ",", "die", "uns", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Doch nie vergessen war,", "tokens": ["Doch", "nie", "ver\u00b7ges\u00b7sen", "war", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Dem Haupt, das Gott erkoren,", "tokens": ["Dem", "Haupt", ",", "das", "Gott", "er\u00b7ko\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die Kaiserkrone dar!", "tokens": ["Die", "Kai\u00b7ser\u00b7kro\u00b7ne", "dar", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "st