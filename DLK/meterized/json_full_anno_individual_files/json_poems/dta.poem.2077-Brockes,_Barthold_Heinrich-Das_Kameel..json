{"dta.poem.2077": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Das Kameel.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Gegen Pferde trefflich n\u00fctzt; Ueberdas durch schnelles", "tokens": ["Ge\u00b7gen", "Pfer\u00b7de", "treff\u00b7lich", "n\u00fctzt", ";", "Ue\u00b7ber\u00b7das", "durch", "schnel\u00b7les"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "ADJD", "VVFIN", "$.", "NN", "APPR", "ADJA"], "meter": "--+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Laufen,", "tokens": ["Lau\u00b7fen", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Wozu sichs gewehnen l\u00e4\u00dft, ungemeinen Vortheil", "tokens": ["Wo\u00b7zu", "sichs", "ge\u00b7weh\u00b7nen", "l\u00e4\u00dft", ",", "un\u00b7ge\u00b7mei\u00b7nen", "Vor\u00b7theil"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "PIS", "VVINF", "VVFIN", "$,", "ADJA", "NN"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "schafft,", "tokens": ["schafft", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Weil es durch den starken Knocher und der z\u00e4hen Ner-", "tokens": ["Weil", "es", "durch", "den", "star\u00b7ken", "Kno\u00b7cher", "und", "der", "z\u00e4\u00b7hen", "Ner"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "ADJA", "NN", "KON", "ART", "ADJA", "TRUNC"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.6": {"text": "ven Kraft", "tokens": ["ven", "Kraft"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+", "measure": "iambic.single"}, "line.7": {"text": "Fast nicht zu erm\u00fcden ist, welches n\u00fctzlich auf der", "tokens": ["Fast", "nicht", "zu", "er\u00b7m\u00fc\u00b7den", "ist", ",", "wel\u00b7ches", "n\u00fctz\u00b7lich", "auf", "der"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "PTKNEG", "PTKZU", "VVINF", "VAFIN", "$,", "PRELS", "ADJD", "APPR", "ART"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.8": {"text": "Reise.", "tokens": ["Rei\u00b7se", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.9": {"text": "Seine s\u00fc\u00dfe Milch ist trinkbar, gleichfalls dient sein Fleisch", "tokens": ["Sei\u00b7ne", "s\u00fc\u00b7\u00dfe", "Milch", "ist", "trink\u00b7bar", ",", "gleich\u00b7falls", "dient", "sein", "Fleisch"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "ADJD", "$,", "ADV", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.10": {"text": "zur Speise.", "tokens": ["zur", "Spei\u00b7se", "."], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.11": {"text": "Auch versp\u00fcrt man, da\u00df von ihm manches Theil zur Ar-", "tokens": ["Auch", "ver\u00b7sp\u00fcrt", "man", ",", "da\u00df", "von", "ihm", "man\u00b7ches", "Theil", "zur", "Ar"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "$,", "KOUS", "APPR", "PPER", "PIAT", "NN", "APPRART", "NN"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.12": {"text": "zeney,", "tokens": ["ze\u00b7ney", ","], "token_info": ["word", "punct"], "pos": ["PTKVZ", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.13": {"text": "Aeu\u00dferlich und innerlich, heilsam zu gebrauchen sey.", "tokens": ["A\u00b7e\u00b7u\u00b7\u00dfer\u00b7lich", "und", "in\u00b7ner\u00b7lich", ",", "heil\u00b7sam", "zu", "ge\u00b7brau\u00b7chen", "sey", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "$,", "ADJD", "PTKZU", "VVINF", "VAFIN", "$."], "meter": "+-+-+-+--+-+-+-+", "measure": "trochaic.octa.plus.relaxed"}, "line.14": {"text": "Da\u00df denn nun f\u00fcr dieses, Thier auch das menschliche", "tokens": ["Da\u00df", "denn", "nun", "f\u00fcr", "die\u00b7ses", ",", "Thier", "auch", "das", "menschli\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ADV", "APPR", "PDAT", "$,", "NN", "ADV", "ART", "ADJA"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.15": {"text": "Geschlechte", "tokens": ["Ge\u00b7schlech\u00b7te"], "token_info": ["word"], "pos": ["NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.16": {"text": "Dem, der es f\u00fcr ihn erschaffen, ehrerbietig danken", "tokens": ["Dem", ",", "der", "es", "f\u00fcr", "ihn", "er\u00b7schaf\u00b7fen", ",", "ehr\u00b7er\u00b7bie\u00b7tig", "dan\u00b7ken"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PDS", "$,", "PRELS", "PPER", "APPR", "PPER", "VVPP", "$,", "ADJD", "VVINF"], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.17": {"text": "m\u00f6chte!", "tokens": ["m\u00f6ch\u00b7te", "!"], "token_info": ["word", "punct"], "pos": ["VMFIN", "$."], "meter": "+-", "measure": "trochaic.single"}}}}}