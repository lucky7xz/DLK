{"textgrid.poem.65100": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es sind nur Meinungen, die uns entzweien,", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es sind nur Meinungen, die uns entzweien,", "tokens": ["Es", "sind", "nur", "Mei\u00b7nun\u00b7gen", ",", "die", "uns", "ent\u00b7zwei\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Doch, kommt des Strebens letztes Ziel zur Sprache,", "tokens": ["Doch", ",", "kommt", "des", "Stre\u00b7bens", "letz\u00b7tes", "Ziel", "zur", "Spra\u00b7che", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VVFIN", "ART", "NN", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sind wir Vertreter ", "tokens": ["Sind", "wir", "Ver\u00b7tre\u00b7ter"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "PPER", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Und Kampfgenossen in getrennten Reihen.", "tokens": ["Und", "Kampf\u00b7ge\u00b7nos\u00b7sen", "in", "ge\u00b7trenn\u00b7ten", "Rei\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Von Schmerz und Noth die Menschheit zu befreien,", "tokens": ["Von", "Schmerz", "und", "Noth", "die", "Menschheit", "zu", "be\u00b7frei\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Da\u00df, die jetzt unter nieder'm Kerkerdache", "tokens": ["Da\u00df", ",", "die", "jetzt", "un\u00b7ter", "nie\u00b7der'm", "Ker\u00b7ker\u00b7da\u00b7che"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "$,", "PRELS", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Aufst\u00f6hnend tr\u00e4umt, zu bess'rem Sein erwache,", "tokens": ["Auf\u00b7st\u00f6h\u00b7nend", "tr\u00e4umt", ",", "zu", "bess'\u00b7rem", "Sein", "er\u00b7wa\u00b7che", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "$,", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die\u00df ist der Dienst, dem wir uns Beide weihen.", "tokens": ["Die\u00df", "ist", "der", "Dienst", ",", "dem", "wir", "uns", "Bei\u00b7de", "wei\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "PRELS", "PPER", "PRF", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Du suchst das Heil in den entschwund'nen Zeiten,", "tokens": ["Du", "suchst", "das", "Heil", "in", "den", "ent\u00b7schwun\u00b7d'\u00b7nen", "Zei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+---+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Den l\u00e4ngst des Geistes frischem Hauch erleg'nen;", "tokens": ["Den", "l\u00e4ngst", "des", "Geis\u00b7tes", "fri\u00b7schem", "Hauch", "er\u00b7leg'\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "AR