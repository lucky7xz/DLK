{"textgrid.poem.41147": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "8.", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Eine Geistin ist dieses, die im Leben einst ganz", "tokens": ["Ei\u00b7ne", "Geis\u00b7tin", "ist", "die\u00b7ses", ",", "die", "im", "Le\u00b7ben", "einst", "ganz"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PDS", "$,", "PRELS", "APPRART", "NN", "ADV", "ADV"], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Einzig gelebt hat f\u00fcr Spiel und f\u00fcr Tanz;", "tokens": ["Ein\u00b7zig", "ge\u00b7lebt", "hat", "f\u00fcr", "Spiel", "und", "f\u00fcr", "Tanz", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "VAFIN", "APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Sie hatte kein Herz, hat auch keins gekannt", "tokens": ["Sie", "hat\u00b7te", "kein", "Herz", ",", "hat", "auch", "keins", "ge\u00b7kannt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "$,", "VAFIN", "ADV", "PIAT", "VVPP"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Als das Herz auf der Karte, Coeur A\u00df benannt.", "tokens": ["Als", "das", "Herz", "auf", "der", "Kar\u00b7te", ",", "Co\u00b7eur", "A\u00df", "be\u00b7nannt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPR", "ART", "NN", "$,", "NN", "NN", "VVPP", "$."], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.5": {"text": "In den Spiel- \u2013 in den Tanzsaal, in den Betsaal 