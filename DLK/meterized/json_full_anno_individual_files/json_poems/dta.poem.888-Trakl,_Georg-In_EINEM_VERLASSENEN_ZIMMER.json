{"dta.poem.888": {"metadata": {"author": {"name": "Trakl, Georg", "birth": "N.A.", "death": "N.A."}, "title": "In EINEM VERLASSENEN ZIMMER", "genre": "Lyrik", "period": "N.A.", "pub_year": "1913", "urn": "urn:nbn:de:kobv:b4-30357-9", "language": ["de:0.85", "nl:0.14"], "booktitle": "Trakl, Georg: Gedichte. Leipzig, 1913."}, "poem": {"stanza.1": {"line.1": {"text": "Fenster, bunte Blumenbeeten,", "tokens": ["Fens\u00b7ter", ",", "bun\u00b7te", "Blu\u00b7men\u00b7bee\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Eine Orgel spielt herein.", "tokens": ["Ei\u00b7ne", "Or\u00b7gel", "spielt", "her\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schatten tanzen an Tapeten,", "tokens": ["Schat\u00b7ten", "tan\u00b7zen", "an", "Ta\u00b7pe\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wunderlich ein toller Reihn.", "tokens": ["Wun\u00b7der\u00b7lich", "ein", "tol\u00b7ler", "Reihn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Lichterloh die B\u00fcsche wehen", "tokens": ["Lich\u00b7ter\u00b7loh", "die", "B\u00fc\u00b7sche", "we\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und ein Schwarm von M\u00fccken schwingt,", "tokens": ["Und", "ein", "Schwarm", "von", "M\u00fc\u00b7cken", "schwingt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Fern im Acker Sensen m\u00e4hen", "tokens": ["Fern", "im", "A\u00b7cker", "Sen\u00b7sen", "m\u00e4\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPRART", "NN", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und ein altes Wasser singt.", "tokens": ["Und", "ein", "al\u00b7tes", "Was\u00b7ser", "singt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wessen Atem kommt mich kosen?", "tokens": ["Wes\u00b7sen", "A\u00b7tem", "kommt", "mich", "ko\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schwalben irre Zeichen ziehn.", "tokens": ["Schwal\u00b7ben", "ir\u00b7re", "Zei\u00b7chen", "ziehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Leise flie\u00dft im Grenzenlosen", "tokens": ["Lei\u00b7se", "flie\u00dft", "im", "Gren\u00b7zen\u00b7lo\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dort das goldne Waldland hin.", "tokens": ["Dort", "das", "gold\u00b7ne", "Wald\u00b7land", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Flammen flackern in den Beeten.", "tokens": ["Flam\u00b7men", "fla\u00b7ckern", "in", "den", "Bee\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wirr verz\u00fcckt der tolle Reihn", "tokens": ["Wirr", "ver\u00b7z\u00fcckt", "der", "tol\u00b7le", "Reihn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "An den gelblichen Tapeten.", "tokens": ["An", "den", "gelb\u00b7li\u00b7chen", "Ta\u00b7pe\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Jemand schaut zur T\u00fcr herein.", "tokens": ["Je\u00b7mand", "schaut", "zur", "T\u00fcr", "her\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPRART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Weihrauch duftet s\u00fc\u00df und Birne", "tokens": ["Weih\u00b7rauch", "duf\u00b7tet", "s\u00fc\u00df", "und", "Bir\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ADJD", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und es d\u00e4mmern Glas und Truh.", "tokens": ["Und", "es", "d\u00e4m\u00b7mern", "Glas", "und", "Truh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADJA", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Langsam beugt die hei\u00dfe Stirne", "tokens": ["Lang\u00b7sam", "beugt", "die", "hei\u00b7\u00dfe", "Stir\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sich den wei\u00dfen Sternen zu.", "tokens": ["Sich", "den", "wei\u00b7\u00dfen", "Ster\u00b7nen", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}