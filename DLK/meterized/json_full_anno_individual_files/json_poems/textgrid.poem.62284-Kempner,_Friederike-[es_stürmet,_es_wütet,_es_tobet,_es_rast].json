{"textgrid.poem.62284": {"metadata": {"author": {"name": "Kempner, Friederike", "birth": "N.A.", "death": "N.A."}, "title": "[es st\u00fcrmet, es w\u00fctet, es tobet, es rast]", "genre": "verse", "period": "N.A.", "pub_year": 1868, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es st\u00fcrmet, es w\u00fctet, es tobet, es rast,", "tokens": ["Es", "st\u00fcr\u00b7met", ",", "es", "w\u00fc\u00b7tet", ",", "es", "to\u00b7bet", ",", "es", "rast", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Als sollte die Welt sich neuer gestalten,", "tokens": ["Als", "soll\u00b7te", "die", "Welt", "sich", "neu\u00b7er", "ge\u00b7stal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VMFIN", "ART", "NN", "PRF", "ADJD", "VVPP", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Es legt sich der Sturm, das Toben h\u00f6rt auf", "tokens": ["Es", "legt", "sich", "der", "Sturm", ",", "das", "To\u00b7ben", "h\u00f6rt", "auf"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "ART", "NN", "$,", "ART", "NN", "VVFIN", "APPR"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und alles, es bleibet beim Alten.", "tokens": ["Und", "al\u00b7les", ",", "es", "blei\u00b7bet", "beim", "Al\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "$,", "PPER", "VVFIN", "APPRART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.2": {"line.1": {"text": "Im geistigen Leben ist's ebenso,", "tokens": ["Im", "geis\u00b7ti\u00b7gen", "Le\u00b7ben", "ist's", "e\u00b7ben\u00b7so", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VAFIN", "ADV", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Bedeutende Menschen, sie ringen,", "tokens": ["Be\u00b7deu\u00b7ten\u00b7de", "Men\u00b7schen", ",", "sie", "rin\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PPER", "VVFIN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Sie k\u00e4mpfen, sie opfern, erk\u00e4mpfen nichts,", "tokens": ["Sie", "k\u00e4mp\u00b7fen", ",", "sie", "op\u00b7fern", ",", "er\u00b7k\u00e4mp\u00b7fen", "nichts", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVINF", "$,", "VVFIN", "PIS", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Die Torheit, sie bleibt in den Dingen!", "tokens": ["Die", "Tor\u00b7heit", ",", "sie", "bleibt", "in", "den", "Din\u00b7gen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.3": {"line.1": {"text": "Es st\u00fcrmet, es w\u00fctet, es tobet, es rast,", "tokens": ["Es", "st\u00fcr\u00b7met", ",", "es", "w\u00fc\u00b7tet", ",", "es", "to\u00b7bet", ",", "es", "rast", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Als sollte die Welt sich neuer gestalten,", "tokens": ["Als", "soll\u00b7te", "die", "Welt", "sich", "neu\u00b7er", "ge\u0