{"dta.poem.2249": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Epigramma  \n auf einen erbaulich lehrenden  \n  aber \u00fcbel lebenden Priester,  \n  wie selbiger starb.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Sobald dein N\u00e4chster dir misf\u00e4llt,", "tokens": ["So\u00b7bald", "dein", "N\u00e4chs\u00b7ter", "dir", "mis\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wirst du auch ihm gar bald misfallen.", "tokens": ["Wirst", "du", "auch", "ihm", "gar", "bald", "mis\u00b7fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "PPER", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wann nun dein N\u00e4chster alle Welt;", "tokens": ["Wann", "nun", "dein", "N\u00e4chs\u00b7ter", "al\u00b7le", "Welt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPOSAT", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So bleibet dieses festgestellt:", "tokens": ["So", "blei\u00b7bet", "die\u00b7ses", "fest\u00b7ge\u00b7stellt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PDS", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Misf\u00e4llt sie dir; misf\u00e4llst du allen.", "tokens": ["Mis\u00b7f\u00e4llt", "sie", "dir", ";", "mis\u00b7f\u00e4llst", "du", "al\u00b7len", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "$.", "VVFIN", "PPER", "PIAT", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}