{"textgrid.poem.40100": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "Zuflucht", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hinterm kleinen Haus am kleinen Weiher,", "tokens": ["Hin\u00b7term", "klei\u00b7nen", "Haus", "am", "klei\u00b7nen", "Wei\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "dicht umdunkelt rings von Weidenruten,", "tokens": ["dicht", "um\u00b7dun\u00b7kelt", "rings", "von", "Wei\u00b7den\u00b7ru\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ADV", "APPR", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "breitet nickend eine junge Pappel", "tokens": ["brei\u00b7tet", "ni\u00b7ckend", "ei\u00b7ne", "jun\u00b7ge", "Pap\u00b7pel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "ihre Zweige zu den tiefen Fluten.", "tokens": ["ih\u00b7re", "Zwei\u00b7ge", "zu", "den", "tie\u00b7fen", "Flu\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Seltsam heimlich ist's an diesem Orte;", "tokens": ["Selt\u00b7sam", "heim\u00b7lich", "ist's", "an", "die\u00b7sem", "Or\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "VAFIN", "APPR", "PDAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "schon als Knabe hab' ich hier gesessen", "tokens": ["schon", "als", "Kna\u00b7be", "hab'", "ich", "hier", "ge\u00b7ses\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "NN", "VAFIN", "PPER", "ADV", "VVPP"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "und umschwiegen von den hohen Binsen", "tokens": ["und", "um\u00b7schwie\u00b7gen", "von", "den", "ho\u00b7hen", "Bin\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "weinend so mein junges Leid vergessen.", "tokens": ["wei\u00b7nend", "so", "mein", "jun\u00b7ges", "Leid", "ver\u00b7ges\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "ADV", "PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Wieder starr' ich in das schwarze Wasser,", "tokens": ["Wie\u00b7der", "starr'", "ich", "in", "das", "schwar\u00b7ze", "Was\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "aber keine Thr\u00e4ne kann ich finden;", "tokens": ["a\u00b7ber", "kei\u00b7ne", "Thr\u00e4\u00b7ne", "kann", "ich", "fin\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "nur die Pappelzweige seh' ich winkend", "tokens": ["nur", "die", "Pap\u00b7pel\u00b7zwei\u00b7ge", "seh'", "ich", "win\u00b7kend"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "VVFIN", "PPER", "VVPP"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "dort sich spiegeln in den stillen Gr\u00fcnden.", "tokens": ["dort", "sich", "spie\u00b7geln", "in", "den", "stil\u00b7len", "Gr\u00fcn\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PRF", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Hinterm kleinen Haus am kleinen Weiher,", "tokens": ["Hin\u00b7term", "klei\u00b7nen", "Haus", "am", "klei\u00b7nen", "Wei\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "dicht umdunkelt rings von Weidenruten,", "tokens": ["dicht", "um\u00b7dun\u00b7kelt", "rings", "von", "Wei\u00b7den\u00b7ru\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ADV", "APPR", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "breitet nickend eine junge Pappel", "tokens": ["brei\u00b7tet", "ni\u00b7ckend", "ei\u00b7ne", "jun\u00b7ge", "Pap\u00b7pel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "ihre Zweige zu den tiefen Fluten.", "tokens": ["ih\u00b7re", "Zwei\u00b7ge", "zu", "den", "tie\u00b7fen", "Flu\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Seltsam heimlich ist's an diesem Orte;", "tokens": ["Selt\u00b7sam", "heim\u00b7lich", "ist's", "an", "die\u00b7sem", "Or\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD"