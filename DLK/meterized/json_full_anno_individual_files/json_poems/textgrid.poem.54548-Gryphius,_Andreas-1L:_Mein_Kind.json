{"textgrid.poem.54548": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mein Kind/ ", "genre": "verse", "period": "N.A.", "pub_year": 1640, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein Kind/ ", "tokens": ["Mein", "Kind", "/"], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$("], "meter": "-+", "measure": "iambic.single"}, "line.2": {"text": "Und sucht/ O F\u00fcrstin! H\u00fclff aus der Durchlauchten Hand;", "tokens": ["Und", "sucht", "/", "O", "F\u00fcrs\u00b7tin", "!", "H\u00fclff", "aus", "der", "Durch\u00b7lauch\u00b7ten", "Hand", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$(", "NE", "NN", "$.", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+---+-+", "measure": "unknown.measure.penta"}, "line.3": {"text": "Sie tret' an Gottes statt und helff/ O Himmels-Pfand!", "tokens": ["Sie", "tret'", "an", "Got\u00b7tes", "statt", "und", "helff", "/", "O", "Him\u00b7mels\u00b7Pfand", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "PTKVZ", "KON", "NN", "$(", "NE", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der/ die von etwas mehr denn Menschlich wird besprungen.", "tokens": ["Der", "/", "die", "von", "et\u00b7was", "mehr", "denn", "Menschlich", "wird", "be\u00b7sprun\u00b7gen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$(", "ART", "APPR", "PIS", "ADV", "ADV", "NE", "VAFIN", "VVINF", "$."], "meter": "+--+-+-+--+-", "measure": "iambic.penta.invert"}, "line.5": {"text": "Ich flehe vor mein Kind/ und werd' aus Angst bezwungen", "tokens": ["Ich", "fle\u00b7he", "vor", "mein", "Kind", "/", "und", "werd'", "aus", "Angst", "be\u00b7zwun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$(", "KON", "VAFIN", "APPR", "NN", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Zu schweigen: Wehmuth ists die meine Lippen band/", "tokens": ["Zu", "schwei\u00b7gen", ":", "Weh\u00b7muth", "ists", "die", "mei\u00b7ne", "Lip\u00b7pen", "band", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$.", "NN", "VAFIN", "ART", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Der Mutter Thr\u00e4nen-Fluth erhitzt den grausen Brand/", "tokens": ["Der", "Mut\u00b7ter", "Thr\u00e4\u00b7nen\u00b7Fluth", "er\u00b7hitzt", "den", "grau\u00b7sen", "Brand", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Der nunmehr durch di\u00df Hertz bi\u00df an den Geist gedrungen.", "tokens": ["Der", "nun\u00b7mehr", "durch", "di\u00df", "Hertz", "bi\u00df", "an", "den", "Geist", "ge\u00b7drun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "PDS", "NN", "APPR", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Prince\u00df' ich bitte nur di\u00df einig/ sie verg\u00f6nne", "tokens": ["Prin\u00b7ce\u00df'", "ich", "bit\u00b7te", "nur", "di\u00df", "ei\u00b7nig", "/", "sie", "ver\u00b7g\u00f6n\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVIMP", "PPER", "ADV", "ADV", "