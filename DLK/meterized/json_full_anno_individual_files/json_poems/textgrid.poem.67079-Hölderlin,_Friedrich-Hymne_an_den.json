{"textgrid.poem.67079": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Hymne an den", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Jubel! Jubel", "tokens": ["Ju\u00b7bel", "!", "Ju\u00b7bel"], "token_info": ["word", "punct", "word"], "pos": ["NN", "$.", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Dir auf der Wolke!", "tokens": ["Dir", "auf", "der", "Wol\u00b7ke", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Erstgeborner", "tokens": ["Erst\u00b7ge\u00b7bor\u00b7ner"], "token_info": ["word"], "pos": ["NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Der hohen Natur!", "tokens": ["Der", "ho\u00b7hen", "Na\u00b7tur", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Aus Kronos Halle", "tokens": ["Aus", "Kro\u00b7nos", "Hal\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NE", "NE"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Schwebst du herab,", "tokens": ["Schwebst", "du", "her\u00b7ab", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Zu neuen, geheiligten Sch\u00f6pfungen", "tokens": ["Zu", "neu\u00b7en", ",", "ge\u00b7hei\u00b7lig\u00b7ten", "Sch\u00f6p\u00b7fun\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN"], "meter": "-+--+---+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Hold und majest\u00e4tisch herab.", "tokens": ["Hold", "und", "ma\u00b7jes\u00b7t\u00e4\u00b7tisch", "her\u00b7ab", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ADJD", "ADV", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.2": {"line.1": {"text": "Ha! bei der Unsterblichen,", "tokens": ["Ha", "!", "bei", "der", "U\u00b7nsterb\u00b7li\u00b7chen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "APPR", "ART", "NN", "$,"], "meter": "+--+---", "measure": "dactylic.di.plus"}, "line.2": {"text": "Die dich gebar,", "tokens": ["Die", "dich", "ge\u00b7bar", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Dir gleichet keiner", "tokens": ["Dir", "glei\u00b7chet", "kei\u00b7ner"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "PIS"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Unter den Br\u00fcdern,", "tokens": ["Un\u00b7ter", "den", "Br\u00fc\u00b7dern", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Den V\u00f6lkerbeherrschern,", "tokens": ["Den", "V\u00f6l\u00b7ker\u00b7be\u00b7herr\u00b7schern", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Den Angebeteten allen!", "tokens": ["Den", "An\u00b7ge\u00b7be\u00b7te\u00b7ten", "al\u00b7len", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Dir sang in der Wiege den Weihgesang", "tokens": ["Dir", "sang", "in", "der", "Wie\u00b7ge", "den", "Weih\u00b7ge\u00b7sang"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Im blutenden Panzer die ernste Gefahr,", "tokens": ["Im", "blu\u00b7ten\u00b7den", "Pan\u00b7zer", "die", "erns\u00b7te", "Ge\u00b7fahr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Zu gerechtem Siege reichte den Stahl", "tokens": ["Zu", "ge\u00b7rech\u00b7tem", "Sie\u00b7ge", "reich\u00b7te", "den", "Stahl"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "NN"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Die heilige Freiheit dir.", "tokens": ["Die", "hei\u00b7li\u00b7ge", "Frei\u00b7heit", "dir", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Von Freude gl\u00fchten,", "tokens": ["Von", "Freu\u00b7de", "gl\u00fch\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Von zaubrischer Liebe deine Schl\u00e4fe,", "tokens": ["Von", "zaub\u00b7ri\u00b7scher", "Lie\u00b7be", "dei\u00b7ne", "Schl\u00e4\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Die goldgelockten Schl\u00e4fe.", "tokens": ["Die", "gold\u00b7ge\u00b7lock\u00b7ten", "Schl\u00e4\u00b7fe", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Lange s\u00e4umtest du unter den G\u00f6ttern", "tokens": ["Lan\u00b7ge", "s\u00e4um\u00b7test", "du", "un\u00b7ter", "den", "G\u00f6t\u00b7tern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Und dachtest der kommenden Wunder.", "tokens": ["Und", "dach\u00b7test", "der", "kom\u00b7men\u00b7den", "Wun\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Vor\u00fcber schwebten wie silbern Gew\u00f6lk", "tokens": ["Vor\u00b7\u00fc\u00b7ber", "schweb\u00b7ten", "wie", "sil\u00b7bern", "Ge\u00b7w\u00f6lk"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "KOKOM", "ADJA", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Am liebenden Auge dir", "tokens": ["Am", "lie\u00b7ben\u00b7den", "Au\u00b7ge", "dir"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "PPER"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Die Geschlechter alle!", "tokens": ["Die", "Ge\u00b7schlech\u00b7ter", "al\u00b7le", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Die seligen Geschlechter.", "tokens": ["Die", "se\u00b7li\u00b7gen", "Ge\u00b7schlech\u00b7ter", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.5": {"line.1": {"text": "Im Angesichte der G\u00f6tter", "tokens": ["Im", "An\u00b7ge\u00b7sich\u00b7te", "der", "G\u00f6t\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Beschlo\u00df dein Mund,", "tokens": ["Be\u00b7schlo\u00df", "dein", "Mund", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Auf Liebe dein Reich zu gr\u00fcnden.", "tokens": ["Auf", "Lie\u00b7be", "dein", "Reich", "zu", "gr\u00fcn\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Da staunten die Himmlischen alle.", "tokens": ["Da", "staun\u00b7ten", "die", "Himm\u00b7li\u00b7schen", "al\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PIAT", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Zu br\u00fcderlicher Umarmung,", "tokens": ["Zu", "br\u00fc\u00b7der\u00b7li\u00b7cher", "Um\u00b7ar\u00b7mung", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Neigte sein k\u00f6niglich Haupt", "tokens": ["Neig\u00b7te", "sein", "k\u00f6\u00b7nig\u00b7lich", "Haupt"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "ADJD", "NN"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.7": {"text": "Der Donnerer nieder zu dir.", "tokens": ["Der", "Don\u00b7ne\u00b7rer", "nie\u00b7der", "zu", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "APPR", "PPER", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.8": {"text": "Du gr\u00fcndest auf Liebe dein Reich.", "tokens": ["Du", "gr\u00fcn\u00b7dest", "auf", "Lie\u00b7be", "dein", "Reich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "PPOSAT", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.6": {"line.1": {"text": "Du kommst und Orpheus Liebe", "tokens": ["Du", "kommst", "und", "Or\u00b7pheus", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "NE", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Schwebet empor zum Auge der Welt", "tokens": ["Schwe\u00b7bet", "em\u00b7por", "zum", "Au\u00b7ge", "der", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PTKVZ", "APPRART", "NN", "ART", "NN"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Und Orpheus Liebe", "tokens": ["Und", "Or\u00b7pheus", "Lie\u00b7be"], "token_info": ["word", "word", "word"], "pos": ["KON", "NE", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Wallet nieder zum Acheron.", "tokens": ["Wal\u00b7let", "nie\u00b7der", "zum", "A\u00b7che\u00b7ron", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "APPRART", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.5": {"text": "Du schwingest den Zauberstab,", "tokens": ["Du", "schwin\u00b7gest", "den", "Zau\u00b7ber\u00b7stab", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Und Aphrodit\u00e4s G\u00fcrtel ersieht", "tokens": ["Und", "A\u00b7phro\u00b7di\u00b7t\u00e4s", "G\u00fcr\u00b7tel", "er\u00b7sieht"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NE", "NE", "VVFIN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.7": {"text": "Der trunkene M\u00e4onide.", "tokens": ["Der", "trun\u00b7ke\u00b7ne", "M\u00e4o\u00b7ni\u00b7de", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+---+-", "measure": "dactylic.init"}, "line.8": {"text": "Ha! M\u00e4onide! wie du!", "tokens": ["Ha", "!", "M\u00e4o\u00b7ni\u00b7de", "!", "wie", "du", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$.", "NN", "$.", "PWAV", "PPER", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.9": {"text": "So liebte keiner, wie du;", "tokens": ["So", "lieb\u00b7te", "kei\u00b7ner", ",", "wie", "du", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "$,", "PWAV", "PPER", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.10": {"text": "Die Erd und Ozean", "tokens": ["Die", "Erd", "und", "O\u00b7ze\u00b7an"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Und die Riesengeister, die Helden der Erde", "tokens": ["Und", "die", "Rie\u00b7sen\u00b7geis\u00b7ter", ",", "die", "Hel\u00b7den", "der", "Er\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "$,", "ART", "NN", "ART", "NN"], "meter": "--+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "Umfa\u00dfte dein Herz!", "tokens": ["Um\u00b7fa\u00df\u00b7te", "dein", "Herz", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.13": {"text": "Und die Himmel und alle die Himmlischen", "tokens": ["Und", "die", "Him\u00b7mel", "und", "al\u00b7le", "die", "Himm\u00b7li\u00b7schen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "KON", "PIS", "ART", "NN"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.14": {"text": "Umfa\u00dfte dein Herz.", "tokens": ["Um\u00b7fa\u00df\u00b7te", "dein", "Herz", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.15": {"text": "Auch die Blumen, die Bien auf der Blume", "tokens": ["Auch", "die", "Blu\u00b7men", ",", "die", "Bi\u