{"textgrid.poem.24360": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Uber die Statue K\u00f6nig Wilhelms de\u00df III. in Dublin/ als selbige 1710. des Nachts j\u00e4mmerlich zugerichtet ward", "genre": "verse", "period": "N.A.", "pub_year": 1701, "urn": "N.A.", "language": ["de:0.71", "af:0.28"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Von b\u00f6sen Buben ward die ", "tokens": ["Von", "b\u00f6\u00b7sen", "Bu\u00b7ben", "ward", "die"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VAFIN", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die man dem ", "tokens": ["Die", "man", "dem"], "token_info": ["word", "word", "word"], "pos": ["ART", "PIS", "ART"], "meter": "++-", "measure": "unknown.measure.di"}, "line.3": {"text": "Als den Verbrechern nun ihr Urtheil zuerkandt/", "tokens": ["Als", "den", "Ver\u00b7bre\u00b7chern", "nun", "ihr", "Ur\u00b7theil", "zu\u00b7er\u00b7kandt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So nahm der K\u00e4yser selbst mit L\u00e4cheln seine Hand/", "tokens": ["So", "nahm", "der", "K\u00e4y\u00b7ser", "selbst", "mit", "L\u00e4\u00b7cheln", "sei\u00b7ne", "Hand", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "APPR", "NN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und sprach den Richtern zu: Beschauet mein Gesicht/", "tokens": ["Und", "sprach", "den", "Rich\u00b7tern", "zu", ":", "Be\u00b7schau\u00b7et", "mein", "Ge\u00b7sicht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKVZ", "$.", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ist keine Wunde da/ so straffet diese nicht.", "tokens": ["Ist", "kei\u00b7ne", "Wun\u00b7de", "da", "/", "so", "straf\u00b7fet", "die\u00b7se", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "ADV", "$(", "ADV", "VVFIN", "PDS", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Da nun die Bo\u00dfheit auch ", "tokens": ["Da", "nun", "die", "Bo\u00df\u00b7heit", "auch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "NN", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "So hat Europa schon die Finger aufgerichtet/", "tokens": ["So", "hat", "Eu\u00b7ro\u00b7pa", "schon", "die", "Fin\u00b7ger", "auf\u00b7ge\u00b7rich\u00b7tet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NE", "ADV", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Und wei\u00dft auf ihre Brust: hier/ spricht sie/ ist das Schild/", "tokens": ["Und", "wei\u00dft", "auf", "ih\u00b7re", "Brust", ":", "hier", "/", "spricht", "sie", "/", "ist", "das", "Schild", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "$.", "ADV", "$(", "VVFIN", "PPER", "$(", "VAFIN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Dahinter steht noch gantz Wilhelms des Helden Bild.", "tokens": ["Da\u00b7hin\u00b7ter", "steht", "noch", "gantz", "Wil\u00b7helms", "des", "Hel\u00b7den", "Bild", "."], "token_info": ["word", "w