{"textgrid.poem.36311": {"metadata": {"author": {"name": "Schlegel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Die neue Schule", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dieser wei\u00df sich sehr bescheiden,", "tokens": ["Die\u00b7ser", "wei\u00df", "sich", "sehr", "be\u00b7schei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PRF", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Jener bl\u00e4st die Backen voll;", "tokens": ["Je\u00b7ner", "bl\u00e4st", "die", "Ba\u00b7cken", "voll", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "VVFIN", "ART", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dieser ist im Ernste toll,", "tokens": ["Die\u00b7ser", "ist", "im", "Erns\u00b7te", "toll", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPRART", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Jener mu\u00df ihn noch beneiden.", "tokens": ["Je\u00b7ner", "mu\u00df", "ihn", "noch", "be\u00b7nei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Alle Narrheit kann ich leiden,", "tokens": ["Al\u00b7le", "Nar\u00b7rheit", "kann", "ich", "lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ob sie genialisch knalle,", "tokens": ["Ob", "sie", "ge\u00b7ni\u00b7a\u00b7lisch", "knal\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Oder blumenlieblich walle;", "tokens": ["O\u00b7der", "blu\u00b7men\u00b7lieb\u00b7lich", "wal\u00b7le", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Denn ich werd' es nie vergessen", "tokens": ["Denn", "ich", "werd'", "es", "nie", "ver\u00b7ges\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "PPER", "ADV", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Was des Meisters Kraft ermessen;", "tokens": ["Was", "des", "Meis\u00b7ters", "Kraft", "er\u00b7mes\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Eines schickt sich nicht f\u00fcr alle.", "tokens": ["Ei\u00b7nes", "schickt", "sich", "nicht", "f\u00fcr", "al\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PRF", "PTKNEG", "APPR", "PIS", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Um das Feuer zu ern\u00e4hren,", "tokens": ["Um", "das", "Feu\u00b7er", "zu", "er\u00b7n\u00e4h\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sind viel zarte Geister n\u00f6tig,", "tokens": ["Sind", "viel", "zar\u00b7te", "Geis\u00b7ter", "n\u00f6\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "ADJA", "NN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Die zu allem Dienst erb\u00f6tig,", "tokens": ["Die", "zu", "al\u00b7lem", "Dienst", "er\u00b7b\u00f6\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PIS", "NN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Um die Heiden zu bekehren.", "tokens": ["Um", "die", "Hei\u00b7den", "zu", "be\u00b7keh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Mag der L\u00e4rm sich nun vermehren,", "tokens": ["Mag", "der", "L\u00e4rm", "sich", "nun", "ver\u00b7meh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "PRF", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Suche jeder wen er reibe,", "tokens": ["Su\u00b7che", "je\u00b7der", "wen", "er", "rei\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PWS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Wisse jeder was er schreibe,", "tokens": ["Wis\u00b7se", "je\u00b7der", "was", "er", "schrei\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PWS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und wenn schrecklich alle Dummen", "tokens": ["Und", "wenn", "schreck\u00b7lich", "al\u00b7le", "Dum\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ADJD", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Aus den dunklen L\u00f6chern brummen,", "tokens": ["Aus", "den", "dunk\u00b7len", "L\u00f6\u00b7chern", "brum\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Sehe jeder wie er's treibe.", "tokens": ["Se\u00b7he", "je\u00b7der", "wie", "er's", "trei\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "KOKOM", "PIS", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Ein'ge haben wir entz\u00fcndet,", "tokens": ["Ein'\u00b7ge", "ha\u00b7ben", "wir", "ent\u00b7z\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die nun schon alleine flammen;", "tokens": ["Die", "nun", "schon", "al\u00b7lei\u00b7ne", "flam\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch die Menge h\u00e4lt zusammen,", "tokens": ["Doch", "die", "Men\u00b7ge", "h\u00e4lt", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Viel Gesindel treu verb\u00fcndet.", "tokens": ["Viel", "Ge\u00b7sin\u00b7del", "treu", "ver\u00b7b\u00fcn\u00b7det", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wer den Unverstand ergr\u00fcndet,", "tokens": ["Wer", "den", "Un\u00b7ver\u00b7stand", "er\u00b7gr\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "H\u00e4lt sich alle gern vom Leibe,", "tokens": ["H\u00e4lt", "sich", "al\u00b7le", "gern", "vom", "Lei\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PIS", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Die geboren sind vom Weibe.", "tokens": ["Die", "ge\u00b7bo\u00b7ren", "sind", "vom", "Wei\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VVPP", "VAFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Ist der Bienenschwarm erregt,", "tokens": ["Ist", "der", "Bie\u00b7nen\u00b7schwarm", "er\u00b7regt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Den das neu'ste Wort bewegt,", "tokens": ["Den", "das", "neu'\u00b7ste", "Wort", "be\u00b7wegt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Sehe jeder wo er bleibe.", "tokens": ["Se\u00b7he", "je\u00b7der", "wo", "er", "blei\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PWAV", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "M\u00f6gen sie gel\u00e4ufig schwatzen,", "tokens": ["M\u00f6\u00b7gen", "sie", "ge\u00b7l\u00e4u\u00b7fig", "schwat\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Was sie dennoch nie begreifen;", "tokens": ["Was", "sie", "den\u00b7noch", "nie", "be\u00b7grei\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Manche m\u00fcssen irre schweifen,", "tokens": ["Man\u00b7che", "m\u00fcs\u00b7sen", "ir\u00b7re", "schwei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PIS", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Viele K\u00fcnstler werden platzen.", "tokens": ["Vie\u00b7le", "K\u00fcnst\u00b7ler", "wer\u00b7den", "plat\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Jeden Sommer fliegen Spatzen,", "tokens": ["Je\u00b7den", "Som\u00b7mer", "flie\u00b7gen", "Spat\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Freuen sich am eignen Schalle,", "tokens": ["Freu\u00b7en", "sich", "am", "eig\u00b7nen", "Schal\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Reizte dies dir je die Galle?", "tokens": ["Reiz\u00b7te", "dies", "dir", "je", "die", "Gal\u00b7le", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDS", "PPER", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "La\u00df sie alle selig spielen,", "tokens": ["La\u00df", "sie", "al\u00b7le", "se\u00b7lig", "spie\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PIS", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Sorge du nur gut zu zielen,", "tokens": ["Sor\u00b7ge", "du", "nur", "gut", "zu", "zie\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Und wer steht, da\u00df er nicht falle.", "tokens": ["Und", "wer", "steht", ",", "da\u00df", "er", "nicht", "fal\u00b7le", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VVFIN", "$,", "KOUS", "PPER", "PTKNEG", "VVFIN", "$."], "meter": "--+-+-+-", "measure": "anapaest.init"}}, "stanza.5": {"line.1": {"text": "Dieser wei\u00df sich sehr bescheiden,", "tokens": ["Die\u00b7ser", "wei\u00df", "sich", "sehr", "be\u00b7schei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PRF", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Jener bl\u00e4st die Backen voll;", "tokens": ["Je\u00b7ner", "bl\u00e4st", "die", "Ba\u00b7cken", "voll", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "VVFIN", "ART", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dieser ist im Ernste toll,", "tokens": ["Die\u00b7ser", "ist", "im", "Erns\u00b7te", "toll", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPRART", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Jener mu\u00df ihn noch beneiden.", "tokens": ["Je\u00b7ner", "mu\u00df", "ihn", "noch", "be\u00b7nei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Alle Narrheit kann ich leiden,", "tokens": ["Al\u00b7le", "Nar\u00b7rheit", "kann", "ich", "lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "tr