{"textgrid.poem.38365": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Z z Zeisig", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Komm her du sch\u00f6nes Zeiselein, komm fliege her behend,", "tokens": ["Komm", "her", "du", "sch\u00f6\u00b7nes", "Zei\u00b7sel\u00b7ein", ",", "komm", "flie\u00b7ge", "her", "be\u00b7hend", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPER", "ADJA", "NN", "$,", "VVFIN", "VVFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+--++-+--", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Sing, spring auf gr\u00fcnem Reiselein, und mach dem Lied ein End,", "tokens": ["Sing", ",", "spring", "auf", "gr\u00fc\u00b7nem", "Rei\u00b7sel\u00b7ein", ",", "und", "mach", "dem", "Lied", "ein", "End", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "APPR", "ADJA", "NN", "$,", "KON", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.3": {"text": "Ihn preisen alle V\u00f6gelein mit ihrer s\u00fc\u00dfen Stimm.", "tokens": ["Ihn", "prei\u00b7sen", "al\u00b7le", "V\u00f6\u00b7ge\u00b7lein", "mit", "ih\u00b7rer", "s\u00fc\u00b7\u00dfen", "Stimm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}, "stanza.2": {"line.1": {"text": "Wohin geht all dies Dichten, du edles Federspiel,", "tokens": ["Wo\u00b7hin", "geht", "all", "dies", "Dich\u00b7ten", ",", "du", "ed\u00b7les", "Fe\u00b7der\u00b7spiel", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PIAT", "PDS", "NN", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Als da\u00df wir alles richten zu gutem End und Ziel,", "tokens": ["Als", "da\u00df", "wir", "al\u00b7les", "rich\u00b7ten", "zu", "gu\u00b7tem", "End", "und", "Ziel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "PIS", "VVINF", "PTKZU", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Da\u00df wir im Herzen sorgen f\u00fcr einen guten Klang,", "tokens": ["Da\u00df", "wir", "im", "Her\u00b7zen", "sor\u00b7gen", "f\u00fcr", "ei\u00b7nen", "gu\u00b7ten", "Klang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Wer weis ob heut, ob morgen uns r\u00fchrt der lezt Gesang.", "tokens": ["Wer", "weis", "ob", "heut", ",", "ob", "mor\u00b7gen", "uns", "r\u00fchrt", "der", "lezt", "Ge\u00b7sang", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PTKVZ", "KOUS", "ADV", "$,", "KOUS", "ADV", "PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "O sagt ihr lieben V\u00f6gelein, wer ists der euch erh\u00e4lt,", "tokens": ["O", "sagt", "ihr", "lie\u00b7ben", "V\u00f6\u00b7ge\u00b7lein", ",", "wer", "ists", "der", "euch", "er\u00b7h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPOSAT", "ADJA", "NN", "$,", "PWS", "VAFIN", "ART", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "Wo fliegt ihr hin, wo kehrt ihr ein, wenn Schnee im Winter f\u00e4llt,", "tokens": ["Wo", "fliegt", "ihr", "hin", ",", "wo", "kehrt", "ihr", "ein", ",", "wenn", "Schnee", "im", "Win\u00b7ter", "f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PTKVZ", "$,", "PWAV", "VVFIN", "PPER", "PTKVZ", "$,", "KOUS", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.3": {"text": "Wo nehmt ihr eure Nahrung, so viel als ihr begehrt?", "tokens": ["Wo", "nehmt", "ihr", "eu\u00b7re", "Nah\u00b7rung", ",", "so", "viel", "als", "ihr", "be\u00b7gehrt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PPOSAT", "NN", "$,", "ADV", "ADV", "KOUS", "PPER", "VVPP", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Es zeigt ja die Erfahrung, da\u00df Gott euch all ern\u00e4hrt.", "tokens": ["Es", "zeigt", "ja", "die", "Er\u00b7fah\u00b7rung", ",", "da\u00df", "Gott", "euch", "all", "er\u00b7n\u00e4hrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "$,", "KOUS", "NN", "PPER", "PIAT", "VVPP", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.4": {"line.1": {"text": "Ihr habt kein Feld, kein Heller Geld, nichts das die Tasche f\u00fcllt,", "tokens": ["Ihr", "habt", "kein", "Feld", ",", "kein", "Hel\u00b7ler", "Geld", ",", "nichts", "das", "die", "Ta\u00b7sche", "f\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "$,", "PIAT", "NN", "NN", "$,", "PIS", "ART", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "Der Tannebaum ist euer Zelt, troz dem, der euch was stiehlt,", "tokens": ["Der", "Tan\u00b7ne\u00b7baum", "ist", "eu\u00b7er", "Zelt", ",", "troz", "dem", ",", "der", "euch", "was", "stiehlt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "$,", "APPR", "ART", "$,", "PRELS", "PPER", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.3": {"text": "Euer Pflug ist lustig singen, stets lobt ihr Gott den Herrn,", "tokens": ["Eu\u00b7er", "Pflug", "ist", "lus\u00b7tig", "sin\u00b7gen", ",", "stets", "lobt", "ihr", "Gott", "den", "Herrn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "VVINF", "$,", "ADV", "VVFIN", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Die T\u00f6ne thut ihr schwingen bis zu dem Abendstern.", "tokens": ["Die", "T\u00f6\u00b7ne", "thut", "ihr", "schwin\u00b7gen", "bis", "zu", "dem", "A\u00b7bends\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "VVFIN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Ihr habt nicht Koch, nicht Keller, und seyd so wohlgemuth,", "tokens": ["Ihr", "habt", "nicht", "Koch", ",", "nicht", "Kel\u00b7ler", ",", "und", "seyd", "so", "wohl\u00b7ge\u00b7muth", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "NE", "$,", "PTKNEG", "NN", "$,", "KON", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Ihr trinkt nicht Muskateller, und habt so freudig Blut,", "tokens": ["Ihr", "trinkt", "nicht", "Mus\u00b7ka\u00b7tel\u00b7ler", ",", "und", "habt", "so", "freu\u00b7dig", "Blut", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "NN", "$,", "KON", "VAFIN", "ADV", "ADJD", "NN", "$,"], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Nichts haben, nichts begehren, ist euer Liverei,", "tokens": ["Nichts", "ha\u00b7ben", ",", "nichts", "be\u00b7geh\u00b7ren", ",", "ist", "eu\u00b7er", "Li\u00b7ve\u00b7rei", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "$,", "PIS", "VVINF", "$,", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Ihr habt ein guten Herren, er h\u00e4lt euch alle frei.", "tokens": ["Ihr", "habt", "ein", "gu\u00b7ten", "Her\u00b7ren", ",", "er", "h\u00e4lt", "euch", "al\u00b7le", "frei", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,", "PPER", "VVFIN", "PPER", "PIS", "ADJD", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.6": {"line.1": {"text": "Gott sey mein Herz auch heimgestellt, was er thut ist gethan,", "tokens": ["Gott", "sey", "mein", "Herz", "auch", "heim\u00b7ge\u00b7stellt", ",", "was", "er", "thut", "ist", "ge\u00b7than", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "ADV", "VVFIN", "$,", "PWS", "PPER", "VVFIN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "Wenn Sonn und Mond vom Himmel f\u00e4llt, er ists, der helfen kann,", "tokens": ["Wenn", "Sonn", "und", "Mond", "vom", "Him\u00b7mel", "f\u00e4llt", ",", "er", "ists", ",", "der", "hel\u00b7fen", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "APPRART", "NN", "VVFIN", "$,", "PPER", "VAFIN", "$,", "PRELS", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.3": {"text": "Was lebt auf Erd, in L\u00fcften schwebt, was sich im Wasser r\u00fchrt,", "tokens": ["Was", "lebt", "auf", "Erd", ",", "in", "L\u00fcf\u00b7ten", "schwebt", ",", "was", "sich", "im", "Was\u00b7ser", "r\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "APPR", "NN", "$,", "APPR", "NN", "VVFIN", "$,", "PRELS", "PRF", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "Gott all mit einem Finger hebt, ohn alle M\u00fch regiert.", "tokens": ["Gott", "all", "mit", "ei\u00b7nem", "Fin\u00b7ger", "hebt", ",", "ohn", "al\u00b7le", "M\u00fch", "re\u00b7giert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "APPR", "ART", "NN", "VVFIN", "$,", "KOUI", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}, "stanza.7": {"line.1": {"text": "Kein Sperling von dem Dache f\u00e4llt, von meinem Haupt kein Haar,", "tokens": ["Kein", "Sper\u00b7ling", "von", "dem", "Da\u00b7che", "f\u00e4llt", ",", "von", "mei\u00b7nem", "Haupt", "kein", "Haar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "ART", "NN", "VVFIN", "$,", "APPR", "PPOSAT", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "Es sey dann, da\u00df ihms wohlgef\u00e4llt, der ewig ist und war,", "tokens": ["Es", "sey", "dann", ",", "da\u00df", "ihms", "wohl\u00b7ge\u00b7f\u00e4llt", ",", "der", "e\u00b7wig", "ist", "und", "war", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "$,", "KOUS", "PPER", "VVFIN", "$,", "PRELS", "ADJD", "VAFIN", "KON", "VAFIN", "$,"], "meter": "-+--+--+-+-+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Er ruft dem Storch zu seiner Zeit, der Lerch, der Nachtigall,", "tokens": ["Er", "ruft", "dem", "Storch", "zu", "sei\u00b7ner", "Zeit", ",", "der", "Lerch", ",", "der", "Nach\u00b7ti\u00b7gall", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "Er f\u00fchr uns all zur Seeligkeit, bewahr uns vor dem Fall.", "tokens": ["Er", "f\u00fchr", "uns", "all", "zur", "See\u00b7lig\u00b7keit", ",", "be\u00b7wahr", "uns", "vor", "dem", "Fall", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIAT", "APPRART", "NN", "$,", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}, "stanza.8": {"line.1": {"text": "Den ganzen weiten Himmelssaal durchstreicht ihr Freudenklang,", "tokens": ["Den", "gan\u00b7zen", "wei\u00b7ten", "Him\u00b7mels\u00b7saal", "durch\u00b7streicht", "ihr", "Freu\u00b7den\u00b7klang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "Mit Freud dort ewig singen die Englein auf neun Ch\u00f6r,", "tokens": ["Mit", "Freud", "dort", "e\u00b7wig", "sin\u00b7gen", "die", "En\u00b7glein", "auf", "neun", "Ch\u00f6r", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "ADJD", "VVFIN", "ART", "NN", "APPR", "CARD", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Vor Freud thut ewig springen das ganze Himmelsheer.", "tokens": ["Vor", "Freud", "thut", "e\u00b7wig", "sprin\u00b7gen", "das", "gan\u00b7ze", "Him\u00b7mels\u00b7heer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ADJD", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.9": {"line.1": {"text": "Musik dort ewig w\u00e4hret, zu lang doch keinem w\u00e4hrt,", "tokens": ["Mu\u00b7sik", "dort", "e\u00b7wig", "w\u00e4h\u00b7ret", ",", "zu", "lang", "doch", "kei\u00b7nem", "w\u00e4hrt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADJD", "VVFIN", "$,", "PTKA", "ADJD", "ADV", "PIS", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Je mehr sie wird geh\u00f6ret, je mehr sie wird begehrt,", "tokens": ["Je", "mehr", "sie", "wird", "ge\u00b7h\u00f6\u00b7ret", ",", "je", "mehr", "sie", "wird", "be\u00b7gehrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "VAFIN", "VVPP", "$,", "ADV", "ADV", "PPER", "VAFIN", "VVPP", "$,"], "meter": "+--+-+--+-+-+", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Wer Gott hier thut verehren, ihm dient mit Sang und Klang,", "tokens": ["Wer", "Gott", "hier", "thut", "ver\u00b7eh\u00b7ren", ",", "ihm", "dient", "mit", "Sang", "und", "Klang", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "ADV", "VVFIN", "VVFIN", "$,", "PPER", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Der wird dort ewig h\u00f6ren himmlischen Vogelsang.", "tokens": ["Der", "wird", "dort", "e\u00b7wig", "h\u00f6\u00b7ren", "himm\u00b7li\u00b7schen", "Vo\u00b7gel\u00b7sang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ADJD", "VVINF", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.10": {"line.1": {"text": "Komm her du sch\u00f6nes Zeiselein, komm fliege her behend,", "tokens": ["Komm", "her", "du", "sch\u00f6\u00b7nes", "Zei\u00b7sel\u00b7ein", ",", "komm", "flie\u00b7ge", "her", "be\u00b7hend", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPER", "ADJA", "NN", "$,", "VVFIN", "VVFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+--++-+--", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Sing, spring auf gr\u00fcnem Reiselein, und mach dem Lied ein End,", "tokens": ["Sing", ",", "spring", "auf", "gr\u00fc\u00b7nem", "Rei\u00b7sel\u00b7ein", ",", "und", "mach", "dem", "Lied", "ein", "End", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "APPR", "ADJA", "NN", "$,", "KON", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.3": {"text": "Ihn preisen alle V\u00f6gelein mit i