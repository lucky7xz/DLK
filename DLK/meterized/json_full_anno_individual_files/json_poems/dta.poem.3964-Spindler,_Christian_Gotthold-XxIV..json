{"dta.poem.3964": {"metadata": {"author": {"name": "Spindler, Christian Gotthold", "birth": "N.A.", "death": "N.A."}, "title": "XxIV.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-20581-9", "language": ["de:0.99"], "booktitle": "Spindler, Christian Gotthold: Unschuldige Jugend-Fr\u00fcchte. Leipzig, 1745."}, "poem": {"stanza.1": {"line.1": {"text": "Ein Traum, ach englische Louise!", "tokens": ["Ein", "Traum", ",", "ach", "eng\u00b7li\u00b7sche", "Lou\u00b7i\u00b7se", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "XY", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Von dem beraubten Paradiese", "tokens": ["Von", "dem", "be\u00b7raub\u00b7ten", "Pa\u00b7ra\u00b7die\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Hat mich in letzter Nacht erschreckt,", "tokens": ["Hat", "mich", "in", "letz\u00b7ter", "Nacht", "er\u00b7schreckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der sey dir durch di\u00df Blat entdeckt.", "tokens": ["Der", "sey", "dir", "durch", "di\u00df", "Blat", "ent\u00b7deckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "APPR", "PDS", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}