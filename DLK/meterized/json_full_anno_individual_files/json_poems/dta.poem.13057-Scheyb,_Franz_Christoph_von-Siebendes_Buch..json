{"dta.poem.13057": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Siebendes Buch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201eder Mond begab sich hin, auch fast ein jeder Stern,", "tokens": ["\u201e", "der", "Mond", "be\u00b7gab", "sich", "hin", ",", "auch", "fast", "ein", "je\u00b7der", "Stern", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VVFIN", "PRF", "PTKVZ", "$,", "ADV", "ADV", "ART", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "\u201ean seinen Himmels-Krei\u00df; betrachtete von fern", "tokens": ["\u201e", "an", "sei\u00b7nen", "Him\u00b7mels\u00b7Krei\u00df", ";", "be\u00b7trach\u00b7te\u00b7te", "von", "fern"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "APPR", "PPOSAT", "NN", "$.", "ADJA", "APPR", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "\u201eder Sonne Gold-Triumpf. Je mehr man den sah funckeln,", "tokens": ["\u201e", "der", "Son\u00b7ne", "Gold\u00b7Tri\u00b7umpf", ".", "Je", "mehr", "man", "den", "sah", "fun\u00b7ckeln", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "NN", "$.", "ADV", "ADV", "PIS", "ART", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "\u201eje mehr sah man den Mond f\u00fcr Ehrfurcht sich verdunckeln.", "tokens": ["\u201e", "je", "mehr", "sah", "man", "den", "Mond", "f\u00fcr", "Ehr\u00b7furcht", "sich", "ver\u00b7dun\u00b7ckeln", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ADV", "VVFIN", "PIS", "ART", "NN", "APPR", "NN", "PRF", "VVINF", "$."], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.5": {"text": "525\u201dSo brach er also lo\u00df: Der Sonne fehlts an nichts;", "tokens": ["\"", "So", "brach", "er", "al\u00b7so", "lo\u00df", ":", "Der", "Son\u00b7ne", "fehlts", "an", "nichts", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPER", "ADV", "PTKVZ", "$.", "ART", "NN", "VVFIN", "APPR", "PIS", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "\u201ezu was bedarff sie dann des schwachen Sternen-Lichts?", "tokens": ["\u201e", "zu", "was", "be\u00b7darff", "sie", "dann", "des", "schwa\u00b7chen", "Ster\u00b7nen\u00b7Lichts", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "PIS", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "\u201esie prangt mit eigner Macht; sie kann sich selber kr\u00f6nen,", "tokens": ["\u201e", "sie", "prangt", "mit", "eig\u00b7ner", "Macht", ";", "sie", "kann", "sich", "sel\u00b7ber", "kr\u00f6\u00b7nen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$.", "PPER", "VMFIN", "PRF", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "\u201eich selber mu\u00df von ihr mein sch\u00f6nstes Licht entlehnen.", "tokens": ["\u201e", "ich", "sel\u00b7ber", "mu\u00df", "von", "ihr", "mein", "sch\u00f6ns\u00b7tes", "Licht", "ent\u00b7leh\u00b7nen", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "ADV", "VMFIN", "APPR", "PPER", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}