{"textgrid.poem.41678": {"metadata": {"author": {"name": "Baudelaire, Charles", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der Kurtisanen Blick, der seltsam zu uns gleitet,", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Kurtisanen Blick, der seltsam zu uns gleitet,", "tokens": ["Der", "Kur\u00b7ti\u00b7sa\u00b7nen", "Blick", ",", "der", "selt\u00b7sam", "zu", "uns", "glei\u00b7tet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ADJD", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dem fl\u00fcchtgen Zitterstrahl des blassen Mondes gleich,", "tokens": ["Dem", "fl\u00fccht\u00b7gen", "Zit\u00b7ter\u00b7strahl", "des", "blas\u00b7sen", "Mon\u00b7des", "gleich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wann er herniedertaucht zum leicht gerillten Teich,", "tokens": ["Wann", "er", "her\u00b7nie\u00b7der\u00b7taucht", "zum", "leicht", "ge\u00b7rill\u00b7ten", "Teich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "APPRART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "In dem um Silberglanz die Flut sich l\u00e4ssig breitet;", "tokens": ["In", "dem", "um", "Sil\u00b7ber\u00b7glanz", "die", "Flut", "sich", "l\u00e4s\u00b7sig", "brei\u00b7tet", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "APPR", "NN", "ART", "NN", "PRF", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Der letzte Beutel Gold in eines Spielers Hand;", "tokens": ["Der", "letz\u00b7te", "Beu\u00b7tel", "Gold", "in", "ei\u00b7nes", "Spie\u00b7lers", "Hand", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein t\u00e4ndelnd-dreister Ku\u00df der magren Adeline;", "tokens": ["Ein", "t\u00e4n\u00b7deln\u00b7ddreis\u00b7ter", "Ku\u00df", "der", "mag\u00b7ren", "A\u00b7de\u00b7li\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ein ferner Schmeichelklang von m\u00fcder Violine", "tokens": ["Ein", "fer\u00b7ner", "Schmei\u00b7chel\u00b7klang", "von", "m\u00fc\u00b7der", "Vi\u00b7o\u00b7li\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wie weher Klagelaut, der sich der Brust entwand ...", "tokens": ["Wie", "we\u00b7her", "Kla\u00b7ge\u00b7laut", ",", "der", "sich", "der", "Brust", "ent\u00b7wand", "..."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "NN", "$,", "PRELS", "PRF", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Das alles, Flasche, gleicht dem Gl\u00fcck nicht, das du sendest,", "tokens": ["Das", "al\u00b7les", ",", "Fla\u00b7sche", ",", "gleicht", "dem", "Gl\u00fcck", "nicht", ",", "das", "du", "sen\u00b7dest", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "$,", "NN", "$,", "VVFIN", "ART", "NN", "PTKNEG", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn du den Balsamtrank aus reichem Innern spendest,", "tokens": ["Wenn", "du", "den", "Bal\u00b7sam\u00b7trank", "aus", "rei\u00b7chem", "In\u00b7nern", "spen\u00b7dest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der das erloschne Herz des Dichters neu entfacht,", "tokens": ["Der", "das", "er\u00b7lo\u00b7schne", "Herz", "des", "Dich\u00b7ters", "neu", "ent\u00b7facht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "ART", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Du tr\u00e4ufelst Hoffnung ihm und Jugend und das Leben", "tokens": ["Du", "tr\u00e4u\u00b7felst", "Hoff\u00b7nung", "ihm", "und", "Ju\u00b7gend", "und", "das", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "PPER", "KON", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und Stolz, den einzgen Schatz, den Armut uns gegeben,", "tokens": ["Und", "Stolz", ",", "den", "einz\u00b7gen", "Schatz", ",", "den", "Ar\u00b7mut", "uns", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "ART", "ADJA", "NN", "$,", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text":