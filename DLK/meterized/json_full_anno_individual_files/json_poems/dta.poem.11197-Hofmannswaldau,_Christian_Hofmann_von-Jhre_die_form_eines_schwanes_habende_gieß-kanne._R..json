{"dta.poem.11197": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Jhre die form eines schwanes habende  \n gie\u00df-kanne.  \n  R.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wann sie sich w\u00e4scht, flieg ich auf ihren h\u00e4nden her:", "tokens": ["Wann", "sie", "sich", "w\u00e4scht", ",", "flieg", "ich", "auf", "ih\u00b7ren", "h\u00e4n\u00b7den", "her", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "VVFIN", "$,", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wann es vollbracht, schwimm ich: ihr becken ist mein", "tokens": ["Wann", "es", "voll\u00b7bracht", ",", "schwimm", "ich", ":", "ihr", "be\u00b7cken", "ist", "mein"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "VVPP", "$,", "VVFIN", "PPER", "$.", "PPER", "VVINF", "VAFIN", "PPOSAT"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "meer.", "tokens": ["meer", "."], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+", "measure": "single.up"}}}}}