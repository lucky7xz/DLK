{"dta.poem.19766": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Die wiedergefundene K\u00f6nigstochter.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519168", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es hat ein K\u00f6nig ein T\u00f6chterlein,               ", "tokens": ["Es", "hat", "ein", "K\u00f6\u00b7nig", "ein", "T\u00f6ch\u00b7ter\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Mit Namen hie\u00df es Annelein;", "tokens": ["Mit", "Na\u00b7men", "hie\u00df", "es", "An\u00b7nel\u00b7ein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es sa\u00df an einem Rainelein,", "tokens": ["Es", "sa\u00df", "an", "ei\u00b7nem", "Rai\u00b7nel\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Las auf die kleinen Steinelein.", "tokens": ["Las", "auf", "die", "klei\u00b7nen", "Stei\u00b7ne\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Es kam ein fremder Kr\u00e4mer in's Land,", "tokens": ["Es", "kam", "ein", "frem\u00b7der", "Kr\u00e4\u00b7mer", "in's", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Er wurf ihm dar ein seidnes Band: :,:", "tokens": ["Er", "wurf", "ihm", "dar", "ein", "seid\u00b7nes", "Band", ":", ":", ",", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "ART", "PPOSAT", "NN", "$.", "$.", "$,", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Jezt must du mit mir in fremde Land.", "tokens": ["Jezt", "must", "du", "mit", "mir", "in", "frem\u00b7de", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Er trugs vor einer Frau Wirthin Haus,", "tokens": ["Er", "trugs", "vor", "ei\u00b7ner", "Frau", "Wirt\u00b7hin", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "ART", "NN", "NN", "NN", "$,"], "meter": "-+-+-++-+", "measure": "unknown.measure.penta"}, "line.2": {"text": "Er gabs f\u00fcr einen Bankert aus:", "tokens": ["Er", "gabs", "f\u00fcr", "ei\u00b7nen", "Ban\u00b7kert", "aus", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Frau Wirthin, liebe Frau Wirthin mein,", "tokens": ["Frau", "Wirt\u00b7hin", ",", "lie\u00b7be", "Frau", "Wirt\u00b7hin", "mein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "ADJA", "NN", "NN", "PPOSAT", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Verdinget mir mein Kindelein.", "tokens": ["Ver\u00b7din\u00b7get", "mir", "mein", "Kin\u00b7de\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "O ja! o ja! das will ich wohl,", "tokens": ["O", "ja", "!", "o", "ja", "!", "das", "will", "ich", "wohl", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ITJ", "$.", "FM", "ADV", "$.", "PDS", "VMFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich will ihm thun doch also wohl, :,:", "tokens": ["Ich", "will", "ihm", "thun", "doch", "al\u00b7so", "wohl", ",", ":", ",", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct", "punct"], "pos": ["PPER", "VMFIN", "PPER", "VVINF", "KON", "ADV", "ADV", "$,", "$.", "$,", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gleich wie ein' Mutter eim Kind thun soll.", "tokens": ["Gleich", "wie", "ein'", "Mut\u00b7ter", "eim", "Kind", "thun", "soll", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ART", "NN", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Und als die Jahrszeit ummen war,", "tokens": ["Und", "als", "die", "Jahrs\u00b7zeit", "um\u00b7men", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und es zu seinen Jahren kam:", "tokens": ["Und", "es", "zu", "sei\u00b7nen", "Jah\u00b7ren", "kam", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es wollt ein Herr ausreiten", "tokens": ["Es", "wollt", "ein", "Herr", "aus\u00b7rei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und er wollt ausgahn weiben.", "tokens": ["Und", "er", "wollt", "aus\u00b7gahn", "wei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "VVINF", "VVINF", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}}, "stanza.6": {"line.1": {"text": "Er ritt vor einer Frau Wirthin Haus", "tokens": ["Er", "ritt", "vor", "ei\u00b7ner", "Frau", "Wirt\u00b7hin", "Haus"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "NN", "NN"], "meter": "-+-+-++-+", "measure": "unknown.measure.penta"}, "line.2": {"text": "Die sch\u00f6ne Magd treit ihm Wein heraus:", "tokens": ["Die", "sch\u00f6\u00b7ne", "Magd", "treit", "ihm", "Wein", "he\u00b7raus", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "NN", "PTKVZ", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Frau Wirthin, liebe Frau Wirthin mein! :,:", "tokens": ["Frau", "Wirt\u00b7hin", ",", "lie\u00b7be", "Frau", "Wirt\u00b7hin", "mein", "!", ":", ",", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct", "punct", "punct"], "pos": ["NN", "NN", "$,", "ADJA", "NN", "NN", "PPOSAT", "$.", "$(", "$,", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ist das euer T\u00f6chterlein?", "tokens": ["Ist", "das", "eu\u00b7er", "T\u00f6ch\u00b7ter\u00b7lein", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Oder ist es eures Sohnes Weib?", "tokens": ["O\u00b7der", "ist", "es", "eu\u00b7res", "Soh\u00b7nes", "Weib", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Da\u00df es so wundersch\u00f6n mag seyn. :,:", "tokens": ["Da\u00df", "es", "so", "wun\u00b7der\u00b7sch\u00f6n", "mag", "seyn", ".", ":", ",", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "VMFIN", "VAINF", "$.", "$.", "$,", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Es ist doch nicht mein T\u00f6chterlein,", "tokens": ["Es", "ist", "doch", "nicht", "mein", "T\u00f6ch\u00b7ter\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PTKNEG", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es ist doch nicht meines Sohnes Weib,", "tokens": ["Es", "ist", "doch", "nicht", "mei\u00b7nes", "Soh\u00b7nes", "Weib", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PTKNEG", "PPOSAT", "NN", "NE", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Es ist nur mein armes S\u00fcdeli,", "tokens": ["Es", "ist", "nur", "mein", "ar\u00b7mes", "S\u00fc\u00b7de\u00b7li", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Es weist meinen G\u00e4sten die St\u00fcbeli.", "tokens": ["Es", "weist", "mei\u00b7nen", "G\u00e4s\u00b7ten", "die", "St\u00fc\u00b7be\u00b7li", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "-+--+--+--", "measure": "amphibrach.tri.plus"}}, "stanza.8": {"line.1": {"text": "Frau Wirthin, liebe Frau Wirthin mein,", "tokens": ["Frau", "Wirt\u00b7hin", ",", "lie\u00b7be", "Frau", "Wirt\u00b7hin", "mein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "ADJA", "NN", "NN", "PPOSAT", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Erlaubet mir ein Nacht oder drei, :,:", "tokens": ["Er\u00b7lau\u00b7bet", "mir", "ein", "Nacht", "o\u00b7der", "drei", ",", ":", ",", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "KON", "CARD", "$,", "$.", "$,", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "So lang das euer Willen mag seyn!", "tokens": ["So", "lang", "das", "eu\u00b7er", "Wil\u00b7len", "mag", "seyn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "PPOSAT", "NN", "VMFIN", "VAINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "O ja! o ja! das will ich wol,", "tokens": ["O", "ja", "!", "o", "ja", "!", "das", "will", "ich", "wol", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ITJ", "$.", "FM", "ADV", "$.", "PDS", "VMFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es soll doch euch erlaubet sein, :,:", "tokens": ["Es", "soll", "doch", "euch", "er\u00b7lau\u00b7bet", "sein", ",", ":", ",", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PPER", "VVFIN", "VAINF", "$,", "$.", "$,", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So lang das euer Willen mag seyn.", "tokens": ["So", "lang", "das", "eu\u00b7er", "Wil\u00b7len", "mag", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "PPOSAT", "NN", "VMFIN", "VAINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Er nahm sch\u00f6n Annelein bei der Hand,", "tokens": ["Er", "nahm", "sch\u00f6n", "An\u00b7nel\u00b7ein", "bei", "der", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+----+-+", "measure": "dactylic.init"}, "line.2": {"text": "Er f\u00fchrt es in eine Schlafkammer lang,", "tokens": ["Er", "f\u00fchrt", "es", "in", "ei\u00b7ne", "Schlaf\u00b7kam\u00b7mer", "lang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "ADJD", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Er f\u00fchrt es vor ein sch\u00f6nes Bett,", "tokens": ["Er", "f\u00fchrt", "es", "vor", "ein", "sch\u00f6\u00b7nes", "Bett", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ob es die Nacht bei ihm schlafen w\u00f6lt.", "tokens": ["Ob", "es", "die", "Nacht", "bei", "ihm", "schla\u00b7fen", "w\u00f6lt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "PPER", "VVINF", "VVFIN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.11": {"line.1": {"text": "Der Herzog zog aus sein goldiges Schwerdt,", "tokens": ["Der", "Her\u00b7zog", "zog", "aus", "sein", "gol\u00b7di\u00b7ges", "Schwerdt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Er leit es zwischen beide Herz!", "tokens": ["Er", "leit", "es", "zwi\u00b7schen", "bei\u00b7de", "Herz", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das Schwerd soll weder hauen noch schneiden,", "tokens": ["Das", "Schwerd", "soll", "we\u00b7der", "hau\u00b7en", "noch", "schnei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "KON", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Das Annelein soll ein M\u00e4gedli bleiben.", "tokens": ["Das", "An\u00b7nel\u00b7ein", "soll", "ein", "M\u00e4\u00b7ged\u00b7li", "blei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.12": {"line.1": {"text": "Ach Annelein kehr dich umher!", "tokens": ["Ach", "An\u00b7nel\u00b7ein", "kehr", "dich", "um\u00b7her", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nun klag mir deinen Kummer schwer',", "tokens": ["Nun", "klag", "mir", "dei\u00b7nen", "Kum\u00b7mer", "schwer'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Klag mir alles was du weist,", "tokens": ["Klag", "mir", "al\u00b7les", "was", "du", "weist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PIS", "PWS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Was du in deinem Herzen treist.", "tokens": ["Was", "du", "in", "dei\u00b7nem", "Her\u00b7zen", "treist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.13": {"line.1": {"text": "Sag, wer ist dein Vater? Sag' wer ist deine", "tokens": ["Sag", ",", "wer", "ist", "dein", "Va\u00b7ter", "?", "Sag'", "wer", "ist", "dei\u00b7ne"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "PWS", "VAFIN", "PPOSAT", "NN", "$.", "NN", "PWS", "VAFIN", "PPOSAT"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Mutter?", "tokens": ["Mut\u00b7ter", "?"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "\u201eder Herr K\u00f6nig ist mein Vater, Frau K\u00f6nigin ist", "tokens": ["\u201e", "der", "Herr", "K\u00f6\u00b7nig", "ist", "mein", "Va\u00b7ter", ",", "Frau", "K\u00f6\u00b7ni\u00b7gin", "ist"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "ART", "NN", "NN", "VAFIN", "PPOSAT", "NN", "$,", "NN", "NN", "VAFIN"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.4": {"text": "meine Mutter,", "tokens": ["mei\u00b7ne", "Mut\u00b7ter", ","], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Ich hab einen Bruder hei\u00dft Mannigfalt,", "tokens": ["Ich", "hab", "ei\u00b7nen", "Bru\u00b7der", "hei\u00dft", "Man\u00b7nig\u00b7falt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVFIN", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plu