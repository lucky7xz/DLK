{"dta.poem.18844": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang von", "birth": "N.A.", "death": "N.A."}, "title": "Auserw\u00e4hlte Frauen .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1819", "urn": "urn:nbn:de:kobv:b4-200905191692", "language": ["de:0.99"], "booktitle": "Goethe, Johann Wolfgang von: West-\u00f6stlicher Divan. Stuttgart, 1819."}, "poem": {"stanza.1": {"line.1": {"text": "Frauen sollen nichts verlieren,", "tokens": ["Frau\u00b7en", "sol\u00b7len", "nichts", "ver\u00b7lie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PIS", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Reiner Treue ziemt zu hoffen;", "tokens": ["Rei\u00b7ner", "Treu\u00b7e", "ziemt", "zu", "hof\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch wir wissen nur von vieren", "tokens": ["Doch", "wir", "wis\u00b7sen", "nur", "von", "vie\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ADV", "APPR", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die alldort schon eingetroffen.", "tokens": ["Die", "all\u00b7dort", "schon", "ein\u00b7ge\u00b7trof\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "VVPP", "$."], "meter": "-++-+-+-", "measure": "unknown.measure.tetra"}}, "stanza.2": {"line.1": {"text": "Erst Suleika, Erdensonne,", "tokens": ["Erst", "Sul\u00b7ei\u00b7ka", ",", "Er\u00b7den\u00b7son\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADV", "NE", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gegen Jussuf ganz Begierde,", "tokens": ["Ge\u00b7gen", "Jus\u00b7suf", "ganz", "Be\u00b7gier\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Nun, des Paradieses Wonne,", "tokens": ["Nun", ",", "des", "Pa\u00b7ra\u00b7die\u00b7ses", "Won\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gl\u00e4nzt sie der Entsagung Zierde.", "tokens": ["Gl\u00e4nzt", "sie", "der", "Ent\u00b7sa\u00b7gung", "Zier\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Dann die Allgebenedeyte,", "tokens": ["Dann", "die", "All\u00b7ge\u00b7be\u00b7ne\u00b7dey\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die den Heiden Heil geboren,", "tokens": ["Die", "den", "Hei\u00b7den", "Heil", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und, get\u00e4uscht, in bitterm Leide,", "tokens": ["Und", ",", "ge\u00b7t\u00e4uscht", ",", "in", "bit\u00b7term", "Lei\u00b7de", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VVPP", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sah den Sohn am Kreuz verloren.", "tokens": ["Sah", "den", "Sohn", "am", "Kreuz", "ver\u00b7lo\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Mahom\u2019s Gattinn auch! Sie baute", "tokens": ["Ma\u00b7hom's", "Gat\u00b7tinn", "auch", "!", "Sie", "bau\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NE", "NN", "ADV", "$.", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wohlfahrt ihm und Herrlichkeiten,", "tokens": ["Wohl\u00b7fahrt", "ihm", "und", "Herr\u00b7lich\u00b7kei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KON", "NN", "$,"], "meter": "----+-+-", "measure": "unknown.measure.di"}, "line.3": {"text": "Und empfahl bey Lebenszeiten", "tokens": ["Und", "emp\u00b7fahl", "bey", "Le\u00b7bens\u00b7zei\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Einen Gott und eine Traute.", "tokens": ["Ei\u00b7nen", "Gott", "und", "ei\u00b7ne", "Trau\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Kommt Fatima dann die Holde,", "tokens": ["Kommt", "Fa\u00b7ti\u00b7ma", "dann", "die", "Hol\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "ADV", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Tochter, Gattinn sonder Fehle,", "tokens": ["Toch\u00b7ter", ",", "Gat\u00b7tinn", "son\u00b7der", "Feh\u00b7le", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Englisch allerreinste Seele", "tokens": ["Eng\u00b7lisch", "al\u00b7ler\u00b7reins\u00b7te", "See\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["NE", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In dem Leib von Honiggolde.", "tokens": ["In", "dem", "Leib", "von", "Ho\u00b7nig\u00b7gol\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Diese finden wir alldorten;", "tokens": ["Die\u00b7se", "fin\u00b7den", "wir", "all\u00b7dor\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und wer Frauenlob gepriesen", "tokens": ["Und", "wer", "Frau\u00b7en\u00b7lob", "ge\u00b7prie\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PWS", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Der verdient an ewigen Orten", "tokens": ["Der", "ver\u00b7dient", "an", "e\u00b7wi\u00b7gen", "Or\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "APPR", "ADJA", "NN"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Lustzuwandeln wohl mit diesen.", "tokens": ["Lust\u00b7zu\u00b7wan\u00b7deln", "wohl", "mit", "die\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "PDAT", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}