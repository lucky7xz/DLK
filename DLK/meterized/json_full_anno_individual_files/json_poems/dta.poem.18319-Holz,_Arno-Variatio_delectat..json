{"dta.poem.18319": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "Variatio delectat.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1886", "urn": "urn:nbn:de:kobv:b4-200905192665", "language": ["de:0.99"], "booktitle": "Holz, Arno: Das Buch der Zeit. Lieder eines Modernen. Z\u00fcrich, 1886."}, "poem": {"stanza.1": {"line.1": {"text": "Die Welt ist wirklich ein Narrenhaus.", "tokens": ["Die", "Welt", "ist", "wirk\u00b7lich", "ein", "Nar\u00b7ren\u00b7haus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "ART", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ewig sich selbst bleibt ihr uralter Schwindel,", "tokens": ["E\u00b7wig", "sich", "selbst", "bleibt", "ihr", "ur\u00b7al\u00b7ter", "Schwin\u00b7del", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PRF", "ADV", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Manchmal nur wechselt sie schlau seine Windel;", "tokens": ["Manch\u00b7mal", "nur", "wech\u00b7selt", "sie", "schlau", "sei\u00b7ne", "Win\u00b7del", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "ADJD", "PPOSAT", "NN", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.4": {"text": "Den Teufel verlacht sie und wirft sich ins Knie", "tokens": ["Den", "Teu\u00b7fel", "ver\u00b7lacht", "sie", "und", "wirft", "sich", "ins", "Knie"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "KON", "VVFIN", "PRF", "APPRART", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Vor der Mutter Gottes von Medici!", "tokens": ["Vor", "der", "Mut\u00b7ter", "Got\u00b7tes", "von", "Me\u00b7di\u00b7ci", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "APPR", "NE", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}}}}}