{"textgrid.poem.40812": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: \u00bbich schaute Bilder einst von Sudlerh\u00e4nden,", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbich schaute Bilder einst von Sudlerh\u00e4nden,", "tokens": ["\u00bb", "ich", "schau\u00b7te", "Bil\u00b7der", "einst", "von", "Sud\u00b7ler\u00b7h\u00e4n\u00b7den", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "NN", "ADV", "APPR", "NE", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da hatten Mond und Sonne Mund und Nasen,", "tokens": ["Da", "hat\u00b7ten", "Mond", "und", "Son\u00b7ne", "Mund", "und", "Na\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NN", "KON", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da sah den Sturm ich hinter Wolkenw\u00e4nden", "tokens": ["Da", "sah", "den", "Sturm", "ich", "hin\u00b7ter", "Wol\u00b7ken\u00b7w\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPER", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Als wind'gen Jungen volle Backen blasen.", "tokens": ["Als", "win\u00b7d'\u00b7gen", "Jun\u00b7gen", "vol\u00b7le", "Ba\u00b7cken", "bla\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "ADJA", "NN", "VVINF", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Ein \u00fcbler Maler ist der Schmerz, gleich ihnen,", "tokens": ["Ein", "\u00fcb\u00b7ler", "Ma\u00b7ler", "ist", "der", "Schmerz", ",", "gleich", "ih\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ART", "NN", "$,", "ADV", "PPER", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Denn, blick' ich auf aus diesen Finsternissen,", "tokens": ["Denn", ",", "blick'", "ich", "auf", "aus", "die\u00b7sen", "Fins\u00b7ter\u00b7nis\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VVFIN", "PPER", "APPR", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Seh' ich nur fromme, heil'ge Menschenmienen", "tokens": ["Seh'", "ich", "nur", "from\u00b7me", ",", "heil'\u00b7ge", "Men\u00b7schen\u00b7mie\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Als Sterne, Sonn' und Mond vom Himmel gr\u00fc\u00dfen.", "tokens": ["Als", "Ster\u00b7ne", ",", "Sonn'", "und", "Mond", "vom", "Him\u00b7mel", "gr\u00fc\u00b7\u00dfen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "$,", "NN", "KON", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "O Menschenantlitz, wundervoller Spiegel,", "tokens": ["O", "Men\u00b7schen\u00b7ant\u00b7litz", ",", "wun\u00b7der\u00b7vol\u00b7ler", "Spie\u00b7gel", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vom lauen Hauch der Gottheit leis umflossen!", "tokens": ["Vom", "lau\u00b7en", "Hauch", "der", "Got\u00b7theit", "leis", "um\u00b7flos\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Du heilig Buch, in dessen Purpursiegel", "tokens": ["Du", "hei\u00b7lig", "Buch", ",", "in", "des\u00b7sen", "Pur\u00b7pur\u00b7sie\u00b7gel"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "ADJD", "NN", "$,", "APPR", "PRELAT", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Des Himmels ew'ge R\u00e4thsel tief verschlossen!", "tokens": ["Des", "Him\u00b7mels", "ew'\u00b7ge", "R\u00e4th\u00b7sel", "tief", "ver\u00b7schlos\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Doch ist der Spiegel unpolirt befunden,", "tokens": ["Doch", "ist", "der", "Spie\u00b7gel", "un\u00b7po\u00b7lirt", "be\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das sch\u00f6ne Buch verklebt mit schn\u00f6dem Kleister", "tokens": ["Das", "sch\u00f6\u00b7ne", "Buch", "ver\u00b7klebt", "mit", "schn\u00f6\u00b7dem", "Kleis\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und, ach, in Fell unsaub'ren Thiers gebunden.", "tokens": ["Und", ",", "ach", ",", "in", "Fell", "un\u00b7saub'\u00b7ren", "Thiers", "ge\u00b7bun\u00b7den", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ITJ", "$,", "APPR", "NN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Und dennoch, was verloren ich mit Beben,", "tokens": ["Und", "den\u00b7noch", ",", "was", "ver\u00b7lo\u00b7ren", "ich", "mit", "Be\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "PWS", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich les' es drin, in altem Glanze tagend!", "tokens": ["Ich", "les'", "es", "drin", ",", "in", "al\u00b7tem", "Glan\u00b7ze", "ta\u00b7gend", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$,", "APPR", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "All', was ein Antlitz nur vermag zu geben,", "tokens": ["All'", ",", "was", "ein", "Ant\u00b7litz", "nur", "ver\u00b7mag", "zu", "ge\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "PRELS", "ART", "NN", "ADV", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Gibt deines mir, wenn Alles gleich versagend!", "tokens": ["Gibt", "dei\u00b7nes", "mir", ",", "wenn", "Al\u00b7les", "gleich", "ver\u00b7sa\u00b7gend", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "PPER", "$,", "KOUS", "PIS", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Wie, als der Lava schwarze Krusten sprangen,", "tokens": ["Wie", ",", "als", "der", "La\u00b7va", "schwar\u00b7ze", "Krus\u00b7ten", "spran\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "KOUS", "ART", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das heitre Bild des Liebesgotts draus blickte,", "tokens": ["Das", "heit\u00b7re", "Bild", "des", "Lie\u00b7bes\u00b7gotts", "draus", "blick\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "PAV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So find' im Furchenschutte deiner Wangen", "tokens": ["So", "find'", "im", "Fur\u00b7chen\u00b7schut\u00b7te", "dei\u00b7ner", "Wan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das L\u00e4cheln ich, de\u00df Glanz mich einst entz\u00fcckte.", "tokens": ["Das", "L\u00e4\u00b7cheln", "ich", ",", "de\u00df", "Glanz", "mich", "einst", "ent\u00b7z\u00fcck\u00b7te", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "$,", "ART", "NN", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Die Wolken deiner Stirne m\u00fcssen sinken,", "tokens": ["Die", "Wol\u00b7ken", "dei\u00b7ner", "Stir\u00b7ne", "m\u00fcs\u00b7sen", "sin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich lasse reinen, lichten Himmel tagen,", "tokens": ["Ich", "las\u00b7se", "rei\u00b7nen", ",", "lich\u00b7ten", "Him\u00b7mel", "ta\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "$,", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Drauf der Gedanken Stern' und Sonnen blinken,", "tokens": ["Drauf", "der", "Ge\u00b7dan\u00b7ken", "Stern'", "und", "Son\u00b7nen", "blin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und k\u00fchn gewalt'ge Regenbogen schlagen.", "tokens": ["Und", "k\u00fchn", "ge\u00b7walt'\u00b7ge", "Re\u00b7gen\u00b7bo\u00b7gen", "schla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Die Augen dein, im Zauberschlaf seit Jahren", "tokens": ["Die", "Au\u00b7gen", "dein", ",", "im", "Zau\u00b7ber\u00b7schlaf", "seit", "Jah\u00b7ren"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "$,", "APPRART", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zween B\u00e4ren gleich in busch'ger H\u00f6hle sitzend", "tokens": ["Zween", "B\u00e4\u00b7ren", "gleich", "in", "busch'\u00b7ger", "H\u00f6h\u00b7le", "sit\u00b7zend"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "NN", "ADV", "APPR", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Den Bann l\u00f6s' ich! Sie werden, was sie waren:", "tokens": ["Den", "Bann", "l\u00f6s'", "ich", "!", "Sie", "wer\u00b7den", ",", "was", "sie", "wa\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$.", "PPER", "VAFIN", "$,", "PRELS", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zwei K\u00f6nigskinder, in Demanten blitzend!", "tokens": ["Zwei", "K\u00f6\u00b7nigs\u00b7kin\u00b7der", ",", "in", "De\u00b7man\u00b7ten", "blit\u00b7zend", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "$,", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Dein Mund, versperrt wie dieses Kerkers Pforte,", "tokens": ["Dein", "Mund", ",", "ver\u00b7sperrt", "wie", "die\u00b7ses", "Ker\u00b7kers", "Pfor\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "KOKOM", "PDAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Er thut sich auf nun als Triumphesbogen,", "tokens": ["Er", "thut", "sich", "auf", "nun", "als", "Tri\u00b7um\u00b7phes\u00b7bo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ADV", "KOUS", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Draus die geharn'schten Sieger: Ernstesworte,", "tokens": ["Draus", "die", "ge\u00b7harn'schten", "Sie\u00b7ger", ":", "Erns\u00b7tes\u00b7wor\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PAV", "ART", "ADJA", "NN", "$.", "NN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.4": {"text": "Bekr\u00e4nzte Jungfraun: Liebesworte wogen.", "tokens": ["Be\u00b7kr\u00e4nz\u00b7te", "Jung\u00b7fraun", ":", "Lie\u00b7bes\u00b7wor\u00b7te", "wo\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Dein Busen, klanglos, wie die d\u00fcrre Scholle,", "tokens": ["Dein", "Bu\u00b7sen", ",", "klang\u00b7los", ",", "wie", "die", "d\u00fcr\u00b7re", "Schol\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADJD", "$,", "PWAV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "W\u00f6lbt sich zum Dom voll s\u00fc\u00dfer Liedert\u00f6ne;", "tokens": ["W\u00f6lbt", "sich", "zum", "Dom", "voll", "s\u00fc\u00b7\u00dfer", "Lie\u00b7der\u00b7t\u00f6\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPRART", "NN", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Aus deines Leibs formloser Felsenrolle", "tokens": ["Aus", "dei\u00b7nes", "Leibs", "form\u00b7lo\u00b7ser", "Fel\u00b7sen\u00b7rol\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Entsteigt der delph'sche Gott in ew'ger Sch\u00f6ne!", "tokens": ["Ent\u00b7steigt", "der", "del\u00b7ph'\u00b7sche", "Gott", "in", "ew'\u00b7ger", "Sch\u00f6\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.11": {"line.1": {"text": "Selbst deiner eh'rnen Hand kann ich nicht z\u00fcrnen,", "tokens": ["Selbst", "dei\u00b7ner", "eh'r\u00b7nen", "Hand", "kann", "ich", "nicht", "z\u00fcr\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "ADJA", "NN", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wenn sie die Fesseln pr\u00fcft, ob sie nicht weichen;", "tokens": ["Wenn", "sie", "die", "Fes\u00b7seln", "pr\u00fcft", ",", "ob", "sie", "nicht", "wei\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$,", "KOUS", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ich seh' sie Kron' und Lorber w\u00fcrd'gen Stirnen", "tokens": ["Ich", "seh'", "sie", "Kron'", "und", "Lor\u00b7ber", "w\u00fcrd'\u00b7gen", "Stir\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "NN", "KON", "NN", "VVFIN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und mild ein labend Brod der Armut reichen.", "tokens": ["Und", "mild", "ein", "la\u00b7bend", "Brod", "der", "Ar\u00b7mut", "rei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ART", "ADJD", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.12": {"line.1": {"text": "Du finstrer Schlie\u00dfer dieser ird'schen H\u00f6lle,", "tokens": ["Du", "finst\u00b7rer", "Schlie\u00b7\u00dfer", "die\u00b7ser", "ird'\u00b7schen", "H\u00f6l\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie jauchzt mein Herz bei deiner Schl\u00fcssel Klingen!", "tokens": ["Wie", "jauchzt", "mein", "Herz", "bei", "dei\u00b7ner", "Schl\u00fcs\u00b7sel", "Klin\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Du bist Sankt Peter mir, vor dem zur Stelle", "tokens": ["Du", "bist", "Sankt", "Pe\u00b7ter", "mir", ",", "vor", "dem", "zur", "Stel\u00b7le"], "token_info": ["word", "word", "word", "word", "word"