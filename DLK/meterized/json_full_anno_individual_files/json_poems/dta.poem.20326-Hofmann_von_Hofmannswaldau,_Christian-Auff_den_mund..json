{"dta.poem.20326": {"metadata": {"author": {"name": "Hofmann von Hofmannswaldau, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Auff den mund.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1695", "urn": "urn:nbn:de:kobv:b4-200905197751", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Sch\u00f6ner mund/ darff ich dich fragen/", "tokens": ["Sch\u00f6\u00b7ner", "mund", "/", "darff", "ich", "dich", "fra\u00b7gen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$(", "VMFIN", "PPER", "PRF", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Was bedeut dein purpur-schein?", "tokens": ["Was", "be\u00b7deut", "dein", "pur\u00b7pur\u00b7schein", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Weil die augen Ph\u00f6bus-wagen/", "tokens": ["Weil", "die", "au\u00b7gen", "Ph\u00f6\u00b7bus\u00b7wa\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "So must du Aurora seyn.", "tokens": ["So", "must", "du", "Au\u00b7ro\u00b7ra", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "NE", "VAINF", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}