{"textgrid.poem.55342": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "1", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich sah die Welt mit liebevollen Blicken,", "tokens": ["Ich", "sah", "die", "Welt", "mit", "lie\u00b7be\u00b7vol\u00b7len", "Bli\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und Welt und ich, wir schwelgten im Entz\u00fccken;", "tokens": ["Und", "Welt", "und", "ich", ",", "wir", "schwelg\u00b7ten", "im", "Ent\u00b7z\u00fc\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "PPER", "$,", "PPER", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So duftig war, belebend, immer frisch", "tokens": ["So", "duf\u00b7tig", "war", ",", "be\u00b7le\u00b7bend", ",", "im\u00b7mer", "frisch"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["ADV", "ADJD", "VAFIN", "$,", "VVPP", "$,", "ADV", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wie Fels, wie Strom, so Bergwald und Geb\u00fcsch.", "tokens": ["Wie", "Fels", ",", "wie", "Strom", ",", "so", "Berg\u00b7wald", "und", "Ge\u00b7b\u00fcsch", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "$,", "PWAV", "NN", "$,", "ADV", "NE", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Doch unverm\u00f6gend Streben, Nachgelalle", "tokens": ["Doch", "un\u00b7ver\u00b7m\u00f6\u00b7gend", "Stre\u00b7ben", ",", "Nach\u00b7ge\u00b7lal\u00b7le"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["KON", "ADJD", "NN", "$,", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Bracht oft den Stift, den Pinsel bracht's zu Falle;", "tokens": ["Bracht", "oft", "den", "Stift", ",", "den", "Pin\u00b7sel", "bracht's", "zu", "Fal\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$,", "ART", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "---+-+-+-+-", "measure": "unknown.measure.tetra"}, "line.7": {"text": "Auf neues Wagnis endlich blieb doch nur", "tokens": ["Auf", "neu\u00b7es", "Wag\u00b7nis", "end\u00b7lich", "blieb", "doch", "nur"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ADV", "VVFIN", "ADV", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Vom besten Wollen halb und halbe Spur.", "tokens": ["Vom", "bes\u00b7ten", "Wol\u00b7len", "halb", "und", "hal\u00b7be", "Spur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ADJD", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ihr J\u00fcngern aber, die ihr unverzagt", "tokens": ["Ihr", "J\u00fcn\u00b7gern", "a\u00b7ber", ",", "die", "ihr", "un\u00b7ver\u00b7zagt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADV", "$,", "PRELS", "PPER", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Unausgesprochnes auszusprechen wagt,", "tokens": ["Un\u00b7aus\u00b7ge\u00b7sproch\u00b7nes", "aus\u00b7zu\u00b7spre\u00b7chen", "wagt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "VVIZU", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Den Sinn, woran die Hand sich stotternd ma\u00df,", "tokens": ["Den", "Sinn", ",", "wo\u00b7ran", "die", "Hand", "sich", "stot\u00b7ternd", "ma\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "ART", "NN", "PRF", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Das Unverm\u00f6gen liebevoll verga\u00df,", "tokens": ["Das", "Un\u00b7ver\u00b7m\u00f6\u00b7gen", "lie\u00b7be\u00b7voll", "ver\u00b7ga\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ihr seid es, die, was ich und ihr gefehlt,", "tokens": ["Ihr", "seid", "es", ",", "die", ",", "was", "ich", "und", "ihr", "ge\u00b7fehlt", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "$,", "PRELS", "$,", "PWS", "PPER", "KON", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Dem weiten Kreis der Kunstwelt nicht verhehlt.", "tokens": ["Dem", "wei\u00b7ten", "Kreis", "der", "Kunst\u00b7welt", "nicht", "ver\u00b7hehlt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und wie dem Walde geht's den Bl\u00e4ttern allen,", "tokens": ["Und", "wie", "dem", "Wal\u00b7de", "geht's", "den", "Bl\u00e4t\u00b7tern", "al\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "VVFIN", "ART", "NN", "PIAT", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Sie knospen, gr\u00fcnen, welken ab und fallen.", "tokens": ["Sie", "knos