{"dta.poem.13868": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "An Philipp .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "So ist unser Fest vergangen,", "tokens": ["So", "ist", "un\u00b7ser", "Fest", "ver\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und die lust'gen Kerzen l\u00f6schen aus,", "tokens": ["Und", "die", "lust'\u00b7gen", "Ker\u00b7zen", "l\u00f6\u00b7schen", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Doch die Sterne drau\u00dfen prangen,", "tokens": ["Doch", "die", "Ster\u00b7ne", "drau\u00b7\u00dfen", "pran\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und die f\u00fchren mich und Dich nach Haus.", "tokens": ["Und", "die", "f\u00fch\u00b7ren", "mich", "und", "Dich", "nach", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "VVFIN", "PPER", "KON", "PPER", "APPR", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}