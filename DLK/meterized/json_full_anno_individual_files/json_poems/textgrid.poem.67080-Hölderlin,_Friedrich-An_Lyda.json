{"textgrid.poem.67080": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "An Lyda", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Trunken, wie im hellen Morgenstrahle", "tokens": ["Trun\u00b7ken", ",", "wie", "im", "hel\u00b7len", "Mor\u00b7gen\u00b7strah\u00b7le"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "PWAV", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Der Pilote seinen Ozean,", "tokens": ["Der", "Pi\u00b7lo\u00b7te", "sei\u00b7nen", "O\u00b7ze\u00b7an", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Wie die Seligen Elysens Tale", "tokens": ["Wie", "die", "Se\u00b7li\u00b7gen", "E\u00b7ly\u00b7sens", "Ta\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "NE", "NE"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Staunt ich meiner Liebe Freuden an,", "tokens": ["Staunt", "ich", "mei\u00b7ner", "Lie\u00b7be", "Freu\u00b7den", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Tal' und Haine lachten neugeboren,", "tokens": ["Tal'", "und", "Hai\u00b7ne", "lach\u00b7ten", "neu\u00b7ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "ADJA", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Wo ich wallte, trank ich G\u00f6ttlichkeit,", "tokens": ["Wo", "ich", "wall\u00b7te", ",", "trank", "ich", "G\u00f6tt\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Ha! von ihr zum Liebling auserkoren,", "tokens": ["Ha", "!", "von", "ihr", "zum", "Lieb\u00b7ling", "au\u00b7ser\u00b7ko\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "APPR", "PPOSAT", "APPRART", "NN", "VVIZU", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "H\u00f6hnt ich stolzen Muts Geschick und Zeit.", "tokens": ["H\u00f6hnt", "ich", "stol\u00b7zen", "Muts", "Ge\u00b7schick", "und", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJA", "NN", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Stolzer ward und edler das Verlangen,", "tokens": ["Stol\u00b7zer", "ward", "und", "ed\u00b7ler", "das", "Ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "KON", "ADJA", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Als mein Geist der Liebe Kraft erschwang,", "tokens": ["Als", "mein", "Geist", "der", "Lie\u00b7be", "Kraft", "er\u00b7schwang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Myriaden w\u00e4hnt ich zu umfangen,", "tokens": ["My\u00b7ria\u00b7den", "w\u00e4hnt", "ich", "zu", "um\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn ich Liebe, trunken Liebe sang,", "tokens": ["Wenn", "ich", "Lie\u00b7be", ",", "trun\u00b7ken", "Lie\u00b7be", "sang", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "$,", "ADJD", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Wie der Fr\u00fchlingshimmel, weit und helle,", "tokens": ["Wie", "der", "Fr\u00fch\u00b7lings\u00b7him\u00b7mel", ",", "weit", "und", "hel\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "ADJD", "KON", "ADJA", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Wie die Perle sch\u00f6n und ungetr\u00fcbt,", "tokens": ["Wie", "die", "Per\u00b7le", "sch\u00f6n", "und", "un\u00b7ge\u00b7tr\u00fcbt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Rein und stille wie der Weisheit Quelle", "tokens": ["Rein", "und", "stil\u00b7le", "wie", "der", "Weis\u00b7heit", "Quel\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "VVFIN", "KOKOM", "ART", "NN", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "War das Herz von ihr, von ihr geliebt.", "tokens": ["War", "das", "Herz", "von", "ihr", ",", "von", "ihr", "ge\u00b7liebt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "APPR", "PPER", "$,", "APPR", "PPER", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Sieh! im Stolze hatt ich oft geschworen,", "tokens": ["Sieh", "!", "im", "Stol\u00b7ze", "hatt", "ich", "oft", "ge\u00b7schwo\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "APPRART", "ADJA", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Unverg\u00e4nglich dieser Herzverein!", "tokens": ["Un\u00b7ver\u00b7g\u00e4ng\u00b7lich", "die\u00b7ser", "Herz\u00b7ver\u00b7ein", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PDAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Lyda mir, zum Heile mir geboren,", "tokens": ["Ly\u00b7da", "mir", ",", "zum", "Hei\u00b7le", "mir", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "$,", "APPRART", "NN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Lyda mein, wie meine Seele mein,", "tokens": ["Ly\u00b7da", "mein", ",", "wie", "mei\u00b7ne", "See\u00b7le", "mein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "$,", "PWAV", "PPOSAT", "NN", "PPOSAT", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Aber neidisch trat die Scheidestunde,", "tokens": ["A\u00b7ber", "nei\u00b7disch", "trat", "die", "Schei\u00b7des\u00b7tun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Treues M\u00e4dchen! zwischen mich und dich,", "tokens": ["Treu\u00b7es", "M\u00e4d\u00b7chen", "!", "zwi\u00b7schen", "mich", "und", "dich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "APPR", "PPER", "KON", "PPER", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Nimmer, nimmer auf dem Erdenrunde,", "tokens": ["Nim\u00b7mer", ",", "nim\u00b7mer", "auf", "dem", "Er\u00b7den\u00b7run\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Lyda! nahn die trauten Arme sich.", "tokens": ["Ly\u00b7da", "!", "nahn", "die", "trau\u00b7ten", "Ar\u00b7me", "sich", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "APPR", "ART", "ADJA", "NN", "PRF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Stille wallst du nun am Rebenh\u00fcgel,", "tokens": ["Stil\u00b7le", "wallst", "du", "nun", "am", "Re\u00b7ben\u00b7h\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wo ich dich und deinen Himmel fand,", "tokens": ["Wo", "ich", "dich", "und", "dei\u00b7nen", "Him\u00b7mel", "fand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "KON", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Wo dein Auge, deiner W\u00fcrde Spiegel,", "tokens": ["Wo", "dein", "Au\u00b7ge", ",", "dei\u00b7ner", "W\u00fcr\u00b7de", "Spie\u00b7gel", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Mich allm\u00e4chtig, ewig an dich band!", "tokens": ["Mich", "all\u00b7m\u00e4ch\u00b7tig", ",", "e\u00b7wig", "an", "dich", "band", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "$,", "ADJD", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Schnell ist unser Fr\u00fchling hingeflogen!", "tokens": ["Schnell", "ist", "un\u00b7ser", "Fr\u00fch\u00b7ling", "hin\u00b7ge\u00b7flo\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "O du Einzige! vergib, vergib!", "tokens": ["O", "du", "Ein\u00b7zi\u00b7ge", "!", "ver\u00b7gib", ",", "ver\u00b7gib", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "NE", "ADJA", "$.", "VVIMP", "$,", "VVIMP", "$."], "meter": "+-+---+-+", "measure": "unknown.measure.tetra"}, "line.7": {"text": "Deinen Frieden hat sie dir entzogen,", "tokens": ["Dei\u00b7nen", "Frie\u00b7den", "hat", "sie", "dir", "ent\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Meine Liebe, tr\u00e4nenvoll und tr\u00fcb.", "tokens": ["Mei\u00b7ne", "Lie\u00b7be", ",", "tr\u00e4\u00b7nen\u00b7voll", "und", "tr\u00fcb", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Als ich deinem Zauber hingegeben", "tokens": ["Als", "ich", "dei\u00b7nem", "Zau\u00b7ber", "hin\u00b7ge\u00b7ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Erd und Himmel \u00fcber dir verga\u00df,", "tokens": ["Erd", "und", "Him\u00b7mel", "\u00fc\u00b7ber", "dir", "ver\u00b7ga\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Ach! so selig in der Liebe Leben,", "tokens": ["Ach", "!", "so", "se\u00b7lig", "in", "der", "Lie\u00b7be", "Le\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "ADV", "ADJD", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Lyda! meine Lyda! dacht ich das?", "tokens": ["Ly\u00b7da", "!", "mei\u00b7ne", "Ly\u00b7da", "!", "dacht", "ich", "das", "?"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PPOSAT", "NE", "$.", "VVFIN", "PPER", "PDS", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "Trunken, wie im hellen Morgenstrahle", "tokens": ["Trun\u00b7ken", ",", "wie", "im", "hel\u00b7len", "Mor\u00b7gen\u00b7strah\u00b7le"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "PWAV", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Der Pilote seinen Ozean,", "tokens": ["Der", "Pi\u00b7lo\u00b7te", "sei\u00b7nen", "O\u00b7ze\u00b7an", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Wie die Seligen Elysens Tale", "tokens": ["Wie", "die", "Se\u00b7li\u00b7gen", "E\u00b7ly\u00b7sens", "Ta\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "NE", "NE"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Staunt ich meiner Liebe Freuden an,", "tokens": ["Staunt", "ich", "mei\u00b7ner", "Lie\u00b7be", "Freu\u00b7den", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+", "measure