{"textgrid.poem.62405": {"metadata": {"author": {"name": "Schenkendorf, Max von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Hat das Geisterreich mich auserlesen,", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hat das Geisterreich mich auserlesen,", "tokens": ["Hat", "das", "Geis\u00b7ter\u00b7reich", "mich", "au\u00b7ser\u00b7le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Hab' ich seinen Winken je gelauscht,", "tokens": ["Hab'", "ich", "sei\u00b7nen", "Win\u00b7ken", "je", "ge\u00b7lauscht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPOSAT", "NN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Irdisches mit Ewigen vertauscht \u2013", "tokens": ["Ir\u00b7di\u00b7sches", "mit", "E\u00b7wi\u00b7gen", "ver\u00b7tauscht", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ADJA", "VVPP", "$("], "meter": "+-+-+---+", "measure": "unknown.measure.tetra"}, "line.4": {"text": "O so la\u00df mich jetzt das R\u00e4thsel l\u00f6sen!", "tokens": ["O", "so", "la\u00df", "mich", "jetzt", "das", "R\u00e4th\u00b7sel", "l\u00f6\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "VVIMP", "PPER", "ADV", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Wie dem J\u00fcngling die erharrte Stunde,", "tokens": ["Wie", "dem", "J\u00fcng\u00b7ling", "die", "er\u00b7harr\u00b7te", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "ADJA", "