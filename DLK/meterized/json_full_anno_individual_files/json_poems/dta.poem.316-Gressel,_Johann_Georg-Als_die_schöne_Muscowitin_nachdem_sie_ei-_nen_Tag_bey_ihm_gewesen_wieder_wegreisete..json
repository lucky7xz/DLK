{"dta.poem.316": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Als die sch\u00f6ne Muscowitin/ nachdem sie ei-  \n nen Tag bey ihm gewesen/ wieder wegreisete.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Nur einen Tag die sch\u00f6nen Augen sehn", "tokens": ["Nur", "ei\u00b7nen", "Tag", "die", "sch\u00f6\u00b7nen", "Au\u00b7gen", "sehn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ist schon genug mein Hertze zu entz\u00fcnden/", "tokens": ["Ist", "schon", "ge\u00b7nug", "mein", "Hert\u00b7ze", "zu", "ent\u00b7z\u00fcn\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "PPOSAT", "VVFIN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch lange nicht/ da\u00df es auch mag geschehn", "tokens": ["Doch", "lan\u00b7ge", "nicht", "/", "da\u00df", "es", "auch", "mag", "ge\u00b7schehn"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "PTKNEG", "$(", "KOUS", "PPER", "ADV", "VMFIN", "VVINF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Verlangte H\u00fclff\u2019 in solcher Zeit zu finden.", "tokens": ["Ver\u00b7lang\u00b7te", "H\u00fclff'", "in", "sol\u00b7cher", "Zeit", "zu", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "PIAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Vergebne Quaal/ und \u00e4ngstigs Seuffzen macht", "tokens": ["Ver\u00b7geb\u00b7ne", "Qua\u00b7al", "/", "und", "\u00e4ngs\u00b7tigs", "Seuff\u00b7zen", "macht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$(", "KON", "ADJA", "NN", "VVFIN"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Die lange Nacht.", "tokens": ["Die", "lan\u00b7ge", "Nacht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Die lange Nacht zeugt endlich Tag und Licht", "tokens": ["Die", "lan\u00b7ge", "Nacht", "zeugt", "end\u00b7lich", "Tag", "und", "Licht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "NN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wenn Finsterni\u00df die Erde h\u00e4lt bedecket/", "tokens": ["Wenn", "Fins\u00b7ter\u00b7ni\u00df", "die", "Er\u00b7de", "h\u00e4lt", "be\u00b7de\u00b7cket", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ART", "NN", "VVFIN", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mir aber bleibt kein Hoffen \u00fcbrig nicht/", "tokens": ["Mir", "a\u00b7ber", "bleibt", "kein", "Hof\u00b7fen", "\u00fcb\u00b7rig", "nicht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PIAT", "NN", "ADJD", "PTKNEG", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Da\u00df bald ein Tag beliebten Schein erwecket.", "tokens": ["Da\u00df", "bald", "ein", "Tag", "be\u00b7lieb\u00b7ten", "Schein", "er\u00b7we\u00b7cket", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Mir wird vertauscht der angenehme Schein", "tokens": ["Mir", "wird", "ver\u00b7tauscht", "der", "an\u00b7ge\u00b7neh\u00b7me", "Schein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "VVPP", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "In Dunckel seyn.", "tokens": ["In", "Dun\u00b7ckel", "seyn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAINF", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "In Dunckel seyn manch treuer Sinn genie\u00dft", "tokens": ["In", "Dun\u00b7ckel", "seyn", "manch", "treu\u00b7er", "Sinn", "ge\u00b7nie\u00dft"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "PPOSAT", "PIAT", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die sch\u00f6nste Frucht der Zucker s\u00fcssen Liebe/", "tokens": ["Die", "sch\u00f6ns\u00b7te", "Frucht", "der", "Zu\u00b7cker", "s\u00fcs\u00b7sen", "Lie\u00b7be", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mein Hertz allein vergalte Wermuth k\u00fc\u00dft", "tokens": ["Mein", "Hertz", "al\u00b7lein", "ver\u00b7gal\u00b7te", "Wer\u00b7muth", "k\u00fc\u00dft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADV", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Vor jene Kost der angenehmen Triebe.", "tokens": ["Vor", "je\u00b7ne", "Kost", "der", "an\u00b7ge\u00b7neh\u00b7men", "Trie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Was sch\u00f6nes sehn ohn Lindrung seiner Noht", "tokens": ["Was", "sch\u00f6\u00b7nes", "sehn", "ohn", "Lind\u00b7rung", "sei\u00b7ner", "Noht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "ADJA", "VVINF", "APPR", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Ist schon der Tod.", "tokens": ["Ist", "schon", "der", "Tod", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Ist schon der Tod bey manchen sehr verha\u00dft/", "tokens": ["Ist", "schon", "der", "Tod", "bey", "man\u00b7chen", "sehr", "ver\u00b7ha\u00dft", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "APPR", "PIAT", "ADV", "ADJD", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die niemahls nicht was str\u00e4ffliches ver\u00fcbet?", "tokens": ["Die", "nie\u00b7mahls", "nicht", "was", "str\u00e4ff\u00b7li\u00b7ches", "ver\u00b7\u00fc\u00b7bet", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PTKNEG", "PWS", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mein Hertz dennoch mit allen Recht erbla\u00dft/", "tokens": ["Mein", "Hertz", "den\u00b7noch", "mit", "al\u00b7len", "Recht", "er\u00b7bla\u00dft", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "APPR", "PIAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Weil es zu dreist ein Engels-Bild geliebet.", "tokens": ["Weil", "es", "zu", "dreist", "ein", "En\u00b7gels\u00b7Bild", "ge\u00b7lie\u00b7bet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKZU", "VVFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Allein der Spruch und seine Todes-Art", "tokens": ["Al\u00b7lein", "der", "Spruch", "und", "sei\u00b7ne", "To\u00b7des\u00b7Art"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Scheint gar zu hart.", "tokens": ["Scheint", "gar", "zu", "hart", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PTKA", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Scheint gar zu hart mein Hertze dir zu seyn/", "tokens": ["Scheint", "gar", "zu", "hart", "mein", "Hert\u00b7ze", "dir", "zu", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PTKA", "ADJD", "PPOSAT", "VVFIN", "PPER", "PTKZU", "VAINF", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und f\u00fcrchtest du ich m\u00f6chte seyn von Eysen?", "tokens": ["Und", "f\u00fcrch\u00b7test", "du", "ich", "m\u00f6ch\u00b7te", "seyn", "von", "Ey\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPER", "VMFIN", "VAINF", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So stelle nur die harte Pr\u00f6be ein/", "tokens": ["So", "stel\u00b7le", "nur", "die", "har\u00b7te", "Pr\u00f6\u00b7be", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "ADJA", "NN", "ART", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ich will mich gern wie Wachs bey dir erweisen.", "tokens": ["Ich", "will", "mich", "gern", "wie", "Wachs", "bey", "dir", "er\u00b7wei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "KOKOM", "NN", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Kehr nur zur\u00fcck/ erf\u00fclle mich mit Lust", "tokens": ["Kehr", "nur", "zu\u00b7r\u00fcck", "/", "er\u00b7f\u00fcl\u00b7le", "mich", "mit", "Lust"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "ADV", "PTKVZ", "$(", "VVFIN", "PRF", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Abgott der Brust.", "tokens": ["Ab\u00b7gott", "der", "Brust", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.6": {"line.1": {"text": "Abgott der Brust/ die reine Brunst beseelt/", "tokens": ["Ab\u00b7gott", "der", "Brust", "/", "die", "rei\u00b7ne", "Brunst", "be\u00b7seelt", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$(", "ART", "ADJA", "NN", "VVPP", "$("], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "So mir dein Strahl darinnen angeflammet.", "tokens": ["So", "mir", "dein", "Strahl", "da\u00b7rin\u00b7nen", "an\u00b7ge\u00b7flam\u00b7met", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "PPOSAT", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dein heller Blitz nicht viel von jenem scheelt/", "tokens": ["Dein", "hel\u00b7ler", "Blitz", "nicht", "viel", "von", "je\u00b7nem", "scheelt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PTKNEG", "ADV", "APPR", "PDAT", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der ", "tokens": ["Der"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.5": {"text": "Jetzt mu\u00df ich auch/ wie ihr vor dem geschehn/", "tokens": ["Jetzt", "mu\u00df", "ich", "auch", "/", "wie", "ihr", "vor", "dem", "ge\u00b7schehn", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "$(", "PWAV", "PPER", "APPR", "ART", "VVINF", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Durchs Feur vergehn.", "tokens": ["Durchs", "Feur", "ver\u00b7gehn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVINF", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.7": {"line.1": {"text": "Durchs Feur vergehn im peinlichen Gericht", "tokens": ["Durchs", "Feur", "ver\u00b7gehn", "im", "pein\u00b7li\u00b7chen", "Ge\u00b7richt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVINF", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Nur aber die/ so Zauberey getrieben/", "tokens": ["Nur", "a\u00b7ber", "die", "/", "so", "Zau\u00b7be\u00b7rey", "ge\u00b7trie\u00b7ben", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "$(", "ADV", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nicht die wie ich ein sch\u00f6nes Angesicht", "tokens": ["Nicht", "die", "wie", "ich", "ein", "sch\u00f6\u00b7nes", "An\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ART", "PWAV", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ersehen und zugleich/ dasselbe lieben.", "tokens": ["Er\u00b7se\u00b7hen", "und", "zu\u00b7gleich", "/", "das\u00b7sel\u00b7be", "lie\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "KON", "ADV", "$(", "PDAT", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ach kehre doch das Urtheil meiner Pein", "tokens": ["Ach", "keh\u00b7re", "doch", "das", "Ur\u00b7theil", "mei\u00b7ner", "Pein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "VVFIN", "ADV", "ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "In Gn\u00e4dig-seyn.", "tokens": ["In", "Gn\u00e4\u00b7dig\u00b7seyn", "."], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NE", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.8": {"line.1": {"text": "In Gn\u00e4dig-seyn wird aber der auch offt/", "tokens": ["In", "Gn\u00e4\u00b7dig\u00b7seyn", "wird", "a\u00b7ber", "der", "auch", "offt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VAFIN", "ADV", "ART", "ADV", "ADV", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der schon begnadt/ durch solche Gunst verlohren/", "tokens": ["Der", "schon", "be\u00b7gnadt", "/", "durch", "sol\u00b7che", "Gunst", "ver\u00b7loh\u00b7ren", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVPP", "$(", "APPR", "PIAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Weil ihm das Wort der Gnade unverhofft", "tokens": ["Weil", "ihm", "das", "Wort", "der", "Gna\u00b7de", "un\u00b7ver\u00b7hofft"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "ART", "NN", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ein Donnerschlag in Hertzen und in Ohren/", "tokens": ["Ein", "Don\u00b7ner\u00b7schlag", "in", "Hert\u00b7zen", "und", "in", "Oh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "KON", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Drum will ich nicht ver\u00e4ndern meinen Tod", "tokens": ["Drum", "will", "ich", "nicht", "ver\u00b7\u00e4n\u00b7dern", "mei\u00b7nen", "Tod"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VMFIN", "PPER", "PTKNEG", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und dein Gebot.", "tokens": ["Und", "dein", "Ge\u00b7bot", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.9": {"line.1": {"text": "Und dein Gebot der Augen mir befiehlt", "tokens": ["Und", "dein", "Ge\u00b7bot", "der", "Au\u00b7gen", "mir", "be\u00b7fiehlt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "ART", "NN", "PPER", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die sch\u00f6nste Zier der Wangen zu verehren/", "tokens": ["Die", "sch\u00f6ns\u00b7te", "Zier", "der", "Wan\u00b7gen", "zu", "ver\u00b7eh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "PTKZU", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Es heischt die Gluth/ so um die Br\u00fcste spielt", "tokens": ["Es", "heischt", "die", "Gluth", "/", "so", "um", "die", "Br\u00fcs\u00b7te", "spielt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "$(", "ADV", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Da\u