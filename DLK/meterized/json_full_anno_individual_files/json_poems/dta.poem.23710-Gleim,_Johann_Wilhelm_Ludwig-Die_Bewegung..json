{"dta.poem.23710": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Die Bewegung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-200905197682", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Verfolgt mich, ihr Sch\u00f6nen,", "tokens": ["Ver\u00b7folgt", "mich", ",", "ihr", "Sch\u00f6\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PPOSAT", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Verfolgt mich ins Gr\u00fcne.", "tokens": ["Ver\u00b7folgt", "mich", "ins", "Gr\u00fc\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Es soll uns, in B\u00fcschen,", "tokens": ["Es", "soll", "uns", ",", "in", "B\u00fc\u00b7schen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "$,", "APPR", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Nur Zefir verfolgen.", "tokens": ["Nur", "Ze\u00b7fir", "ver\u00b7fol\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Wir wollen uns iagen,", "tokens": ["Wir", "wol\u00b7len", "uns", "i\u00b7a\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und, wenn wir, in Th\u00e4lern,", "tokens": ["Und", ",", "wenn", "wir", ",", "in", "Th\u00e4\u00b7lern", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PPER", "$,", "APPR", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Auf Klippen und H\u00fcgeln,", "tokens": ["Auf", "Klip\u00b7pen", "und", "H\u00fc\u00b7geln", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Uns m\u00fcde geiaget;", "tokens": ["Uns", "m\u00fc\u00b7de", "gei\u00b7a\u00b7get", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVPP", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "So wollen wir schlummern.", "tokens": ["So", "wol\u00b7len", "wir", "schlum\u00b7mern", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}}}}