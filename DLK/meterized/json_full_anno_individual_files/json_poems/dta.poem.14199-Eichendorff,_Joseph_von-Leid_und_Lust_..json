{"dta.poem.14199": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Leid und Lust .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Euch Wolken beneid' ich", "tokens": ["Euch", "Wol\u00b7ken", "be\u00b7neid'", "ich"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "NN", "VVFIN", "PPER"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "In blauer Luft,", "tokens": ["In", "blau\u00b7er", "Luft", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Wie schwingt' Ihr Euch freudig", "tokens": ["Wie", "schwingt'", "Ihr", "Euch", "freu\u00b7dig"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "PPER", "ADJD"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Ueber Berg und Kluft!", "tokens": ["Ue\u00b7ber", "Berg", "und", "Kluft", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}