"meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "La\u00df uns einf\u00e4ltig werden,", "tokens": ["La\u00df", "uns", "ein\u00b7f\u00e4l\u00b7tig", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und vor dir hier auf Erden", "tokens": ["Und", "vor", "dir", "hier", "auf", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPER", "ADV", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wie Kinder fromm und fr\u00f6lich seyn.", "tokens": ["Wie", "Kin\u00b7der", "fromm", "und", "fr\u00f6\u00b7lich", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ADJD", "KON", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.13": {"line.1": {"text": "\u2013 paullo maiora canamus,", "tokens": ["\u2013", "paul\u00b7lo", "mai\u00b7o\u00b7ra", "ca\u00b7na\u00b7mus", ","], "token_info": ["punct", "word", "word", "word", "punct"], "pos": ["$(", "FM.la", "FM.la", "FM.la", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "non omnes arbusta iuvant, humilesque myricae.", "tokens": ["non", "om\u00b7nes", "ar\u00b7bus\u00b7ta", "i\u00b7u\u00b7vant", ",", "hu\u00b7mi\u00b7les\u00b7que", "my\u00b7ri\u00b7cae", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "$,", "FM.la", "FM.la", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}}, "stanza.14": {"line.1": {"text": "Und also auf diesen Zuruf lebt wohl, meine schlechte, und jedermanns bessere Volkslieder!", "tokens": ["Und", "al\u00b7so", "auf", "die\u00b7sen", "Zu\u00b7ruf", "lebt", "wohl", ",", "mei\u00b7ne", "schlech\u00b7te", ",", "und", "je\u00b7der\u00b7manns", "bes\u00b7se\u00b7re", "Volks\u00b7lie\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PDAT", "NN", "VVFIN", "ADV", "$,", "PPOSAT", "VVFIN", "$,", "KON", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+--+-+-+-+--+--+--+--", "measure": "iambic.octa.plus.relaxed"}}}}}