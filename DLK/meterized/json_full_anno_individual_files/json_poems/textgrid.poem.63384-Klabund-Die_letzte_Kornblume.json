{"textgrid.poem.63384": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "Die letzte Kornblume", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sie ging, den Weg zu k\u00fcrzen, \u00fcbers Feld.", "tokens": ["Sie", "ging", ",", "den", "Weg", "zu", "k\u00fcr\u00b7zen", ",", "\u00fc\u00b7bers", "Feld", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ART", "NN", "PTKZU", "VVINF", "$,", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Es war gem\u00e4ht. Die \u00c4hren eingefahren.", "tokens": ["Es", "war", "ge\u00b7m\u00e4ht", ".", "Die", "\u00c4h\u00b7ren", "ein\u00b7ge\u00b7fah\u00b7ren", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$.", "ART", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die braunen Stoppeln stachen in die Luft,", "tokens": ["Die", "brau\u00b7nen", "Stop\u00b7peln", "sta\u00b7chen", "in", "die", "Luft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Als h\u00e4tte sich der Erdgott schlecht rasiert.", "tokens": ["Als", "h\u00e4t\u00b7te", "sich", "der", "Erd\u00b7gott", "schlecht", "ra\u00b7siert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PRF", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Sie ging und ging. Und pl\u00f6tzlich traf sie", "tokens": ["Sie", "ging", "und", "ging", ".", "Und", "pl\u00f6tz\u00b7lich", "traf", "sie"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "$.", "KON", "ADJD", "VVFIN", "PPER"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Auf die letzte blaue Blume dieses Sommers.", "tokens": ["Auf", "die", "letz\u00b7te", "blau\u00b7e", "Blu\u00b7me", "die\u00b7ses", "Som\u00b7mers", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN", "PDAT", "NN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.7": {"text": "Sie sah die Blume an. Die Blume sie. Und beide dachten", "tokens": ["Sie", "sah", "die", "Blu\u00b7me", "an", ".", "Die", "Blu\u00b7me", "sie", ".", "Und", "bei\u00b7de", "dach\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKVZ", "$.", "ART", "NN", "PPER", "$.", "KON", "PIS", "VVFIN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.8": {"text": "(sofern die Menschen denken k\u00f6nnen, dachte die Blume...)", "tokens": ["(", "so\u00b7fern", "die", "Men\u00b7schen", "den\u00b7ken", "k\u00f6n\u00b7nen", ",", "dach\u00b7te", "die", "Blu\u00b7me", "...", ")"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "ART", "NN", "VVINF", "VMINF", "$,", "VVFIN", "ART", "NN", "$(", "$("], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "Dachten ganz das gleiche:", "tokens": ["Dach\u00b7ten", "ganz", "das", "glei\u00b7che", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.10": {"text": "Du bist die letzte Bl\u00fcte dieses Sommers,", "tokens": ["Du", "bist", "die", "letz\u00b7te", "Bl\u00fc\u00b7te",