{"textgrid.poem.33790": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "Der Kinder Kreuzfahrt", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sie gingen, Gottes Reich zu erben,", "tokens": ["Sie", "gin\u00b7gen", ",", "Got\u00b7tes", "Reich", "zu", "er\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "NN", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die zarten Herzen ri\u00df der Wahn", "tokens": ["Die", "zar\u00b7ten", "Her\u00b7zen", "ri\u00df", "der", "Wahn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In Elend hin und in Verderben,", "tokens": ["In", "E\u00b7lend", "hin", "und", "in", "Ver\u00b7der\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKVZ", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Wallfahrt ging mit Kreuz und Fahn'.", "tokens": ["Die", "Wall\u00b7fahrt", "ging", "mit", "Kreuz", "und", "Fahn'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sie hofften, wie der Ernte Halmen", "tokens": ["Sie", "hoff\u00b7ten", ",", "wie", "der", "Ern\u00b7te", "Hal\u00b7men"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der Heiden K\u00f6pfe abzum\u00e4hn,", "tokens": ["Der", "Hei\u00b7den", "K\u00f6p\u00b7fe", "ab\u00b7zu\u00b7m\u00e4hn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und glaubten schon mit Siegespalmen", "tokens": ["Und", "glaub\u00b7ten", "schon", "mit", "Sie\u00b7ge\u00b7spal\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Den Himmel aufgetan zu sehn.", "tokens": ["Den", "Him\u00b7mel", "auf\u00b7ge\u00b7tan", "zu", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Wer aber bald im Sturm und Regen,", "tokens": ["Wer", "a\u00b7ber", "bald", "im", "Sturm", "und", "Re\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADV", "APPRART", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Wer mochte der Erkrankten pflegen?", "tokens": ["Wer", "moch\u00b7te", "der", "Er\u00b7krank\u00b7ten", "pfle\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ach, statt des Himmels Armen streckte", "tokens": ["Ach", ",", "statt", "des", "Him\u00b7mels", "Ar\u00b7men", "streck\u00b7te"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "KOUI", "ART", "NN", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Heide sich, die \u00d6de aus,", "tokens": ["Die", "Hei\u00b7de", "sich", ",", "die", "\u00d6\u00b7de", "aus", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "$,", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Hunger kam, und Gram bedeckte", "tokens": ["Der", "Hun\u00b7ger", "kam", ",", "und", "Gram", "be\u00b7deck\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "KON", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den Blick, voll Sehnsuchtweh nach Haus.", "tokens": ["Den", "Blick", ",", "voll", "Sehn\u00b7sucht\u00b7weh", "nach", "Haus", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Nicht der, den einst Tobias gr\u00fc\u00dfte,", "tokens": ["Nicht", "der", ",", "den", "einst", "To\u00b7bias", "gr\u00fc\u00df\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "$,", "PRELS", "ADV", "NE", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Nahm ihrer letzten Stunde wahr;", "tokens": ["Nahm", "ih\u00b7rer", "letz\u00b7ten", "Stun\u00b7de", "wahr", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der Engel Hagar's in der W\u00fcste", "tokens": ["Der", "En\u00b7gel", "Ha\u00b7ga\u00b7r's", "in", "der", "W\u00fcs\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NE", "APPR", "ART", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Erschien vor der bedr\u00e4ngten Schar.", "tokens": ["Er\u00b7schien", "vor", "der", "be\u00b7dr\u00e4ng\u00b7ten", "Schar", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Er sah sie 's Kreuz gen Himmel halten,", "tokens": ["Er", "sah", "sie", "'s", "Kreuz", "gen", "Him\u00b7mel", "hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PPER", "NN", "APPR", "NN", "VVINF", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Und ihre schwachen Stimmlein schallten.", "tokens": ["Und", "ih\u00b7re", "schwa\u00b7chen", "Stimm\u00b7lein", "schall\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "\u00bbdie Drossel singt am fr\u00fchen Morgen,", "tokens": ["\u00bb", "die", "Dros\u00b7sel", "singt", "am", "fr\u00fc\u00b7hen", "Mor\u00b7gen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Nachtigall in sp\u00e4ter Nacht,", "tokens": ["Die", "Nach\u00b7ti\u00b7gall", "in", "sp\u00e4\u00b7ter", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "An unserm leeren Bett voll Sorgen", "tokens": ["An", "un\u00b7serm", "lee\u00b7ren", "Bett", "voll", "Sor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "ADJD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Mutter unter Tr\u00e4nen wacht;", "tokens": ["Die", "Mut\u00b7ter", "un\u00b7ter", "Tr\u00e4\u00b7nen", "wacht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "O da\u00df sie Gott verwandeln wollte", "tokens": ["O", "da\u00df", "sie", "Gott", "ver\u00b7wan\u00b7deln", "woll\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "KOUS", "PPER", "NN", "VVINF", "VMFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die Tr\u00e4nen all' in einen Tau,", "tokens": ["Die", "Tr\u00e4\u00b7nen", "all'", "in", "ei\u00b7nen", "Tau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIS", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der unsre Lippen netzen sollte,", "tokens": ["Der", "uns\u00b7re", "Lip\u00b7pen", "net\u00b7zen", "soll\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Von Durst und Glut verdorrt und rauh!", "tokens": ["Von", "Durst", "und", "Glut", "ver\u00b7dorrt", "und", "rauh", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVPP", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "O qu\u00f6ll' uns euer Gru\u00df und Segen", "tokens": ["O", "qu\u00f6ll'", "uns", "eu\u00b7er", "Gru\u00df", "und", "Se\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "PPOSAT", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Vom Himmel als ein milder Regen!", "tokens": ["Vom", "Him\u00b7mel", "als", "ein", "mil\u00b7der", "Re\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KOKOM", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Die Erde brennt, wohin wir schauen,", "tokens": ["Die", "Er\u00b7de", "brennt", ",", "wo\u00b7hin", "wir", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PWAV", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Sonne gl\u00fcht im Untergehn.", "tokens": ["Die", "Son\u00b7ne", "gl\u00fcht", "im", "Un\u00b7ter\u00b7gehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ihr Eltern, bald in Edens Auen", "tokens": ["Ihr", "El\u00b7tern", ",", "bald", "in", "E\u00b7dens", "Au\u00b7en"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "ADV", "APPR", "NE", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sollt ihr uns alle wiedersehn!", "tokens": ["Sollt", "ihr", "uns", "al\u00b7le", "wie\u00b7der\u00b7sehn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "PIS", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Lebt wohl, ihr Eltern, wir verschmachten,", "tokens": ["Lebt", "wohl", ",", "ihr", "El\u00b7tern", ",", "wir", "ver\u00b7schmach\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Doch wird befrei'n des Retters Schwert", "tokens": ["Doch", "wird", "be\u00b7frei'n", "des", "Ret\u00b7ters", "Schwert"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "APPR", 