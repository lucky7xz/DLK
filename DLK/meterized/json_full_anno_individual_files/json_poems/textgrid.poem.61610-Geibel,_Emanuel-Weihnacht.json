{"textgrid.poem.61610": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Weihnacht", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie bewegt mich wundersam", "tokens": ["Wie", "be\u00b7wegt", "mich", "wun\u00b7der\u00b7sam"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Euer Hall, ihr Weihnachtsglocken,", "tokens": ["Eu\u00b7er", "Hall", ",", "ihr", "Weih\u00b7nachts\u00b7glo\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Die ihr k\u00fcndet mit Frohlocken,", "tokens": ["Die", "ihr", "k\u00fcn\u00b7det", "mit", "Froh\u00b7lo\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df zur Welt die Gnade kam.", "tokens": ["Da\u00df", "zur", "Welt", "die", "Gna\u00b7de", "kam", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPRART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "\u00dcberm Hause schien der Stern,", "tokens": ["\u00dc\u00b7berm", "Hau\u00b7se", "schien", "der", "Stern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und in Lilien stand die Krippe,", "tokens": ["Und", "in", "Li\u00b7li\u00b7en", "stand", "die", "Krip\u00b7pe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NE", "VVFIN", "ART", "NN", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Wo der Engel reine Lippe", "tokens": ["Wo", "der", "En\u00b7gel", "rei\u00b7ne", "Lip\u00b7pe"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hosianna sang dem Herrn.", "tokens": ["Ho\u00b7si\u00b7an\u00b7na", "sang", "dem", "Herrn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Herz, und was geschah vordem,", "tokens": ["Herz", ",", "und", "was", "ge\u00b7schah", "vor\u00b7dem", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "PWS", "VVFIN", "ADV", "$,"], "meter": "+-+-+--", "measure": "unknown.measure.tri"}, "line.2": {"text": "Dir zum Heil erneut sich's heute:", "tokens": ["Dir", "zum", "Heil", "er\u00b7neut", "sich's", "heu\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "VVFIN", "PIS", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Dies ged\u00e4mpfte Festgel\u00e4ute", "tokens": ["Dies", "ge\u00b7d\u00e4mpf\u00b7te", "Fest\u00b7ge\u00b7l\u00e4u\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["PDS", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ruft auch dich nach Bethlehem.", "tokens": ["Ruft", "auch", "dich", "nach", "Beth\u00b7le\u00b7hem", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPER", "APPR", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Mit den Hirten darfst du ziehn,", "tokens": ["Mit", "den", "Hir\u00b7ten", "darfst", "du", "