{"dta.poem.13540": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Ankl\u00e4nge .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Und der Streit mu\u00df sich vers\u00f6hnen,", "tokens": ["Und", "der", "Streit", "mu\u00df", "sich", "ver\u00b7s\u00f6h\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VMFIN", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und die Wonne und den Schmerz", "tokens": ["Und", "die", "Won\u00b7ne", "und", "den", "Schmerz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "KON", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mu\u00df er ewig himmelw\u00e4rts", "tokens": ["Mu\u00df", "er", "e\u00b7wig", "him\u00b7mel\u00b7w\u00e4rts"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ADJD", "ADV"], "meter": "+-+-+--", "measure": "unknown.measure.tri"}, "line.4": {"text": "Schlagen nun in vollen T\u00f6nen:", "tokens": ["Schla\u00b7gen", "nun", "in", "vol\u00b7len", "T\u00f6\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ewig's Tr\u00e4umen von den Fernen!", "tokens": ["E\u00b7wig's", "Tr\u00e4u\u00b7men", "von", "den", "Fer\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "ADJA", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Endlich ist das Herz erwacht", "tokens": ["End\u00b7lich", "ist", "das", "Herz", "er\u00b7wacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Unter Blumen, Klang und Sternen", "tokens": ["Un\u00b7ter", "Blu\u00b7men", ",", "Klang", "und", "Ster\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "In der dunkelgr\u00fcnen Nacht.", "tokens": ["In", "der", "dun\u00b7kel\u00b7gr\u00fc\u00b7nen", "Nacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}