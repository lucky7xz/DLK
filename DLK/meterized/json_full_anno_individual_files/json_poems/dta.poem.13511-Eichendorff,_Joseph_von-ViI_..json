{"dta.poem.13511": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "ViI .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Der Wandrer, von der Heimath weit,", "tokens": ["Der", "Wand\u00b7rer", ",", "von", "der", "Hei\u00b7math", "weit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn rings die Gr\u00fcnde schweigen,", "tokens": ["Wenn", "rings", "die", "Gr\u00fcn\u00b7de", "schwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der Schiffer in Meeres Einsamkeit,", "tokens": ["Der", "Schif\u00b7fer", "in", "Mee\u00b7res", "Ein\u00b7sam\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wenn die Stern' aus den Fluten steigen:", "tokens": ["Wenn", "die", "Stern'", "aus", "den", "Flu\u00b7ten", "stei\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}}, "stanza.2": {"line.1": {"text": "Die beide schauern und lesen", "tokens": ["Die", "bei\u00b7de", "schau\u00b7ern", "und", "le\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PIS", "VVINF", "KON", "VVINF"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "In stiller Nacht", "tokens": 