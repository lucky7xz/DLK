{"textgrid.poem.37390": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Teufelswurst", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das Pf\u00e4fflein sa\u00df beim Fr\u00fchst\u00fcckschmaus.", "tokens": ["Das", "Pf\u00e4f\u00b7flein", "sa\u00df", "beim", "Fr\u00fch\u00b7st\u00fcck\u00b7schmaus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er schaut und zieht die Stirne kraus.", "tokens": ["Er", "schaut", "und", "zieht", "die", "Stir\u00b7ne", "kraus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wer, fragt er, hat die Wurst gebracht?", "tokens": ["Wer", ",", "fragt", "er", ",", "hat", "die", "Wurst", "ge\u00b7bracht", "?"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "$,", "VVFIN", "PPER", "$,", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die K\u00f6chin sprach: Es war die Liese,", "tokens": ["Die", "K\u00f6\u00b7chin", "sprach", ":", "Es", "war", "die", "Lie\u00b7se", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "PPER", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die Alte von der G\u00e4nsewiese.", "tokens": ["Die", "Al\u00b7te", "von", "der", "G\u00e4n\u00b7se\u00b7wie\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Drum, rief er, sah ich in letzter Nacht,", "tokens": ["Drum", ",", "rief", "er", ",", "sah", "ich", "in", "letz\u00b7ter", "Nacht", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "VVFIN", "PPER", "$,", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Wie durch die Luft in feurigem Bogen", "tokens": ["Wie", "durch", "die", "Luft", "in", "feu\u00b7ri\u00b7gem", "Bo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Der B\u00f6se in ihren Schlot geflogen.", "tokens": ["Der", "B\u00f6\u00b7se", "in", "ih\u00b7ren", "Schlot", "ge\u00b7flo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Verdammte Hex,", "tokens": ["Ver\u00b7damm\u00b7te", "Hex", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "Ich riech, ich schmeck's,", "tokens": ["Ich", "riech", ",", "ich", "schmeck's", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.11": {"text": "Der Teufel hat die Wurst gemacht.", "tokens": ["Der", "Teu\u00b7fel", "hat", "die", "Wurst", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Spitz, da geh her! \u2013 Der Hund, nicht faul,", "tokens": ["Spitz", ",", "da", "geh", "her", "!", "\u2013", "Der", "Hund", ",", "nicht", "faul", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "VVFIN", "PTKVZ", "$.", "$(", "ART", "NN", "$,", "PTKNEG", "ADJD", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.13": {"text": "Verzehrt die Wurst und leckt das Maul.", "tokens": ["Ver\u00b7zehrt", "die", "Wurst", "und", "leckt", "das", "Maul", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Er nimmt das Gute, ohne zu fragen,", "tokens": ["Er", "nimmt", "das", "Gu\u00b7te", ",", "oh\u00b7ne", "zu", "fra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "KOUI", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.15": {"text": "Ob's Beelzebub unter dem Schwanz getragen.", "tokens": ["Ob's", "Beel\u00b7ze\u00b7bub", "un\u00b7ter", "dem", "Schwanz", "ge\u00b7tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}}, "stanza.2": {"line.1": {"text": "Das Pf\u00e4fflein sa\u00df beim Fr\u00fchst\u00fcckschmaus.", "tokens": ["Das", "Pf\u00e4f\u00b7flein", "sa\u00df", "beim", "Fr\u00fch\u00b7st\u00fcck\u00b7schmaus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er schaut und zieht die Stirne kraus.", "tokens": ["Er", "schaut", "und", "zieht", "die", "Stir\u00b7ne", "kraus", "."], "token_info