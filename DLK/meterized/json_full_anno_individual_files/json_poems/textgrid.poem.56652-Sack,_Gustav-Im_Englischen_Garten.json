{"textgrid.poem.56652": {"metadata": {"author": {"name": "Sack, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Im Englischen Garten", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als ich aus meiner Stammtaberne", "tokens": ["Als", "ich", "aus", "mei\u00b7ner", "Stamm\u00b7ta\u00b7ber\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "mich gestern fortgemacht,", "tokens": ["mich", "ge\u00b7stern", "fort\u00b7ge\u00b7macht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVPP", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "hing in die sp\u00f6ttisch stille Gartennacht", "tokens": ["hing", "in", "die", "sp\u00f6t\u00b7tisch", "stil\u00b7le", "Gar\u00b7ten\u00b7nacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "der Mond herab gleich einer leuchtenden Papierlaterne.", "tokens": ["der", "Mond", "her\u00b7ab", "gleich", "ei\u00b7ner", "leuch\u00b7ten\u00b7den", "Pa\u00b7pier\u00b7la\u00b7ter\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}}, "stanza.2": {"line.1": {"text": "Mit einem Sichelschwert, krumm wie die H\u00fclse der Luzerne,", "tokens": ["Mit", "ei\u00b7nem", "Si\u00b7chel\u00b7schwert", ",", "krumm", "wie", "die", "H\u00fcl\u00b7se", "der", "Lu\u00b7zer\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NE", "$,", "ADJD", "KOKOM", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "hat ungeh\u00f6rt die Nacht", "tokens": ["hat", "un\u00b7ge\u00b7h\u00f6rt", "die", "Nacht"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "ADJD", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "unter dem Rasen einen Schnitt gemacht", "tokens": ["un\u00b7ter", "dem", "Ra\u00b7sen", "ei\u00b7nen", "Schnitt", "ge\u00b7macht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVPP"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.4": {"text": "und l\u00e4\u00dft die Erde st\u00fcrzen in die sammetschwarze Ferne;", "tokens": ["und", "l\u00e4\u00dft", "die", "Er\u00b7de", "st\u00fcr\u00b7zen", "in", "die", "sam\u00b7met\u00b7schwar\u00b7ze", "Fer\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}}, "stanza.3": {"line.1": {"text": "und singend h\u00e4lt sie in den weichen H\u00e4nden", "tokens": ["und", "sin\u00b7gend", "h\u00e4lt", "sie", "in", "den", "wei\u00b7chen", "H\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "dies Rund von wulstigen Schattenw\u00e4nden,", "tokens": ["dies", "Rund", "von", "wuls\u00b7ti\u00b7gen", "Schat\u00b7ten\u00b7w\u00e4n\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "in dem ich wie von einer t\u00f6nereichen Schale", "tokens": ["in", "dem", "ich", "wie", "von", "ei\u00b7ner", "t\u00f6\u00b7ne\u00b7rei\u00b7chen", "Scha\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "PPER", "KOKOM", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "getragen viele tausend tausend Male", "tokens": ["ge\u00b7tra\u00b7gen", "vie\u00b7le", "tau\u00b7send", "tau\u00b7send", "Ma\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVPP", "PIAT", "CARD", "CARD", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "an Leonor gedacht,", "tokens": ["an", "Le\u00b7o\u00b7nor", "ge\u00b7dacht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "in dieser braunen sp\u00f6ttisch stillen Gartennacht.", "tokens": ["in", "die\u00b7ser", "brau\u00b7nen", "sp\u00f6t\u00b7tisch", "stil\u00b7len", "Gar\u00b7ten\u00b7nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Als ich aus meiner Stammtaberne", "tokens": ["Als", "ich", "aus", "mei\u00b7ner", "Stamm\u00b7ta\u00b7ber\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "mich gestern fortgemacht,", "tokens": ["mich", "ge\u00b7stern", "fort\u00b7ge\u00b7macht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVPP", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "hing in die sp\u00f6ttisch stille Gartennacht", "tokens": ["hing", "in", "die", "sp\u00f6t\u00b7tisch", "stil\u00b7le", "Gar\u00b7ten\u00b7nacht"], "token_info": ["word", "wor