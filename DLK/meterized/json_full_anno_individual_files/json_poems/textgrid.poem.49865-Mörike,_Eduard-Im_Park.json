{"textgrid.poem.49865": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Im Park", "genre": "verse", "period": "N.A.", "pub_year": 1847, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sieh, der Kastanie kindliches Laub h\u00e4ngt noch wie der feuchte", "tokens": ["Sieh", ",", "der", "Kas\u00b7ta\u00b7nie", "kind\u00b7li\u00b7ches", "Laub", "h\u00e4ngt", "noch", "wie", "der", "feuch\u00b7te"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "ART", "NN", "ADJA", "NN", "VVFIN", "ADV", "KOKOM", "ART", "ADJA"], "meter": "+-+-++--+--+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Fl\u00fcgel des Papillons, wenn er die H\u00fclle verlie\u00df;", "tokens": ["Fl\u00fc\u00b7gel", "des", "Pa\u00b7pil\u00b7lons", ",", "wenn", "er", "die", "H\u00fcl\u00b7le", "ver\u00b7lie\u00df", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Aber in laulicher Nacht der k\u00fcrzeste Regen entfaltet", "tokens": ["A\u00b7ber", "in", "lau\u00b7li\u00b7cher", "Nacht", "der", "k\u00fcr\u00b7zes\u00b7te", "Re\u00b7gen", "ent\u00b7fal\u00b7tet"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN"], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Leise die F\u00e4cher und deckt schnelle den luftigen Gang.", "tokens": ["Lei\u00b7se", "die", "F\u00e4\u00b7cher", "und", "deckt", "schnel\u00b7le", "den", "luf\u00b7ti\u00b7gen", "Gang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "KON", "VVFIN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.5": {"text": "\u2013 Du magst eilen, o himmlischer Fr\u00fchling, oder verweilen,", "tokens": ["\u2013", "Du", "magst", "ei\u00b7len", ",", "o", "himm\u00b7li\u00b7scher", "Fr\u00fch\u00b7ling", ",", "o\u00b7der", "ver\u00b7wei\u00b7len", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "PPER", "VMFIN", "VVFIN", "$,", "FM", "ADJA", "NN", "$,", "KON", "VVINF", "$,"], "meter": "-+---+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Immer dem trunkenen Sinn fliehst du, ein Wunder, vorbei.", "tokens": ["Im\u00b7mer", "dem", "trun\u00b7ke\u00b7nen", "Sinn", "fliehst", "du", ",", "ein", "Wun\u00b7der", ",", "vor\u00b7bei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VVFIN", "PPER", "$,", "ART", "NN", "$,", "PTKVZ", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Sieh, der Kastanie kindliches Laub h\u00e4ngt noch wie der feuchte", "tokens": ["Sieh", ",", "der", "Kas\u00b7ta\u00b7nie", "kind\u00b7li\u00b7ches", "Laub", "h\u00e4ngt", "noch", "wie", "der", "feuch\u00b7te"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "ART", "NN", "ADJA", "NN", "VVFIN", "ADV", "KOKOM", "ART", "ADJA"], "meter": "+-+-++--+--+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Fl\u00fcgel des Papillons, wenn er die H\u00fclle verlie\u00df;", "tokens": ["Fl\u00fc\u00b7gel", "des", "Pa\u00b7pil\u00b7lons", ",", "wenn", "er", "die", "H\u00fcl\u00b7le", "ver\u00b7lie\u00df", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "KOUS", "PPER