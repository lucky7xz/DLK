{"dta.poem.19939": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "Vorw\u00e4rts!", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905193111", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Neues Wirken, neues Streben", "tokens": ["Neu\u00b7es", "Wir\u00b7ken", ",", "neu\u00b7es", "Stre\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist in Menschenbrust erwacht,", "tokens": ["Ist", "in", "Men\u00b7schen\u00b7brust", "er\u00b7wacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ein neues frisches Leben", "tokens": ["Und", "ein", "neu\u00b7es", "fri\u00b7sches", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hebt sich aus der alten Nacht.", "tokens": ["Hebt", "sich", "aus", "der", "al\u00b7ten", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Vorw\u00e4rts! vorw\u00e4rts! hat gehei\u00dfen", "tokens": ["Vor\u00b7w\u00e4rts", "!", "vor\u00b7w\u00e4rts", "!", "hat", "ge\u00b7hei\u00b7\u00dfen"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["ADV", "$.", "PTKVZ", "$.", "VAFIN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bl\u00fcchers m\u00e4cht'ger Schlachtgesang.", "tokens": ["Bl\u00fc\u00b7chers", "m\u00e4cht'\u00b7ger", "Schlacht\u00b7ge\u00b7sang", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "\u201er\u00fcckw\u00e4rts! r\u00fcckw\u00e4rts!\u201c das sind Weisen", "tokens": ["\u201e", "r\u00fcck\u00b7w\u00e4rts", "!", "r\u00fcck\u00b7w\u00e4rts", "!", "\u201c", "das", "sind", "Wei\u00b7sen"], "token_info": ["punct", "word", "punct", "word", "punct", "punct", "word", "word", "word"], "pos": ["$(", "ADV", "$.", "ADV", "$.", "$(", "PDS", "VAFIN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wohl aus Busen irr und krank.", "tokens": ["Wohl", "aus", "Bu\u00b7sen", "irr", "und", "krank", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NE", "NE", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Kreuz und Adler j\u00fcngst noch hie\u00dfen", "tokens": ["Kreuz", "und", "Ad\u00b7ler", "j\u00fcngst", "noch", "hie\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "ADV", "ADV", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Unsre Oriflammen wir,", "tokens": ["Uns\u00b7re", "O\u00b7ri\u00b7flam\u00b7men", "wir", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und nun sollten wir erkiesen", "tokens": ["Und", "nun", "soll\u00b7ten", "wir", "er\u00b7kie\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VMFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Einen Krebs zum Siegspanier?", "tokens": ["Ei\u00b7nen", "Krebs", "zum", "Siegs\u00b7pa\u00b7nier", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "B\u00fcrgerss\u00f6hne, Ritterskinder", "tokens": ["B\u00fcr\u00b7gers\u00b7s\u00f6h\u00b7ne", ",", "Rit\u00b7ter\u00b7skin\u00b7der"], "token_info": ["word", "punct", "word"], "pos": ["NN", "$,", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wurden Br\u00fcder im Gefecht,", "tokens": ["Wur\u00b7den", "Br\u00fc\u00b7der", "im", "Ge\u00b7fecht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und nun ruft ihr: \u201eDer ist minder,", "tokens": ["Und", "nun", "ruft", "ihr", ":", "\u201e", "Der", "ist", "min\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "$.", "$(", "PDS", "VAFIN", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Der ist mehr, nach altem Recht!\u201c", "tokens": ["Der", "ist", "mehr", ",", "nach", "al\u00b7tem", "Recht", "!", "\u201c"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "VAFIN", "ADV", "$,", "APPR", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Aber h\u00f6rts! als sie vergossen", "tokens": ["A\u00b7ber", "h\u00f6rts", "!", "als", "sie", "ver\u00b7gos\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "$.", "KOUS", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da ihr Blut mit gleicher Ehr',", "tokens": ["Da", "ihr", "Blut", "mit", "glei\u00b7cher", "Ehr'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ists in einen Strom zerflossen,", "tokens": ["Ists", "in", "ei\u00b7nen", "Strom", "zer\u00b7flos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und den theilt ihr nimmermehr!", "tokens": ["Und", "den", "theilt", "ihr", "nim\u00b7mer\u00b7mehr", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Die Gleichtapfern, die Gleichfreien", "tokens": ["Die", "Gleich\u00b7tap\u00b7fern", ",", "die", "Gleich\u00b7frei\u00b7en"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "NN"], "meter": "--+--++-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Sammelte das gleiche Haus,", "tokens": ["Sam\u00b7mel\u00b7te", "das", "glei\u00b7che", "Haus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+---+-+", "measure": "dactylic.init"}, "line.3": {"text": "Euer R\u00fcckw\u00e4rts-R\u00fcckw\u00e4rts-Schreien", "tokens": ["Eu\u00b7er", "R\u00fc\u00b7ck\u00b7w\u00e4rts\u00b7R\u00fc\u00b7ck\u00b7w\u00e4rts\u00b7Schrei\u00b7en"], "token_info": ["word", "word"], "pos": ["PPOSAT", "NN"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Ruft sie Arm in Arm heraus;", "tokens": ["Ruft", "sie", "Arm", "in", "Arm", "he\u00b7raus", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "APPR", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Da\u00df sie zeigen ihre Wunden", "tokens": ["Da\u00df", "sie", "zei\u00b7gen", "ih\u00b7re", "Wun\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Blutend neu von euch erweckt;", "tokens": ["Blu\u00b7tend", "neu", "von", "euch", "er\u00b7weckt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "APPR", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie sie gleichen Tod gefunden,", "tokens": ["Wie", "sie", "glei\u00b7chen", "Tod", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie sie gleiche Erde deckt.", "tokens": ["Wie", "sie", "glei\u00b7che", "Er\u00b7de", "deckt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Vorw\u00e4rts! Vorw\u00e4rts! weiter! weiter!", "tokens": ["Vor\u00b7w\u00e4rts", "!", "Vor\u00b7w\u00e4rts", "!", "wei\u00b7ter", "!", "wei\u00b7ter", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "$.", "ADV", "$.", "ADV", "$.", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ueber Tr\u00fcmmer ewig todt.", "tokens": ["Ue\u00b7ber", "Tr\u00fcm\u00b7mer", "e\u00b7wig", "todt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Weh', o B\u00fcrgerfahne, heiter", "tokens": ["Weh'", ",", "o", "B\u00fcr\u00b7ger\u00b7fah\u00b7ne", ",", "hei\u00b7ter"], "token_info": ["word", "punct", "word", "word", "punct", "word"], "pos": ["NN", "$,", "FM", "NN", "$,", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In das frische Morgenroth!", "tokens": ["In", "das", "fri\u00b7sche", "Mor\u00b7gen\u00b7roth", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}