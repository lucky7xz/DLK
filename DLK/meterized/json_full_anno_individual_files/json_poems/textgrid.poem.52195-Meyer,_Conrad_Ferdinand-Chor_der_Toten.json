{"textgrid.poem.52195": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Chor der Toten", "genre": "verse", "period": "N.A.", "pub_year": 1861, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wir Toten, wir Toten sind gr\u00f6\u00dfere Heere", "tokens": ["Wir", "To\u00b7ten", ",", "wir", "To\u00b7ten", "sind", "gr\u00f6\u00b7\u00dfe\u00b7re", "Hee\u00b7re"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "NN", "$,", "PPER", "NN", "VAFIN", "ADJA", "NN"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Als ihr auf der Erde, als ihr auf dem Meere!", "tokens": ["Als", "ihr", "auf", "der", "Er\u00b7de", ",", "als", "ihr", "auf", "dem", "Mee\u00b7re", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "$,", "KOUS", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "Wir pfl\u00fcgten das Feld mit geduldigen Taten,", "tokens": ["Wir", "pfl\u00fcg\u00b7ten", "das", "Feld", "mit", "ge\u00b7dul\u00b7di\u00b7gen", "Ta\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Ihr schwinget die Sicheln und schneidet die Saaten,", "tokens": ["Ihr", "schwin\u00b7get", "die", "Si\u00b7cheln", "und", "schnei\u00b7det", "die", "Saa\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.5": {"text": "Und was wir vollendet und was wir begonnen,", "tokens": ["Und", "was", "wir", "voll\u00b7en\u00b7det", "und", "was", "wir", "be\u00b7gon\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VVPP", "KON", "PWS", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.6": {"text": "Das f\u00fcllt noch dort oben die rauschenden Bronnen,", "tokens": ["Das", "f\u00fcllt", "noch", "dort", "o\u00b7ben", "die", "rau\u00b7schen\u00b7den", "Bron\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ADV", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.7": {"text": "Und all unser Lieben und Hassen und Hadern,", "tokens": ["Und", "all", "un\u00b7ser", "Lie\u00b7ben", "und", "Has\u00b7sen", "und", "Ha\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "PPOSAT", "ADJA", "KON", "NN", "KON", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.8": {"text": "Das klopft noch dort oben in sterblichen Adern,", "tokens": ["Das", "klopft", "noch", "dort", "o\u00b7ben", "in", "sterb\u00b7li\u00b7chen", "A\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ADV", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.9": {"text": "Und was wir an g\u00fcltigen S\u00e4tzen gefunden,", "tokens": ["Und", "was", "wir", "an", "g\u00fcl\u00b7ti\u00b7gen", "S\u00e4t\u00b7zen", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.10": {"text": "Dran bleibt aller irdische Wandel gebunden,", "tokens": ["Dran", "bleibt", "al\u00b7ler", "ir\u00b7di\u00b7sche", "Wan\u00b7del", "ge\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PIAT", "ADJA", "NN", "VVPP", "$,"], "meter": "--+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Und unsere T\u00f6ne, Gebilde, Gedichte", "tokens": ["Und", "un\u00b7se\u00b7re", "T\u00f6\u00b7ne", ",", "Ge\u00b7bil\u00b7de", ",", "Ge\u00b7dich\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word"], "pos": ["KON", "PPOSAT", "NN", "$,", "NN", "$,", "NN"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.12": {"text": "Erk\u00e4mpfen den Lorbeer im strahlenden Lichte,", "tokens": ["Er\u00b7k\u00e4mp\u00b7fen", "den", "Lor\u00b7beer", "im", "strah\u00b7len\u00b7den", "Lich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.13": {"text": "Wir suchen noch immer die menschlichen Ziele \u2013", "tokens": ["Wir", "su\u00b7chen", "noch", "im\u00b7mer", "die", "menschli\u00b7chen", "Zie\u00b7le", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Drum ehret und opfert! Denn unser sind viele!", "tokens": ["Drum", "eh\u00b7ret", "und", "op\u00b7fert", "!", "Denn", "un\u00b7ser", "sind", "vie\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "KON", "VVFIN", "$.", "KON", "PPOSAT", "VAFIN", "PIS", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}}, "stanza.2": {"line.1": {"text": "Wir Toten, wir Toten sind gr\u00f6\u00dfere Heere", "tokens": ["Wir", "To\u00b7ten", ",", "wir", "To\