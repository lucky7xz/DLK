{"dta.poem.11596": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Trost.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905193969", "language": ["de:0.99"], "booktitle": "M\u00f6rike, Eduard: Gedichte. Stuttgart, 1838."}, "poem": {"stanza.1": {"line.1": {"text": "Ja, mein Gl\u00fcck, das lang gewohnte,", "tokens": ["Ja", ",", "mein", "Gl\u00fcck", ",", "das", "lang", "ge\u00b7wohn\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPOSAT", "NN", "$,", "PRELS", "ADJD", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Endlich hat es mich verlassen!", "tokens": ["End\u00b7lich", "hat", "es", "mich", "ver\u00b7las\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PRF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "\u2014 Ja, die liebsten Freunde seh ich", "tokens": ["Ja", ",", "die", "liebs\u00b7ten", "Freun\u00b7de", "seh", "ich"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["$(", "PTKANT", "$,", "ART", "ADJA", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Achselzuckend von mir weichen,", "tokens": ["Ach\u00b7sel\u00b7zu\u00b7ckend", "von", "mir", "wei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "APPR", "PPER", "VVINF", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.5": {"text": "Und die gnadenreichen G\u00f6tter,", "tokens": ["Und", "die", "gna\u00b7den\u00b7rei\u00b7chen", "G\u00f6t\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Die am besten H\u00fclfe w\u00fc\u00dften,", "tokens": ["Die", "am", "bes\u00b7ten", "H\u00fcl\u00b7fe", "w\u00fc\u00df\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Kehren h\u00f6hnisch mir den R\u00fccken.", "tokens": ["Keh\u00b7ren", "h\u00f6h\u00b7nisch", "mir", "den", "R\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Was beginnen? werd' ich etwa,", "tokens": ["Was", "be\u00b7gin\u00b7nen", "?", "werd'", "ich", "et\u00b7wa", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVINF", "$.", "VAFIN", "PPER", "ADV", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.9": {"text": "Meinen Lebenstag verw\u00fcnschend,", "tokens": ["Mei\u00b7nen", "Le\u00b7bens\u00b7tag", "ver\u00b7w\u00fcn\u00b7schend", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Rasch nach Gift und Messer greifen?", "tokens": ["Rasch", "nach", "Gift", "und", "Mes\u00b7ser", "grei\u00b7fen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "KON", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Das sey ferne! vielmehr mu\u00df man", "tokens": ["Das", "sey", "fer\u00b7ne", "!", "viel\u00b7mehr", "mu\u00df", "man"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ADV", "$.", "ADV", "VMFIN", "PIS"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.12": {"text": "Stille sich im Herzen fassen.", "tokens": ["Stil\u00b7le", "sich", "im", "Her\u00b7zen", "fas\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Und ich sprach zu meinem Herzen:", "tokens": ["Und", "ich", "sprach", "zu", "mei\u00b7nem", "Her\u00b7zen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "La\u00df uns fest zusammenhalten!", "tokens": ["La\u00df", "uns", "fest", "zu\u00b7sam\u00b7men\u00b7hal\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn wir kennen uns einander,", "tokens": ["Denn", "wir", "ken\u00b7nen", "uns", "ein\u00b7an\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "PRF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie ihr Nest die Schwalbe kennet,", "tokens": ["Wie", "ihr", "Nest", "die", "Schwal\u00b7be", "ken\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wie die Cither kennt den S\u00e4nger,", "tokens": ["Wie", "die", "Cit\u00b7her", "kennt", "den", "S\u00e4n\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wie sich Schwert und Schild erkennen,", "tokens": ["Wie", "sich", "Schwert", "und", "Schild", "er\u00b7ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "NE", "KON", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Schild und Schwert einander lieben.", "tokens": ["Schild", "und", "Schwert", "ein\u00b7an\u00b7der", "lie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Solch ein Paar, wer mag es scheiden?", "tokens": ["Solch", "ein", "Paar", ",", "wer", "mag", "es", "schei\u00b7den", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ART", "NN", "$,", "PWS", "VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Als ich dieses Wort gesprochen,", "tokens": ["Als", "ich", "die\u00b7ses", "Wort", "ge\u00b7spro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PDAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "H\u00fcpfte mir das Herz im Busen,", "tokens": ["H\u00fcpf\u00b7te", "mir", "das", "Herz", "im", "Bu\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Das noch erst geweinet hatte.", "tokens": ["Das", "noch", "erst", "ge\u00b7wei\u00b7net", "hat\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ADV", "VVPP", "VAFIN", "$."], "meter": "--+-+-+-", "measure": "anapaest.init"}}}}}