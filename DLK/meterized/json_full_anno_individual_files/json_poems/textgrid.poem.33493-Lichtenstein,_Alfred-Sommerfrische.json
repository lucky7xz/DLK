{"textgrid.poem.33493": {"metadata": {"author": {"name": "Lichtenstein, Alfred", "birth": "N.A.", "death": "N.A."}, "title": "Sommerfrische", "genre": "verse", "period": "N.A.", "pub_year": 1913, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Himmel ist wie eine blaue Qualle.", "tokens": ["Der", "Him\u00b7mel", "ist", "wie", "ei\u00b7ne", "blau\u00b7e", "Qual\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "KOKOM", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und rings sind Felder, gr\u00fcne Wiesenh\u00fcgel \u2013", "tokens": ["Und", "rings", "sind", "Fel\u00b7der", ",", "gr\u00fc\u00b7ne", "Wie\u00b7sen\u00b7h\u00fc\u00b7gel", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "NN", "$,", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Friedliche Welt, du gro\u00dfe Mausefalle,", "tokens": ["Fried\u00b7li\u00b7che", "Welt", ",", "du", "gro\u00b7\u00dfe", "Mau\u00b7se\u00b7fal\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Entk\u00e4m ich endlich dir ... O h\u00e4tt ich Fl\u00fcgel \u2013", "tokens": ["Ent\u00b7k\u00e4m", "ich", "end\u00b7lich", "dir", "...", "O", "h\u00e4tt", "ich", "Fl\u00fc\u00b7gel", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "PPER", "$(", "NE", "VAFIN", "PPER", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Man w\u00fcrfelt. S\u00e4uft. Man schwatzt von Zukunftsstaaten.", "tokens": ["Man", "w\u00fcr