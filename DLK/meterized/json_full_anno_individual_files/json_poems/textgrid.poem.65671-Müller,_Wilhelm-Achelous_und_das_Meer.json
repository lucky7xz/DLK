{"textgrid.poem.65671": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Achelous und das Meer", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbachelous, Achelous, sag', was toben deine Wellen?", "tokens": ["\u00bb", "ac\u00b7he\u00b7lous", ",", "A\u00b7che\u00b7lous", ",", "sag'", ",", "was", "to\u00b7ben", "dei\u00b7ne", "Wel\u00b7len", "?"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "XY", "$,", "NE", "$,", "VVFIN", "$,", "PWS", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+--+-+--+-+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Haben Pindus wei\u00dfe Gipfel dich berauscht mit jungen Quellen?", "tokens": ["Ha\u00b7ben", "Pin\u00b7dus", "wei\u00b7\u00dfe", "Gip\u00b7fel", "dich", "be\u00b7rauscht", "mit", "jun\u00b7gen", "Quel\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NE", "ADJA", "NN", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Rissen wasserschwere Wolken sich an seinen scharfen Spitzen", "tokens": ["Ris\u00b7sen", "was\u00b7ser\u00b7schwe\u00b7re", "Wol\u00b7ken", "sich", "an", "sei\u00b7nen", "schar\u00b7fen", "Spit\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "ADJA", "NN", "PRF", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Von einander und entluden sich mit Donnern und mit Blitzen?", "tokens": ["Von", "ein\u00b7an\u00b7der", "und", "ent\u00b7lu\u00b7den", "sich", "mit", "Don\u00b7nern", "und", "mit", "Blit\u00b7zen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "KON", "VVFIN", "PRF", "APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.5": {"text": "Sag', woher der wilde Taumel, welcher h\u00e4uptlings deine Wogen", "tokens": ["Sag'", ",", "wo\u00b7her", "der", "wil\u00b7de", "Tau\u00b7mel", ",", "wel\u00b7cher", "h\u00e4upt\u00b7lings", "dei\u00b7ne", "Wo\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "PWAV", "ART", "ADJA", "NN", "$,", "PRELS", "ADV", "PPOSAT", "NN"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.6": {"text": "St\u00fcrzt in meine stillen Fluthen, die kein Wind hat \u00fcberflogen?\u00ab", "tokens": ["St\u00fcrzt", "in", "mei\u00b7ne", "stil\u00b7len", "Flut\u00b7hen", ",", "die", "kein", "Wind", "hat", "\u00fc\u00b7berf\u00b7lo\u00b7gen", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "APPR", "PPOSAT", "ADJA", "NN", "$,", "PRELS", "PIAT", "NN", "VAFIN", "VVPP", "$.", "$("], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}, "stanza.2": {"line.1": {"text": "Keine junge Wasserquelle hat berauscht mich alten Zecher,", "tokens": ["Kei\u00b7ne", "jun\u00b7ge", "Was\u00b7ser\u00b7quel\u00b7le", "hat", "be\u00b7rauscht", "mich", "al\u00b7ten", "Ze\u00b7cher", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VAFIN", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "'s ward kein Wasserschlauch zerrissen von dem j\u00e4hen Wolkenbrecher.", "tokens": ["'s", "ward", "kein", "Was\u00b7ser\u00b7schlauch", "zer\u00b7ris\u00b7sen", "von", "dem", "j\u00e4\u00b7hen", "Wol\u00b7ken\u00b7bre\u00b7cher", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "VVPP", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.3": {"text": "Was ich taumle? Was ich st\u00fcrze? Was es tobt in meinem Bette? \u2013", "tokens": ["Was", "ich", "taum\u00b7le", "?", "Was", "ich", "st\u00fcr\u00b7ze", "?", "Was", "es", "tobt", "in", "mei\u00b7nem", "Bet\u00b7te", "?", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$.", "PWS", "PPER", "VVFIN", "$.", "PWS", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$.", "$("], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Vater Ozean, o da\u00df ich warmes Blut f\u00fcr dich noch h\u00e4tte!", "tokens": ["Va\u00b7ter", "O\u00b7ze\u00b7an", ",", "o", "da\u00df", "ich", "war\u00b7mes", "Blut", "f\u00fcr", "dich", "noch", "h\u00e4t\u00b7te", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "FM", "KOUS", "PPER", "ADJA", "NN", "APPR", "PPER", "ADV", "VAFIN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.5": {"text": "Warmes Blut hab' ich getrunken, warmes Blut in vollen Z\u00fcgen,", "tokens": ["War\u00b7mes", "Blut", "hab'", "ich", "ge\u00b7trun\u00b7ken", ",", "war\u00b7mes", "Blut", "in", "vol\u00b7len", "Z\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "PPER", "VVPP", "$,", "ADJA", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.6": {"text": "Warmes Blut der freien Griechen, die an meinen Ufern liegen,", "tokens": ["War\u00b7mes", "Blut", "der", "frei\u00b7en", "Grie\u00b7chen", ",", "die", "an", "mei\u00b7nen", "U\u00b7fern", "lie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "ADJA", "NN", "$,", "PRELS", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.7": {"text": "Hingestreckt auf Lorbeerzweigen, \u00fcberweht von Siegesfahnen,", "tokens": ["Hin\u00b7ge\u00b7streckt", "auf", "Lor\u00b7beer\u00b7zwei\u00b7gen", ",", "\u00fc\u00b7ber\u00b7weht", "von", "Sie\u00b7ges\u00b7fah\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "$,", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.8": {"text": "Hoch umrauscht vom Geisterreigen ihrer Br\u00fcder, ihrer Ahnen.", "tokens": ["Hoch", "um\u00b7rauscht", "vom", "Geis\u00b7ter\u00b7rei\u00b7gen", "ih\u00b7rer", "Br\u00fc\u00b7der", ",", "ih\u00b7rer", "Ah\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "APPRART", "NN", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.9": {"text": "Solches Blut hab' ich getrunken heut' von den Agr\u00e4er Fluren \u2013", "tokens": ["Sol\u00b7ches", "Blut", "hab'", "ich", "ge\u00b7trun\u00b7ken", "heut'", "von", "den", "Ag\u00b7r\u00e4\u00b7er", "Flu\u00b7ren", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PPER", "VVPP", "ADV", "APPR", "ART", "NN", "NN", "$("], "meter": "+-+-+-+-+--+--+-", "measure": "trochaic.septa.relaxed"}, "line.10": {"text": "Fragst du auch nach Sklavenblute? \u2013 In Mor\u00e4sten such' die Spuren", "tokens": ["Fragst", "du", "auch", "nach", "Skla\u00b7ven\u00b7blu\u00b7te", "?", "\u2013", "In", "Mo\u00b7r\u00e4s\u00b7ten", "such'", "die", "Spu\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "NN", "$.", "$(", "APPR", "NN", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.11": {"text": "Seiner Str\u00f6me; jeden lauen Tropfen hab' ich ausgespieen:", "tokens": ["Sei\u00b7ner", "Str\u00f6\u00b7me", ";", "je\u00b7den", "lau\u00b7en", "Trop\u00b7fen", "hab'", "ich", "aus\u00b7ge\u00b7spi\u00b7een", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "PIAT", "ADJA", "NN", "VAFIN", "PPER", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.12": {"text": "Freies Griechenblut nur trank ich, kannt' es wohl an seinem Gl\u00fchen.", "tokens": ["Frei\u00b7es", "Grie\u00b7chen\u00b7blut", "nur", "trank", "ich", ",", "kannt'", "es", "wohl", "an", "sei\u00b7nem", "Gl\u00fc\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "VVFIN", "PPER", "$,", "VMFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.13": {"text": "Vater Ozean, da fing ich an von alter Zeit zu tr\u00e4umen", "tokens": ["Va\u00b7ter", "O\u00b7ze\u00b7an", ",", "da", "fing", "ich", "an", "von", "al\u00b7ter", "Zeit", "zu", "tr\u00e4u\u00b7men"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "NE", "$,", "ADV", "VVFIN", "PPER", "APPR", "APPR", "ADJA", "NN", "PTKZU", "VVINF"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.14": {"text": "Und von junger Freiheitswonne brausend mich emporzub\u00e4umen,", "tokens": ["Und", "von", "jun\u00b7ger", "Frei\u00b7heits\u00b7won\u00b7ne", "brau\u00b7send", "mich", "em\u00b7por\u00b7zu\u00b7b\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "ADJD", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.15": {"text": "Also da\u00df des Ufers Bande mich nicht l\u00e4nger konnten halten,", "tokens": ["Al\u00b7so", "da\u00df", "des", "U\u00b7fers", "Ban\u00b7de", "mich", "nicht", "l\u00e4n\u00b7ger", "konn\u00b7ten", "hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "ART", "NN", "NN", "PPER", "PTKNEG", "ADJD", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.16": {"text": "Da\u00df erzitterten die Ebnen und die Berge wiederschallten.", "tokens": ["Da\u00df", "er\u00b7zit\u00b7ter\u00b7ten", "die", "Eb\u00b7nen", "und", "die", "Ber\u00b7ge", "wie\u00b7der\u00b7schall\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "ART", "NN", "KON", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.17": {"text": "Nimm mich auf, du Weltumarmer, trage meine hohen Wogen", "tokens": ["Nimm", "mich", "auf", ",", "du", "Wel\u00b7tu\u00b7mar\u00b7mer", ",", "tra\u00b7ge", "mei\u00b7ne", "ho\u00b7hen", "Wo\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "PTKVZ", "$,", "PPER", "NN", "$,", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.18": {"text": "Ungemischt und ungeb\u00e4ndigt, mit dem Blut, das sie gesogen,", "tokens": ["Un\u00b7ge\u00b7mischt", "und", "un\u00b7ge\u00b7b\u00e4n\u00b7digt", ",", "mit", "dem", "Blut", ",", "das", "sie", "ge\u00b7so\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "$,", "APPR", "ART", "NN", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.19": {"text": "Fort gen Norden und gen Westen, da\u00df sie an die Ufer schlagen,", "tokens": ["Fort", "gen", "Nor\u00b7den", "und", "gen", "Wes\u00b7ten", ",", "da\u00df", "sie", "an", "die", "U\u00b7fer", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "NN", "KON", "APPR", "NN", "$,", "KOUS", "PPER", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.20": {"text": "Und den Felsen und den Menschen laute Kund' aus Hellas sagen!", "tokens": ["Und", "den", "Fel\u00b7sen", "und", "den", "Men\u00b7schen", "lau\u00b7te", "Kund'", "aus", "Hel\u00b7las", "sa\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "KON", "ART", "NN", "VVFIN", "NN", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}, "stanza.3": {"line.1": {"text": "\u00bbachelous, Achelous, sag', was toben deine Wellen?", "tokens": ["\u00bb", "ac\u00b7he\u00b7lous", ",", "A\u00b7che\u00b7lous", ",", "sag'", ",", "was", "to\u00b7ben", "dei\u00b7ne", "Wel\u00b7len", "?"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "XY", "$,", "NE", "$,", "VVFIN", "$,", "PWS", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+--+-+--+-+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Haben Pindus wei\u00dfe Gipfel dich berauscht mit jungen Quellen?", "tokens": ["Ha\u00b7ben", "Pin\u00b7dus", "wei\u00b7\u00d