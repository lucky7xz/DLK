{"textgrid.poem.61056": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Wolle keiner mich fragen", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wolle keiner mich fragen,", "tokens": ["Wol\u00b7le", "kei\u00b7ner", "mich", "fra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "PRF", "VVINF", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Warum mein Herz so schl\u00e4gt,", "tokens": ["Wa\u00b7rum", "mein", "Herz", "so", "schl\u00e4gt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich kann's nicht fassen, nicht sagen,", "tokens": ["Ich", "kann's", "nicht", "fas\u00b7sen", ",", "nicht", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "VVINF", "$,", "PTKNEG", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Was mich bewegt.", "tokens": ["Was", "mich", "be\u00b7wegt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Als wie im Traume schwanken", "tokens": ["Als", "wie", "im", "Trau\u00b7me", "schwan\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "KOKOM", "APPRART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Trunken die Sinne mir;", "tokens": ["Trun\u00b7ken", "die", "Sin\u00b7ne", "mir", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PPER", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Alle meine Gedanken", "tokens": ["Al\u00b7le", "mei\u00b7ne", "Ge\u00b7dan\u00b7ken"], "token_info": ["word", "word", "word"], "pos": ["PIAT", "PPOSAT", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Sind nur bei dir.", "tokens": ["Sind", "nur", "bei", "dir", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Ich habe die Welt vergessen,", "tokens": ["Ich", "ha\u00b7be", "die", "Welt", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Seit ich dein Auge gesehn;", "tokens": ["Seit", "ich", "dein", "Au\u00b7ge", "ge\u00b7sehn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Ich m\u00f6chte dich an mich pressen", "tokens": ["Ich", "m\u00f6ch\u00b7te", "dich", "an", "mich", "pres\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PRF", "APPR", "PPER", "VVFIN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und still im Ku\u00df vergehn.", "tokens": ["Und", "still", "im", "Ku\u00df", "ver\u00b7gehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Mein Leben m\u00f6cht' ich lassen", "tokens": ["Mein", "Le\u00b7ben", "m\u00f6cht'", "ich", "las\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Um ein L\u00e4cheln von dir,", "