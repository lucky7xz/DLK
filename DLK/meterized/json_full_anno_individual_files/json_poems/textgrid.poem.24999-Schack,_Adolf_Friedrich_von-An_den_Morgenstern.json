{"textgrid.poem.24999": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "An den Morgenstern", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Von allen, die am Himmel sind,", "tokens": ["Von", "al\u00b7len", ",", "die", "am", "Him\u00b7mel", "sind", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "$,", "PRELS", "APPRART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie dich lieb' ich nicht einen,", "tokens": ["Wie", "dich", "lieb'", "ich", "nicht", "ei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "PPER", "PTKNEG", "ART", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Mein Auge h\u00e4ngt, wie da ich Kind,", "tokens": ["Mein", "Au\u00b7ge", "h\u00e4ngt", ",", "wie", "da", "ich", "Kind", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,", "KOKOM", "KOUS", "PPER", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "An deinem Glanz, dem reinen.", "tokens": ["An", "dei\u00b7nem", "Glanz", ",", "dem", "rei\u00b7nen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "ART", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Noch tr\u00e4umend liegt der junge Tag", "tokens": ["Noch", "tr\u00e4u\u00b7mend", "liegt", "der", "jun\u00b7ge", "Tag"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auf den begr\u00fcnten Matten