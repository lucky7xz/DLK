{"textgrid.poem.33768": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "Friedrich und Ezelin", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mit dem zweiten Kaiser Friedrich ritt dereinst Held Ezelin,", "tokens": ["Mit", "dem", "zwei\u00b7ten", "Kai\u00b7ser", "Fried\u00b7rich", "ritt", "de\u00b7reinst", "Held", "E\u00b7ze\u00b7lin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NE", "VVFIN", "ADV", "NN", "NE", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Fr\u00f6hlich ritten beide F\u00fcrsten l\u00e4ngs bebl\u00fcmter Au'n dahin,", "tokens": ["Fr\u00f6h\u00b7lich", "rit\u00b7ten", "bei\u00b7de", "F\u00fcrs\u00b7ten", "l\u00e4ngs", "be\u00b7bl\u00fcm\u00b7ter", "Au'n", "da\u00b7hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PIAT", "NN", "APPR", "ADJA", "NN", "PAV", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Und sechshundert Ritter sprengten im Gefolg des Kaisers vor,", "tokens": ["Und", "sechs\u00b7hun\u00b7dert", "Rit\u00b7ter", "spreng\u00b7ten", "im", "Ge\u00b7folg", "des", "Kai\u00b7sers", "vor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "CARD", "NN", "VVFIN", "APPRART", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+--+-+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "Und mit Ezelin sechshundert ritten durch Pavia's Tor.", "tokens": ["Und", "mit", "E\u00b7ze\u00b7lin", "sechs\u00b7hun\u00b7dert", "rit\u00b7ten", "durch", "Pa\u00b7via's", "Tor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "pun