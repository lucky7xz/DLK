{"textgrid.poem.58620": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "52.", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Seel', die Gott verlobt und ihre ganze Liebe", "tokens": ["Die", "Seel'", ",", "die", "Gott", "ver\u00b7lobt", "und", "ih\u00b7re", "gan\u00b7ze", "Lie\u00b7be"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "NN", "VVPP", "KON", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von Kreaturentrost in ihn allein einkehrt", "tokens": ["Von", "Kre\u00b7a\u00b7tu\u00b7ren\u00b7trost", "in", "ihn", "al\u00b7lein", "ein\u00b7kehrt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "APPR", "PPER", "ADV", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und sich beschneidet so aus reinem Liebestriebe,", "tokens": ["Und", "sich", "be\u00b7schnei\u00b7det", "so", "aus", "rei\u00b7nem", "Lie\u00b7be\u00b7strie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "VVFIN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die ist nicht d\u00fcrr und schlecht, sie ist bei Gott geehrt;", "tokens": ["Die", "ist", "nicht", "d\u00fcrr", "und", "schlecht", ",", "sie", "ist", "bei", "Gott", "ge\u00b7ehrt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PTKNEG", "ADJD", "KON", "ADJD", "$,", "PPER", "VAFIN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ihr Werk ist Sabbatruh, nur Gott ist ihr Verlangen,", "tokens": ["Ihr", "Werk", "ist", "Sab\u00b7ba\u00b7truh", ",", "nur", "Gott", "ist", "ihr", "Ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "NE", "$,", "ADV", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Sie h\u00e4lt am Liebesbund, wodurch sie ihm vertraut,", "tokens": ["Sie", "h\u00e4lt", "am", "Lie\u00b7bes\u00b7bund", ",", "wo\u00b7durch", "sie", "ihm", "ver\u00b7traut", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$,", "PWAV", "PPER", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Drum sie im sch\u00f6nsten Kranz vor Gottes Thron soll prangen,", "tokens": ["Drum", "sie", "im", "sch\u00f6ns\u00b7ten", "Kranz", "vor", "Got\u00b7tes", "Thron", "soll", "pran\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "APPRART", "ADJA", "NN", "APPR", "NN", "NN", "VMFIN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ihr ew'ger Nam' soll sein: Des Himmelsk\u00f6nigs Braut.", "tokens": ["Ihr", "ew'\u00b7ger", "Nam'", "soll", "sein", ":", "Des", "Him\u00b7mels\u00b7k\u00f6\u00b7nigs", "Braut", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VMFIN", "VAINF", "$.", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die Seel', die Gott verlobt und ihre ganze Liebe", "tokens": ["Die", "Seel'", ",", "die", "Gott", "ver\u00b7lobt", "und", "ih\u00b7re", "gan\u00b7ze", "Lie\u0