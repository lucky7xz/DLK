{"dta.poem.10501": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "LvIII.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Zu fragmentisch ist Welt und Leben,", "tokens": ["Zu", "frag\u00b7men\u00b7tisch", "ist", "Welt", "und", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "VAFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ich will mich zum deutschen Professor begeben,", "tokens": ["Ich", "will", "mich", "zum", "deut\u00b7schen", "Pro\u00b7fes\u00b7sor", "be\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "APPRART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Der wei\u00df das Leben zusammen zu setzen,", "tokens": ["Der", "wei\u00df", "das", "Le\u00b7ben", "zu\u00b7sam\u00b7men", "zu", "set\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "VVINF", "PTKZU", "VVINF", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und er macht ein verst\u00e4ndlich System daraus;", "tokens": ["Und", "er", "macht", "ein", "ver\u00b7st\u00e4nd\u00b7lich", "Sys\u00b7tem", "da\u00b7raus", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJD", "NN", "PAV", "$."], "meter": "--+--+-+-++", "measure": "anapaest.di.plus"}, "line.5": {"text": "Mit seinen Nachtm\u00fctzen ", "tokens": ["Mit", "sei\u00b7nen", "Nacht\u00b7m\u00fct\u00b7zen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-++-", "measure": "unknown.measure.tri"}, "line.6": {"text": "Stopft er die L\u00fccken des Weltenbau's.", "tokens": ["Stopft", "er", "die", "L\u00fc\u00b7cken", "des", "Wel\u00b7ten\u00b7bau'", "s."], "token_info": ["word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["VVFIN", "PPER", "ART", "NN", "ART", "NN", "NE"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}}}}