{"dta.poem.22917": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "224. Wie man zur Einigkeit gelangt.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1657", "urn": "urn:nbn:de:kobv:b4-200905198292", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Geistreiche Sinn- vnd Schlussrime. Wien, 1657."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn sich der Mensch entzieht der mannigfaltigkeit/", "tokens": ["Wenn", "sich", "der", "Mensch", "ent\u00b7zieht", "der", "man\u00b7nig\u00b7fal\u00b7tig\u00b7keit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und kehrt sich ein zu Gott/ kombt er zur Einigkeit.", "tokens": ["Und", "kehrt", "sich", "ein", "zu", "Gott", "/", "kombt", "er", "zur", "Ei\u00b7nig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ART", "APPR", "NN", "$(", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}