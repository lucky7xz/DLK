{"dta.poem.10811": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "Der Unglaube.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198774", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Unglaub ist die Quell woraus die\nUnruh fliest,", "tokens": ["Der", "Un\u00b7glaub", "ist", "die", "Quell", "wo\u00b7raus", "die", "Un\u00b7ruh", "fliest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Woraus das bittre Weh, das uns", "tokens": ["Wo\u00b7raus", "das", "bitt\u00b7re", "Weh", ",", "das", "uns"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN", "$,", "PRELS", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Woraus das Ungl\u00fck kommt, und", "tokens": ["Wo\u00b7raus", "das", "Un\u00b7gl\u00fck", "kommt", ",", "und"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["PWAV", "ART", "NN", "VVFIN", "$,", "KON"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Was uns hie in der Zeit, dort", "tokens": ["Was", "uns", "hie", "in", "der", "Zeit", ",", "dort"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["PWS", "PPER", "ADV", "APPR", "ART", "NN", "$,", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der Mensch, der arme Mensch ist leider von Na-", "tokens": ["Der", "Mensch", ",", "der", "ar\u00b7me", "Mensch", "ist", "lei\u00b7der", "von", "Na"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "NN", "VAFIN", "ADV", "APPR", "TRUNC"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.6": {"text": "Die ungl\u00fckselige verdorbne Kreatur,", "tokens": ["Die", "un\u00b7gl\u00fck\u00b7se\u00b7li\u00b7ge", "ver\u00b7dorb\u00b7ne", "Kre\u00b7a\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die am Verstande blind, am Willen ganz verkeh-", "tokens": ["Die", "am", "Ver\u00b7stan\u00b7de", "blind", ",", "am", "Wil\u00b7len", "ganz", "ver\u00b7keh"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "APPRART", "NN", "ADJD", "$,", "APPRART", "NN", "ADV", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Die sehend doch nicht sieht, und h\u00f6rend doch nicht", "tokens": ["Die", "se\u00b7hend", "doch", "nicht", "sieht", ",", "und", "h\u00f6\u00b7rend", "doch", "nicht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "ADV", "PTKNEG", "VVFIN", "$,", "KON", "ADJD", "ADV", "PTKNEG"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.9": {"text": "Er liebt die Finsternis mehr als das klare Licht,", "tokens": ["Er", "liebt", "die", "Fins\u00b7ter\u00b7nis", "mehr", "als", "das", "kla\u00b7re", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PIAT", "KOKOM", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und daher kommt es auch da\u00df er des Glaubens", "tokens": ["Und", "da\u00b7her", "kommt", "es", "auch", "da\u00df", "er", "des", "Glau\u00b7bens"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PAV", "VVFIN", "PPER", "ADV", "KOUS", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Die er aus\u00fcben mu\u00df, nicht also wil erkennen;", "tokens": ["Die", "er", "aus\u00b7\u00fc\u00b7ben", "mu\u00df", ",", "nicht", "al\u00b7so", "wil", "er\u00b7ken\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVINF", "VMFIN", "$,", "PTKNEG", "ADV", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Weil der Verstand verderbt; so kan er nicht recht", "tokens": ["Weil", "der", "Ver\u00b7stand", "ver\u00b7derbt", ";", "so", "kan", "er", "nicht", "recht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "VVPP", "$.", "ADV", "VMFIN", "PPER", "PTKNEG", "ADJD"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.13": {"text": "Was falsch von dem was wahr; des Willens Lei-", "tokens": ["Was", "falsch", "von", "dem", "was", "wahr", ";", "des", "Wil\u00b7lens", "Lei"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "ADJD", "APPR", "ART", "PWS", "ADJD", "$.", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Bemeistert sich bei ihm der regen Urtheils Krafft;", "tokens": ["Be\u00b7meis\u00b7tert", "sich", "bei", "ihm", "der", "re\u00b7gen", "Ur\u00b7theils", "Krafft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PPER", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.15": {"text": "Sie regt der Zweiffel Heer, die irrenden Gedan-", "tokens": ["Sie", "regt", "der", "Zweif\u00b7fel", "Heer", ",", "die", "ir\u00b7ren\u00b7den", "Ge\u00b7dan"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Die um der Warheit Licht, als wie die Wespen", "tokens": ["Die", "um", "der", "War\u00b7heit", "Licht", ",", "als", "wie", "die", "Wes\u00b7pen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "NN", "$,", "KOUS", "KOKOM", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Sie schw\u00e4rmen hin und her auf ihrer Jrrebahn,", "tokens": ["Sie", "schw\u00e4r\u00b7men", "hin", "und", "her", "auf", "ih\u00b7rer", "Jr\u00b7re\u00b7bahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "KON", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Sie suchen nicht die Ruh, wo man sie finden", "tokens": ["Sie", "su\u00b7chen", "nicht", "die", "Ruh", ",", "wo", "man", "sie", "fin\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "ART", "NN", "$,", "PWAV", "PIS", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Weil sie der Unverstand durch blinden Trieb regie-", "tokens": ["Weil", "sie", "der", "Un\u00b7ver\u00b7stand", "durch", "blin\u00b7den", "Trieb", "re\u00b7gie"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "ADJA", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Und der verkehrte Sinn, das Blendlicht stets ver-", "tokens": ["Und", "der", "ver\u00b7kehr\u00b7te", "Sinn", ",", "das", "Blend\u00b7licht", "stets", "ver"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "$,", "ART", "NN", "ADV", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Wie elend ist ein Mensch der sich ums Ziel be-", "tokens": ["Wie", "e\u00b7lend", "ist", "ein", "Mensch", "der", "sich", "ums", "Ziel", "be"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "VAFIN", "ART", "NN", "ART", "PRF", "APPRART", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "Und durch ein blendend Glas das ihn betrieget", "tokens": ["Und", "durch", "ein", "blen\u00b7dend", "Glas", "das", "ihn", "be\u00b7trie\u00b7get"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJD", "NN", "ART", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.23": {"text": "Dem d\u00fcnket da\u00df er k\u00f6nn, den rechten Zwek errei-", "tokens": ["Dem", "d\u00fcn\u00b7ket", "da\u00df", "er", "k\u00f6nn", ",", "den", "rech\u00b7ten", "Zwek", "er\u00b7rei"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "KOUS", "PPER", "VMFIN", "$,", "ART", "ADJA", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Er folget dem Betrug und mu\u00df vom Weg abwei-", "tokens": ["Er", "fol\u00b7get", "dem", "Be\u00b7trug", "und", "mu\u00df", "vom", "Weg", "ab\u00b7wei"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "KON", "VMFIN", "APPRART", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "So gehts denselben auch der sich ein Gut vor-", "tokens": ["So", "gehts", "den\u00b7sel\u00b7ben", "auch", "der", "sich", "ein", "Gut", "vor"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PDS", "ADV", "ART", "PRF", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.26": {"text": "Das er erhalten wil, doch aber nicht erwegt,", "tokens": ["Das", "er", "er\u00b7hal\u00b7ten", "wil", ",", "doch", "a\u00b7ber", "nicht", "er\u00b7wegt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "VVINF", "VMFIN", "$,", "ADV", "ADV", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Was ihm recht vortheilhafft; drum mu\u00df er sich ver-", "tokens": ["Was", "ihm", "recht", "vor\u00b7theil\u00b7hafft", ";", "drum", "mu\u00df", "er", "sich", "ver"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "ADJD", "VVPP", "$.", "PAV", "VMFIN", "PPER", "PRF", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.28": {"text": "Und von der rechten Bahn der klaren Warheit ir-", "tokens": ["Und", "von", "der", "rech\u00b7ten", "Bahn", "der", "kla\u00b7ren", "War\u00b7heit", "ir"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Der Unglaub meint zu sehn, und siehet warlich", "tokens": ["Der", "Un\u00b7glaub", "meint", "zu", "sehn", ",", "und", "sie\u00b7het", "war\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PTKZU", "VVINF", "$,", "KON", "VVFIN", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.30": {"text": "Es ist der Seelen Aug nur ein verblendend Licht,", "tokens": ["Es", "ist", "der", "See\u00b7len", "Aug", "nur", "ein", "ver\u00b7blen\u00b7dend", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "NN", "ADV", "ART", "VVPP", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Und der Verstand bei ihm vom D\u00fcnkel eingenom-", "tokens": ["Und", "der", "Ver\u00b7stand", "bei", "ihm", "vom", "D\u00fcn\u00b7kel", "ein\u00b7ge\u00b7nom"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "PPER", "APPRART", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Drum kan er auch nie recht zum Licht der Warheit", "tokens": ["Drum", "kan", "er", "auch", "nie", "recht", "zum", "Licht", "der", "War\u00b7heit"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VMFIN", "PPER", "ADV", "ADV", "ADJD", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.33": {"text": "Wie elend ist ein Mensch in solcher Finsternis,", "tokens": ["Wie", "e\u00b7lend", "ist", "ein", "Mensch", "in", "sol\u00b7cher", "Fins\u00b7ter\u00b7nis", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "ART", "NN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Er hofft offt ohne Grund, und bleibet ungewis;", "tokens": ["Er", "hofft", "offt", "oh\u00b7ne", "Grund", ",", "und", "blei\u00b7bet", "un\u00b7ge\u00b7wis", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "NN", "$,", "KON", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Er w\u00fcnschet, und die Furcht der Henker banger", "tokens": ["Er", "w\u00fcn\u00b7schet", ",", "und", "die", "Furcht", "der", "Hen\u00b7ker", "ban\u00b7ger"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KON", "ART", "NN", "ART", "NN", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.36": {"text": "Sagt immer stets voraus, er m\u00fc\u00df sein Ziel verfeh-", "tokens": ["Sagt", "im\u00b7mer", "stets", "vo\u00b7raus", ",", "er", "m\u00fc\u00df", "sein", "Ziel", "ver\u00b7feh"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ADV", "PTKVZ", "$,", "PPER", "VMFIN", "PPOSAT", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Wer seinen ", "tokens": ["Wer", "sei\u00b7nen"], "token_info": ["word", "word"], "pos": ["PWS", "PPOSAT"], "meter": "-+-", "measure": "amphibrach.single"}, "line.38": {"text": "Die uns in Leiden Trost, in Ungl\u00fck H\u00fclf an-", "tokens": ["Die", "uns", "in", "Lei\u00b7den", "Trost", ",", "in", "Un\u00b7gl\u00fck", "H\u00fclf", "an"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "PPER", "APPR", "ADJA", "NN", "$,", "APPR", "NN", "NE", "TRUNC"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.39": {"text": "Der ist den Schiffern gleich, die auf den wilden", "tokens": ["Der", "ist", "den", "Schif\u00b7fern", "gleich", ",", "die", "auf", "den", "wil\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "NN", "ADV", "$,", "PRELS", "APPR", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.40": {"text": "Bei tobenden Orcan, bald auf, bald nieder schwel-", "tokens": ["Bei", "to\u00b7ben\u00b7den", "Or\u00b7can", ",", "bald", "auf", ",", "bald", "nie\u00b7der", "schwel"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "ADV", "PTKVZ", "$,", "ADV", "PTKVZ", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Nicht wissen wo sie seyn. Bald wirfft der Sturm", "tokens": ["Nicht", "wis\u00b7sen", "wo", "sie", "seyn", ".", "Bald", "wirfft", "der", "Sturm"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKNEG", "VVINF", "PWAV", "PPER", "VAINF", "$.", "ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.42": {"text": "Der lauter Wirbel regt, ihr Schiflein in die", "tokens": ["Der", "lau\u00b7ter", "Wir\u00b7bel", "regt", ",", "ihr", "Schi\u00b7flein", "in", "die"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "PIAT", "NN", "VVFIN", "$,", "PPOSAT", "NN", "APPR", "ART"], "meter": "-+-+-+-+-++", "measure": "unknown.measure.hexa"}, "line.43": {"text": "Bald st\u00f6\u00dft des Wassers Fall sie nieder, da sie sin-", "tokens": ["Bald", "st\u00f6\u00dft", "des", "Was\u00b7sers", "Fall", "sie", "nie\u00b7der", ",", "da", "sie", "sin"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN", "PPER", "PTKVZ", "$,", "KOUS", "PPER", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Bis da\u00df sie endlich drauf den bittren Todt eintrin-", "tokens": ["Bis", "da\u00df", "sie", "end\u00b7lich", "drauf", "den", "bit\u00b7tren", "Todt", "ein\u00b7trin"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "KOUS", "PPER", "ADV", "PAV", "ART", "ADJA", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Weil Mast und Anker fehlt. Ein Mensch der kei-", "tokens": ["Weil", "Mast", "und", "An\u00b7ker", "fehlt", ".", "E