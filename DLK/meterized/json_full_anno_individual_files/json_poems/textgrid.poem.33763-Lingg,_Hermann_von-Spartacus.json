{"textgrid.poem.33763": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "Spartacus", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Versammelt hielt sein Sklavenheer", "tokens": ["Ver\u00b7sam\u00b7melt", "hielt", "sein", "Skla\u00b7ven\u00b7heer"], "token_info": ["word", "word", "word", "word"], "pos": ["VVPP", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Thracier Spartacus am Meer,", "tokens": ["Der", "Thra\u00b7cier", "Spar\u00b7ta\u00b7cus", "am", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und auf zum rauchenden Vesuv", "tokens": ["Und", "auf", "zum", "rau\u00b7chen\u00b7den", "Ve\u00b7suv"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Erklang der wilde Freiheitsruf:", "tokens": ["Er\u00b7klang", "der", "wil\u00b7de", "Frei\u00b7heits\u00b7ruf", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Von nun an M\u00e4nner, nicht mehr Sklaven", "tokens": ["Von", "nun", "an", "M\u00e4n\u00b7ner", ",", "nicht", "mehr", "Skla\u00b7ven"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADV", "APPR", "NN", "$,", "PTKNEG", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Erheben wir das Schwert und strafen", "tokens": ["Er\u00b7he\u00b7ben", "wir", "das", "Schwert", "und", "stra\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "KON", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Der Unterdr\u00fccker \u00dcbermut.", "tokens": ["Der", "Un\u00b7ter\u00b7dr\u00fc\u00b7cker", "\u00dc\u00b7ber\u00b7mut", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Du Berg dort, blitz in unsre Rache!", "tokens": ["Du", "Berg", "dort", ",", "blitz", "in", "uns\u00b7re", "Ra\u00b7che", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ADV", "$,", "VVIMP", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Der Menschheit ganzes Herz erwache", "tokens": ["Der", "Menschheit", "gan\u00b7zes", "Herz", "er\u00b7wa\u00b7che"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.10": {"text": "In uns um ihr verlornes Gut.", "tokens": ["In", "uns", "um", "ihr", "ver\u00b7lor\u00b7nes", "Gut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Germanen, Scythen, Perser, Parther,", "tokens": ["Ger\u00b7ma\u00b7nen", ",", "Scyt\u00b7hen", ",", "Per\u00b7ser", ",", "Par\u00b7ther", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Illyrier, Gallier, Dacier, Sparter,", "tokens": ["Il\u00b7ly\u00b7rier", ",", "Gal\u00b7lier", ",", "Da\u00b7cier", ",", "Spar\u00b7ter", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.13": {"text": "Jetzt treffet, da\u00df die Wunde klafft!", "tokens": ["Jetzt", "tref\u00b7fet", ",", "da\u00df", "die", "Wun\u00b7de", "klafft", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "KOUS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Wir waren lang genug die Schl\u00e4chter", "tokens": ["Wir", "wa\u00b7ren", "lang", "ge\u00b7nug", "die", "Schl\u00e4ch\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADJD", "ADV", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "F\u00fcr dieses Volkes Blutgel\u00e4chter,", "tokens": ["F\u00fcr", "die\u00b7ses", "Vol\u00b7kes", "Blut\u00b7ge\u00b7l\u00e4ch\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Genug die M\u00f6rder unsrer Kraft.", "tokens": ["Ge\u00b7nug", "die", "M\u00f6r\u00b7der", "uns\u00b7rer", "Kraft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ein Tiger lauert in der Schlucht.", "tokens": ["Ein", "Ti\u00b7ger", "lau\u00b7ert", "in", "der", "Schlucht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auf, Nubier, jagt ihn in die Flucht!", "tokens": ["Auf", ",", "Nu\u00b7bier", ",", "jagt", "ihn", "in", "die", "Flucht", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$,", "NN", "$,", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Wolf ist's, Cimbern, der euch droht,", "tokens": ["Ein", "Wolf", "ist's", ",", "Cim\u00b7bern", ",", "der", "euch", "droht", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NE", "NE", "$,", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Schwingt eure Keulen, schlagt ihn tot!", "tokens": ["Schwingt", "eu\u00b7re", "Keu\u00b7len", ",", "schlagt", "ihn", "tot", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Beweis't die Kraft in euren Sehnen,", "tokens": ["Be\u00b7weis't", "die", "Kraft", "in", "eu\u00b7ren", "Seh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die ihr so oft in den Arenen", "tokens": ["Die", "ihr", "so", "oft", "in", "den", "A\u00b7re\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ADV", "ADV", "APPR", "ART", "NN"], "meter": "-+-+--+--", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Beim lauten Beifallruf erprobt!", "tokens": ["Beim", "lau\u00b7ten", "Bei\u00b7fall\u00b7ruf", "er\u00b7probt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Doch diesmal, wenn der Sand zerstoben,", "tokens": ["Doch", "dies\u00b7mal", ",", "wenn", "der", "Sand", "zer\u00b7sto\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "KOUS", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Soll euch der tote R\u00f6mer loben,", "tokens": ["Soll", "euch", "der", "to\u00b7te", "R\u00f6\u00b7mer", "lo\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Wie lebend er euch nie gelobt.", "tokens": ["Wie", "le\u00b7bend", "er", "euch", "nie", "ge\u00b7lobt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Erhebt die Schwerter, schwingt die Sensen!", "tokens": ["Er\u00b7hebt", "die", "Schwer\u00b7ter", ",", "schwingt", "die", "Sen\u00b7sen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Gebt ihnen Feste, gebt Circensen,", "tokens": ["Gebt", "ih\u00b7nen", "Fes\u00b7te", ",", "gebt", "Cir\u00b7cen\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "NN", "$,", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Gebt einen Gladiatorenkampf!", "tokens": ["Gebt", "ei\u00b7nen", "Gla\u00b7di\u00b7a\u00b7to\u00b7ren\u00b7kampf", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.14": {"text": "K\u00e4mpft, k\u00e4mpft, bis \u00fcber Leichenwogen", "tokens": ["K\u00e4mpft", ",", "k\u00e4mpft", ",", "bis", "\u00fc\u00b7ber", "Lei\u00b7chen\u00b7wo\u00b7gen"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$,", "VVFIN", "$,", "KOUS", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Das Ro\u00df der Ritter Purpurtogen", "tokens": ["Das", "Ro\u00df", "der", "Rit\u00b7ter", "Pur\u00b7pur\u00b7to\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "In Staub zum Rost der Kette stampf'!", "tokens": ["In", "Staub", "zum", "Rost", "der", "Ket\u00b7te", "stampf'", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPRART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Zerfallen mu\u00df dies Pantheon,", "tokens": ["Zer\u00b7fal\u00b7len", "mu\u00df", "dies", "Pan\u00b7the\u00b7on", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PDS", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dies Rom, wie ein Kolo\u00df von Ton;", "tokens": ["Dies", "Rom", ",", "wie", "ein", "Ko\u00b7lo\u00df", "von", "Ton", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NE", "$,", "PWAV", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sein Ruhm werd' aus der Welt gewischt,", "tokens": ["Sein", "Ruhm", "werd'", "aus", "der", "Welt", "ge\u00b7wischt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie Nachts ein Meteor erlischt.", "tokens": ["Wie", "Nachts", "ein", "Me\u00b7te\u00b7or", "er\u00b7lischt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Herab von ihren Marmortreppen", "tokens": ["Her\u00b7ab", "von", "ih\u00b7ren", "Mar\u00b7mor\u00b7trep\u00b7pen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wird man der W\u00f6lfin Beute schleppen,", "tokens": ["Wird", "man", "der", "W\u00f6l\u00b7fin", "Beu\u00b7te", "schlep\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Hinab in alle freie Welt;", "tokens": ["Hin\u00b7ab", "in", "al\u00b7le", "frei\u00b7e", "Welt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Bald t\u00f6nt das Echo freier Lieder", "tokens": ["Bald", "t\u00f6nt", "das", "E\u00b7cho", "frei\u00b7er", "Lie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Durch Thraciens Gebirge wider", "tokens": ["Durch", "Thra\u00b7ci\u00b7ens", "Ge\u00b7bir\u00b7ge", "wi\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NE", "NN", "PTKVZ"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.10": {"text": "Zum nordumst\u00fcrmten Hirtenzelt;", "tokens": ["Zum", "nor\u00b7dum\u00b7st\u00fcrm\u00b7ten", "Hir\u00b7ten\u00b7zelt", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Erbl\u00fchn wird wieder Saat den Fluren,", "tokens": ["Er\u00b7bl\u00fchn", "wird", "wie\u00b7der", "Saat", "den", "Flu\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Wo sonst die Siegeswagen fuhren,", "tokens": ["Wo", "sonst", "die", "Sie\u00b7ges\u00b7wa\u00b7gen", "fuh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "F\u00fcr die der Erdkreis schien zu schmal.", "tokens": ["F\u00fcr", "die", "der", "Erd\u00b7kreis", "schien", "zu", "schmal", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "VVFIN", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Zum Kampf denn, R\u00f6mer! La\u00dft uns streiten!", "tokens": ["Zum", "Kampf", "denn", ",", "R\u00f6\u00b7mer", "!", "La\u00dft", "uns", "strei\u00b7ten", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "$,", "NN", "$.", "VVIMP", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Es gr\u00fc\u00dfen euch die Todgeweihten,", "tokens": ["Es", "gr\u00fc\u00b7\u00dfen", "euch", "die", "Tod\u00b7ge\u00b7weih\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Und so wie heut zum letztenmal!", "tokens": ["Und", "so", "wie", "heut", "zum", "letz\u00b7ten\u00b7mal", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "KOKOM", "ADV", "APPRART", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Versammelt hielt sein Sklavenheer", "tokens": ["Ver\u00b7sam\u00b7melt", "hielt", "sein", "Skla\u00b7ven\u00b7heer"], "token_info": ["word", "word", "word", "word"], "pos": ["VVPP", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Thracier Spartacus am Meer,", "tokens": ["Der", "Thra\u00b7cier", "Spar\u00b7ta\u00b7cus", "am", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und auf zum rauchenden Vesuv", "tokens": ["Und", "auf", "zum", "rau\u00b7chen\u00b7den", "Ve\u00b7suv"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Erklang der wilde Freiheitsruf:", "tokens": ["Er\u00b7klang", "der", "wil\u00b7de", "Frei\u00b7heits\u00b7ruf", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Von nun an M\u00e4nner, nicht mehr Sklaven", "tokens": ["Von", "nun", "an", "M\u00e4n\u00b7ner", ",", "nicht", "mehr", "Skla\u00b7ven"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADV", "APPR", "NN", "$,", "PTKNEG", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Erheben wir das Schwert und strafen", "tokens": ["Er\u00b7he\u00b7ben", "wir", "das", "Schwert", "und", "stra\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "KON", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Der Unterdr\u00fccker \u00dcbermut.", "tokens": ["Der", "Un\u00b7ter\u00b7dr\u00fc\u00b7cker", "\u00dc\u00b7ber\u00b7mut", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Du Be