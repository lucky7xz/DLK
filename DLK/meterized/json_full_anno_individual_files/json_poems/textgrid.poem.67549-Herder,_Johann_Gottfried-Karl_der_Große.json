{"textgrid.poem.67549": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Karl der Gro\u00dfe", "genre": "verse", "period": "N.A.", "pub_year": 1770, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbwar er, deutsches Vaterland,", "tokens": ["\u00bb", "war", "er", ",", "deut\u00b7sches", "Va\u00b7ter\u00b7land", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "VAFIN", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "M\u00f6rder Dir oder Heiland?\u00ab", "tokens": ["M\u00f6r\u00b7der", "Dir", "o\u00b7der", "Hei\u00b7land", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "PPER", "KON", "NN", "$.", "$("], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Vieh und Heim, das war Dein Gut,", "tokens": ["Vieh", "und", "Heim", ",", "das", "war", "Dein", "Gut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "PDS", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und ein freies, edles Blut.", "tokens": ["Und", "ein", "frei\u00b7es", ",", "ed\u00b7les", "Blut", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Er vergo\u00df Dein freies Blut,", "tokens": ["Er", "ver\u00b7go\u00df", "Dein", "frei\u00b7es", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Nahm Dir Heim und Gut und Muth,", "tokens": ["Nahm", "Dir", "Heim", "und", "Gut", "und", "Muth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "KON", "ADJD", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und gab Dir \u2013 ha! Affentand,", "tokens": ["Und", "gab", "Dir", "\u2013", "ha", "!", "Af\u00b7fen\u00b7tand", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$(", "ITJ", "$.", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Den nicht Lai', nicht Pfaff verstand!", "tokens": ["Den", "nicht", "Lai'", ",", "nicht", "Pfaff", "ver\u00b7stand", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "NN", "$,", "PTKNEG", "NE", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Gab Dir Sitte! \u2013 Knechtes Muth,", "tokens": ["Gab", "Dir", "Sit\u00b7te", "!", "\u2013", "Knech\u00b7tes", "Muth", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "$.", "$(", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Demuth! tr\u00e4ges, schleichend Blut,", "tokens": ["De\u00b7muth", "!", "tr\u00e4\u00b7ges", ",", "schlei\u00b7chend", "Blut", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$.", "ADJA", "$,", "ADJD", "NN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Wie dem Wurm in Wintergruft,", "tokens": ["Wie", "dem", "Wurm", "in", "Win\u00b7ter\u00b7gruft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Eh ihn Lenz und Sonne ruft.", "tokens": ["Eh", "ihn", "Lenz", "und", "Son\u00b7ne", "ruft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "KON", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Gab Dir Sitt', und l\u00f6schte aus", "tokens": ["Gab", "Dir", "Sitt'", ",", "und", "l\u00f6schte", "aus"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "NN", "$,", "KON", "VVFIN", "PTKVZ"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Tugend, warf in Staub und Graus", "tokens": ["Tu\u00b7gend", ",", "warf", "in", "Staub", "und", "Graus"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Deine V\u00e4ter! Ahnenzeit!", "tokens": ["Dei\u00b7ne", "V\u00e4\u00b7ter", "!", "Ah\u00b7nen\u00b7zeit", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Lobgesang der Ewigkeit!", "tokens": ["Lob\u00b7ge\u00b7sang", "der", "E\u00b7wig\u00b7keit", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Deine Sprache! Sieggewinn", "tokens": ["Dei\u00b7ne", "Spra\u00b7che", "!", "Sieg\u00b7ge\u00b7winn"], "token_info": ["word", "word", "punct", "word"], "pos": ["PPOSAT", "NN", "$.", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "War und Kampf Dir Eins nur! Sinn", "tokens": ["War", "und", "Kampf", "Dir", "Eins", "nur", "!", "Sinn"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["VAFIN", "KON", "NN", "PPER", "NN", "ADV", "$.", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und Gef\u00fchl! Kraft und Verstand!", "tokens": ["Und", "Ge\u00b7f\u00fchl", "!", "Kraft", "und", "Ver\u00b7stand", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$.", "NN", "KON", "NN", "$."], "meter": "+-++--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Wort und Herz und Geist und Hand", "tokens": ["Wort", "und", "Herz", "und", "Geist", "und", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "KON", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Dir nur Eins! Der V\u00e4ter Schaar,", "tokens": ["Dir", "nur", "Eins", "!", "Der", "V\u00e4\u00b7ter", "Schaar", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "NN", "$.", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ahnen, Adel, G\u00f6tter! War", "tokens": ["Ah\u00b7nen", ",", "A\u00b7del", ",", "G\u00f6t\u00b7ter", "!", "War"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["NN", "$,", "NN", "$,", "NN", "$.", "VAFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mann und Hermann Dir nicht Gott,", "tokens": ["Mann", "und", "Her\u00b7mann", "Dir", "nicht", "Gott", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NE", "PPER", "PTKNEG", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Vorbild, Anbild in der Noth?", "tokens": ["Vor\u00b7bild", ",", "An\u00b7bild", "in", "der", "Noth", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Mann ein Mann! Die edle Braut,", "tokens": ["Mann", "ein", "Mann", "!", "Die", "ed\u00b7le", "Braut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$.", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Kind und Heimath ihr vertraut,", "tokens": ["Kind", "und", "Hei\u00b7math", "ihr", "ver\u00b7traut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "PPER", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Tochter Freia's! Ha, sie trat,", "tokens": ["Toch\u00b7ter", "Frei\u00b7a's", "!", "Ha", ",", "sie", "trat", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NE", "$.", "ITJ", "$,", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Eine Mana, in den Rath", "tokens": ["Ei\u00b7ne", "Ma\u00b7na", ",", "in", "den", "Rath"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NE", "$,", "APPR", "ART", "NN"], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.8": {"line.1": {"text": "Der Erk\u00fcrer! Ha, da sang", "tokens": ["Der", "Er\u00b7k\u00fc\u00b7rer", "!", "Ha", ",", "da", "sang"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$.", "ITJ", "$,", "ADV", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Barde seinen Lobgesang", "tokens": ["Bar\u00b7de", "sei\u00b7nen", "Lob\u00b7ge\u00b7sang"], "token_info": ["word", "word", "word"], "pos": ["NN", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Lauter! Schwester Mana lacht", "tokens": ["Lau\u00b7ter", "!", "Schwes\u00b7ter", "Ma\u00b7na", "lacht"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$.", "NN", "NE", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Lieblicher in ihrer Pracht", "tokens": ["Lieb\u00b7li\u00b7cher", "in", "ih\u00b7rer", "Pracht"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Auf der Schwester Angesicht", "tokens": ["Auf", "der", "Schwes\u00b7ter", "An\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Nieder! sch\u00f6n und keusch! das Licht", "tokens": ["Nie\u00b7der", "!", "sch\u00f6n", "und", "keusch", "!", "das", "Licht"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "$.", "ADJD", "KON", "ADJD", "$.", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Geht dem Barden auf! \u2013 Ach, ab", "tokens": ["Geht", "dem", "Bar\u00b7den", "auf", "!", "\u2013", "Ach", ",", "ab"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "punct", "word"], "pos": ["VVFIN", "ART", "NN", "PTKVZ", "$.", "$(", "ITJ", "$,", "PTKVZ"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Bard' und Braut und Mann, ins Grab", "tokens": ["Bard'", "und", "Braut", "und", "Mann", ",", "ins", "Grab"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "KON", "NN", "KON", "NN", "$,", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Sind sie All' ersunken. \u2013 Noch", "tokens": ["Sind", "sie", "All'", "er\u00b7sun\u00b7ken", ".", "\u2013", "Noch"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word"], "pos": ["VAFIN", "PPER", "PIS", "VVINF", "$.", "$(", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Will der M\u00f6rder Sterbhauch doch", "tokens": ["Will", "der", "M\u00f6r\u00b7der", "Sterb\u00b7hauch", "doch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "ART", "NN", "NN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Retten! \u2013 Nicht! \u2013 der Bardeklang", "tokens": ["Ret\u00b7ten", "!", "\u2013", "Nicht", "!", "\u2013", "der", "Bar\u00b7de\u00b7klang"], "token_info": ["word", "punct", "punct", "word", "punct", "punct", "word", "word"], "pos": ["NN", "$.", "$(", "PTKNEG", "$.", "$(", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wandelt! wird ihm Fluchgesang!", "tokens": ["Wan\u00b7delt", "!", "wird", "ihm", "Fluch\u00b7ge\u00b7sang", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "VAFIN", "PPER", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Fluch ihm! \u2013 M\u00f6rder war nicht werth,", "tokens": ["Fluch", "ihm", "!", "\u2013", "M\u00f6r\u00b7der", "war", "nicht", "werth", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$.", "$(", "NN", "VAFIN", "PTKNEG", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sie zu retten! M\u00f6rder f\u00e4hrt", "tokens": ["Sie", "zu", "ret\u00b7ten", "!", "M\u00f6r\u00b7der", "f\u00e4hrt"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "PTKZU", "VVINF", "$.", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Selbst hinnach, und all sein Reich", "tokens": ["Selbst", "hin\u00b7nach", ",", "und", "all", "sein", "Reich"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$,", "KON", "PIAT", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wird, wie Deutschland, W\u00fcste gleich!", "tokens": ["Wird", ",", "wie", "Deutschland", ",", "W\u00fcs\u00b7te", "gleich", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "$,", "PWAV", "NE", "$,", "VVFIN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Fromm und kahl und feist und Kind", "tokens": ["Fromm", "und", "kahl", "und", "feist", "und", "Kind"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "KON", "ADJD", "KON", "ADJD", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Waren seine S\u00f6hne! sind", "tokens": ["Wa\u00b7ren", "sei\u00b7ne", "S\u00f6h\u00b7ne", "!", "sind"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["VAFIN", "PPOSAT", "NN", "$.", "VAFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Alle w\u00fcrdelos! ihr Thron", "tokens": ["Al\u00b7le", "w\u00fcr\u00b7de\u00b7los", "!", "ihr", "Thron"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PIAT", "ADJD", "$.", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Taumelt! und dort ahnt sie schon", "tokens": ["Tau\u00b7melt", "!", "und", "dort", "ahnt", "sie", "schon"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$.", "KON", "ADV", "VVFIN", "PPER", "ADV"], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.13": {"line.1": {"text": "Rache! Dort auf Meeres Sand", "tokens": ["Ra\u00b7che", "!", "Dort", "auf", "Mee\u00b7res", "Sand"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$.", "ADV", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Nordens Deutsche! Vaterland", "tokens": ["Nor\u00b7dens", "Deut\u00b7sche", "!", "Va\u00b7ter\u00b7land"], "token_info": ["word", "word", "punct", "word"], "pos": ["NE", "NE", "$.", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Athmet Rache! V\u00e4terblut,", "tokens": ["Ath\u00b7met", "Ra\u00b7che", "!", "V\u00e4\u00b7ter\u00b7blut", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["NE", "NN", "$.", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ha, die schwarze gro\u00dfe Fluth", "tokens": ["Ha", ",", "die", "schwar\u00b7ze", "gro\u00b7\u00dfe", "Fluth"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "ART", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "Brauset! seufzet! \u2013 Gottes Buch", "tokens": ["Brau\u00b7set", "!", "seuf\u00b7zet", "!", "\u2013", "Got\u00b7tes", "Buch"], "token_info": ["word", "punct", "word", "punct", "punct", "word", "word"], "pos": ["VVFIN", "$.", "VVFIN", "$.", "$(", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist erf\u00fcllt! \u2013 der V\u00f6lkerfluch", "tokens": ["Ist", "er\u00b7f\u00fcllt", "!", "\u2013", "der", "V\u00f6l\u00b7ker\u00b7fluch"], "token_info": ["word", "word", "punct", "punct", "word", "word"], "pos": ["VAFIN", "VVPP", "$.", "$(", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Trifft! \u2013 da schreiet Deutschlands Blut,", "tokens": ["Trifft", "!", "\u2013", "da", "schrei\u00b7et", "Deutschlands", "Blut", ","], "token_info": ["word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "$(", "ADV", "VVFIN", "NE", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Und sie tanzen hin die Fluth", "tokens": ["Und", "sie", "tan\u00b7zen", "hin", "die", "Fluth"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ADV", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.15": {"line.1": {"text": "Ihrer Rach'. Und unterthan", "tokens": ["Ih\u00b7rer", "Rach'", ".", "Und", "un\u00b7ter\u00b7than"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "$.", "KON", "ADJD"], "m