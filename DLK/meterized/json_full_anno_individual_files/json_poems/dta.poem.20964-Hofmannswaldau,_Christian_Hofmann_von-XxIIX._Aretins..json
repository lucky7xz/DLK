{"dta.poem.20964": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "XxIIX.  \n Aretins.", "genre": "Lyrik; Prosa; Drama", "period": "N.A.", "pub_year": "1679", "urn": "urn:nbn:de:kobv:b4-20289-1", "language": ["de:0.99"], "booktitle": "Hofmann von Hofmannswaldau, Christian: Deutsche Ubersetzungen und Gedichte. Breslau, 1679."}, "poem": {"stanza.1": {"line.1": {"text": "Hier liegt ein geiler Mann/ so der verkehrten", "tokens": ["Hier", "liegt", "ein", "gei\u00b7ler", "Mann", "/", "so", "der", "ver\u00b7kehr\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$(", "ADV", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Welt", "tokens": ["Welt"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Den Griff der Schlipffrigkeit hat k\u00fcnstlich vorge-", "tokens": ["Den", "Griff", "der", "Schlipf\u00b7frig\u00b7keit", "hat", "k\u00fcnst\u00b7lich", "vor\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "VAFIN", "ADJD", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "stellt.", "tokens": ["stellt", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Die Venus da\u00df ihr Sohn den Bogen besser deh-", "tokens": ["Die", "Ve\u00b7nus", "da\u00df", "ihr", "Sohn", "den", "Bo\u00b7gen", "bes\u00b7ser", "deh"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KOUS", "PPOSAT", "NN", "ART", "NN", "ADJD", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "ne/", "tokens": ["ne", "/"], "token_info": ["word", "punct"], "pos": ["NE", "$("], "meter": "-", "measure": "single.down"}, "line.7": {"text": "Nahm sein verbultes Hertz/ und gab es ihm zur", "tokens": ["Nahm", "sein", "ver\u00b7bul\u00b7tes", "Hertz", "/", "und", "gab", "es", "ihm", "zur"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "$(", "KON", "VVFIN", "PPER", "PPER", "APPRART"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Sehne.", "tokens": ["Seh\u00b7ne", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}}}}}