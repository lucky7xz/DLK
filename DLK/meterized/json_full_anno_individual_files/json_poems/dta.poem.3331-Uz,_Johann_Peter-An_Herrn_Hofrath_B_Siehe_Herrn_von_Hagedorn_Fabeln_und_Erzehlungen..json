{"dta.poem.3331": {"metadata": {"author": {"name": "Uz, Johann Peter", "birth": "N.A.", "death": "N.A."}, "title": "An Herrn Hofrath B   Siehe Herrn von Hagedorn Fabeln und Erzehlungen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1755", "urn": "urn:nbn:de:kobv:b4-20614-6", "language": ["de:0.99"], "booktitle": "Uz, Johann Peter: Lyrische und andere Gedichte. 2. Aufl. Ansbach, 1755."}, "poem": {"stanza.1": {"line.1": {"text": "Ich kann wie auf dem Land und als ein Sch\u00e4fer leben:", "tokens": ["Ich", "kann", "wie", "auf", "dem", "Land", "und", "als", "ein", "Sch\u00e4\u00b7fer", "le\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "KOKOM", "APPR", "ART", "NN", "KON", "KOUS", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als Sch\u00e4fer? ich betr\u00fcge mich!", "tokens": ["Als", "Sch\u00e4\u00b7fer", "?", "ich", "be\u00b7tr\u00fc\u00b7ge", "mich", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "$.", "PPER", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wer wird mir Sch\u00e4ferinnen geben?", "tokens": ["Wer", "wird", "mir", "Sch\u00e4\u00b7fe\u00b7rin\u00b7nen", "ge\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und ohne Sch\u00e4ferinn sind Sch\u00e4fer j\u00e4mmerlich.", "tokens": ["Und", "oh\u00b7ne", "Sch\u00e4\u00b7fe\u00b7rinn", "sind", "Sch\u00e4\u00b7fer", "j\u00e4m\u00b7mer\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VAFIN", "NE", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Zwar M\u00e4dchen sind hier, wie G\u00f6ttinnen,", "tokens": ["Zwar", "M\u00e4d\u00b7chen", "sind", "hier", ",", "wie", "G\u00f6t\u00b7tin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "NN", "VAFIN", "ADV", "$,", "PWAV", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "So artig, als die Sch\u00e4ferinnen;", "tokens": ["So", "ar\u00b7tig", ",", "als", "die", "Sch\u00e4\u00b7fe\u00b7rin\u00b7nen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "KOUS", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Doch nicht so fromm, wie sie und ich.", "tokens": ["Doch", "nicht", "so", "fromm", ",", "wie", "sie", "und", "ich", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ADV", "ADJD", "$,", "PWAV", "PPER", "KON", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Sie sind, wie \u00fcberall die Quelle s\u00fcsser Schmerzen,", "tokens": ["Sie", "sind", ",", "wie", "\u00fc\u00b7be\u00b7rall", "die", "Quel\u00b7le", "s\u00fcs\u00b7ser", "Schmer\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "PWAV", "ADV", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Voll Unschuld auf der Stirn, voll Schelmerey im Herzen.", "tokens": ["Voll", "Un\u00b7schuld", "auf", "der", "Stirn", ",", "voll", "Schel\u00b7me\u00b7rey", "im", "Her\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "APPR", "ART", "NN", "$,", "ADJD", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "So schlimm die\u00df V\u00f6lkchen ist, wer liebt es, leider! nicht?", "tokens": ["So", "schlimm", "die\u00df", "V\u00f6lk\u00b7chen", "ist", ",", "wer", "liebt", "es", ",", "lei\u00b7der", "!", "nicht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "ADJD", "PDS", "NN", "VAFIN", "$,", "PWS", "VVFIN", "PPER", "$,", "ADV", "$.", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Ein sch\u00f6ner Blick war stets dem Weisen \u00fcberlegen:", "tokens": ["Ein", "sch\u00f6\u00b7ner", "Blick", "war", "stets", "dem", "Wei\u00b7sen", "\u00fc\u00b7berl\u00b7e\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Ein Blick entrunzelt sein Gesicht:", "tokens": ["Ein", "Blick", "ent\u00b7run\u00b7zelt", "sein", "Ge\u00b7sicht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Der Fromme s\u00fcndigt ihrentwegen,", "tokens": ["Der", "From\u00b7me", "s\u00fcn\u00b7digt", "ih\u00b7rent\u00b7we\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Schielt \u00fcbern Cubach weg und spricht:", "tokens": ["Schielt", "\u00fc\u00b7bern", "Cu\u00b7bach", "weg", "und", "spricht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "PTKVZ", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Ach! w\u00e4r kein M\u00e4dchen auf der Erden,", "tokens": ["Ach", "!", "w\u00e4r", "kein", "M\u00e4d\u00b7chen", "auf", "der", "Er\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VAFIN", "PIAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Wir w\u00fcrden alle seelig werden!", "tokens": ["Wir", "w\u00fcr\u00b7den", "al\u00b7le", "see\u00b7lig", "wer\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIS", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}