{"dta.poem.21406": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Caput XXIII.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-30602-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Rheinwein stimmt mich immer weich,", "tokens": ["Der", "Rhein\u00b7wein", "stimmt", "mich", "im\u00b7mer", "weich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und l\u00f6st jedwedes Zerw\u00fcrfni\u00df", "tokens": ["Und", "l\u00f6st", "jed\u00b7we\u00b7des", "Zer\u00b7w\u00fcrf\u00b7ni\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PIAT", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "In meiner Brust, entz\u00fcndet darinn", "tokens": ["In", "mei\u00b7ner", "Brust", ",", "ent\u00b7z\u00fcn\u00b7det", "da\u00b7rinn"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "VVFIN", "PAV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Menschenliebe Bed\u00fcrfni\u00df.", "tokens": ["Der", "Men\u00b7schen\u00b7lie\u00b7be", "Be\u00b7d\u00fcrf\u00b7ni\u00df", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Es treibt mich aus dem Zimmer hinaus,", "tokens": ["Es", "treibt", "mich", "aus", "dem", "Zim\u00b7mer", "hin\u00b7aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "NN", "APZR", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Ich mu\u00df in den Stra\u00dfen schlendern;", "tokens": ["Ich", "mu\u00df", "in", "den", "Stra\u00b7\u00dfen", "schlen\u00b7dern", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Die Seele sucht eine Seele und sp\u00e4h\u2019t", "tokens": ["Die", "See\u00b7le", "sucht", "ei\u00b7ne", "See\u00b7le", "und", "sp\u00e4h't"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "KON", "VVFIN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Nach z\u00e4rtlich wei\u00dfen Gew\u00e4ndern.", "tokens": ["Nach", "z\u00e4rt\u00b7lich", "wei\u00b7\u00dfen", "Ge\u00b7w\u00e4n\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "In solchen Momenten zerflie\u00dfe ich fast", "tokens": ["In", "sol\u00b7chen", "Mo\u00b7men\u00b7ten", "zer\u00b7flie\u00b7\u00dfe", "ich", "fast"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "PPER", "ADV"], "meter": "-+-+---+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Vor Wehmuth und vor Sehnen;", "tokens": ["Vor", "Weh\u00b7muth", "und", "vor", "Seh\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Katzen scheinen mir alle grau,", "tokens": ["Die", "Kat\u00b7zen", "schei\u00b7nen", "mir", "al\u00b7le", "grau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PIS", "ADJD", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die Weiber alle Helenen. \u2013 \u2013 \u2013", "tokens": ["Die", "Wei\u00b7ber", "al\u00b7le", "He\u00b7le\u00b7nen", ".", "\u2013", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct", "punct"], "pos": ["ART", "NN", "PIAT", "NN", "$.", "$(", "$(", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Und als ich auf die Drehbahn kam,", "tokens": ["Und", "als", "ich", "auf", "die", "Dreh\u00b7bahn", "kam", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da sah ich im Mondenschimmer", "tokens": ["Da", "sah", "ich", "im", "Mon\u00b7den\u00b7schim\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ein hehres Weib, ein wunderbar", "tokens": ["Ein", "heh\u00b7res", "Weib", ",", "ein", "wun\u00b7der\u00b7bar"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hochbusiges Frauenzimmer.", "tokens": ["Hoch\u00b7bu\u00b7si\u00b7ges", "Frau\u00b7en\u00b7zim\u00b7mer", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}}}}