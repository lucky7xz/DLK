{"textgrid.poem.36670": {"metadata": {"author": {"name": "Waiblinger, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Sechstes Lied", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dein gedenk' ich, Nazarene,", "tokens": ["Dein", "ge\u00b7denk'", "ich", ",", "Na\u00b7za\u00b7re\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "VVFIN", "PPER", "$,", "NE", "$,"], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.2": {"text": "Wenn das Schiff mich nach dem Eiland", "tokens": ["Wenn", "das", "Schiff", "mich", "nach", "dem", "Ei\u00b7land"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "PRF", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Theokrits, auf griech'sche Erde,", "tokens": ["Theo\u00b7krits", ",", "auf", "griech'\u00b7sche", "Er\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "---+-+-", "measure": "unknown.measure.di"}, "line.4": {"text": "Nach der Heimat des Ulysses,", "tokens": ["Nach", "der", "Hei\u00b7mat", "des", "U\u00b7lys\u00b7ses", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Ueber's weite Meer entf\u00fchrt.", "tokens": ["Ue\u00b7ber's", "wei\u00b7te", "Meer", "ent\u00b7f\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Aber unsre W\u00fcnsche schwinden", "tokens": ["A\u00b7ber", "uns\u00b7re", "W\u00fcn\u00b7sche", "schwin\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Oft wie Rauch dahin; der Fr\u00fchling", "tokens": ["Oft", "wie", "Rauch", "da\u00b7hin", ";", "der", "Fr\u00fch\u00b7ling"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "KOKOM", "NN", "PAV", "$.", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Er erfreut, und wir genie\u00dfen", "tokens": ["Er", "er\u00b7freut", ",", "und", "wir", "ge\u00b7nie\u00b7\u00dfen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KON", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wohl den Balsamduft der Bl\u00fcten,", "tokens": ["Wohl", "den", "Bal\u00b7sam\u00b7duft", "der", "Bl\u00fc\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Doch die reifen Fr\u00fcchte nicht.", "tokens": ["Doch", "die", "rei\u00b7fen", "Fr\u00fcch\u00b7te", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Gl\u00fcht uns auch die volle Traube", "tokens": ["Gl\u00fcht", "uns", "auch", "die", "vol\u00b7le", "Trau\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schon entgegen, lechzt der Gaumen", "tokens": ["Schon", "ent\u00b7ge\u00b7gen", ",", "lechzt", "der", "Gau\u00b7men"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "PTKVZ", "$,", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Nach dem Trunke, so entf\u00fchret", "tokens": ["Nach", "dem", "Trun\u00b7ke", ",", "so", "ent\u00b7f\u00fch\u00b7ret"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "ADV", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Uns der Gott im Sinnenrausche", "tokens": ["Uns", "der", "Gott", "im", "Sin\u00b7nen\u00b7rau\u00b7sche"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "ART", "NN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Den gebornen s\u00fc\u00dfen Wein.", "tokens": ["Den", "ge\u00b7bor\u00b7nen", "s\u00fc\u00b7\u00dfen", "Wein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Nie mehr soll ich denn die Felsen,", "tokens": ["Nie", "mehr", "soll", "ich", "denn", "die", "Fel\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VMFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nimmermehr die Feigenh\u00fcgel,", "tokens": ["Nim\u00b7mer\u00b7mehr", "die", "Fei\u00b7gen\u00b7h\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Luft'ge holde Schattenwege", "tokens": ["Luft'\u00b7ge", "hol\u00b7de", "Schat\u00b7ten\u00b7we\u00b7ge"], "token_info": ["word", "word", "word"], "pos": ["NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Der Kastanienhaine, nimmer", "tokens": ["Der", "Kas\u00b7ta\u00b7ni\u00b7en\u00b7hai\u00b7ne", ",", "nim\u00b7mer"], "token_info": ["word", "word", "punct", "word"], "pos": ["ART", "NN", "$,", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Mein Olevano mehr sehn?", "tokens": ["Mein", "O\u00b7le\u00b7va\u00b7no", "mehr", "sehn", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NE", "ADV", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Nimmermehr der Serpentara", "tokens": ["Nim\u00b7mer\u00b7mehr", "der", "Ser\u00b7pen\u00b7ta\u00b7ra"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ART", "NE"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Rauhe wilde Wand besteigen,", "tokens": ["Rau\u00b7he", "wil\u00b7de", "Wand", "be\u00b7stei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Nimmermehr die sch\u00f6nen Berge", "tokens": ["Nim\u00b7mer\u00b7mehr", "die", "sch\u00f6\u00b7nen", "Ber\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Tief im Lichtblau eines sanften", "tokens": ["Tief", "im", "Licht\u00b7blau", "ei\u00b7nes", "sanf\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPRART", "NN", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "M\u00e4dchenauges l\u00e4cheln sehn?", "tokens": ["M\u00e4d\u00b7chen\u00b7au\u00b7ges", "l\u00e4\u00b7cheln", "sehn", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Weil sie meinem Leben drohen,", "tokens": ["Weil", "sie", "mei\u00b7nem", "Le\u00b7ben", "dro\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und mich hassen, wie den Pluto,", "tokens": ["Und", "mich", "has\u00b7sen", ",", "wie", "den", "Plu\u00b7to", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVINF", "$,", "PWAV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Der dem blumenvollen Enna", "tokens": ["Der", "dem", "blu\u00b7men\u00b7vol\u00b7len", "En\u00b7na"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ART", "ADJA", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Mit verwegner Kraft die sch\u00f6nste", "tokens": ["Mit", "ver\u00b7weg\u00b7ner", "Kraft", "die", "sch\u00f6ns\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Sch\u00e4ferin hinweggeraubt?", "tokens": ["Sch\u00e4\u00b7fe\u00b7rin", "hin\u00b7weg\u00b7ge\u00b7raubt", "?"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Sei's denn, liebe Nazarene,", "tokens": ["Sei's", "denn", ",", "lie\u00b7be", "Na\u00b7za\u00b7re\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "KON", "$,", "VVFIN", "NE", "$,"], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.2": {"text": "Ob wir auch uns wiedersehen,", "tokens": ["Ob", "wir", "auch", "uns", "wie\u00b7der\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ob du mit dem Nonnenschleier", "tokens": ["Ob", "du", "mit", "dem", "Non\u00b7nen\u00b7schlei\u00b7er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Auch vertauschest deine farb'ge", "tokens": ["Auch", "ver\u00b7tau\u00b7schest", "dei\u00b7ne", "fa\u00b7rb'\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Feenhafte Zaubertracht,", "tokens": ["Fe\u00b7en\u00b7haf\u00b7te", "Zau\u00b7bert\u00b7racht", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Eine Schuld doch mu\u00df ich s\u00fchnen,", "tokens": ["Ei\u00b7ne", "Schuld", "doch", "mu\u00df", "ich", "s\u00fch\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Eine andere begehend,", "tokens": ["Ei\u00b7ne", "an\u00b7de\u00b7re", "be\u00b7ge\u00b7hend", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Einer meine Treue brechend,", "tokens": ["Ei\u00b7ner", "mei\u00b7ne", "Treu\u00b7e", "bre\u00b7chend", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Einer andern sie bewahrend,", "tokens": ["Ei\u00b7ner", "an\u00b7dern", "sie", "be\u00b7wah\u00b7rend", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Beiden meine Reue weihn.", "tokens": ["Bei\u00b7den", "mei\u00b7ne", "Reu\u00b7e", "weihn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Zwar die Sch\u00f6nste bleibst du immer", "tokens": ["Zwar", "die", "Sch\u00f6ns\u00b7te", "bleibst", "du", "im\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "VVFIN", "PPER", "ADV"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Deines reizenden Geschlechtes,", "tokens": ["Dei\u00b7nes", "rei\u00b7zen\u00b7den", "Ge\u00b7schlech\u00b7tes", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Zwar vollkommner malte Sanzio", "tokens": ["Zwar", "voll\u00b7komm\u00b7ner", "mal\u00b7te", "San\u00b7zio"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VVFIN", "NE"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nie ein Weib, und nie Correggio", "tokens": ["Nie", "ein", "Weib", ",", "und", "nie", "Cor\u00b7reg\u00b7gio"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "$,", "KON", "ADV", "NE"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Einer Grazie Wunderbild.", "tokens": ["Ei\u00b7ner", "Gra\u00b7zie", "Wun\u00b7der\u00b7bild", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Doch es gibt ein Herz voll Liebe,", "tokens": ["Doch", "es", "gibt", "ein", "Herz", "voll", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "ADJD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Voll Geduld und Treu und Langmuth,", "tokens": ["Voll", "Ge\u00b7duld", "und", "Treu", "und", "Lang\u00b7muth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "KON", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie's in seiner geist'gen Sch\u00f6ne,", "tokens": ["Wie's", "in", "sei\u00b7ner", "geist'\u00b7gen", "Sch\u00f6\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "So lebendig, leidend, f\u00fchlend,", "tokens": ["So", "le\u00b7ben\u00b7dig", ",", "lei\u00b7dend", ",", "f\u00fch\u00b7lend", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADJD", "$,", "VVPP", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Ariosto nicht besang.", "tokens": ["A\u00b7rios\u00b7to", "nicht", "be\u00b7sang", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Alles schuld' ich ihm, vor allen", "tokens": ["Al\u00b7les", "schuld'", "ich", "ihm", ",", "vor", "al\u00b7len"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PIS", "VVFIN", "PPER", "PPER", "$,", "APPR", "PIAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dieses Herz! Ich kann's nicht theilen,", "tokens": ["Die\u00b7ses", "Herz", "!", "Ich", "kann's", "nicht", "thei\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "$.", "PPER", "VMFIN", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und damit nicht seine Leiden", "tokens": ["Und", "da\u00b7mit", "nicht", "sei\u00b7ne", "Lei\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PAV", "PTKNEG", "PPOSAT", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Ueber unsern Frevel kommen,", "tokens": ["Ue\u00b7ber", "un\u00b7sern", "Fre\u00b7vel", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Sag' ich dir mein Lebewohl!", "tokens": ["Sag'", "ich", "dir", "mein", "Le\u00b7be\u00b7wohl", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Dein gedenk' ich, Nazarene,", "tokens": ["Dein", "ge\u00b7denk'", "ich", ",", "Na\u00b7za\u00b7re\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "VVFIN", "PPER", "$,", "NE", "$,"], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.2": {"text": "Wenn das Schiff mich nach dem Eiland", "tokens": ["Wenn", "das", "Schiff", "mich", "nach", "dem", "Ei\u00b7land"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "PRF", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Theokrits, auf griech'sche Erde,", "tokens": ["Theo\u00b7krits", ",", "auf", "griech'\u00b7sche", "Er\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "---+-+-", "measure": "unknown.measure.di"}, "line.4": {"text": "Nach der Heimat des Ulysses,", "tokens": ["Nach", "der", "Hei\u00b7mat", "des", "U\u00b7lys\u00b7ses", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Ueber's weite Meer entf\u00fchrt.", "tokens": ["Ue\u00b7ber's", "wei\u00b7te", "Meer", "ent\u00b7f\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Aber unsre W\u00fcnsche schwinden", "tokens": ["A\u00b7ber", "uns\u00b7re", "W\u00fcn\u00b7sche", "schwin\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Oft wie Rauch dahin; der Fr\u00fchling", "tokens": ["Oft", "wie", "Rauch", "da\u00b7hin", ";", "der", "Fr\u00fch\u00b7ling"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "KOKOM", "NN", "PAV", "$.", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Er erfreut, und wir genie\u00dfen", "tokens": ["Er", "er\u00b7freut", ",", "und", "wir", "ge\u00b7nie\u00b7\u00dfen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KON", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wohl den Balsamduft der Bl\u00fcten,", "tokens": ["Wohl", "den", "Bal\u00b7sam\u00b7duft", "der", "Bl\u00fc\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Doch die reifen Fr\u00fcchte nicht.", "tokens": ["Doch", "die", "rei\u00b7fen", "Fr\u00fcch\u00b7te", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "Gl\u00fcht uns auch die volle Traube", "tokens": ["Gl\u00fcht", "uns", "auch", "die", "vol\u00b7le", "Trau\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schon entgegen, lechzt der Gaumen", "tokens": ["Schon", "ent\u00b7ge\u00b7gen", ",", "lechzt", "der", "Gau\u00b7men"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "PTKVZ", "$,", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Nach dem Trunke, so entf\u00fchret", "tokens": ["Nach", "dem", "Trun\u00b7ke", ",", "so", "ent\u00b7f\u00fch\u00b7ret"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "ADV", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Uns der Gott im Sinnenrausche", "tokens": ["Uns", "der", "Gott", "im", "Sin\u00b7nen\u00b7rau\u00b7sche"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "ART", "NN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Den gebornen s\u00fc\u00dfen Wein.", "tokens": ["Den", "ge\u00b7bor\u00b7nen", "s\u00fc\u00b7\u00dfen", "Wein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.15": {"line.1": {"text": "Nie mehr soll ich denn die Felsen,", "tokens": ["Nie", "mehr", "soll", "ich", "denn", "die", "Fel\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VMFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nimmermehr die Feigenh\u00fcgel,", "tokens": ["Nim\u00b7mer\u00b7mehr", "die", "Fei\u00b7gen\u00b7h\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Luft'ge holde Schattenwege", "tokens": ["Luft'\u00b7ge", "hol\u00b7de", "Schat\u00b7ten\u00b7we\u00b7ge"], "token_info": ["word", "word", "word"], "pos": ["NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Der Kastanienhaine, nimmer", "tokens": ["Der", "Kas\u00b7ta\u00b7ni\u00b7en\u00b7hai\u00b7ne", ",", "nim\u00b7mer"], "token_info": ["word", "word", "punct", "word"], "pos": ["ART", "NN", "$,", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Mein Olevano mehr sehn?", "tokens": ["Mein", "O\u00b7le\u00b7va\u00b7no", "mehr", "sehn", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NE", "ADV", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.16": {"line.1": {"text": "Nimmermehr der Serpentara", "tokens": ["Nim\u00b7mer\u00b7mehr", "der", "Ser\u00b7pen\u00b7ta\u00b7ra"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ART", "NE"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Rauhe wilde Wand besteigen,", "tokens": ["Rau\u00b7he", "wil\u00b7de", "Wand", "be\u00b7stei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Nimmermehr die sch\u00f6nen Berge", "tokens": ["Nim\u00b7mer\u00b7mehr", "die", "sch\u00f6\u00b7nen", "Ber\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Tief im Lichtblau eines sanften", "tokens": ["Tief", "im", "Licht\u00b7blau", "ei\u00b7nes", "sanf\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPRART", "NN", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "M\u00e4dchenauges l\u00e4cheln sehn?", "tokens": ["M\u00e4d\u00b7chen\u00b7au\u00b7ges", "l\u00e4\u00b7cheln", "sehn", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.17": {"line.1": {"text": "Weil sie meinem Leben drohen,", "tokens": ["Weil", "sie", "mei\u00b7nem", "Le\u00b7ben", "dro\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und mich hassen, wie den Pluto,", "tokens": ["Und", "mich", "has\u00b7sen", ",", "wie", "den", "Plu\u00b7to", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVINF", "$,", "PWAV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Der dem blumenvollen Enna", "tokens": ["Der", "dem", "blu\u00b7men\u00b7vol\u00b7len", "En\u00b7na"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ART", "ADJA", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Mit verwegner Kraft die sch\u00f6nste", "tokens": ["Mit", "ver\u00b7weg\u00b7ner", "Kraft", "die", "sch\u00f6ns\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Sch\u00e4ferin hinweggeraubt?", "tokens": ["Sch\u00e4\u00b7fe\u00b7rin", "hin\u00b7weg\u00b7ge\u00b7raubt", "?"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.18": {"line.1": {"text": "Sei's denn, liebe Nazarene,", "tokens": ["Sei's", "denn", ",", "lie\u00b7be", "Na\u00b7za\u00b7re\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "KON", "$,", "VVFIN", "NE", "$,"], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.2": {"text": "Ob wir auch uns wiedersehen,", "tokens": ["Ob", "wir", "auch", "uns", "wie\u00b7der\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measu