{"dta.poem.20979": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "XlIII.  \n Marien de Medices.", "genre": "Lyrik; Prosa; Drama", "period": "N.A.", "pub_year": "1679", "urn": "urn:nbn:de:kobv:b4-20289-1", "language": ["de:0.99"], "booktitle": "Hofmann von Hofmannswaldau, Christian: Deutsche Ubersetzungen und Gedichte. Breslau, 1679."}, "poem": {"stanza.1": {"line.1": {"text": "Die Mutter-Stadt Florentz lie\u00df mich nicht ohne", "tokens": ["Die", "Mut\u00b7ter\u00b7Stadt", "Flo\u00b7rentz", "lie\u00df", "mich", "nicht", "oh\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NE", "VVFIN", "PPER", "PTKNEG", "APPR"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Schmertzen/ (tzen.", "tokens": ["Schmert\u00b7zen", "/", "(", "tzen", "."], "token_info": ["word", "punct", "punct", "word", "punct"], "pos": ["NN", "$(", "$(", "VVINF", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Paris empfing mich zwar/ doch nicht mit treuen Her-", "tokens": ["Pa\u00b7ris", "emp\u00b7fing", "mich", "zwar", "/", "doch", "nicht", "mit", "treu\u00b7en", "Her"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "ADV", "$(", "ADV", "PTKNEG", "APPR", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Den ich zu gro\u00df gemacht/ stie\u00df mich in strenge Flucht/", "tokens": ["Den", "ich", "zu", "gro\u00df", "ge\u00b7macht", "/", "stie\u00df", "mich", "in", "stren\u00b7ge", "Flucht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "PTKA", "ADJD", "VVPP", "$(", "VVFIN", "PRF", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Zu C\u00f6lln hat mich der Tod nicht ungew\u00fcntscht ge-", "tokens": ["Zu", "C\u00f6lln", "hat", "mich", "der", "Tod", "nicht", "un\u00b7ge\u00b7w\u00fcnt\u00b7scht", "ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "VAFIN", "PPER", "ART", "NN", "PTKNEG", "ADJD", "TRUNC"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "sucht.", "tokens": ["sucht", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-", "measure": "single.down"}}}}}