{"dta.poem.21868": {"metadata": {"author": {"name": "Stieler, Kaspar von", "birth": "N.A.", "death": "N.A."}, "title": "XxIV.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1660", "urn": "urn:nbn:de:kobv:b4-20586-2", "language": ["de:0.99"], "booktitle": "Filidor der Dorfferer [i. e. Stieler, Kaspar von]: Die Geharnschte Venus. Hamburg, 1660."}, "poem": {"stanza.1": {"line.1": {"text": "Das Ey\u00df zerbricht. Die Sch\u00f6nheit l\u00e4st sich", "tokens": ["Das", "Ey\u00df", "zer\u00b7bricht", ".", "Die", "Sch\u00f6n\u00b7heit", "l\u00e4st", "sich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$.", "ART", "NN", "VVFIN", "PRF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Amor fleucht aus Tetis Schoosse her.", "tokens": ["Der", "A\u00b7mor", "fleucht", "aus", "Te\u00b7tis", "Schoos\u00b7se", "her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "APPR", "NE", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mein\u2019 erste Funk\u2019 entz\u00fcndt sich au\u00df dem", "tokens": ["Mein'", "ers\u00b7te", "Funk'", "ent\u00b7z\u00fcndt", "sich", "au\u00df", "dem"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "PRF", "APPR", "ART"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ists m\u00fcglich/ auch im Wasser gl\u00fcend stehen?", "tokens": ["Ists", "m\u00fcg\u00b7lich", "/", "auch", "im", "Was\u00b7ser", "gl\u00fcend", "ste\u00b7hen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "$(", "ADV", "APPRART", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}}}}