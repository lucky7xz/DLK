{"dta.poem.14164": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Der letzte Gru\u00df .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Am Wege dort spielte ihr Kindlein,", "tokens": ["Am", "We\u00b7ge", "dort", "spiel\u00b7te", "ihr", "Kin\u00b7dlein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+--++-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Das glich ihr recht auf ein Haar,", "tokens": ["Das", "glich", "ihr", "recht", "auf", "ein", "Haar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Ich k\u00fc\u00dft's auf sein rothes M\u00fcndlein:", "tokens": ["Ich", "k\u00fc\u00dft's", "auf", "sein", "ro\u00b7thes", "M\u00fcnd\u00b7lein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "\u201egott seg'ne dich immerdar!\u201c", "tokens": ["\u201e", "gott", "seg'\u00b7ne", "dich", "im\u00b7mer\u00b7dar", "!", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADJD", "VVFIN", "PPER", "ADV", "$.", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}