{"textgrid.poem.66056": {"metadata": {"author": {"name": "Falke, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Der t\u00f6richte J\u00e4ger", "genre": "verse", "period": "N.A.", "pub_year": 1884, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Er zog hinaus, das Gl\u00fcck zu fangen,", "tokens": ["Er", "zog", "hin\u00b7aus", ",", "das", "Gl\u00fcck", "zu", "fan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und jagte mit erhitzten Wangen", "tokens": ["Und", "jag\u00b7te", "mit", "er\u00b7hitz\u00b7ten", "Wan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Bis in den sp\u00e4ten Abendschein.", "tokens": ["Bis", "in", "den", "sp\u00e4\u00b7ten", "A\u00b7bend\u00b7schein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Umsonst, es war ein schlimmes Jagen,", "tokens": ["Um\u00b7sonst", ",", "es", "war", "ein", "schlim\u00b7mes", "Ja\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Er kehrte m\u00fcde und zerschlagen", "tokens": ["Er", "kehr\u00b7te", "m\u00fc\u00b7de", "und", "zer\u00b7schla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "KON", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "In seine warme H\u00fctte ein.", "tokens": ["In", "sei\u00b7ne", "war\u00b7me", "H\u00fct\u00b7te", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Da sa\u00df in schlichtem Werkelkleide,", "tokens": ["Da", "sa\u00df", "in", "schlich\u00b7tem", "Wer\u00b7kel\u00b7klei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dem wilden J\u00e4ger schier zu Leide,", "tokens": ["Dem", "wil\u00b7den", "J\u00e4\u00b7ger", "schier", "zu", "Lei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Am Herde eine stille Magd.", "tokens": ["Am", "Her\u00b7de", "ei\u00b7ne", "stil\u00b7le", "Magd", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sie reichte ihm den Trunk, den Bissen,", "tokens": ["Sie", "reich\u00b7te", "ihm", "den", "Trunk", ",", "den", "Bis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und ging zu Hand ihm, dienstbeflissen,", "tokens": ["Und", "ging", "zu", "Hand", "ihm", ",", "dienst\u00b7be\u00b7flis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "PPER", "$,", "PDS", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wie es dem m\u00fcden Mann behagt.", "tokens": ["Wie", "es", "dem", "m\u00fc\u00b7den", "Mann", "be\u00b7hagt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Sie hatte still sich eingefunden", "tokens": ["Sie", "hat\u00b7te", "still", "sich", "ein\u00b7ge\u00b7fun\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADJD", "PRF", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und ungefragt, vor Jahr und Stunden,", "tokens": ["Und", "un\u00b7ge\u00b7fragt", ",", "vor", "Jahr", "und", "Stun\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und ihre Treue nahm er hin.", "tokens": ["Und", "ih\u00b7re", "Treu\u00b7e", "nahm", "er", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Heut sa\u00df sie bla\u00df zu seinen F\u00fc\u00dfen,", "tokens": ["Heut", "sa\u00df", "sie", "bla\u00df", "zu", "sei\u00b7nen", "F\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Er lie\u00df sie seinen Unmut b\u00fc\u00dfen,", "tokens": ["Er", "lie\u00df", "sie", "sei\u00b7nen", "Un\u00b7mut", "b\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Das fl\u00fcchtige Wild lag ihm im Sinn.", "tokens": ["Das", "fl\u00fcch\u00b7ti\u00b7ge", "Wild", "lag", "ihm", "im", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Und mu\u00df ich mich zu Tode hetzen,", "tokens": ["Und", "mu\u00df", "ich", "mich", "zu", "To\u00b7de", "het\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PRF", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Es soll mein hei\u00dfes Herz ergetzen,", "tokens": ["Es", "soll", "mein", "hei\u00b7\u00dfes", "Herz", "er\u00b7get\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Rief er und rief sein letztes Wort,", "tokens": ["Rief", "er", "und", "rief", "sein", "letz\u00b7tes", "Wort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KON", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und kehrte grollend ihr den R\u00fccken", "tokens": ["Und", "kehr\u00b7te", "grol\u00b7lend", "ihr", "den", "R\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD", "PPER", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und setzte \u00fcber Traumesbr\u00fccken", "tokens": ["Und", "setz\u00b7te", "\u00fc\u00b7ber", "Trau\u00b7mes\u00b7br\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die Jagd nach seinem Wilde fort.", "tokens": ["Die", "Jagd", "nach", "sei\u00b7nem", "Wil\u00b7de", "fort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Am Morgen, eh die V\u00f6gel girrten,", "tokens": ["Am", "Mor\u00b7gen", ",", "eh", "die", "V\u00f6\u00b7gel", "girr\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Erwacht er. Seine Blicke irrten", "tokens": ["Er\u00b7wacht", "er", ".", "Sei\u00b7ne", "Bli\u00b7cke", "irr\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$.", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Schlaftrunken \u00fcber Bett und Wand", "tokens": ["Schlaf\u00b7trun\u00b7ken", "\u00fc\u00b7ber", "Bett", "und", "Wand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und hin zum Herd. Da stand im Scheine", "tokens": ["Und", "hin", "zum", "Herd", ".", "Da", "stand", "im", "Schei\u00b7ne"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPRART", "NN", "$.", "ADV", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Des Feuers, bleich am wei\u00dfen Steine,", "tokens": ["Des", "Feu\u00b7ers", ",", "bleich", "am", "wei\u00b7\u00dfen", "Stei\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die Magd, ihr B\u00fcndel in der Hand.", "tokens": ["Die", "Magd", ",", "ihr", "B\u00fcn\u00b7del", "in", "der", "Hand", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPOSAT", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Wohin? Was treibt dich? \u2013 La\u00df mich wandern,", "tokens": ["Wo\u00b7hin", "?", "Was", "treibt", "dich", "?", "\u2013", "La\u00df", "mich", "wan\u00b7dern", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "PWS", "VVFIN", "PPER", "$.", "$(", "VVIMP", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mein Dienst geh\u00f6rt jetzt einem andern,", "tokens": ["Mein", "Dienst", "ge\u00b7h\u00f6rt", "jetzt", "ei\u00b7nem", "an\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Leb wohl, ich kehre nicht zur\u00fcck.", "tokens": ["Leb", "wohl", ",", "ich", "keh\u00b7re", "nicht", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "$,", "PPER", "VVFIN", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Schon stand sie drau\u00dfen vor der Pforte,", "tokens": ["Schon", "stand", "sie", "drau\u00b7\u00dfen", "vor", "der", "Pfor\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Er h\u00f6rt nur noch die Abschiedsworte:", "tokens": ["Er", "h\u00f6rt", "nur", "noch", "die", "Ab\u00b7schieds\u00b7wor\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Vergi\u00df mich nicht, ich war das Gl\u00fcck.", "tokens": ["Ver\u00b7gi\u00df", "mich", "nicht", ",", "ich", "war", "das", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKNEG", "$,", "PPER", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Er zog hinaus, das Gl\u00fcck zu fangen,", "tokens": ["Er", "zog", "hin\u00b7aus", ",", "das", "Gl\u00fcck", "zu", "fan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und jagte mit erhitzten Wangen", "tokens": ["Und", "jag\u00b7te", "mit", "er\u00b7hitz\u00b7ten", "Wan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Bis in den sp\u00e4ten Abendschein.", "tokens": ["Bis", "in", "den", "sp\u00e4\u00b7ten", "A\u00b7bend\u00b7schein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Umsonst, es war ein schlimmes Jagen,", "tokens": ["Um\u00b7sonst", ",", "es", "war", "ein", "schlim\u00b7mes", "Ja\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Er kehrte m\u00fcde und zerschlagen", "tokens": ["Er", "kehr\u00b7te", "m\u00fc\u00b7de",