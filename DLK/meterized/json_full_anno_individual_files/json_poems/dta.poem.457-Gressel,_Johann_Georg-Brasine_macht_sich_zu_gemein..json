{"dta.poem.457": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Brasine  macht sich zu gemein.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Brasine liesse n\u00e4chst den tummen Sch\u00e4ffer ein/", "tokens": ["Bra\u00b7si\u00b7ne", "lies\u00b7se", "n\u00e4chst", "den", "tum\u00b7men", "Sch\u00e4f\u00b7fer", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "ART", "ADJA", "NN", "ART", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und lie\u00df vor ihrer Th\u00fcr den treuen ", "tokens": ["Und", "lie\u00df", "vor", "ih\u00b7rer", "Th\u00fcr", "den", "treu\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "ART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ha! dacht\u2019 ich machst du dich mit ", "tokens": ["Ha", "!", "dacht'", "ich", "machst", "du", "dich", "mit"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "VVFIN", "PPER", "VVFIN", "PPER", "PRF", "APPR"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.4": {"text": "So mag ein ander gehn in den entbl\u00fcmten Garten.", "tokens": ["So", "mag", "ein", "an\u00b7der", "gehn", "in", "den", "ent\u00b7bl\u00fcm\u00b7ten", "Gar\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "ADJD", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}