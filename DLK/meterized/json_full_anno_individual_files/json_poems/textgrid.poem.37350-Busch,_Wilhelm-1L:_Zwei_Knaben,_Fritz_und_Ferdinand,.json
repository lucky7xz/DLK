t"], "pos": ["NN", "NE", "PAV", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Denn eben schlich aus dunkler Laube,", "tokens": ["Denn", "e\u00b7ben", "schlich", "aus", "dunk\u00b7ler", "Lau\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "In fester Faust ein spanisch Rohr,", "tokens": ["In", "fes\u00b7ter", "Faust", "ein", "spa\u00b7nisch", "Rohr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Der aufmerksame Schramm hervor.", "tokens": ["Der", "auf\u00b7merk\u00b7sa\u00b7me", "Schramm", "her\u00b7vor", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Auch Ferdinand sah ihn beizeiten", "tokens": ["Auch", "Fer\u00b7di\u00b7nand", "sah", "ihn", "bei\u00b7zei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "NE", "VVFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Und t\u00e4t am Stamm heruntergleiten", "tokens": ["Und", "t\u00e4t", "am", "Stamm", "her\u00b7un\u00b7ter\u00b7glei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "In \u00c4ngstlichkeit und gro\u00dfer Hast,", "tokens": ["In", "\u00c4ngst\u00b7lich\u00b7keit", "und", "gro\u00b7\u00dfer", "Hast", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Doch eh er unten Fu\u00df gefa\u00dft,", "tokens": ["Doch", "eh", "er", "un\u00b7ten", "Fu\u00df", "ge\u00b7fa\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Begr\u00fc\u00dft ihn Schramm bereits mit Streichen,", "tokens": ["Be\u00b7gr\u00fc\u00dft", "ihn", "Schramm", "be\u00b7reits", "mit", "Strei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "Als wollt er einen Stein erweichen.", "tokens": ["Als", "wollt", "er", "ei\u00b7nen", "Stein", "er\u00b7wei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VMFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.29": {"text": "Der Ferdinand, voll Schmerz und Hitze,", "tokens": ["Der", "Fer\u00b7di\u00b7nand", ",", "voll", "Schmerz", "und", "Hit\u00b7ze", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "$,", "ADJD", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.30": {"text": "Entfloh und suchte seinen Fritze.", "tokens": ["Ent\u00b7floh", "und", "such\u00b7te", "sei\u00b7nen", "Frit\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.31": {"text": "Wie angewurzelt blieb er stehn.", "tokens": ["Wie", "an\u00b7ge\u00b7wur\u00b7zelt", "blieb", "er", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVPP", "VVFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "Ach, h\u00e4tt' er es doch nie gesehn:", "tokens": ["Ach", ",", "h\u00e4tt'", "er", "es", "doch", "nie", "ge\u00b7sehn", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VAFIN", "PPER", "PPER", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "Die K\u00e4the hat den Fritz gek\u00fc\u00dft,", "tokens": ["Die", "K\u00e4\u00b7the", "hat", "den", "Fritz", "ge\u00b7k\u00fc\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NE", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.34": {"text": "Worauf sie eine Birne i\u00dft.", "tokens": ["Wo\u00b7rauf", "sie", "ei\u00b7ne", "Bir\u00b7ne", "i\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.35": {"text": "Seit dies geschah, ist Ferdinand", "tokens": ["Seit", "dies", "ge\u00b7schah", ",", "ist", "Fer\u00b7di\u00b7nand"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PDS", "VVFIN", "$,", "VAFIN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.36": {"text": "Mit Fritz nicht mehr so gut bekannt.", "tokens": ["Mit", "Fritz", "nicht", "mehr", "so", "gut", "be\u00b7kannt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "PTKNEG", "ADV", "ADV", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}