{"textgrid.poem.37350": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Zwei Knaben, Fritz und Ferdinand,", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zwei Knaben, Fritz und Ferdinand,", "tokens": ["Zwei", "Kna\u00b7ben", ",", "Fritz", "und", "Fer\u00b7di\u00b7nand", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "$,", "NE", "KON", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die gingen immer Hand in Hand,", "tokens": ["Die", "gin\u00b7gen", "im\u00b7mer", "Hand", "in", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und selbst in einer Herzensfrage", "tokens": ["Und", "selbst", "in", "ei\u00b7ner", "Her\u00b7zens\u00b7fra\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Trat ihre Einigkeit zutage.", "tokens": ["Trat", "ih\u00b7re", "Ei\u00b7nig\u00b7keit", "zu\u00b7ta\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Sie liebten beide Nachbars K\u00e4thchen,", "tokens": ["Sie", "lieb\u00b7ten", "bei\u00b7de", "Nach\u00b7bars", "K\u00e4th\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "NE", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ein blondgelocktes, kleines M\u00e4dchen.", "tokens": ["Ein", "blond\u00b7ge\u00b7lock\u00b7tes", ",", "klei\u00b7nes", "M\u00e4d\u00b7chen", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Einst sagte die verschmitzte Dirne:", "tokens": ["Einst", "sag\u00b7te", "die", "ver\u00b7schmitz\u00b7te", "Dir\u00b7ne", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Wer holt mir eine Sommerbirne,", "tokens": ["Wer", "holt", "mir", "ei\u00b7ne", "Som\u00b7mer\u00b7bir\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Recht saftig, aber nicht zu klein?", "tokens": ["Recht", "saf\u00b7tig", ",", "a\u00b7ber", "nicht", "zu", "klein", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "$,", "ADV", "PTKNEG", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Hernach soll er der Beste sein.", "tokens": ["Her\u00b7nach", "soll", "er", "der", "Bes\u00b7te", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ART", "NN", "VAINF", "$."], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.11": {"text": "Der Fritz nahm seinen Freund beiseit", "tokens": ["Der", "Fritz", "nahm", "sei\u00b7nen", "Freund", "bei\u00b7seit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NE", "VVFIN", "PPOSAT", "NN", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Und sprach: Das machen wir zu zweit;", "tokens": ["Und", "sprach", ":", "Das", "ma\u00b7chen", "wir", "zu", "zweit", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "PDS", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Da dr\u00fcben wohnt der alte Schramm,", "tokens": ["Da", "dr\u00fc\u00b7ben", "wohnt", "der", "al\u00b7te", "Schramm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Der hat den sch\u00f6nsten Birnenstamm;", "tokens": ["Der", "hat", "den", "sch\u00f6ns\u00b7ten", "Bir\u00b7nen\u00b7stamm", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Du steigst hinauf und sch\u00fcttelst sacht,", "tokens": ["Du", "steigst", "hin\u00b7auf", "und", "sch\u00fct\u00b7telst", "sacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "KON", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Ich lese auf und gebe acht.", "tokens": ["Ich", "le\u00b7se", "auf", "und", "ge\u00b7be", "acht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "KON", "VVFIN", "CARD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Gesagt, getan. Sie sind am Ziel.", "tokens": ["Ge\u00b7sagt", ",", "ge\u00b7tan", ".", "Sie", "sind", "am", "Ziel", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "VVPP", "$.", "PPER", "VAFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Schon als die erste Birne fiel,", "tokens": ["Schon", "als", "die", "ers\u00b7te", "Bir\u00b7ne", "fiel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Macht Fritz damit sich aus dem Staube,", "tokens": ["Macht", "Fritz", "da\u00b7mit", "sich", "aus", "dem", "Stau\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "PAV", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Denn eben schlich aus dunkler Laube,", "tokens": ["Denn", "e\u00b7ben", "schlich", "aus", "dunk\u00b7ler", "Lau\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "In fester Faust ein spanisch Rohr,", "tokens": ["In", "fes\u00b7ter", "Faust", "ein", "spa\u00b7nisch", "Rohr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Der aufmerksame Schramm hervor.", "tokens": ["Der", "auf\u00b7merk\u00b7sa\u00b7me", "Schramm", "her\u00b7vor", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Auch Ferdinand sah ihn beizeiten", "tokens": ["Auch", "Fer\u00b7di\u00b7nand", "sah", "ihn", "bei\u00b7zei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "NE", "VVFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Und t\u00e4t am Stamm heruntergleiten", "tokens": ["Und", "t\u00e4t", "am", "Stamm", "her\u00b7un\u00b7ter\u00b7glei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "In \u00c4ngstlichkeit und gro\u00dfer Hast,", "tokens": ["In", "\u00c4ngst\u00b7lich\u00b7keit", "und", "gro\u00b7\u00dfer", "Hast", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Doch eh er unten Fu\u00df gefa\u00dft,", "tokens": ["Doch", "eh", "er", "un\u00b7ten", "Fu\u00df", "ge\u00b7fa\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Begr\u00fc\u00dft ihn Schramm bereits mit Streichen,", "tokens": ["Be\u00b7gr\u00fc\u00dft", "ihn", "Schramm", "be\u00b7reits", "mit", "Strei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "Als wollt er einen Stein erweichen.", "tokens": ["Als", "wollt", "er", "ei\u00b7nen", "Stein", "er\u00b7wei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VMFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.29": {"text": "Der Ferdinand, voll Schmerz und Hitze,", "tokens": ["Der", "Fer\u00b7di\u00b7nand", ",", "voll", "Schmerz", "und", "Hit\u00b7ze", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "$,", "ADJD", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.30": {"text": "Entfloh und suchte seinen Fritze.", "tokens": ["Ent\u00b7floh", "und", "such\u00b7te", "sei\u00b7nen", "Frit\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.31": {"text": "Wie angewurzelt blieb er stehn.", "tokens": ["Wie", "an\u00b7ge\u00b7wur\u00b7zelt", "blieb", "er", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVPP", "VVFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "Ach, h\u00e4tt' er es doch nie gesehn:", "tokens": ["Ach", ",", "h\u00e4tt'", "er", "es", "doch", "nie", "ge\u00b7sehn", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VAFIN", "PPER", "PPER", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "Die K\u00e4the hat den Fritz gek\u00fc\u00dft,", "tokens": ["Die", "K\u00e4\u00b7the", "hat", "den", "Fritz", "ge\u00b7k\u00fc\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NE", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.34": {"text": "Worauf sie eine Birne i\u00dft.", "tokens": ["Wo\u00b7rauf", "sie", "ei\u00b7ne", "Bir\u00b7ne", "i\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.35": {"text": "Seit dies geschah, ist Ferdinand", "tokens": ["Seit", "dies", "ge\u00b7schah", ",", "ist", "Fer\u00b7di\u00b7nand"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PDS", "VVFIN", "$,", "VAFIN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.36": {"text": "Mit Fritz nicht mehr so gut bekannt.", "tokens": ["Mit", "Fritz", "nicht", "mehr", "so", "gut", "be\u00b7kannt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "PTKNEG", "ADV", "ADV", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Zwe