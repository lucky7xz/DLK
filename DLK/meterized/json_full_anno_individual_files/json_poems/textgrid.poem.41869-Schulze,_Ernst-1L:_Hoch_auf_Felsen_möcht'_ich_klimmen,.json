{"textgrid.poem.41869": {"metadata": {"author": {"name": "Schulze, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: Hoch auf Felsen m\u00f6cht' ich klimmen,", "genre": "verse", "period": "N.A.", "pub_year": 1803, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hoch auf Felsen m\u00f6cht' ich klimmen,", "tokens": ["Hoch", "auf", "Fel\u00b7sen", "m\u00f6cht'", "ich", "klim\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo die Wolken n\u00e4chtlich thronen", "tokens": ["Wo", "die", "Wol\u00b7ken", "n\u00e4cht\u00b7lich", "thro\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und von bleichem Duft umwoben", "tokens": ["Und", "von", "blei\u00b7chem", "Duft", "um\u00b7wo\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Fern des Lebens Bild erbla\u00dft.", "tokens": ["Fern", "des", "Le\u00b7bens", "Bild", "er\u00b7bla\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ueber Str\u00f6me m\u00f6cht' ich schwimmen,", "tokens": ["Ue\u00b7ber", "Str\u00f6\u00b7me", "m\u00f6cht'", "ich", "schwim\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "M\u00f6cht' in dunklen W\u00fcsten wohnen", "tokens": ["M\u00f6cht'", "in", "dunk\u00b7len", "W\u00fcs\u00b7ten", "woh\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "APPR", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und durch Nacht und Sturmestoben", "tokens": ["Und", "durch", "Nacht", "und", "Stur\u00b7me\u00b7sto\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "KON", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.8": {"text": "Wandeln ohne Ruh' und Rast.", "tokens": ["Wan\u00b7deln", "oh\u00b7ne", "Ruh'", "und", "Rast", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Nur der Bilder rasches Walten", "tokens": ["Nur", "der", "Bil\u00b7der", "ra\u00b7sches", "Wal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kann den innern Sturm beschw\u00f6ren,", "tokens": ["Kann", "den", "in\u00b7nern", "Sturm", "be\u00b7schw\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und doch zieht's mich in die Stille", "tokens": ["Und", "doch", "zieht's", "mich", "in", "die", "Stil\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PRF", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Zu des Herzens Kampf zur\u00fcck.", "tokens": ["Zu", "des", "Her\u00b7zens", "Kampf", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Denn ich zage, zu erkalten,", "tokens": ["Denn", "ich", "za\u00b7ge", ",", "zu", "er\u00b7kal\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Mag die Gluth mich auch verzehren;", "tokens": ["Mag", "die", "Gluth", "mich", "auch", "ver\u00b7zeh\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "PPER", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Was mich t\u00e4uscht, das ist mein Wille,", "tokens": ["Was", "mich", "t\u00e4uscht", ",", "das", "ist", "mein", "Wil\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "PDS", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Was mich qu\u00e4lt, mein einz'ges Gl\u00fcck.", "tokens": ["Was", "mich", "qu\u00e4lt", ",", "mein", "einz'\u00b7ges", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Schmerzlich tracht' ich nach dem Sch\u00f6nen,", "tokens": ["Schmerz\u00b7lich", "tracht'", "ich", "nach", "dem", "Sch\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Weil ich Sch\u00f6nes lieb' im Herzen;", "tokens": ["Weil", "ich", "Sch\u00f6\u00b7nes", "lieb'", "im", "Her\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch das Sch\u00f6ne l\u00e4\u00dft mich zagen,", "tokens": ["Doch", "das", "Sch\u00f6\u00b7ne", "l\u00e4\u00dft", "mich", "za\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Weil ich ewig fern ihm bin.", "tokens": ["Weil", "ich", "e\u00b7wig", "fern", "ihm", "bin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "ADJD", "PPER", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ew'ge T\u00e4uschung, ew'ges Sehnen,", "tokens": ["Ew'\u00b7ge", "T\u00e4u\u00b7schung", ",", "ew'\u00b7ges", "Seh\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Bange Lust und bittre Schmerzen,", "tokens": ["Ban\u00b7ge", "Lust", "und", "bitt\u00b7re", "Schmer\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "KON", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Furcht und Hoffnung, Fliehn und Wagen,", "tokens": ["Furcht", "und", "Hoff\u00b7nung", ",", "Fliehn", "und", "Wa\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Zarte Lieb' ist dein Gewinn!", "tokens": ["Zar\u00b7te", "Lieb'", "ist", "dein", "Ge\u00b7winn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Weh, die Nacht ist ohne Sterne,", "tokens": ["Weh", ",", "die", "Nacht", "ist", "oh\u00b7ne", "Ster\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "VAFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ohne Farb' und Licht der Morgen,", "tokens": ["Oh\u00b7ne", "Fa\u00b7rb'", "und", "Licht", "der", "Mor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ART", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Und kein Leben giebt das Wachen", "tokens": ["Und", "kein", "Le\u00b7ben", "giebt", "das", "Wa\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und der Schlummer keine Ruh.", "tokens": ["Und", "der", "Schlum\u00b7mer", "kei\u00b7ne", "Ruh", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und so treib' ich durch die Ferne,", "tokens": ["Und", "so", "treib'", "ich", "durch", "die", "Fer\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Auf dem Meer entzweyter Sorgen,", "tokens": ["Auf", "dem", "Meer", "ent\u00b7zwey\u00b7ter", "Sor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Steuerlos, im morschen Rachen", "tokens": ["Steu\u00b7er\u00b7los", ",", "im", "mor\u00b7schen", "Ra\u00b7chen"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Tr\u00fcgerischen K\u00fcsten zu.", "tokens": ["Tr\u00fc\u00b7ge\u00b7ri\u00b7schen", "K\u00fcs\u00b7ten", "zu", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Und ich achte nicht der Wellen,", "tokens": ["Und", "ich", "ach\u00b7te", "nicht", "der", "Wel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PTKNEG", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Suche nicht der Noth zu wehren,", "tokens": ["Su\u00b7che", "nicht", "der", "Noth", "zu", "weh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Weil ein einziger Gedanke", "tokens": ["Weil", "ein", "ein\u00b7zi\u00b7ger", "Ge\u00b7dan\u00b7ke"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "T\u00f6dtend meine Kraft umflicht.", "tokens": ["T\u00f6d\u00b7tend", "mei\u00b7ne", "Kraft", "um\u00b7flicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Mag der schwache Kahn zerschellen,", "tokens": ["Mag", "der", "schwa\u00b7che", "Kahn", "zer\u00b7schel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Mag er heim zum Ufer kehren,", "tokens": ["Mag", "er", "heim", "zum", "U\u00b7fer", "keh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKVZ", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Tod, ich zittre nicht; ich danke,", "tokens": ["Tod", ",", "ich", "zitt\u00b7re", "nicht", ";", "ich", "dan\u00b7ke", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VVFIN", "PTKNEG", "$.", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Gl\u00fcck, dir deine Rettung nicht!", "tokens": ["Gl\u00fcck", ",", "dir", "dei\u00b7ne", "Ret\u00b7tung", "nicht", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "PPOSAT", "NN", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Hoch auf Felsen m\u00f6cht' ich klimmen,", "tokens": ["Hoch", "auf", "Fel\u00b7sen", "m\u00f6cht'", "ich", "klim\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo die Wolken n\u00e4chtlich thronen", "tokens": ["Wo", "die", "Wol\u00b7ken", "n\u00e4cht\u00b7lich", "thro\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und von bleichem Duft umwoben", "tokens": ["Und", "von", "blei\u00b7chem", "Duft", "um\u00b7wo\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Fern des Lebens Bild erbla\u00dft.", "tokens": ["Fern", "des", "Le\u00b7bens", "Bild", "er\u00b7bla\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ueber Str\u00f6me m\u00f6cht' ich schwimmen,", "tokens": ["Ue\u00b7ber", "Str\u00f6\u00b7me", "m\u00f6cht'", "ich", "schwim\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "M\u00f6cht' in dunklen W\u00fcsten wohnen", "tokens": ["M\u00f6cht'", "in", "dunk\u00b7len", "W\u00fcs\u00b7ten", "woh\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "APPR", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und durch Nacht und Sturmestoben", "tokens": ["Und", "durch", "Nacht", "und", "Stur\u00b7me\u00b7sto\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "KON", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.8": {"text": "Wandeln ohne Ruh' und Rast.", "tokens": ["Wan\u00b7deln", "oh\u00b7ne", "Ruh'", "und", "Rast", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Nur der Bilder rasches Walten", "tokens": ["Nur", "der", "Bil\u00b7der", "ra\u00b7sches", "Wal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kann den innern Sturm beschw\u00f6ren,", "tokens": ["Kann", "den", "in\u00b7nern", "Sturm", "be\u00b7schw\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und doch zieht's mich in die Stille", "tokens": ["Und", "doch", "zieht's", "mich", "in", "die", "Stil\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PRF", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Zu des Herzens Kampf zur\u00fcck.", "tokens": ["Zu", "des", "Her\u00b7zens", "Kampf", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Denn ich zage, zu erkalten,", "tokens": ["Denn", "ich", "za\u00b7ge", ",", "zu", "er\u00b7kal\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Mag die Gluth mich auch verzehren;", "tokens": ["Mag", "die", "Gluth", "mich", "auch", "ver\u00b7zeh\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "PPER", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Was mich t\u00e4uscht, das ist mein Wille,", "tokens": ["Was", "mich", "t\u00e4uscht", ",", "das", "ist", "mein", "Wil\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "PDS", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Was mich qu\u00e4lt, mein einz'ges Gl\u00fcck.", "tokens": ["Was", "mich", "qu\u00e4lt", ",", "mein", "einz'\u00b7ges", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Schmerzlich tracht' ich