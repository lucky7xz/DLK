{"textgrid.poem.63897": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "Auf Schlo\u00df Labers", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vom luftigen Altan, an dessen Brustwehr,", "tokens": ["Vom", "luf\u00b7ti\u00b7gen", "Al\u00b7tan", ",", "an", "des\u00b7sen", "Brust\u00b7wehr", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "APPR", "PRELAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In Efeu dicht geh\u00fcllt, ein Bienenschwarm", "tokens": ["In", "E\u00b7feu", "dicht", "ge\u00b7h\u00fcllt", ",", "ein", "Bie\u00b7nen\u00b7schwarm"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "NE", "ADJD", "VVPP", "$,", "ART", "NN"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Gesch\u00e4ftig summend um die Bl\u00fcthen schwirrt,", "tokens": ["Ge\u00b7sch\u00e4f\u00b7tig", "sum\u00b7mend", "um", "die", "Bl\u00fc\u00b7then", "schwirrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Gern schau' ich nieder, wenn der Tag verbla\u00dft.", "tokens": ["Gern", "schau'", "ich", "nie\u00b7der", ",", "wenn", "der", "Tag", "ver\u00b7bla\u00dft", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$,", "KOUS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Zu meinen F\u00fc\u00dfen senkt die Halde sich", "tokens": ["Zu", "mei\u00b7nen", "F\u00fc\u00b7\u00dfen", "senkt", "die", "Hal\u00b7de", "sich"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN", "PRF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Mit ihrer Rebeng\u00e4rten \u00dcberschwang,", "tokens": ["Mit", "ih\u00b7rer", "Re\u00b7ben\u00b7g\u00e4r\u00b7ten", "\u00dc\u00b7bersc\u00b7hwang", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Noch gl\u00fchend von der Sonne Feuerku\u00df,", "tokens": ["Noch", "gl\u00fc\u00b7hend", "von", "der", "Son\u00b7ne", "Feu\u00b7er\u00b7ku\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Und aus dem Gr\u00fcn mit ihren Zinnent\u00fcrmchen", "tokens": ["Und", "aus", "dem", "Gr\u00fcn", "mit", "ih\u00b7ren", "Zin\u00b7nen\u00b7t\u00fcrm\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Ragen die stillen Schl\u00f6sser, Trautmannsdorf,", "tokens": ["Ra\u00b7gen", "die", "stil\u00b7len", "Schl\u00f6s\u00b7ser", ",", "Traut\u00b7manns\u00b7dorf", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,", "NN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.10": {"text": "Rametz, zur Rechten Planta \u2013 wohlbekannt.", "tokens": ["Ra\u00b7metz", ",", "zur", "Rech\u00b7ten", "Plan\u00b7ta", "\u2013", "wohl\u00b7be\u00b7kannt", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "APPRART", "NN", "NE", "$(", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Und tiefer, wo in ihrer Felsenkluft", "tokens": ["Und", "tie\u00b7fer", ",", "wo", "in", "ih\u00b7rer", "Fel\u00b7sen\u00b7kluft"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "$,", "PWAV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Die wilde Passer rauscht, die schatt'gen Gassen", "tokens": ["Die", "wil\u00b7de", "Pas\u00b7ser", "rauscht", ",", "die", "schatt'\u00b7gen", "Gas\u00b7sen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Merans, aus deren Mitte sich der Turm", "tokens": ["Me\u00b7rans", ",", "aus", "de\u00b7ren", "Mit\u00b7te", "sich", "der", "Turm"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "APPR", "PRELAT", "NN", "PRF", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Der alten Kirche hebt. Weit dr\u00fcben aber", "tokens": ["Der", "al\u00b7ten", "Kir\u00b7che", "hebt", ".", "Weit", "dr\u00fc\u00b7ben", "a\u00b7ber"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$.", "NN", "ADV", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Kommt, hin und wieder in der Sonne blitzend,", "tokens": ["Kommt", ",", "hin", "und", "wie\u00b7der", "in", "der", "Son\u00b7ne", "blit\u00b7zend", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADV", "KON", "ADV", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Die Etsch herab, wie ein mutwillig Kind", "tokens": ["Die", "Etsch", "her\u00b7ab", ",", "wie", "ein", "mut\u00b7wil\u00b7lig", "Kind"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "$,", "PWAV", "ART", "ADJD", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Die Treppenstufen niederspringt, und reicht", "tokens": ["Die", "Trep\u00b7pen\u00b7stu\u00b7fen", "nie\u00b7der\u00b7springt", ",", "und", "reicht"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "KON", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Der Schwester vom Passeiertal die Hand", "tokens": ["Der", "Schwes\u00b7ter", "vom", "Pas\u00b7sei\u00b7er\u00b7tal", "die", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Und gr\u00fc\u00dft vertraut hinauf zu Schlo\u00df Tirol.", "tokens": ["Und", "gr\u00fc\u00dft", "ver\u00b7traut", "hin\u00b7auf", "zu", "Schlo\u00df", "Ti\u00b7rol."], "token_info": ["word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["KON", "VVFIN", "ADJD", "ADV", "APPR", "NN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Gesegnetes Gefilde, m\u00e4rchenhaft", "tokens": ["Ge\u00b7se\u00b7gne\u00b7tes", "Ge\u00b7fil\u00b7de", ",", "m\u00e4r\u00b7chen\u00b7haft"], "token_info": ["word", "word", "punct", "word"], "pos": ["ADJA", "NN", "$,", "NN"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Geschm\u00fcckt mit Anmut, vom erhabnen Kranz", "tokens": ["Ge\u00b7schm\u00fcckt", "mit", "An\u00b7mut", ",", "vom", "er\u00b7hab\u00b7nen", "Kranz"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVPP", "APPR", "NN", "$,", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der Bergesh\u00f6hn umblaut, der tiefer jetzt", "tokens": ["Der", "Ber\u00b7ges\u00b7h\u00f6hn", "um\u00b7blaut", ",", "der", "tie\u00b7fer", "jetzt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVPP", "$,", "PRELS", "ADJD", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Sich f\u00e4rbt, bis an den h\u00f6chsten nackten Firnen", "tokens": ["Sich", "f\u00e4rbt", ",", "bis", "an", "den", "h\u00f6chs\u00b7ten", "nack\u00b7ten", "Fir\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PRF", "VVFIN", "$,", "KOUS", "APPR", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Der letzte Purpurhauch erlischt! Nun liegt", "tokens": ["Der", "letz\u00b7te", "Pur\u00b7pur\u00b7hauch", "er\u00b7lischt", "!", "Nun", "liegt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVPP", "$.", "ADV", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Die weite Runde still, als hielte sie", "tokens": ["Die", "wei\u00b7te", "Run\u00b7de", "still", ",", "als", "hiel\u00b7te", "sie"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$,", "KOUS", "VVFIN", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Den Atem an. Und drunten in den H\u00e4usern", "tokens": ["Den", "A\u00b7tem", "an", ".", "Und", "drun\u00b7ten", "in", "den", "H\u00e4u\u00b7sern"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PTKVZ", "$.", "KON", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Glimmt Licht an Lichtlein auf, wie in der D\u00e4mmrung", "tokens": ["Glimmt", "Licht", "an", "Licht\u00b7lein", "auf", ",", "wie", "in", "der", "D\u00e4mm\u00b7rung"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "NN", "APPR", "NN", "PTKVZ", "$,", "PWAV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Leuchtk\u00e4fer funkeln durch ein Gartenland,", "tokens": ["Leucht\u00b7k\u00e4\u00b7fer", "fun\u00b7keln", "durch", "ein", "Gar\u00b7ten\u00b7land", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Am dunklen Berge dort beim Eggerbauern", "tokens": ["Am", "dunk\u00b7len", "Ber\u00b7ge", "dort", "beim", "Eg\u00b7ger\u00b7bau\u00b7ern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Noch ein versprengtes F\u00fcnkchen. Aber golden", "tokens": ["Noch", "ein", "ver\u00b7spreng\u00b7tes", "F\u00fcnk\u00b7chen", ".", "A\u00b7ber", "gol\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN", "$.", "KON", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Ob all dem Erdgeleuchte schwebt die Sichel", "tokens": ["Ob", "all", "dem", "Erd\u00b7ge\u00b7leuch\u00b7te", "schwebt", "die", "Si\u00b7chel"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIAT", "ART", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Des Mondes still dahin im reinen \u00c4ther,", "tokens": ["Des", "Mon\u00b7des", "still", "da\u00b7hin", "im", "rei\u00b7nen", "\u00c4\u00b7ther", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "PAV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Und ihre taubeschwerten Fittiche", "tokens": ["Und", "ih\u00b7re", "tau\u00b7be\u00b7schwer\u00b7ten", "Fit\u00b7ti\u00b7che"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.15": {"text": "Entfaltet jetzt die Nacht.", "tokens": ["Ent\u00b7fal\u00b7tet", "jetzt", "die", "Nacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Auf meine Seele", "tokens": ["Auf", "mei\u00b7ne", "See\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Senkt Schwermut sich herab. Sie schweift zur\u00fcck", "tokens": ["Senkt", "Schwer\u00b7mut", "sich", "her\u00b7ab", ".", "Sie", "schweift", "zu\u00b7r\u00fcck"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "NN", "PRF", "ADV", "$.", "PPER", "VVFIN", "PTKVZ"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "In langversunkne Zeit, das Auge sucht", "tokens": ["In", "lang\u00b7ver\u00b7sunk\u00b7ne", "Zeit", ",", "das", "Au\u00b7ge", "sucht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Im n\u00e4cht'gen Schatten drunten jenes Haus,", "tokens": ["Im", "n\u00e4cht'\u00b7gen", "Schat\u00b7ten", "drun\u00b7ten", "je\u00b7nes", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wo sommerlang ich schwerstes Leid erduldet", "tokens": ["Wo", "som\u00b7mer\u00b7lang", "ich", "schwers\u00b7tes", "Leid", "er\u00b7dul\u00b7det"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und rings um mich die Kraft und Segensf\u00fclle", "tokens": ["Und", "rings", "um", "mich", "die", "Kraft", "und", "Se\u00b7gens\u00b7f\u00fcl\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "PRF", "ART", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Der \u00fcppigen Natur ein Hohn mir deucht'", "tokens": ["Der", "\u00fcp\u00b7pi\u00b7gen", "Na\u00b7tur", "ein", "Hohn", "mir", "deucht'"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "PPER", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Auf mein verarmend Dasein. Ihre Zauber", "tokens": ["Auf", "mein", "ver\u00b7ar\u00b7mend", "Da\u00b7sein", ".", "Ih\u00b7re", "Zau\u00b7ber"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PPOSAT", "VVPP", "VAINF", "$.", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Besel'gen nur den Gl\u00fccklichen. Wer hat,", "tokens": ["Be\u00b7sel'\u00b7gen", "nur", "den", "Gl\u00fcck\u00b7li\u00b7chen", ".", "Wer", "hat", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "$.", "PWS", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Dem wird gegeben \u2013 unbarmherz'ge Weisheit,", "tokens": ["Dem", "wird", "ge\u00b7ge\u00b7ben", "\u2013", "un\u00b7barm\u00b7her\u00b7z'\u00b7ge", "Weis\u00b7heit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "VVPP", "$(", "ADJA", "NN", "$,"], "meter": "-+-+----+-+-", "measure": "unknown.measure.tetra"}, "line.11": {"text": "Die eines Bettlers spottet!", "tokens": ["Die", "ei\u00b7nes", "Bett\u00b7lers", "spot\u00b7tet", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Doch die Nacht,", "tokens": ["Doch", "die", "Nacht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.13": {"text": "Die blasse Schatten aus den Gr\u00e4bern weckt,", "tokens": ["Die", "blas\u00b7se", "Schat\u00b7ten", "aus", "den", "Gr\u00e4\u00b7bern", "weckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Hat Balsam auch f\u00fcr alte Wunden. Sacht", "tokens": ["Hat", "Bal\u00b7sam", "auch", "f\u00fcr", "al\u00b7te", "Wun\u00b7den", ".", "Sacht"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["VAFIN", "NE", "ADV", "APPR", "ADJA", "NN", "$.", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Vom hellgestirnten Firmamente tr\u00e4uft", "tokens": ["Vom", "hell\u00b7ges\u00b7tirn\u00b7ten", "Fir\u00b7ma\u00b7men\u00b7te", "tr\u00e4uft"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Ein Friede nieder, an das Ew'ge mahnend,", "tokens": ["Ein", "Frie\u00b7de", "nie\u00b7der", ",", "an", "das", "Ew'\u00b7ge", "mah\u00b7nend", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,", "APPR", "ART", "ADJA", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Und schauernd f\u00fchlt von einer h\u00f6hern Macht", "tokens": ["Und", "schau\u00b7ernd", "f