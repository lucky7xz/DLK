{"textgrid.poem.48192": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1858, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mit achtzehn Jahr und roten Wangen,", "tokens": ["Mit", "acht\u00b7zehn", "Jahr", "und", "ro\u00b7ten", "Wan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da sei's, da wandre nach Paris,", "tokens": ["Da", "sei's", ",", "da", "wand\u00b7re", "nach", "Pa\u00b7ris", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "$,", "ADV", "VVFIN", "APPR", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn noch kein tieferes Verlangen", "tokens": ["Wenn", "noch", "kein", "tie\u00b7fe\u00b7res", "Ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sich dir ins Herze niederlie\u00df;", "tokens": ["Sich", "dir", "ins", "Her\u00b7ze", "nie\u00b7der\u00b7lie\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wenn unser Bestes: Lieb' und Treue,", "tokens": ["Wenn", "un\u00b7ser", "Bes\u00b7tes", ":", "Lieb'", "und", "Treu\u00b7e", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "$.", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Du nicht begehrst und nicht vermi\u00dft,", "tokens": ["Du", "nicht", "be\