{"textgrid.poem.66193": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "Wiens Willkomm an Kaiser Joseph", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Willkommen Held, in deiner Strahlenw\u00fcrde!", "tokens": ["Will\u00b7kom\u00b7men", "Held", ",", "in", "dei\u00b7ner", "Strah\u00b7len\u00b7w\u00fcr\u00b7de", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Willkommen, Held!", "tokens": ["Will\u00b7kom\u00b7men", ",", "Held", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Siehst du die Kinder um dich her versammeln?", "tokens": ["Siehst", "du", "die", "Kin\u00b7der", "um", "dich", "her", "ver\u00b7sam\u00b7meln", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPR", "PRF", "APZR", "VVINF", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "H\u00f6rst du den Namen \u2013 Vater Joseph stammeln?", "tokens": ["H\u00f6rst", "du", "den", "Na\u00b7men", "\u2013", "Va\u00b7ter", "Jo\u00b7se\u00b7ph", "stam\u00b7meln", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$(", "NN", "NE", "VVFIN", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Willkommen, Stolz der Welt!", "tokens": ["Will\u00b7kom\u00b7men", ",", "Stolz", "der", "Welt", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Oft bebten wir; denn ach! aus unsern Armen", "tokens": ["Oft", "beb\u00b7ten", "wir", ";", "denn", "ach", "!", "aus", "un\u00b7sern", "Ar\u00b7men"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$.", "KON", "XY", "$.", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Riss'st du dich los,", "tokens": ["Riss'st", "du", "dich", "los", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "PTKVZ", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Verachtetest den Flug der T\u00fcrkenpfeile", "tokens": ["Ver\u00b7ach\u00b7te\u00b7test", "den", "Flug", "der", "T\u00fcr\u00b7ken\u00b7pfei\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und wiegtest dich im st\u00fcrmenden Geheule", "tokens": ["Und", "wieg\u00b7test", "dich", "im", "st\u00fcr\u00b7men\u00b7den", "Ge\u00b7heu\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Der Kriegsgefahren gro\u00df.", "tokens": ["Der", "Kriegs\u00b7ge\u00b7fah\u00b7ren", "gro\u00df", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Um deine Lenden brausten schwarze Wetter;", "tokens": ["Um", "dei\u00b7ne", "Len\u00b7den", "braus\u00b7ten", "schwar\u00b7ze", "Wet\u00b7ter", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Du aber standst", "tokens": ["Du", "a\u00b7ber", "standst"], "token_info": ["word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Im Wuthgebr\u00fcll des rasenden Get\u00fcmmels,", "tokens": ["Im", "Wuth\u00b7ge\u00b7br\u00fcll", "des", "ra\u00b7sen\u00b7den", "Ge\u00b7t\u00fcm\u00b7mels", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Weil du den Schutz des dir geneigten Himmels", "tokens": ["Weil", "du", "den", "Schutz", "des", "dir", "ge\u00b7neig\u00b7ten", "Him\u00b7mels"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "ART", "PPER", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "In deiner Seel' empfandst.", "tokens": ["In", "dei\u00b7ner", "Seel'", "emp\u00b7fandst", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Nun danken wir mit erdgebeugter Stirne", "tokens": ["Nun", "dan\u00b7ken", "wir", "mit", "erd\u00b7ge\u00b7beug\u00b7ter", "Stir\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dem Herrn der Welt,", "tokens": ["Dem", "Herrn", "der", "Welt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Dem M\u00e4chtigen, der dir den Harnisch schnallte,", "tokens": ["Dem", "M\u00e4ch\u00b7ti\u00b7gen", ",", "der", "dir", "den", "Har\u00b7nisch", "schnall\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "p