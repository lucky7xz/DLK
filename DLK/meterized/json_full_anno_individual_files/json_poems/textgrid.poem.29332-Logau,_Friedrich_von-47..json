{"textgrid.poem.29332": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "47.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Venus soll man mehr nicht sprechen; nur Lustinne soll man sagen,", "tokens": ["Ve\u00b7nus", "soll", "man", "mehr", "nicht", "spre\u00b7chen", ";", "nur", "Lus\u00b7tin\u00b7ne", "soll", "man", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PIS", "ADV", "PTKNEG", "VVINF", "$.", "ADV", "NN", "VMFIN", "PIS", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Als wann Name zu der Sache k\u00fcnt ein ander Art beytragen.", "tokens": ["Als", "wann", "Na\u00b7me", "zu", "der", "Sa\u00b7che", "k\u00fcnt", "ein", "an\u00b7der", "Art", "bey\u00b7tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PWAV", "NN", "APPR", "ART", "NN", "VVFIN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Ist lateinisch Venus Hure, wird Lustinne deutsch nicht fr\u00f6mer;", "tokens": ["Ist", "la\u00b7tei\u00b7nisch", "Ve\u00b7nus", "Hu\u00b7re", ",", "wird", "Lus\u00b7tin\u00b7ne", "deutsch", "nicht", "fr\u00f6\u00b7mer", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "NN", "NN", "$,", "VAFIN", "NN", "ADJD", "PTKNEG", "ADJD", "$."], "meter": "+-+-+-+--+--+-+-", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Ob ein Schuster nicht verstehet, was mit Venus meint ein R\u00f6mer,", "tokens": ["Ob", "ein", "Schus\u00b7ter", "nicht", "ver\u00b7ste\u00b7het", ",", "was", "mit", "Ve\u00b7nus", "meint", "ein", "R\u00f6\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PTKNEG", "VVFIN", "$,", "PRELS", "APPR", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.5": {"text": "Wird er fast noch minder wissen, was ein Deutscher mit Lustinne", "tokens": ["Wird", "er", "fast", "noch", "min\u00b7der", "wis\u00b7sen", ",", "was", "ein", "Deut\u00b7scher", "mit", "Lus\u00b7tin\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "ADV", "VVINF", "$,", "PRELS", "ART", "NN", "APPR", "NN"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.6": {"text": "F\u00fcr Verstand und Deutung f\u00fchrt. Wann wir Christen in dem Sinne", "tokens": ["F\u00fcr", "Ver\u00b7stand", "und", "Deu\u00b7tung", "f\u00fchrt", ".", "Wann", "wir", "Chris\u00b7ten", "in", "dem", "Sin\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "$.", "PWAV", "PPER", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.7": {"text": "Nicht der Heyden Wesen hausten, wurden wenig ihre Worte", "tokens": ["Nicht", "der", "Hey\u00b7den", "We\u00b7sen", "haus\u00b7ten", ",", "wur\u00b7den", "we\u00b7nig", "ih\u00b7re", "Wor\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKNEG", "ART", "ADJA", "NN", "VVFIN", "$,", "VAFIN", "PIS", "PPOSAT", "NN"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.8": {"text": "\u00c4rgern durch die blossen Namen, die so kennlich aller Orte.", "tokens": ["\u00c4r\u00b7gern", "durch", "die", "blos\u00b7sen", "Na\u00b7me