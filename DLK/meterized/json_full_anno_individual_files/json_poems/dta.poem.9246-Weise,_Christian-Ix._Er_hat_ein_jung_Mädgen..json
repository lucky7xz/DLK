{"dta.poem.9246": {"metadata": {"author": {"name": "Weise, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Ix.  \n Er hat ein jung M\u00e4dgen.", "genre": "Lyrik; Drama; Prosa", "period": "N.A.", "pub_year": "1701", "urn": "urn:nbn:de:kobv:b4-25043-0", "language": ["de:0.85", "no:0.14"], "booktitle": "Weise, Christian: \u00dcberfl\u00fc\u00dfige Gedancken Der gr\u00fcnenden jugend. Leipzig, 1701."}, "poem": {"stanza.1": {"line.1": {"text": "Beht ihr grossen jungfern fort/", "tokens": ["Beht", "ihr", "gros\u00b7sen", "jung\u00b7fern", "fort", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "ADJA", "PTKVZ", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und gedencket nicht ein wort/", "tokens": ["Und", "ge\u00b7den\u00b7cket", "nicht", "ein", "wort", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "ART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df ich mich um euch betr\u00fcbe/", "tokens": ["Da\u00df", "ich", "mich", "um", "euch", "be\u00b7tr\u00fc\u00b7be", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "APPR", "PPER", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dann mein sinn wird offenbahr/", "tokens": ["Dann", "mein", "sinn", "wird", "of\u00b7fen\u00b7bahr", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VAFIN", "ADJD", "$("], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.5": {"text": "Und das m\u00e4dgen das ich liebe/", "tokens": ["Und", "das", "m\u00e4d\u00b7gen", "das", "ich", "lie\u00b7be", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "PRELS", "PPER", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Geht nunmehr ins zw\u00f6lffte jahr.", "tokens": ["Geht", "nun\u00b7mehr", "ins", "zw\u00f6lff\u00b7te", "jahr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "2. Jhre zarte freundlichkeit", "tokens": ["Ih\u00b7re", "zar\u00b7te", "freund\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Spielt in keuscher sicherheit/", "tokens": ["Spielt", "in", "keu\u00b7scher", "si\u00b7cher\u00b7heit", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und bestrahlt die sch\u00f6nen wangen", "tokens": ["Und", "be\u00b7strahlt", "die", "sch\u00f6\u00b7nen", "wan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Durch dergleichen \u00fcberflu\u00df/", "tokens": ["Durch", "derg\u00b7lei\u00b7chen", "\u00fc\u00b7berf\u00b7lu\u00df", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIS", "PTKVZ", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Da\u00df ich \u00fcber dem verlangen", "tokens": ["Da\u00df", "ich", "\u00fc\u00b7ber", "dem", "ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Auch zum kinde werden mu\u00df.", "tokens": ["Auch", "zum", "kin\u00b7de", "wer\u00b7den", "mu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VAINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "3. Jhre jugend ist noch rein/", "tokens": ["Ih\u00b7re", "ju\u00b7gend", "ist", "noch", "rein", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "ADJD", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und bewahrt den glatten schein", "tokens": ["Und", "be\u00b7wahrt", "den", "glat\u00b7ten", "schein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "In der einfalt ihres hertzens/", "tokens": ["In", "der", "ein\u00b7falt", "ih\u00b7res", "hert\u00b7zens", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN", "$("], "meter": "+-++--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Andre lieben in den wind/", "tokens": ["And\u00b7re", "lie\u00b7ben", "in", "den", "wind", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "ART", "NE", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Welche schon des stillen schmertzens", "tokens": ["Wel\u00b7che", "schon", "des", "stil\u00b7len", "schmert\u00b7zens"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Aus der \u00fcbung kundig sind.", "tokens": ["Aus", "der", "\u00fc\u00b7bung", "kun\u00b7dig", "sind", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "4. Hofft man auf die rosen nicht/", "tokens": ["Hofft", "man", "auf", "die", "ro\u00b7sen", "nicht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPR", "ART", "NN", "PTKNEG", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Eh die gr\u00fcne knospe bricht/", "tokens": ["Eh", "die", "gr\u00fc\u00b7ne", "knos\u00b7pe", "bricht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ist sie aber aufgebrochen/", "tokens": ["Ist", "sie", "a\u00b7ber", "auf\u00b7ge\u00b7bro\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "VVPP", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wird man leicht des handels satt/", "tokens": ["Wird", "man", "leicht", "des", "han\u00b7dels", "satt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "ADJD", "ART", "NN", "ADJD", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Dann wer wei\u00df/ wer sie berochen/", "tokens": ["Dann", "wer", "wei\u00df", "/", "wer", "sie", "be\u00b7ro\u00b7chen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PWS", "VVFIN", "$(", "PWS", "PPER", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und zuvor begriffen hat.", "tokens": ["Und", "zu\u00b7vor", "be\u00b7grif\u00b7fen", "hat", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVPP", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "5. Drum/ ihr jungfern last mich gehn/", "tokens": ["Drum", "/", "ihr", "jung\u00b7fern", "last", "mich", "gehn", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$(", "PPOSAT", "ADJA", "VVFIN", "PPER", "VVINF", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist mein liebgen nicht so sch\u00f6n", "tokens": ["Ist", "mein", "lieb\u00b7gen", "nicht", "so", "sch\u00f6n"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPOSAT", "VVFIN", "PTKNEG", "ADV", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Als ein bild von sechzehn jahren/", "tokens": ["Als", "ein", "bild", "von", "sech\u00b7zehn", "jah\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPR", "CARD", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nun so darff ich auch die list", "tokens": ["Nun", "so", "darff", "ich", "auch", "die", "list"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VMFIN", "PPER", "ADV", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Mir zum schimpffe nicht erfahren.", "tokens": ["Mir", "zum", "schimpf\u00b7fe", "nicht", "er\u00b7fah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "VVFIN", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Da\u00df sie falsch und eckel ist.", "tokens": ["Da\u00df", "sie", "falsch", "und", "ec\u00b7kel", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "KON", "ADJD", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "6. Mangelt ihr am gliedern was/", "tokens": ["Man\u00b7gelt", "ihr", "am", "glie\u00b7dern", "was", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "PWS", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ach wie bald ersetzt sie das/", "tokens": ["Ach", "wie", "bald", "er\u00b7setzt", "sie", "das", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ADV", "VVFIN", "PPER", "ART", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ist doch die\u00df die beste freude/", "tokens": ["Ist", "doch", "die\u00df", "die", "bes\u00b7te", "freu\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PDS", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wann die jugend sachte bl\u00fcht/", "tokens": ["Wann", "die", "ju\u00b7gend", "sach\u00b7te", "bl\u00fcht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und man seiner augen-weide", "tokens": ["Und", "man", "sei\u00b7ner", "au\u00b7gen\u00b7wei\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PIS", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Untern h\u00e4nden wachsen sieht.", "tokens": ["Un\u00b7tern", "h\u00e4n\u00b7den", "wach\u00b7sen", "sieht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "VVINF", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "7. Nun das ist mein fester schlu\u00df/", "tokens": ["Nun", "das", "ist", "mein", "fes\u00b7ter", "schlu\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDS", "VAFIN", "PPOSAT", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und wofern ich warten mu\u00df/", "tokens": ["Und", "wo\u00b7fern", "ich", "war\u00b7ten", "mu\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVINF", "VMFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Will ich lieber jetzt in zeiten", "tokens": ["Will", "ich", "lie\u00b7ber", "jetzt", "in", "zei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ADV", "ADV", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nach der s\u00fcssen liebe gehn/", "tokens": ["Nach", "der", "s\u00fcs\u00b7sen", "lie\u00b7be", "gehn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "VVFIN", "VVINF", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Als da\u00df ich so gar von weiten", "tokens": ["Als", "da\u00df", "ich", "so", "gar", "von", "wei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "PPER", "ADV", "ADV", "APPR", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Soll mit meiner hoffnung stehn.", "tokens": ["Soll", "mit", "mei\u00b7ner", "hoff\u00b7nung", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}