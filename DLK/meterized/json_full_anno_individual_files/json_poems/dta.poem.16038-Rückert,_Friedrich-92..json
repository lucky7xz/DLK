{"dta.poem.16038": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "92.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mein Goldschmidt, in Geduld mu\u00dft du die Zeit erwarten;", "tokens": ["Mein", "Gold\u00b7schmidt", ",", "in", "Ge\u00b7duld", "mu\u00dft", "du", "die", "Zeit", "er\u00b7war\u00b7ten", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "APPR", "NN", "VMFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Knappen la\u00df im Berg erst machen ihre Fahrten.", "tokens": ["Die", "Knap\u00b7pen", "la\u00df", "im", "Berg", "erst", "ma\u00b7chen", "ih\u00b7re", "Fahr\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "ADV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Im H\u00fcttendampfe la\u00df Pochjungen wacker pochen,", "tokens": ["Im", "H\u00fct\u00b7ten\u00b7damp\u00b7fe", "la\u00df", "Poch\u00b7jun\u00b7gen", "wa\u00b7cker", "po\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVIMP", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und im Hochofen rein das Erz aus Schlacken kochen.", "tokens": ["Und", "im", "Hoch\u00b7o\u00b7fen", "rein", "das", "Erz", "aus", "Schla\u00b7cken", "ko\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "ADJD", "ART", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}}, "stanza.3": {"line.1": {"text": "Hier gilt die derbe Faust statt feiner Fingerspitze,", "tokens": ["Hier", "gilt", "die", "der\u00b7be", "Faust", "statt", "fei\u00b7ner", "Fin\u00b7ger\u00b7spit\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und vorarbeiten mu\u00df Handwerkerflei\u00df dem Witze.", "tokens": ["Und", "vor\u00b7ar\u00b7bei\u00b7ten", "mu\u00df", "Hand\u00b7wer\u00b7ker\u00b7flei\u00df", "dem", "Wit\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VMFIN", "NN", "ART", "NN", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Wo ihr Beruf erlischt, beginnet deine Sendung;", "tokens": ["Wo", "ihr", "Be\u00b7ruf", "er\u00b7lischt", ",", "be\u00b7gin\u00b7net", "dei\u00b7ne", "Sen\u00b7dung", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "VVPP", "$,", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sie liefern dir den Stoff, du gibst ihm die Vollendung.", "tokens": ["Sie", "lie\u00b7fern", "dir", "den", "Stoff", ",", "du", "gibst", "ihm", "die", "Vol\u00b7len\u00b7dung", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "$,", "PPER", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+--+--", "measure": "iambic.penta.relaxed"}}}}}