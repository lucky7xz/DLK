{"dta.poem.9951": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "60.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Auch den allertieffsten grund kan ich mit den f\u00fcssen gr\u00fcnden/", "tokens": ["Auch", "den", "al\u00b7ler\u00b7tieffs\u00b7ten", "grund", "kan", "ich", "mit", "den", "f\u00fcs\u00b7sen", "gr\u00fcn\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VMFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$("], "meter": "--+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Da ich doch mit meinem haupte fast bi\u00df an die wolcken geh.", "tokens": ["Da", "ich", "doch", "mit", "mei\u00b7nem", "haup\u00b7te", "fast", "bi\u00df", "an", "die", "wol\u00b7cken", "geh."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["KOUS", "PPER", "ADV", "APPR", "PPOSAT", "VVFIN", "ADV", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.3": {"text": "V\u00f6gel/ wilde thier und fisch/ sich in menge bey mir finden/", "tokens": ["V\u00f6\u00b7gel", "/", "wil\u00b7de", "thier", "und", "fisch", "/", "sich", "in", "men\u00b7ge", "bey", "mir", "fin\u00b7den", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "ADJA", "NN", "KON", "ADJD", "$(", "PRF", "APPR", "NN", "APPR", "PPER", "VVINF", "$("], "meter": "+-+-+-+-+--+-+-", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Und zu einer zeit so wohn ich in der erd und in der see.", "tokens": ["Und", "zu", "ei\u00b7ner", "zeit", "so", "wohn", "ich", "in", "der", "erd", "und", "in", "der", "see", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "KON", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}}}}