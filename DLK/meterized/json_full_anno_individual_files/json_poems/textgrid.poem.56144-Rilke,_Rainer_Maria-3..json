{"textgrid.poem.56144": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Spiegel noch nie hat man wissend beschrieben,", "tokens": ["Spie\u00b7gel", "noch", "nie", "hat", "man", "wis\u00b7send", "be\u00b7schrie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADV", "VAFIN", "PIS", "ADJD", "VVPP", "$,"], "meter": "+--++-+--+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "was ihr in euerem Wesen seid.", "tokens": ["was", "ihr", "in", "eu\u00b7e\u00b7rem", "We\u00b7sen", "seid", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ihr, wie mit lauter L\u00f6chern von Sieben", "tokens": ["Ihr", ",", "wie", "mit", "lau\u00b7ter", "L\u00f6\u00b7chern", "von", "Sie\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "$,", "PWAV", "APPR", "PIAT", "NN", "APPR", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "erf\u00fcllten Zwischenr\u00e4ume der Zeit.", "tokens": ["er\u00b7f\u00fcll\u00b7ten", "Zwi\u00b7schen\u00b7r\u00e4u\u00b7me", "der", "Zeit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.2": {"line.1": {"text": "Ihr, noch des leeren Saales Verschwender \u2013,", "tokens": ["Ihr", ",", "noch", "des", "lee\u00b7ren", "Saa\u00b7les", "Ver\u00b7schwen\u00b7der", "\u2013", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "$,",