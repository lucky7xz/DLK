{"dta.poem.7614": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "29.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn ihr Orakel wollt, sollt ihr Orakel h\u00f6ren;", "tokens": ["Wenn", "ihr", "O\u00b7ra\u00b7kel", "wollt", ",", "sollt", "ihr", "O\u00b7ra\u00b7kel", "h\u00f6\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VMFIN", "$,", "VMFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Geist ist \u00fcberall, man darf ihn nur beschw\u00f6ren.", "tokens": ["Der", "Geist", "ist", "\u00fc\u00b7be\u00b7rall", ",", "man", "darf", "ihn", "nur", "be\u00b7schw\u00f6\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "$,", "PIS", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "An wen die Welt glaubt, wer an sich glaubt, ist Prophet,", "tokens": ["An", "wen", "die", "Welt", "glaubt", ",", "wer", "an", "sich", "glaubt", ",", "ist", "Pro\u00b7phet", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PWS", "ART", "NN", "VVFIN", "$,", "PWS", "APPR", "PRF", "VVFIN", "$,", "VAFIN", "NN", "$,"], "meter": "+--+--+-+--+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Theurg und Philosoph, Apostel und Poet.", "tokens": ["Theurg", "und", "Phi\u00b7lo\u00b7soph", ",", "A\u00b7pos\u00b7tel", "und", "Po\u00b7et", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.3": {"line.1": {"text": "Denn einer ist der Geist, der in den vielen waltet,", "tokens": ["Denn", "ei\u00b7ner", "ist", "der", "Geist", ",", "der", "in", "den", "vie\u00b7len", "wal\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "ART", "NN", "$,", "PRELS", "APPR", "ART", "PIAT", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der nur die Fl\u00fcgel nicht in allen gleich entfaltet.", "tokens": ["Der", "nur", "die", "Fl\u00fc\u00b7gel", "nicht", "in", "al\u00b7len", "gleich", "ent\u00b7fal\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "PTKNEG", "APPR", "PIAT", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Die Raupen sehn erstaunt den Schmetterling sich wiegen,", "tokens": ["Die", "Rau\u00b7pen", "sehn", "er\u00b7staunt", "den", "Schmet\u00b7ter\u00b7ling", "sich", "wie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VVFIN", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und denken nicht im Traum, da\u00df sie auch sollten fliegen.", "tokens": ["Und", "den\u00b7ken", "nicht", "im", "Traum", ",", "da\u00df", "sie", "auch", "soll\u00b7ten", "flie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "APPRART", "NN", "$,", "KOUS", "PPER", "ADV", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Das Raupenvolk der Zeit ist zur Verwandlung reif,", "tokens": ["Das", "Rau\u00b7pen\u00b7volk", "der", "Zeit", "ist", "zur", "Ver\u00b7wand\u00b7lung", "reif", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VAFIN", "APPRART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es tr\u00e4gt sein Todtenkreuz im falben R\u00fcckenstreif.", "tokens": ["Es", "tr\u00e4gt", "sein", "Tod\u00b7ten\u00b7kreuz", "im", "fal\u00b7ben", "R\u00fc\u00b7cken\u00b7streif", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Sie freut der Bl\u00e4tterfra\u00df nicht mehr, des sie sich freuten,", "tokens": ["Sie", "freut", "der", "Bl\u00e4t\u00b7ter\u00b7fra\u00df", "nicht", "mehr", ",", "des", "sie", "sich", "freu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKNEG", "ADV", "$,", "PRELS", "PPER", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es treibt sie innre Qual noch einmal sich zu h\u00e4uten.", "tokens": ["Es", "treibt", "sie", "inn\u00b7re", "Qual", "noch", "ein\u00b7mal", "sich", "zu", "h\u00e4u\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJA", "NN", "ADV", "ADV", "PRF", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Sie wechseln eine Haut, und bleiben Raupen noch,", "tokens": ["Sie", "wech\u00b7seln", "ei\u00b7ne", "Haut", ",", "und", "blei\u00b7ben", "Rau\u00b7pen", "noch", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "KON", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und wechselten sie zehn, sie blieben Raupen doch.", "tokens": ["Und", "wech\u00b7sel\u00b7ten", "sie", "zehn", ",", "sie", "blie\u00b7ben", "Rau\u00b7pen", "doch", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "CARD", "$,", "PPER", "VVFIN", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Von gift'gen Wespen sind die meisten angestochen,", "tokens": ["Von", "gift'\u00b7gen", "Wes\u00b7pen", "sind", "die", "meis\u00b7ten", "an\u00b7ge\u00b7sto\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VAFIN", "ART", "PIAT", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Lebendig innen aufgezehrt an Mark und Knochen.", "tokens": ["Le\u00b7ben\u00b7dig", "in\u00b7nen", "auf\u00b7ge\u00b7zehrt", "an", "Mark", "und", "Kno\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "VVPP", "APPR", "NN", "KON", "NN", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}}, "stanza.9": {"line.1": {"text": "Und wann aus ihnen schon frei werden soll der Sohn", "tokens": ["Und", "wann", "aus", "ih\u00b7nen", "schon", "frei", "wer\u00b7den", "soll", "der", "Sohn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "APPR", "PPER", "ADV", "ADJD", "VAINF", "VMFIN", "ART", "NN"], "meter": "-+-+-+++-+-+", "measure": "unknown.measure.septa"}, "line.2": {"text": "Des Himmels, fliegt mit Hohn ein Schwarm Geschmei\u00df davon.", "tokens": ["Des", "Him\u00b7mels", ",", "fliegt", "mit", "Hohn", "ein", "Schwarm", "Ge\u00b7schmei\u00df", "da\u00b7von", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "APPR", "NN", "ART", "NN", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.10": {"line.1": {"text": "Euch, zahme R\u00e4upchen, hier hat man auf Maulbeerbl\u00e4tter", "tokens": ["Euch", ",", "zah\u00b7me", "R\u00e4upc\u00b7hen", ",", "hier", "hat", "man", "auf", "Maul\u00b7be\u00b7er\u00b7bl\u00e4t\u00b7ter"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "$,", "ADJA", "NN", "$,", "ADV", "VAFIN", "PIS", "APPR", "NN"], "meter": "-+-+--+--+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Gesetzt, vor Hagelschlag gesichert und Sturmwetter.", "tokens": ["Ge\u00b7setzt", ",", "vor", "Ha\u00b7gel\u00b7schlag", "ge\u00b7si\u00b7chert", "und", "Sturm\u00b7wet\u00b7ter", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "APPR", "NN", "VVPP", "KON", "NN", "$."], "meter": "-+-+-+--+-+--", "measure": "iambic.penta.relaxed"}}, "stanza.11": {"line.1": {"text": "Jetzt wollet ihr mit Ruh in eur Gespinnst euch spinnen;", "tokens": ["Jetzt", "wol\u00b7let", "ihr", "mit", "Ruh", "in", "eur", "Ge\u00b7spinnst", "euch", "spin\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "NN", "APPR", "PPOSAT", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dem hei\u00dfen Wassertod nicht werdet ihr entrinnen.", "tokens": ["Dem", "hei\u00b7\u00dfen", "Was\u00b7ser\u00b7tod", "nicht", "wer\u00b7det", "ihr", "ent\u00b7rin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKNEG", "VAFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.12": {"line.1": {"text": "Denn billig wollen sich die Hirten, die euch weiden,", "tokens": ["Denn", "bil\u00b7lig", "wol\u00b7len", "sich", "die", "Hir\u00b7ten", ",", "die", "euch", "wei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VMFIN", "PRF", "ART", "NN", "$,", "PRELS", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nun gegen Winterfrost in eure Seide kleiden.", "tokens": ["Nun", "ge\u00b7gen", "Win\u00b7ter\u00b7frost", "in", "eu\u00b7re", "Sei\u00b7de", "klei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.13": {"line.1": {"text": "Die wilden Raupen dort, im Grase nicht bemerkt,", "tokens": ["Die", "wil\u00b7den", "Rau\u00b7pen", "dort", ",", "im", "Gra\u00b7se", "nicht", "be\u00b7merkt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "$,", "APPRART", "NN", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In Freiheit wachsen sie, vom Hauch der Nacht gest\u00e4rkt.", "tokens": ["In", "Frei\u00b7heit", "wach\u00b7sen", "sie", ",", "vom", "Hauch", "der", "Nacht", "ge\u00b7st\u00e4rkt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "$,", "APPRART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.14": {"line.1": {"text": "Als Puppen kn\u00fcpfen sie sich auf am lichten Faden;", "tokens": ["Als", "Pup\u00b7pen", "kn\u00fcp\u00b7fen", "sie", "sich", "auf", "am", "lich\u00b7ten", "Fa\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVFIN", "PPER", "PRF", "APPR", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den goldnen Masken wird nicht Winterk\u00e4lte schaden.", "tokens": ["Den", "gold\u00b7nen", "Mas\u00b7ken", "wird", "nicht", "Win\u00b7ter\u00b7k\u00e4l\u00b7te", "scha\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PTKNEG", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.15": {"line.1": {"text": "Kalt wird der Winter seyn, erfroren werden stehn", "tokens": ["Kalt", "wird", "der", "Win\u00b7ter", "seyn", ",", "er\u00b7fro\u00b7ren", "wer\u00b7den", "stehn"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VAFIN", "ART", "NN", "VAINF", "$,", "VVINF", "VAFIN", "VVINF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Viel stolze Nasen, die aus seidnen Kr\u00e4gen sehn.", "tokens": ["Viel", "stol\u00b7ze", "Na\u00b7sen", ",", "die", "aus", "seid\u00b7nen", "Kr\u00e4\u00b7gen", "sehn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$,", "PRELS", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.16": {"line.1": {"text": "Vom ersten Fr\u00fchlingsstral belebt, wird dann entschweben", "tokens": ["Vom", "ers\u00b7ten", "Fr\u00fch\u00b7lings\u00b7stral", "be\u00b7lebt", ",", "wird", "dann", "ent\u00b7schwe\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "VVPP", "$,", "VAFIN", "ADV", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Zukunft Schmetterling; Heil denen, die's erleben!", "tokens": ["Der", "Zu\u00b7kunft", "Schmet\u00b7ter\u00b7ling", ";", "Heil", "de\u00b7nen", ",", "die's", "er\u00b7le\u00b7ben", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$.", "NN", "PDS", "$,", "PDS", "VVINF", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}}}}}