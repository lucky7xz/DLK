{"dta.poem.18297": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "An die W\u00f6lfflinge.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1886", "urn": "urn:nbn:de:kobv:b4-200905192665", "language": ["de:0.99"], "booktitle": "Holz, Arno: Das Buch der Zeit. Lieder eines Modernen. Z\u00fcrich, 1886."}, "poem": {"stanza.1": {"line.1": {"text": "Allw\u00f6chentlich ein Buch von zwanzig Bogen!", "tokens": ["All\u00b7w\u00f6\u00b7chent\u00b7lich", "ein", "Buch", "von", "zwan\u00b7zig", "Bo\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPR", "CARD", "NN", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Wir aber thun stets unsre alte Frage:", "tokens": ["Wir", "a\u00b7ber", "thun", "stets", "uns\u00b7re", "al\u00b7te", "Fra\u00b7ge", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVINF", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Habt ihr euch immer noch nicht ausgelogen?", "tokens": ["Habt", "ihr", "euch", "im\u00b7mer", "noch", "nicht", "aus\u00b7ge\u00b7lo\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "ADV", "ADV", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Seht, eure Herzen wickelt ihr in Watte", "tokens": ["Seht", ",", "eu\u00b7re", "Her\u00b7zen", "wi\u00b7ckelt", "ihr", "in", "Wat\u00b7te"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "PPOSAT", "NN", "VVFIN", "PPER", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und malt drauf zierlich: Vorsicht! Porzellan!", "tokens": ["Und", "malt", "drauf", "zier\u00b7lich", ":", "Vor\u00b7sicht", "!", "Por\u00b7zel\u00b7lan", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "PAV", "ADJD", "$.", "NN", "$.", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und ist auch manches \u201eVater, Mensch und Gatte\u201c,", "tokens": ["Und", "ist", "auch", "man\u00b7ches", "\u201e", "Va\u00b7ter", ",", "Mensch", "und", "Gat\u00b7te", "\u201c", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VAFIN", "ADV", "PIS", "$(", "NN", "$,", "NN", "KON", "NN", "$(", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sein Lumpenpack ist jedenfalls im Thran.", "tokens": ["Sein", "Lum\u00b7pen\u00b7pack", "ist", "je\u00b7den\u00b7falls", "im", "Thran", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "O, werft ins Feuer euer Flickenkleid,", "tokens": ["O", ",", "werft", "ins", "Feu\u00b7er", "eu\u00b7er", "Fli\u00b7cken\u00b7kleid", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "APPRART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Am n\u00e4chsten Stein zertr\u00fcmmert euern Psalter,", "tokens": ["Am", "n\u00e4chs\u00b7ten", "Stein", "zer\u00b7tr\u00fcm\u00b7mert", "eu\u00b7ern", "Psal\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Denn uns \u201eModernen\u201c liegt die Bronzezeit", "tokens": ["Denn", "uns", "\u201e", "Mo\u00b7der\u00b7nen", "\u201c", "liegt", "die", "Bron\u00b7ze\u00b7zeit"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "$(", "NN", "$(", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wahrhaftig n\u00e4her als das Mittelalter!", "tokens": ["Wahr\u00b7haf\u00b7tig", "n\u00e4\u00b7her", "als", "das", "Mit\u00b7tel\u00b7al\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}