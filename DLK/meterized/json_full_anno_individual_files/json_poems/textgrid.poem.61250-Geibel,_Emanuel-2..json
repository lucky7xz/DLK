{"textgrid.poem.61250": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich bin der Sturm, der f\u00e4hrt dem Norden zu,", "tokens": ["Ich", "bin", "der", "Sturm", ",", "der", "f\u00e4hrt", "dem", "Nor\u00b7den", "zu", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PRELS", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Du bist die mondbegl\u00e4nzte Meeresruh' -", "tokens": ["Du", "bist", "die", "mond\u00b7be\u00b7gl\u00e4nz\u00b7te", "Mee\u00b7res\u00b7ruh'"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wie stimmt ein solches Ich zu solchem Du!", "tokens": ["Wie", "stimmt", "ein", "sol\u00b7ches", "Ich", "zu", "sol\u00b7chem", "Du", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "ADJA", "PPER", "APPR", "PIAT", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Du bist der Strahl, der sich auf Lilien wiegt,", "tokens": ["Du", "bist", "der", "Strahl", ",", "der", "sich", "auf", "Li\u00b7li\u00b7en", "wiegt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PRELS", "PRF", "APPR", "NE", "VVFIN", "$,"], "meter": "-+-+--++--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Der Hagel ich, der aus der Wolke fliegt -", "tokens": ["Der", "Ha\u00b7gel", "ich", ",", "der", "aus", "der", "Wol\u00b7ke", "fliegt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PPER", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "O ew'ge Kluft, die zwischen beiden liegt!", "tokens": ["O", "ew'\u00b7ge", "Kluft", ",", "die", "zwi\u00b7schen", "bei\u00b7den", "liegt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "PRELS", "APPR", "PIAT", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ich unstet, wild, der Erde d\u00fcstrer Gast,", "tokens": ["Ich", "un\u00b7stet", ",", "wild", ",", "der", "Er\u00b7de", "d\u00fcst\u00b7rer", "Gast", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADJD", "$,", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Du himmlisch heiter, wie die Engel fast -", "tokens": ["Du", "himm\u00b7lisch", "hei\u00b7ter", ",", "wie", "die", "En\u00b7gel", "fast"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADJD", "ADJD", "$,", "PWAV", "ART", "NN", "ADV", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Nun zeig', o Liebe, da\u00df du Allmacht hast!", "tokens": ["Nun", "zeig'", ",", "o", "Lie\u00b7be", ",", "da\u00df", "du", "All\u00b7macht", "hast", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "FM", "NN", "$,", "KOUS", "PPER", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ich bin der Sturm, der f\u00e4hrt dem Norden zu,", "tokens": ["Ich", "bin", "der", "Sturm", ",", "der", "f\u00e4hrt", "dem", "Nor\u00b