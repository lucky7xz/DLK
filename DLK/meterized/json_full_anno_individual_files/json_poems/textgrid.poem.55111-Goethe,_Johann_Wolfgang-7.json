{"textgrid.poem.55111": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "7", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "War uners\u00e4ttlich nach viel tausend K\u00fcssen,", "tokens": ["War", "un\u00b7er\u00b7s\u00e4tt\u00b7lich", "nach", "viel", "tau\u00b7send", "K\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "APPR", "PIAT", "CARD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und mu\u00dft mit ", "tokens": ["Und", "mu\u00dft", "mit"], "token_info": ["word", "word", "word"], "pos": ["KON", "VMFIN", "APPR"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Nach herber Trennung tiefempfundnem Leiden", "tokens": ["Nach", "her\u00b7ber", "Tren\u00b7nung", "tie\u00b7femp\u00b7fund\u00b7nem", "Lei\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "War mir das Ufer, dem ich mich entrissen,", "tokens": ["War", "mir", "das", "U\u00b7fer", ",", "dem", "ich", "mich", "ent\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "$,", "PRELS", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Mit Wohnungen, mit Bergen, H\u00fcgeln, Fl\u00fcssen,", "tokens": ["Mit", "Woh\u00b7nun\u00b7gen", ",", "mit", "Ber\u00b7gen", ",", "H\u00fc\u00b7geln", ",", "Fl\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "$,", "APPR", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Solang ich's deutlich sah, ein Schatz der Freuden;", "tokens": ["So\u00b7lang", "ich's", "deut\u00b7lich", "sah", ",", "ein", "Schatz", "der", "Freu\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "ADJD", "VVFIN", "$,", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Zuletzt im Blauen blieb ein Augenweiden", "tokens": ["Zu\u00b7letzt", "im", "Blau\u00b7en", "blieb", "ein", "Au\u00b7gen\u00b7wei\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "An fernentwichnen lichten Finsternissen.", "tokens": ["An", "fer\u00b7nent\u00b7wich\u00b7nen", "lich\u00b7ten", "Fins\u00b7ter\u00b7nis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADJA", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}, "stanza.3": {"line.1": {"text": "Und endlich, als das Meer den Blick umgrenzte,", "tokens": ["Und", "end\u00b7lich", ",", "als", "das", "Meer", "den", "Blick", "um\u00b7grenz\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "KOUS", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Fiel mir zur\u00fcck ins Herz mein hei\u00df Verlangen;", "tokens": ["Fiel", "mir", "zu\u00b7r\u00fcck", "ins", "Herz", "mein", "hei\u00df", "Ver\u00b7lan\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "APPRART", "NN", "PPOSAT", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ich suchte mein Verlornes gar verdrossen.", "tokens": ["Ich", "such\u00b7te", "mein", "Ver\u00b7lor\u00b7nes", "gar", "ver\u00b7dros\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Da war es gleich, als ob der Himmel gl\u00e4nzte;", "tokens": ["Da", "war", "es", "gleich", ",", "als", "ob", "der", "Him\u00b7mel", "gl\u00e4nz\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "$,", "KOKOM", "KOUS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mir schien, als w\u00e4re nichts mir, nichts entgangen,", "tokens": ["Mir", "schien", ",", "als", "w\u00e4\u00b7re", "nichts", "mir", ",", "nichts", "ent\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOKOM", "VAFIN", "PIS", "PPER", "$,", "PIS", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Als h\u00e4tt ich alles, was ich je genossen.", "tokens": ["Als", "h\u00e4tt", "ich", "al\u00b7les", ",", "was", "ich", "je", "ge\u00b7nos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPER", "PIS", "$,", "PWS", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "War uners\u00e4ttlich nach viel tausend K\u00fcssen,", "tokens": ["War", "un\u00b7er\u00b7s\u00e4tt\u00b7lich", "nach", "viel", "tau\u00b7send", "K\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "APPR", "PIAT", "CARD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "