{"dta.poem.22694": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "244. Die Unerh\u00f6rte Verkehrung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1657", "urn": "urn:nbn:de:kobv:b4-200905198292", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Geistreiche Sinn- vnd Schlussrime. Wien, 1657."}, "poem": {"stanza.1": {"line.1": {"text": "Es kehrt sich alles umb: die Burg ist in der H\u00f6le/", "tokens": ["Es", "kehrt", "sich", "al\u00b7les", "umb", ":", "die", "Burg", "ist", "in", "der", "H\u00f6\u00b7le", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PIS", "PTKVZ", "$.", "ART", "NN", "VAFIN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Krippe wird ein Thron/ der Tag kombt in der", "tokens": ["Die", "Krip\u00b7pe", "wird", "ein", "Thron", "/", "der", "Tag", "kombt", "in", "der"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$(", "ART", "NN", "VVFIN", "APPR", "ART"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nacht/", "tokens": ["Nacht", "/"], "token_info": ["word", "punct"], "pos": ["NN", "$("], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Die Jungfrau bringt ein Kind: Ach Mensch bi\u00df", "tokens": ["Die", "Jung\u00b7frau", "bringt", "ein", "Kind", ":", "Ach", "Mensch", "bi\u00df"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$.", "ITJ", "NN", "APPR"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "auch bedacht/", "tokens": ["auch", "be\u00b7dacht", "/"], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVPP", "$("], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Da\u00df sich verkehre wol/ dein Hertze Geist und Seele.", "tokens": ["Da\u00df", "sich", "ver\u00b7keh\u00b7re", "wol", "/", "dein", "Hert\u00b7ze", "Geist", "und", "See\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "VVFIN", "ADV", "$(", "PPOSAT", "VVFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}