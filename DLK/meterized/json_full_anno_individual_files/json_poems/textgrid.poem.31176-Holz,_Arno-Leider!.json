{"textgrid.poem.31176": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "Leider!", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Welt ist heute verteufelt praktisch,", "tokens": ["Die", "Welt", "ist", "heu\u00b7te", "ver\u00b7teu\u00b7felt", "prak\u00b7tisch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "VVFIN", "ADJD", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Verteufelt praktisch mit Mann und Maus,", "tokens": ["Ver\u00b7teu\u00b7felt", "prak\u00b7tisch", "mit", "Mann", "und", "Maus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und selbst die neun Musen sehen didaktisch", "tokens": ["Und", "selbst", "die", "neun", "Mu\u00b7sen", "se\u00b7hen", "di\u00b7dak\u00b7tisch"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "CARD", "NN", "VVFIN", "PPER"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wie englische Gouvernanten aus!", "tokens": ["Wie", "eng\u00b7li\u00b7sche", "Gou\u00b7ver\u00b7nan\u00b7ten", "aus", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Die Rosen verbl\u00fchn und der Wein versauert,", "tokens": ["Die", "Ro\u00b7sen", "ver\u00b7bl\u00fchn", "und", "der", "Wein", "ver\u00b7sau\u00b7ert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "KON", "ART", "NN", "VVPP", "$,"], "meter": "-+--+--+-+-