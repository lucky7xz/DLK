{"textgrid.poem.43019": {"metadata": {"author": {"name": "Aston, Louise", "birth": "N.A.", "death": "N.A."}, "title": "7. An George Sand", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O naht mit Lorbeerkr\u00e4nzen, naht mit Palmen!", "tokens": ["O", "naht", "mit", "Lor\u00b7beer\u00b7kr\u00e4n\u00b7zen", ",", "naht", "mit", "Pal\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "NN", "$,", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Freiheit Majest\u00e4t ist neu erwacht;", "tokens": ["Der", "Frei\u00b7heit", "Ma\u00b7jes\u00b7t\u00e4t", "ist", "neu", "er\u00b7wacht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ein Evangelium kam \u00fcber Nacht,", "tokens": ["Ein", "E\u00b7van\u00b7ge\u00b7li\u00b7um", "kam", "\u00fc\u00b7ber", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "+-----+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Herniederrauschend in Gewitterpsalmen;", "tokens": ["Her\u00b7nie\u00b7der\u00b7rau\u00b7schend", "in", "Ge\u00b7wit\u00b7ter\u00b7psal\u00b7men", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und, was vom alten Wahn umnachtet", "tokens": ["Und", ",", "was", "vom", "al\u00b7ten", "Wahn", "um\u00b7nach\u00b7tet"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "$,", "PRELS", "APPRART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Nach Rettung und Erl\u00f6sung schmachtet:", "tokens": ["Nach", "Ret\u00b7tung", "und", "Er\u00b7l\u00f6\u00b7sung", "schmach\u00b7tet", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Das eile zu des neuen Geistes Fahnen!", "tokens": ["Das", "ei\u00b7le", "zu", "des", "neu\u00b7en", "Geis\u00b7tes", "Fah\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Das streu' ihm Blumen auf die Siegerbahnen!", "tokens": ["Das", "streu'", "ihm", "Blu\u00b7men", "auf", "die", "Sie\u00b7ger\u00b7bah\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Nicht Jeanne d'Arc mit Frankreichs Heldens\u00f6hnen", "tokens": ["Nicht", "Jean\u00b7ne", "d'\u00b7A\u00b7rc", "mit", "Fran\u00b7kreichs", "Hel\u00b7den\u00b7s\u00f6h\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "NE", "NE", "APPR", "NE", "NN"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Hat sich dem neuen, heil'gen Kampf geweiht;", "tokens": ["Hat", "sich", "dem", "neu\u00b7en", ",", "heil'\u00b7gen", "Kampf", "ge\u00b7weiht", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PRF", "ART", "ADJA", "$,", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Nicht K\u00f6nigen, nicht V\u00f6lkern gilt der Streit:", "tokens": ["Nicht", "K\u00f6\u00b7ni\u00b7gen", ",", "nicht", "V\u00f6l\u00b7kern", "gilt", "der", "Streit", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "$,", "PTKNEG", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Den ", "tokens": ["Den"], "token_info": ["word"], "pos": ["ART"], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Nicht winkt der Andacht Lebenssonne,", "tokens": ["Nicht", "winkt", "der", "An\u00b7dacht", "Le\u00b7bens\u00b7son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Das Bild der himmlischen Madonne;", "tokens": ["Das", "Bild", "der", "himm\u00b7li\u00b7schen", "Ma\u00b7don\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Ein and'res Bild wird sch\u00fctzend uns umschweben,", "tokens": ["Ein", "an\u00b7d'\u00b7res", "Bild", "wird", "sch\u00fct\u00b7zend", "uns", "um\u00b7schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADJD", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.8": {"text": "Aus andern Z\u00fcgen spricht ein and'res Leben.", "tokens": ["Aus", "an\u00b7dern", "Z\u00fc\u00b7gen", "spricht", "ein", "an\u00b7d'\u00b7res", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Mag jener Traum die ", "tokens": ["Mag", "je\u00b7ner", "Traum", "die"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "PDAT", "NN", "ART"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "L\u00e4ngst schwand dahin der Heil'gen Wundermacht.", "tokens": ["L\u00e4ngst", "schwand", "da\u00b7hin", "der", "Heil'\u00b7gen", "Wun\u00b7der\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PAV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Es ziehn die ", "tokens": ["Es", "ziehn", "die"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Es gilt des ", "tokens": ["Es", "gilt", "des"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "Empor, aus trauriger Beth\u00f6rung!", "tokens": ["Em\u00b7por", ",", "aus", "trau\u00b7ri\u00b7ger", "Be\u00b7th\u00f6\u00b7rung", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Empor, in heiliger Emp\u00f6rung!", "tokens": ["Em\u00b7por", ",", "in", "hei\u00b7li\u00b7ger", "Em\u00b7p\u00f6\u00b7rung", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Ein Heldenweib, mit flammenden Panieren,", "tokens": ["Ein", "Hel\u00b7den\u00b7weib", ",", "mit", "flam\u00b7men\u00b7den", "Pa\u00b7nie\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Wird euch zum Sieg, wird euch zur Freiheit f\u00fchren!", "tokens": ["Wird", "euch", "zum", "Sieg", ",", "wird", "euch", "zur", "Frei\u00b7heit", "f\u00fch\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPRART", "NN", "$,", "VAFIN", "PPER", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Auf ihren Bannern gl\u00e4nzt im Morgenlichte", "tokens": ["Auf", "ih\u00b7ren", "Ban\u00b7nern", "gl\u00e4nzt", "im", "Mor\u00b7gen\u00b7lich\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das ", "tokens": ["Das"], "token_info": ["word"], "pos": ["PDS"], "meter": "-", "measure": "single.down"}, "line.3": {"text": "Das nur dem eig'nen ", "tokens": ["Das", "nur", "dem", "eig'\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "ADV", "ART", "ADJA"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Dem Losungswort der neuen Weltgeschichte!", "tokens": ["Dem", "Lo\u00b7sungs\u00b7wort", "der", "neu\u00b7en", "Welt\u00b7ge\u00b7schich\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Das freie Weib, es schm\u00fcckt die Fahne!", "tokens": ["Das", "frei\u00b7e", "Weib", ",", "es", "schm\u00fcckt", "die", "Fah\u00b7ne", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Von ", "tokens": ["Von"], "token_info": ["word"], "pos": ["APPR"], "meter": "-", "measure": "single.down"}, "line.7": {"text": "Dem Vater Wahn mit seiner Tochter S\u00fcnde,", "tokens": ["Dem", "Va\u00b7ter", "Wahn", "mit", "sei\u00b7ner", "Toch\u00b7ter", "S\u00fcn\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Dem bl\u00f6den Vater mit dem bl\u00f6den Kinde.", "tokens": ["Dem", "bl\u00f6\u00b7den", "Va\u00b7ter", "mit", "dem", "bl\u00f6\u00b7den", "Kin\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "H\u00f6rt ihr des neuen Geistes lautes Mahnen?", "tokens": ["H\u00f6rt", "ihr", "des", "neu\u00b7en", "Geis\u00b7tes", "lau\u00b7tes", "Mah\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "O, nicht der ", "tokens": ["O", ",", "nicht", "der"], "token_info": ["word", "punct", "word", "word"], "pos": ["NE", "$,", "PTKNEG", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Zum Himmel klagend dringt der Schmerzensschrei,", "tokens": ["Zum", "Him\u00b7mel", "kla\u00b7gend", "dringt", "der", "Schmer\u00b7zens\u00b7schrei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der H\u00fclferuf von hundert ", "tokens": ["Der", "H\u00fcl\u00b7fe\u00b7ruf", "von", "hun\u00b7dert"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "CARD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Soll ich mich spr\u00f6dem Stolze f\u00fcgen,", "tokens": ["Soll", "ich", "mich", "spr\u00f6\u00b7dem", "Stol\u00b7ze", "f\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Als Lelia mein Herz besiegen?", "tokens": ["Als", "Le\u00b7lia", "mein", "Herz", "be\u00b7sie\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Soll ich, Pulcherien gleich, dem Wahn entsagen,", "tokens": ["Soll", "ich", ",", "Pulc\u00b7he\u00b7ri\u00b7en", "gleich", ",", "dem", "Wahn", "ent\u00b7sa\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "$,", "NN", "ADV", "$,", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.8": {"text": "Das Gl\u00fcck zugleich nur mit der Schande tragen?", "tokens": ["Das", "Gl\u00fcck", "zu\u00b7gleich", "nur", "mit", "der", "Schan\u00b7de", "tra\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Doch all' der Kampf, der in der Brust der Frauen", "tokens": ["Doch", "all'", "der", "Kampf", ",", "der", "in", "der", "Brust", "der", "Frau\u00b7en"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "ART", "NN", "$,", "PRELS", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So schmerzensreich, doch zukunftsvoll, sich regt,", "tokens": ["So", "schmer\u00b7zens\u00b7reich", ",", "doch", "zu\u00b7kunfts\u00b7voll", ",", "sich", "regt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADV", "ADJD", "$,", "PRF", "VVFIN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Der schon im Scho\u00df ein sch\u00f6n'res Leben tr\u00e4gt,", "tokens": ["Der", "schon", "im", "Scho\u00df", "ein", "sch\u00f6n'\u00b7res", "Le\u00b7ben", "tr\u00e4gt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPRART", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Das wir nur ahnen, nur prophetisch schauen: \u2013", "tokens": ["Das", "wir", "nur", "ah\u00b7nen", ",", "nur", "pro\u00b7phe\u00b7tisch", "schau\u00b7en", ":", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ART", "PPER", "ADV", "VVINF", "$,", "ADV", "ADJD", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "In lebenskr\u00e4ftige Gestalten!", "tokens": ["In", "le\u00b7bens\u00b7kr\u00e4f\u00b7ti\u00b7ge", "Ge\u00b7stal\u00b7ten", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Den Kampf der ", "tokens": ["Den", "Kampf", "der"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "Vermachtest du den sp\u00e4testen Geschlechtern!", "tokens": ["Ver\u00b7mach\u00b7test", "du", "den", "sp\u00e4\u00b7tes\u00b7ten", "Ge\u00b7schlech\u00b7tern", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Ber\u00fchrt von deines Geistes Zauberstrahl,", "tokens": ["Be\u00b7r\u00fchrt", "von", "dei\u00b7nes", "Geis\u00b7tes", "Zau\u00b7ber\u00b7strahl", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Kann k\u00fchner ich der Menge Spott verachten:", "tokens": ["Kann", "k\u00fch\u00b7ner", "ich", "der", "Men\u00b7ge", "Spott", "ver\u00b7ach\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADJA", "PPER", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mag sie vor gold'nen K\u00e4lbern beten,", "tokens": ["Mag", "sie", "vor", "gold'\u00b7nen", "K\u00e4l\u00b7bern", "be\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und frevelnd l\u00e4stern die Propheten;", "tokens": ["Und", "fre\u00b7velnd", "l\u00e4s\u00b7tern", "die", "Pro\u00b7phe\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "O naht mit Lorbeerkr\u00e4nzen, naht mit Palmen!", "tokens": ["O", "naht", "mit", "Lor\u00b7beer\u00b7kr\u00e4n\u00b7zen", ",", "naht", "mit", "Pal\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "NN", "$,", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Freiheit Majest\u00e4t ist neu erwacht;", "tokens": ["Der", "Frei\u00b7heit", "Ma\u00b7jes\u00b7t\u00e4t", "ist", "neu", "er\u00b7wacht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ein Evangelium kam \u00fcber Nacht,", "tokens": ["Ein", "E\u00b7van\u00b