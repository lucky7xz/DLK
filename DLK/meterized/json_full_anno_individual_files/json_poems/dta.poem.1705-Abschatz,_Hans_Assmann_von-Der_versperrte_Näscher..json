{"dta.poem.1705": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Der versperrte N\u00e4scher.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Ich war nach fremder Kost verstohlen ausgegangen/", "tokens": ["Ich", "war", "nach", "frem\u00b7der", "Kost", "ver\u00b7stoh\u00b7len", "aus\u00b7ge\u00b7gan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ADJA", "NN", "VVINF", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ward dr\u00fcber unverhofft verschlossen und gefangen/", "tokens": ["Ward", "dr\u00fc\u00b7ber", "un\u00b7ver\u00b7hofft", "ver\u00b7schlos\u00b7sen", "und", "ge\u00b7fan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PAV", "ADJD", "VVPP", "KON", "ADJD", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Dem kleinen Leibe ward der grosse Raum fast klein/", "tokens": ["Dem", "klei\u00b7nen", "Lei\u00b7be", "ward", "der", "gros\u00b7se", "Raum", "fast", "klein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ART", "ADJA", "NN", "ADV", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Doch must ein Loch zu Spei\u00df und Tranck mein Trost noch seyn.", "tokens": ["Doch", "must", "ein", "Loch", "zu", "Spei\u00df", "und", "Tranck", "mein", "Trost", "noch", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "NN", "APPR", "NN", "KON", "NN", "PPOSAT", "NN", "ADV", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wann jeder N\u00e4scher so solt jedern Bissen b\u00fcssen/", "tokens": ["Wann", "je\u00b7der", "N\u00e4\u00b7scher", "so", "solt", "je\u00b7dern", "Bis\u00b7sen", "b\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "ADV", "VMFIN", "PIAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Was w\u00fcrde Weisens Buch von so viel N\u00e4schern wissen!", "tokens": ["Was", "w\u00fcr\u00b7de", "Wei\u00b7sens", "Buch", "von", "so", "viel", "N\u00e4\u00b7schern", "wis\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "NN", "NN", "APPR", "ADV", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Doch macht Gel\u00fcck und Gunst viel schlimme N\u00e4scher frey:", "tokens": ["Doch", "macht", "Ge\u00b7l\u00fcck", "und", "Gunst", "viel", "schlim\u00b7me", "N\u00e4\u00b7scher", "frey", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "KON", "NN", "PIAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Drum bleibt die gantze Welt voll List und N\u00e4scherey.", "tokens": ["Drum", "bleibt", "die", "gant\u00b7ze", "Welt", "voll", "List", "und", "N\u00e4\u00b7sche\u00b7rey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "ADJA", "NN", "ADJD", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}