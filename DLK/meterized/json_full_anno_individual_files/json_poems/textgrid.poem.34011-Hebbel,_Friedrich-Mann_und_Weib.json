{"textgrid.poem.34011": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Mann und Weib", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dem Weibe ist ein sch\u00f6nes Loos beschieden,", "tokens": ["Dem", "Wei\u00b7be", "ist", "ein", "sch\u00f6\u00b7nes", "Loos", "be\u00b7schie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Was sie auch hat, sie hat es ganz und immer,", "tokens": ["Was", "sie", "auch", "hat", ",", "sie", "hat", "es", "ganz", "und", "im\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "VAFIN", "$,", "PPER", "VAFIN", "PPER", "ADV", "KON", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sie freut sich an des fernsten Sternes Schimmer,", "tokens": ["Sie", "freut", "sich", "an", "des", "ferns\u00b7ten", "Ster\u00b7nes", "Schim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Allein sie schlie\u00dft sich ab in klarem Frieden.", "tokens": ["Al\u00b7lein", "sie", "schlie\u00dft", "sich", "ab", "in", "kla\u00b7rem", "Frie\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "PRF", "PTKVZ", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Der Mann wird nie so sehr vom Gl\u00fcck gemieden,", "tokens": ["Der", "Mann", "wird", "nie", "so", "sehr", "vom", "Gl\u00fcck", "ge\u00b7mie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADV", "ADV", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Als er es meidet, denn er fa\u00dft es nimmer,", "tokens": ["Als", "er", "es", "mei\u00b7det", ",", "denn", "er", "fa\u00dft", "es", "nim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "$,", "KON", "PPER", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Gleichg\u00fcltig, wird es besser, wird es schlimmer,", "tokens": ["Gleich\u00b7g\u00fcl\u00b7tig", ",", "wird", "es", "bes\u00b7ser", ",", "wird", "es", "schlim\u00b7mer", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "VAFIN", "PPER", "ADJD", "$,", "VAFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Er h\u00f6rt nicht auf, das Dasein umzuschmieden.", "tokens": ["Er", "h\u00f6rt", "nicht", "auf", ",", "das", "Da\u00b7sein", "um\u00b7zu\u00b7schmie\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "PTKVZ", "$,", "ART", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ihr ist es, wie ein zugeworf'ner Faden,", "tokens": ["Ihr", "ist", "es", ",", "wie", "ein", "zu\u00b7ge\u00b7wor\u00b7f'\u00b7ner", "Fa\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "$,", "PWAV", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Sie h\u00e4lt sich d'ran, und schaudert vor den Wogen,", "tokens": ["Sie", "h\u00e4lt", "sich", "d'\u00b7ran", ",", "und", "schau\u00b7dert", "vor", "den", "Wo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PAV", "$,", "KON", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Die unten dr\u00e4u'n, und trinkt des Himmels L\u00fcfte.", "tokens": ["Die", "un\u00b7ten", "dr\u00e4u'n", ",", "und", "trinkt", "des", "Him\u00b7mels", "L\u00fcf\u00b7te", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "$,", "KON", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Er widersteht nicht, sich im Meer zu baden,", "tokens": ["Er", "wi\u00b7der\u00b7steht", "nicht", ",", "sich", "im", "Meer", "zu", "ba\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PRF", "APPRART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und forscht, vom hellen Leben abgezogen,", "tokens": ["Und", "forscht", ",", "vom", "hel\u00b7len", "Le\u00b7ben", "ab\u00b7ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "APPRART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ob Gott sich nicht verbirgt im Schoo\u00df der Gr\u00fcfte.", "tokens": ["Ob", "Gott", "sich", "nicht", "ver\u00b7birgt", "im", "Schoo\u00df", "der", "Gr\u00fcf\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "