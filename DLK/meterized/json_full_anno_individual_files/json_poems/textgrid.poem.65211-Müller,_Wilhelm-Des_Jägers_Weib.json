ure": "iambic.tetra"}, "line.2": {"text": "Und macht die Brust mir leicht,", "tokens": ["Und", "macht", "die", "Brust", "mir", "leicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PPER", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wenn's wiederhallt von Kluft zu Kluft,", "tokens": ["Wenn's", "wie\u00b7der\u00b7hallt", "von", "Kluft", "zu", "Kluft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von Berg zu Berge steigt.", "tokens": ["Von", "Berg", "zu", "Ber\u00b7ge", "steigt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.16": {"line.1": {"text": "Doch, Liebster, dringt zu Ohren dir", "tokens": ["Doch", ",", "Liebs\u00b7ter", ",", "dringt", "zu", "Oh\u00b7ren", "dir"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "$,", "NN", "$,", "VVFIN", "APPR", "NN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Einmal der helle Klang,", "tokens": ["Ein\u00b7mal", "der", "hel\u00b7le", "Klang", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Glaub' nicht, es sei das Herze mir", "tokens": ["Glaub'", "nicht", ",", "es", "sei", "das", "Her\u00b7ze", "mir"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PTKNEG", "$,", "PPER", "VAFIN", "PDS", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So froh, wie mein Gesang.", "tokens": ["So", "froh", ",", "wie", "mein", "Ge\u00b7sang", "."], "token_info": 