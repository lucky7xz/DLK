{"textgrid.poem.39674": {"metadata": {"author": {"name": "G\u00fcnderode, Karoline von", "birth": "N.A.", "death": "N.A."}, "title": "Zilia an Edgar", "genre": "verse", "period": "N.A.", "pub_year": 1793, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Edgar komm! ich wein auf Islands K\u00fcste,", "tokens": ["O", "Ed\u00b7gar", "komm", "!", "ich", "wein", "auf", "Is\u00b7lands", "K\u00fcs\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "$.", "PPER", "ADV", "APPR", "NE", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mein m\u00fcder Blick durchirrt das weite Meer,", "tokens": ["Mein", "m\u00fc\u00b7der", "Blick", "durch\u00b7irrt", "das", "wei\u00b7te", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Doch, er durchsp\u00e4ht umsonst die Wasserw\u00fcste!", "tokens": ["Doch", ",", "er", "durch\u00b7sp\u00e4ht", "um\u00b7sonst", "die", "Was\u00b7ser\u00b7w\u00fcs\u00b7te", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PPER", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mein Edgar kehret nimmer nimmer mehr.", "tokens": ["Mein", "Ed\u00b7gar", "keh\u00b7ret", "nim\u00b7mer", "nim\u00b7mer", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ich weine einsam am verla\u00df'nen Strande", "tokens": ["Ich", "wei\u00b7ne", "ein\u00b7sam", "am", "ver\u00b7la\u00df'\u00b7nen", "Stran\u00b7