{"textgrid.poem.32706": {"metadata": {"author": {"name": "Miller, Johann Martin", "birth": "N.A.", "death": "N.A."}, "title": "1L: Stolz auf mein Vaterland und mich,", "genre": "verse", "period": "N.A.", "pub_year": 1772, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Stolz auf mein Vaterland und mich,", "tokens": ["Stolz", "auf", "mein", "Va\u00b7ter\u00b7land", "und", "mich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "KON", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Veracht' ich, stolzes M\u00e4dchen, dich!", "tokens": ["Ver\u00b7acht'", "ich", ",", "stol\u00b7zes", "M\u00e4d\u00b7chen", ",", "dich", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ADJA", "NN", "$,", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zwar z\u00e4rtlich ist ein deutscher Mann,", "tokens": ["Zwar", "z\u00e4rt\u00b7lich", "ist", "ein", "deut\u00b7scher", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Doch keines Sklav' und Unterthan.", "tokens": ["Doch", "kei\u00b7nes", "Skla\u00b7v'", "und", "Un\u00b7ter\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "KON", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Ich hab' ein gutes Herz, und kam,", "tokens": ["Ich", "hab'", "ein", "gu\u00b7tes", "Herz", ",", "und", "kam", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und bot mich dir zum Br\u00e4utigam;", "tokens": ["Und", "bot", "mich", "dir", "zum", "Br\u00e4u\u00b7ti\u00b7gam", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPER", "APPRART", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du aber lachtest ungescheut", "tokens": ["Du", "a\u00b7ber", "lach\u00b7test", "un\u00b7ge\u00b7scheut"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der deutschen Offenherzigkeit;", "tokens": ["Der", "deut\u00b7schen", "Of\u00b7fen\u00b7her\u00b7zig\u00b7keit", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und wolltest, wie dir vor geschehn,", "tokens": ["Und", "woll\u00b7test", ",", "wie", "dir", "vor", "ge\u00b7schehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "$,", "PWAV", "PPER", "APPR", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mich tief im Staube schmeicheln sehn.", "tokens": ["Mich", "tief", "im", "Stau\u00b7be", "schmei\u00b7cheln", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "APPRART", "NN", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ha! glaube, M\u00e4dchen: Schmeichelei", "tokens": ["Ha", "!", "glau\u00b7be", ",", "M\u00e4d\u00b7chen", ":", "Schmei\u00b7che\u00b7lei"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["ITJ", "$.", "VVFIN", "$,", "NN", "$.", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Ist deutschen Seelen Sklaverei!", "tokens": ["Ist", "deut\u0