{"dta.poem.15589": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "22.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Ameis' unterm Fu\u00df der Leute wird zertreten,", "tokens": ["Die", "A\u00b7meis'", "un\u00b7term", "Fu\u00df", "der", "Leu\u00b7te", "wird", "zer\u00b7tre\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "ART", "NN", "VAFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und in dem Angesicht die Flieg' ist unerbeten.", "tokens": ["Und", "in", "dem", "An\u00b7ge\u00b7sicht", "die", "Flieg'", "ist", "un\u00b7er\u00b7be\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die Ameis' unterm Fu\u00df der Leute bist du nicht,", "tokens": ["Die", "A\u00b7meis'", "un\u00b7term", "Fu\u00df", "der", "Leu\u00b7te", "bist", "du", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "ART", "NN", "VAFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Noch auch die Fliege, die sie sticht ins Angesicht.", "tokens": ["Noch", "auch", "die", "Flie\u00b7ge", ",", "die", "sie", "sticht", "ins", "An\u00b7ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "O dank' es deinem Gl\u00fcck, da\u00df so ist deine Lage,", "tokens": ["O", "dank'", "es", "dei\u00b7nem", "Gl\u00fcck", ",", "da\u00df", "so", "ist", "dei\u00b7ne", "La\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "PPOSAT", "NN", "$,", "KOUS", "ADV", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wo dir die Welt nicht wird, noch du wirst ihr zur Plage.", "tokens": ["Wo", "dir", "die", "Welt", "nicht", "wird", ",", "noch", "du", "wirst", "ihr", "zur", "Pla\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "PTKNEG", "VAFIN", "$,", "ADV", "PPER", "VAFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}