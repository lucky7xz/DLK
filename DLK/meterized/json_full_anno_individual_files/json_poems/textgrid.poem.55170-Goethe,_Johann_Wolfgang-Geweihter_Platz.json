{"textgrid.poem.55170": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Geweihter Platz", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn zu den Reihen der Nymphen, versammelt in heiliger Mondnacht,", "tokens": ["Wenn", "zu", "den", "Rei\u00b7hen", "der", "Nym\u00b7phen", ",", "ver\u00b7sam\u00b7melt", "in", "hei\u00b7li\u00b7ger", "Mond\u00b7nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "ART", "NN", "$,", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Sich die Grazien heimlich herab vom Olympus gesellen:", "tokens": ["Sich", "die", "Gra\u00b7zi\u00b7en", "heim\u00b7lich", "her\u00b7ab", "vom", "O\u00b7lym\u00b7pus", "ge\u00b7sel\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "NN", "ADJD", "ADV", "APPRART", "NE", "VVPP", "$."], "meter": "--+--+--+-+---+-", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Hier belauscht sie der Dichter und h\u00f6rt die sch\u00f6nen Ges\u00e4nge,", "tokens": ["Hier", "be\u00b7lauscht", "sie", "der", "Dich\u00b7ter", "und", "h\u00f6rt", "die", "sch\u00f6\u00b7nen", "Ge\u00b7s\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "KON", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Sieht verschwiegener T\u00e4nze geheimnisvolle Bewegung.", "tokens": ["Sieht", "ver\u00b7schwie\u00b7ge\u00b7ner", "T\u00e4n\u00b7ze", "ge\u00b7heim\u00b7nis\u00b7vol\u00b7le", "Be\u00b7we\u00b7gung", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.5": {"text": "Was der Himmel nur Herrliches hat, was gl\u00fccklich die Erde", "tokens": ["Was", "der", "Him\u00b7mel", "nur", "Herr\u00b7li\u00b7ches", "hat", ",", "was", "gl\u00fcck\u00b7lich", "die", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "ART", "NN", "ADV", "NN", "VAFIN", "$,", "PRELS", "ADJD", "ART", "NN"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.6": {"text": "Reizendes immer gebar, das erscheint dem wachenden Tr\u00e4umer.", "tokens": ["Rei\u00b7zen\u00b7des", "im\u00b7mer", "ge\u00b7bar", ",", "das", "er\u00b7scheint", "dem", "wa\u00b7chen\u00b7den", "Tr\u00e4u\u00b7mer", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADJD", "$,", "PDS", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.7": {"text": "Alles erz\u00e4hlt er den Musen, und da\u00df die G\u00f6tter nicht z\u00fcrnen,", "tokens": ["Al\u00b7les", "er\u00b7z\u00e4hlt", "er", "den", "Mu\u00b7sen", ",", "und", "da\u00df", "die", "G\u00f6t\u00b7ter", "nicht", "z\u00fcr\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "ART", "NN", "$,", "KON", "KOUS", "ART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.8": {"text": "Lehren die Musen ihn gleich bescheiden Geheimnisse sprechen.", "tokens": ["Leh\u00b7ren", "die", "Mu\u00b7sen", "ihn", "gleich", "be\u00b7schei\u00b7den", "Ge\u00b7heim\u00b7nis\u00b7se", "spre\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN"