{"dta.poem.19511": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Viston.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-200905193933", "language": ["de:0.99"], "booktitle": "Meyer, Conrad Ferdinand: Gedichte. Leipzig, 1882."}, "poem": {"stanza.1": {"line.1": {"text": "Als ich j\u00fcngst vom Pfad verirrt war,", "tokens": ["Als", "ich", "j\u00fcngst", "vom", "Pfad", "ver\u00b7irrt", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPRART", "NN", "VVPP", "VAFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo kein J\u00e4ger und kein Hirt war,", "tokens": ["Wo", "kein", "J\u00e4\u00b7ger", "und", "kein", "Hirt", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "KON", "PIAT", "NN", "VAFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "F\u00fchrt' ein Licht aus dunkelm Tann", "tokens": ["F\u00fchrt'", "ein", "Licht", "aus", "dun\u00b7kelm", "Tann"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Mich an eines H\u00fcttleins Schwelle,", "tokens": ["Mich", "an", "ei\u00b7nes", "H\u00fctt\u00b7leins", "Schwel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Drin bei matter Ampelhelle", "tokens": ["Drin", "bei", "mat\u00b7ter", "Am\u00b7pel\u00b7hel\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Eine greise Parze spann.", "tokens": ["Ei\u00b7ne", "grei\u00b7se", "Par\u00b7ze", "spann", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Drau\u00dfen schlug der Wind die Schwingen,", "tokens": ["Drau\u00b7\u00dfen", "schlug", "der", "Wind", "die", "Schwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und die Bergesstr\u00f6me singen", "tokens": ["Und", "die", "Ber\u00b7ges\u00b7str\u00f6\u00b7me", "sin\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "H\u00f6rt' ich ihren dunkeln Sang ...", "tokens": ["H\u00f6rt'", "ich", "ih\u00b7ren", "dun\u00b7keln", "Sang", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und ich sah den Faden schweben,", "tokens": ["Und", "ich", "sah", "den", "Fa\u00b7den", "schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und der Faden schien ein Leben \u2014", "tokens": ["Und", "der", "Fa\u00b7den", "schien", "ein", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Meines? dacht' ich zauberbang.", "tokens": ["Mei\u00b7nes", "?", "dacht'", "ich", "zau\u00b7ber\u00b7bang", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "$.", "VVFIN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wage, Mensch, die h\u00f6chsten Fl\u00fcge,", "tokens": ["Wa\u00b7ge", ",", "Mensch", ",", "die", "h\u00f6chs\u00b7ten", "Fl\u00fc\u00b7ge", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Deiner Parze starre Z\u00fcge", "tokens": ["Dei\u00b7ner", "Par\u00b7ze", "star\u00b7re", "Z\u00fc\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sehen l\u00e4ngst das nahe Ziel!", "tokens": ["Se\u00b7hen", "l\u00e4ngst", "das", "na\u00b7he", "Ziel", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Tummle dich, ein k\u00fchner Ringer:", "tokens": ["Tumm\u00b7le", "dich", ",", "ein", "k\u00fch\u00b7ner", "Rin\u00b7ger", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ihre hagern, harten Finger", "tokens": ["Ih\u00b7re", "ha\u00b7gern", ",", "har\u00b7ten", "Fin\u00b7ger"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Enden bald das edle Spiel ...", "tokens": ["En\u00b7den", "bald", "das", "ed\u00b7le", "Spiel", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Eine Thr\u00e4ne seh' ich schimmern?", "tokens": ["Ei\u00b7ne", "Thr\u00e4\u00b7ne", "seh'", "ich", "schim\u00b7mern", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "An der Wand mit Silberflimmern", "tokens": ["An", "der", "Wand", "mit", "Sil\u00b7ber\u00b7flim\u00b7mern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Hangt ein d\u00fcrrer Todtenkranz ...", "tokens": ["Hangt", "ein", "d\u00fcr\u00b7rer", "Tod\u00b7ten\u00b7kranz", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Irgend einen alten Jammer", "tokens": ["Ir\u00b7gend", "ei\u00b7nen", "al\u00b7ten", "Jam\u00b7mer"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "In der Alpenh\u00fctte Kammer", "tokens": ["In", "der", "Al\u00b7pen\u00b7h\u00fct\u00b7te", "Kam\u00b7mer"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Spinnt ein Weib im Ampelglanz.", "tokens": ["Spinnt", "ein", "Weib", "im", "Am\u00b7pel\u00b7glanz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}