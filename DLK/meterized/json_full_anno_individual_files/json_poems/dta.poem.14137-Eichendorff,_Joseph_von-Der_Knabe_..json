{"dta.poem.14137": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Der Knabe .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Das V\u00f6glein frisch die Fl\u00fcgel r\u00fchrt \u2014", "tokens": ["Das", "V\u00f6\u00b7glein", "frisch", "die", "Fl\u00fc\u00b7gel", "r\u00fchrt"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es ruft: kommst Du nicht balde? \u2014", "tokens": ["Es", "ruft", ":", "kommst", "Du", "nicht", "bal\u00b7de", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "$.", "VVFIN", "PPER", "PTKNEG", "ADV", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das hat das V\u00f6gelein verf\u00fchrt,", "tokens": ["Das", "hat", "das", "V\u00f6\u00b7ge\u00b7lein", "ver\u00b7f\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Fort flog's zum gr\u00fcnen Walde \u2014", "tokens": ["Fort", "flo\u00b7g's", "zum", "gr\u00fc\u00b7nen", "Wal\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "APPRART", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}