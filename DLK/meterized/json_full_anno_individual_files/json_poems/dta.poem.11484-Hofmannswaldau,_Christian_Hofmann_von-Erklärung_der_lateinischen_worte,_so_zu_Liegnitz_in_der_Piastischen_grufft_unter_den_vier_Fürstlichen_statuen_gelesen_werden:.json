{"dta.poem.11484": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Erkl\u00e4rung der lateinischen worte, so zu  \n Liegnitz in der Piastischen grufft unter den  \n vier F\u00fcrstlichen statuen gelesen  \n werden:", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Verla\u00dfne F\u00fcrstin! ach! dein Hertzog ist dahin!", "tokens": ["Ver\u00b7la\u00df\u00b7ne", "F\u00fcrs\u00b7tin", "!", "ach", "!", "dein", "Hert\u00b7zog", "ist", "da\u00b7hin", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "XY", "$.", "PPOSAT", "NN", "VAFIN", "PAV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich schau\u2019 es allzuwohl, da\u00df ich gantz einsam bin.", "tokens": ["Ich", "schau'", "es", "all\u00b7zu\u00b7wohl", ",", "da\u00df", "ich", "gantz", "ein\u00b7sam", "bin", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$,", "KOUS", "PPER", "ADV", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "O himmel! solt\u2019 ich mich nun nicht zu tode weinen?", "tokens": ["O", "him\u00b7mel", "!", "solt'", "ich", "mich", "nun", "nicht", "zu", "to\u00b7de", "wei\u00b7nen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "VMFIN", "PPER", "PRF", "ADV", "PTKNEG", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Denn wann die sonne stirbt, wie kan der monde scheinen?", "tokens": ["Denn", "wann", "die", "son\u00b7ne", "stirbt", ",", "wie", "kan", "der", "mon\u00b7de", "schei\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "VVFIN", "$,", "PWAV", "VMFIN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Schaust du nicht, wie dein sohn die nacht der einsamkeit", "tokens": ["Schaust", "du", "nicht", ",", "wie", "dein", "sohn", "die", "nacht", "der", "ein\u00b7sam\u00b7keit"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKNEG", "$,", "PWAV", "PPOSAT", "NN", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Durch seiner tugenden erlauchten glantz zerstreut?", "tokens": ["Durch", "sei\u00b7ner", "tu\u00b7gen\u00b7den", "er\u00b7lauch\u00b7ten", "glantz", "zer\u00b7streut", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ein printz, wie dieser ist, kan auf den trauer-b\u00fchnen", "tokens": ["Ein", "printz", ",", "wie", "die\u00b7ser", "ist", ",", "kan", "auf", "den", "trau\u00b7e\u00b7rb\u00fch\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PWAV", "PDS", "VAFIN", "$,", "VMFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Auch seiner mutter wohl vor eine sonne dienen.", "tokens": ["Auch", "sei\u00b7ner", "mut\u00b7ter", "wohl", "vor", "ei\u00b7ne", "son\u00b7ne", "die\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Alein es folgt dein sohn dir in das sternen-dach,", "tokens": ["Al\u00b7ein", "es", "folgt", "dein", "sohn", "dir", "in", "das", "ster\u00b7nen\u00b7dach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "VVFIN", "PPOSAT", "NN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mein Vater und mein F\u00fcrst! selbst auf dem fusse nach;", "tokens": ["Mein", "Va\u00b7ter", "und", "mein", "F\u00fcrst", "!", "selbst", "auf", "dem", "fus\u00b7se", "nach", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "PPOSAT", "NN", "$.", "ADV", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Doch, Hertzogin! getrost! sind wir nicht mehr auf erden,", "tokens": ["Doch", ",", "Hert\u00b7zo\u00b7gin", "!", "ge\u00b7trost", "!", "sind", "wir", "nicht", "mehr", "auf", "er\u00b7den", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "NN", "$.", "VVPP", "$.", "VAFIN", "PPER", "PTKNEG", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So wird dir Leopold zu einer sonne werden.", "tokens": ["So", "wird", "dir", "Leo\u00b7pold", "zu", "ei\u00b7ner", "son\u00b7ne", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NE", "APPR", "ART", "NN", "VAINF", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "O D\u00fcstre finsterni\u00df! Der hoffnung stern und licht", "tokens": ["O", "D\u00fcst\u00b7re", "fins\u00b7ter\u00b7ni\u00df", "!", "Der", "hoff\u00b7nung", "stern", "und", "licht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "VVFIN", "$.", "ART", "NN", "VVINF", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ist leider ausgelescht! Ach! hilfft der himmel nicht,", "tokens": ["Ist", "lei\u00b7der", "aus\u00b7ge\u00b7lescht", "!", "Ach", "!", "hilfft", "der", "him\u00b7mel", "nicht", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "VVPP", "$.", "ITJ", "$.", "VVFIN", "ART", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und l\u00e4\u00dft in dieser nacht mir eine sonne scheinen;", "tokens": ["Und", "l\u00e4\u00dft", "in", "die\u00b7ser", "nacht", "mir", "ei\u00b7ne", "son\u00b7ne", "schei\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PDAT", "NN", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So mu\u00df Charlotte sich gewi\u00df zu tode weinen.", "tokens": ["So", "mu\u00df", "Char\u00b7lot\u00b7te", "sich", "ge\u00b7wi\u00df", "zu", "to\u00b7de", "wei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "NN", "PRF", "ADV", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}