{"textgrid.poem.51138": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Lied vom Schuft", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein armer Teufel ist der Schuft,", "tokens": ["Ein", "ar\u00b7mer", "Teu\u00b7fel", "ist", "der", "Schuft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er wei\u00df: es kennt ihn jedes Kind;", "tokens": ["Er", "wei\u00df", ":", "es", "kennt", "ihn", "je\u00b7des", "Kind", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PPER", "VVFIN", "PPER", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er wandelt wie ein Tr\u00e4umender,", "tokens": ["Er", "wan\u00b7delt", "wie", "ein", "Tr\u00e4u\u00b7men\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.4": {"text": "Wo unverdorbne Menschen sind.", "tokens": ["Wo", "un\u00b7ver\u00b7dorb\u00b7ne", "Men\u00b7schen", "sind", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ein dummer Teufel ist der Schuft,", "tokens": ["Ein", "dum\u00b7mer", "Teu\u00b7fel", "ist", "der", "Schuft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Weil doch er der Geprellte ist,", "tokens": ["Weil", "doch", "er", "der", "G