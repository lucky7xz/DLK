{"textgrid.poem.43753": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der Schwalben Abschied", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O sieh, wie allenthalben", "tokens": ["O", "sieh", ",", "wie", "al\u00b7len\u00b7thal\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NE", "PTKVZ", "$,", "PWAV", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sich sammeln unsre Schwalben!", "tokens": ["Sich", "sam\u00b7meln", "uns\u00b7re", "Schwal\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sie haben sich auf den d\u00fcrren Ast", "tokens": ["Sie", "ha\u00b7ben", "sich", "auf", "den", "d\u00fcr\u00b7ren", "Ast"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PRF", "APPR", "ART", "ADJA", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "In unserm Apfelbaum gesetzt", "tokens": ["In", "un\u00b7serm", "Ap\u00b7fel\u00b7baum", "ge\u00b7setzt"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und halten noch eine kurze Rast", "tokens": ["Und", "hal\u00b7ten", "noch", "ei\u00b7ne", "kur\u00b7ze", "Rast"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "ART", "ADJA", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Und zwitschern ein Lied zu guter Letzt:", "tokens": ["Und", "zwit\u00b7schern", "ein", "Lied", "zu", "gu\u00b7ter", "Letzt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word",