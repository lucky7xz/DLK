{"textgrid.poem.65137": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: \u00bbnur sanfter m\u00f6g ich mich geberden,\u00ab", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbnur sanfter m\u00f6g ich mich geberden,\u00ab", "tokens": ["\u00bb", "nur", "sanf\u00b7ter", "m\u00f6g", "ich", "mich", "ge\u00b7ber\u00b7den", ",", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "ADJD", "VMFIN", "PPER", "PRF", "VVPP", "$,", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So redest du mir liebvoll zu,", "tokens": ["So", "re\u00b7dest", "du", "mir", "lieb\u00b7voll", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u00bbdann k\u00f6nne Alles gut noch werden,", "tokens": ["\u00bb", "dann", "k\u00f6n\u00b7ne", "Al\u00b7les", "gut", "noch", "wer\u00b7den", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VMFIN", "PIS", "ADJD", "ADV", "VAINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dem Sturme folgen s\u00fc\u00dfe Ruh.\u00ab", "tokens": ["Dem", "Stur\u00b7me", "fol\u00b7gen", "s\u00fc\u00b7\u00dfe", "Ruh", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVINF", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mir aber ward solch sanfte Milde", "tokens": ["Mir", "a\u00b7ber", "ward", "solch", "sanf\u00b7te", "Mil\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VAFIN", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Von der Natur nicht eingefl\u00f6\u00dft!", "tokens": ["Von", "der", "Na\u00b7tur", "nicht", "ein\u00b7ge\u00b7fl\u00f6\u00dft", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Es ruft mein Herz, das hei\u00dfe, wilde:", "tokens": ["Es", "ruft", "mein", "Herz", ",", "das", "hei\u00b7\u00dfe", ",", "wil\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,", "PDS", "VVFIN", "$,", "ADJA", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Fluch Allem, was von dir mich st\u00f6\u00dft!", "tokens": ["Fluch", "Al\u00b7lem", ",", "was", "von", "dir", "mich", "st\u00f6\u00dft", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIS", "$,", "PRELS", "APPR", "PPER", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Und ob der Engel Ruf ersch\u00f6lle,", "tokens": ["Und", "ob", "der", "En\u00b7gel", "Ruf", "er\u00b7sch\u00f6l\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Fest bliebe ich an dich gebannt!", "tokens": ["Fest", "blie\u00b7be", "ich", "an", "dich", "ge\u00b7bannt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Des Himmels Lust, die Qual der H\u00f6lle,", "tokens": ["Des", "Him\u00b7mels", "Lust", ",", "die", "Qual", "der", "H\u00f6l\u00b7le", ","], "token_info": ["word",