{"textgrid.poem.34034": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Rom", "genre": "verse", "period": "N.A.", "pub_year": 1838, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Rom, schon bist du Ruine und wirst noch weniger werden,", "tokens": ["Rom", ",", "schon", "bist", "du", "Ru\u00b7i\u00b7ne", "und", "wirst", "noch", "we\u00b7ni\u00b7ger", "wer\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "VAFIN", "PPER", "NN", "KON", "VAFIN", "ADV", "ADV", "VAINF", "$,"], "meter": "+-+--+--+----+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Aber dein Himmel verb\u00fcrgt dennoch die ewige Stadt.", "tokens": ["A\u00b7ber", "dein", "Him\u00b7mel", "ver\u00b7b\u00fcrgt", "den\u00b7noch", "die", "e\u00b7wi\u00b7ge", "Stadt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Wo die Myrthe gedeiht, und wo der Lorbeer nicht mangelt,", "tokens": ["Wo", "die", "Myr\u00b7the", "ge\u00b7deiht", ",", "und", "wo", "der", "Lor\u00b7beer", "nicht", "man\u00b7gelt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "$,", "KON", "PWAV", "ART", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "+-+--+-+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Siedeln zu Liebe und Krieg immer auch Menschen sich an.", "tokens": ["Sie\u00b7deln", "zu", "Lie\u00b7be", "und", "Krieg", "im\u00b7mer", "auch", "Men\u00b7schen", "sich", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "ADV", "ADV", "NN", "PRF", "PTKVZ", "$."], "meter": "-+-+--+---+--+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Colloseum, Rotunda, ihr wurdet christliche Kirchen,", "tokens": ["Col\u00b7lo\u00b7se\u00b7um", ",", "Ro\u00b7tun\u00b7da", ",", "ihr", "wur\u00b7det", "christ\u00b7li\u00b7che", "Kir\u00b7chen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NE", "$,", "PPER", "VAFIN", "ADJA", "NN", "$,"], "meter": "+---+-+-+-+--+-", "measure": "trochaic.hexa.relaxed"}, "line.6": {"text": "Weil euch dieses allein vor der Zerst\u00f6rung gesch\u00fctzt,", "tokens": ["Weil", "euch", "die\u00b7ses", "al\u00b7lein", "vor", "der", "Zer\u00b7st\u00f6\u00b7rung", "ge\u00b7sch\u00fctzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PDS", "ADV", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+---+--+--+", "measure": "trochaic.penta.relaxed"}, "line.7": {"text": "Denn der stumpfe Zelot ergriff die Axt des Barbaren,", "tokens": ["Denn", "der", "stump\u00b7fe", "Ze\u00b7lot", "er\u00b7griff", "die", "Axt", "des", "Bar\u00b7ba\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "--+-+--+-+----", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Als sie ihm endlich entsank, aber der weisere Papst", "tokens": ["Als", "sie", "ihm", "end\u00b7lich", "ent\u00b7sank", ",", "a\u00b7ber", "der", "wei\u00b7se\u00b7re", "Papst"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "ADV", "VVFIN", "$,", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+--++--+--+", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "Schirmte euch durch den Altar und durch die Heiligenbilder", "tokens": ["Schirm\u00b7te", "euch", "durch", "den", "Al\u00b7tar", "und", "durch", "die", "Hei\u00b7li\u00b7gen\u00b7bil\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "KON", "APPR", "ART", "NN"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.10": {"text": "Still vor der letzten Gefahr, welche euch drohte bis heut'.", "tokens": ["Still", "vor", "der", "letz\u00b7ten", "Ge\u00b7fahr", ",", "wel\u00b7che", "euch", "droh\u00b7te", "bis", "heut'", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN", "$,", "PRELS", "PPER", "VVFIN", "APPR", "ADV", "$."], "meter": "---+--+---+--+", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Dennoch kommt es mir vor, als h\u00e4tt' man erschlag'nen Titanen", "tokens": ["Den\u00b7noch", "kommt", "es", "mir", "vor", ",", "als", "h\u00e4tt'", "man", "er\u00b7schlag'\u00b7nen", "Ti\u00b7ta\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "PTKVZ", "$,", "KOKOM", "VAFIN", "PIS", "ADJA", "NN"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.12": {"text": "Nach dem Tode das Kreuz noch auf die Stirne gebrannt.", "tokens": ["Nach", "dem", "To\u00b7de", "das", "Kreuz", "noch", "auf", "die", "Stir\u00b7ne", "ge\u00b7brannt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "ADV", "APPR", "ART", "NN", "VVPP", "$."], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}}, "stanza.2": {"line.1": {"text": "Rom, schon bist du Ruine und wirst noch weniger werden,", "tokens": ["Rom", ",", "schon", "bist", "du", "Ru\u00b7i\u00b7ne", "und", "wirst", "noch", "we\u00b7ni\u00b7ger", "wer\u00b7den", ","],