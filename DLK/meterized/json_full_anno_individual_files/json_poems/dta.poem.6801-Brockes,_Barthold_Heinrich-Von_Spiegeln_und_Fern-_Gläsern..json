{"dta.poem.6801": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von Spiegeln und Fern-  \n Gl\u00e4sern.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "O vortheilhaffte Lehr! O Anmuht! recht zu sehn,", "tokens": ["O", "vor\u00b7theil\u00b7haff\u00b7te", "Lehr", "!", "O", "An\u00b7muht", "!", "recht", "zu", "sehn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$.", "NE", "NN", "$.", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Durch welche Regel doch die Z\u00fcg\u2019 in uns entstehn,", "tokens": ["Durch", "wel\u00b7che", "Re\u00b7gel", "doch", "die", "Z\u00fcg'", "in", "uns", "ent\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "NN", "ADV", "ART", "NN", "APPR", "PPER", "VVINF", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Da\u00df blo\u00df der Geist vermag zu richten und zu sp\u00fcren,", "tokens": ["Da\u00df", "blo\u00df", "der", "Geist", "ver\u00b7mag", "zu", "rich\u00b7ten", "und", "zu", "sp\u00fc\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "VVFIN", "PTKZU", "VVINF", "KON", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da\u00df wir in allen dem, was uns verf\u00fchren,", "tokens": ["Da\u00df", "wir", "in", "al\u00b7len", "dem", ",", "was", "uns", "ver\u00b7f\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PIAT", "PDS", "$,", "PRELS", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Nicht weniger uns auch belehren,", "tokens": ["Nicht", "we\u00b7ni\u00b7ger", "uns", "auch", "be\u00b7leh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die Sinnen wol und recht regieren,", "tokens": ["Die", "Sin\u00b7nen", "wol", "und", "recht", "re\u00b7gie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "KON", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Auch sie verleiten kan; man die Vernunfft mu\u00df h\u00f6ren!", "tokens": ["Auch", "sie", "ver\u00b7lei\u00b7ten", "kan", ";", "man", "die", "Ver\u00b7nunfft", "mu\u00df", "h\u00f6\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVINF", "VMFIN", "$.", "PIS", "ART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}