{"dta.poem.13114": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Achtes Buch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201enun sagt! versp\u00fchrt ihr nicht ein so erwecktes Herz?", "tokens": ["\u201e", "nun", "sagt", "!", "ver\u00b7sp\u00fchrt", "ihr", "nicht", "ein", "so", "er\u00b7weck\u00b7tes", "Herz", "?"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "$.", "VVFIN", "PPER", "PTKNEG", "ART", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "\u201eliebt ihr nicht dieses Paar? ist etwann anderw\u00e4rts", "tokens": ["\u201e", "liebt", "ihr", "nicht", "die\u00b7ses", "Paar", "?", "ist", "et\u00b7wann", "an\u00b7der\u00b7w\u00e4rts"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "VVFIN", "PPER", "PTKNEG", "PDAT", "NN", "$.", "VAFIN", "ADV", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "475\u201dWas eure Neigung hem\u0303t? ich wei\u00df, ihr m\u00fc\u00dft es lieben;", "tokens": ["\"", "Was", "eu\u00b7re", "Nei\u00b7gung", "hem\u0303t", "?", "ich", "wei\u00df", ",", "ihr", "m\u00fc\u00dft", "es", "lie\u00b7ben", ";"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "PPOSAT", "NN", "VVFIN", "$.", "PPER", "VVFIN", "$,", "PPER", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "\u201ejhr f\u00fchlt in eurer Brust die Reizung eingeschrieben.", "tokens": ["\u201e", "jhr", "f\u00fchlt", "in", "eu\u00b7rer", "Brust", "die", "Rei\u00b7zung", "ein\u00b7ge\u00b7schrie\u00b7ben", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "\u201ewohlan! ich bin die Leyr, der Cither Klang, der Mund,", "tokens": ["\u201e", "wo\u00b7hlan", "!", "ich", "bin", "die", "Leyr", ",", "der", "Cit\u00b7her", "Klang", ",", "der", "Mund", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "ADV", "$.", "PPER", "VAFIN", "ART", "NN", "$,", "ART", "ADJA", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "\u201edas Sayten-Spiel, die Fl\u00f6t, ich mache dieses kund:", "tokens": ["\u201e", "das", "Say\u00b7ten\u00b7Spiel", ",", "die", "Fl\u00f6t", ",", "ich", "ma\u00b7che", "die\u00b7ses", "kund", ":"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "$,", "ART", "NN", "$,", "PPER", "VVFIN", "PDS", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "\u201eich wei\u00df durch meine Stimm und Sing-Kunst auszubreiten,", "tokens": ["\u201e", "ich", "wei\u00df", "durch", "mei\u00b7ne", "Stimm", "und", "Sing\u00b7Kunst", "aus\u00b7zu\u00b7brei\u00b7ten", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "KON", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "480\u201dWas eure Seele r\u00fchrt, um was die Sinnen streiten.", "tokens": ["\"", "Was", "eu\u00b7re", "See\u00b7le", "r\u00fchrt", ",", "um", "was", "die", "Sin\u00b7nen", "strei\u00b7ten", "."], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "PPOSAT", "NN", "VVFIN", "$,", "APPR", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}