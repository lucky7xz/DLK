{"dta.poem.13554": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Sonnette .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Die alten Felsen spalten sich mit Krachen,", "tokens": ["Die", "al\u00b7ten", "Fel\u00b7sen", "spal\u00b7ten", "sich", "mit", "Kra\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PRF", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von dr\u00fcben gr\u00fc\u00dfen schon verwandte Lieder,", "tokens": ["Von", "dr\u00fc\u00b7ben", "gr\u00fc\u00b7\u00dfen", "schon", "ver\u00b7wand\u00b7te", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VVFIN", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Zum ew'gen Meere f\u00fchrt Er alle wieder.", "tokens": ["Zum", "ew'\u00b7gen", "Mee\u00b7re", "f\u00fchrt", "Er", "al\u00b7le", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "PPER", "PIS", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}