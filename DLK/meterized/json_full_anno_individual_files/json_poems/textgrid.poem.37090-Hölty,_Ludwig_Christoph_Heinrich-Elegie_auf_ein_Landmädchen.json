{"textgrid.poem.37090": {"metadata": {"author": {"name": "H\u00f6lty, Ludwig Christoph Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Elegie auf ein Landm\u00e4dchen", "genre": "verse", "period": "N.A.", "pub_year": 1774, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schwermuthsvoll und dumpfig halt Gel\u00e4ute", "tokens": ["Schwer\u00b7muths\u00b7voll", "und", "dump\u00b7fig", "halt", "Ge\u00b7l\u00e4u\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "KON", "ADJD", "VVFIN", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Vom bemoo\u00dften Kirchenthurm herab;", "tokens": ["Vom", "be\u00b7moo\u00df\u00b7ten", "Kir\u00b7chen\u00b7thurm", "her\u00b7ab", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ADV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "V\u00e4ter weinen, Kinder, M\u00fctter, Br\u00e4ute;", "tokens": ["V\u00e4\u00b7ter", "wei\u00b7nen", ",", "Kin\u00b7der", ",", "M\u00fct\u00b7ter", ",", "Br\u00e4u\u00b7te", ";"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "VVINF", "$,", "NN", "$,", "NN", "$,", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und der Todtengr\u00e4ber gr\u00e4bt ein Grab.", "tokens": ["Und", "der", "Tod\u00b7ten\u00b7gr\u00e4\u00b7ber", "gr\u00e4bt", "ein", "Grab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.5": {"text": "Angethan mit einem Sterbekleide,", "tokens": ["An\u00b7ge\u00b7than", "mit", "ei\u00b7nem", "Ster\u00b7be\u00b7klei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Eine Blumenkron' im blonden Haar,", "tokens": ["Ei\u00b7ne", "Blu\u00b7men\u00b7kron'", "im", "blon\u00b7den", "Haar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Schlummert R\u00f6\u00dfchen, so der Mutter Freude,", "tokens": ["Schlum\u00b7mert", "R\u00f6\u00df\u00b7chen", ",", "so", "der", "Mut\u00b7ter", "Freu\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "ADV", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "So der Stolz des Dorfes war.", "tokens": ["So", "der", "Stolz", "des", "Dor\u00b7fes", "war", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ART", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ihre Lieben, voll des Mi\u00dfgeschickes,", "tokens": ["Ih\u00b7re", "Lie\u00b7ben", ",", "voll", "des", "Mi\u00df\u00b7ge\u00b7schi\u00b7ckes", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "$,", "ADJD", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Denken nicht an Pf\u00e4nderspiel und Tanz,", "tokens": ["Den\u00b7ken", "nicht", "an", "Pf\u00e4n\u00b7der\u00b7spiel", "und", "Tanz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Stehn am Sarge, winden, na\u00dfes Blickes,", "tokens": ["Stehn", "am", "Sar\u00b7ge", ",", "win\u00b7den", ",", "na\u00b7\u00dfes", "Bli\u00b7ckes", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$,", "VVFIN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Ihrer Freundin einen Todtenkranz.", "tokens": ["Ih\u00b7rer", "Freun\u00b7din", "ei\u00b7nen", "Tod\u00b7ten\u00b7kranz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Ach, kein M\u00e4dchen war der Thr\u00e4nen werther,", "tokens": ["Ach", ",", "kein", "M\u00e4d\u00b7chen", "war", "der", "Thr\u00e4\u00b7nen", "wert\u00b7her", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PIAT", "NN", "VAFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.6": {"text": "Als du gutes, frommes M\u00e4dchen bist,", "tokens": ["Als", "du", "gu\u00b7tes", ",", "from\u00b7mes", "M\u00e4d\u00b7chen", "bist", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJA", "$,", "ADJA", "NN", "VAFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Und im Himmel ist kein Geist verkl\u00e4rter,", "tokens": ["Und", "im", "Him\u00b7mel", "ist", "kein", "Geist", "ver\u00b7kl\u00e4r\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VAFIN", "PIAT", "NN", "ADJD", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Als die Seele R\u00f6\u00dfchens ist.", "tokens": ["Als", "die", "See\u00b7le", "R\u00f6\u00df\u00b7chens", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wie ein Engel, stand, im Sch\u00e4ferkleide,", "tokens": ["Wie", "ein", "En\u00b7gel", ",", "stand", ",", "im", "Sch\u00e4\u00b7fer\u00b7klei\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "VVFIN", "$,", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Sie vor ihrer kleinen H\u00fcttenth\u00fcr.", "tokens": ["Sie", "vor", "ih\u00b7rer", "klei\u00b7nen", "H\u00fct\u00b7tent\u00b7h\u00fcr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Wiesenblumen waren ihr Geschmeide,", "tokens": ["Wie\u00b7sen\u00b7blu\u00b7men", "wa\u00b7ren", "ihr", "Ge\u00b7schmei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und ein Veilchen ihres Busens Zier.", "tokens": ["Und", "ein", "Veil\u00b7chen", "ih\u00b7res", "Bu\u00b7sens", "Zier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Ihre F\u00e4cher waren Zephyrs Fl\u00fcgel,", "tokens": ["Ih\u00b7re", "F\u00e4\u00b7cher", "wa\u00b7ren", "Ze\u00b7phyrs", "Fl\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "NN", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Und der Morgenhayn ihr Putzgemach,", "tokens": ["Und", "der", "Mor\u00b7gen\u00b7hayn", "ihr", "Putz\u00b7ge\u00b7mach", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.7": {"text": "Diese Silberquellen ihre Spiegel,", "tokens": ["Die\u00b7se", "Sil\u00b7ber\u00b7quel\u00b7len", "ih\u00b7re", "Spie\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Ihre Schminke dieser Bach.", "tokens": ["Ih\u00b7re", "Schmin\u00b7ke", "die\u00b7ser", "Bach", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PDAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Sittsamkeit umflo\u00df, wie Mondenschimmer,", "tokens": ["Sitt\u00b7sam\u00b7keit", "um\u00b7flo\u00df", ",", "wie", "Mon\u00b7den\u00b7schim\u00b7mer", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "PWAV", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Ihre Rosenwangen, ihren Blick,", "tokens": ["Ih\u00b7re", "Ro\u00b7sen\u00b7wan\u00b7gen", ",", "ih\u00b7ren", "Blick", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Nimmer wich der Seraph, Unschuld, nimmer,", "tokens": ["Nim\u00b7mer", "wich", "der", "Se\u00b7raph", ",", "Un\u00b7schuld", ",", "nim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "NN", "$,", "ADV", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Von der holden Sch\u00e4ferin zur\u00fcck.", "tokens": ["Von", "der", "hol\u00b7den", "Sch\u00e4\u00b7fe\u00b7rin", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "J\u00fcnglingsblicke taumelten, voll Feuer,", "tokens": ["J\u00fcng\u00b7lings\u00b7bli\u00b7cke", "tau\u00b7mel\u00b7ten", ",", "voll", "Feu\u00b7er", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "ADJD", "NN", "$,"], "meter": "+-+-+---+-", "measure": "unknown.measure.tetra"}, "line.6": {"text": "Nach dem Reiz des lieben M\u00e4dchens hin,", "tokens": ["Nach", "dem", "Reiz", "des", "lie\u00b7ben", "M\u00e4d\u00b7chens", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Aber keiner, als ihr Vielgetreuer,", "tokens": ["A\u00b7ber", "kei\u00b7ner", ",", "als", "ihr", "Viel\u00b7ge\u00b7treu\u00b7er", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "$,", "KOUS", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "R\u00fchrte jemahls ihren Sinn.", "tokens": ["R\u00fchr\u00b7te", "je\u00b7mahls", "ih\u00b7ren", "Sinn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Keiner als ihr Willhelm! Fr\u00fchlingsweihe", "tokens": ["Kei\u00b7ner", "als", "ihr", "Will\u00b7helm", "!", "Fr\u00fch\u00b7lings\u00b7wei\u00b7he"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["PIS", "KOKOM", "PPOSAT", "NN", "$.", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Rief die Edeln in den Buchenhayn,", "tokens": ["Rief", "die", "E\u00b7deln", "in", "den", "Bu\u00b7chen\u00b7hayn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Angeblinkt von Mayenhimmelbl\u00e4ue,", "tokens": ["An\u00b7ge\u00b7blinkt", "von", "Ma\u00b7yen\u00b7him\u00b7mel\u00b7bl\u00e4u\u00b7e", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "NE", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Flogen sie den deutschen Ringelreihn.", "tokens": ["Flo\u00b7gen", "sie", "den", "deut\u00b7schen", "Rin\u00b7gel\u00b7reihn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "R\u00f6\u00dfchen gab ihm B\u00e4nder, mancher Farbe,", "tokens": ["R\u00f6\u00df\u00b7chen", "gab", "ihm", "B\u00e4n\u00b7der", ",", "man\u00b7cher", "Far\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "NN", "$,", "PIAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Kam die Erndt', an seinen Schnitterhut,", "tokens": ["Kam", "die", "Erndt'", ",", "an", "sei\u00b7nen", "Schnit\u00b7ter\u00b7hut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Sa\u00df mit ihm auf einer Weitzengarbe,", "tokens": ["Sa\u00df", "mit", "ihm", "auf", "ei\u00b7ner", "Weit\u00b7zen\u00b7gar\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "L\u00e4chelt' ihm zur Arbeit Muth.", "tokens": ["L\u00e4\u00b7chelt'", "ihm", "zur", "Ar\u00b7beit", "Muth", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Band den Weitzen, welchen Willhelm m\u00e4hte,", "tokens": ["Band", "den", "Weit\u00b7zen", ",", "wel\u00b7chen", "Will\u00b7helm", "m\u00e4h\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "PWAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Band, und \u00e4ugelt' ihrem Liebling nach,", "tokens": ["Band", ",", "und", "\u00e4u\u00b7gelt'", "ih\u00b7rem", "Lieb\u00b7ling", "nach", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Bis die K\u00fchlung kam, und Abendr\u00f6the", "tokens": ["Bis", "die", "K\u00fch\u00b7lung", "kam", ",", "und", "A\u00b7bend\u00b7r\u00f6\u00b7the"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "$,", "KON", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Durch die falben Westgew\u00f6lke brach.", "tokens": ["Durch", "die", "fal\u00b7ben", "West\u00b7ge\u00b7w\u00f6l\u00b7ke", "brach", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "\u00dcber alles war ihm R\u00f6\u00dfchen theuer,", "tokens": ["\u00dc\u00b7ber", "al\u00b7les", "war", "ihm", "R\u00f6\u00df\u00b7chen", "theu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "VAFIN", "PPER", "NN", "ADJD", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "War sein Taggedanke, war sein Traum.", "tokens": ["War", "sein", "Tag\u00b7ge\u00b7dan\u00b7ke", ",", "war", "sein", "Traum", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "$,", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Wie sich R\u00f6\u00dfchen liebten und ihr Treuer,", "tokens": ["Wie", "sich", "R\u00f6\u00df\u00b7chen", "lieb\u00b7ten", "und", "ihr", "Treu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "NN", "VVFIN", "KON", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Lieben sich die Engel kaum.", "tokens": ["Lie\u00b7ben", "sich", "die", "En\u00b7gel", "kaum", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "PRF", "ART", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Willhelm, Willhelm! Sterbeglocken hallen,", "tokens": ["Will\u00b7helm", ",", "Will\u00b7helm", "!", "Ster\u00b7be\u00b7glo\u00b7cken", "hal\u00b7len", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "NE", "$.", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Und die Grabges\u00e4nge heben an,", "tokens": ["Und", "die", "Grab\u00b7ge\u00b7s\u00e4n\u00b7ge", "he\u00b7ben", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Schwarzbeflorte Trauerleute wallen,", "tokens": ["Schwarz\u00b7be\u00b7flor\u00b7te", "Trau\u00b7er\u00b7leu\u00b7te", "wal\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und die Todtenkrone weht voran.", "tokens": ["Und", "die", "Tod\u00b7ten\u00b7kro\u00b7ne", "weht", "vo\u00b7ran", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "p