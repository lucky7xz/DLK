{"dta.poem.18249": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "28.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1886", "urn": "urn:nbn:de:kobv:b4-200905192665", "language": ["de:0.99"], "booktitle": "Holz, Arno: Das Buch der Zeit. Lieder eines Modernen. Z\u00fcrich, 1886."}, "poem": {"stanza.1": {"line.1": {"text": "Ein blutiger Frevel ist diese Zeit!", "tokens": ["Ein", "blu\u00b7ti\u00b7ger", "Fre\u00b7vel", "ist", "die\u00b7se", "Zeit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PDAT", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Am h\u00f6lzernen Kreuz verr\u00f6chelt der Gott,", "tokens": ["Am", "h\u00f6l\u00b7zer\u00b7nen", "Kreuz", "ver\u00b7r\u00f6\u00b7chelt", "der", "Gott", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Kindern und Thoren ein seichter Spott;", "tokens": ["Kin\u00b7dern", "und", "Tho\u00b7ren", "ein", "seich\u00b7ter", "Spott", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Verl\u00f6scht ist am Himmel das letzte Roth,", "tokens": ["Ver\u00b7l\u00f6scht", "ist", "am", "Him\u00b7mel", "das", "letz\u00b7te", "Roth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Ueber die Welt hin schreitet der Tod,", "tokens": ["Ue\u00b7ber", "die", "Welt", "hin", "schrei\u00b7tet", "der", "Tod", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Und trunken durch die Gewitternacht klingt", "tokens": ["Und", "trun\u00b7ken", "durch", "die", "Ge\u00b7wit\u00b7ter\u00b7nacht", "klingt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+--+-++", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Das s\u00fcndige Lied, das die Nachtigall singt.", "tokens": ["Das", "s\u00fcn\u00b7di\u00b7ge", "Lied", ",", "das", "die", "Nach\u00b7ti\u00b7gall", "singt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.2": {"line.1": {"text": "Die Menschheit weint um ihr Paradies,", "tokens": ["Die", "Menschheit", "weint", "um", "ihr", "Pa\u00b7ra\u00b7dies", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Draus sie ihr eigener D\u00e4mon verstie\u00df,", "tokens": ["Draus", "sie", "ihr", "ei\u00b7ge\u00b7ner", "D\u00e4\u00b7mon", "ver\u00b7stie\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Und heimlich zischt ihr die rothe Wuth", "tokens": ["Und", "heim\u00b7lich", "zischt", "ihr", "die", "ro\u00b7the", "Wuth"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ihre Parole zu: Gold und Blut!", "tokens": ["Ih\u00b7re", "Pa\u00b7ro\u00b7le", "zu", ":", "Gold", "und", "Blut", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKVZ", "$.", "NN", "KON", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.5": {"text": "Gold und Blut, Blut und Gold!", "tokens": ["Gold", "und", "Blut", ",", "Blut", "und", "Gold", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "+-++-+", "measure": "unknown.measure.tetra"}, "line.6": {"text": "Hei wie das klappert, hei wie das rollt!", "tokens": ["Hei", "wie", "das", "klap\u00b7pert", ",", "hei", "wie", "das", "rollt", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "PDS", "VVFIN", "$,", "ADJD", "KOKOM", "PDS", "VVFIN", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Und w\u00fcst dazwischen kr\u00e4ht der Hahn:", "tokens": ["Und", "w\u00fcst", "da\u00b7zwi\u00b7schen", "kr\u00e4ht", "der", "Hahn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PAV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Volksohnmacht und C\u00e4sarenwahn!", "tokens": ["Volk\u00b7sohn\u00b7macht", "und", "C\u00e4\u00b7sa\u00b7ren\u00b7wahn", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NE", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.3": {"line.1": {"text": "Und immer dunkler wird die Nacht,", "tokens": ["Und", "im\u00b7mer", "dunk\u00b7ler", "wird", "die", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJA", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Liebe schl\u00e4ft ein und der Ha\u00df erwacht", "tokens": ["Die", "Lie\u00b7be", "schl\u00e4ft", "ein", "und", "der", "Ha\u00df", "er\u00b7wacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "KON", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und immer \u00fcppiger dehnt sich die Lust", "tokens": ["Und", "im\u00b7mer", "\u00fcp\u00b7pi\u00b7ger", "dehnt", "sich", "die", "Lust"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADJD", "VVFIN", "PRF", "ART", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und immer angstvoller schwillt die Brust;", "tokens": ["Und", "im\u00b7mer", "angst\u00b7vol\u00b7ler", "schwillt", "die", "Brust", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "VVFIN", "ART", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Kein Stern, der blau durch die Wolken bricht,", "tokens": ["Kein", "Stern", ",", "der", "blau", "durch", "die", "Wol\u00b7ken", "bricht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PRELS", "ADJD", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Kein Lied, das s\u00fc\u00df von Erl\u00f6sung spricht \u2014", "tokens": ["Kein", "Lied", ",", "das", "s\u00fc\u00df", "von", "Er\u00b7l\u00f6\u00b7sung", "spricht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PRELS", "ADJD", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Mein Herz schl\u00e4gt laut, mein Gewissen schreit:", "tokens": ["Mein", "Herz", "schl\u00e4gt", "laut", ",", "mein", "Ge\u00b7wis\u00b7sen", "schreit", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJD", "$,", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Ein blutiger Frevel ist diese Zeit!", "tokens": ["Ein", "blu\u00b7ti\u00b7ger", "Fre\u00b7vel", "ist", "die\u00b7se", "Zeit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PDAT", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}}}}