meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Innerlicher Sehnsucht f\u00fchlet,", "tokens": ["In\u00b7ner\u00b7li\u00b7cher", "Sehn\u00b7sucht", "f\u00fch\u00b7let", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ja, wo noch vor Schmerzen seyn", "tokens": ["Ja", ",", "wo", "noch", "vor", "Schmer\u00b7zen", "seyn"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "PWAV", "ADV", "APPR", "NN", "VAINF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Und Verstellung vor den Leuten", "tokens": ["Und", "Ver\u00b7stel\u00b7lung", "vor", "den", "Leu\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Niemahls aus dem Garne gleiten,", "tokens": ["Nie\u00b7mahls", "aus", "dem", "Gar\u00b7ne", "glei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Ist die Lust schon engelrein.", "tokens": ["Ist", "die", "Lust", "schon", "en\u00b7gel\u00b7rein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Solche Lust steht Engeln an,", "tokens": ["Sol\u00b7che", "Lust", "steht", "En\u00b7geln", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Die noch Unschuld an sich haben;", "tokens": ["Die", "noch", "Un\u00b7schuld", "an", "sich", "ha\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "NN", "APPR", "PRF", "VAFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Auf der Wangen Rosenbahn", "tokens": ["Auf", "der", "Wan\u00b7gen", "Ro\u00b7sen\u00b7bahn"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NE"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Darf man nicht sein Pfund vergraben.", "tokens": ["Darf", "man", "nicht", "sein", "Pfund", "ver\u00b7gra\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "PTKNEG", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Klug, verschwiegen und getreu", "tokens": ["Klug", ",", "ver\u00b7schwie\u00b7gen", "und", "ge\u00b7treu"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "VVPP", "KON", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Macht die Liebe stets zur Tugend;", "tokens": ["Macht", "die", "Lie\u00b7be", "stets", "zur", "Tu\u00b7gend", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADV", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Kinder, braucht den Lenz der Jugend,", "tokens": ["Kin\u00b7der", ",", "braucht", "den", "Lenz", "der", "Ju\u00b7gend", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Sonst ereilt euch sp\u00e4te Reu.", "tokens": ["Sonst", "er\u00b7eilt", "euch", "sp\u00e4\u00b7te", "Reu", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVFIN", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Schwester an der Redligkeit,", "tokens": ["Schwes\u00b7ter", "an", "der", "Red\u00b7lig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Du, o Schwester am Gem\u00fcthe,", "tokens": ["Du", ",", "o", "Schwes\u00b7ter", "am", "Ge\u00b7m\u00fc\u00b7the", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "FM", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "G\u00f6nne mir nur jederzeit", "tokens": ["G\u00f6n\u00b7ne", "mir", "nur", "je\u00b7der\u00b7zeit"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PPER", "ADV", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ein'ge Strahlen deiner G\u00fcte;", "tokens": ["Ein'\u00b7ge", "Strah\u00b7len", "dei\u00b7ner", "G\u00fc\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Schlag dein Meineid aus dem Sinn,", "tokens": ["Schlag", "dein", "Mei\u00b7neid", "aus", "dem", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.6": {"text": "Der dich nechst herumgef\u00fchret:", "tokens": ["Der", "dich", "nechst", "her\u00b7um\u00b7ge\u00b7f\u00fch\u00b7ret", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Wer ein falsches Herz verlieret,", "tokens": ["Wer", "ein", "fal\u00b7sches", "Herz", "ver\u00b7lie\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "De\u00dfen Schaden ist Gewinn.", "tokens": ["De\u00b7\u00dfen", "Scha\u00b7den", "ist", "Ge\u00b7winn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VAFIN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}