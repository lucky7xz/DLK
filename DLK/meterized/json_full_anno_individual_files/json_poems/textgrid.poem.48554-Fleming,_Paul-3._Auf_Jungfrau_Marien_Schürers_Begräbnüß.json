 "iambic.tri"}, "line.13": {"text": "Im \u00dcbrigen will ich,", "tokens": ["Im", "\u00dcb\u00b7ri\u00b7gen", "will", "ich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VMFIN", "PPER", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.14": {"text": "wie ich denn soll, durch meiner Verse Preis", "tokens": ["wie", "ich", "denn", "soll", ",", "durch", "mei\u00b7ner", "Ver\u00b7se", "Preis"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADV", "VMFIN", "$,", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "gesch\u00e4ftig sein umb dich,", "tokens": ["ge\u00b7sch\u00e4f\u00b7tig", "sein", "umb", "dich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAINF", "APPR", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.16": {"text": "will wenden an nicht angelehrten Flei\u00df,", "tokens": ["will", "wen\u00b7den", "an", "nicht", "an\u00b7ge\u00b7lehr\u00b7ten", "Flei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "APPR", "PTKNEG", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "da\u00df die, so dieses lesen,", "tokens": ["da\u00df", "die", ",", "so", "die\u00b7ses", "le\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "$,", "ADV", "PDAT", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.18": {"text": "auch melden meinen Sin,", "tokens": ["auch", "mel\u00b7den", "mei\u00b7nen", "Sin", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.19": {"text": "da\u00df ich dir hold gewesen,", "tokens": ["da\u00df", "ich", "dir", "hold", "ge\u00b7we\u00b7sen", 