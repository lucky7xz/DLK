{"textgrid.poem.48554": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "3. Auf Jungfrau Marien Sch\u00fcrers Begr\u00e4bn\u00fc\u00df", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die hei\u00dfe Z\u00e4hrenbach", "tokens": ["Die", "hei\u00b7\u00dfe", "Z\u00e4h\u00b7ren\u00b7bach"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "rinnt nur umbsonst die roten Backen ab.", "tokens": ["rinnt", "nur", "um\u00b7bsonst", "die", "ro\u00b7ten", "Ba\u00b7cken", "ab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Kein herzerzwungnes Ach", "tokens": ["Kein", "her\u00b7zer\u00b7zwung\u00b7nes", "Ach"], "token_info": ["word", "word", "word"], "pos": ["PIAT", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "f\u00fcllt f\u00fcr uns aus das schon gemachte Grab.", "tokens": ["f\u00fcllt", "f\u00fcr", "uns", "aus", "das", "schon", "ge\u00b7mach\u00b7te", "Grab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "APPR", "PRELS", "ADV", "ADJA", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.5": {"text": "Man hat noch nie vernommen,", "tokens": ["Man", "hat", "noch", "nie", "ver\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "da\u00df auf die Klagewort'", "tokens": ["da\u00df", "auf", "die", "Kla\u00b7ge\u00b7wort'"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "ein Geist sei r\u00fcckw\u00e4rts kommen,", "tokens": ["ein", "Geist", "sei", "r\u00fcck\u00b7w\u00e4rts", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "der einmal schon war fort.", "tokens": ["der", "ein\u00b7mal", "schon", "war", "fort", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "VAFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Die tugendvolle Lust", "tokens": ["Die", "tu\u00b7gend\u00b7vol\u00b7le", "Lust"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "der sch\u00f6nen vor, itzt totenfarbnen Stadt", "tokens": ["der", "sch\u00f6\u00b7nen", "vor", ",", "itzt", "to\u00b7ten\u00b7farb\u00b7nen", "Stadt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "PTKVZ", "$,", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "hat auch an das gemu\u00dft,", "tokens": ["hat", "auch", "an", "das", "ge\u00b7mu\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "ART", "ADJD", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.12": {"text": "was kurzer Zeit so Viel' ermordet hat.", "tokens": ["was", "kur\u00b7zer", "Zeit", "so", "Viel'", "er\u00b7mor\u00b7det", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJA", "NN", "ADV", "ADV", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Was uns an ihr beliebet,", "tokens": ["Was", "uns", "an", "ihr", "be\u00b7lie\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.14": {"text": "liegt vor uns kalt und tot.", "tokens": ["liegt", "vor", "uns", "kalt", "und", "tot", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.15": {"text": "Sei, junge Welt, betr\u00fcbet!", "tokens": ["Sei", ",", "jun\u00b7ge", "Welt", ",", "be\u00b7tr\u00fc\u00b7bet", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "$,", "ADJA", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.16": {"text": "Dich r\u00fchrt die meiste Not.", "tokens": ["Dich", "r\u00fchrt", "die", "meis\u00b7te", "Not", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.17": {"text": "Die neun Aonien,", "tokens": ["Die", "neun", "A\u00b7o\u00b7ni\u00b7en", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "CARD", "NN", "$,"], "meter": "-+-+--", "measure": "unknown.measure.di"}, "line.18": {"text": "der Nymphen Zunft, die ganze G\u00f6tterschar", "tokens": ["der", "Nym\u00b7phen", "Zunft", ",", "die", "gan\u00b7ze", "G\u00f6t\u00b7ter\u00b7schar"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "samt allen Chariten", "tokens": ["samt", "al\u00b7len", "Cha\u00b7ri\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PIAT", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.20": {"text": "stehn tief betr\u00fcbt umb die verh\u00fcllte Bahr'.", "tokens": ["stehn", "tief", "be\u00b7tr\u00fcbt", "umb", "die", "ver\u00b7h\u00fcll\u00b7te", "Bahr'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "VVPP", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Apollo kan nichts sagen,", "tokens": ["A\u00b7pol\u00b7lo", "kan", "nichts", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PIS", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.22": {"text": "ist Leid und \u00c4chzens voll,", "tokens": ["ist", "Leid", "und", "\u00c4ch\u00b7zens", "voll", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "KON", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.23": {"text": "hat mir es aufgetragen,", "tokens": ["hat", "mir", "es", "auf\u00b7ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "VVPP", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.24": {"text": "da\u00df ich sie klagen soll.", "tokens": ["da\u00df", "ich", "sie", "kla\u00b7gen", "soll", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.25": {"text": "Die Tugenden gehn aus", "tokens": ["Die", "Tu\u00b7gen\u00b7den", "gehn", "aus"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PTKVZ"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.26": {"text": "und samblen ein Cypre\u00df und Majoran;", "tokens": ["und", "samb\u00b7len", "ein", "Cyp\u00b7re\u00df", "und", "Ma\u00b7jo\u00b7ran", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.27": {"text": "sie winden manchen Strau\u00df", "tokens": ["sie", "win\u00b7den", "man\u00b7chen", "Strau\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.28": {"text": "und ziehen sie mit bunten Kr\u00e4nzen an.", "tokens": ["und", "zie\u00b7hen", "sie", "mit", "bun\u00b7ten", "Kr\u00e4n\u00b7zen", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.29": {"text": "Sie malen alle Pl\u00e4tze", "tokens": ["Sie", "ma\u00b7len", "al\u00b7le", "Pl\u00e4t\u00b7ze"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.30": {"text": "und rufen aus vor ihr,", "tokens": ["und", "ru\u00b7fen", "aus", "vor", "ihr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "APPR", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.31": {"text": "da\u00df man in Blumen setze", "tokens": ["da\u00df", "man", "in", "Blu\u00b7men", "set\u00b7ze"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.32": {"text": "sie, aller Blumen Zier.", "tokens": ["sie", ",", "al\u00b7ler", "Blu\u00b7men", "Zier", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PIAT", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.33": {"text": "Der deutsche Helikon", "tokens": ["Der", "deut\u00b7sche", "He\u00b7li\u00b7kon"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.34": {"text": "hat schwarzen Flor und Binden vorgetan.", "tokens": ["hat", "schwar\u00b7zen", "Flor", "und", "Bin\u00b7den", "vor\u00b7ge\u00b7tan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.35": {"text": "Cytheris und ihr Sohn,", "tokens": ["Cy\u00b7the\u00b7ris", "und", "ihr", "Sohn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "PPOSAT", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.36": {"text": "das liebe Kind, ziehn Trauerkleider an", "tokens": ["das", "lie\u00b7be", "Kind", ",", "ziehn", "Trau\u00b7er\u00b7klei\u00b7der", "an"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "VVFIN", "NN", "APPR"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.37": {"text": "und folgen derer Leichen,", "tokens": ["und", "fol\u00b7gen", "de\u00b7rer", "Lei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PDS", "NN", "$,"], "meter": "-+---+-", "measure": "dactylic.init"}, "line.38": {"text": "auf die sie dachten schon,", "tokens": ["auf", "die", "sie", "dach\u00b7ten", "schon", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "VVFIN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.39": {"text": "wie sie ihr wolten reichen", "tokens": ["wie", "sie", "ihr", "wol\u00b7ten", "rei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "PPER", "VMFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.40": {"text": "den Malschatz, ihren Lohn.", "tokens": ["den", "Mal\u00b7schatz", ",", "ih\u00b7ren", "Lohn", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.41": {"text": "Der bleiche W\u00fcrger steht,", "tokens": ["Der", "blei\u00b7che", "W\u00fcr\u00b7ger", "steht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.42": {"text": "als reu' es ihn, was er an ihr getan,", "tokens": ["als", "reu'", "es", "ihn", ",", "was", "er", "an", "ihr", "ge\u00b7tan", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPER", "PPER", "$,", "PWS", "PPER", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.43": {"text": "weil ihm zu Sinnen geht,", "tokens": ["weil", "ihm", "zu", "Sin\u00b7nen", "geht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.44": {"text": "was er ver\u00fcbt und nun nicht \u00e4ndern kan.", "tokens": ["was", "er", "ver\u00b7\u00fcbt", "und", "nun", "nicht", "\u00e4n\u00b7dern", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "KON", "ADV", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Doch solt' ihn was gereuen?", "tokens": ["Doch", "solt'", "ihn", "was", "ge\u00b7reu\u00b7en", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PIS", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Er w\u00fcrget Tag f\u00fcr Tag,", "tokens": ["Er", "w\u00fcr\u00b7get", "Tag", "f\u00fcr", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "f\u00fcr dem sich Alle scheuen,", "tokens": ["f\u00fcr", "dem", "sich", "Al\u00b7le", "scheu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PRF", "PIS", "VVINF", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "den Niemand scheuchen mag.", "tokens": ["den", "Nie\u00b7mand", "scheu\u00b7chen", "mag."], "token_info": ["word", "word", "word", "abbreviation"], "pos": ["ART", "PIS", "VVFIN", "NE"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Wie sollen wir ihm tun?", "tokens": ["Wie", "sol\u00b7len", "wir", "ihm", "tun", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "PPER", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Wir hei\u00dfen dich, Leib, unter deiner Gruft", "tokens": ["Wir", "hei\u00b7\u00dfen", "dich", ",", "Leib", ",", "un\u00b7ter", "dei\u00b7ner", "Gruft"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$,", "NN", "$,", "APPR", "PPOSAT", "NN"], "meter": "-+-+++-+-+", "measure": "zehnsilber"}, "line.7": {"text": "in sanfter Stille ruhn,", "tokens": ["in", "sanf\u00b7ter", "Stil\u00b7le", "ruhn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "bis dich einmal die Seele wieder ruft,", "tokens": ["bis", "dich", "ein\u00b7mal", "die", "See\u00b7le", "wie\u00b7der", "ruft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "die Seele, die schon h\u00f6ret,", "tokens": ["die", "See\u00b7le", ",", "die", "schon", "h\u00f6\u00b7ret", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "was sie vor nicht gewu\u00dft,", "tokens": ["was", "sie", "vor", "nicht", "ge\u00b7wu\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "bis sie auch dich verehret", "tokens": ["bis", "sie", "auch", "dich", "ver\u00b7eh\u00b7ret"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "mit jener langen Lust.", "tokens": ["mit", "je\u00b7ner", "lan\u00b7gen", "Lust", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "Im \u00dcbrigen will ich,", "tokens": ["Im", "\u00dcb\u00b7ri\u00b7gen", "will", "ich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VMFIN", "PPER", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.14": {"text": "wie ich denn soll, durch meiner Verse Preis", "tokens": ["wie", "ich", "denn", "soll", ",", "durch", "mei\u00b7ner", "Ver\u00b7se", "Preis"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADV", "VMFIN", "$,", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "gesch\u00e4ftig sein umb dich,", "tokens": ["ge\u00b7sch\u00e4f\u00b7tig", "sein", "umb", "dich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAINF", "APPR", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.16": {"text": "will wenden an nicht angelehrten Flei\u00df,", "tokens": ["will", "wen\u00b7den", "an", "nicht", "an\u00b7ge\u00b7lehr\u00b7ten", "Flei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "p