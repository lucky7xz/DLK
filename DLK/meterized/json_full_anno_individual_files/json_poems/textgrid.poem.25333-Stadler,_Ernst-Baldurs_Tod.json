{"textgrid.poem.25333": {"metadata": {"author": {"name": "Stadler, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "Baldurs Tod", "genre": "verse", "period": "N.A.", "pub_year": 1898, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Starr stand er,", "tokens": ["Starr", "stand", "er", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Den Todespfeil im Herzen,", "tokens": ["Den", "To\u00b7des\u00b7pfeil", "im", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wei\u00dfe Rosenbl\u00fcten im blonden Haar,", "tokens": ["Wei\u00b7\u00dfe", "Ro\u00b7sen\u00b7bl\u00fc\u00b7ten", "im", "blon\u00b7den", "Haar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Stand und wankte nicht.", "tokens": ["Stand", "und", "wank\u00b7te", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "PTKNEG", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "J\u00e4hes Grauen l\u00e4hmte die Himmel, und ein Schluchzen trug", "tokens": ["J\u00e4\u00b7hes", "Grau\u00b7en", "l\u00e4hm\u00b7te", "die", "Him\u00b7mel", ",", "und", "ein", "Schluch\u00b7zen", "trug"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "ART", "NN", "$,", "KON", "ART", "NN", "VVFIN"], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.6": {"text": "Zitternd der Abendwind durch d\u00e4mmernde Welten.", "tokens": ["Zit\u00b7ternd", "der", "Ab\u00b7end\u00b7wind", "durch", "d\u00e4m\u00b7mern\u00b7de", "Wel\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "+--+---+--+-", "measure": "dactylic.di.plus"}, "line.7": {"text": "Um seine F\u00fc\u00dfe schlug die Abendsonne,", "tokens": ["Um", "sei\u00b7ne", "F\u00fc\u00b7\u00dfe", "schlug", "die", "A\u00b7bend\u00b7son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Die Wolkenwehre niederrauschend, goldne Wogen.", "tokens": ["Die", "Wol\u00b7ken\u00b7weh\u00b7re", "nie\u00b7der\u00b7rau\u00b7schend", ",", "gold\u00b7ne", "Wo\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Vom Ost", "tokens": ["Vom", "Ost"], "token_info": ["word", "word"], "pos": ["APPRART", "NN"], "meter": "-+", "measure": "iambic.single"}, "line.10": {"text": "Trotzig den Blick gesenkt wuchs Loki aus der Nacht.", "tokens": ["Trot\u00b7zig", "den", "Blick", "ge\u00b7senkt", "wuchs", "Lo\u00b7ki", "aus", "der", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "VVPP", "VVFIN", "NE", "APPR", "ART", "NN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}}, "stanza.2": {"line.1": {"text": "\u00bbh\u00f6rt ihr das Lied, das Weltenschicksalslied?", "tokens": ["\u00bb", "h\u00f6rt", "ihr", "das", "Lied", ",", "das", "Wel\u00b7ten\u00b7schick\u00b7sals\u00b7lied", "?"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "ART", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wie Wetterschlag gellt's durch zermorschte Saiten.", "tokens": ["Wie", "Wet\u00b7ter\u00b7schlag", "gellt's", "durch", "zer\u00b7morschte", "Sai\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "NE", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Wild rauscht die Esche auf,", "tokens": ["Wild", "rauscht", "die", "E\u00b7sche", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "Urds Lippen beben.", "tokens": ["Urds", "Lip\u00b7pen", "be\u00b7ben", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NN", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Auf Flammenrossen peitscht im Sturm die Nacht.", "tokens": ["Auf", "Flam\u00b7men\u00b7ros\u00b7sen", "peitscht", "im", "Sturm", "die", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Die letzte Nacht, die Schicksalshochzeitsnacht:", "tokens": ["Die", "letz\u00b7te", "Nacht", ",", "die", "Schick\u00b7sals\u00b7hoch\u00b7zeits\u00b7nacht", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Eckpfeiler bersten, Balken stieben \u2013", "tokens": ["Eck\u00b7pfei\u00b7ler", "bers\u00b7ten", ",", "Bal\u00b7ken", "stie\u00b7ben", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "In Feuermeeren sinkt die alte Welt ...", "tokens": ["In", "Feu\u00b7er\u00b7mee\u00b7ren", "sinkt", "die", "al\u00b7te", "Welt", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "H\u00f6rt ihr das Lied, das Weltenschicksalslied?", "tokens": ["H\u00f6rt", "ihr", "das", "Lied", ",", "das", "Wel\u00b7ten\u00b7schick\u00b7sals\u00b7lied", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Hell schwillt es auf. Aus seinem Rauschen", "tokens": ["Hell", "schwillt", "es", "auf", ".", "Aus", "sei\u00b7nem", "Rau\u00b7schen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "PTKVZ", "$.", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Gl\u00fcht neuer Welten neues Morgenrot.", "tokens": ["Gl\u00fcht", "neu\u00b7er", "Wel\u00b7ten", "neu\u00b7es", "Mor\u00b7gen\u00b7rot", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Und neues Leben quillt aus meinem Blut", "tokens": ["Und", "neu\u00b7es", "Le\u00b7ben", "quillt", "aus", "mei\u00b7nem", "Blut"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "In roten Kelchen. Stirbt und bl\u00fcht \u2013", "tokens": ["In", "ro\u00b7ten", "Kel\u00b7chen", ".", "Stirbt", "und", "bl\u00fcht", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$.", "VVFIN", "KON", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Und stirbt ... Bis einst", "tokens": ["Und", "stirbt", "...", "Bis", "einst"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "$(", "APPR", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.15": {"text": "Ein gro\u00dfer Sonntag allen Welten d\u00e4mmert:", "tokens": ["Ein", "gro\u00b7\u00dfer", "Sonn\u00b7tag", "al\u00b7len", "Wel\u00b7ten", "d\u00e4m\u00b7mert", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Da braust ein einig Glockenl\u00e4uten durch die Luft.", "tokens": ["Da", "braust", "ein", "ei\u00b7nig", "Glo\u00b7cken\u00b7l\u00e4u\u00b7ten", "durch", "die", "Luft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJD", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Heilige Chor\u00e4le", "tokens": ["Hei\u00b7li\u00b7ge", "Cho\u00b7r\u00e4\u00b7le"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.18": {"text": "Reicht ewiger Morgenglanz von Hain zu Hain,", "tokens": ["Reicht", "e\u00b7wi\u00b7ger", "Mor\u00b7gen\u00b7glanz", "von", "Hain", "zu", "Hain", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "APPR", "NN", "APPR", "NE", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.19": {"text": "Und alles Leben ist", "tokens": ["Und", "al\u00b7les", "Le\u00b7ben", "ist"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "VAFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.20": {"text": "Ein einzig hehres Opferfest von Licht und Liebe.\u00ab", "tokens": ["Ein", "ein\u00b7zig", "heh\u00b7res", "Op\u00b7fer\u00b7fest", "von", "Licht", "und", "Lie\u00b7be", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "APPR", "NN", "KON", "NN", "$.", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Aufschauernd brach er auf das Purpurlager.", "tokens": ["Auf\u00b7schau\u00b7ernd", "brach", "er", "auf", "das", "Pur\u00b7pur\u00b7la\u00b7ger", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "Krampfend griff", "tokens": ["Kramp\u00b7fend", "griff"], "token_info": ["word", "word"], "pos": ["ADJD", "VVFIN"], "meter": "+-+", "measure": "trochaic.di"}, "line.23": {"text": "Die Hand zum letzten Male in das Quellgold,", "tokens": ["Die", "Hand", "zum", "letz\u00b7ten", "Ma\u00b7le", "in", "das", "Quell\u00b7gold", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "Das um ihn flutete und lie\u00df es breit", "tokens": ["Das", "um", "ihn", "flu\u00b7te\u00b7te", "und", "lie\u00df", "es", "breit"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "APPR", "PPER", "VVFIN", "KON", "VVFIN", "PPER", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "Und leuchtend durch die starren Finger rieseln.", "tokens": ["Und", "leuch\u00b7tend", "durch", "die", "star\u00b7ren", "Fin\u00b7ger", "rie\u00b7seln", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.26": {"text": "Funkensplitter stoben klingend durch den Abend,", "tokens": ["Fun\u00b7ken\u00b7split\u00b7ter", "sto\u00b7ben", "klin\u00b7gend", "durch", "den", "A\u00b7bend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.27": {"text": "Hei\u00dfes Wundenblut", "tokens": ["Hei\u00b7\u00dfes", "Wun\u00b7den\u00b7blut"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.28": {"text": "Troff durch die Wolken nieder \u2013 Sonnensamen", "tokens": ["Troff", "durch", "die", "Wol\u00b7ken", "nie\u00b7der", "\u2013", "Son\u00b7nen\u00b7sa\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["NN", "APPR", "ART", "NN", "PTKVZ", "$(", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.29": {"text": "Knospen zu zeugen roter Zauberbl\u00fcten.", "tokens": ["Knos\u00b7pen", "zu", "zeu\u00b7gen", "ro\u00b7ter", "Zau\u00b7ber\u00b7bl\u00fc\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VVFIN", "ADJA", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.30": {"text": "Ein greller Windsto\u00df prasselt in die Glut ...", "tokens": ["Ein", "grel\u00b7ler", "Wind\u00b7sto\u00df", "pras\u00b7selt", "in", "die", "Glut", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.31": {"text": "Ein Schrei ... Und Nacht ...", "tokens": ["Ein", "Schrei", "...", "Und", "Nacht", "..."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "KON", "NN", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.32": {"text": "Und Baldur to