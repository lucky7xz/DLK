{"dta.poem.19286": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "19.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nicht ist das Seyn zuerst und wird nachher gedacht,", "tokens": ["Nicht", "ist", "das", "Seyn", "zu\u00b7erst", "und", "wird", "nach\u00b7her", "ge\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VAFIN", "ART", "NN", "ADV", "KON", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vielmehr vom Denken erst wird Seyn hervorgebracht.", "tokens": ["Viel\u00b7mehr", "vom", "Den\u00b7ken", "erst", "wird", "Seyn", "her\u00b7vor\u00b7ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "ADV", "VAFIN", "PPOSAT", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Des Denkens Vorrang vor dem Seyn ist darin kund:", "tokens": ["Des", "Den\u00b7kens", "Vor\u00b7rang", "vor", "dem", "Seyn", "ist", "da\u00b7rin", "kund", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPR", "ART", "NN", "VAFIN", "PAV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Des Sch\u00f6pfers Denken ist der Sch\u00f6pfung innrer Grund.", "tokens": ["Des", "Sch\u00f6p\u00b7fers", "Den\u00b7ken", "ist", "der", "Sch\u00f6p\u00b7fung", "inn\u00b7rer", "Grund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Gott denkt sich selbst, und ist; er denkt, so ist die Welt,", "tokens": ["Gott", "denkt", "sich", "selbst", ",", "und", "ist", ";", "er", "denkt", ",", "so", "ist", "die", "Welt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PRF", "ADV", "$,", "KON", "VAFIN", "$.", "PPER", "VVFIN", "$,", "ADV", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und sein Gedank' ist das, was sie im Seyn erh\u00e4lt.", "tokens": ["Und", "sein", "Ge\u00b7dank'", "ist", "das", ",", "was", "sie", "im", "Seyn", "er\u00b7h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "PDS", "$,", "PRELS", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Gott denkt sich selbst, und ist; du denkst dich selbst und bist,", "tokens": ["Gott", "denkt", "sich", "selbst", ",", "und", "ist", ";", "du", "denkst", "dich", "selbst", "und", "bist", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PRF", "ADV", "$,", "KON", "VAFIN", "$.", "PPER", "VVFIN", "PPER", "ADV", "KON", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bist ewig wie Gott selbst, weil er dein Denken ist.", "tokens": ["Bist", "e\u00b7wig", "wie", "Gott", "selbst", ",", "weil", "er", "dein", "Den\u00b7ken", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "KOKOM", "NN", "ADV", "$,", "KOUS", "PPER", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Wie k\u00f6nnte je dein Seyn im Denken untergehn,", "tokens": ["Wie", "k\u00f6nn\u00b7te", "je", "dein", "Seyn", "im", "Den\u00b7ken", "un\u00b7ter\u00b7gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "ADV", "PPOSAT", "NN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da es das ist woraus mu\u00df ewig Seyn entstehn?", "tokens": ["Da", "es", "das", "ist", "wo\u00b7raus", "mu\u00df", "e\u00b7wig", "Seyn", "ent\u00b7stehn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PDS", "VAFIN", "ADV", "VMFIN", "ADJD", "PPOSAT", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Wer sagt, da\u00df sich der Quell in seinem Strom verliert,", "tokens": ["Wer", "sagt", ",", "da\u00df", "sich", "der", "Quell", "in", "sei\u00b7nem", "Strom", "ver\u00b7liert", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "KOUS", "PRF", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da ewig er vielmehr aus sich den Strom gebiert?", "tokens": ["Da", "e\u00b7wig", "er", "viel\u00b7mehr", "aus", "sich", "den", "Strom", "ge\u00b7biert", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "ADV", "APPR", "PRF", "ART", "NN", "VVPP", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}}}}}