{"dta.poem.17103": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "204.  Man achtt das Ewge nicht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Ach weh! umb eitle Lust verschertzt man Gutt und Biutt:", "tokens": ["Ach", "weh", "!", "umb", "eit\u00b7le", "Lust", "ver\u00b7schertzt", "man", "Gutt", "und", "Biutt", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "$.", "APPR", "ADJA", "NN", "VVFIN", "PIS", "ADJD", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und umb die Ewige fast niemand rweben thut!", "tokens": ["Und", "umb", "die", "E\u00b7wi\u00b7ge", "fast", "nie\u00b7mand", "rwe\u00b7ben", "thut", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "ADV", "PIS", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}