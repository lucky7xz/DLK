{"dta.poem.15990": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "44.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Warum oft gl\u00fccklich statt des Guten sei der B\u00f6se?", "tokens": ["Wa\u00b7rum", "oft", "gl\u00fcck\u00b7lich", "statt", "des", "Gu\u00b7ten", "sei", "der", "B\u00f6\u00b7se", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADJD", "APPR", "ART", "NN", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Frage fragest du, und willst, da\u00df ich sie l\u00f6se.", "tokens": ["Die", "Fra\u00b7ge", "fra\u00b7gest", "du", ",", "und", "willst", ",", "da\u00df", "ich", "sie", "l\u00f6\u00b7se", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,", "KON", "VMFIN", "$,", "KOUS", "PPER", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Den Knoten l\u00f6s' ich nicht, ich hau' ihn so entzwei:", "tokens": ["Den", "Kno\u00b7ten", "l\u00f6s'", "ich", "nicht", ",", "ich", "hau'", "ihn", "so", "ent\u00b7zwei", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKNEG", "$,", "PPER", "VVFIN", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df nie der B\u00f6se statt des Guten gl\u00fccklich sei.", "tokens": ["Da\u00df", "nie", "der", "B\u00f6\u00b7se", "statt", "des", "Gu\u00b7ten", "gl\u00fcck\u00b7lich", "sei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "APPR", "ART", "NN", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Er ist begl\u00fcckt, wenn er ein Gl\u00fcck wei\u00df zu verdienen,", "tokens": ["Er", "ist", "be\u00b7gl\u00fcckt", ",", "wenn", "er", "ein", "Gl\u00fcck", "wei\u00df", "zu", "ver\u00b7die\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das einem Bessern nicht des Dienstes werth geschienen.", "tokens": ["Das", "ei\u00b7nem", "Bes\u00b7sern", "nicht", "des", "Diens\u00b7tes", "werth", "ge\u00b7schie\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "PTKNEG", "ART", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Er ist anstelliger, arbeitsamer vielleicht,", "tokens": ["Er", "ist", "an\u00b7stel\u00b7li\u00b7ger", ",", "ar\u00b7beit\u00b7sa\u00b7mer", "viel\u00b7leicht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "$,", "ADV", "ADV", "$,"], "meter": "-+-+--+-+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und billig wird der Lohn dem Flei\u00dfigen gereicht.", "tokens": ["Und", "bil\u00b7lig", "wird", "der", "Lohn", "dem", "Flei\u00b7\u00dfi\u00b7gen", "ge\u00b7reicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Es ist der Erde Lohn, der mit ihm wird begraben;", "tokens": ["Es", "ist", "der", "Er\u00b7de", "Lohn", ",", "der", "mit", "ihm", "wird", "be\u00b7gra\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "NN", "$,", "PRELS", "APPR", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Gute nur wird den des Himmels ewig haben.", "tokens": ["Der", "Gu\u00b7te", "nur", "wird", "den", "des", "Him\u00b7mels", "e\u00b7wig", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VAFIN", "ART", "ART", "NN", "ADJD", "VAINF", "$."], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}}}}}