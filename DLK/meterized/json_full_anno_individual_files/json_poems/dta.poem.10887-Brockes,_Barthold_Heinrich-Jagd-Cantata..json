{"dta.poem.10887": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Jagd-Cantata.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1740", "urn": "urn:nbn:de:kobv:b4-200905198572", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wir verehren dessen G\u00fcte,", "tokens": ["Wir", "ver\u00b7eh\u00b7ren", "des\u00b7sen", "G\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PDS", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mit erkenntlichem Gem\u00fcthe,", "tokens": ["Mit", "er\u00b7kennt\u00b7li\u00b7chem", "Ge\u00b7m\u00fc\u00b7the", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Der die W\u00e4lder und Gefilde,", "tokens": ["Der", "die", "W\u00e4l\u00b7der", "und", "Ge\u00b7fil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Uns zur Lust, mit schnellem Wilde", "tokens": ["Uns", "zur", "Lust", ",", "mit", "schnel\u00b7lem", "Wil\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "APPRART", "NN", "$,", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "F\u00fcllt, sie, und durch sie uns, n\u00e4hrt.", "tokens": ["F\u00fcllt", ",", "sie", ",", "und", "durch", "sie", "uns", ",", "n\u00e4hrt", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "PPER", "$,", "KON", "APPR", "PPER", "PPER", "$,", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Ist nicht Gott, f\u00fcr so viel Gaben,", "tokens": ["Ist", "nicht", "Gott", ",", "f\u00fcr", "so", "viel", "Ga\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "NN", "$,", "APPR", "ADV", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Die wir, blo\u00df durch Jhn nur, haben,", "tokens": ["Die", "wir", ",", "blo\u00df", "durch", "Jhn", "nur", ",", "ha\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "PPER", "$,", "ADV", "APPR", "PPER", "ADV", "$,", "VAFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Dankens- und bewunderns-werth?", "tokens": ["Dan\u00b7kens", "und", "be\u00b7wun\u00b7derns\u00b7werth", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["TRUNC", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}