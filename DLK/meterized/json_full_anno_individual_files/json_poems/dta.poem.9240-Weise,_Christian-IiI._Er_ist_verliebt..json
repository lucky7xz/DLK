{"dta.poem.9240": {"metadata": {"author": {"name": "Weise, Christian", "birth": "N.A.", "death": "N.A."}, "title": "IiI.  \n Er ist verliebt.", "genre": "Lyrik; Drama; Prosa", "period": "N.A.", "pub_year": "1701", "urn": "urn:nbn:de:kobv:b4-25043-0", "language": ["de:0.99"], "booktitle": "Weise, Christian: \u00dcberfl\u00fc\u00dfige Gedancken Der gr\u00fcnenden jugend. Leipzig, 1701."}, "poem": {"stanza.1": {"line.1": {"text": "Du liebstes kind! so wilst du dich ergeben/", "tokens": ["Du", "liebs\u00b7tes", "kind", "!", "so", "wilst", "du", "dich", "er\u00b7ge\u00b7ben", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$.", "ADV", "VMFIN", "PPER", "PRF", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und darf ich nicht in ungewi\u00dfheit leben/", "tokens": ["Und", "darf", "ich", "nicht", "in", "un\u00b7ge\u00b7wi\u00df\u00b7heit", "le\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PTKNEG", "APPR", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ach! sieh den wunsch des reinen hertzens an/", "tokens": ["Ach", "!", "sieh", "den", "wunsch", "des", "rei\u00b7nen", "hert\u00b7zens", "an", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VVIMP", "ART", "NN", "ART", "ADJA", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der blo\u00df in dir vergn\u00fcgung finden kan.", "tokens": ["Der", "blo\u00df", "in", "dir", "ver\u00b7gn\u00fc\u00b7gung", "fin\u00b7den", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "PPER", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "2. Ich bin zu schwach mein leiden zu verbl\u00fcmen/", "tokens": ["Ich", "bin", "zu", "schwach", "mein", "lei\u00b7den", "zu", "ver\u00b7bl\u00fc\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKA", "ADJD", "PPOSAT", "VVINF", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich mu\u00df den glantz an deiner tugend r\u00fchmen/", "tokens": ["Ich", "mu\u00df", "den", "glantz", "an", "dei\u00b7ner", "tu\u00b7gend", "r\u00fch\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und wann ich die\u00df in meiner einfalt thu/", "tokens": ["Und", "wann", "ich", "die\u00df", "in", "mei\u00b7ner", "ein\u00b7falt", "thu", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "PDS", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "So nehm ich nur an angst und schmertzen zu.", "tokens": ["So", "nehm", "ich", "nur", "an", "angst", "und", "schmert\u00b7zen", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "NN", "KON", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "3. Dein sch\u00f6nes haar/ die augen und die wangen/", "tokens": ["Dein", "sch\u00f6\u00b7nes", "haar", "/", "die", "au\u00b7gen", "und", "die", "wan\u00b7gen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$(", "ART", "NN", "KON", "ART", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sind starck genug vor mich und mein verlangen/", "tokens": ["Sind", "starck", "ge\u00b7nug", "vor", "mich", "und", "mein", "ver\u00b7lan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "ADV", "APPR", "PPER", "KON", "PPOSAT", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch was dein sinn durch zeichen von sich giebt/", "tokens": ["Doch", "was", "dein", "sinn", "durch", "zei\u00b7chen", "von", "sich", "giebt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPOSAT", "NN", "APPR", "NN", "APPR", "PRF", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Das macht mich erst bi\u00df auf den todt verliebt.", "tokens": ["Das", "macht", "mich", "erst", "bi\u00df", "auf", "den", "todt", "ver\u00b7liebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "ADV", "APPR", "ART", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "4. Ach la\u00df/ mein kind/ mich nicht vergebens hoffen/", "tokens": ["Ach", "la\u00df", "/", "mein", "kind", "/", "mich", "nicht", "ver\u00b7ge\u00b7bens", "hof\u00b7fen", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "$(", "PPOSAT", "NN", "$(", "PPER", "PTKNEG", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und hat mich ja die s\u00fcsse last betroffen/", "tokens": ["Und", "hat", "mich", "ja", "die", "s\u00fcs\u00b7se", "last", "be\u00b7trof\u00b7fen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "ART", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So la\u00df dein liecht das labsal meiner pein/", "tokens": ["So", "la\u00df", "dein", "liecht", "das", "lab\u00b7sal", "mei\u00b7ner", "pein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPOSAT", "NN", "ART", "NN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und einen blick mein liebstes merckmahl seyn.", "tokens": ["Und", "ei\u00b7nen", "blick", "mein", "liebs\u00b7tes", "merck\u00b7mahl", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PPOSAT", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "5. Schau nur auf mich/ die neider m\u00f6gen stechen", "tokens": ["Schau", "nur", "auf", "mich", "/", "die", "nei\u00b7der", "m\u00f6\u00b7gen", "ste\u00b7chen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "ADV", "APPR", "PPER", "$(", "ART", "NN", "VMFIN", "VVINF"], "meter": "---+-+-+-+-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Und die\u00df und das von meiner liebe sprechen.", "tokens": ["Und", "die\u00df", "und", "das", "von", "mei\u00b7ner", "lie\u00b7be", "spre\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "KON", "ART", "APPR", "PPOSAT", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ich habe mich bi\u00dfher genug erkl\u00e4rt/", "tokens": ["Ich", "ha\u00b7be", "mich", "bi\u00df\u00b7her", "ge\u00b7nug", "er\u00b7kl\u00e4rt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Da\u00df dich mein hertz aus reiner pflicht begehrt.", "tokens": ["Da\u00df", "dich", "mein", "hertz", "aus", "rei\u00b7ner", "pflicht", "be\u00b7gehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "6. Du kanst dich schon an meine seele binden/", "tokens": ["Du", "kanst", "dich", "schon", "an", "mei\u00b7ne", "see\u00b7le", "bin\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Du wirst an treu nicht meines gleichen finden;", "tokens": ["Du", "wirst", "an", "treu", "nicht", "mei\u00b7nes", "glei\u00b7chen", "fin\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ADJD", "PTKNEG", "PPOSAT", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und bin ich nicht der pralerey gewohnt/", "tokens": ["Und", "bin", "ich", "nicht", "der", "pra\u00b7le\u00b7rey", "ge\u00b7wohnt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PTKNEG", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "So hat mein mund der warheit nie verschont.", "tokens": ["So", "hat", "mein", "mund", "der", "war\u00b7heit", "nie", "ver\u00b7schont", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "7. Ich stelle mich in dein geneigt belieben/", "tokens": ["Ich", "stel\u00b7le", "mich", "in", "dein", "ge\u00b7neigt", "be\u00b7lie\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "PPOSAT", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Du kanst mich jetzt erfreuen und betr\u00fcben/", "tokens": ["Du", "kanst", "mich", "jetzt", "er\u00b7freu\u00b7en", "und", "be\u00b7tr\u00fc\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "VVINF", "KON", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ich ehre dich und w\u00fcnsche deiner zier/", "tokens": ["Ich", "eh\u00b7re", "dich", "und", "w\u00fcn\u00b7sche", "dei\u00b7ner", "zier", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "KON", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Indessen steht der ausschlag noch bey dir.", "tokens": ["In\u00b7des\u00b7sen", "steht", "der", "aus\u00b7schlag", "noch", "bey", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "8. Nun wilst du dich zu gleicher brunst bekennen/", "tokens": ["Nun", "wilst", "du", "dich", "zu", "glei\u00b7cher", "brunst", "be\u00b7ken\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "APPR", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So wird uns auch kein ungewitter trennen/", "tokens": ["So", "wird", "uns", "auch", "kein", "un\u00b7ge\u00b7wit\u00b7ter", "tren\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "PIAT", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Jm leben wil ich stets dein wiederschein/", "tokens": ["Jm", "le\u00b7ben", "wil", "ich", "stets", "dein", "wie\u00b7der\u00b7schein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "VVINF", "VMFIN", "PPER", "ADV", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Jm tode selbst dein treuer schatten seyn.", "tokens": ["Jm", "to\u00b7de", "selbst", "dein", "treu\u00b7er", "schat\u00b7ten", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "PPOSAT", "ADJA", "VVFIN", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}