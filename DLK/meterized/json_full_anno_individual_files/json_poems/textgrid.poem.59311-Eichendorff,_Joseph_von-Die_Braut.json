{"textgrid.poem.59311": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Die Braut", "genre": "verse", "period": "N.A.", "pub_year": 1809, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wann die B\u00e4ume bl\u00fchn und sprossen", "tokens": ["Wann", "die", "B\u00e4u\u00b7me", "bl\u00fchn", "und", "spros\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "VVINF", "KON", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und die Lerche kehrt zur\u00fcck,", "tokens": ["Und", "die", "Ler\u00b7che", "kehrt", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denkt die Seele der Genossen,", "tokens": ["Denkt", "die", "See\u00b7le", "der", "Ge\u00b7nos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "F\u00fchlet fern' und nahes Gl\u00fcck.", "tokens": ["F\u00fch\u00b7let", "fern'", "und", "na\u00b7hes", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "KON", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Selig Weinen sel'ger Herzen!", "tokens": ["Se\u00b7lig", "Wei\u00b7nen", "sel'\u00b7ger", "Her\u00b7zen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn das Herz nichts weiter will,", "tokens": ["Wenn", "das", "Herz", "nichts", "wei\u00b7ter", "will", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PIS", "ADV", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nicht von Lust erf\u00fcllt, noch Schmerzen,", "tokens": ["Nicht", "von", "Lust", "er\u00b7f\u00fcllt", ",", "noch", "Schmer\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "NN", "VVPP", "$,", "ADV", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Aber fr\u00f6hlich ist und still.", "tokens": ["A\u00b7ber", "fr\u00f6h\u00b7lich", "ist", "und", "still", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Frischer sich die H\u00fcgel kr\u00e4nzen,", "tokens": ["Fri\u00b7scher", "sich", "die", "H\u00fc\u00b7gel", "kr\u00e4n\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PRF", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Heitrer lacht das weite Blau,", "tokens": ["Hei\u00b7trer", "lacht", "das", "wei\u00b7te", "Blau", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Alle Blumen sch\u00f6ner gl\u00e4nzen", "tokens": ["Al\u00b7le", "Blu\u00b7men", "sch\u00f6\u00b7ner", "gl\u00e4n\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Durch des Auges s\u00fc\u00dfen Tau.", "tokens": ["Durch", "des", "Au\u00b7ges", "s\u00fc\u00b7\u00dfen", "Tau", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Und soll denn das Lieben leiden,", "tokens": ["Und", "soll", "denn", "das", "Lie\u00b7ben", "lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "ART", "ADJA", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und, wer leidet, krank auch sein,", "tokens": ["Und", ",", "wer", "lei\u00b7det", ",", "krank", "auch", "sein", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PWS", "VVFIN", "$,", "ADJD", "ADV", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ach, so will ich keine Freuden,", "tokens": ["Ach", ",", "so", "will", "ich", "kei\u00b7ne", "Freu\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADV", "VMFIN", "PPER", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und mag nicht gesund mehr sein!", "tokens": ["Und", "mag", "nicht", "ge\u00b7sund", "mehr", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PTKNEG", "ADJD", "ADV", "VAINF", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Wann die B\u00e4ume bl\u00fchn und sprossen", "tokens": ["Wann", "die", "B\u00e4u\u00b7me", "bl\u00fchn", "und", "spros\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "VVINF", "KON", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und die Lerche kehrt zur\u00fcck,", "tokens": ["Und", "die", "Ler\u00b7che", "kehrt", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3"