{"textgrid.poem.63570": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "[ja, du bist noch jung und gr\u00fcn]", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ja, du bist noch jung und gr\u00fcn,", "tokens": ["Ja", ",", "du", "bist", "noch", "jung", "und", "gr\u00fcn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VAFIN", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "K\u00fchl dein Blick, dein L\u00e4cheln herbe,", "tokens": ["K\u00fchl", "dein", "Blick", ",", "dein", "L\u00e4\u00b7cheln", "her\u00b7be", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und sie schelten's eitles M\u00fchn,", "tokens": ["Und", "sie", "schel\u00b7ten's", "eit\u00b7les", "M\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df ich heut schon dich umwerbe.", "tokens": ["Da\u00df", "ich", "heut", "schon", "dich", "um\u00b7wer\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Doch dein Auge t\u00e4uscht mich nicht,", "tokens": ["Doch", "dein", "Au\u00b7ge", "t\u00e4uscht", "mich", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "+-+-+-+", "measure": "trochai