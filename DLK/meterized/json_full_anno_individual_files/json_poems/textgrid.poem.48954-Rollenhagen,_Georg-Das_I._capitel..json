{"textgrid.poem.48954": {"metadata": {"author": {"name": "Rollenhagen, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Das I. capitel.", "genre": "verse", "period": "N.A.", "pub_year": 1575, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das hofhalten, die feind und macht,", "tokens": ["Das", "hof\u00b7hal\u00b7ten", ",", "die", "feind", "und", "macht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "ART", "NN", "KON", "VVFIN", "$,"], "meter": "-++--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Das blutbad und erschrecklich schlacht", "tokens": ["Das", "blut\u00b7bad", "und", "er\u00b7schreck\u00b7lich", "schlacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ADJD", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der manhaften fr\u00f6sch und meushelden", "tokens": ["Der", "man\u00b7haf\u00b7ten", "fr\u00f6sch", "und", "meus\u00b7hel\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "KON", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Wil ich in disem buch vermelden.", "tokens": ["Wil", "ich", "in", "di\u00b7sem", "buch", "ver\u00b7mel\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Got verleih dazu rat und gnad,", "tokens": ["Got", "ver\u00b7leih", "da\u00b7zu", "rat", "und", "gnad", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PAV", "VVFIN", "KON", "NN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.6": {"text": "Das es zur ler und lust gerat.", "tokens": ["Das", "es", "zur", "ler", "und", "lust", "ge\u00b7rat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "APPRART", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ihr freien schulk\u00fcnst algemein,", "tokens": ["Ihr", "frei\u00b7en", "schul\u00b7k\u00fcnst", "al\u00b7ge\u00b7mein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "So der poeten ", "tokens": ["So", "der", "po\u00b7e\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ART", "NN"], "meter": "++-+-", "measure": "iambic.di"}, "line.9": {"text": "Tret auch herzu und steht mir bei,", "tokens": ["Tret", "auch", "her\u00b7zu", "und", "steht", "mir", "bei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "KON", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Das ich, was n\u00fctz und lieblich sei,", "tokens": ["Das", "ich", ",", "was", "n\u00fctz", "und", "lieb\u00b7lich", "sei", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "$,", "PRELS", "ADJD", "KON", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Weislich bedenk, k\u00fcnstlich aufzeich,", "tokens": ["Weis\u00b7lich", "be\u00b7denk", ",", "k\u00fcnst\u00b7lich", "auf\u00b7zeich", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$,", "ADJD", "ADJD", "$,"], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.12": {"text": "Das euch zu eren auch gereich.", "tokens": ["Das", "euch", "zu", "e\u00b7ren", "auch", "ge\u00b7reich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "PTKZU", "VVINF", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Denn weil ihr seid jungfreulein zart,", "tokens": ["Denn", "weil", "ihr", "seid", "jung\u00b7freu\u00b7lein", "zart", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VAFIN", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "So bleibt ihr stets fr\u00f6licher art,", "tokens": ["So", "bleibt", "ihr", "stets", "fr\u00f6\u00b7li\u00b7cher", "art", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.15": {"text": "Seht nicht ernstlich saur alle stund,", "tokens": ["Seht", "nicht", "ernst\u00b7lich", "saur", "al\u00b7le", "stund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ADJD", "VVFIN", "PIS", "VVFIN", "$,"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.16": {"text": "Sagt oft war mit lachendem mund.", "tokens": ["Sagt", "oft", "war", "mit", "la\u00b7chen\u00b7dem", "mund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "VAFIN", "APPR", "ADJA", "NN", "$."], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.17": {"text": "Damit im scherz die gute ler", "tokens": ["Da\u00b7mit", "im", "scherz", "die", "gu\u00b7te", "ler"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "APPRART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Bei der jugend schaff desto mer,", "tokens": ["Bei", "der", "ju\u00b7gend", "schaff", "des\u00b7to", "mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "Lasset die auch etwas weisheit", "tokens": ["Las\u00b7set", "die", "auch", "et\u00b7was", "weis\u00b7heit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADV", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Allhie lesen in fr\u00f6lichkeit,", "tokens": ["A\u00b7llhie", "le\u00b7sen", "in", "fr\u00f6\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.21": {"text": "Und an fr\u00f6schen und meusen sehen,", "tokens": ["Und", "an", "fr\u00f6\u00b7schen", "und", "meu\u00b7sen", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "KON", "VVINF", "VVINF", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.22": {"text": "Wie es pflegt in der welt zu gehen,", "tokens": ["Wie", "es", "pflegt", "in", "der", "welt", "zu", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "APPR", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.23": {"text": "Wie kans besser sein, denn das Musen", "tokens": ["Wie", "kans", "bes\u00b7ser", "sein", ",", "denn", "das", "Mu\u00b7sen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "VMFIN", "ADJD", "VAINF", "$,", "KON", "ART", "NN"], "meter": "-+---+-+-", "measure": "dactylic.init"}, "line.24": {"text": "Einmal reden von frosch und meusen?", "tokens": ["Ein\u00b7mal", "re\u00b7den", "von", "frosch", "und", "meu\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "APPR", "ADJD", "KON", "VVINF", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.25": {"text": "Und ihr junge lustige knaben,", "tokens": ["Und", "ihr", "jun\u00b7ge", "lus\u00b7ti\u00b7ge", "kna\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "ADJA", "NN", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.26": {"text": "Die lust zu erbar kurzweil haben", "tokens": ["Die", "lust", "zu", "er\u00b7bar", "kurz\u00b7weil", "ha\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PTKA", "ADJD", "ADJD", "VAFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.27": {"text": "Und suchet gern bei allen sachen,", "tokens": ["Und", "su\u00b7chet", "gern", "bei", "al\u00b7len", "sa\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "Das ihr in freuden habt zu lachen,", "tokens": ["Das", "ihr", "in", "freu\u00b7den", "habt", "zu", "la\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "APPR", "NN", "VAFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.29": {"text": "Wollet den reimen on beschweren", "tokens": ["Wol\u00b7let", "den", "rei\u00b7men", "on", "be\u00b7schwe\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "ART", "ADJA", "APPR", "VVINF"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.30": {"text": "Mit gutem nachdenken zuh\u00f6ren,", "tokens": ["Mit", "gu\u00b7tem", "nach\u00b7den\u00b7ken", "zu\u00b7h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.31": {"text": "Sol euch on zweifel mer nutz schaffen", "tokens": ["Sol", "euch", "on", "zwei\u00b7fel", "mer", "nutz", "schaf\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "APPR", "NN", "ADJA", "NN", "VVINF"], "meter": "-+-+---+-", "measure": "unknown.measure.tri"}, "line.32": {"text": "Denn alles narrenspil der affen,", "tokens": ["Denn", "al\u00b7les", "nar\u00b7ren\u00b7spil", "der", "af\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.33": {"text": "Der(en) man auch zu lachen pflegt,", "tokens": ["Der", "(", "en", ")", "man", "auch", "zu", "la\u00b7chen", "pflegt", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$(", "FM", "$(", "PIS", "ADV", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.34": {"text": "Obs gleich nicht vil in beutel tregt.", "tokens": ["Obs", "gleich", "nicht", "vil", "in", "beu\u00b7tel", "tregt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PTKNEG", "ADV", "APPR", "NE", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.35": {"text": "Die alten aber, die ihr ler", "tokens": ["Die", "al\u00b7ten", "a\u00b7ber", ",", "die", "ihr", "ler"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "ADV", "$,", "PRELS", "PPOSAT", "NN"], "meter": "-+-+-++-", "measure": "unknown.measure.tetra"}, "line.36": {"text": "Mit ernstem pochen machen schwer", "tokens": ["Mit", "erns\u00b7tem", "po\u00b7chen", "ma\u00b7chen", "schwer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVINF", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.37": {"text": "Und keine scherz mer leiden wollen,", "tokens": ["Und", "kei\u00b7ne", "scherz", "mer", "lei\u00b7den", "wol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.38": {"text": "Dismal ihr urlaub haben sollen,", "tokens": ["Dis\u00b7mal", "ihr", "ur\u00b7laub", "ha\u00b7ben", "sol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VAINF", "VMFIN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.39": {"text": "Ein wenig treten \u00fcberseit,", "tokens": ["Ein", "we\u00b7nig", "tre\u00b7ten", "\u00fc\u00b7ber\u00b7seit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.40": {"text": "Wollen sie h\u00f6ren ander zeit,", "tokens": ["Wol\u00b7len", "sie", "h\u00f6\u00b7ren", "an\u00b7der", "zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVFIN", "ADJD", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.41": {"text": "Wenn uns die nasn auch werden blaue", "tokens": ["Wenn", "uns", "die", "nasn", "auch", "wer\u00b7den", "blau\u00b7e"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "VAFIN", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.42": {"text": "Und har und bart geferbet graue,", "tokens": ["Und", "har", "und", "bart", "ge\u00b7fer\u00b7bet", "grau\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KON", "ADJD", "VVPP", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.43": {"text": "Odr noch wol eh, zu guter stund.", "tokens": ["Odr", "noch", "wol", "eh", ",", "zu", "gu\u00b7ter", "stund", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADV", "KOUS", "$,", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.44": {"text": "Wermut ist nicht immer gesund,", "tokens": ["Wer\u00b7mut", "ist", "nicht", "im\u00b7mer", "ge\u00b7sund", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PTKNEG", "ADV", "ADJD", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.45": {"text": "Man trinkt auch wol ein neuen wein", "tokens": ["Man", "trinkt", "auch", "wol", "ein", "neu\u00b7en", "wein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "ADV", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.46": {"text": "Und leckt ein frischen honigseim,", "tokens": ["Und", "leckt", "ein", "fri\u00b7schen", "ho\u00b7ni\u00b7gseim", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.47": {"text": "Damit sich die natur verneue.", "tokens": ["Da\u00b7mit", "sich", "die", "na\u00b7tur", "ver\u00b7neu\u00b7e", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.48": {"text": "Was teglich ist, bringet abscheue,", "tokens": ["Was", "teg\u00b7lich", "ist", ",", "brin\u00b7get", "ab\u00b7scheu\u00b7e", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VAFIN", "$,", "VVFIN", "VVFIN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.49": {"text": "Wie auch der alten meister trutz.", "tokens": ["Wie", "auch", "der", "al\u00b7ten", "meis\u00b7ter", "trutz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "ADJA", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.50": {"text": "Der wechsel ist vol lust und nutz", "tokens": ["Der", "wech\u00b7sel", "ist", "vol", "lust", "und", "nutz"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADJD", "VVFIN", "KON", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.51": {"text": "Und macht zur arbeit munter herzen:", "tokens": ["Und", "macht", "zur", "ar\u00b7beit", "mun\u00b7ter", "her\u00b7zen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.52": {"text": "Dazu dienet dis unser scherzen,", "tokens": ["Da\u00b7zu", "die\u00b7net", "dis", "un\u00b7ser", "scher\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PDS", "PPOSAT", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.53": {"text": "Das wir in gottes namn anfangen.", "tokens": ["Das", "wir", "in", "got\u00b7tes", "namn", "an\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.54": {"text": "Also ist der handel angangen.", "tokens": ["Al\u00b7so", "ist", "der", "han\u00b7del", "an\u00b7gan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Das hofhalten, die feind und macht,", "tokens": ["Das", "hof\u00b7hal\u00b7ten", ",", "die", "feind", "und", "macht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "ART", "NN", "KON", "VVFIN", "$,"], "meter": "-++--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Das blutbad und erschrecklich 