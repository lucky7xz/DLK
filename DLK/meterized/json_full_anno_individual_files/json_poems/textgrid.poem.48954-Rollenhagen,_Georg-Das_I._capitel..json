", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$(", "FM", "$(", "PIS", "ADV", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.34": {"text": "Obs gleich nicht vil in beutel tregt.", "tokens": ["Obs", "gleich", "nicht", "vil", "in", "beu\u00b7tel", "tregt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PTKNEG", "ADV", "APPR", "NE", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.35": {"text": "Die alten aber, die ihr ler", "tokens": ["Die", "al\u00b7ten", "a\u00b7ber", ",", "die", "ihr", "ler"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "ADV", "$,", "PRELS", "PPOSAT", "NN"], "meter": "-+-+-++-", "measure": "unknown.measure.tetra"}, "line.36": {"text": "Mit ernstem pochen machen schwer", "tokens": ["Mit", "erns\u00b7tem", "po\u00b7chen", "ma\u00b7chen", "schwer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVINF", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.37": {"text": "Und keine scherz mer leiden wollen,", "tokens": ["Und", "kei\u00b7ne", "scherz", "mer", "lei\u00b7den", "wol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.38": {"text": "Dismal ihr urlaub haben sollen,", "tokens": ["Dis\u00b7mal", "ihr", "ur\u00b7laub", "ha\u00b7ben", "sol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VAINF", "VMFIN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.39": {"text": "Ein wenig treten \u00fcberseit,", "tokens": ["Ein", "we\u00b7nig", "tre\u00b7ten", "\u00fc\u00b7ber\u00b7seit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.40": {"text": "Wollen sie h\u00f6ren ander zeit,", "tokens": ["Wol\u00b7len", "sie", "h\u00f6\u00b7ren", "an\u00b7der", "zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVFIN", "ADJD", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.41": {"text": "Wenn uns die nasn auch werden blaue", "tokens": ["Wenn", "uns", "die", "nasn", "auch", "wer\u00b7den", "blau\u00b7e"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "VAFIN", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.42": {"text": "Und har und bart geferbet graue,", "tokens": ["Und", "har", "und", "bart", "ge\u00b7fer\u00b7bet", "grau\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KON", "ADJD", "VVPP", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.43": {"text": "Odr noch wol eh, zu guter stund.", "tokens": ["Odr", "noch", "wol", "eh", ",", "zu", "gu\u00b7ter", "stund", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADV", "KOUS", "$,", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.44": {"text": "Wermut ist nicht immer gesund,", "tokens": ["Wer\u00b7mut", "ist", "nicht", "im\u00b7mer", "ge\u00b7sund", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PTKNEG", "ADV", "ADJD", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.45": {"text": "Man trinkt auch wol ein neuen wein", "tokens": ["Man", "trinkt", "auch", "wol", "ein", "neu\u00b7en", "wein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "ADV", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.46": {"text": "Und leckt ein frischen honigseim,", "tokens": ["Und", "leckt", "ein", "fri\u00b7schen", "ho\u00b7ni\u00b7gseim", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.47": {"text": "Damit sich die natur verneue.", "tokens": ["Da\u00b7mit", "sich", "die", "na\u00b7tur", "ver\u00b7neu\u00b7e", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.48": {"text": "Was teglich ist, bringet abscheue,", "tokens": ["Was", "teg\u00b7lich", "ist", ",", "brin\u00b7get", "ab\u00b7scheu\u00b7e", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VAFIN", "$,", "VVFIN", "VVFIN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.49": {"text": "Wie auch der alten meister trutz.", "tokens": ["Wie", "auch", "der", "al\u00b7ten", "meis\u00b7ter", "trutz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "ADJA", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.50": {"text": "Der wechsel ist vol lust und nutz", "tokens": ["Der", "wech\u00b7sel", "ist", "vol", "lust", "und", "nutz"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADJD", "VVFIN", "KON", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.51": {"text": "Und macht zur arbeit munter herzen:", "tokens": ["Und", "macht", "zur", "ar\u00b7beit", "mun\u00b7ter", "her\u00b7zen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.52": {"text": "Dazu dienet dis unser scherzen,", "tokens": ["Da\u00b7zu", "die\u00b7net", "dis", "un\u00b7ser", "scher\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PDS", "PPOSAT", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.53": {"text": "Das wir in gottes namn anfangen.", "tokens": ["Das", "wir", "in", "got\u00b7tes", "namn", "an\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.54": {"text": "Also ist der handel angangen.", "tokens": ["Al\u00b7so", "ist", "der", "han\u00b7del", "an\u00b7gan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}}}}}