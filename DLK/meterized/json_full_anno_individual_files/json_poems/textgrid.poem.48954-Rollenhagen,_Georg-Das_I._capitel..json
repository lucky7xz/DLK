, "measure": "iambic.di"}, "line.9": {"text": "Tret auch herzu und steht mir bei,", "tokens": ["Tret", "auch", "her\u00b7zu", "und", "steht", "mir", "bei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "KON", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Das ich, was n\u00fctz und lieblich sei,", "tokens": ["Das", "ich", ",", "was", "n\u00fctz", "und", "lieb\u00b7lich", "sei", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "$,", "PRELS", "ADJD", "KON", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Weislich bedenk, k\u00fcnstlich aufzeich,", "tokens": ["Weis\u00b7lich", "be\u00b7denk", ",", "k\u00fcnst\u00b7lich", "auf\u00b7zeich", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$,", "ADJD", "ADJD", "$,"], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.12": {"text": "Das euch zu eren auch gereich.", "tokens": ["Das", "euch", "zu", "e\u00b7ren", "auch", "ge\u00b7reich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "PTKZU", "VVINF", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Denn weil ihr seid jungfreulein zart,", "tokens": ["Denn", "weil", "ihr", "seid", "jung\u00b7freu\u00b7lein", "zart", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VAFIN", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "So bleibt ihr stets fr\u00f6licher art,", "tokens": ["So", "bleibt", "ihr", "stets", "fr\u00f6\u00b7li\u00b7cher", "art", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.15": {"text": "Seht nicht ernstlich saur alle stund,", "tokens": ["Seht", "nicht", "ernst\u00b7lich", "saur", "al\u00b7le", "stund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ADJD", "VVFIN", "PIS", "VVFIN", "$,"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.16": {"text": "Sagt oft war mit lachendem mund.", "tokens": ["Sagt", "oft", "war", "mit", "la\u00b7chen\u00b7dem", "mund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "VAFIN", "APPR", "ADJA", "NN", "$."], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.17": {"text": "Damit im scherz die gute ler", "tokens": ["Da\u00b7mit", "im", "scherz", "die", "gu\u00b7te", "ler"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "APPRART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Bei der jugend schaff desto mer,", "tokens": ["Bei", "der", "ju\u00b7gend", "schaff", "des\u00b7to", "mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "Lasset die auch etwas weisheit", "tokens": ["Las\u00b7set", "die", "auch", "et\u00b7was", "weis\u00b7heit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADV", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Allhie lesen in fr\u00f6lichkeit,", "tokens": ["A\u00b7llhie", "le\u00b7sen", "in", "fr\u00f6\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.21": {"text": "Und an fr\u00f6schen und meusen sehen,", "tokens": ["Und", "an", "fr\u00f6\u00b7