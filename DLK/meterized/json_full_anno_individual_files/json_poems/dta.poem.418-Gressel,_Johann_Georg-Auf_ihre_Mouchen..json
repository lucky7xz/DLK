{"dta.poem.418": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Auf ihre  Mouchen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Ach wirff doch Silvia die schwartzen Flecken hin/", "tokens": ["Ach", "wirff", "doch", "Sil\u00b7via", "die", "schwart\u00b7zen", "Fle\u00b7cken", "hin", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "ADV", "NE", "ART", "ADJA", "NN", "PTKVZ", "$("], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Wo die Natur zu seyn die Flecken selbst hinf\u00fchret/", "tokens": ["Wo", "die", "Na\u00b7tur", "zu", "seyn", "die", "Fle\u00b7cken", "selbst", "hin\u00b7f\u00fch\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PTKZU", "VAINF", "ART", "NN", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Denn sie beschimpffen nur die Wangen/ Mund und Kinn/", "tokens": ["Denn", "sie", "be\u00b7schimpf\u00b7fen", "nur", "die", "Wan\u00b7gen", "/", "Mund", "und", "Kinn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "ART", "NN", "$(", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Du bist ja sch\u00f6n genug/ und trefflich ausgezieret.", "tokens": ["Du", "bist", "ja", "sch\u00f6n", "ge\u00b7nug", "/", "und", "treff\u00b7lich", "aus\u00b7ge\u00b7zie\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "ADV", "$(", "KON", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Vielleicht gedenckst du dich dem Himmel zu vergleichen?", "tokens": ["Viel\u00b7leicht", "ge\u00b7denckst", "du", "dich", "dem", "Him\u00b7mel", "zu", "ver\u00b7glei\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wohl! siehe Sonn\u2019 und Mond f\u00fcr deiner Pracht erbleichen.", "tokens": ["Wohl", "!", "sie\u00b7he", "Sonn'", "und", "Mond", "f\u00fcr", "dei\u00b7ner", "Pracht", "er\u00b7blei\u00b7chen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "VVIMP", "NN", "KON", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}