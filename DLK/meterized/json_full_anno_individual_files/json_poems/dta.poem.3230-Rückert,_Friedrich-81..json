{"dta.poem.3230": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "81.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Bewiesen hat ein Freund von Geisterseherei,", "tokens": ["Be\u00b7wie\u00b7sen", "hat", "ein", "Freund", "von", "Geis\u00b7ter\u00b7se\u00b7he\u00b7rei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df jeder Dichter auch ein Geisterseher sei.", "tokens": ["Da\u00df", "je\u00b7der", "Dich\u00b7ter", "auch", "ein", "Geis\u00b7ter\u00b7se\u00b7her", "sei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "ADV", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "F\u00fcr einen Dichter hab' ich mich bisher gehalten,", "tokens": ["F\u00fcr", "ei\u00b7nen", "Dich\u00b7ter", "hab'", "ich", "mich", "bis\u00b7her", "ge\u00b7hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "PPER", "PRF", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und wohl hab' ich geseh'n auch geistige Gestalten.", "tokens": ["Und", "wohl", "hab'", "ich", "ge\u00b7seh'n", "auch", "geis\u00b7ti\u00b7ge", "Ge\u00b7stal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "VVFIN", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Doch Geister, was die Herrn mit ihren Geistern meinen,", "tokens": ["Doch", "Geis\u00b7ter", ",", "was", "die", "Herrn", "mit", "ih\u00b7ren", "Geis\u00b7tern", "mei\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "PRELS", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nie sah ich einen Geist, und will auch nie seh'n einen.", "tokens": ["Nie", "sah", "ich", "ei\u00b7nen", "Geist", ",", "und", "will", "auch", "nie", "seh'n", "ei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,", "KON", "VMFIN", "ADV", "ADV", "VVFIN", "ART", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Entweder bin ich denn kein Dichter, seh' ich ein,", "tokens": ["Ent\u00b7we\u00b7der", "bin", "ich", "denn", "kein", "Dich\u00b7ter", ",", "seh'", "ich", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "PIAT", "NN", "$,", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Oder ein Dichter mu\u00df kein Geisterseher seyn.", "tokens": ["O\u00b7der", "ein", "Dich\u00b7ter", "mu\u00df", "kein", "Geis\u00b7ter\u00b7se\u00b7her", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VMFIN", "PIAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}