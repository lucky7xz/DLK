{"textgrid.poem.40367": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "Furchtbar schlimm", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vater, Vater, der Weihnachtsmann!", "tokens": ["Va\u00b7ter", ",", "Va\u00b7ter", ",", "der", "Weih\u00b7nachts\u00b7mann", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "ART", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Eben hat er ganz laut geblasen,", "tokens": ["E\u00b7ben", "hat", "er", "ganz", "laut", "ge\u00b7bla\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADJD", "VVPP", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "viel lauter als der Postwagenmann.", "tokens": ["viel", "lau\u00b7ter", "als", "der", "Post\u00b7wa\u00b7gen\u00b7mann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Er ist gleich wieder weitergegangen,", "tokens": ["Er", "ist", "gleich", "wie\u00b7der", "wei\u00b7ter\u00b7ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "und hat zwei furchtbar lange Nasen,", "tokens": ["und", "hat", "zwei", "furcht\u00b7bar", "lan\u00b7ge", "Na\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "CARD", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "die waren ganz mit Eis behangen.", "tokens": ["die", "wa\u00b7ren", "ganz", "mit", "Eis", "be\u00b7han\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und die eine war wie ein Schornstein,", "tokens": ["Und", "die", "ei\u00b7ne", "war", "wie", "ein", "Schorn\u00b7stein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PIS", "VAFIN", "KOKOM", "ART", "NN", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "die andre ganz klein wie'n Fliegenbein,", "tokens": ["die", "and\u00b7re", "ganz", "klein", "wie'n", "Flie\u00b7gen\u00b7bein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADV", "ADJD", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "darauf ritten lauter, lauter Engelein,", "tokens": ["da\u00b7rauf", "rit\u00b7ten", "lau\u00b7ter", ",", "lau\u00b7ter", "En\u00b7ge\u00b7lein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ADJD", "$,", "PIAT", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.10": {"text": "die hielten eine gro\u00dfm\u00e4chtige Leine;", "tokens": ["die", "hiel\u00b7ten", "ei\u00b7ne", "gro\u00df\u00b7m\u00e4ch\u00b7ti\u00b7ge", "Lei\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "und seine Stiefel waren wie Deine.", "tokens": ["und", "sei\u00b7ne", "Stie\u00b7fel", "wa\u00b7ren", "wie", "Dei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "KOKOM", "PPOSAT", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "Und an der Leine, da ging ein Herr,", "tokens": ["Und", "an", "der", "Lei\u00b7ne", ",", "da", "ging", "ein", "Herr", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.13": {"text": "ja wirklich, Vater, wie'n alter B\u00e4r,", "tokens": ["ja", "wirk\u00b7lich", ",", "Va\u00b7ter", ",", "wie'n", "al\u00b7ter", "B\u00e4r", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "NN", "$,", "KOUS", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.14": {"text": "und die Engelein machten hottehott;", "tokens": ["und", "die", "En\u00b7ge\u00b7lein", "mach\u00b7ten", "hot\u00b7te\u00b7hott", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ADJD", "$."], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.15": {"text": "ich glaube, das war der liebe Gott.", "tokens": ["ich", "glau\u00b7be", ",", "das", "war", "der", "lie\u00b7be", "Gott", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PDS", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.16": {"text": "Denn er brummte furchtbar mit dem Mund,", "tokens": ["Denn", "er", "brumm\u00b7te", "furcht\u00b7bar", "mit", "dem", "Mund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "--+-++--+", "measure": "iambic.tetra.chol"}, "line.17": {"text": "ganz furchtbar schlimm! ja wirklich! und \u2013", "tokens": ["ganz", "furcht\u00b7bar", "schlimm", "!", "ja", "wirk\u00b7lich", "!", "und", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADJD", "ADJD", "$.", "ADV", "ADJD", "$.", "KON", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "\"aber Detta, du schwindelst ja;", "tokens": ["\"", "a\u00b7ber", "Det\u00b7ta", ",", "du", "schwin\u00b7delst", "ja", ";"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "NE", "$,", "PPER", "VVFIN", "ADV", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "das sind ja wieder lauter L\u00fcgen!\"", "tokens": ["das", "sind", "ja", "wie\u00b7der", "lau\u00b7ter", "L\u00fc\u00b7gen", "!", "\""], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ADV", "PIAT", "NN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Na, was schad't denn das, Papa?", "tokens": ["Na", ",", "was", "schad't", "denn", "das", ",", "Pa\u00b7pa", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ITJ", "$,", "PWS", "VVFIN", "KON", "PDS", "$,", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Das macht mir doch soviel Vergn\u00fcgen!", "tokens": ["Das", "macht", "mir", "doch", "so\u00b7viel", "Ver\u00b7gn\u00fc\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "\"so? \u2013 Na ja.\"", "tokens": ["\"", "so", "?", "\u2013", "Na", "ja", ".", "\""], "token_info": ["punct", "word", "punct", "punct", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "$.", "$(", "ITJ", "ADV", "$.", "$("], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.5": {"line.1": {"text": "Vate