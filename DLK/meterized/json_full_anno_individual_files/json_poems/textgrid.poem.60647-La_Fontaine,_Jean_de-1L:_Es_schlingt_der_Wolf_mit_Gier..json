{"textgrid.poem.60647": {"metadata": {"author": {"name": "La Fontaine, Jean de", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es schlingt der Wolf mit Gier.", "genre": "verse", "period": "N.A.", "pub_year": 1658, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es schlingt der Wolf mit Gier.", "tokens": ["Es", "schlingt", "der", "Wolf", "mit", "Gier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NE", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Als einst ein solches Tier", "tokens": ["Als", "einst", "ein", "sol\u00b7ches", "Tier"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "PIAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Gelage hielt, geschah's zu seinem Todesschrecken,", "tokens": ["Ge\u00b7la\u00b7ge", "hielt", ",", "ge\u00b7scha\u00b7h's", "zu", "sei\u00b7nem", "To\u00b7des\u00b7schre\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "NE", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Da\u00df ihm ein Knochen blieb in seiner Kehle stecken.", "tokens": ["Da\u00df", "ihm", "ein", "Kno\u00b7chen", "blieb", "in", "sei\u00b7ner", "Keh\u00b7le", "ste\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Er meinte schon, er m\u00fcsse alsobald verrecken,", "tokens": ["Er", "mein\u00b7te", "schon", ",", "er", "m\u00fcs\u00b7se", "al\u00b7so\u00b7bald", "ver\u00b7re\u00b7cken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "wo