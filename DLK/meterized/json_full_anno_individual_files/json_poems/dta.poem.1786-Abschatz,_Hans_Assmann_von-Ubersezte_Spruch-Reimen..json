{"dta.poem.1786": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Ubersezte Spruch-Reimen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Das Gl\u00fccke wendet sich/ der Ehre Rauch verschwindet/", "tokens": ["Das", "Gl\u00fc\u00b7cke", "wen\u00b7det", "sich", "/", "der", "Eh\u00b7re", "Rauch", "ver\u00b7schwin\u00b7det", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "$(", "ART", "NN", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Man k\u00f6m\u0303t um Geld und Gutt/ das sch\u00f6ne Weib wird alt", "tokens": ["Man", "k\u00f6m\u0303t", "um", "Geld", "und", "Gutt", "/", "das", "sch\u00f6\u00b7ne", "Weib", "wird", "alt"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "APPR", "NN", "KON", "NN", "$(", "ART", "ADJA", "NN", "VAFIN", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ein Freund bleibt wie er ist. Nicht Alter/ noch Gewalt/", "tokens": ["Ein", "Freund", "bleibt", "wie", "er", "ist", ".", "Nicht", "Al\u00b7ter", "/", "noch", "Ge\u00b7walt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KOKOM", "PPER", "VAFIN", "$.", "PTKNEG", "NN", "$(", "ADV", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Nicht Neyd noch Gl\u00fccke trennt/ was Lieb und Treue bindet:", "tokens": ["Nicht", "Neyd", "noch", "Gl\u00fc\u00b7cke", "trennt", "/", "was", "Lieb", "und", "Treu\u00b7e", "bin\u00b7det", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "ADV", "NN", "VVFIN", "$(", "PWS", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Was die Natur verkn\u00fcpfft/ wird offtermahls zurissen/", "tokens": ["Was", "die", "Na\u00b7tur", "ver\u00b7kn\u00fcpfft", "/", "wird", "off\u00b7ter\u00b7mahls", "zu\u00b7ris\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VVPP", "$(", "VAFIN", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Was Freundschafft feste macht/ wird ewig halten m\u00fcssen.", "tokens": ["Was", "Freund\u00b7schafft", "fes\u00b7te", "macht", "/", "wird", "e\u00b7wig", "hal\u00b7ten", "m\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "ADJA", "VVFIN", "$(", "VAFIN", "ADJD", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}