{"dta.poem.2359": {"metadata": {"author": {"name": "Neumark, Georg", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1652", "urn": "urn:nbn:de:kobv:b4-20428-0", "language": ["de:0.99"], "booktitle": "Neumark, Georg: Poetisch- und Musikalisches Lustw\u00e4ldchen. Hamburg, 1652."}, "poem": {"stanza.1": {"line.1": {"text": "Ich bin m\u00fcde mehr zu leben/", "tokens": ["Ich", "bin", "m\u00fc\u00b7de", "mehr", "zu", "le\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "ADV", "PTKZU", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nim mich liebster Gott zu Dir/", "tokens": ["Nim", "mich", "liebs\u00b7ter", "Gott", "zu", "Dir", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADJA", "NN", "APPR", "PPER", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mu\u00df Ich doch im ", "tokens": ["Mu\u00df", "Ich", "doch", "im"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ADV", "APPRART"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "T\u00e4glich in Betr\u00fcbn\u00fc\u00df schweben.", "tokens": ["T\u00e4g\u00b7lich", "in", "Be\u00b7tr\u00fcb\u00b7n\u00fc\u00df", "schwe\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Meine gr\u00f6ste ", "tokens": ["Mei\u00b7ne", "gr\u00f6s\u00b7te"], "token_info": ["word", "word"], "pos": ["PPOSAT", "ADJA"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Leufft dahin in Traurigkeit.", "tokens": ["Leufft", "da\u00b7hin", "in", "Trau\u00b7rig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PAV", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "M\u00f6cht\u2019 es Dir mein Gott gefallen/", "tokens": ["M\u00f6cht'", "es", "Dir", "mein", "Gott", "ge\u00b7fal\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "PPOSAT", "NN", "VVPP", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wolt\u2019 Ich hertzlich gern in", "tokens": ["Wolt'", "Ich", "hertz\u00b7lich", "gern", "in"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ADV", "ADV", "APPR"], "meter": "+-+---", "measure": "unknown.measure.di"}, "line.3": {"text": "Da mein ", "tokens": ["Da", "mein"], "token_info": ["word", "word"], "pos": ["KOUS", "PPOSAT"], "meter": "-+", "measure": "iambic.single"}, "line.4": {"text": "Da mein schmertzen-volles Wallen/", "tokens": ["Da", "mein", "schmert\u00b7zen\u00b7vol\u00b7les", "Wal\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Dieses ", "tokens": ["Die\u00b7ses"], "token_info": ["word"], "pos": ["PDAT"], "meter": "+-", "measure": "trochaic.single"}, "line.6": {"text": "Und sein endlichs Ende findt.", "tokens": ["Und", "sein", "end\u00b7lichs", "En\u00b7de", "findt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Ich verschmachte fast f\u00fcr Sorgen/", "tokens": ["Ich", "ver\u00b7schmach\u00b7te", "fast", "f\u00fcr", "Sor\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Meine milde ", "tokens": ["Mei\u00b7ne", "mil\u00b7de"], "token_info": ["word", "word"], "pos": ["PPOSAT", "NN"], "met