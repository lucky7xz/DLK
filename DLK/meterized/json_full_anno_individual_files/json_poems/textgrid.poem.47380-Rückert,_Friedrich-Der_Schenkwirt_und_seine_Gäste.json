{"textgrid.poem.47380": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Der Schenkwirt und seine G\u00e4ste", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zur Rede ward ich j\u00fcngst gestellt", "tokens": ["Zur", "Re\u00b7de", "ward", "ich", "j\u00fcngst", "ge\u00b7stellt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VAFIN", "PPER", "ADV", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von meinem Freund, dem alten,", "tokens": ["Von", "mei\u00b7nem", "Freund", ",", "dem", "al\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Versprochen habe mehr der Welt", "tokens": ["Ver\u00b7spro\u00b7chen", "ha\u00b7be", "mehr", "der", "Welt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mein Dichten als gehalten.", "tokens": ["Mein", "Dich\u00b7ten", "als", "ge\u00b7hal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KOKOM", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Den Vorwurf hab' ich umgestellt,", "tokens": ["Den", "Vor\u00b7wurf", "hab'", "ich", "um\u00b7ge\u00b7stellt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Erwidernd meinem Alten:", "tokens": ["Er\u00b7wi\u00b7dernd", "mei\u00b7nem", "Al\u00b7ten", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "PPO