so", "wird", "kr\u00e4n\u00b7ken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "ADV", "VAFIN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "Da\u00df mit grossem ", "tokens": ["Da\u00df", "mit", "gros\u00b7sem"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "APPR", "ADJA"], "meter": "+-+-", "measure": "trochaic.di"}, "line.22": {"text": "Er von heissen Liebesgluhten/", "tokens": ["Er", "von", "heis\u00b7sen", "Lie\u00b7bes\u00b7gluh\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.23": {"text": "Oder von den Meeresfluhten", "tokens": ["O\u00b7der", "von", "den", "Mee\u00b7res\u00b7fluh\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "Seiner Thr\u00e4nen sterben mu\u00df.", "tokens": ["Sei\u00b7ner", "Thr\u00e4\u00b7nen", "ster\u00b7ben", "mu\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}