{"textgrid.poem.32334": {"metadata": {"author": {"name": "Lessing, Gotthold Ephraim", "birth": "N.A.", "death": "N.A."}, "title": "11. Auf die Europa", "genre": "verse", "period": "N.A.", "pub_year": 1755, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als Zeus Europen lieb gewann,", "tokens": ["Als", "Zeus", "Eu\u00b7ro\u00b7pen", "lieb", "ge\u00b7wann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "NE", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nahm er, die Sch\u00f6ne zu besiegen,", "tokens": ["Nahm", "er", ",", "die", "Sch\u00f6\u00b7ne", "zu", "be\u00b7sie\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Verschiedene Gestalten an,", "tokens": ["Ver\u00b7schie\u00b7de\u00b7ne", "Ge\u00b7stal\u00b7ten", "an", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Verschieden ihr verschiedlich anzuliegen.", "tokens": ["Ver\u00b7schie\u00b7den", "ihr", "ver\u00b7schied\u00b7lich", "an\u00b7zu\u00b7lie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "VVIZU", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Als Gott zuerst erschien er ihr;", "tokens": ["Als", "Gott", "zu\u00b7erst", "er\u00b7schien", "er", "ihr", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADV", "VVFIN", "PPER", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dann als ein Mann, und endlich als ein Tier.", "tokens": ["Dann", "als", "ein", "Mann", ",", "und", "end\u00b7lich", "als", "ein", "Tier", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "ART", "NN", "$,", "KON", "ADV", "KOUS", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Umsonst legt er, als Gott, den Himmel ihr zu F\u00fc\u00dfen:", "tokens": ["Um\u00b7sonst", "legt", "er", ",", "als", "Gott", ",", "den", "Him\u00b7mel", "ihr", "zu", "F\u00fc\u00b7\u00dfen", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "NN", "$,", "ART", "NN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Stolz fliehet sie vor seinen K\u00fcssen.", "tokens": ["Stolz", "flie\u00b7het", "sie", "vor", "sei\u00b7nen", "K\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Umsonst fleht er, als Mann, in schmeichelhaftem Ton:", "tokens": ["Um\u00b7sonst", "fleht", "er", ",", "als", "Mann", ",", "in", "schmei\u00b7chel\u00b7haf\u00b7tem", "Ton", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "NN", "$,", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Verachtung war der Liebe Lohn.", "tokens": ["Ver\u00b7ach\u00b7tung", "war", "der", "Lie\u00b7be", "Lohn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Zuletzt \u2013 mein sch\u00f6n Geschlecht, gesagt zu deinen Ehren! \u2013", "tokens": ["Zu\u00b7letzt", "\u2013", "mein", "sch\u00f6n", "Ge\u00b7schlecht", ",", "ge\u00b7sagt", "zu", "dei\u00b7nen", "Eh\u00b7ren", "!", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "$(", "PPOSAT", "ADJD", "NN", "$,", "VVPP", "APPR", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Lie\u00df sie \u2013 von wem? \u2013 vom Bullen sich bet\u00f6ren.", "tokens": ["Lie\u00df", "sie", "\u2013", "von", "wem", "?", "\u2013", "vom", "Bul\u00b7len", "sich", "be\u00b7t\u00f6\u00b7ren", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$(", "APPR", "PWS", "$.", "$(", "APPRART", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Als Zeus Europen lieb gewann,", "tokens": ["Als", "Zeus", "Eu\u00b7ro\u00b7pen", "lieb", "ge\u00b7wann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "NE", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nahm er, die Sch\u00f6ne zu besiegen,", "tokens": ["Nahm", "er", ",", "die", "Sch\u00f6\u00b7ne", "zu", "be\u00b7sie\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Verschiedene Gestalten an,", "tokens": ["Ver\u00b7schie\u00b7de\u00b7ne", "Ge\u00b7stal\u00b7ten", "an", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Verschieden ihr verschiedlich anzuliegen.", "tokens": ["Ver\u00b7schie\u00b7den", "ihr", "ver\u00b7schied\u00b7lich", "an\u00b7zu\u00b7lie\u00b7