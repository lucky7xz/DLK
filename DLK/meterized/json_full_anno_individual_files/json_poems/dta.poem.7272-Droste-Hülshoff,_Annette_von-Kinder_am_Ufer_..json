{"dta.poem.7272": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Kinder am Ufer .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-20090519994", "language": ["de:0.99"], "booktitle": "Droste-H\u00fclshoff, Annette von: Gedichte. Stuttgart u. a., 1844."}, "poem": {"stanza.1": {"line.1": {"text": "O sieh doch! siehst du nicht die Blumenwolke", "tokens": ["O", "sieh", "doch", "!", "siehst", "du", "nicht", "die", "Blu\u00b7men\u00b7wol\u00b7ke"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ADV", "$.", "VVFIN", "PPER", "PTKNEG", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da dr\u00fcben in dem tiefsten Weiherkolke?", "tokens": ["Da", "dr\u00fc\u00b7ben", "in", "dem", "tiefs\u00b7ten", "Wei\u00b7her\u00b7kol\u00b7ke", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "O! das ist sch\u00f6n! h\u00e4tt' ich nur einen Stecken,", "tokens": ["O", "!", "das", "ist", "sch\u00f6n", "!", "h\u00e4tt'", "ich", "nur", "ei\u00b7nen", "Ste\u00b7cken", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PDS", "VAFIN", "ADJD", "$.", "VAFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Schmalzwei\u00dfe Kelch' mit dunkelrothen Flecken,", "tokens": ["Schmal\u00b7zwei\u00b7\u00dfe", "Kelch'", "mit", "dun\u00b7kel\u00b7ro\u00b7then", "Fle\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und jede Glocke ist frisirt so fein", "tokens": ["Und", "je\u00b7de", "Glo\u00b7cke", "ist", "fri\u00b7sirt", "so", "fein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "VAFIN", "VVPP", "ADV", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Wie unser w\u00e4chsern Engelchen im Schrein.", "tokens": ["Wie", "un\u00b7ser", "w\u00e4ch\u00b7sern", "En\u00b7gel\u00b7chen", "im", "Schrein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+---+", "measure": "zehnsilber"}, "line.7": {"text": "Was meinst du, schneid' ich einen Haselstab,", "tokens": ["Was", "meinst", "du", ",", "schneid'", "ich", "ei\u00b7nen", "Ha\u00b7sel\u00b7stab", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$,", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Und wat' ein wenig in die Furth hinab?", "tokens": ["Und", "wat'", "ein", "we\u00b7nig", "in", "die", "Furth", "hin\u00b7ab", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "PIS", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Pah! Fr\u00f6sch' und Hechte k\u00f6nnen mich nicht schrecken \u2014", "tokens": ["Pah", "!", "Fr\u00f6sch'", "und", "Hech\u00b7te", "k\u00f6n\u00b7nen", "mich", "nicht", "schre\u00b7cken"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "NN", "KON", "NN", "VMFIN", "PPER", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Allein, ob nicht vielleicht der Wassermann", "tokens": ["Al\u00b7lein", ",", "ob", "nicht", "viel\u00b7leicht", "der", "Was\u00b7ser\u00b7mann"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "KOUS", "PTKNEG", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Dort in den langen Kr\u00e4utern hocken kann?", "tokens": ["Dort", "in", "den", "lan\u00b7gen", "Kr\u00e4u\u00b7tern", "ho\u00b7cken", "kann", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Ich geh, ich gehe schon \u2014 ich gehe nicht \u2014", "tokens": ["Ich", "geh", ",", "ich", "ge\u00b7he", "schon", "ich", "ge\u00b7he", "nicht"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "ADV", "$(", "PPER", "VVFIN", "PTKNEG", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.13": {"text": "Mich d\u00fcnkt, ich sah am Grunde ein Gesicht \u2014", "tokens": ["Mich", "d\u00fcnkt", ",", "ich", "sah", "am", "Grun\u00b7de", "ein", "Ge\u00b7sicht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "APPRART", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Komm lass' uns lieber heim, die Sonne sticht!", "tokens": ["Komm", "lass'", "uns", "lie\u00b7ber", "heim", ",", "die", "Son\u00b7ne", "sticht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "VVFIN", "PPER", "ADV", "PTKVZ", "$,", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}