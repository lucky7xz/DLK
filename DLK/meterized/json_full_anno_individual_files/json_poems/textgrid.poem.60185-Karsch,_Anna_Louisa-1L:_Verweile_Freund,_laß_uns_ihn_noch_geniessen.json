{"textgrid.poem.60185": {"metadata": {"author": {"name": "Karsch, Anna Louisa", "birth": "N.A.", "death": "N.A."}, "title": "1L: Verweile Freund, la\u00df uns ihn noch geniessen", "genre": "verse", "period": "N.A.", "pub_year": 1762, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Verweile Freund, la\u00df uns ihn noch geniessen", "tokens": ["Ver\u00b7wei\u00b7le", "Freund", ",", "la\u00df", "uns", "ihn", "noch", "ge\u00b7nies\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "VVIMP", "PPER", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den Fr\u00fchlings-Abend, der gef\u00fchlt", "tokens": ["Den", "Fr\u00fch\u00b7lings\u00b7A\u00b7bend", ",", "der", "ge\u00b7f\u00fchlt"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "VVPP"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Von Blumen wird, die geizig sich verschliessen", "tokens": ["Von", "Blu\u00b7men", "wird", ",", "die", "gei\u00b7zig", "sich", "ver\u00b7schlies\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VAFIN", "$,", "PRELS", "ADJD", "PRF", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wenn sie der Thau gek\u00fchlt.", "tokens": ["Wenn", "sie", "der", "Thau", "ge\u00b7k\u00fchlt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Des Tages Thron wird von der Nacht besessen;", "tokens": ["Des", "Ta\u00b7ges", "Thron", "wird", "von", "der", "Nacht", "be\u00b7ses\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit tausend Sonnen \u00fcberstreut", "tokens": ["Mit", "tau\u00b7send", "Son\u00b7nen", "\u00fc\u00b7bers\u00b7treut"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "CARD", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Schwebt \u00fcber uns, von keiner Hand gemessen,", "tokens": ["Schwebt", "\u00fc\u00b7ber", "uns", ",", "von", "kei\u00b7ner", "Hand", "ge\u00b7mes\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "$,", "APPR", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ihr k\u00f6nigliches Kleid.", "tokens": ["Ihr", "k\u00f6\u00b7nig\u00b7li\u00b7ches", "Kleid", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Um ihren Sitz herrscht feyerliche Stille;", "tokens": ["Um", "ih\u00b7ren", "Sitz", "herrscht", "fey\u00b7er\u00b7li\u00b7che", "Stil\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Aus ihrem unumw\u00f6lkten Schoo\u00df", "tokens": ["Aus", "ih\u00b7rem", "un\u00b7um\u00b7w\u00f6lk\u00b7ten", "Schoo\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "F\u00e4hrt nicht der Blitz, nicht brechen mit Gebr\u00fclle", "tokens": ["F\u00e4hrt", "nicht", "der", "Blitz", ",", "nicht", "bre\u00b7chen", "mit", "Ge\u00b7br\u00fcl\u00b7le"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PTKNEG", "ART", "NN", "$,", "PTKNEG", "VVINF", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Donner Gottes los.", "tokens": ["Die", "Don\u00b7ner", "Got\u00b7tes", "los", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Doch, h\u00f6re Freund, was donnert uns zur Seite,", "tokens": ["Doch", ",", "h\u00f6\u00b7re", "Freund", ",", "was", "don\u00b7nert", "uns", "zur", "Sei\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VVFIN", "NN", "$,", "PWS", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das Ufer zittert von dem Knall,", "tokens": ["Das", "U\u00b7fer", "zit\u00b7tert", "von", "dem", "Knall", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gleich dem Get\u00f6s aus fernem Kriegesstreite;", "tokens": ["Gleich", "dem", "Ge\u00b7t\u00f6s", "aus", "fer\u00b7nem", "Krie\u00b7ges\u00b7strei\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}, "line.4": {"text": "Und Antwort giebt der Wall.", "tokens": ["Und", "Ant\u00b7wort", "giebt", "der", "Wall", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "ART", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Die Citadell, der Dom, die F\u00fcrsten-H\u00e4user", "tokens": ["Die", "Ci\u00b7ta\u00b7dell", ",", "der", "Dom", ",", "die", "F\u00fcrs\u00b7ten\u00b7H\u00e4u\u00b7ser"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die rufen diesem Donner nach \u2013 \u2013", "tokens": ["Die", "ru\u00b7fen", "die\u00b7sem", "Don\u00b7ner", "nach", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "VVFIN", "PDAT", "NN", "APPR", "$(", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So riefen H\u00fcgel j\u00fcngst, da Lorbeerreiser", "tokens": ["So", "rie\u00b7fen", "H\u00fc\u00b7gel", "j\u00fcngst", ",", "da", "Lor\u00b7beer\u00b7rei\u00b7ser"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "NN", "ADV", "$,", "KOUS", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Held in Sachsen brach!", "tokens": ["Der", "Held", "in", "Sach\u00b7sen", "brach", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Mein Blick verfolgt die steigende Raquete", "tokens": ["Mein", "Blick", "ver\u00b7folgt", "die", "stei\u00b7gen\u00b7de", "Ra\u00b7que\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die um den Rang der Sterne wirbt,", "tokens": ["Die", "um", "den", "Rang", "der", "Ster\u00b7ne", "wirbt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und da ihr Stolz von ewig gl\u00e4nzen redte,", "tokens": ["Und", "da", "ihr", "Stolz", "von", "e\u00b7wig", "gl\u00e4n\u00b7zen", "red\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPOSAT", "NN", "APPR", "ADJD", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Verl\u00f6scht und niederstirbt.", "tokens": ["Ver\u00b7l\u00f6scht", "und", "nie\u00b7ders\u00b7tirbt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "So hoch empor ist stolzer Muth gestiegen", "tokens": ["So", "hoch", "em\u00b7por", "ist", "stol\u00b7zer", "Muth", "ge\u00b7stie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "PTKVZ", "VAFIN", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In Friedrichs starken Feinden oft,", "tokens": ["In", "Fried\u00b7richs", "star\u00b7ken", "Fein\u00b7den", "oft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn sie von Wuth entflammt, sein Niederliegen", "tokens": ["Wenn", "sie", "von", "Wuth", "ent\u00b7flammt", ",", "sein", "Nie\u00b7der\u00b7lie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "NN", "VVPP", "$,", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Gew\u00fcnschet und gehofft.", "tokens": ["Ge\u00b7w\u00fcn\u00b7schet", "und", "ge\u00b7hofft", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "KON", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Was kommt dort auf dem Wasser hergezogen?", "tokens": ["Was", "kommt", "dort", "auf", "dem", "Was\u00b7ser", "her\u00b7ge\u00b7zo\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sind Mars und Venus voller Gluth", "tokens": ["Sind", "Mars", "und", "Ve\u00b7nus", "vol\u00b7ler", "Gluth"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "NN", "KON", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Von ihrer Laufbahn itzt herabgeflogen,", "tokens": ["Von", "ih\u00b7rer", "Lauf\u00b7bahn", "itzt", "her\u00b7ab\u00b7ge\u00b7flo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und brennen in der Fluth?", "tokens": ["Und", "bren\u00b7nen", "in", "der", "Fluth", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Sie treiben sich \u2013 \u2013 nun fahren sie zusammen", "tokens": ["Sie", "trei\u00b7ben", "sich", "\u2013", "\u2013", "nun", "fah\u00b7ren", "sie", "zu\u00b7sam\u00b7men"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "$(", "$(", "ADV", "VVFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie Pandamus und Diomed;", "tokens": ["Wie", "Pan\u00b7da\u00b7mus", "und", "Dio\u00b7med", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NE", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Zwo Schiffen gleich, wenn jegliches in Flammen", "tokens": ["Zwo", "Schif\u00b7fen", "gleich", ",", "wenn", "jeg\u00b7li\u00b7ches", "in", "Flam\u00b7men"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["CARD", "NN", "ADV", "$,", "KOUS", "PIS", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Gesetzt, zu Wolken geht.", "tokens": ["Ge\u00b7setzt", ",", "zu", "Wol\u00b7ken", "geht", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Freund, sage mir welch lieblich Ungeheuer", "tokens": ["Freund", ",", "sa\u00b7ge", "mir", "welch", "lieb\u00b7lich", "Un\u00b7ge\u00b7heu\u00b7er"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "VVFIN", "PPER", "PWAT", "ADJD", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Ward von der Kunst hervorgebracht?", "tokens": ["Ward", "von", "der", "Kunst", "her\u00b7vor\u00b7ge\u00b7bracht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Jetzt wird der Strom vom hochgespr\u00fctztem Feuer", "tokens": ["Jetzt", "wird", "der", "Strom", "vom", "hoch\u00b7ge\u00b7spr\u00fctz\u00b7tem", "Feu\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dem Aetna gleich gemacht!", "tokens": ["Dem", "A\u00b7et\u00b7na", "gleich", "ge\u00b7macht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "ADV", "VVPP", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.11": {"line.1": {"text": "Sch\u00f6nflammigt springt in tausend grossen Funken", "tokens": ["Sch\u00f6n\u00b7flam\u00b7migt", "springt", "in", "tau\u00b7send", "gros\u00b7sen", "Fun\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "APPR", "CARD", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Bogen Pracht, ich sehe sie", "tokens": ["Der", "Bo\u00b7gen", "Pracht", ",", "ich", "se\u00b7he", "sie"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "NN", "$,", "PPER", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und denke von der Zukunft Freude trunken:", "tokens": ["Und", "den\u00b7ke", "von", "der", "Zu\u00b7kunft", "Freu\u00b7de", "trun\u00b7ken", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "So springt zu Sans-Souci", "tokens": ["So", "springt", "zu", "Sans\u00b7Sou\u00b7ci"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "NE"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Dem Sieger hochentgegen jede Quelle.", "tokens": ["Dem", "Sie\u00b7ger", "ho\u00b7chent\u00b7ge\u00b7gen", "je\u00b7de", "Quel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Marmor-S\u00e4ulen regen sich;", "tokens": ["Die", "Mar\u00b7mor\u00b7S\u00e4u\u00b7len", "re\u00b7gen", "sich", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "PRF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Roms Helden-Geister wollen aus der H\u00f6lle", "tokens": ["Roms", "Hel\u00b7den\u00b7Geis\u00b7ter", "wol\u00b7len", "aus", "der", "H\u00f6l\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "VMFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Herauf zu Friederich!", "tokens": ["Her\u00b7auf", "zu", "Frie\u00b7de\u00b7rich", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Verweile Freund, la\u00df uns ihn noch geniessen", "tokens": ["Ver\u00b7wei\u00b7le", "Freund", ",", "la\u00df", "uns", "ihn", "noch", "ge\u00b7nies\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "VVIMP", "PPER", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den Fr\u00fchlings-Abend, der gef\u00fchlt", "tokens": ["Den", "Fr\u00fch\u00b7lings\u00b7A\u00b7bend", ",", "der", "ge\u00b7f\u00fchlt"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "VVPP"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Von Blumen wird, die geizig sich verschliessen", "tokens": ["Von", "Blu\u00b7men", "wird", ",", "die", "gei\u00b7zig", "sich", "ver\u00b7schlies\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VAFIN", "$,", "PRELS", "ADJD", "PRF", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wenn sie der Thau gek\u00fchlt.", "tokens": ["Wenn", "sie", "der", "Thau", "ge\u00b7k\u00fchlt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Des Tages Thron wird von der Nacht besessen;", "tokens": ["Des", "Ta\u00b7ges", "Thron", "wird", "von", "der", "Nacht", "be\u00b7ses\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-