{"textgrid.poem.63036": {"metadata": {"author": {"name": "Pfeffel, Gottlieb Konrad", "birth": "N.A.", "death": "N.A."}, "title": "1L: Das Reich der Affen ist, wie Pohlen,", "genre": "verse", "period": "N.A.", "pub_year": 1786, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das Reich der Affen ist, wie Pohlen,", "tokens": ["Das", "Reich", "der", "Af\u00b7fen", "ist", ",", "wie", "Poh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VAFIN", "$,", "PWAV", "NE", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Wahlreich; nur da\u00df Kapriolen", "tokens": ["Ein", "Wahl\u00b7reich", ";", "nur", "da\u00df", "Kap\u00b7ri\u00b7o\u00b7len"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$.", "ADV", "KOUS", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der Pavianen Thron verleihn.", "tokens": ["Der", "Pa\u00b7vi\u00b7a\u00b7nen", "Thron", "ver\u00b7leihn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Am Churtag ziehn die Candidaten", "tokens": ["Am", "Chur\u00b7tag", "ziehn", "die", "Can\u00b7di\u00b7da\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "In einen lichten Palmenhayn.", "tokens": ["In", "ei\u00b7nen", "lich\u00b7ten", "Pal\u00b7men\u00b7hayn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Hier weist das Volk und die Magnaten", "tokens": ["Hier", "weist", "das", "Volk", "und", "die", "Mag\u00b7na\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Den Werbern eine Cocosnu\u00df,", "tokens": ["Den", "Wer\u00b7bern", "ei\u00b7ne", "Co\u00b7cos\u00b7nu\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Die der im Sprunge pfl\u00fccken mu\u00df,", "tokens": ["Die", "der", "im", "Sprun\u00b7ge", "pfl\u00fc\u00b7cken", "mu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "APPRART", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Der sich zum Rang des Autokraten", "tokens": ["Der", "sich", "zum", "Rang", "des", "Au\u00b7to\u00b7kra\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PRF", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Erheben will. Vor kurzem starb", "tokens": ["Er\u00b7he\u00b7ben", "will", ".", "Vor", "kur\u00b7zem", "starb"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VMFIN", "$.", "APPR", "ADJA", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Schach Matz der vierte, gro\u00df an Thaten;", "tokens": ["Schach", "Matz", "der", "vier\u00b7te", ",", "gro\u00df", "an", "Tha\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ART", "ADJA", "$,", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Er fra\u00df f\u00fcr drey. Sogleich bewarb", "tokens": ["Er", "fra\u00df", "f\u00fcr", "drey", ".", "Sog\u00b7leich", "be\u00b7warb"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "CARD", "$.", "ADV", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Ein Trupp von sieben Exzellenzen", "tokens": ["Ein", "Trupp", "von", "sie\u00b7ben", "Ex\u00b7zel\u00b7len\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "CARD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Mit blauem Bart und falben Schw\u00e4nzen", "tokens": ["Mit", "blau\u00b7em", "Bart", "und", "fal\u00b7ben", "Schw\u00e4n\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Sich um den Thron. Zuletzt erschien", "tokens": ["Sich", "um", "den", "Thron", ".", "Zu\u00b7letzt", "er\u00b7schien"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PRF", "APPR", "ART", "NN", "$.", "ADV", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Auch noch ein achter Paladin,", "tokens": ["Auch", "noch", "ein", "ach\u00b7ter", "Pa\u00b7la\u00b7din", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "An den kein Mensch im Traume dachte.", "tokens": ["An", "den", "kein", "Mensch", "im", "Trau\u00b7me", "dach\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PIAT", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Es war ein mi\u00dfgeschaffner Zwerg,", "tokens": ["Es", "war", "ein", "mi\u00df\u00b7ge\u00b7schaff\u00b7ner", "Zwerg", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Mit einem H\u00f6cker wie ein Berg", "tokens": ["Mit", "ei\u00b7nem", "H\u00f6\u00b7cker", "wie", "ein", "Berg"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "KOKOM", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Und einem Stelzfu\u00df. Alles lachte:", "tokens": ["Und", "ei\u00b7nem", "Stelz\u00b7fu\u00df", ".", "Al\u00b7les", "lach\u00b7te", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$.", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Er lachte mit. Der Kampf begann,", "tokens": ["Er", "lach\u00b7te", "mit", ".", "Der", "Kampf", "be\u00b7gann", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$.", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Die Streiter sprangen Mann f\u00fcr Mann,", "tokens": ["Die", "Strei\u00b7ter", "spran\u00b7gen", "Mann", "f\u00fcr", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Wie Fl\u00f6he nach dem schlanken Aste,", "tokens": ["Wie", "Fl\u00f6\u00b7he", "nach", "dem", "schlan\u00b7ken", "As\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "An dem der V\u00f6lker Schicksal hieng.", "tokens": ["An", "dem", "der", "V\u00f6l\u00b7ker", "Schick\u00b7sal", "hieng", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Sie thaten Wunder; jeder fa\u00dfte", "tokens": ["Sie", "tha\u00b7ten", "Wun\u00b7der", ";", "je\u00b7der", "fa\u00df\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "$.", "PIS", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Den Preis. Doch von den sieben fieng", "tokens": ["Den", "Preis", ".", "Doch", "von", "den", "sie\u00b7ben", "fi\u00b7eng"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$.", "KON", "APPR", "ART", "CARD", "VVFIN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.27": {"text": "Ihn keiner; wie ein Aal entgieng", "tokens": ["Ihn", "kei\u00b7ner", ";", "wie", "ein", "Aal", "ent\u00b7gieng"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "PIS", "$.", "PWAV", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Die Nu\u00df, die stets zu fallen drohte,", "tokens": ["Die", "Nu\u00df", ",", "die", "stets", "zu", "fal\u00b7len", "droh\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.29": {"text": "Des Haschers ausgestreckter Pfote,", "tokens": ["Des", "Ha\u00b7schers", "aus\u00b7ge\u00b7streck\u00b7ter", "Pfo\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.30": {"text": "Nur eine Faser hielt sie noch,", "tokens": ["Nur", "ei\u00b7ne", "Fa\u00b7ser", "hielt", "sie", "noch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "Als der Aesop an seiner Kr\u00fccke", "tokens": ["Als", "der", "A\u00b7e\u00b7sop", "an", "sei\u00b7ner", "Kr\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.32": {"text": "Auf den verla\u00dfnen Kampfplatz kroch.", "tokens": ["Auf", "den", "ver\u00b7la\u00df\u00b7nen", "Kampf\u00b7platz", "kroch", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "Von Hoffnung und vom blinden Gl\u00fccke", "tokens": ["Von", "Hoff\u00b7nung", "und", "vom", "blin\u00b7den", "Gl\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.34": {"text": "Befl\u00fcgelt, wagt er einen Satz,", "tokens": ["Be\u00b7fl\u00fc\u00b7gelt", ",", "wagt", "er", "ei\u00b7nen", "Satz", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.35": {"text": "Der freylich nur die Luft bewegte!", "tokens": ["Der", "frey\u00b7lich", "nur", "die", "Luft", "be\u00b7weg\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.36": {"text": "Und doch dem schiefen Junker Matz", "tokens": ["Und", "doch", "dem", "schie\u00b7fen", "Jun\u00b7ker", "Matz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.37": {"text": "Das K\u00f6nigreich zu F\u00fc\u00dfen legte.", "tokens": ["Das", "K\u00f6\u00b7nig\u00b7reich", "zu", "F\u00fc\u00b7\u00dfen", "leg\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.38": {"text": "Man hob ihn schwebend auf den Thron", "tokens": ["Man", "hob", "ihn", "schwe\u00b7bend", "auf", "den", "Thron"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "PPER", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.39": {"text": "Und aus dem bunten Chor der Z\u00fcnfte", "tokens": ["Und", "aus", "dem", "bun\u00b7ten", "Chor", "der", "Z\u00fcnf\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.40": {"text": "Erscholl der laute Jubelton:", "tokens": ["Er\u00b7scholl", "der", "lau\u00b7te", "Ju\u00b7bel\u00b7ton", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.41": {"text": "Es lebe K\u00f6nig Matz, der f\u00fcnfte!", "tokens": ["Es", "le\u00b7be", "K\u00f6\u00b7nig", "Matz", ",", "der", "f\u00fcnf\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "NE", "$,", "ART", "ADJA", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Das Reich der Affen ist, wie Pohlen,", "tokens": ["Das", "Reich", "der", "Af\u00b7fen", "ist", ",", "wie", "Poh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VAFIN", "$,", "PWAV", "NE", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Wahlreich; nur da\u00df Kapriolen", "tokens": ["Ein", "Wahl\u00b7reich", ";", "nur", "da\u00df"