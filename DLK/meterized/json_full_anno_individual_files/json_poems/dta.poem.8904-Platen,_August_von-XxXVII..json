{"dta.poem.8904": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "XxXVII.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Da nie dem Halben ich mein Herz ergeben:", "tokens": ["Da", "nie", "dem", "Hal\u00b7ben", "ich", "mein", "Herz", "er\u00b7ge\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "PPER", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Kunst gelobt' ich ganz ein ganzes Leben,", "tokens": ["Der", "Kunst", "ge\u00b7lobt'", "ich", "ganz", "ein", "gan\u00b7zes", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und wenn ich sterbe, sterb' ich f\u00fcr das Sch\u00f6ne.", "tokens": ["Und", "wenn", "ich", "ster\u00b7be", ",", "sterb'", "ich", "f\u00fcr", "das", "Sch\u00f6\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Doch w\u00fcnsch' ich, da\u00df man Bessere bekr\u00f6ne,", "tokens": ["Doch", "w\u00fcn\u00b7sch'", "ich", ",", "da\u00df", "man", "Bes\u00b7se\u00b7re", "be\u00b7kr\u00f6\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "KOUS", "PIS", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Mich aber ziehen lasse, wo ich neben", "tokens": ["Mich", "a\u00b7ber", "zie\u00b7hen", "las\u00b7se", ",", "wo", "ich", "ne\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "ADV", "VVINF", "VVFIN", "$,", "PWAV", "PPER", "APPR"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dem H\u00f6chsten lernen kann, nach Hohem streben,", "tokens": ["Dem", "H\u00f6chs\u00b7ten", "ler\u00b7nen", "kann", ",", "nach", "Ho\u00b7hem", "stre\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VMFIN", "$,", "APPR", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ja, da\u00df man mir mein Vaterland verp\u00f6ne!", "tokens": ["Ja", ",", "da\u00df", "man", "mir", "mein", "Va\u00b7ter\u00b7land", "ver\u00b7p\u00f6\u00b7ne", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "KOUS", "PIS", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ich lieb' es d'rum in keinem Sinne minder,", "tokens": ["Ich", "lieb'", "es", "d'\u00b7rum", "in", "kei\u00b7nem", "Sin\u00b7ne", "min\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPR", "PIAT", "NN", "ADV", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Da stets ich mich in seinem Dienst verzehre,", "tokens": ["Da", "stets", "ich", "mich", "in", "sei\u00b7nem", "Dienst", "ver\u00b7zeh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch w\u00e4r' ich gern das fernste seiner Kinder.", "tokens": ["Doch", "w\u00e4r'", "ich", "gern", "das", "ferns\u00b7te", "sei\u00b7ner", "Kin\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "ART", "ADJA", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Geschieht's, da\u00df je den innern Schatz ich mehre,", "tokens": ["Ge\u00b7schieht's", ",", "da\u00df", "je", "den", "in\u00b7nern", "Schatz", "ich", "meh\u00b7re", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KOUS", "ADV", "ART", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So bleibt der Fund, wenn l\u00e4ngst dahin der Finder,", "tokens": ["So", "bleibt", "der", "Fund", ",", "wenn", "l\u00e4ngst", "da\u00b7hin", "der", "Fin\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "KOUS", "ADV", "PAV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein sichres Eigenthum der deutschen Ehre.", "tokens": ["Ein", "sich\u00b7res", "Ei\u00b7gen\u00b7thum", "der", "deut\u00b7schen", "Eh\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}