{"dta.poem.13219": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Z ehndes  B uch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Sie schwieg: und gleich darauf entstunde bey dem Krei\u00df", "tokens": ["Sie", "schwieg", ":", "und", "gleich", "da\u00b7rauf", "ent\u00b7stun\u00b7de", "bey", "dem", "Krei\u00df"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "KON", "ADV", "PAV", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von der Gef\u00e4lligkeit ein murmelnder Beweis.", "tokens": ["Von", "der", "Ge\u00b7f\u00e4l\u00b7lig\u00b7keit", "ein", "mur\u00b7meln\u00b7der", "Be\u00b7weis", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Es war noch niemand da, was anders vorzubringen,", "tokens": ["Es", "war", "noch", "nie\u00b7mand", "da", ",", "was", "an\u00b7ders", "vor\u00b7zu\u00b7brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PIS", "ADV", "$,", "PRELS", "ADV", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So da\u00df die Blicke nur dort an den Kindern hiengen.", "tokens": ["So", "da\u00df", "die", "Bli\u00b7cke", "nur", "dort", "an", "den", "Kin\u00b7dern", "hien\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "ART", "NN", "ADV", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Inzwischen h\u00f6rt\u2019 ich still das treu-gesinnte Wort:", "tokens": ["I\u00b7nzwi\u00b7schen", "h\u00f6rt'", "ich", "still", "das", "treu\u00b7ge\u00b7sinn\u00b7te", "Wort", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "470\u201dO Seegen-reiche Frau! du f\u00fchrst uns an den Port", "tokens": ["\"", "O", "See\u00b7gen\u00b7rei\u00b7che", "Frau", "!", "du", "f\u00fchrst", "uns", "an", "den", "Port"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "NE", "ADJA", "NN", "$.", "PPER", "VVFIN", "PPER", "APPR", "ART", "NE"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "\u201edes wahren Wohlergehns! Ein anderes versezte:", "tokens": ["\u201e", "des", "wah\u00b7ren", "Woh\u00b7ler\u00b7gehns", "!", "Ein", "an\u00b7de\u00b7res", "ver\u00b7sez\u00b7te", ":"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ART", "ADJA", "NN", "$.", "ART", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "\u201ewer ist der dich nicht mehr als K\u00f6nigreiche sch\u00e4zte?", "tokens": ["\u201e", "wer", "ist", "der", "dich", "nicht", "mehr", "als", "K\u00f6\u00b7nig\u00b7rei\u00b7che", "sch\u00e4z\u00b7te", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VAFIN", "ART", "PPER", "PTKNEG", "ADV", "KOUS", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Thalia selber auch brach in den Ausdruck aus:", "tokens": ["Tha\u00b7lia", "sel\u00b7ber", "auch", "brach", "in", "den", "Aus\u00b7druck", "aus", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADV", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.10": {"text": "\u201eo dreymahl gl\u00fcckliches von GOtt bewahrtes Haus!", "tokens": ["\u201e", "o", "drey\u00b7mahl", "gl\u00fcck\u00b7li\u00b7ches", "von", "Gott", "be\u00b7wahr\u00b7tes", "Haus", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM", "ADV", "ADJA", "APPR", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "475\u201dW\u00e4r nicht der Thron in Schutt, die Stadt in Graus verkehret?", "tokens": ["\"", "W\u00e4r", "nicht", "der", "Thron", "in", "Schutt", ",", "die", "Stadt", "in", "Graus", "ver\u00b7keh\u00b7ret", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "PTKNEG", "ART", "NN", "APPR", "NN", "$,", "ART", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "\u201eh\u00e4tt nicht der Feind das Land mit Schwert und Brand verheeret,", "tokens": ["\u201e", "h\u00e4tt", "nicht", "der", "Feind", "das", "Land", "mit", "Schwert", "und", "Brand", "ver\u00b7hee\u00b7ret", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "PTKNEG", "ART", "NN", "ART", "NN", "APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "\u201ewie er von Anbeginn des Kriegs mit Hochmuth schwor;", "tokens": ["\u201e", "wie", "er", "von", "An\u00b7be\u00b7ginn", "des", "Kriegs", "mit", "Hoch\u00b7muth", "schwor", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "PPER", "APPR", "NN", "ART", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "\u201ewann nicht die K\u00f6niginn mit ihrem Tugend-Chor", "tokens": ["\u201e", "wann", "nicht", "die", "K\u00f6\u00b7ni\u00b7ginn", "mit", "ih\u00b7rem", "Tu\u00b7gen\u00b7dChor"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "PWAV", "PTKNEG", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "\u201eregent gewesen w\u00e4r? h\u00e4tt sie ihr nicht gegleichet,", "tokens": ["\u201e", "re\u00b7gent", "ge\u00b7we\u00b7sen", "w\u00e4r", "?", "h\u00e4tt", "sie", "ihr", "nicht", "ge\u00b7glei\u00b7chet", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "VAPP", "VAFIN", "$.", "VAFIN", "PPER", "PPER", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "480\u201dSo h\u00e4tten wir schon l\u00e4ngst den Untergang erreichet.", "tokens": ["\"", "So", "h\u00e4t\u00b7ten", "wir", "schon", "l\u00e4ngst", "den", "Un\u00b7ter\u00b7gang", "er\u00b7rei\u00b7chet", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VAFIN", "PPER", "ADV", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "\u201emit einem Wort: es ist nichts als ", "tokens": ["\u201e", "mit", "ei\u00b7nem", "Wort", ":", "es", "ist", "nichts", "als"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "APPR", "ART", "NN", "$.", "PPER", "VAFIN", "PIS", "KOKOM"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "\u201edurch die das Vaterland den Sturm vergehen sah!", "tokens": ["\u201e", "durch", "die", "das", "Va\u00b7ter\u00b7land", "den", "Sturm", "ver\u00b7ge\u00b7hen", "sah", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "PRELS", "ART", "NN", "ART", "NN", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Vernahm ich wieder still ..... Von einer andern Seite:", "tokens": ["Ver\u00b7nahm", "ich", "wie\u00b7der", "still", ".....", "Von", "ei\u00b7ner", "an\u00b7dern", "Sei\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PTKVZ", "$.", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "\u201eo da\u00df ihr GOtt die Zeit des sp\u00e4tsten Alters weihte!", "tokens": ["\u201e", "o", "da\u00df", "ihr", "Gott", "die", "Zeit", "des", "sp\u00e4ts\u00b7ten", "Al\u00b7ters", "weih\u00b7te", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM", "KOUS", "PPOSAT", "NN", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "485Aus allen Herzen drang ein treuer Wunsch empor.", "tokens": ["al\u00b7len", "Her\u00b7zen", "drang", "ein", "treu\u00b7er", "Wunsch", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.22": {"text": "Jmmittelst machte sich die Fr\u00f6mmigkeit hervor,", "tokens": ["Jm\u00b7mit\u00b7telst", "mach\u00b7te", "sich", "die", "Fr\u00f6m\u00b7mig\u00b7keit", "her\u00b7vor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Als ob sie die Geduld des Throns wollt unterbrechen;", "tokens": ["Als", "ob", "sie", "die", "Ge\u00b7duld", "des", "Throns", "wollt", "un\u00b7ter\u00b7bre\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "ART", "NN", "ART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Sie neigte sich, da sie begunnte so zu sprechen:", "tokens": ["Sie", "neig\u00b7te", "sich", ",", "da", "sie", "be\u00b7gunn\u00b7te", "so", "zu", "spre\u00b7chen", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "$,", "KOUS", "PPER", "VVFIN", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}