{"dta.poem.14918": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Der armen Sch\u00f6nheit Lebenslauf.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Da sagt sie: Was hilft mir mein Sehen,", "tokens": ["Da", "sagt", "sie", ":", "Was", "hilft", "mir", "mein", "Se\u00b7hen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$.", "PWS", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Ich w\u00fcnscht', ich w\u00e4re lieber blind,", "tokens": ["Ich", "w\u00fcnscht'", ",", "ich", "w\u00e4\u00b7re", "lie\u00b7ber", "blind", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da alle furchtsam von mir gehen,", "tokens": ["Da", "al\u00b7le", "furcht\u00b7sam", "von", "mir", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADJD", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Weil gar so sch\u00f6n mein' Augen sind. \u2014", "tokens": ["Weil", "gar", "so", "sch\u00f6n", "mein'", "Au\u00b7gen", "sind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "ADV", "ADV", "ADJD", "PPOSAT", "NN", "VAFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}