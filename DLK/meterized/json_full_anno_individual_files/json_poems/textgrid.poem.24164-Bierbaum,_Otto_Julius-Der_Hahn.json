{"textgrid.poem.24164": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Der Hahn", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich bin der Hahn,", "tokens": ["Ich", "bin", "der", "Hahn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Der Muselman,", "tokens": ["Der", "Mu\u00b7sel\u00b7man", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+--", "measure": "dactylic.init"}, "line.3": {"text": "Ich habe drei\u00dfig Hennen;", "tokens": ["Ich", "ha\u00b7be", "drei\u00b7\u00dfig", "Hen\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "CARD", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Im Hof und auf der Tennen", "tokens": ["Im", "Hof", "und", "auf", "der", "Ten\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "KON", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Umgackert mich die bunte Schaar,", "tokens": ["Um\u00b7ga\u00b7ckert", "mich", "die", "bun\u00b7te", "Schaar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df sies stets neu erkennen:", "tokens": ["Da\u00df", "sies", "stets", "neu", "er\u00b7ken\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Der Muselman.", "tokens": ["Der", "Mu\u00b7sel\u00b7man", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+--", "measure": "dactylic.init"}}, "stanza.2": {"line.1": {"text": "Doch bin ich nicht ein S\u00e4nger blo\u00df:", "tokens": ["Doch", "bin", "ich", "nicht", "ein", "S\u00e4n\u00b7ger", "blo\u00df", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PTKNEG", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Als Ritter bin ich gleichfalls gro\u00df;", "tokens": ["Als", "Rit\u00b7ter", "bin", "ich", "gleich\u00b7falls", "gro\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VAFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Betrachtet meine Sporen!", "tokens": ["Be\u00b7trach\u00b7tet", "mei\u00b7ne", "Spo\u00b7ren", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zwei Fahnen trag ich voller Glanz:", "tokens": ["Zwei", "Fah\u00b7nen", "trag", "ich", "vol\u00b7ler", "Glanz", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Eine auf dem Kopf, eine auf dem Schwanz;", "tokens": ["Ei\u00b7ne", "auf", "dem", "Kopf", ",", "ei\u00b7ne", "auf", "dem", "Schwanz", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "$,", "ART", "APPR", "ART", "NN", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "Der Arme ist verloren,", "tokens": ["Der", "Ar\u00b7me", "ist", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Der gegen mich zu stelzen wagt;", "tokens": ["Der", "ge\u00b7gen", "mich", "zu", "stel\u00b7zen", "wagt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPER", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Es sei ihm glatt vorausgesagt:", "tokens": ["Es", "sei", "ihm", "glatt", "vor\u00b7aus\u00b7ge\u00b7sagt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Er ist dem Tod erkoren.", "tokens": ["Er", "ist", "dem", "Tod", "er\u00b7ko\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Bereits in aller Hahnenfr\u00fch", "tokens": ["Be\u00b7reits", "in", "al\u00b7ler", "Hah\u00b7nen\u00b7fr\u00fch"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Thu ich ein sieghaft kikriki,", "tokens": ["Thu", "ich", "ein", "sieg\u00b7haft", "kik\u00b7ri\u00b7ki", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nicht ohne Pr\u00e4ludieren.", "tokens": ["Nicht", "oh\u00b7ne", "Pr\u00e4\u00b7lu\u00b7die\u00b7ren", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Bleich flieht der Sterne feiger Hauf,", "tokens": ["Bleich", "flieht", "der", "Ster\u00b7ne", "fei\u00b7ger", "Hauf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und p\u00fcnktlich zieht die Sonne auf:", "tokens": ["Und", "p\u00fcnkt\u00b7lich", "zieht", "die", "Son\u00b7ne", "auf", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ich thu sie kommandieren.", "tokens": ["Ich", "thu", "sie", "kom\u00b7man\u00b7die\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Bei so viel Tugend und Talent", "tokens": ["Bei", "so", "viel", "Tu\u00b7gend", "und", "Ta\u00b7lent"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "PIAT", "NN", "KON", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Versteht sichs, da\u00df ein Monument", "tokens": ["Ver\u00b7steht", "sichs", ",", "da\u00df", "ein", "Mo\u00b7nu\u00b7ment"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PIS", "$,", "KOUS", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mir wird nach meinem Tode.", "tokens": ["Mir", "wird", "nach", "mei\u00b7nem", "To\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ich werde auf dem Kirchturm stehn", "tokens": ["Ich", "wer\u00b7de", "auf", "dem", "Kirch\u00b7turm", "stehn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "ART", "NN", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und \u00fcber tausend Giebel sehn", "tokens": ["Und", "\u00fc\u00b7ber", "tau\u00b7send", "Gie\u00b7bel", "sehn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "CARD", "NN", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und hunderttausend Schlote.", "tokens": ["Und", "hun\u00b7dert\u00b7tau\u00b7send", "Schlo\u00b7te", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "CARD", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Mit Fingern weist man auf mich bin,", "tokens": ["Mit", "Fin\u00b7gern", "weist", "man", "auf", "mich", "bin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PIS", "APPR", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Weil ich dann ein Exempel bin;", "tokens": ["Weil", "ich", "dann", "ein", "Ex\u00b7em\u00b7pel", "bin", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "VAFIN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.9": {"text": "Der Vater spricht zum Sohne:", "tokens": ["Der", "Va\u00b7ter", "spricht", "zum", "Soh\u00b7ne", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Blick auf zum Hahn,", "tokens": ["Blick", "auf", "zum", "Hahn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "APPRART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.11": {"text": "Blick himmelan,", "tokens": ["Blick", "him\u00b7me\u00b7lan", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "ADV", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.12": {"text": "Es wird, mein Sohn, es ist kein Wahn,", "tokens": ["Es", "wird", ",", "mein", "Sohn", ",", "es", "ist", "kein", "Wahn", ","], "token_info": ["word", "word", "punct", "word", 