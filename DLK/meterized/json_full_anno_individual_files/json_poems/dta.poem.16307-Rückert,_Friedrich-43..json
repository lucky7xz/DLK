{"dta.poem.16307": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "43.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Pfeile des Geschicks fliegen nach allen Seiten,", "tokens": ["Die", "Pfei\u00b7le", "des", "Ge\u00b7schicks", "flie\u00b7gen", "nach", "al\u00b7len", "Sei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und Menschentugend ist machtlos zum Gegenstreiten.", "tokens": ["Und", "Men\u00b7schen\u00b7tu\u00b7gend", "ist", "macht\u00b7los", "zum", "Ge\u00b7gen\u00b7strei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Nur eine Schutzwehr bleibt, sich ohne Schuld bewahren,", "tokens": ["Nur", "ei\u00b7ne", "Schutz\u00b7wehr", "bleibt", ",", "sich", "oh\u00b7ne", "Schuld", "be\u00b7wah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "$,", "PRF", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Um nicht zu \u00e4u\u00dferm Sturm auch innern zu erfahren.", "tokens": ["Um", "nicht", "zu", "\u00e4u\u00b7\u00dferm", "Sturm", "auch", "in\u00b7nern", "zu", "er\u00b7fah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PTKNEG", "PTKZU", "ADJA", "NN", "ADV", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Denn zwar nicht ganz aufwiegt Unschuld des Schicksals Last,", "tokens": ["Denn", "zwar", "nicht", "ganz", "auf\u00b7wiegt", "Un\u00b7schuld", "des", "Schick\u00b7sals", "Last", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PTKNEG", "ADV", "VVPP", "NN", "ART", "NN", "NE", "$,"], "meter": "----+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Doch leidest du nur halb, was du verdient nicht hast.", "tokens": ["Doch", "lei\u00b7dest", "du", "nur", "halb", ",", "was", "du", "ver\u00b7dient", "nicht", "hast", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADJD", "$,", "PWS", "PPER", "VVFIN", "PTKNEG", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}