{"textgrid.poem.53020": {"metadata": {"author": {"name": "Pyra, Jakob Immanuel", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein grosser Geist, der Sternen Erb und Sohn,", "genre": "verse", "period": "N.A.", "pub_year": 1729, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein grosser Geist, der Sternen Erb und Sohn,", "tokens": ["Ein", "gros\u00b7ser", "Geist", ",", "der", "Ster\u00b7nen", "Erb", "und", "Sohn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Genie\u00dft, o Freund, in ewig hellen Sph\u00e4ren,", "tokens": ["Ge\u00b7nie\u00dft", ",", "o", "Freund", ",", "in", "e\u00b7wig", "hel\u00b7len", "Sph\u00e4\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "FM", "NN", "$,", "APPR", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Weit von der blinden Nacht der tiefen Welt,", "tokens": ["Weit", "von", "der", "blin\u00b7den", "Nacht", "der", "tie\u00b7fen", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der heiligsten Tage.", "tokens": ["Der", "hei\u00b7ligs\u00b7ten", "Ta\u00b7ge", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Die Ehr erweckt die weise Jugend schon,", "tokens": ["Die", "Ehr", "er\u00b7weckt", "die", "wei\u00b7se", "Ju\u00b7gend", "schon", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der v\u00e4terliche Trieb des holden Himmels", "tokens": ["Der", "v\u00e4\u00b7ter\u00b7li\u00b7che", "Trieb", "des", "hol\u00b7den", "Him\u00b7mels"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wacht stets um ihn, und st\u00f6hrt unruhig stets", "tokens": ["Wacht", "stets", "um", "ihn", ",", "und", "st\u00f6hrt", "un\u00b7ru\u00b7hig", "stets"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "ADV", "APPR", "PPER", "$,", "KON", "VVFIN", "ADJD", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Vergiftendes Schlummern.", "tokens": ["Ver\u00b7gif\u00b7ten\u00b7des", "Schlum\u00b7mern", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "Mit weichem Arm zerrei\u00dft er schon das Band", "tokens": ["Mit", "wei\u00b7chem", "Arm", "zer\u00b7rei\u00dft", "er", "schon", "das", "Band"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der Vorurtheil und niedrigen Begierden;", "tokens": ["Der", "Vor\u00b7urt\u00b7heil", "und", "nied\u00b7ri\u00b7gen", "Be\u00b7gier\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Er ehret nur das himmlische Gesetz", "tokens": ["Er", "eh\u00b7ret", "nur", "das", "himm\u00b7li\u00b7sche", "Ge\u00b7setz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der Weisheit und Tugend.", "tokens": ["Der", "Weis\u00b7heit", "und", "Tu\u00b7gend", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.4": {"line.1": {"text": "Durch sie gest\u00e4rckt, hebt er sein siegend Haupt,", "tokens": ["Durch", "sie", "ge\u00b7st\u00e4rckt", ",", "hebt", "er", "sein", "sie\u00b7gend", "Haupt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVPP", "$,", "VVFIN", "PPER", "PPOSAT", "CARD", "NN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.2": {"text": "Vom Sonnenlicht erhellt, aus Dunst und Nebel.", "tokens": ["Vom", "Son\u00b7nen\u00b7licht", "er\u00b7hellt", ",", "aus", "Dunst", "und", "Ne\u00b7bel", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$,", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Ewigkeit zeigt von des Himmels H\u00f6h", "tokens": ["Die", "E\u00b7wig\u00b7keit", "zeigt", "von", "des", "Him\u00b7mels", "H\u00f6h"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "NN"], "meter": "-+-+---+-+", "measure": "zehnsilber"}, "line.4": {"text": "Die Fackel und Palme.", "tokens": ["Die", "Fa\u00b7ckel", "und", "Pal\u00b7me", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.5": {"line.1": {"text": "Er steigt und singt das Lob des Ewigen;", "tokens": ["Er", "steigt", "und", "singt", "das", "Lob", "des", "E\u00b7wi\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "ART", "NN", "ART", "ADJA", "$."], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Es schalt sein Lied von allen Sternen wieder.", "tokens": ["Es", "schalt", "sein", "Lied", "von", "al\u00b7len", "Ster\u00b7nen", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR", "PIAT", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der P\u00f6bel sieht den Glantz der hohen Bahn,", "tokens": ["Der", "P\u00f6\u00b7bel", "sieht", "den", "Glantz", "der", "ho\u00b7hen", "Bahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Er sieht ihn und bellet.", "tokens": ["Er", "sieht", "ihn", "und", "bel\u00b7let", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "KON", "VVFIN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.6": {"line.1": {"text": "Wohlan, mein Freund, la\u00df deine Fl\u00fcgel nie", "tokens": ["Wo\u00b7hlan", ",", "mein", "Freund", ",", "la\u00df", "dei\u00b7ne", "Fl\u00fc\u00b7gel", "nie"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "PPOSAT", "NN", "$,", "VVIMP", "PPOSAT", "NN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "In jener reinsten Luft erm\u00fcdet werden.", "tokens": ["In", "je\u00b7ner", "reins\u00b7ten", "Luft", "er\u00b7m\u00fc\u00b7det", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "VVFIN", "VAINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dein Thirsis folgt, sieh, er versuchet schon", "tokens": ["Dein", "Thir\u00b7sis", "folgt", ",", "sieh", ",", "er", "ver\u00b7su\u00b7chet", "schon"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "$,", "VVFIN", "$,", "PPER", "VVFIN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die wachsenden Schwingen.", "tokens": ["Die", "wach\u00b7sen\u00b7den", "Schwin\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.7": {"line.1": {"text": "O Damon, nichts zerreisset unsern Bund,", "tokens": ["O", "Da\u00b7mon", ",", "nichts", "zer\u00b7reis\u00b7set", "un\u00b7sern", "Bund", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "PIS", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ein gleicher Weg wird beyder Spuren f\u00fchren.", "tokens": ["Ein", "glei\u00b7cher", "Weg", "wird", "bey\u00b7der", "Spu\u00b7ren", "f\u00fch\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Getrost, es gr\u00fcnt des Tempels H\u00f6he schon", "tokens": ["Ge\u00b7trost", ",", "es", "gr\u00fcnt", "des", "Tem\u00b7pels", "H\u00f6\u00b7he", "schon"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PPER", "VVFIN", "ART", "NN", "NN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mit ewigen B\u00e4umen.", "tokens": ["Mit", "e\u00b7wi\u00b7gen", "B\u00e4u\u00b7men", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.8": {"line.1": {"text": "Komm, Doris, komm, durch deine Freundlichkeit", "tokens": ["Komm", ",", "Do\u00b7ris", ",", "komm", ",", "durch", "dei\u00b7ne", "Freund\u00b7lich\u00b7keit"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$,", "NE", "$,", "VVFIN", "$,", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Verkl\u00e4ren sich die allertr\u00fcbsten Wolcken.", "tokens": ["Ver\u00b7kl\u00e4\u00b7ren", "sich", "die", "al\u00b7ler\u00b7tr\u00fcbs\u00b7ten", "Wol\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Komm, w\u00fcrdige mit tugendvollem Blick", "tokens": ["Komm", ",", "w\u00fcr\u00b7di\u00b7ge", "mit", "tu\u00b7gend\u00b7vol\u00b7lem", "Blick"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die Seelen zu st\u00e4rcken.", "tokens": ["Die", "See\u00b7len", "zu", "st\u00e4r\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.9": {"line.1": {"text": "Dein sanftes Bild, der Anmuth Abdruck wird", "tokens": ["Dein", "sanf\u00b7tes", "Bild", ",", "der", "An\u00b7muth", "Ab\u00b7druck", "wird"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "ART", "NN", "NN", "VAFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Durch unsre Hand gemeinschaftlich vollendet,", "tokens": ["Durch", "uns\u00b7re", "Hand", "ge\u00b7mein\u00b7schaft\u00b7lich", "voll\u00b7en\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dereinsten dort, an jenem keuschen Baum,", "tokens": ["De\u00b7reins\u00b7ten", "dort", ",", "an", "je\u00b7nem", "keu\u00b7schen", "Baum", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Im Schatten erhoben.", "tokens": ["Im", "Schat\u00b7ten", "er\u00b7ho\u00b7ben", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.10": {"line.1": {"text": "Der Musen Chor, der Ring der Tugenden,", "tokens": ["Der", "Mu\u00b7sen", "Chor", ",", "der", "Ring", "der", "Tu\u00b7gen\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Und jede Sch\u00e4ferinn wird Blumen sammlen.", "tokens": ["Und", "je\u00b7de", "Sch\u00e4\u00b7fe\u00b7rinn", "wird", "Blu\u00b7men", "samm\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VAFIN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und stets dein Bild, und den geweihten Ast", "tokens": ["Und", "stets", "dein", "Bild", ",", "und", "den", "ge\u00b7weih\u00b7ten", "Ast"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADV", "PPOSAT", "NN", "$,", "KON", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mit Kr\u00e4ntzen beh\u00e4ngen.", "tokens": ["Mit", "Kr\u00e4nt\u00b7zen", "be\u00b7h\u00e4n\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.11": {"line.1": {"text": "Der sch\u00f6nste Tag wird stets dein Festtag seyn.", "tokens": ["Der", "sch\u00f6ns\u00b7te", "Tag", "wird", "stets", "dein", "Fest\u00b7tag", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Da werden sie im Schatten deiner Zweige", "tokens": ["Da", "wer\u00b7den", "sie", "im", "Schat\u00b7ten", "dei\u00b7ner", "Zwei\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "APPRART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dein w\u00fcrdig Lob und deines Mannes Ruhm", "tokens": ["Dein", "w\u00fcr\u00b7dig", "Lob", "und", "dei\u00b7nes", "Man\u00b7nes", "Ruhm"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJD", "NN", "KON", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Im Reihen erneuern.", "tokens": ["Im", "Rei\u00b7hen", "er\u00b7neu\u00b7ern", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.12": {"line.1": {"text": "Du aber wirst mit dieser Mine stehn,", "tokens": ["Du", "a\u00b7ber", "wirst", "mit", "die\u00b7ser", "Mi\u00b7ne", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "APPR", "PDAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Womit du sonst auf dein muthwillig S\u00f6hnchen,", "tokens": ["Wo\u00b7mit", "du", "sonst", "auf", "dein", "mut\u00b7hwil\u00b7lig", "S\u00f6hn\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "APPR", "PPOSAT", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wenn es im Schoosse schertzt, und deinen Mann,", "tokens": ["Wenn", "es", "im", "Schoos\u00b7se", "schertzt", ",", "und", "dei\u00b7nen", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "VVFIN", "$,", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ja mich auch gelachet.", "tokens": ["Ja", "mich", "auch", "ge\u00b7la\u00b7chet", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PPER", "ADV", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.13": {"line.1": {"text": "Ein grosser Geist, der Sternen Erb und Sohn,", "tokens": ["Ein", "gros\u00b7ser", "Geist", ",", "der", "Ster\u00b7nen", "Erb", "und", "Sohn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Genie\u00dft, o Freund, in ewig hellen Sph\u00e4ren,", "tokens": ["Ge\u00b7nie\u00dft", ",", "o", "Freund", ",", "in", "e\u00b7wig", "hel\u00b7len", "Sph\u00e4\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "FM", "NN", "$,", "APPR", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Weit von der blinden Nacht der tiefen Welt,", "tokens": ["Weit", "von", "der", "blin\u00b7den", "Nacht", "der", "tie\u00b7fen", "Welt", "