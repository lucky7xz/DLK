{"dta.poem.5503": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Unn\u00fctzer Nutz des Verstandes.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du hast nunmehr aus allen Kr\u00e4ften, wie wir aus\ndeinen Schriften lesen,", "tokens": ["Du", "hast", "nun\u00b7mehr", "aus", "al\u00b7len", "Kr\u00e4f\u00b7ten", ",", "wie", "wir", "aus", "dei\u00b7nen", "Schrif\u00b7ten", "le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "PIAT", "NN", "$,", "PWAV", "PPER", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+--+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "Dich und die Welt belehren wollen, wie zum Vergn\u00fc-", "tokens": ["Dich", "und", "die", "Welt", "be\u00b7leh\u00b7ren", "wol\u00b7len", ",", "wie", "zum", "Ver\u00b7gn\u00fc"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "KON", "ART", "NN", "VVINF", "VMFIN", "$,", "PWAV", "APPRART", "TRUNC"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.3": {"text": "Allein, du siehst ja leider wol, da\u00df, wie du es auch an-", "tokens": ["Al\u00b7lein", ",", "du", "siehst", "ja", "lei\u00b7der", "wol", ",", "da\u00df", ",", "wie", "du", "es", "auch", "an"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "PPER", "VVFIN", "ADV", "ADV", "ADV", "$,", "KOUS", "$,", "PWAV", "PPER", "PPER", "ADV", "TRUNC"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "Doch, bey den meisten wenigstens, dein Absehn sonder", "tokens": ["Doch", ",", "bey", "den", "meis\u00b7ten", "we\u00b7nigs\u00b7tens", ",", "dein", "Ab\u00b7sehn", "son\u00b7der"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "$,", "APPR", "ART", "VVFIN", "ADV", "$,", "PPOSAT", "NN", "KON"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Dahero ist mir beygefallen, und f\u00e4llt mir eben wieder", "tokens": ["Da\u00b7he\u00b7ro", "ist", "mir", "bey\u00b7ge\u00b7fal\u00b7len", ",", "und", "f\u00e4llt", "mir", "e\u00b7ben", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PAV", "VAFIN", "PPER", "VVINF", "$,", "KON", "VVFIN", "PPER", "ADV", "ADV"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.6": {"text": "Ob du vielleicht des rechten Weges mit deiner Lehr-Art", "tokens": ["Ob", "du", "viel\u00b7leicht", "des", "rech\u00b7ten", "We\u00b7ges", "mit", "dei\u00b7ner", "Lehr\u00b7Art"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "Und ob, durch des Verstandes Kr\u00e4fte, die du zur Richt-", "tokens": ["Und", "ob", ",", "durch", "des", "Ver\u00b7stan\u00b7des", "Kr\u00e4f\u00b7te", ",", "die", "du", "zur", "Richt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "$,", "APPR", "ART", "NN", "NN", "$,", "PRELS", "PPER", "APPRART", "TRUNC"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Vergn\u00fcgen und Zufriedenheit, auf Erden zu erlangen", "tokens": ["Ver\u00b7gn\u00fc\u00b7gen", "und", "Zu\u00b7frie\u00b7den\u00b7heit", ",", "auf", "Er\u00b7den", "zu", "er\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "$,", "APPR", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.9": {"text": "Wenn ich die Sch\u00e4tze des Vergn\u00fcgens, die fast unsch\u00e4tz-", "tokens": ["Wenn", "ich", "die", "Sch\u00e4t\u00b7ze", "des", "Ver\u00b7gn\u00fc\u00b7gens", ",", "die", "fast", "un\u00b7sch\u00e4tz"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "ART", "NN", "$,", "PRELS", "ADV", "TRUNC"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und, da\u00df sie, von der Seeligkeit der erste Grad fast", "tokens": ["Und", ",", "da\u00df", "sie", ",", "von", "der", "See\u00b7lig\u00b7keit", "der", "ers\u00b7te", "Grad", "fast"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "$,", "KOUS", "PPER", "$,", "APPR", "ART", "NN", "ART", "ADJA", "NN", "ADV"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "So scheint hieraus von selbst zu fliessen: da\u00df, da sie recht", "tokens": ["So", "scheint", "hier\u00b7aus", "von", "selbst", "zu", "flies\u00b7sen", ":", "da\u00df", ",", "da", "sie", "recht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PAV", "APPR", "ADV", "PTKZU", "VVINF", "$.", "KOUS", "$,", "KOUS", "PPER", "ADJD"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "Sie nicht in unsern Kr\u00e4ften stehe, und da\u00df ein Raison-", "tokens": ["Sie", "nicht", "in", "un\u00b7sern", "Kr\u00e4f\u00b7ten", "ste\u00b7he", ",", "und", "da\u00df", "ein", "Rai\u00b7son"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "PTKNEG", "APPR", "PPOSAT", "NN", "VVFIN", "$,", "KON", "KOUS", "ART", "TRUNC"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.13": {"text": "Sie zu erlangen, f\u00e4hig sey. Da\u00df also GOtt der HErr", "tokens": ["Sie", "zu", "er\u00b7lan\u00b7gen", ",", "f\u00e4\u00b7hig", "sey", ".", "Da\u00df", "al\u00b7so", "Gott", "der", "Herr"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "PTKZU", "VVINF", "$,", "ADJD", "VAFIN", "$.", "KOUS", "ADV", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.14": {"text": "Um diese Gnade zu erhalten, m\u00fc\u00df\u2019 ernstlich angeflehet seyn.", "tokens": ["Um", "die\u00b7se", "Gna\u00b7de", "zu", "er\u00b7hal\u00b7ten", ",", "m\u00fc\u00df'", "ernst\u00b7lich", "an\u00b7ge\u00b7fle\u00b7het", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PDAT", "NN", "PTKZU", "VVINF", "$,", "VMFIN", "ADJD", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.15": {"text": "Ist dieses wahr, so folgt daraus; da\u00df du, mit allen dei-", "tokens": ["Ist", "die\u00b7ses", "wahr", ",", "so", "folgt", "da\u00b7raus", ";", "da\u00df", "du", ",", "mit", "al\u00b7len", "dei"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "PDAT", "ADJD", "$,", "ADV", "VVFIN", "PAV", "$.", "KOUS", "PPER", "$,", "APPR", "PIAT", "TRUNC"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.16": {"text": "So wie bi\u00dfher, auch k\u00fcnftig hin, nicht viel erkleckliches", "tokens": ["So", "wie", "bi\u00df\u00b7her", ",", "auch", "k\u00fcnf\u00b7tig", "hin", ",", "nicht", "viel", "er\u00b7kleck\u00b7li\u00b7ches"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "KOKOM", "ADV", "$,", "ADV", "ADJD", "PTKVZ", "$,", "PTKNEG", "ADV", "ADJA"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}, "stanza.2": {"line.1": {"text": "Es ist dein Einwurf, liebster Freund, von einer sol-", "tokens": ["Es", "ist", "dein", "Ein\u00b7wurf", ",", "liebs\u00b7ter", "Freund", ",", "von", "ei\u00b7ner", "sol"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "$,", "ADJA", "NN", "$,", "APPR", "ART", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von \u00fcberzeuglicher Gewalt, und nicht zu wiedersteh\u2019n-", "tokens": ["Von", "\u00fc\u00b7berz\u00b7eug\u00b7li\u00b7cher", "Ge\u00b7walt", ",", "und", "nicht", "zu", "wie\u00b7der\u00b7steh'\u00b7n"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "KON", "PTKNEG", "APPR", "TRUNC"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.3": {"text": "Da\u00df ich dir gleich gewonnen gebe. Doch h\u00f6re ein ein-", "tokens": ["Da\u00df", "ich", "dir", "gleich", "ge\u00b7won\u00b7nen", "ge\u00b7be", ".", "Doch", "h\u00f6\u00b7re", "ein", "ein"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "ADV", "VVPP", "VVFIN", "$.", "KON", "VVFIN", "ART", "TRUNC"], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Vermeinst du nicht, da\u00df um den Glantz von deiner", "tokens": ["Ver\u00b7meinst", "du", "nicht", ",", "da\u00df", "um", "den", "Glantz", "von", "dei\u00b7ner"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKNEG", "$,", "KOUS", "APPR", "ART", "NN", "APPR", "PPOSAT"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und um, in einer wahren Andacht, GOtt anzuflehen,", "tokens": ["Und", "um", ",", "in", "ei\u00b7ner", "wah\u00b7ren", "An\u00b7dacht", ",", "Gott", "an\u00b7zu\u00b7fle\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PTKVZ", "$,", "APPR", "ART", "ADJA", "NN", "$,", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Man der Vernunft ben\u00f6thigt sey, und sie gebrauchen", "tokens": ["Man", "der", "Ver\u00b7nunft", "be\u00b7n\u00f6t\u00b7higt", "sey", ",", "und", "sie", "ge\u00b7brau\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PIS", "ART", "NN", "VVPP", "VAFIN", "$,", "KON", "PPER", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}