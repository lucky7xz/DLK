{"textgrid.poem.38307": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Todesahndung einer W\u00f6chnerin", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein Auge wankt,", "tokens": ["Mein", "Au\u00b7ge", "wankt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Am Mond erkrankt,", "tokens": ["Am", "Mond", "er\u00b7krankt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Er m\u00f6chte mir beyspringen,", "tokens": ["Er", "m\u00f6ch\u00b7te", "mir", "bey\u00b7sprin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mir drohn des Todes Klingen.", "tokens": ["Mir", "drohn", "des", "To\u00b7des", "Klin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Mu\u00df Sichelschein", "tokens": ["Mu\u00df", "Si\u00b7chel\u00b7schein"], "token_info": ["word", "word"], "pos": ["VMFIN", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Den Zirkel rund", "tokens": ["Den", "Zir\u00b7kel", "rund"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "ADJD"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Zur Todesfackel f\u00fcllen,", "tokens": ["Zur", "To\u00b7des\u00b7fa\u00b7ckel", "f\u00fcl\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ich bild mirs ein,", "tokens": ["Ich", "bild", "mirs", "ein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NE", "NE", "PTKVZ", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Ich sterb zur Stund;", "tokens": ["Ich", "sterb", "zur", "Stund", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "Helft weinen ihr Gespielen!", "tokens": ["Helft", "wei\u00b7nen", "ihr", "Ge\u00b7spie\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Verg\u00f6nnt es mir,", "tokens": ["Ver\u00b7g\u00f6nnt", "es", "mir", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Das Gr\u00fcn hinf\u00fcr", "tokens": ["Das", "Gr\u00fcn", "hin\u00b7f\u00fcr"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "APPR"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Allhier noch anzuschauen,", "tokens": ["All\u00b7hier", "noch", "an\u00b7zu\u00b7schau\u00b7en", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVIZU", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Auf Bergen, Thal und Auen;", "tokens": ["Auf", "Ber\u00b7gen", ",", "Thal", "und", "Au\u00b7en", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Was Laub und Bl\u00fcth", "tokens": ["Was", "Laub", "und", "Bl\u00fcth"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "NN", "KON", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Ins Auge tr\u00e4gt,", "tokens": ["Ins", "Au\u00b7ge", "tr\u00e4gt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "An Buchen, Eichen, Tannen,", "tokens": ["An", "Bu\u00b7chen", ",", "Ei\u00b7chen", ",", "Tan\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und was nur hie", "tokens": ["Und", "was", "nur", "hie"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PWS", "ADV", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Der Fr\u00fchling pflegt,", "tokens": ["Der", "Fr\u00fch\u00b7ling", "pflegt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "F\u00fcr Teppich aufzuspannen.", "tokens": ["F\u00fcr", "Tep\u00b7pich", "auf\u00b7zu\u00b7span\u00b7nen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Die Wasserfl\u00fc\u00df", "tokens": ["Die", "Was\u00b7ser\u00b7fl\u00fc\u00df"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Bezeugen die\u00df,", "tokens": ["Be\u00b7zeu\u00b7gen", "die\u00df", ","], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "PDS", "$,"], "meter": "-+--", "measure": "dactylic.init"}, "line.3": {"text": "Die rauschend weiter flie\u00dfen,", "tokens": ["Die", "rau\u00b7schend", "wei\u00b7ter", "flie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die B\u00fcsche gr\u00fcn begiessen;", "tokens": ["Die", "B\u00fc\u00b7sche", "gr\u00fcn", "be\u00b7gies\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Nie stehn sie still,", "tokens": ["Nie", "stehn", "sie", "still", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Sind ohne Ruh,", "tokens": ["Sind", "oh\u00b7ne", "Ruh", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Die Reis' mir anzudeuten;", "tokens": ["Die", "Reis'", "mir", "an\u00b7zu\u00b7deu\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVIZU", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Wenn ich erf\u00fcllt", "tokens": ["Wenn", "ich", "er\u00b7f\u00fcllt"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPER", "VVPP"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Mein Werk dazu,", "tokens": ["Mein", "Werk", "da\u00b7zu", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PAV", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "Nach den erkannten Zeiten.", "tokens": ["Nach", "den", "er\u00b7kann\u00b7ten", "Zei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ein Monat Licht,", "tokens": ["Ein", "Mo\u00b7nat", "Licht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Von hinnen fl\u00fccht;", "tokens": ["Von", "hin\u00b7nen", "fl\u00fccht", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Das Trauern in dem Hirne", "tokens": ["Das", "Trau\u00b7ern", "in", "dem", "Hir\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Treibts Uhrwerk der Gestirne.", "tokens": ["Treibts", "Uhr\u00b7werk", "der", "Ge\u00b7stir\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wohlan so lauf", "tokens": ["Wo\u00b7hlan", "so", "lauf"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADV", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "O Thr\u00e4n den Weg,", "tokens": ["O", "Thr\u00e4n", "den", "Weg", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Zur Wanderschaft mu\u00dft fliessen;", "tokens": ["Zur", "Wan\u00b7der\u00b7schaft", "mu\u00dft", "flies\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Verlobt zum Kauf", "tokens": ["Ver\u00b7lobt", "zum", "Kauf"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "APPRART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Dich niederleg,", "tokens": ["Dich", "nie\u00b7der\u00b7leg", ","], "token_info": ["word", "word", "punct"], "pos": ["PPER", "PTKVZ", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "Den j\u00fcngsten Tag zu gr\u00fcssen.", "tokens": ["Den", "j\u00fcng\u00b7sten", "Tag", "zu", "gr\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Wenn ich schon klag,", "tokens": ["Wenn", "ich", "schon", "klag", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "So viel ich mag,", "tokens": ["So", "viel", "ich", "mag", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "VMFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Mein schwache Stimm zu heben,", "tokens": ["Mein", "schwa\u00b7che", "Stimm", "zu", "he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Weil ich m\u00f6cht l\u00e4nger leben;", "tokens": ["Weil", "ich", "m\u00f6cht", "l\u00e4n\u00b7ger", "le\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VMFIN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Mein Herz vernimmt", "tokens": ["Mein", "Herz", "ver\u00b7nimmt"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "In gleichem Schall,", "tokens": ["In", "glei\u00b7chem", "Schall", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Umsonst ist mein Bewerben.", "tokens": ["Um\u00b7sonst", "ist", "mein", "Be\u00b7wer\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Es bringt die Stimm", "tokens": ["Es", "bringt", "die", "Stimm"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Im Wiederhall,", "tokens": ["Im", "Wie\u00b7der\u00b7hall", ","], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "Ich m\u00fcsse leider sterben!", "tokens": ["Ich", "m\u00fcs\u00b7se", "lei\u00b7der", "ster\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Die Klinge z\u00fcck,", "tokens": ["Die", "Klin\u00b7ge", "z\u00fcck", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Ich nicht verr\u00fcck", "tokens": ["Ich", "nicht", "ver\u00b7r\u00fcck"], "token_info": ["word", "word", "word"], "pos": ["PPER", "PTKNEG", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Die perlenweisse Kehle,", "tokens": ["Die", "per\u00b7len\u00b7weis\u00b7se", "Keh\u00b7le", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Gott gnadet meiner Seele!", "tokens": ["Gott", "gna\u00b7det", "mei\u00b7ner", "See\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "In wei\u00df und roth", "tokens": ["In", "wei\u00df", "und", "roth"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "VVFIN", "KON", "ADJD"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Geziert will seyn,", "tokens": ["Ge\u00b7ziert", "will", "seyn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "VMFIN", "VAINF", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "In hocherw\u00fcnschten Farben;", "tokens": ["In", "ho\u00b7cher\u00b7w\u00fcnschten", "Far\u00b7ben", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Denn Jesu Tod", "tokens": ["Denn", "Je\u00b7su", "Tod"], "token_info": ["word", "word", "word"], "pos": ["KON", "NE", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Bricht R\u00f6selein,", "tokens": ["Bricht", "R\u00f6\u00b7se\u00b7lein", ","], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "Die nie bisher verdarben.", "tokens": ["Die", "nie", "bis\u00b7her", "ver\u00b7dar\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Mein Auge wankt,", "tokens": ["Mein", "Au\u00b7ge", "wankt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Am Mond erkrankt,", "tokens": ["Am", "Mond", "er\u00b7krankt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Er m\u00f6chte mir beyspringen,", "tokens": ["Er", "m\u00f6ch\u00b7te", "mir", "bey\u00b7sprin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mir drohn des Todes Klingen.", "tokens": ["Mir", "drohn", "des", "To\u00b7des", "Klin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Mu\u00df Sichelschein", "tokens": ["Mu\u00df", "Si\u00b7chel\u00b7schein"], "token_info": ["word", "word"], "pos": ["VMF