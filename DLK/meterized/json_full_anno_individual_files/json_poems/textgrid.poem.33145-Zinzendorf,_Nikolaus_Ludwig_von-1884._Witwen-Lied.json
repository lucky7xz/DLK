{"textgrid.poem.33145": {"metadata": {"author": {"name": "Zinzendorf, Nikolaus Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "1884. Witwen-Lied", "genre": "verse", "period": "N.A.", "pub_year": 1730, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du schl\u00f6ssest alles gern in deine wunden, und seligtest darin all unsre stunden.", "tokens": ["Du", "schl\u00f6s\u00b7sest", "al\u00b7les", "gern", "in", "dei\u00b7ne", "wun\u00b7den", ",", "und", "se\u00b7lig\u00b7test", "da\u00b7rin", "all", "uns\u00b7re", "stun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "ADV", "APPR", "PPOSAT", "ADJA", "$,", "KON", "VVFIN", "PAV", "PIAT", "PPOSAT", "ADJA", "$."], "meter": "-+-+-+-+-+--+-+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}}, "stanza.2": {"line.1": {"text": "So viel noch aussen her herum vagiren, sie m\u00f6gen fromm seyn oder resoniren;", "tokens": ["So", "viel", "noch", "aus\u00b7sen", "her", "he\u00b7rum", "va\u00b7gi\u00b7ren", ",", "sie", "m\u00f6\u00b7gen", "fromm", "seyn", "o\u00b7der", "re\u00b7so\u00b7ni\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "VVFIN", "ADV", "APZR", "VVINF", "$,", "PPER", "VMFIN", "ADJD", "VAINF", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+--+-+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}}, "stanza.3": {"line.1": {"text": "So denkt man immer, das wirds Lamm betr\u00fcben, ihr herze f\u00fchlt noch nicht das blutge lieben;", "tokens": ["So", "denkt", "man", "im\u00b7mer", ",", "das", "wirds", "Lamm", "be\u00b7tr\u00fc\u00b7ben", ",", "ihr", "her\u00b7ze", "f\u00fchlt", "noch", "nicht", "das", "blut\u00b7ge", "lie\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ADV", "$,", "ART", "ADJA", "NN", "VVINF", "$,", "PPOSAT", "ADJA", "VVFIN", "ADV", "PTKNEG", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+--+-+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}}, "stanza.4": {"line.1": {"text": "Wann wird doch tugend, samt dem wissen allen, in die f\u00fcnf offne wunden-brunnen fallen?", "tokens": ["Wann", "wird", "doch", "tu\u00b7gend", ",", "samt", "dem", "wis\u00b7sen", "al\u00b7len", ",", "in", "die", "f\u00fcnf", "off\u00b7ne", "wun\u00b7den\u00b7brun\u00b7nen", "fal\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ADV", "ADJD", "$,", "APPR", "ART", "VVFIN", "PIS", "$,", "APPR", "ART", "CARD", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+--+-+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}}, "stanza.5": {"line.1": {"text": "Man habe, was man hat, es ist nichts gutes, wenns nicht ist aus der quelle dieses blutes.", "tokens": ["Man", "ha\u00b7be", ",", "was", "man", "hat", ",", "es", "ist", "nichts", "gu\u00b7tes", ",", "wenns", "nicht", "ist", "aus", "der", "quel\u00b7le", "die\u00b7ses", "blu\u00b7tes", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "$,", "PRELS", "PIS", "VAFIN", "$,", "PPER", "VAFIN", "PIS", "ADJA", "$,", "KOUS", "PTKNEG", "VAFIN", "APPR", "ART", "NN", "PDAT", "ADJA", "$."], "meter": "-+-+-+-+-+--+-+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}}, "stanza.6": {"line.1": {"text": "O L\u00e4mmlein, lust-spiel unsrer innren sinnen! hab aller unsrer witwen herzen innen.", "tokens": ["O", "L\u00e4mm\u00b7lein", ",", "lust\u00b7spiel", "uns\u00b7rer", "inn\u00b7ren", "sin\u00b7nen", "!", "hab", "al\u00b7ler", "uns\u00b7rer", "wit\u00b7wen", "her\u00b7zen", "in\u00b7nen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "NN", "PPOSAT", "ADJA", "VVINF", "$.", "VAFIN", "PIAT", "PPOSAT", "NN", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+--+-+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}}, "stanza.7": {"line.1": {"text": "Dein Geist regier in ihrem witwen-hause in iedem winkelgen mit blut-gesause.", "tokens": ["Dein", "Geist", "re\u00b7gier", "in", "ih\u00b7rem", "wit\u00b7wen\u00b7hau\u00b7se", "in", "ie\u00b7dem", "win\u00b7kel\u00b7gen", "mit", "blut\u00b7ge\u00b7sau\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "APPR", "PPOSAT", "NN", "APPR", "PIS", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+--+-+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}}, "stanza.8": {"line.1": {"text": "Im alten bunde pflegt man dich zu preisen als mann ", "tokens": ["Im", "al\u00b7ten", "bun\u00b7de", "pflegt", "man", "dich", "zu", "prei\u00b7sen", "als", "mann"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "ADJA", "VVFIN", "PIS", "PRF", "APPR", "NN", "KOUS", "NN"], "meter": "-+-+-+-+-+-++", "measure": "unknown.measure.septa"}}, "stanza.9": {"line.1": {"text": "Im neuen kanst du keine weinen sehen, du liessest eh ein wunderwerk geschehen:", "tokens": ["Im", "neu\u00b7en", "kanst", "du", "kei\u00b7ne", "wei\u00b7nen", "se\u00b7hen", ",", "du", "lies\u00b7sest", "eh", "ein", "wun\u00b7der\u00b7werk", "ge\u00b7sche\u00b7hen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "VMFIN", "PPER", "PIAT", "VVINF", "VVINF", "$,", "PPER", "VVFIN", "KOUS", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+--+-+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}}, "stanza.10": {"line.1": {"text": "Denn an dem creuze unter tausend schmerzen lag dir Marie die witwe noch am herzen.", "tokens": ["Denn", "an", "dem", "creu\u00b7ze", "un\u00b7ter", "tau\u00b7send", "schmer\u00b7zen", "lag", "dir", "Ma\u00b7rie", "die", "wit\u00b7we", "noch", "am", "her\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "APPR", "CARD", "VVINF", "VVFIN", "PPER", "NE", "ART", "NN", "ADV", "APPRART", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+--+-+-+-", "measure": "iambic.octa.plus.relaxed"}}, "stanza.11": {"line.1": {"text": "Beleb das witwen-chor mit blutes-kr\u00e4ften, und la\u00df sie selig seyn in lichts-gesch\u00e4fften.", "tokens": ["Be\u00b7leb", "das", "wit\u00b7wen\u00b7chor", "mit", "blu\u00b7tes\u00b7kr\u00e4f\u00b7ten", ",", "und", "la\u00df", "sie", "se\u00b7lig", "seyn", "in", "lichts\u00b7ge\u00b7sch\u00e4ff\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ADJA", "$,", "KON", "VVIMP", "PPER", "ADJD", "VAINF", "APPR", "NN", "$."], "meter": "-+-+-+-+-+--+-+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}}, "stanza.12": {"line.1": {"text": "Sie sind mit-s\u00fcnderlein im kirchen-sprengel; befiehl auch sie dem schutze deiner engel;", "tokens": ["Sie", "sind", "mit\u00b7s\u00fcn\u00b7der\u00b7lein", "im", "kir\u00b7chen\u00b7spren\u00b7gel", ";", "be\u00b7fiehl", "auch", "sie", "dem", "schut\u00b7ze", "dei\u00b7ner", "en\u00b7gel", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "APPRART",