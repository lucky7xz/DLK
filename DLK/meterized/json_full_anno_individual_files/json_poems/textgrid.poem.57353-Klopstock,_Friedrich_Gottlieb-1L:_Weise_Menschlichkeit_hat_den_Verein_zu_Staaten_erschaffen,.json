{"textgrid.poem.57353": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "1L: Weise Menschlichkeit hat den Verein zu Staaten erschaffen,", "genre": "verse", "period": "N.A.", "pub_year": 1792, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Weise Menschlichkeit hat den Verein zu Staaten erschaffen,", "tokens": ["Wei\u00b7se", "Menschlich\u00b7keit", "hat", "den", "Ver\u00b7ein", "zu", "Staa\u00b7ten", "er\u00b7schaf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VAFIN", "ART", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "+-+-+--+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Hat zum Leben das Leben gemacht!", "tokens": ["Hat", "zum", "Le\u00b7ben", "das", "Le\u00b7ben", "ge\u00b7macht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPRART", "NN", "ART", "NN", "VVPP", "$."], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Wilde leben nicht; sie sind jetzt Pflanzen, dann athmen", "tokens": ["Wil\u00b7de", "le\u00b7ben", "nicht", ";", "sie", "sind", "jetzt", "Pflan\u00b7zen", ",", "dann", "ath\u00b7men"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "VVFIN", "PTKNEG", "$.", "PPER", "VAFIN", "ADV", "NN", "$,", "ADV", "VVINF"], "meter": "+-+-+-+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Sie als Thier' ohne Seelengenuss.", "tokens": ["Sie", "als", "Thier'", "oh\u00b7ne", "See\u00b7len\u00b7ge\u00b7nuss", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "KOUS", "NN", "APPR", "NN", "$."], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.5": {"text": "Hoch stieg in Europa empor des Vereins Ausbildung,", "tokens": ["Hoch", "stieg", "in", "Eu\u00b7ro\u00b7pa", "em\u00b7por", "des", "Ver\u00b7eins", "Aus\u00b7bil\u00b7dung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "APPR", "NE", "PTKVZ", "ART", "NN", "NN", "$,"], "meter": "+--+---+--+-+-", "measure": "dactylic.di.plus"}, "line.6": {"text": "Naht dem letzten der Ziele stets mehr;", "tokens": ["Naht", "dem", "letz\u00b7ten", "der", "Zie\u00b7le", "stets", "mehr", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "ART", "NN", "ADV", "ADV", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.7": {"text": "Ist nicht des Zeichners Entwurf, ist beynahe K\u00fcnstler vollendung,", "tokens": ["Ist", "nicht", "des", "Zeich\u00b7ners", "Ent\u00b7wurf", ",", "ist", "bey\u00b7na\u00b7he", "K\u00fcnst\u00b7ler", "vol\u00b7len\u00b7dung", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "ART", "NN", "NN", "$,", "VAFIN", "ADV", "NN", "NN", "$,"], "meter": "-+-+--+--+-+-+--", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Raphaels, oder Angelo's Werk,", "tokens": ["Ra\u00b7phaels", ",", "o\u00b7der", "An\u00b7ge\u00b7lo's", "Werk", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KON", "NE", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.9": {"text": "Raphaels, oder Angelo's Werk, wenn der Zauber der Farb' auch", "tokens": ["Ra\u00b7phaels", ",", "o\u00b7der", "An\u00b7ge\u00b7lo's", "Werk", ",", "wenn", "der", "Zau\u00b7ber", "der", "Fa\u00b7rb'", "auch"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "KON", "NE", "NN", "$,", "KOUS", "ART", "NN", "ART", "NN", "ADV"], "meter": "-+--+--+--+--+-+", "measure": "amphibrach.penta.plus"}, "line.10": {"text": "Hier und da Verzeichnung besch\u00f6nt.", "tokens": ["Hier", "und", "da", "Ver\u00b7zeich\u00b7nung", "be\u00b7sch\u00f6nt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KON", "KOUS", "NN", "VVFIN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.11": {"text": "Aber so bald die Beherscher der Nazionen statt ihrer", "tokens": ["A\u00b7ber", "so", "bald", "die", "Be\u00b7her\u00b7scher", "der", "Na\u00b7zi\u00b7o\u00b7nen", "statt", "ih\u00b7rer"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "ART", "NN", "ART", "NN", "APPR", "PPOSAT"], "meter": "+--+--+--+-+-+--", "measure": "elegiambus"}, "line.12": {"text": "Handeln; dann gebeut kein Gesetz,", "tokens": ["Han\u00b7deln", ";", "dann", "ge\u00b7beut", "kein", "Ge\u00b7setz", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ADV", "VVFIN", "PIAT", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.13": {"text": "Das dem B\u00fcrger gebeut, dann werben die Herschenden Wilde,", "tokens": ["Das", "dem", "B\u00fcr\u00b7ger", "ge\u00b7beut", ",", "dann", "wer\u00b7ben", "die", "Her\u00b7schen\u00b7den", "Wil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "VVFIN", "$,", "ADV", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}, "line.14": {"text": "L\u00f6wen, oder entz\u00fcndendes Kraut.", "tokens": ["L\u00f6\u00b7wen", ",", "o\u00b7der", "ent\u00b7z\u00fcn\u00b7den\u00b7des", "Kraut", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "ADJA", "NN", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.15": {"text": "Und jetzt wolt ihr sogar des Volkes Blut, das der Ziele", "tokens": ["Und", "jetzt", "wolt", "ihr", "so\u00b7gar", "des", "Vol\u00b7kes", "Blut", ",", "das", "der", "Zie\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "VMFIN", "PPER", "ADV", "ART", "NN", "NN", "$,", "PRELS", "ART", "NN"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.16": {"text": "Letztem vor allen V\u00f6lkern sich naht,", "tokens": ["Letz\u00b7tem", "vor", "al\u00b7len", "V\u00f6l\u00b7kern", "sich", "naht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "APPR", "PIAT", "NN", "PRF", "VVFIN", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.17": {"text": "Das, die belorberte Furie, Krieg der Erobrung, verbannend,", "tokens": ["Das", ",", "die", "be\u00b7lor\u00b7ber\u00b7te", "Fu\u00b7rie", ",", "Krieg", "der", "E\u00b7rob\u00b7rung", ",", "ver\u00b7ban\u00b7nend", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PDS", "$,", "ART", "ADJA", "NN", "$,", "NN", "ART", "NN", "$,", "VVPP", "$,"], "meter": "-+-+--+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.18": {"text": "Aller Gesetze sch\u00f6nstes sich gab;", "tokens": ["Al\u00b7ler", "Ge\u00b7set\u00b7ze", "sch\u00f6ns\u00b7tes", "sich", "gab", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJA", "PRF", "VVFIN", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.19": {"text": "Wolt das gepeinigte Volk, das Selbsterretter, der Freyheit", "tokens": ["Wolt", "das", "ge\u00b7pei\u00b7nig\u00b7te", "Volk", ",", "das", "Selbs\u00b7ter\u00b7ret\u00b7ter", ",", "der", "Frey\u00b7heit"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["VMFIN", "ART", "ADJA", "NN", "$,", "ART", "NN", "$,", "ART", "NN"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.20": {"text": "Gipfel erstieg, von der furchtbaren H\u00f6h,", "tokens": ["Gip\u00b7fel", "er\u00b7stieg", ",", "von", "der", "furcht\u00b7ba\u00b7ren", "H\u00f6h", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.21": {"text": "Feuer und Schwert in der Hand, herunter st\u00fcrzen, es zwingen", "tokens": ["Feu\u00b7er", "und", "Schwert", "in", "der", "Hand", ",", "her\u00b7un\u00b7ter", "st\u00fcr\u00b7zen", ",", "es", "zwin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NN", "KON", "NN", "APPR", "ART", "NN", "$,", "ADJD", "VVINF", "$,", "PPER", "VVINF"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.22": {"text": "Wilden von neuem dienstbar zu seyn;", "tokens": ["Wil\u00b7den", "von", "neu\u00b7em", "dienst\u00b7bar", "zu", "seyn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "ADJD", "PTKZU", "VAINF", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.23": {"text": "Wolt, dass der Richter der Welt, und, bebt, auch eurer, dem Menschen", "tokens": ["Wolt", ",", "dass", "der", "Rich\u00b7ter", "der", "Welt", ",", "und", ",", "bebt", ",", "auch", "eu\u00b7rer", ",", "dem", "Men\u00b7schen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["VMFIN", "$,", "KOUS", "ART", "NN", "ART", "NN", "$,", "KON", "$,", "VVFIN", "$,", "ADV", "PPOSAT", "$,", "ART", "NN"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.24": {"text": "Rechte nicht gab, erweisen durch Mord!", "tokens": ["Rech\u00b7te", "nicht", "gab", ",", "er\u00b7wei\u00b7sen", "durch", "Mord", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "VVFIN", "$,", "VVPP", "APPR", "NN", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.25": {"text": "M\u00f6chtet ihr, ehe das Schwert von der Wunde triefet, der Klugheit", "tokens": ["M\u00f6ch\u00b7tet", "ihr", ",", "e\u00b7he", "das", "Schwert", "von", "der", "Wun\u00b7de", "trie\u00b7fet", ",", "der", "Klug\u00b7heit"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VMFIN", "PPER", "$,", "KOUS", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,", "ART", "NN"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.26": {"text": "Ernste, warnende Winke verstehn!", "tokens": ["Erns\u00b7te", ",", "war\u00b7nen\u00b7de", "Win\u00b7ke", "ver\u00b7stehn", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJA", "NN", "VVINF", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.27": {"text": "M\u00f6chtet ihr sehn! Es entgl\u00fcht schon in euren Landen die Asche,", "tokens": ["M\u00f6ch\u00b7tet", "ihr", "sehn", "!", "Es", "ent\u00b7gl\u00fcht", "schon", "in", "eu\u00b7ren", "Lan\u00b7den", "die", "A\u00b7sche", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVINF", "$.", "PPER", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.28": {"text": "Wird von erwachenden Funken schon roth.", "tokens": ["Wird", "von", "er\u00b7wa\u00b7chen\u00b7den", "Fun\u00b7ken", "schon", "roth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ADJA", "NN", "ADV", "ADJD", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.29": {"text": "Fragt die H\u00f6flinge nicht, noch die mit Verdienste gebornen,", "tokens": ["Fragt", "die", "H\u00f6f\u00b7lin\u00b7ge", "nicht", ",", "noch", "die", "mit", "Ver\u00b7diens\u00b7te", "ge\u00b7bor\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKNEG", "$,", "ADV", "ART", "APPR", "NN", "VVPP", "$,"], "meter": "+--+-+--+-+--+-", "measure": "hexameter"}, "line.30": {"text": "Deren Blut in den Schlachten euch fliesst;", "tokens": ["De\u00b7ren", "Blut", "in", "den", "Schlach\u00b7ten", "euch", "fliesst", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "APPR", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.31": {"text": "Fragt, der blinken die Pflugschaar l\u00e4sst, die Gemeinen des Heeres,", "tokens": ["Fragt", ",", "der", "blin\u00b7ken", "die", "Pflug\u00b7schaar", "l\u00e4sst", ",", "die", "Ge\u00b7mei\u00b7nen", "des", "Hee\u00b7res", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PRELS", "VVFIN", "ART", "NN", "VVFIN", "$,", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.32": {"text": "Deren Blut auch Wasser nicht ist:", "tokens": ["De\u00b7ren", "Blut", "auch", "Was\u00b7ser", "nicht", "ist", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "ADV", "NN", "PTKNEG", "VAFIN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.33": {"text": "Und durch redliche Antwort erf\u00fchret ihr, oder durch lautes", "tokens": ["Und", "durch", "red\u00b7li\u00b7che", "Ant\u00b7wort", "er\u00b7f\u00fch\u00b7ret", "ihr", ",", "o\u00b7der", "durch", "lau\u00b7tes"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN", "VVFIN", "PPER", "$,", "KON", "APPR", "ADJA"], "meter": "--+--+--+-+-+-+-", "measure": "anapaest.tri.plus"}, "line.34": {"text": "Schweigen, was in der Asche sie sehn.", "tokens": ["Schwei\u00b7gen", ",", "was", "in", "der", "A\u00b7sche", "sie", "sehn", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "APPR", "ART", "NN", "PPER", "VVINF", "$."], "meter": "+-++-+--+", "measure": "iambic.penta.chol"}, "line.35": {"text": "Doch ihr verachtet sie. Spielt denn des neugestalteten Krieges", "tokens": ["Doch", "ihr", "ver\u00b7ach\u00b7tet", "sie", ".", "Spielt", "denn", "des", "neu\u00b7ge\u00b7stal\u00b7te\u00b7ten", "Krie\u00b7ges"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPER", "$.", "NN", "KON", "ART", "ADJA", "NN"], "meter": "-+-+--+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.36": {"text": "Nie versuchtes, schreckliches Spiel,", "tokens": ["Nie", "ver\u00b7such\u00b7tes", ",", "schreck\u00b7li\u00b7ches", "Spiel", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.37": {"text": "Alzuschreckliches! Denn in den Kriegen werden verg\u00f6tzten", "tokens": ["Al\u00b7zu\u00b7schreck\u00b7li\u00b7ches", "!", "Denn", "in", "den", "Krie\u00b7gen", "wer\u00b7den", "ver\u00b7g\u00f6tz\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$.", "KON", "APPR", "ART", "NN", "VAFIN", "VVINF"], "meter": "+-+-+-+-+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.38": {"text": "Herschern Menschenopfer gebracht.", "tokens": ["Her\u00b7schern", "Men\u00b7schen\u00b7op\u00b7fer", "ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NN", "VVPP", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.39": {"text": "Sterbliche wissen nicht, was Gott thun wird: doch gewahren", "tokens": ["Sterb\u00b7li\u00b7che", "wis\u00b7sen", "nicht", ",", "was", "Gott", "thun", "wird", ":", "doch", "ge\u00b7wah\u00b7ren"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "VVFIN", "PTKNEG", "$,", "PRELS", "NN", "VVINF", "VAFIN", "$.", "ADV", "VVINF"], "meter": "+--+-+-+-+--+-", "measure": "hexameter"}, "line.40": {"text": "Sie, wenn grosse Dinge geschehn,", "tokens": ["Sie", ",", "wenn", "gros\u00b7se", "Din\u00b7ge", "ge\u00b7schehn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "KOUS", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.41": {"text": "Jetzt sein langsames Wandeln, jetzt donnernden Gang der Entscheidung,", "tokens": ["Jetzt", "sein", "lang\u00b7sa\u00b7mes", "Wan\u00b7deln", ",", "jetzt", "don\u00b7nern\u00b7den", "Gang", "der", "Ent\u00b7schei\u00b7dung", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "ADJA", "NN", "$,", "ADV", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+--+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.42": {"text": "Der mit furchtbarer Eil' es vollbringt.", "tokens": ["Der", "mit", "furcht\u00b7ba\u00b7rer", "Eil'", "es", "voll\u00b7bringt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.43": {"text": "Wer zu t\u00e4uschen vermag, und mich liebt, der t\u00e4uscht den Erlebung", "tokens": ["Wer", "zu", "t\u00e4u\u00b7schen", "ver\u00b7mag", ",", "und", "mich", "liebt", ",", "der", "t\u00e4uscht", "den", "Er\u00b7le\u00b7bung"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "PTKZU", "VVINF", "VVFIN", "$,", "KON", "PPER", "VVFIN", "$,", "PRELS", "VVFIN", "ART", "NN"], "meter": "--+--+--+-+-+-+", "measure": "anapaest.tri.plus"}, "line.44": {"text": "W\u00fcnschenden, weissagt donnernden Gang.", "tokens": ["W\u00fcn\u00b7schen\u00b7den", ",", "weis\u00b7sagt", "don\u00b7nern\u00b7den", "Gang", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "ADJA", "NN", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}}, "stanza.2": {"line.1": {"text": "Weise Menschlichkeit hat den Verein zu Staaten erschaffen,", "tokens": ["Wei\u00b7se", "Menschlich\u00b7keit", "hat", "den", "Ver\u00b7ein", "zu", "Staa\u00b7ten", "er\u00b7schaf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VAFIN", "ART", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "+-+-+--+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Hat zum Leben das Leben gemacht!", "tokens": ["Hat", "zum", "Le\u00b7ben", "das", "Le\u00b7ben", "ge\u00b7macht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPRART", "NN", "ART", "NN", "VVPP", "$."], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Wilde leben nicht; sie sind jetzt Pflanzen, dann athmen", "tokens": ["Wil\u00b7de", "le\u00b7ben", "nicht", ";", "sie", "sind", "jetzt", "Pflan\u00b7zen", ",", "dann", "ath\u00b7men"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "VVFIN", "PTKNEG", "$.", "PPER", "VAFIN", "ADV", "NN", "$,", "ADV", "VVINF"], "meter": "+-+-+-+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Sie als Thier' ohne Seelengenuss.", "tokens": ["Sie", "als", "Thier'", "oh\u00b7ne", "See\u00b7len\u00b7ge\u00b7nuss", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "KOUS", "NN", "APPR", "NN", "$."], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.5": {"text": "Hoch stieg in Europa empor des Vereins Ausbildung,", "tokens": ["Hoch", "stieg", "in", "Eu\u00b7ro\u00b7pa", "em\u00b7por", "des", "Ver\u00b7eins", "Aus\u00b7bil\u00b7dung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "APPR", "NE", "PTKVZ", "ART", "NN", "NN", "$,"], "meter": "+--+---+--+-+-", "measure": "dactylic.di.plus"}, "line.6": {"text": "Naht dem letzten der Ziele stets mehr;", "tokens": ["Naht", "dem", "letz\u00b7ten", "der", "Zie\u00b7le", "stets", "mehr", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "ART", "NN", "ADV", "ADV", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.7": {"text": "Ist nicht des Zeichners Entwurf, ist beynahe K\u00fcnstler vollendung,", "tokens": ["Ist", "nicht", "des", "Zeich\u00b7ners", "Ent\u00b7wurf", ",", "ist", "bey\u00b7na\u00b7he", "K\u00fcnst\u00b7ler", "vol\u00b7len\u00b7dung", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "ART", "NN", "NN", "$,", "VAFIN", "ADV", "NN", "NN", "$,"], "meter": "-+-+--+--+-+-+--", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Raphaels, oder Angelo's Werk,", "tokens": ["Ra\u00b7phaels", ",", "o\u00b7der", "An\u00b7ge\u00b7lo's", "Werk", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KON", "NE", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.9": {"text": "Raphaels, oder Angelo's Werk, wenn der Zauber der Farb' auch", "tokens": ["Ra\u00b7phaels", ",", "o\u00b7der", "An\u00b7ge\u00b7lo's", "Werk", ",", "wenn", "der", "Zau\u00b7ber", "der", "Fa\u00b7rb'", "auch"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "KON", "NE", "NN", "$,", "KOUS", "ART", "NN", "ART", "NN", "ADV"], "meter": "-+--+--+--+--+-+", "measure": "amphibrach.penta.plus"}, "line.10": {"text": "Hier und da Verzeichnung besch\u00f6nt.", "tokens": ["Hier", "und", "da", "Ver\u00b7zeich\u00b7nung", "be\u00b7sch\u00f6nt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KON", "KOUS", "NN", "VVFIN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.11": {"text": "Aber so bald die Beherscher der Nazionen statt ihrer", "tokens": ["A\u00b7ber", "so", "bald", "die", "Be\u00b7her\u00b7scher", "der", "Na\u00b7zi\u00b7o\u00b7nen", "statt", "ih\u00b7rer"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "ART", "NN", "ART", "NN", "APPR", "PPOSAT"], "meter": "+--+--+--+-+-+--", "measure": "elegiambus"}, "line.12": {"text": "Handeln; dann gebeut kein Gesetz,", "tokens": ["Han\u00b7deln", ";", "dann", "ge\u00b7beut", "kein", "Ge\u00b7setz", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ADV", "VVFIN", "PIAT", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.13": {"text": "Das dem B\u00fcrger gebeut, dann werben die Herschenden Wilde,", "tokens": ["Das", "dem", "B\u00fcr\u00b7ger", "ge\u00b7beut", ",", "dann", "wer\u00b7ben", "die", "Her\u00b7schen\u00b7den", "Wil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "VVFIN", "$,", "ADV", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}, "line.14": {"text": "L\u00f6wen, oder entz\u00fcndendes Kraut.", "tokens": ["L\u00f6\u00b7wen", ",", "o\u00b7der", "ent\u00b7z\u00fcn\u00b7den\u00b7des", "Kraut", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "ADJA", "NN", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.15": {"text": "Und jetzt wolt ihr sogar des Volkes Blut, das der Ziele", "tokens": ["Und", "jetzt", "wolt", "ihr", "so\u00b7gar", "des", "Vol\u00b7kes", "Blut", ",", "das", "der", "Zie\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "VMFIN", "PPER", "ADV", "ART", "NN", "NN", "$,", "PRELS", "ART", "NN"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.16": {"text": "Letztem vor allen V\u00f6lkern sich naht,", "tokens": ["Letz\u00b7tem", "vor", "al\u00b7len", "V\u00f6l\u00b7kern", "sich", "naht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "APPR", "PIAT", "NN", "PRF", "VVFIN", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.17": {"text": "Das, die belorberte Furie, Krieg der Erobrung, verbannend,", "tokens": ["Das", ",", "die", "be\u00b7lor\u00b7ber\u00b7te", "Fu\u00b7rie", ",", "Krieg", "der", "E\u00b7rob\u00b7rung", ",", "ver\u00b7ban\u00b7nend", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PDS", "$,", "ART", "ADJA", "NN", "$,", "NN", "ART", "NN", "$,", "VVPP", "$,"], "meter": "-+-+--+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.18": {"text": "Aller Gesetze sch\u00f6nstes sich gab;", "tokens": ["Al\u00b7ler", "Ge\u00b7set\u00b7ze", "sch\u00f6ns\u00b7tes", "sich", "gab", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJA", "PRF", "VVFIN", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.19": {"text": "Wolt das gepeinigte Volk, das Selbsterretter, der Freyheit", "tokens": ["Wolt", "das", "ge\u00b7pei\u00b7nig\u00b7te", "Volk", ",", "das", "Selbs\u00b7ter\u00b7ret\u00b7ter", ",", "der", "Frey\u00b7heit"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["VMFIN", "ART", "ADJA", "NN", "$,", "ART", "NN", "$,", "ART", "NN"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.20": {"text": "Gipfel erstieg, von der furchtbaren H\u00f6h,", "tokens": ["Gip\u00b7fel", "er\u00b7stieg", ",", "von", "der", "furcht\u00b7ba\u00b7ren", "H\u00f6h", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.21": {"text": "Feuer und Schwert in der Hand, herunter st\u00fcrzen, es zwingen", "tokens": ["Feu\u00b7er", "und", "Schwert", "in", "der", "Hand", ",", "her\u00b7un\u00b7ter", "st\u00fcr\u00b7zen", ",", "es", "zwin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NN", "KON", "NN", "APPR", "ART", "NN", "$,", "ADJD", "VVINF", "$,", "PPER", "VVINF"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.22": {"text": "Wilden von neuem dienstbar zu seyn;", "tokens": ["Wil\u00b7den", "von", "neu\u00b7em", "dienst\u00b7bar", "zu", "seyn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "ADJD", "PTKZU", "VAINF", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.23": {"text": "Wolt, dass der Richter der Welt, und, bebt, auch eurer, dem Menschen", "tokens": ["Wolt", ",", "dass", "der", "Rich\u00b7ter", "der", "Welt", ",", "und", ",", "bebt", ",", "auch", "eu\u00b7rer", ",", "dem", "Men\u00b7schen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["VMFIN", "$,", "KOUS", "ART", "NN", "ART", "NN", "$,", "KON", "$,", "VVFIN", "$,", "ADV", "PPOSAT", "$,", "ART", "NN"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.24": {"text": "Rechte nicht gab, erweisen durch Mord!", "tokens": ["Rech\u00b7te", "nicht", "gab", ",", "er\u00b7wei\u00b7sen", "durch", "Mord", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "VVFIN", "$,", "VVPP", "APPR", "NN", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.25": {"text": "M\u00f6chtet ihr, ehe das Schwert von der Wunde triefet, der Klugheit", "tokens": ["M\u00f6ch\u00b7tet", "ihr", ",", "e\u00b7he", "das", "Schwert", "von", "der", "Wun\u00b7de", "trie\u00b7fet", ",", "der", "Klug\u00b7heit"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VMFIN", "PPER", "$,", "KOUS", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,", "ART", "NN"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.26": {"text": "Ernste, warnende Winke verstehn!", "tokens": ["Erns\u00b7te", ",", "war\u00b7nen\u00b7de", "Win\u00b7ke", "ver\u00b7stehn", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJA", "NN", "VVINF", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.27": {"text": "M\u00f6chtet ihr sehn! Es entgl\u00fcht schon in euren Landen die Asche,", "tokens": ["M\u00f6ch\u00b7tet", "ihr", "sehn", "!", "Es", "ent\u00b7gl\u00fcht", "schon", "in", "eu\u00b7ren", "Lan\u00b7den", "die", "A\u00b7sche", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVINF", "$.", "PPER", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.28": {"text": "Wird von erwachenden Funken schon roth.", "tokens": ["Wird", "von", "er\u00b7wa\u00b7chen\u00b7den", "Fun\u00b7ken", "schon", "roth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ADJA", "NN", "ADV", "ADJD", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.29": {"text": "Fragt die H\u00f6flinge nicht, noch die mit Verdienste gebornen,", "tokens": ["Fragt", "die", "H\u00f6f\u00b7lin\u00b7ge", "nicht", ",", "noch", "die", "mit", "Ver\u00b7diens\u00b7te", "ge\u00b7bor\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKNEG", "$,", "ADV", "ART", "APPR", "NN", "VVPP", "$,"], "meter": "+--+-+--+-+--+-", "measure": "hexameter"}, "line.30": {"text": "Deren Blut in den Schlachten euch fliesst;", "tokens": ["De\u00b7ren", "Blut", "in", "den", "Schlach\u00b7ten", "euch", "fliesst", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "APPR", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.31": {"text": "Fragt, der blinken die Pflugschaar l\u00e4sst, die Gemeinen des Heeres,", "tokens": ["Fragt", ",", "der", "blin\u00b7ken", "die", "Pflug\u00b7schaar", "l\u00e4sst", ",", "die", "Ge\u00b7mei\u00b7nen", "des", "Hee\u00b7res", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PRELS", "VVFIN", "ART", "NN", "VVFIN", "$,", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.32": {"text": "Deren Blut auch Wasser nicht ist:", "tokens": ["De\u00b7ren", "Blut", "auch", "Was\u00b7ser", "nicht", "ist", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "ADV", "NN", "PTKNEG", "VAFIN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.33": {"text": "Und durch redliche Antwort erf\u00fchret ihr, oder durch lautes", "tokens": ["Und", "durch", "red\u00b7li\u00b7che", "Ant\u00b7wort", "er\u00b7f\u00fch\u00b7ret", "ihr", ",", "o\u00b7der", "durch", "lau\u00b7tes"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN", "VVFIN", "PPER", "$,", "KON", "APPR", "ADJA"], "meter": "--+--+--+-+-+-+-", "measure": "anapaest.tri.plus"}, "line.34": {"text": "Schweigen, was in der Asche sie sehn.", "tokens": ["Schwei\u00b7gen", ",", "was", "in", "der", "A\u00b7sche", "sie", "sehn", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "APPR", "ART", "NN", "PPER", "VVINF", "$."], "meter": "+-++-+--+", "measure": "iambic.penta.chol"}, "line.35": {"text": "Doch ihr verachtet sie. Spielt denn des neugestalteten Krieges", "tokens": ["Doch", "ihr", "ver\u00b7ach\u00b7tet", "sie", ".", "Spielt", "denn", "des", "neu\u00b7ge\u00b7stal\u00b7te\u00b7ten", "Krie\u00b7ges"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPER", "$.", "NN", "KON", "ART", "ADJA", "NN"], "meter": "-+-+--+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.36": {"text": "Nie versuchtes, schreckliches Spiel,", "tokens": ["Nie", "ver\u00b7such\u00b7tes", ",", "schreck\u00b7li\u00b7ches", "Spiel", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.37": {"text": "Alzuschreckliches! Denn in den Kriegen werden verg\u00f6tzten", "tokens": ["Al\u00b7zu\u00b7schreck\u00b7li\u00b7ches", "!", "Denn", "in", "den", "Krie\u00b7gen", "wer\u00b7den", "ver\u00b7g\u00f6tz\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$.", "KON", "APPR", "ART", "NN", "VAFIN", "VVINF"], "meter": "+-+-+-+-+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.38": {"text": "Herschern Menschenopfer gebracht.", "tokens": ["Her\u00b7schern", "Men\u00b7schen\u00b7op\u00b7fer", "ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NN", "VVPP", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.39": {"text": "Sterbliche wissen nicht, was Gott thun wird: doch gewahren", "tokens": ["Sterb\u00b7li\u00b7che", "wis\u00b7sen", "nicht", ",", "was", "Gott", "thun", "wird", ":", "doch", "ge\u00b7wah\u00b7ren"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "VVFIN", "PTKNEG", "$,", "PRELS", "NN", "VVINF", "VAFIN", "$.", "ADV", "VVINF"], "meter": "+--+-+-+-+--+-", "measure": "hexameter"}, "line.40": {"text": "Sie, wenn grosse Dinge geschehn,", "tokens": ["Sie", ",", "wenn", "gros\u00b7se", "Din\u00b7ge", "ge\u00b7schehn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "KOUS", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.41": {"text": "Jetzt sein langsames Wandeln, jetzt donnernden Gang der Entscheidung,", "tokens": ["Jetzt", "sein", "lang\u00b7sa\u00b7mes", "Wan\u00b7deln", ",", "jetzt", "don\u00b7nern\u00b7den", "Gang", "der", "Ent\u00b7schei\u00b7dung", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "ADJA", "NN", "$,", "ADV", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+--+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.42": {"text": "Der mit furchtbarer Eil' es vollbringt.", "tokens": ["Der", "mit", "furcht\u00b7ba\u00b7rer", "Eil'", "es", "voll\u00b7bringt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.43": {"text": "Wer zu t\u00e4uschen vermag, und mich liebt, der t\u00e4uscht den Erlebung", "tokens": ["Wer", "zu", "t\u00e4u\u00b7schen", "ver\u00b7mag", ",", "und", "mich", "liebt", ",", "der", "t\u00e4uscht", "den", "Er\u00b7le\u00b7bung"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "PTKZU", "VVINF", "VVFIN", "$,", "KON", "PPER", "VVFIN", "$,", "PRELS", "VVFIN", "ART", "NN"], "meter": "--+--+--+-+-+-+", "measure": "anapaest.tri.plus"}, "line.44": {"text": "W\u00fcnschenden, weissagt donnernden Gang.", "tokens": ["W\u00fcn\u00b7schen\u00b7den", ",", "weis\u00b7sagt", "don\u00b7nern\u00b7den", "Gang", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "ADJA", "NN", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}}}}}