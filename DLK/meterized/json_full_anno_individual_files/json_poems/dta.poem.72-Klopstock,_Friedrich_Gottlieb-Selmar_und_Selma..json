{"dta.poem.72": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "Selmar und Selma.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1771", "urn": "urn:nbn:de:kobv:b4-200905199486", "language": ["de:0.99"], "booktitle": "[Klopstock, Friedrich Gottlieb]: Oden. Hamburg, 1771."}, "poem": {"stanza.1": {"line.1": {"text": "Meine Selma, wenn aber der Tod uns Liebende", "tokens": ["Mei\u00b7ne", "Sel\u00b7ma", ",", "wenn", "a\u00b7ber", "der", "Tod", "uns", "Lie\u00b7ben\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NE", "$,", "KOUS", "ADV", "ART", "NN", "PPER", "NN"], "meter": "+--+-+--+-+--", "measure": "iambic.penta.invert"}, "line.2": {"text": "Wenn dein Geschick dich zuerst zu den Unsterbli-", "tokens": ["Wenn", "dein", "Ge\u00b7schick", "dich", "zu\u00b7erst", "zu", "den", "Un\u00b7ster\u00b7bli"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "ADV", "APPR", "ART", "TRUNC"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Ach, so werd ich um dich mein ganzes Leben durch-", "tokens": ["Ach", ",", "so", "werd", "ich", "um", "dich", "mein", "gan\u00b7zes", "Le\u00b7ben", "durch"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "ADV", "VAFIN", "PPER", "APPR", "PPER", "PPOSAT", "ADJA", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Jeden n\u00e4chtlichen Tag, jede noch tr\u00fcbere Nacht!", "tokens": ["Je\u00b7den", "n\u00e4cht\u00b7li\u00b7chen", "Tag", ",", "je\u00b7de", "noch", "tr\u00fc\u00b7be\u00b7re", "Nacht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$,", "PIAT", "ADV", "ADJA", "NN", "$."], "meter": "+-+--++--+--+", "measure": "trochaic.hexa.relaxed"}, "line.5": {"text": "Jede Stunde, die sonst in deiner Umarmung vorbey flo\u00df,", "tokens": ["Je\u00b7de", "Stun\u00b7de", ",", "die", "sonst", "in", "dei\u00b7ner", "Um\u00b7ar\u00b7mung", "vor\u00b7bey", "flo\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PRELS", "ADV", "APPR", "PPOSAT", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+--+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.6": {"text": "Jede Minute, die uns, z\u00e4rtlich genossen, entfloh!", "tokens": ["Je\u00b7de", "Mi\u00b7nu\u00b7te", ",", "die", "uns", ",", "z\u00e4rt\u00b7lich", "ge\u00b7nos\u00b7sen", ",", "ent\u00b7floh", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PRELS", "PPER", "$,", "ADJD", "VVPP", "$,", "VVFIN", "$."], "meter": "+--+-+-+--+--+", "measure": "iambic.hexa.invert"}, "line.7": {"text": "Ach, so vergehen mir dann die \u00fcbrigen Jahre voll", "tokens": ["Ach", ",", "so", "ver\u00b7ge\u00b7hen", "mir", "dann", "die", "\u00fcb\u00b7ri\u00b7gen", "Jah\u00b7re", "voll"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "ADV", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "ADJD"], "meter": "+--+--+-+--+-+", "measure": "dactylic.di.plus"}, "line.8": {"text": "Wie der vergangenen keins ungeliebt uns entfloh.", "tokens": ["Wie", "der", "ver\u00b7gan\u00b7ge\u00b7nen", "keins", "un\u00b7ge\u00b7liebt", "uns", "ent\u00b7floh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "PIAT", "ADJD", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.9": {"text": "Ach mein Selmar, wenn k\u00fcnftig der Tod uns Lie-", "tokens": ["Ach", "mein", "Sel\u00b7mar", ",", "wenn", "k\u00fcnf\u00b7tig", "der", "Tod", "uns", "Lie"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "PPOSAT", "NE", "$,", "KOUS", "ADJD", "ART", "NN", "PPER", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "Wenn dein Geschick dich zuerst zu den Unsterb-", "tokens": ["Wenn", "dein", "Ge\u00b7schick", "dich", "zu\u00b7erst", "zu", "den", "Un\u00b7ster\u00b7b"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "ADV", "APPR", "ART", "TRUNC"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.11": {"text": "Ach, dann wein\u2019 ich um dich mein ganzes \u00fcbriges Leben,", "tokens": ["Ach", ",", "dann", "wein'", "ich", "um", "dich", "mein", "gan\u00b7zes", "\u00fcb\u00b7ri\u00b7ges", "Le\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADV", "VVFIN", "PPER", "APPR", "PPER", "PPOSAT", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "Jeden unbrauchbaren Tag, jede mir schreckliche", "tokens": ["Je\u00b7den", "un\u00b7brauch\u00b7ba\u00b7ren", "Tag", ",", "je\u00b7de", "mir", "schreck\u00b7li\u00b7che"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PIAT", "ADJA", "NN", "$,", "PIAT", "PPER", "ADJA"], "meter": "+-+-+--+--+--", "measure": "trochaic.penta.relaxed"}, "line.13": {"text": "Jede Stunde, die sonst, mit deinem L\u00e4cheln erheitert,", "tokens": ["Je\u00b7de", "Stun\u00b7de", ",", "die", "sonst", ",", "mit", "dei\u00b7nem", "L\u00e4\u00b7cheln", "er\u00b7hei\u00b7tert", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PRELS", "ADV", "$,", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.14": {"text": "Unter dem s\u00fcssen Gespr\u00e4ch z\u00e4rtlicher Thr\u00e4nen ent-", "tokens": ["Un\u00b7ter", "dem", "s\u00fcs\u00b7sen", "Ge\u00b7spr\u00e4ch", "z\u00e4rt\u00b7li\u00b7cher", "Thr\u00e4\u00b7nen", "ent"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "ADJA", "NN", "TRUNC"], "meter": "+--+--++--+--", "measure": "dactylic.di.plus"}, "line.15": {"text": "Ach so vergehen mir dann die \u00fcbrigen Tage voll Schwer-", "tokens": ["Ach", "so", "ver\u00b7ge\u00b7hen", "mir", "dann", "die", "\u00fcb\u00b7ri\u00b7gen", "Ta\u00b7ge", "voll", "Schwer"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "ADV", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "ADJD", "TRUNC"], "meter": "+--+--+-+--+--+", "measure": "dactylic.di.plus"}, "line.16": {"text": "Wie der vergangenen keins ungeliebt uns entfloh.", "tokens": ["Wie", "der", "ver\u00b7gan\u00b7ge\u00b7nen", "keins", "un\u00b7ge\u00b7liebt", "uns", "ent\u00b7floh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "PIAT", "ADJD", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.17": {"text": "Meine Selma, du wolltest nach mir nur Tage noch", "tokens": ["Mei\u00b7ne", "Sel\u00b7ma", ",", "du", "woll\u00b7test", "nach", "mir", "nur", "Ta\u00b7ge", "noch"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NE", "$,", "PPER", "VMFIN", "APPR", "PPER", "ADV", "NN", "ADV"], "meter": "+-+--+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.18": {"text": "Und ich br\u00e4chte noch die Jahre voll Traurigkeit zu?", "tokens": ["Und", "ich", "br\u00e4ch\u00b7te", "noch", "die", "Jah\u00b7re", "voll", "Trau\u00b7rig\u00b7keit", "zu", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "ART", "NN", "ADJD", "NN", "PTKVZ", "$."], "meter": "--+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.19": {"text": "Selma, Selma, nur wenig unbrauchbare tr\u00fcbe Mi-", "tokens": ["Sel\u00b7ma", ",", "Sel\u00b7ma", ",", "nur", "we\u00b7nig", "un\u00b7brauch\u00b7ba\u00b7re", "tr\u00fc\u00b7be", "Mi"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "NE", "$,", "ADV", "PIAT", "ADJA", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.20": {"text": "Bring ich, bist du erbla\u00dft, neben dir seelenlos zu!", "tokens": ["Bring", "ich", ",", "bist", "du", "er\u00b7bla\u00dft", ",", "ne\u00b7ben", "dir", "see\u00b7len\u00b7los", "zu", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "VAFIN", "PPER", "VVFIN", "$,", "APPR", "PPER", "ADJD", "PTKVZ", "$."], "meter": "+-+--+-+-+---", "measure": "trochaic.penta.relaxed"}, "line.21": {"text": "Nehme noch Einmal die Hand der Todten, k\u00fcsse dein", "tokens": ["Neh\u00b7me", "noch", "Ein\u00b7mal", "die", "Hand", "der", "Tod\u00b7ten", ",", "k\u00fcs\u00b7se", "dein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "ADV", "ADV", "ART", "NN", "ART", "NN", "$,", "VVFIN", "PPOSAT"], "meter": "+--+--+-+-+-+", "measure": "dactylic.di.plus"}, "line.22": {"text": "Einmal noch, in die Nacht sink ich, und sterbe", "tokens": ["Ein\u00b7mal", "noch", ",", "in", "die", "Nacht", "sink", "ich", ",", "und", "ster\u00b7be"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ADV", "$,", "APPR", "ART", "NN", "VVFIN", "PPER", "$,", "KON", "VVFIN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.23": {"text": "Selmar, ich sterbe nach dir! den Schmerz soll Sel-", "tokens": ["Sel\u00b7mar", ",", "ich", "ster\u00b7be", "nach", "dir", "!", "den", "Schmerz", "soll", "Sel"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "PPER", "VVFIN", "APPR", "PPER", "$.", "ART", "NN", "VMFIN", "TRUNC"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.24": {"text": "Da\u00df er sterbend mich sieht. Selmar, ich sterbe", "tokens": ["Da\u00df", "er", "ster\u00b7bend", "mich", "sieht", ".", "Sel\u00b7mar", ",", "ich", "ster\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "PPER", "VVFIN", "$.", "NE", "$,", "PPER", "VVFIN"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.25": {"text": "Bringe dann auch nur wenig unbrauchbare tr\u00fcbe Mi-", "tokens": ["Brin\u00b7ge", "dann", "auch", "nur", "we\u00b7nig", "un\u00b7brauch\u00b7ba\u00b7re", "tr\u00fc\u00b7be", "Mi"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ADV", "ADV", "PIAT", "ADJA", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.26": {"text": "Bist du, Selmar, erbla\u00dft, neben dir seelenlos zu!", "tokens": ["Bist", "du", ",", "Sel\u00b7mar", ",", "er\u00b7bla\u00dft", ",", "ne\u00b7ben", "dir", "see\u00b7len\u00b7los", "zu", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "NE", "$,", "VVFIN", "$,", "APPR", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+---", "measure": "unknown.measure.penta"}, "line.27": {"text": "Blicke noch einmal dich an, und seufze noch einmal:", "tokens": ["Bli\u00b7cke", "noch", "ein\u00b7mal", "dich", "an", ",", "und", "seuf\u00b7ze", "noch", "ein\u00b7mal", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADV", "PPER", "PTKVZ", "$,", "KON", "VVFIN", "ADV", "ADV", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.28": {"text": "Sink an die ruhende Brust, zittr\u2019 und erblasse", "tokens": ["Sink", "an", "die", "ru\u00b7hen\u00b7de", "Brust", ",", "zit\u00b7tr'", "und", "er\u00b7blas\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,", "VVFIN", "KON", "VVFIN"], "meter": "-+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.29": {"text": "Selma, du st\u00fcrbest nach mir? den Schmerz soll Sel-", "tokens": ["Sel\u00b7ma", ",", "du", "st\u00fcr\u00b7best", "nach", "mir", "?", "den", "Schmerz", "soll", "Sel"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "PPER", "VVFIN", "APPR", "PPER", "$.", "ART", "NN", "VMFIN", "TRUNC"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.30": {"text": "Da\u00df sie sterbend mich sieht. Selma, du stirbst", "tokens": ["Da\u00df", "sie", "ster\u00b7bend", "mich", "sieht", ".", "Sel\u00b7ma", ",", "du", "stirbst"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "PPER", "VVFIN", "$.", "NE", "$,", "PPER", "VVFIN"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.31": {"text": "Selmar, ich sterbe nach dir! Das ist es, was ich vom", "tokens": ["Sel\u00b7mar", ",", "ich", "ster\u00b7be", "nach", "dir", "!", "Das", "ist", "es", ",", "was", "ich", "vom"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "PPER", "VVFIN", "APPR", "PPER", "$.", "PDS", "VAFIN", "PPER", "$,", "PWS", "PPER", "APPRART"], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.32": {"text": "L\u00e4ngst schon mit Thr\u00e4nen erbat. Selmar, ich", "tokens": ["L\u00e4ngst", "schon", "mit", "Thr\u00e4\u00b7nen", "er\u00b7bat", ".", "Sel\u00b7mar", ",", "ich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["ADV", "ADV", "APPR", "NN", "VVFIN", "$.", "NE", "$,", "PPER"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.33": {"text": "Ach wie liebest du mich! Sieh diese weinenden Augen!", "tokens": ["Ach", "wie", "lie\u00b7best", "du", "mich", "!", "Sieh", "die\u00b7se", "wei\u00b7nen\u00b7den", "Au\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "VVFIN", "PPER", "PRF", "$.", "NE", "PDAT", "ADJA", "NN", "$."], "meter": "+--+--+--+--+-", "measure": "dactylic.penta"}, "line.34": {"text": "F\u00fchle die\u00df bebende Herz! Selma, wie liebest du mich!", "tokens": ["F\u00fch\u00b7le", "die\u00df", "be\u00b7ben\u00b7de", "Herz", "!", "Sel\u00b7ma", ",", "wie", "lie\u00b7best", "du", "mich", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDS", "ADJA", "NN", "$.", "NE", "$,", "PWAV", "VVFIN", "PPER", "PRF", "$."], "meter": "+---+-+-+-+--+", "measure": "iambic.hexa.chol"}, "line.35": {"text": "Meine Selma, du st\u00fcrbest nach mir? du f\u00fchltest die", "tokens": ["Mei\u00b7ne", "Sel\u00b7ma", ",", "du", "st\u00fcr\u00b7best", "nach", "mir", "?", "du", "f\u00fchl\u00b7test", "die"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NE", "$,", "PPER", "VVFIN", "APPR", "PPER", "$.", "PPER", "VVFIN", "ART"], "meter": "+--+-+--+-+-+", "measure": "iambic.hexa.invert"}, "line.36": {"text": "Da\u00df du sterbend mich s\u00e4hst? Selma, wie liebest", "tokens": ["Da\u00df", "du", "ster\u00b7bend", "mich", "s\u00e4hst", "?", "Sel\u00b7ma", ",", "wie", "lie\u00b7best"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "PPER", "VVFIN", "$.", "NE", "$,", "PWAV", "VVFIN"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.37": {"text": "Ach wenn eine Sprache doch w\u00e4re, dir alles zu sagen,", "tokens": ["Ach", "wenn", "ei\u00b7ne", "Spra\u00b7che", "doch", "w\u00e4\u00b7re", ",", "dir", "al\u00b7les", "zu", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "KOUS", "ART", "NN", "ADV", "VAFIN", "$,", "PPER", "PIS", "PTKZU", "VVINF", "$,"], "meter": "+-+-+--+--+--+-", "measure": "hexameter"}, "line.38": {"text": "Was mein liebendes Herz, meine Selma, dir f\u00fchlt!", "tokens": ["Was", "mein", "lie\u00b7ben\u00b7des", "Herz", ",", "mei\u00b7ne", "Sel\u00b7ma", ",", "dir", "f\u00fchlt", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "ADJA", "NN", "$,", "PPOSAT", "NE", "$,", "PPER", "VVFIN", "$."], "meter": "+-+--+---+-+", "measure": "trochaic.penta.relaxed"}, "line.39": {"text": "W\u00fcrde die\u00df Aug und sein Blick, und seine Z\u00e4hren", "tokens": ["W\u00fcr\u00b7de", "die\u00df", "Aug", "und", "sein", "Blick", ",", "und", "sei\u00b7ne", "Z\u00e4h\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "PDS", "NN", "KON", "PPOSAT", "NN", "$,", "KON", "PPOSAT", "NN"], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.40": {"text": "Und die\u00df Ach des Gef\u00fchls, das mir gebrochen entfloh,", "tokens": ["Und", "die\u00df", "Ach", "des", "Ge\u00b7f\u00fchls", ",", "das", "mir", "ge\u00b7bro\u00b7chen", "ent\u00b7floh", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "NN", "ART", "NN", "$,", "PRELS", "PPER", "VVPP", "VVFIN", "$,"], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}, "line.41": {"text": "Doch zu einer Sprache der G\u00f6tter, dir alles zu sagen,", "tokens": ["Doch", "zu", "ei\u00b7ner", "Spra\u00b7che", "der", "G\u00f6t\u00b7ter", ",", "dir", "al\u00b7les", "zu", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN", "$,", "PPER", "PIS", "PTKZU", "VVINF", "$,"], "meter": "--+-+--+--+--+-", "measure": "iambic.penta.relaxed"}, "line.42": {"text": "Was mein liebendes Herz, meine Selma dir f\u00fchlt.", "tokens": ["Was", "mein", "lie\u00b7ben\u00b7des", "Herz", ",", "mei\u00b7ne", "Sel\u00b7ma", "dir", "f\u00fchlt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "ADJA", "NN", "$,", "PPOSAT", "NN", "PPER", "VVFIN", "$."], "meter": "+-+--+---+-+", "measure": "trochaic.penta.relaxed"}, "line.43": {"text": "Ach, wenn doch kein Grabmal w\u00e4re, das Liebende", "tokens": ["Ach", ",", "wenn", "doch", "kein", "Grab\u00b7mal", "w\u00e4\u00b7re", ",", "das", "Lie\u00b7ben\u00b7de"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ITJ", "$,", "KOUS", "ADV", "PIAT", "NN", "VAFIN", "$,", "ART", "NN"], "meter": "+-+-+-+--+--", "measure": "trochaic.penta.relaxed"}, "line.44": {"text": "Die einander so treu, die so voll Z\u00e4rtlichkeit sind!", "tokens": ["Die", "ein\u00b7an\u00b7der", "so", "treu", ",", "die", "so", "voll", "Z\u00e4rt\u00b7lich\u00b7keit", "sind", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "ADJD", "$,", "PRELS", "ADV", "ADJD", "NN", "VAFIN", "$."], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}, "line.45": {"text": "Aber weil ihr denn seyd, ihr immer offenen Gr\u00e4ber,", "tokens": ["A\u00b7ber", "weil", "ihr", "denn", "seyd", ",", "ihr", "im\u00b7mer", "of\u00b7fe\u00b7nen", "Gr\u00e4\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "VAFIN", "$,", "PPER", "ADV", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+--+-", "measure": "hexameter"}, "line.46": {"text": "Nehmet zum wenigsten doch nehmet auf einmal", "tokens": ["Neh\u00b7met", "zum", "we\u00b7nigs\u00b7ten", "doch", "neh\u00b7met", "auf", "ein\u00b7mal"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPRART", "PIS", "ADV", "VVFIN", "APPR", "ADV"], "meter": "+--+---+-+-+", "measure": "dactylic.di.plus"}, "line.47": {"text": "H\u00f6rest du mich, der zur Liebe mich schuf? Ach! wenn", "tokens": ["H\u00f6\u00b7rest", "du", "mich", ",", "der", "zur", "Lie\u00b7be", "mich", "schuf", "?", "Ach", "!", "wenn"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["VVFIN", "PPER", "PRF", "$,", "PRELS", "APPRART", "NN", "PPER", "VVFIN", "$.", "ITJ", "$.", "KOUS"], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}, "line.48": {"text": "La\u00df mit eben dem Hauch Selma sterben, und", "tokens": ["La\u00df", "mit", "e\u00b7ben", "dem", "Hauch", "Sel\u00b7ma", "ster\u00b7ben", ",", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["VVIMP", "APPR", "ADV", "ART", "NN", "NE", "VVINF", "$,", "KON"], "meter": "+-+--++-+-+", "measure": "trochaic.hexa.relaxed"}, "line.49": {"text": "Selmar, ich sterbe mit dir! Ich bete mit dir von", "tokens": ["Sel\u00b7mar", ",", "ich", "ster\u00b7be", "mit", "dir", "!", "Ich", "be\u00b7te", "mit", "dir", "von"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "PPER", "VVFIN", "APPR", "PPER", "$.", "PPER", "VVFIN", "APPR", "PPER", "APPR"], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.50": {"text": "Diese Wohlthat herab. Selmar, ich sterbe mit", "tokens": ["Die\u00b7se", "Wohlt\u00b7hat", "her\u00b7ab", ".", "Sel\u00b7mar", ",", "ich", "ster\u00b7be", "mit"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PDAT", "NN", "ADV", "$.", "NE", "$,", "PPER", "VVFIN", "APPR"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}}}}}