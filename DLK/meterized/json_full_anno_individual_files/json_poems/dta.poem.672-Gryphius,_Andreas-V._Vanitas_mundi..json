{"dta.poem.672": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "V.  \n Vanitas mundi.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Was ist die Welt/ ", "tokens": ["Was", "ist", "die", "Welt", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Die mich bis her mit jhrer pracht beth\u00f6ret?", "tokens": ["Die", "mich", "bis", "her", "mit", "jhrer", "pracht", "be\u00b7th\u00f6\u00b7ret", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Wie pl\u00f6tzlich felt/", "tokens": ["Wie", "pl\u00f6tz\u00b7lich", "felt", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Was Alt vnd Jung/ vnd Reich vnd Arm geehret!", "tokens": ["Was", "Alt", "vnd", "Jung", "/", "vnd", "Reich", "vnd", "Arm", "ge\u00b7eh\u00b7ret", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "KON", "NN", "$(", "KON", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Was ist doch alles was man alhier find?", "tokens": ["Was", "ist", "doch", "al\u00b7les", "was", "man", "al\u00b7hier", "find", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "PIS", "PWS", "PIS", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Ein leichter wind!", "tokens": ["Ein", "leich\u00b7ter", "wind", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VAFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "2. Was jtzund bl\u00fcht/", "tokens": ["Was", "jt\u00b7zund", "bl\u00fcht", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Kan noch f\u00fcr abend gantz zutretten werden. ", "tokens": ["Kan", "noch", "f\u00fcr", "a\u00b7bend", "gantz", "zu\u00b7tret\u00b7ten", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "APPR", "ADJD", "ADV", "VVFIN", "VAINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der sich hier m\u00fcht", "tokens": ["Der", "sich", "hier", "m\u00fcht"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PRF", "ADV", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Vmb fl\u00fcchtig geld/ mu\u00df ohne geldt zur erden.", "tokens": ["Vmb", "fl\u00fcch\u00b7tig", "geld", "/", "mu\u00df", "oh\u00b7ne", "geldt", "zur", "er\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADJD", "NN", "$(", "VMFIN", "APPR", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Er samlet fleissig (doch f\u00fcr ander) ein.", "tokens": ["Er", "sam\u00b7let", "fleis\u00b7sig", "(", "doch", "f\u00fcr", "an\u00b7der", ")", "ein", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$(", "ADV", "APPR", "ADJD", "$(", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Vnd stirbt allein.", "tokens": ["Vnd", "stirbt", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "3. ", "tokens": [], "token_info": [], "pos": []}, "line.2": {"text": "Das Seiden spint/ verstrickt sich in sein spinnen. ", "tokens": ["Das", "Sei\u00b7den", "spint", "/", "ver\u00b7strickt", "sich", "in", "sein", "spin\u00b7nen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$(", "VVFIN", "PRF", "APPR", "PPOSAT", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So m\u00fcssen wir ", "tokens": ["So", "m\u00fcs\u00b7sen", "wir"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Durch vnsern fleis/ oft vnsern todt gewinnen. ", "tokens": ["Durch", "vn\u00b7sern", "fleis", "/", "oft", "vn\u00b7sern", "todt", "ge\u00b7win\u00b7nen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$(", "ADV", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Viel hat verstandt/ vnd was vns weise macht;", "tokens": ["Viel", "hat", "ver\u00b7standt", "/", "vnd", "was", "vns", "wei\u00b7se", "macht", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "VVPP", "$(", "KON", "PWS", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "In's grab gebracht. ", "tokens": ["In's", "grab", "ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "4. Der Tulipan", "tokens": ["Der", "Tu\u00b7li\u00b7pan"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Wird weil er gl\u00e4ntzt/ von Jungfrawn abgeschnitten/", "tokens": ["Wird", "weil", "er", "gl\u00e4ntzt", "/", "von", "Jung\u00b7frawn", "ab\u00b7ge\u00b7schnit\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "KOUS", "PPER", "VVFIN", "$(", "APPR", "NE", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Schaw Menschen an/", "tokens": ["Schaw", "Men\u00b7schen", "an", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NN", "PTKVZ", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Sie haben schmach/ vmb das sie sch\u00f6n/ erlitten.", "tokens": ["Sie", "ha\u00b7ben", "schmach", "/", "vmb", "das", "sie", "sch\u00f6n", "/", "er\u00b7lit\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "$(", "APPR", "PRELS", "PPER", "ADJD", "$(", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Vnd (wenn sie nicht entsetzt ein schneller todt;)", "tokens": ["Vnd", "(", "wenn", "sie", "nicht", "ent\u00b7setzt", "ein", "schnel\u00b7ler", "todt", ";)"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "emoticon"], "pos": ["KON", "$(", "KOUS", "PPER", "PTKNEG", "VVFIN", "ART", "ADJA", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Ach! angst vnd noht;", "tokens": ["Ach", "!", "angst", "vnd", "noht", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VVPP", "KON", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "5. Bist du bekandt?", "tokens": ["Bist", "du", "be\u00b7kandt", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "So kan dir jeder deine feil\u2019 aufr\u00fccken.", "tokens": ["So", "kan", "dir", "je\u00b7der", "dei\u00b7ne", "feil'", "auf\u00b7r\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PIAT", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wofern dein standt", "tokens": ["Wo\u00b7fern", "dein", "standt"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPOSAT", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Verborgen ligt/ so wirdt dich jeder dr\u00fccken/", "tokens": ["Ver\u00b7bor\u00b7gen", "ligt", "/", "so", "wirdt", "dich", "je\u00b7der", "dr\u00fc\u00b7cken", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$(", "ADV", "VAFIN", "PPER", "PIS", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Wer reich ist wird beneidet vnd verlacht/", "tokens": ["Wer", "reich", "ist", "wird", "be\u00b7nei\u00b7det", "vnd", "ver\u00b7lacht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VAFIN", "VAFIN", "VVPP", "KON", "VVPP", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Wer arm; der schmacht.", "tokens": ["Wer", "arm", ";", "der", "schmacht", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "ADJD", "$.", "ART", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.7": {"line.1": {"text": "6. ", "tokens": [], "token_info": [], "pos": []}, "line.2": {"text": "Ein schifflin wird bald her bald hin geschmissen/", "tokens": ["Ein", "schiff\u00b7lin", "wird", "bald", "her", "bald", "hin", "ge\u00b7schmis\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VAFIN", "ADV", "ADV", "ADV", "ADV", "VVPP", "$("], "meter": "-+---+-+-+-", "measure": "dactylic.init"}, "line.3": {"text": "So setzt vns zu ", "tokens": ["So", "setzt", "vns", "zu"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PTKZU"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Der sorgen sturm/ wir werden hingerissen", "tokens": ["Der", "sor\u00b7gen", "sturm", "/", "wir", "wer\u00b7den", "hin\u00b7ge\u00b7ris\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "$(", "PPER", "VAFIN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Auff dieses lebens schmertzen volle See.", "tokens": ["Auff", "die\u00b7ses", "le\u00b7bens", "schmert\u00b7zen", "vol\u00b7le", "See", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Da eitel weh!", "tokens": ["Da", "ei\u00b7tel", "weh", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.8": {"line.1": {"text": "7. Wie seelig ist", "tokens": ["Wie", "see\u00b7lig", "ist"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "ADJD", "VAFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Wer schaden frey kan an den Port einfahren!", "tokens": ["Wer", "scha\u00b7den", "frey", "kan", "an", "den", "Port", "ein\u00b7fah\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADJD", "VMFIN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wer jhm erkiest/", "tokens": ["Wer", "jhm", "er\u00b7kiest", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Den rechten lauff der G", "tokens": ["Den", "rech\u00b7ten", "lauff", "der", "G"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NE"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der kan/ ob Wellen/ Bergen gleich\u2019 aufstehn:", "tokens": ["Der", "kan", "/", "ob", "Wel\u00b7len", "/", "Ber\u00b7gen", "gleich'", "auf\u00b7stehn", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "$(", "KOUS", "NN", "$(", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Nicht vntergehn!", "tokens": ["Nicht", "vn\u00b7ter\u00b7gehn", "!"], "token_info": ["word", "word", "punct"], "pos": ["PTKNEG", "VVINF", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}