{"textgrid.poem.36503": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "2. Der Esel und der L\u00f6we", "genre": "verse", "period": "N.A.", "pub_year": 1761, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein L\u00f6we ging mit raschem Schritt,", "tokens": ["Ein", "L\u00f6\u00b7we", "ging", "mit", "ra\u00b7schem", "Schritt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auf einen Esel zu!", "tokens": ["Auf", "ei\u00b7nen", "E\u00b7sel", "zu", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Esel, angst und bange, zittert. \u2013 Du,", "tokens": ["Der", "E\u00b7sel", ",", "angst", "und", "ban\u00b7ge", ",", "zit\u00b7tert", ".", "\u2013", "Du", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "VVPP", "KON", "ADJD", "$,", "VVFIN", "$.", "$(", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Spricht K\u00f6nig L\u00f6we, komm! komm mit!", "tokens": ["Spricht", "K\u00f6\u00b7nig", "L\u00f6\u00b7we", ",", "komm", "!", "komm", "mit", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "NN", "NE", "$,", "VVFIN", "$.", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wir wollen jagen; du sollst machen,", "tokens": ["Wir", "wol\u00b7len", "ja\u00b7gen", ";", "du", "sollst", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "$.", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df alles Wild im Wald' erschrickt.", "tokens": ["Da\u00df", "al\u00b7les", "Wild", "im", "Wald'", "er\u00b7schrickt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADJD", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Herr L\u00f6w', ich f\u00fcrchte mich vor z\u00e4hnevollen Rachen,", "tokens": ["Herr", "L\u00f6\u00b7w'", ",", "ich", "f\u00fcrch\u00b7te", "mich", "vor", "z\u00e4h\u00b7ne\u00b7vol\u00b7len", "Ra\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "PPER", "VVFIN", "PRF", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.2": {"text": "Sagt da der Esel, tief geb\u00fcckt,", "tokens": ["Sagt", "da", "der", "E\u00b7sel", ",", "tief", "ge\u00b7b\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$,", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u00bbschrei! Esel, schrei!\u00ab \u2013 Um Gotteswillen,", "tokens": ["\u00bb", "schrei", "!", "E\u00b7sel", ",", "schrei", "!", "\u00ab", "\u2013", "Um", "Got\u00b7tes\u00b7wil\u00b7len", ","], "token_info": ["punct", "word", "punct", "word", "punct", "word", "punct", "punct", "punct", "word", "word", "punct"], "pos": ["$(", "ADJD", "$.", "NN", "$,", "ADJD", "$.", "$(", "$(", "KOUI", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Herr L\u00f6we, sie geruhen doch zu br\u00fcllen,", "tokens": ["Herr", "L\u00f6\u00b7we", ",", "sie", "ge\u00b7ru\u00b7hen", "doch", "zu", "br\u00fcl\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos