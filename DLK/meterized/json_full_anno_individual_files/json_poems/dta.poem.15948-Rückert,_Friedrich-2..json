{"dta.poem.15948": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Zum Festtisch soll man Aufgew\u00e4rmtes nicht auftischen,", "tokens": ["Zum", "Fest\u00b7tisch", "soll", "man", "Auf\u00b7ge\u00b7w\u00e4rm\u00b7tes", "nicht", "auf\u00b7ti\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VMFIN", "PIS", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+--+--", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Mit frischer Speise soll man frische G\u00e4st' erfrischen.", "tokens": ["Mit", "fri\u00b7scher", "Spei\u00b7se", "soll", "man", "fri\u00b7sche", "G\u00e4st'", "er\u00b7fri\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VMFIN", "PIS", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Doch aufgew\u00e4rmt ist nicht, was von der Vorzeit Tisch", "tokens": ["Doch", "auf\u00b7ge\u00b7w\u00e4rmt", "ist", "nicht", ",", "was", "von", "der", "Vor\u00b7zeit", "Tisch"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVPP", "VAFIN", "PTKNEG", "$,", "PRELS", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Uns zukam; immer bleibt die Paradisfrucht frisch.", "tokens": ["Uns", "zu\u00b7kam", ";", "im\u00b7mer", "bleibt", "die", "Pa\u00b7ra\u00b7dis\u00b7frucht", "frisch", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "ADV", "VVFIN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+