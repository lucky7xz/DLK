{"dta.poem.21228": {"metadata": {"author": {"name": "Czepko, Daniel von", "birth": "N.A.", "death": "N.A."}, "title": "ViII.  \n  An das Pferd/.  \n Das Jhre May:  \n Vor N\u00f6rdlinger Schlacht gerieten.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-20871-8", "language": ["de:0.99"], "booktitle": "Czepko, Daniel von: Triumph Bogen. Breslau, 1641."}, "poem": {"stanza.1": {"line.1": {"text": "Pferd/ voller Hertz vnd Muth! Obgleich die Erd erschrickt/", "tokens": ["Pferd", "/", "vol\u00b7ler", "Hertz", "vnd", "Muth", "!", "Ob\u00b7gleich", "die", "Erd", "er\u00b7schrickt", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "ADJA", "NN", "KON", "NN", "$.", "KOUS", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wann du die Schenckel hebst/ doch wiltu tewrer bieten/", "tokens": ["Wann", "du", "die", "Schen\u00b7ckel", "hebst", "/", "doch", "wil\u00b7tu", "tew\u00b7rer", "bie\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "VVFIN", "$(", "ADV", "VMFIN", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Als bald dich/ ", "tokens": ["Als", "bald", "dich", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPER", "$("], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "Dann schaw du f\u00fchlest auch die ", "tokens": ["Dann", "schaw", "du", "f\u00fch\u00b7lest", "auch", "die"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "VVFIN", "ADV", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ist je der Feind so keck/ da\u00df er jetzt d\u00fcrffte stehn/", "tokens": ["Ist", "je", "der", "Feind", "so", "keck", "/", "da\u00df", "er", "jetzt", "d\u00fcrff\u00b7te", "stehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "ADV", "ADJD", "$(", "KOUS", "PPER", "ADV", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So trag den ", "tokens": ["So", "trag", "den"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VVFIN", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Dann bleibt ER vnverletzt/ so m\u00fcssen wir erkennen/", "tokens": ["Dann", "bleibt", "Er", "vn\u00b7ver\u00b7letzt", "/", "so", "m\u00fcs\u00b7sen", "wir", "er\u00b7ken\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "$(", "ADV", "VMFIN", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da\u00df es vns noch einmahl wird wol auff Erden gehn.", "tokens": ["Da\u00df", "es", "vns", "noch", "ein\u00b7mahl", "wird", "wol", "auff", "Er\u00b7den", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "ADV", "VAFIN", "ADV", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}