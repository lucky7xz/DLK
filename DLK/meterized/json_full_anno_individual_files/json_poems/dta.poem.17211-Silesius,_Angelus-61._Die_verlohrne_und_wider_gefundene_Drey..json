{"dta.poem.17211": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "61.  Die verlohrne und wider gefundene  \n Drey.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Der Groschen/ Sohn/ unds Schaaff/ bin ich mit Geist/", "tokens": ["Der", "Gro\u00b7schen", "/", "Sohn", "/", "unds", "Schaaff", "/", "bin", "ich", "mit", "Geist", "/"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "NN", "$(", "KON", "NN", "$(", "VAFIN", "PPER", "APPR", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Leib/ Seele.", "tokens": ["Leib", "/", "See\u00b7le", "."], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$(", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Verlohrn in frembdem Land/ in einer W\u00fcst\u2019 und H\u00f6le.", "tokens": ["Ver\u00b7lohrn", "in", "fremb\u00b7dem", "Land", "/", "in", "ei\u00b7ner", "W\u00fcst'", "und", "H\u00f6\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$(", "APPR", "ART", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die heilge Dreyfalt kombt und sucht mich alle stunden:", "tokens": ["Die", "heil\u00b7ge", "Drey\u00b7falt", "kombt", "und", "sucht", "mich", "al\u00b7le", "stun\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "KON", "VVFIN", "PPER", "PIAT", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Den Groschen findt der Geist/ der Vatter nimbt den", "tokens": ["Den", "Gro\u00b7schen", "findt", "der", "Geist", "/", "der", "Vat\u00b7ter", "nimbt", "den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$(", "ART", "NN", "VVFIN", "ART"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Sohn/", "tokens": ["Sohn", "/"], "token_info": ["word", "punct"], "pos": ["NN", "$("], "meter": "+", "measure": "single.up"}, "line.7": {"text": "Der Hirte JEsu tr\u00e4gt das Schaaff mit sich davon.", "tokens": ["Der", "Hir\u00b7te", "Je\u00b7su", "tr\u00e4gt", "das", "Schaaff", "mit", "sich", "da\u00b7von", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "VVFIN", "ART", "NN", "APPR", "PRF", "PAV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Schau wie ich Dreyfach bin verlohren und gefunden!", "tokens": ["Schau", "wie", "ich", "Drey\u00b7fach", "bin", "ver\u00b7loh\u00b7ren", "und", "ge\u00b7fun\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "PPER", "PAV", "VAFIN", "VVPP", "KON", "VVPP", "$."], "meter": "++-+-+-+-+-+-", "measure": "unknown.measure.septa"}}}}}