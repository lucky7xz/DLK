{"dta.poem.10081": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Welt-Opera.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20086-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "In der sch\u00f6nsten Opera schlieff Chrysander j\u00fcngst. Ich", "tokens": ["In", "der", "sch\u00f6ns\u00b7ten", "O\u00b7pe\u00b7ra", "schlieff", "Chry\u00b7san\u00b7der", "j\u00fcngst", ".", "Ich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "NE", "ADV", "$.", "PPER"], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}, "line.2": {"text": "lachte.", "tokens": ["lach\u00b7te", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Als ich aber bald darauf die\u00df von neuen \u00fcberdachte,", "tokens": ["Als", "ich", "a\u00b7ber", "bald", "da\u00b7rauf", "die\u00df", "von", "neu\u00b7en", "\u00fc\u00b7berd\u00b7ach\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "PAV", "PDS", "APPR", "ADJA", "VVFIN", "$,"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Lacht\u2019 ich ferner nicht dar\u00fcber, sondern ich betr\u00fcbte mich:", "tokens": ["Lacht'", "ich", "fer\u00b7ner", "nicht", "da\u00b7r\u00fc\u00b7ber", ",", "son\u00b7dern", "ich", "be\u00b7tr\u00fcb\u00b7te", "mich", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PTKNEG", "PAV", "$,", "KON", "PPER", "VVFIN", "PPER", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.5": {"text": "Denn es fiel mir dieses bey: ", "tokens": ["Denn", "es", "fiel", "mir", "die\u00b7ses", "bey", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "PDS", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Die im Schau-Platz dieser Welt, wo am Himmel, wo auf", "tokens": ["Die", "im", "Schau\u00b7Platz", "die\u00b7ser", "Welt", ",", "wo", "am", "Him\u00b7mel", ",", "wo", "auf"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "APPRART", "NN", "PDAT", "NN", "$,", "PWAV", "APPRART", "NN", "$,", "PWAV", "APPR"], "meter": "--+---+--+--+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Erden", "tokens": ["Er\u00b7den"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.8": {"text": "Jmmer Wunder \u00fcber Wunder pr\u00e4chtigst vorgestellet", "tokens": ["Jm\u00b7mer", "Wun\u00b7der", "\u00fc\u00b7ber", "Wun\u00b7der", "pr\u00e4ch\u00b7tigst", "vor\u00b7ge\u00b7stel\u00b7let"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "NN", "APPR", "NN", "ADJD", "VVFIN"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.9": {"text": "werden,", "tokens": ["wer\u00b7den", ","], "token_info": ["word", "punct"], "pos": ["VAINF", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.10": {"text": "Und die uns sowol zum Nutz, als zu unsrer Freud' er-", "tokens": ["Und", "die", "uns", "so\u00b7wol", "zum", "Nutz", ",", "als", "zu", "uns\u00b7rer", "Freud'", "er"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "PPER", "ADV", "APPRART", "NN", "$,", "KOUS", "APPR", "PPOSAT", "NN", "TRUNC"], "meter": "--+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.11": {"text": "schaffen,", "tokens": ["schaf\u00b7fen", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.12": {"text": "Da sie selbe nicht betrachten, eigentlich nichts thun, als", "tokens": ["Da", "sie", "sel\u00b7be", "nicht", "be\u00b7trach\u00b7ten", ",", "ei\u00b7gent\u00b7lich", "nichts", "thun", ",", "als"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["KOUS", "PPER", "VVFIN", "PTKNEG", "VVINF", "$,", "ADV", "PIS", "VVINF", "$,", "KOUS"], "meter": "--+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.13": {"text": "schlaffen!", "tokens": ["schlaf\u00b7fen", "!"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}}}}}