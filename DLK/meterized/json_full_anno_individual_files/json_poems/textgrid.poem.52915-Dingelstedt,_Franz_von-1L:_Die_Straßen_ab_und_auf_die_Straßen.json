{"textgrid.poem.52915": {"metadata": {"author": {"name": "Dingelstedt, Franz von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Stra\u00dfen ab und auf die Stra\u00dfen", "genre": "verse", "period": "N.A.", "pub_year": 1847, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Stra\u00dfen ab und auf die Stra\u00dfen", "tokens": ["Die", "Stra\u00b7\u00dfen", "ab", "und", "auf", "die", "Stra\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PTKVZ", "KON", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Geht der Soldaten-Zapfenstreich,", "tokens": ["Geht", "der", "Sol\u00b7da\u00b7ten\u00b7Zap\u00b7fen\u00b7streich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Trommel rasselt, H\u00f6rner blasen:", "tokens": ["Die", "Trom\u00b7mel", "ras\u00b7selt", ",", "H\u00f6r\u00b7ner", "bla\u00b7sen", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie lau die Nacht, wie warm, wie weich!", "tokens": ["Wie", "lau", "die", "Nacht", ",", "wie", "warm", ",", "wie", "weich", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "$,", "PWAV", "ADJD", "$,", "PWAV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Horch! H\u00f6her schwillt der T\u00f6ne Wogen,", "tokens": ["Horch", "!", "H\u00f6\u00b7her", "schwillt", "der", "T\u00f6\u00b7ne", "Wo\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Gewiegt auf linder Weste Schwing',", "tokens": ["Ge\u00b7wiegt", "auf", "lin\u00b7der", "Wes\u00b7te", "Schwing'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und majest\u00e4tisch lang-gezogen", "tokens": ["Und", "ma\u00b7jes\u00b7t\u00e4\u00b7tisch", "lang\u00b7ge\u00b7zo\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJD", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Steigts auf zum dunklen Himmelsbogen:", "tokens": ["Steigts", "auf", "zum", "dunk\u00b7len", "Him\u00b7mels\u00b7bo\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Dort sitzt er, dem die T\u00f6ne rufen,", "tokens": ["Dort", "sitzt", "er", ",", "dem", "die", "T\u00f6\u00b7ne", "ru\u00b7fen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PRELS", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Beim Mahl im marmornen Palast,", "tokens": ["Beim", "Mahl", "im", "mar\u00b7mor\u00b7nen", "Pa\u00b7last", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+---+", "measure": "unknown.measure.tri"}, "line.3": {"text": "Es hat des Thrones hohe Stufen", "tokens": ["Es", "hat", "des", "Thro\u00b7nes", "ho\u00b7he", "Stu\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Schar der Gro\u00dfen eingefa\u00dft;", "tokens": ["Die", "Schar", "der", "Gro\u00b7\u00dfen", "ein\u00b7ge\u00b7fa\u00dft", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wer z\u00e4hlt, wie oft im Speisesaale", "tokens": ["Wer", "z\u00e4hlt", ",", "wie", "oft", "im", "Spei\u00b7se\u00b7saa\u00b7le"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "$,", "PWAV", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der Becher schon die Runde ging,", "tokens": ["Der", "Be\u00b7cher", "schon", "die", "Run\u00b7de", "ging", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Indes der Chor an dem Portale", "tokens": ["In\u00b7des", "der", "Chor", "an", "dem", "Por\u00b7ta\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Vergeblich rief so viele Male:", "tokens": ["Ver\u00b7geb\u00b7lich", "rief", "so", "vie\u00b7le", "Ma\u00b7le", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und als ein Ton hinaufgeklungen", "tokens": ["Und", "als", "ein", "Ton", "hin\u00b7auf\u00b7ge\u00b7klun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zum Platz, wo er gesessen war,", "tokens": ["Zum", "Platz", ",", "wo", "er", "ge\u00b7ses\u00b7sen", "war", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PWAV", "PPER", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da hat er hoch sein Glas geschwungen", "tokens": ["Da", "hat", "er", "hoch", "sein", "Glas", "ge\u00b7schwun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "PPOSAT", "NN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und ausgerufen trotzig-klar:", "tokens": ["Und", "aus\u00b7ge\u00b7ru\u00b7fen", "trot\u00b7zig\u00b7klar", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da habt Ihr meines Satzes Probe:", "tokens": ["Da", "habt", "Ihr", "mei\u00b7nes", "Sat\u00b7zes", "Pro\u00b7be", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ein deutsches Volk ein gutes Ding;", "tokens": ["Ein", "deut\u00b7sches", "Volk", "ein", "gu\u00b7tes", "Ding", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Am Morgen Aufruhr und Getobe,", "tokens": ["Am", "Mor\u00b7gen", "Auf\u00b7ruhr", "und", "Ge\u00b7to\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und abends, mir und ihm zum Lobe,", "tokens": ["Und", "a\u00b7bends", ",", "mir", "und", "ihm", "zum", "Lo\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "PPER", "KON", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Er sprach's und lachte, da\u00df es dr\u00f6hnte", "tokens": ["Er", "sprach's", "und", "lach\u00b7te", ",", "da\u00df", "es", "dr\u00f6hn\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "$,", "KOUS", "PPER", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und sch\u00fcttelte den wei\u00dfen Bart,", "tokens": ["Und", "sch\u00fct\u00b7tel\u00b7te", "den", "wei\u00b7\u00dfen", "Bart", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das Heer der Schranzen lacht' und h\u00f6hnte", "tokens": ["Das", "Heer", "der", "Schran\u00b7zen", "lacht'", "und", "h\u00f6hn\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dem Herren nach, wie Schranzen Art;", "tokens": ["Dem", "Her\u00b7ren", "nach", ",", "wie", "Schran\u00b7zen", "Art", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,", "PWAV", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Doch drau\u00dfen schwiegen just die Kl\u00e4nge,", "tokens": ["Doch", "drau\u00b7\u00dfen", "schwie\u00b7gen", "just", "die", "Kl\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Sobald er an zu reden fing,", "tokens": ["So\u00b7bald", "er", "an", "zu", "re\u00b7den", "fing", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKVZ", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Lautlos verlief sich das Gedr\u00e4nge,", "tokens": ["Laut\u00b7los", "ver\u00b7lief", "sich", "das", "Ge\u00b7dr\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.8": {"text": "Und keiner sang mehr aus der Menge:", "tokens": ["Und", "kei\u00b7ner", "sang", "mehr", "aus", "der", "Men\u00b7ge", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Da schauerte ein pl\u00f6tzlich Schweigen", "tokens": ["Da", "schau\u00b7er\u00b7te", "ein", "pl\u00f6tz\u00b7lich", "Schwei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und Totenstille durch den Saal,", "tokens": ["Und", "To\u00b7tens\u00b7til\u00b7le", "durch", "den", "Saal", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein kahles Haupt sah man sich neigen,", "tokens": ["Ein", "kah\u00b7les", "Haupt", "sah", "man", "sich", "nei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PIS", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und manche Wange wurde fahl.", "tokens": ["Und", "man\u00b7che", "Wan\u00b7ge", "wur\u00b7de", "fahl", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der blinde Knabe nur im Kreise,", "tokens": ["Der", "blin\u00b7de", "Kna\u00b7be", "nur", "im", "Krei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "In dessen Aug' ein Tropfen hing,", "tokens": ["In", "des\u00b7sen", "Aug'", "ein", "Trop\u00b7fen", "hing", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Stand auf und schritt zum Fenster leise", "tokens": ["Stand", "auf", "und", "schritt", "zum", "Fens\u00b7ter", "lei\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "PTKVZ", "KON", "VVFIN", "APPRART", "NN", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und flisterte f\u00fcr sich die Weise:", "tokens": ["Und", "flis\u00b7ter\u00b7te", "f\u00fcr", "sich", "die", "Wei\u00b7se", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PRF", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Die Stra\u00dfen ab und auf die Stra\u00dfen", "tokens": ["Die", "Stra\u00b7\u00dfen", "ab", "und", "auf", "die", "Stra\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PTKVZ", "KON", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Geht der Soldaten-Zapfenstreich,", "tokens": ["Geht", "der", "Sol\u00b7da\u00b7ten\u00b7Zap\u00b7fen\u00b7streich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text":