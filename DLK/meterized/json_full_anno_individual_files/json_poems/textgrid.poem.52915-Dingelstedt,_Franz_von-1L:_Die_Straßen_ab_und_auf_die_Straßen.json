ale", "tokens": ["In\u00b7des", "der", "Chor", "an", "dem", "Por\u00b7ta\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Vergeblich rief so viele Male:", "tokens": ["Ver\u00b7geb\u00b7lich", "rief", "so", "vie\u00b7le", "Ma\u00b7le", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Und als ein Ton hinaufgeklungen", "tokens": ["Und", "als", "ein", "Ton", "hin\u00b7auf\u00b7ge\u00b7klun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zum Platz, wo er gesessen war,", "tokens": ["Zum", "Platz", ",", "wo", "er", "ge\u00b7ses\u00b7sen", "war", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PWAV", "PPER", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da hat er hoch sein Glas geschwungen", "tokens": ["Da", "hat", "er", "hoch", "sein", "Glas", "ge\u00b7schwun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "PPOSAT", "NN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und ausgerufen trotzig-klar:", "tokens": ["Und", "aus\u00b7ge\u00b7ru\u00b7fen", "trot\u00b7zig\u00b7klar", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da habt Ihr meines Satzes Probe:", "tokens": ["Da", "habt", "Ihr", "mei\u00b7nes", "Sat\u00b7zes", "Pro\u00b7be", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punc