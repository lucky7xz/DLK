{"dta.poem.2116": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Neue Betrachtungen \u00fcber die in der  \n Welt vorhandene Vortrefflich-  \n keiten.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.57", "sv:0.42"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenns m\u00f6glich w\u00e4re, jungen Seelen,", "tokens": ["Wenns", "m\u00f6g\u00b7lich", "w\u00e4\u00b7re", ",", "jun\u00b7gen", "See\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "VAFIN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Noch ehe sie gebohren werden,", "tokens": ["Noch", "e\u00b7he", "sie", "ge\u00b7boh\u00b7ren", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Von aller Sch\u00f6nheit unsrer Erden,", "tokens": ["Von", "al\u00b7ler", "Sch\u00f6n\u00b7heit", "uns\u00b7rer", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Was vorzustellen, zu erz\u00e4hlen;", "tokens": ["Was", "vor\u00b7zu\u00b7stel\u00b7len", ",", "zu", "er\u00b7z\u00e4h\u00b7len", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VVIZU", "$,", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Man g\u00e4be selbigen Bericht", "tokens": ["Man", "g\u00e4\u00b7be", "sel\u00b7bi\u00b7gen", "Be\u00b7richt"], "token_info": ["word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Von Sternen, Mond und Sonnenlicht,", "tokens": ["Von", "Ster\u00b7nen", ",", "Mond", "und", "Son\u00b7nen\u00b7licht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Vom Wechsel zwischen Tag und Nacht,", "tokens": ["Vom", "Wech\u00b7sel", "zwi\u00b7schen", "Tag", "und", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Von der bebl\u00fcmten G\u00e4rten Pracht,", "tokens": ["Von", "der", "be\u00b7bl\u00fcm\u00b7ten", "G\u00e4r\u00b7ten", "Pracht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Von bl\u00e4ttervollen, schattenreichen, und angenehmen k\u00fch-", "tokens": ["Von", "bl\u00e4t\u00b7ter\u00b7vol\u00b7len", ",", "schat\u00b7ten\u00b7rei\u00b7chen", ",", "und", "an\u00b7ge\u00b7neh\u00b7men", "k\u00fch"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["APPR", "VVINF", "$,", "VVFIN", "$,", "KON", "ADJA", "TRUNC"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.10": {"text": "len W\u00e4ldern", "tokens": ["len", "W\u00e4l\u00b7dern"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.11": {"text": "Mit Singev\u00f6geln angef\u00fcllt, von angestralten gr\u00fcnen", "tokens": ["Mit", "Sin\u00b7ge\u00b7v\u00f6\u00b7geln", "an\u00b7ge\u00b7f\u00fcllt", ",", "von", "an\u00b7ge\u00b7stral\u00b7ten", "gr\u00fc\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "VVPP", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.12": {"text": "Feldern", "tokens": ["Fel\u00b7dern"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.13": {"text": "Durchwirkt mit tausendf\u00e4rbgen Blumen, von diaman-", "tokens": ["Durch\u00b7wirkt", "mit", "tau\u00b7send\u00b7f\u00e4rb\u00b7gen", "Blu\u00b7men", ",", "von", "dia\u00b7man"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$,", "APPR", "TRUNC"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.14": {"text": "tengleichem Thau", "tokens": ["ten\u00b7glei\u00b7chem", "Thau"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.15": {"text": "Fr\u00fch angefrischt, geschm\u00fcckt, beperlet. Durch deren", "tokens": ["Fr\u00fch", "an\u00b7ge\u00b7frischt", ",", "ge\u00b7schm\u00fcckt", ",", "be\u00b7per\u00b7let", ".", "Durch", "de\u00b7ren"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word"], "pos": ["ADJD", "VVPP", "$,", "VVPP", "$,", "VVFIN", "$.", "APPR", "PDS"], "meter": "++-+-+-+-+-+", "measure": "unknown.measure.septa"}, "line.16": {"text": "kr\u00e4uterreiche Fl\u00e4che", "tokens": ["kr\u00e4u\u00b7ter\u00b7rei\u00b7che", "Fl\u00e4\u00b7che"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.17": {"text": "Sich flie\u00dfende Crystallen schl\u00e4ngeln und klare silbergleiche", "tokens": ["Sich", "flie\u00b7\u00dfen\u00b7de", "Crys\u00b7tal\u00b7len", "schl\u00e4n\u00b7geln", "und", "kla\u00b7re", "sil\u00b7berg\u00b7lei\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PRF", "ADJA", "NN", "VVINF", "KON", "ADJA", "NN"], "meter": "-+--++-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.18": {"text": "B\u00e4che", "tokens": ["B\u00e4\u00b7che"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.19": {"text": "Bald von der Sonnen Stral gef\u00e4rbt, bald durch der", "tokens": ["Bald", "von", "der", "Son\u00b7nen", "Stral", "ge\u00b7f\u00e4rbt", ",", "bald", "durch", "der"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "NN", "VVPP", "$,", "ADV", "APPR", "ART"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Luft sapphirnes Blau,", "tokens": ["Luft", "sap\u00b7phir\u00b7nes", "Blau", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.21": {"text": "Bald vom smaragdengleichem Gr\u00fcnen beb\u00fcscht- und dick-", "tokens": ["Bald", "vom", "sma\u00b7rag\u00b7den\u00b7glei\u00b7chem", "Gr\u00fc\u00b7nen", "be\u00b7b\u00fcscht", "und", "dick"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "ADJA", "NN", "TRUNC", "KON", "TRUNC"], "meter": "--+--+-+--+-+", "measure": "anapaest.di.plus"}, "line.22": {"text": "begraster H\u00fcgel;", "tokens": ["be\u00b7gras\u00b7ter", "H\u00fc\u00b7gel", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.23": {"text": "W\u00fcrd\u2019 ihnen ferner von der Welt", "tokens": ["W\u00fcrd'", "ih\u00b7nen", "fer\u00b7ner", "von", "der", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Die auf- und untergehnde Sonne, der Zeiten Wechsel,", "tokens": ["Die", "auf", "und", "un\u00b7ter\u00b7gehn\u00b7de", "Son\u00b7ne", ",", "der", "Zei\u00b7ten", "Wech\u00b7sel", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "TRUNC", "KON", "ADJA", "NN", "$,", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.25": {"text": "nebst den Fr\u00fcchten,", "tokens": ["nebst", "den", "Fr\u00fcch\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}}}}}