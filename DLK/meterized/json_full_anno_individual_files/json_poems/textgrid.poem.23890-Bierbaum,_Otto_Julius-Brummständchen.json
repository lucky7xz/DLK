{"textgrid.poem.23890": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Brummst\u00e4ndchen", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "H\u00e4tt ich Geld, ich w\u00fc\u00dfte wohl,", "tokens": ["H\u00e4tt", "ich", "Geld", ",", "ich", "w\u00fc\u00df\u00b7te", "wohl", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "$,", "PPER", "VVFIN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Was ich th\u00e4t, genau:", "tokens": ["Was", "ich", "th\u00e4t", ",", "ge\u00b7nau", ":"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "H\u00e4tt ich Geld, ich n\u00e4hme dich", "tokens": ["H\u00e4tt", "ich", "Geld", ",", "ich", "n\u00e4h\u00b7me", "dich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "NN", "$,", "PPER", "VVFIN", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Augenblicks zur Frau,", "tokens": ["Au\u00b7gen\u00b7blicks", "zur", "Frau", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "N\u00e4hme dich und schleppte dich", "tokens": ["N\u00e4h\u00b7me", "dich", "und", "schlepp\u00b7te", "dich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "KON", "VVFIN", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "In den Liebesbau,", "tokens": ["In", "den", "Lie\u00b7bes\u00b7bau", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "+-+-+", "measure": "trocha