{"textgrid.poem.39363": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nimm die\u00df Gedicht, gewebt aus Lieb' und Leiden,", "genre": "verse", "period": "N.A.", "pub_year": 1797, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nimm die\u00df Gedicht, gewebt aus Lieb' und Leiden,", "tokens": ["Nimm", "die\u00df", "Ge\u00b7dicht", ",", "ge\u00b7webt", "aus", "Lieb'", "und", "Lei\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PDS", "NN", "$,", "VVPP", "APPR", "NN", "KON", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Und dr\u00fcck' es sanft an deine zarte Brust.", "tokens": ["Und", "dr\u00fcck", "es", "sanft", "an", "dei\u00b7ne", "zar\u00b7te", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Was ", "tokens": ["Was"], "token_info": ["word"], "pos": ["PWS"], "meter": "-", "measure": "single.down"}, "line.4": {"text": "Was ", "tokens": ["Was"], "token_info": ["word"], "pos": ["PWS"], "meter": "-", "measure": "single.down"}, "line.5": {"text": "Ungl\u00fccklich Paar! und dennoch zu beneiden;", "tokens": ["Un\u00b7gl\u00fcck\u00b7lich", "Paar", "!", "und", "den\u00b7noch", "zu", "be\u00b7nei\u00b7den", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "$.", "KON", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Sie kannten ja des Daseins h\u00f6chste Lust.", "tokens": ["Sie", "kann\u00b7ten", "ja", "des", "Da\u00b7seins", "h\u00f6chs\u00b7te", "Lust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "La\u00df s\u00fc\u00df und bitter denn uns Thr\u00e4nen mischen,", "tokens": ["La\u00df", "s\u00fc\u00df", "und", "bit\u00b7ter", "denn", "uns", "Thr\u00e4\u00b7nen", "mi\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADJD", "KON", "ADJD", "KON", "PPER", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und mit dem Thau der Treuen Grab erfrischen.", "tokens": ["Und", "mit", "dem", "Thau", "der", "Treu\u00b7en", "Grab", "er\u00b7fri\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Den Sterblichen ward nur ein fl\u00fcchtig Leben:", "tokens": ["Den", "Sterb\u00b7li\u00b7chen", "ward", "nur", "ein", "fl\u00fcch\u00b7tig", "Le\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ART", "ADJD", "NN", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Die\u00df fl\u00fccht'ge Leben, welch ein matter Traum!", "tokens": ["Die\u00df", "fl\u00fccht'\u00b7ge", "Le\u00b7ben", ",", "welch", "ein", "mat\u00b7ter", "Traum", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "NN", "$,", "PWAT", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sie tappen, auch bei ihrem k\u00fchnsten Streben,", "tokens": ["Sie", "tap\u00b7pen", ",", "auch", "bei", "ih\u00b7rem", "k\u00fchns\u00b7ten", "Stre\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADV", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Im Dunkel hin, und kennen selbst sich kaum.", "tokens": ["Im", "Dun\u00b7kel", "hin", ",", "und", "ken\u00b7nen", "selbst", "sich", "kaum", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "$,", "KON", "VVFIN", "ADV", "PRF", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Das Schicksal mag sie dr\u00fccken oder heben:", "tokens": ["Das", "Schick\u00b7sal", "mag", "sie", "dr\u00fc\u00b7cken", "o\u00b7der", "he\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "VVINF", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wo findet ein unendlich Sehnen Raum?", "tokens": ["Wo", "fin\u00b7det", "ein", "un\u00b7end\u00b7lich", "Seh\u00b7nen", "Raum", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "ADJD", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Nur Liebe kann den Erdenstaub befl\u00fcgeln,", "tokens": ["Nur", "Lie\u00b7be", "kann", "den", "Er\u00b7dens\u00b7taub", "be\u00b7fl\u00fc\u00b7geln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Nur sie allein der Himmel Thor entsiegeln.", "tokens": ["Nur", "sie", "al\u00b7lein", "der", "Him\u00b7mel", "Thor", "ent\u00b7sie\u00b7geln", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ADV", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und ach! sie selbst, die K\u00f6nigin der Seelen,", "tokens": ["Und", "ach", "!", "sie", "selbst", ",", "die", "K\u00f6\u00b7ni\u00b7gin", "der", "See\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$.", "PPER", "ADV", "$,", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie oft erf\u00e4hrt sie des Geschickes Neid!", "tokens": ["Wie", "oft", "er\u00b7f\u00e4hrt", "sie", "des", "Ge\u00b7schi\u00b7ckes", "Neid", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VVFIN", "PPER", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Manch liebend Paar zu trennen und zu qu\u00e4len", "tokens": ["Manch", "lie\u00b7bend", "Paar", "zu", "tren\u00b7nen", "und", "zu", "qu\u00e4\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "ADJD", "NN", "PTKZU", "VVINF", "KON", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ist Ha\u00df und Stolz verschworen und bereit.", "tokens": ["Ist", "Ha\u00df", "und", "Stolz", "ver\u00b7schwo\u00b7ren", "und", "be\u00b7reit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "KON", "NN", "VVINF", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Sie m\u00fc\u00dfen schlau die Augenblicke stehlen,", "tokens": ["Sie", "m\u00fc\u00b7\u00dfen", "schlau", "die", "Au\u00b7gen\u00b7bli\u00b7cke", "steh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADJD", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und wachsam lauschen in der Trunkenheit,", "tokens": ["Und", "wach\u00b7sam", "lau\u00b7schen", "in", "der", "Trun\u00b7ken\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVINF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und, wie auf wilder Well' in Ungewittern,", "tokens": ["Und", ",", "wie", "auf", "wil\u00b7der", "Well'", "in", "Un\u00b7ge\u00b7wit\u00b7tern", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PWAV", "APPR", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Vor Todesangst und G\u00f6tterwonne zittern.", "tokens": ["Vor", "To\u00b7de\u00b7sangst", "und", "G\u00f6t\u00b7ter\u00b7won\u00b7ne", "zit\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Doch der Gefahr kann Zagheit nur erliegen,", "tokens": ["Doch", "der", "Ge\u00b7fahr", "kann", "Zag\u00b7heit", "nur", "er\u00b7lie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VMFIN", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Liebe Muth erschwillt, je mehr sie droht.", "tokens": ["Der", "Lie\u00b7be", "Muth", "er\u00b7schwillt", ",", "je", "mehr", "sie", "droht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVPP", "$,", "ADV", "ADV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sich innig fest an den Geliebten schmiegen,", "tokens": ["Sich", "in\u00b7nig", "fest", "an", "den", "Ge\u00b7lieb\u00b7ten", "schmie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "ADJD", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sonst kennt sie keine Zuflucht in der Noth.", "tokens": ["Sonst", "kennt", "sie", "kei\u00b7ne", "Zu\u00b7flucht", "in", "der", "Noth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Entschlo\u00dfen sterben, oder gl\u00fccklich siegen", "tokens": ["Ent\u00b7schlo\u00b7\u00dfen", "ster\u00b7ben", ",", "o\u00b7der", "gl\u00fcck\u00b7lich", "sie\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVINF", "$,", "KON", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ist ihr das erste heiligste Gebot.", "tokens": ["Ist", "ihr", "das", "ers\u00b7te", "hei\u00b7ligs\u00b7te", "Ge\u00b7bot", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Sie f\u00fchlt, vereint, noch frei sich in den Ketten,", "tokens": ["Sie", "f\u00fchlt", ",", "ver\u00b7eint", ",", "noch", "frei", "sich", "in", "den", "Ket\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVPP", "$,", "ADV", "ADJD", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und schaudert nicht bei Todten sich zu betten.", "tokens": ["Und", "schau\u00b7dert", "nicht", "bei", "Tod\u00b7ten", "sich", "zu", "bet\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "APPR", "NN", "PRF", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Ach! schlimmer droh'n ihr l\u00e4chelnde Gefahren,", "tokens": ["Ach", "!", "schlim\u00b7mer", "droh'n", "ihr", "l\u00e4\u00b7cheln\u00b7de", "Ge\u00b7fah\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "ADJD", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wenn sie des Zufalls T\u00fccken \u00fcberwand.", "tokens": ["Wenn", "sie", "des", "Zu\u00b7falls", "T\u00fc\u00b7cken", "\u00fc\u00b7ber\u00b7wand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Verg\u00e4nglichkeit mu\u00df jede Bl\u00fcth' erfahren:", "tokens": ["Ver\u00b7g\u00e4ng\u00b7lich\u00b7keit", "mu\u00df", "je\u00b7de", "Bl\u00fcth'", "er\u00b7fah\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Hat aller Bl\u00fcthen Bl\u00fcthe mehr Bestand?", "tokens": ["Hat", "al\u00b7ler", "Bl\u00fc\u00b7then", "Bl\u00fc\u00b7the", "mehr", "Be\u00b7stand", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Die wie durch Zauber fest geschlungen waren,", "tokens": ["Die", "wie", "durch", "Zau\u00b7ber", "fest", "ge\u00b7schlun\u00b7gen", "wa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "KOKOM", "APPR", "NN", "ADJD", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "L\u00f6st Gl\u00fcck und Ruh und Zeit mit leiser Hand,", "tokens": ["L\u00f6st", "Gl\u00fcck", "und", "Ruh", "und", "Zeit", "mit", "lei\u00b7ser", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NN", "KON", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und, jedem fremden Widerstand entronnen,", "tokens": ["Und", ",", "je\u00b7dem", "frem\u00b7den", "Wi\u00b7der\u00b7stand", "ent\u00b7ron\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PIAT", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Ertr\u00e4nkt sich Lieb' im Becher eigner Wonnen.", "tokens": ["Er\u00b7tr\u00e4nkt", "sich", "Lieb'", "im", "Be\u00b7cher", "eig\u00b7ner", "Won\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "NN", "APPRART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Viel seliger, wenn seine sch\u00f6nste Habe", "tokens": ["Viel", "se\u00b7li\u00b7ger", ",", "wenn", "sei\u00b7ne", "sch\u00f6ns\u00b7te", "Ha\u00b7be"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "$,", "KOUS", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das Herz mit sich in's Land der Schatten rei\u00dft,", "tokens": ["Das", "Herz", "mit", "sich", "in's", "Land", "der", "Schat\u00b7ten", "rei\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PRF", "APPRART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wenn dem Befreier Tod zur Opfergabe", "tokens": ["Wenn", "dem", "Be\u00b7frei\u00b7er", "Tod", "zur", "Op\u00b7fer\u00b7ga\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der s\u00fc\u00dfe Kelch, noch kaum gekostet, fleu\u00dft.", "tokens": ["Der", "s\u00fc\u00b7\u00dfe", "Kelch", ",", "noch", "kaum", "ge\u00b7kos\u00b7tet", ",", "fleu\u00dft", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADV", "ADV", "VVPP", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ein Tempel wird aus der Geliebten Grabe,", "tokens": ["Ein", "Tem\u00b7pel", "wird", "aus", "der", "Ge\u00b7lieb\u00b7ten", "Gra\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Der schimmernd ihren heil'gen Bund umschleu\u00dft.", "tokens": ["Der", "schim\u00b7mernd", "ih\u00b7ren", "heil'\u00b7gen", "Bund", "um\u00b7schleu\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Sie sterben, doch im letzten Athemzuge", "tokens": ["Sie", "ster\u00b7ben", ",", "doch", "im", "letz\u00b7ten", "A\u00b7them\u00b7zu\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "ADV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Entschwingt die Liebe sich zu h\u00f6herm Fluge.", "tokens": ["Ent\u00b7schwingt", "die", "Lie\u00b7be", "sich", "zu", "h\u00f6\u00b7herm", "Flu\u00b7ge", "."], "token_info": ["word", "word", "word", 