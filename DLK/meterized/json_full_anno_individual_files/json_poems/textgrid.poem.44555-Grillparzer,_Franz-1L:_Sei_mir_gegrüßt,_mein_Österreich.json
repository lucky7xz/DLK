, "word", "word", "word"], "pos": ["PWAV", "ADJD", "KON", "NN", "KON", "ADJD", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie l\u00e4ngst auf Formeln brachten,", "tokens": ["Sie", "l\u00e4ngst", "auf", "For\u00b7meln", "brach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Rasch wechselnd die erlogne Glut", "tokens": ["Rasch", "wech\u00b7selnd", "die", "er\u00b7log\u00b7ne", "Glut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "ADJD", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gleich bunten Kleidertrachten;", "tokens": ["Gleich", "bun\u00b7ten", "Klei\u00b7der\u00b7trach\u00b7ten", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Wo selbst die Freiheit, die zur Zeit", "tokens": ["Wo", "selbst", "die", "Frei\u00b7heit", ",", "die", "zur", "Zeit"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "ADV", "ART", "NN", "$,", "PRELS", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hinjauchzt in tausend Stimmen,", "tokens": ["Hin\u00b7jauchzt", "in", "tau\u00b7send", "Stim\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "CARD", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Halb gro\u00dfges\u00e4ugt von Eitelkeit", "tokens": ["Halb", "gro\u00df\u00b7ge\u00b7s\u00e4ugt", "von", "Ei\u00b7tel\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "ADJD", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und von der Lust am Schlimmen.", "tokens": ["Und", "von", "der", "Lust", "am", "Schlim\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Bleib du das Land, das stets du warst,", "tokens": ["Bleib", "du", "das", "Land", ",", "das", "stets", "du", "warst", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "$,", "PDS", "VVFIN", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nur Morgen, wie sonst Abend,", "tokens": ["Nur", "Mor\u00b7gen", ",", "wie", "sonst", "A\u00b7bend", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "$,", "PWAV", "ADV", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Unschuld, die du noch bewahrst,", "tokens": ["Die", "Un\u00b7schuld", ",", "die", "du", "noch", "be\u00b7wahrst", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "An heiterm Sinn erlabend.", "tokens": ["An", "hei\u00b7term", "Sinn", "er\u00b7la\u00b7bend", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Denn, was der Mensch erdacht, erfand,", "tokens": ["Denn", ",", "was", "der", "Mensch", "er\u00b7dacht", ",", "er\u00b7fand", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "$,", "PRELS", "ART", "NN", "VVPP", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Als H\u00f6chstes wird er finden:", "tokens": ["Als", "H\u00f6chs\u00b7tes", "wird", "er", "fin\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "VAFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Gesund nat\u00fcrlichen Verstand", "tokens": ["Ge\u00b7sund", "na\u00b7t\u00fcr\u00b7li\u00b7chen", "Ver\u00b7stand"], "token_info": ["word", "word", "word"], "pos": ["NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und richtiges Empfinden.", "tokens": ["Und", "rich\u00b7ti\u00b7ges", "Emp\u00b7fin\u00b7den", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Sei mir gegr\u00fc\u00dft, mein \u00d6sterreich", "tokens": ["Sei", "mir", "ge\u00b7gr\u00fc\u00dft", ",", "mein", "\u00d6s\u00b7ter\u00b7reich"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VAFIN", "PPER", "VVPP", "$,", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auf deinen neuen Wegen,", "tokens": ["Auf", "dei\u00b7nen", "neu\u00b7en", "We\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Es schl\u00e4gt mein Herz, wie immer gleich,", "tokens": ["Es", "schl\u00e4gt", "mein", "Herz", ",", "wie", "im\u00b7mer", "gleich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,", "PWAV", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auch heute dir entgegen.", "tokens": ["Auch", "heu\u00b7te", "dir", "ent\u00b7ge\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Was di