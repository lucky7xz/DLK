{"textgrid.poem.44555": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sei mir gegr\u00fc\u00dft, mein \u00d6sterreich", "genre": "verse", "period": "N.A.", "pub_year": 1848, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sei mir gegr\u00fc\u00dft, mein \u00d6sterreich", "tokens": ["Sei", "mir", "ge\u00b7gr\u00fc\u00dft", ",", "mein", "\u00d6s\u00b7ter\u00b7reich"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VAFIN", "PPER", "VVPP", "$,", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auf deinen neuen Wegen,", "tokens": ["Auf", "dei\u00b7nen", "neu\u00b7en", "We\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Es schl\u00e4gt mein Herz, wie immer gleich,", "tokens": ["Es", "schl\u00e4gt", "mein", "Herz", ",", "wie", "im\u00b7mer", "gleich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,", "PWAV", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auch heute dir entgegen.", "tokens": ["Auch", "heu\u00b7te", "dir", "ent\u00b7ge\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Was dir gefehlt zu deiner Zier,", "tokens": ["Was", "dir", "ge\u00b7fehlt", "zu", "dei\u00b7ner", "Zier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du hast es dir errungen,", "tokens": ["Du", "hast", "es", "dir", "er\u00b7run\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Halb kindlich fromm erbeten dir", "tokens": ["Halb", "kind\u00b7lich", "fromm", "er\u00b7be\u00b7ten", "dir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "ADJD", "ADJD", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und halb durch Mut erzwungen.", "tokens": ["Und", "halb", "durch", "Mut", "er\u00b7zwun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Die Freiheit strahlt ob deinem Haupt,", "tokens": ["Die", "Frei\u00b7heit", "strahlt", "ob", "dei\u00b7nem", "Haupt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KOUS", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie l\u00e4ngst in deinem Herzen,", "tokens": ["Wie", "l\u00e4ngst", "in", "dei\u00b7nem", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Denn freier warst du, als man glaubt,", "tokens": ["Denn", "frei\u00b7er", "warst", "du", ",", "als", "man", "glaubt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "PPER", "$,", "KOUS", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Es zeigtens deine Schmerzen.", "tokens": ["Es", "zeig\u00b7tens", "dei\u00b7ne", "Schmer\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Nun aber, \u00d6streich, sieh dich vor,", "tokens": ["Nun", "a\u00b7ber", ",", "\u00d6s\u00b7treich", ",", "sieh", "dich", "vor", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "ADJD", "$,", "VVIMP", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es gilt die h\u00f6chsten G\u00fcter,", "tokens": ["Es", "gilt", "die", "h\u00f6chs\u00b7ten", "G\u00fc\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Leih nicht dem Schmeichellaut dein Ohr", "tokens": ["Leih", "nicht", "dem", "Schmei\u00b7chel\u00b7laut", "dein", "Ohr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "PTKNEG", "ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und sei dein eigner H\u00fcter!", "tokens": ["Und", "sei", "dein", "eig\u00b7ner", "H\u00fc\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Geh nicht zur Schule da und dort,", "tokens": ["Geh", "nicht", "zur", "Schu\u00b7le", "da", "und", "dort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "APPRART", "NN", "ADV", "KON", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wo laute Redner l\u00e4rmen,", "tokens": ["Wo", "lau\u00b7te", "Red\u00b7ner", "l\u00e4r\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wo der Gedanke nur im Wort