{"dta.poem.11646": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Der Liebhaber  \n an die hei\u00dfe Quelle in B.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905193969", "language": ["de:0.99"], "booktitle": "M\u00f6rike, Eduard: Gedichte. Stuttgart, 1838."}, "poem": {"stanza.1": {"line.1": {"text": "Du heilest Den und tr\u00f6stest Jenen,", "tokens": ["Du", "hei\u00b7lest", "Den", "und", "tr\u00f6s\u00b7test", "Je\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "KON", "VVFIN", "PDS", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "O Quell, so h\u00f6r' auch meinen Schmerz!", "tokens": ["O", "Quell", ",", "so", "h\u00f6r'", "auch", "mei\u00b7nen", "Schmerz", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "ADV", "VVFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich klage dir mit bittern Thr\u00e4nen", "tokens": ["Ich", "kla\u00b7ge", "dir", "mit", "bit\u00b7tern", "Thr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein hartes, kaltes M\u00e4dchenherz.", "tokens": ["Ein", "har\u00b7tes", ",", "kal\u00b7tes", "M\u00e4d\u00b7chen\u00b7herz", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Es zu erweichen, zu durchgl\u00fchen,", "tokens": ["Es", "zu", "er\u00b7wei\u00b7chen", ",", "zu", "durch\u00b7gl\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "$,", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dir ist es eine leichte Pflicht;", "tokens": ["Dir", "ist", "es", "ei\u00b7ne", "leich\u00b7te", "Pflicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Man kann ja H\u00fchner in dir br\u00fchen,", "tokens": ["Man", "kann", "ja", "H\u00fch\u00b7ner", "in", "dir", "br\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "ADV", "NN", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Warum ein junges G\u00e4nschen nicht?", "tokens": ["Wa\u00b7rum", "ein", "jun\u00b7ges", "G\u00e4n\u00b7schen", "nicht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}