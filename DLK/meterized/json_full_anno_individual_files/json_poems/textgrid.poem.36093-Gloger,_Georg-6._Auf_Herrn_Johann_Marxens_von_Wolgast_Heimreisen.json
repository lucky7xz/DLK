{"textgrid.poem.36093": {"metadata": {"author": {"name": "Gloger, Georg", "birth": "N.A.", "death": "N.A."}, "title": "6. Auf Herrn Johann Marxens von Wolgast Heimreisen", "genre": "verse", "period": "N.A.", "pub_year": 1617, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Woran wir Sterblichen vns pflegen zu ergetzen,", "tokens": ["Wo\u00b7ran", "wir", "Sterb\u00b7li\u00b7chen", "vns", "pfle\u00b7gen", "zu", "er\u00b7get\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "NN", "PPER", "VVINF", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vnd unsre Lust vnd Sinn mit Willen drauf zu setzen,", "tokens": ["Vnd", "uns\u00b7re", "Lust", "vnd", "Sinn", "mit", "Wil\u00b7len", "drauf", "zu", "set\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "KON", "NN", "APPR", "NN", "PAV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ist nicht nur einerley. Der lockt vmb Mitternacht,", "tokens": ["Ist", "nicht", "nur", "ei\u00b7ner\u00b7ley", ".", "Der", "lockt", "vmb", "Mit\u00b7ter\u00b7nacht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "ADV", "ADV", "$.", "ART", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Eh es noch himmelgrawt vnd Phebus auferwacht,", "tokens": ["Eh", "es", "noch", "him\u00b7mel\u00b7grawt", "vnd", "Phe\u00b7bus", "au\u00b7fer\u00b7wacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "KON", "NE", "VVPP", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.5": {"text": "Die gro\u00dfen R\u00fcden auf, vnd wil bey ienen Buchen,", "tokens": ["Die", "gro\u00b7\u00dfen", "R\u00fc\u00b7den", "auf", ",", "vnd", "wil", "bey", "ie\u00b7nen", "Bu\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$,", "KON", "VMFIN", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die Stellung fangen an, der Hindin Sp\u00fcre suchen,", "tokens": ["Die", "Stel\u00b7lung", "fan\u00b7gen", "an", ",", "der", "Hin\u00b7din", "Sp\u00fc\u00b7re", "su\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,", "ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die gestern dorte gieng. Ein andrer achtets nicht,", "tokens": ["Die", "ge\u00b7stern", "dor\u00b7te", "gieng", ".", "Ein", "an\u00b7drer", "ach\u00b7tets", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$.", "ART", "ADJA", "ADV", "PTKNEG", "$,"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.8": {"text": "Der lieber auf der Schul' ein gutes pferd abricht.", "tokens": ["Der", "lie\u00b7ber", "auf", "der", "Schul'", "ein", "gu\u00b7tes", "pferd", "ab\u00b7richt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Der, als ein Kriegesman, wird anders nicht ergetzet,", "tokens": ["Der", ",", "als", "ein", "Krie\u00b7ges\u00b7man", ",", "wird", "an\u00b7ders", "nicht", "er\u00b7get\u00b7zet", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "KOUS", "ART", "NN", "$,", "VAFIN", "ADV", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Als wenn er seine Faust ins Feindes Blute netzet.", "tokens": ["Als", "wenn", "er", "sei\u00b7ne", "Faust", "ins", "Fein\u00b7des", "Blu\u00b7te", "net\u00b7zet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "PPOSAT", "NN", "APPRART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Dem deuchtet Keines recht, sein' h\u00f6chste Lust ist Geld,", "tokens": ["Dem", "deuch\u00b7tet", "Kei\u00b7nes", "recht", ",", "sein'", "h\u00f6chs\u00b7te", "Lust", "ist", "Geld", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PIS", "ADJD", "$,", "PPOSAT", "ADJA", "NN", "VAFIN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Darnach er Seel vnd Geist bis in die newe Welt", "tokens": ["Dar\u00b7nach", "er", "Seel", "vnd", "Geist", "bis", "in", "die", "ne\u00b7we", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "PPER", "NN", "KON", "NN", "APPR", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.13": {"text": "Auf einem Brete hetzt. Ein andrer bleibet sitzen,", "tokens": ["Auf", "ei\u00b7nem", "Bre\u00b7te", "hetzt", ".", "Ein", "an\u00b7drer", "blei\u00b7bet", "sit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$.", "ART", "ADJA", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Wil lieber an dem Tisch, als auf der Stra\u00dfen schwitzen,", "tokens": ["Wil", "lie\u00b7ber", "an", "dem", "Tisch", ",", "als", "auf", "der", "Stra\u00b7\u00dfen", "schwit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "APPR", "ART", "NN", "$,", "KOUS", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Vnd k\u00f6mmt ihn nichts so schwer, als nur das Reisen an,", "tokens": ["Vnd", "k\u00f6mmt", "ihn", "nichts", "so", "schwer", ",", "als", "nur", "das", "Rei\u00b7sen", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PIS", "ADV", "ADJD", "$,", "KOUS", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Weil er des Nachbars Luft schon nicht vertragen kan.", "tokens": ["Weil", "er", "des", "Nach\u00b7bars", "Luft", "schon", "nicht", "ver\u00b7tra\u00b7gen", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "NN", "ADV", "PTKNEG", "VVFIN", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Dein himmelvoller Sinn zu diesem dich verhetzet,", "tokens": ["Dein", "him\u00b7mel\u00b7vol\u00b7ler", "Sinn", "zu", "die\u00b7sem", "dich", "ver\u00b7het\u00b7zet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "APPR", "PDAT", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Darnach er ietzo noch in vollem B\u00fcgen setztet;", "tokens": ["Dar\u00b7nach", "er", "iet\u00b7zo", "noch", "in", "vol\u00b7lem", "B\u00fc\u00b7gen", "setz\u00b7tet", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "ADV", "ADV", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Vnd zwar das sch\u00f6ne Ziel, darnach dein Sinn so ringt,", "tokens": ["Vnd", "zwar", "das", "sch\u00f6\u00b7ne", "Ziel", ",", "dar\u00b7nach", "dein", "Sinn", "so", "ringt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "ADJA", "NN", "$,", "PAV", "PPOSAT", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Ins k\u00fcnftige dein Lob bis an das Blawe bringt.", "tokens": ["Ins", "k\u00fcnf\u00b7ti\u00b7ge", "dein", "Lob", "bis", "an", "das", "Bla\u00b7we", "bringt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "PPOSAT", "NN", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Was Ehreneyfer kan. Vnd wie sich durchgeri\u00dfen", "tokens": ["Was", "Eh\u00b7re\u00b7ney\u00b7fer", "kan", ".", "Vnd", "wie", "sich", "durch\u00b7ge\u00b7ri\u00b7\u00dfen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "NN", "VMFIN", "$.", "KON", "PWAV", "PRF", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Dein loberhitzter Sinn vnd tugendreifer Geist.", "tokens": ["Dein", "lo\u00b7ber\u00b7hitz\u00b7ter", "Sinn", "vnd", "tu\u00b7gen\u00b7drei\u00b7fer", "Geist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Was ", "tokens": ["Was"], "token_info": ["word"], "pos": ["PWS"], "meter": "-", "measure": "single.down"}, "line.24": {"text": "Wird nicht vergebens sein. Was ", "tokens": ["Wird", "nicht", "ver\u00b7ge\u00b7bens", "sein", ".", "Was"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["VAFIN", "PTKNEG", "ADV", "VAINF", "$.", "PWS"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.25": {"text": "Der wolkengleiche Mann, wird ihn in dir beleben,", "tokens": ["Der", "wol\u00b7ken\u00b7glei\u00b7che", "Mann", ",", "wird", "ihn", "in", "dir", "be\u00b7le\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "VAFIN", "PPER", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Wenn er nicht mehr wird sein. ", "tokens": ["Wenn", "er", "nicht", "mehr", "wird", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ADV", "VAFIN", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.27": {"text": "Hat dir zu ", "tokens": ["Hat", "dir", "zu"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "PPER", "APPR"], "meter": "+-+", "measure": "trochaic.di"}, "line.28": {"text": "Nun, mangelt dir noch was, das kanstu dich selbst lehren.", "tokens": ["Nun", ",", "man\u00b7gelt", "dir", "noch", "was", ",", "das", "kans\u00b7tu", "dich", "selbst", "leh\u00b7ren", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "VVFIN", "PPER", "ADV", "PIS", "$,", "PDS", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Itzund zeuch immer hin; zeuch hin, vnd komm mit Ehren", "tokens": ["It\u00b7zund", "zeuch", "im\u00b7mer", "hin", ";", "zeuch", "hin", ",", "vnd", "komm", "mit", "Eh\u00b7ren"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVIMP", "ADV", "PTKVZ", "$.", "VVIMP", "PTKVZ", "$,", "KON", "VVFIN", "APPR", "NN"], "meter": "---+-+-+-+-+-", "measure": "unknown.measure.penta"}, "line.30": {"text": "Bald wieder an den Ort, der dich geboren hat,", "tokens": ["Bald", "wie\u00b7der", "an", "den", "Ort", ",", "der", "dich", "ge\u00b7bo\u00b7ren", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ART", "NN", "$,", "PRELS", "PPER", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Vnd weise, was man hofft noch ietzt, einst in der That.", "tokens": ["Vnd", "wei\u00b7se", ",", "was", "man", "hofft", "noch", "ietzt", ",", "einst", "in", "der", "That", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PRELS", "PIS", "VVFIN", "ADV", "ADV", "$,", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Woran wir Sterblichen vns pflegen zu ergetzen,", "tokens": ["Wo\u00b7ran", "wir", "Sterb\u00b7li\u00b7chen", "vns", "pfle\u00b7gen", "zu", "er\u00b7get\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "NN", "PPER", "VVINF", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vnd unsre Lust vnd Sinn mit Willen drauf zu setzen,", "tokens": ["Vnd", "uns\u00b7re", "Lust", "vnd", "Sinn", "mit", "Wil\u00b7len", "drauf", "zu", "set\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "KON