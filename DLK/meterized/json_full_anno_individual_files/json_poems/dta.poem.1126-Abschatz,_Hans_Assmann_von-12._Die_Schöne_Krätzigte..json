{"dta.poem.1126": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "12. Die Sch\u00f6ne Kr\u00e4tzigte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn Perlen-Schnecken auch von Purpur schwanger schie- ", "tokens": ["Wenn", "Per\u00b7len\u00b7Schne\u00b7cken", "auch", "von", "Pur\u00b7pur", "schwan\u00b7ger", "schie"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "ADV", "APPR", "NN", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "nen/ ", "tokens": ["nen", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "-", "measure": "single.down"}, "line.3": {"text": "Wenn Alabaster sich vermengte mit Porphir/", "tokens": ["Wenn", "A\u00b7la\u00b7bas\u00b7ter", "sich", "ver\u00b7meng\u00b7te", "mit", "Por\u00b7phir", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PRF", "VVFIN", "APPR", "NE", "$("], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Wenn ihrem Venus-Blutt und des Zinobers Zier", "tokens": ["Wenn", "ih\u00b7rem", "Ve\u00b7nus\u00b7Blutt", "und", "des", "Zi\u00b7no\u00b7bers", "Zier"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "KON", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die Rose sezte bey den Schnee der Gelsaminen/", "tokens": ["Die", "Ro\u00b7se", "sez\u00b7te", "bey", "den", "Schnee", "der", "Gel\u00b7sa\u00b7mi\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wenn ihr die Lilien zum Hemmet m\u00fcsten dienen/", "tokens": ["Wenn", "ihr", "die", "Li\u00b7li\u00b7en", "zum", "Hem\u00b7met", "m\u00fcs\u00b7ten", "die\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPRART", "NN", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Sie giengen rothem Gold und hellem Demant f\u00fcr:", "tokens": ["Sie", "gien\u00b7gen", "ro\u00b7them", "Gold", "und", "hel\u00b7lem", "De\u00b7mant", "f\u00fcr", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "KON", "ADJA", "NN", "APPR", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So eben weiset sich der weisse Leib an dir/", "tokens": ["So", "e\u00b7ben", "wei\u00b7set", "sich", "der", "weis\u00b7se", "Leib", "an", "dir", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PRF", "ART", "ADJA", "NN", "APPR", "PPER", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Besprenget mit Coralln und bluttenden Rubinen.", "tokens": ["Be\u00b7spren\u00b7get", "mit", "Co\u00b7ralln", "und", "blut\u00b7ten\u00b7den", "Ru\u00b7bi\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NE", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Du scheinst an deiner Haut ein Tiger-Thier zu seyn.", "tokens": ["Du", "scheinst", "an", "dei\u00b7ner", "Haut", "ein", "Ti\u00b7ger\u00b7Thier", "zu", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "ART", "NN", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "(mit deiner Grausamkeit stimmt solche Kleidung ein/)", "tokens": ["(", "mit", "dei\u00b7ner", "Grau\u00b7sam\u00b7keit", "stimmt", "sol\u00b7che", "Klei\u00b7dung", "ein", "/", ")"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "APPR", "PPOSAT", "NN", "VVFIN", "PIAT", "NN", "ART", "$(", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Ein Himmel aber auch bes\u00e4t mit tausend Sternen.", "tokens": ["Ein", "Him\u00b7mel", "a\u00b7ber", "auch", "be\u00b7s\u00e4t", "mit", "tau\u00b7send", "Ster\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADV", "ADJD", "APPR", "CARD", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Du kanst aus eignem Schmertz izt fremde Qual erlernen:", "tokens": ["Du", "kanst", "aus", "eig\u00b7nem", "Schmertz", "izt", "frem\u00b7de", "Qual", "er\u00b7ler\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "ADJA", "NN", "ADV", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Wenn du dir selber rei\u00dfst das zarte Fell entzwey/", "tokens": ["Wenn", "du", "dir", "sel\u00b7ber", "rei\u00df\u00b7st", "das", "zar\u00b7te", "Fell", "ent\u00b7zwey", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.15": {"text": "So dencke/ wie manch Hertz von dir zerrissen sey.", "tokens": ["So", "den\u00b7cke", "/", "wie", "manch", "Hertz", "von", "dir", "zer\u00b7ris\u00b7sen", "sey", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$(", "KOKOM", "PIAT", "NN", "APPR", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}