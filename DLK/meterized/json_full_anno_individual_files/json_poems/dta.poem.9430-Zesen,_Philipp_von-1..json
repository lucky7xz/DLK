, "PRF", "VVINF", "$("], "meter": "-+-++--+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Es girret die Taube/ l\u00e4\u00dft schallen jhr Lied/", "tokens": ["Es", "gir\u00b7ret", "die", "Tau\u00b7be", "/", "l\u00e4\u00dft", "schal\u00b7len", "jhr", "Lied", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$(", "VVFIN", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Der Weinstock mu\u00df unsere Fr\u00f6ligkeit mehren/", "tokens": ["Der", "Wein\u00b7stock", "mu\u00df", "un\u00b7se\u00b7re", "Fr\u00f6\u00b7lig\u00b7keit", "meh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Die Wiese geht tr\u00e4chtig/ der Feigenbaum bl\u00fcht.", "tokens": ["Die", "Wie\u00b7se", "geht", "tr\u00e4ch\u00b7tig", "/", "der", "Fei\u00b7gen\u00b7baum", "bl\u00fcht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$(", "ART", "NN", "VVFIN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Erhebe doch/ Sch\u00f6ne/", "tokens": ["Er\u00b7he\u00b7be", "doch", "/", "Sch\u00f6\u00b7ne", "/"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "ADV", "$(", "NN", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Dein freuden-geth\u00f6ne/", "tokens": ["Dein", "freu\u00b7den\u00b7get\u00b7h\u00f6\u00b7ne", "/"], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Komm/ eyle mein Licht/", "tokens": ["Komm", "/", "ey\u00b7le", "mein", "Licht", "/"], "token_info": ["word", "punct", "w