{"dta.poem.9430": {"metadata": {"author": {"name": "Zesen, Philipp von", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-20684-8", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Erhebe dich Sch\u00f6ne/ mein bestes verlangen/", "tokens": ["Er\u00b7he\u00b7be", "dich", "Sch\u00f6\u00b7ne", "/", "mein", "bes\u00b7tes", "ver\u00b7lan\u00b7gen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "$(", "PPOSAT", "ADJA", "VVINF", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Auf! Liebeste Schwester und eyle zu mier/", "tokens": ["Auf", "!", "Lie\u00b7bes\u00b7te", "Schwes\u00b7ter", "und", "ey\u00b7le", "zu", "mier", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$.", "ADJA", "NN", "KON", "NN", "APPR", "NE", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Der Regen ist ferne/ der Winter vergangen/", "tokens": ["Der", "Re\u00b7gen", "ist", "fer\u00b7ne", "/", "der", "Win\u00b7ter", "ver\u00b7gan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "$(", "ART", "NN", "VVPP", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Der Fr\u00fchling ist nahe/ bricht fr\u00f6lich herf\u00fcr.", "tokens": ["Der", "Fr\u00fch\u00b7ling", "ist", "na\u00b7he", "/", "bricht", "fr\u00f6\u00b7lich", "her\u00b7f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$(", "VVFIN", "ADJD", "PTKVZ", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Die Blumen uns zieren/", "tokens": ["Die", "Blu\u00b7men", "uns", "zie\u00b7r