{"textgrid.poem.65751": {"metadata": {"author": {"name": "Shakespeare, William", "birth": "N.A.", "death": "N.A."}, "title": "25", "genre": "verse", "period": "N.A.", "pub_year": 1590, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "La\u00df, wem ein g\u00fcnstiges Gestirne tagt,", "tokens": ["La\u00df", ",", "wem", "ein", "g\u00fcns\u00b7ti\u00b7ges", "Ge\u00b7stir\u00b7ne", "tagt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "PWS", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mit \u00f6ffentlicher Ehr' und Titeln prunken.", "tokens": ["Mit", "\u00f6f\u00b7fent\u00b7li\u00b7cher", "Ehr'", "und", "Ti\u00b7teln", "prun\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "NN", "VVINF", "$."], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Ich, dem das Gl\u00fcck so hohen Glanz versagt,", "tokens": ["Ich", ",", "dem", "das", "Gl\u00fcck", "so", "ho\u00b7hen", "Glanz", "ver\u00b7sagt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "ART", "NN", "ADV", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Bin, unbemerkt, von andrer Ehrfurcht trunken.", "tokens": ["Bin", ",", "un\u00b7be\u00b7merkt", ",", "von", "an\u00b7drer", "Ehr\u00b7furcht", "trun\u00b7ken", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "ADJD", "$,", "APPR", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Wie bunte Primeln an der Sonne Blick,", "tokens": ["Wie", "bun\u00b7te", "Pri\u00b7meln", "an", "der", "Son\u00b7ne", "Blick", ","], "token_inf