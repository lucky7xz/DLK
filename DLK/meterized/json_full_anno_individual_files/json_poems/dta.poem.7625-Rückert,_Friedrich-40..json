{"dta.poem.7625": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "40.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Weisheit Anfang ist immer Bewunderung,", "tokens": ["Der", "Weis\u00b7heit", "An\u00b7fang", "ist", "im\u00b7mer", "Be\u00b7wun\u00b7de\u00b7rung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "ADV", "NN", "$,"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Durch ander nichts erh\u00e4lt die Seele Himmelschwung.", "tokens": ["Durch", "an\u00b7der", "nichts", "er\u00b7h\u00e4lt", "die", "See\u00b7le", "Him\u00b7mel\u00b7schwung", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "PIS", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Aus sich und aus der Welt zur Gottheit hingerissen,", "tokens": ["Aus", "sich", "und", "aus", "der", "Welt", "zur", "Got\u00b7theit", "hin\u00b7ge\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "KON", "APPR", "ART", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zu ahnen und zu schaun, zu forschen und zu wissen.", "tokens": ["Zu", "ah\u00b7nen", "und", "zu", "schaun", ",", "zu", "for\u00b7schen", "und", "zu", "wis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "KON", "PTKZU", "VVINF", "$,", "PTKZU", "VVINF", "KON", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wenn erst das Licht du schaust, ohne da\u00df es dich blende,", "tokens": ["Wenn", "erst", "das", "Licht", "du", "schaust", ",", "oh\u00b7ne", "da\u00df", "es", "dich", "blen\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "PPER", "VVFIN", "$,", "KOUI", "KOUS", "PPER", "PRF", "VVFIN", "$,"], "meter": "-+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Nichts zu bewundern ist alsdann der Weisheit Ende.", "tokens": ["Nichts", "zu", "be\u00b7wun\u00b7dern", "ist", "als\u00b7dann", "der", "Weis\u00b7heit", "En\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "PTKZU", "VVINF", "VAFIN", "ADV", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Zum Ende sind noch nicht gedrungen deine Schritte,", "tokens": ["Zum", "En\u00b7de", "sind", "noch", "nicht", "ge\u00b7drun\u00b7gen", "dei\u00b7ne", "Schrit\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "ADV", "PTKNEG", "VVPP", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du stehst bewundernd noch in aller Wunder Mitte.", "tokens": ["Du", "stehst", "be\u00b7wun\u00b7dernd", "noch", "in", "al\u00b7ler", "Wun\u00b7der", "Mit\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ADV", "APPR", "PIAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}