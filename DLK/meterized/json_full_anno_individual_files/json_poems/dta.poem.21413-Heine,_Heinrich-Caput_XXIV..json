{"dta.poem.21413": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Caput XXIV.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-30602-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201edoch sprich, wie kam der Gedanke dir", "tokens": ["\u201e", "doch", "sprich", ",", "wie", "kam", "der", "Ge\u00b7dan\u00b7ke", "dir"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["$(", "ADV", "ADJD", "$,", "PWAV", "VVFIN", "ART", "NN", "PPER"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Zu reisen nach dem Norden", "tokens": ["Zu", "rei\u00b7sen", "nach", "dem", "Nor\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKZU", "VVINF", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "In solcher Jahrzeit? Das Wetter ist", "tokens": ["In", "sol\u00b7cher", "Jahr\u00b7zeit", "?", "Das", "Wet\u00b7ter", "ist"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "$.", "ART", "NN", "VAFIN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Schon winterlich geworden!\u201c", "tokens": ["Schon", "win\u00b7ter\u00b7lich", "ge\u00b7wor\u00b7den", "!", "\u201c"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADJD", "VAPP", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "O, meine G\u00f6ttin! \u2013 erwiederte ich \u2013", "tokens": ["O", ",", "mei\u00b7ne", "G\u00f6t\u00b7tin", "!", "\u2013", "er\u00b7wie\u00b7der\u00b7te", "ich", "\u2013"], "token_info": ["word", "punct", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "PPOSAT", "NN", "$.", "$(", "VVFIN", "PPER", "$("], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Es schlafen tief im Grunde", "tokens": ["Es", "schla\u00b7fen", "tief", "im", "Grun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Des Menschenherzens Gedanken, die oft", "tokens": ["Des", "Men\u00b7schen\u00b7her\u00b7zens", "Ge\u00b7dan\u00b7ken", ",", "die", "oft"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "NN", "$,", "PRELS", "ADV"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Erwachen zur unrechten Stunde.", "tokens": ["Er\u00b7wa\u00b7chen", "zur", "un\u00b7rech\u00b7ten", "Stun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+++-+-", "measure": "unknown.measure.penta"}}, "stanza.3": {"line.1": {"text": "Es ging mir \u00e4u\u00dferlich ziemlich gut,", "tokens": ["Es", "ging", "mir", "\u00e4u\u00b7\u00dfer\u00b7lich", "ziem\u00b7lich", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "ADV", "ADJD", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Doch innerlich war ich beklommen,", "tokens": ["Doch", "in\u00b7ner\u00b7lich", "war", "ich", "be\u00b7klom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und die Beklemmni\u00df t\u00e4glich wuchs \u2013", "tokens": ["Und", "die", "Be\u00b7klemm\u00b7ni\u00df", "t\u00e4g\u00b7lich", "wuchs", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich hatte das Heimweh bekommen.", "tokens": ["Ich", "hat\u00b7te", "das", "Heim\u00b7weh", "be\u00b7kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.4": {"line.1": {"text": "Die sonst so leichte franz\u00f6sische Luft,", "tokens": ["Die", "sonst", "so", "leich\u00b7te", "fran\u00b7z\u00f6\u00b7si\u00b7sche", "Luft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sie fing mich an zu dr\u00fccken;", "tokens": ["Sie", "fing", "mich", "an", "zu", "dr\u00fc\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APZR", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich mu\u00dfte Athem sch\u00f6pfen hier", "tokens": ["Ich", "mu\u00df\u00b7te", "A\u00b7them", "sch\u00f6p\u00b7fen", "hier"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "NN", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In Deutschland, um nicht zu ersticken.", "tokens": ["In", "Deutschland", ",", "um", "nicht", "zu", "er\u00b7sti\u00b7cken", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "KOUI", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}}}}