{"dta.poem.1631": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Antwort.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Warum verweist man uns der edlen Bl\u00e4tter Brauch?", "tokens": ["Wa\u00b7rum", "ver\u00b7weist", "man", "uns", "der", "ed\u00b7len", "Bl\u00e4t\u00b7ter", "Brauch", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PIS", "PRF", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Spielt nicht der kluge Hof/ die meiste Welt mit Rauch?", "tokens": ["Spielt", "nicht", "der", "klu\u00b7ge", "Hof", "/", "die", "meis\u00b7te", "Welt", "mit", "Rauch", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ART", "ADJA", "NN", "$(", "ART", "ADJA", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ranch gutter Einfall glimmt aus unser Pfeiff herf\u00fcr.", "tokens": ["Ranch", "gut\u00b7ter", "Ein\u00b7fall", "glimmt", "aus", "un\u00b7ser", "Pfeiff", "her\u00b7f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wer spielt und buhlt verderbt mehr Zeit und Geld/ als wir.", "tokens": ["Wer", "spielt", "und", "buhlt", "ver\u00b7derbt", "mehr", "Zeit", "und", "Geld", "/", "als", "wir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word