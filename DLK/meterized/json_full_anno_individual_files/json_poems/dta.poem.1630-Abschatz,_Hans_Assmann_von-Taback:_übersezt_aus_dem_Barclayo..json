{"dta.poem.1630": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Taback: \u00fcbersezt aus dem  \n  Barclayo.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Pflantze/ deren Rauch das Gifft", "tokens": ["Pflant\u00b7ze", "/", "de\u00b7ren", "Rauch", "das", "Gifft"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$(", "PRELAT", "NN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Bunter Schlangen \u00fcbertrifft/", "tokens": ["Bun\u00b7ter", "Schlan\u00b7gen", "\u00fc\u00b7bert\u00b7rifft", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Welche die Natur verbannt", "tokens": ["Wel\u00b7che", "die", "Na\u00b7tur", "ver\u00b7bannt"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "ART", "NN", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "In ein weit-entlegnes Land.", "tokens": ["In", "ein", "weit\u00b7ent\u00b7leg\u00b7nes", "Land", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wo der Wilden tummer Geist", "tokens": ["Wo", "der", "Wil\u00b7den", "tum\u00b7mer", "Geist"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Seine Larven G\u00f6tter heist/", "tokens": ["Sei\u00b7ne", "Lar\u00b7ven", "G\u00f6t\u00b7ter", "heist", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wo der Barbarn freche Schaar", "tokens": ["Wo", "der", "Bar\u00b7barn", "fre\u00b7che", "Schaar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Weyland unbeherrschet war.", "tokens": ["Wey\u00b7land", "un\u00b7be\u00b7herr\u00b7schet", "war", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJD", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Wer/ o mehr als H\u00f6llen-Kraut/", "tokens": ["Wer", "/", "o", "mehr", "als", "H\u00f6l\u00b7len\u00b7Kraut", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "$(", "FM", "ADV", "KOUS", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Hat der Fichte dich vertraut?", "tokens": ["Hat", "der", "Fich\u00b7te", "dich", "ver\u00b7traut", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PPER", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Wessen unbehirntes Haubt", "tokens": ["Wes\u00b7sen", "un\u00b7be\u00b7hirn\u00b7tes", "Haubt"], "token_info": ["word", "word", "word"], "pos": ["NN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "War der Sinnen so beraubt/", "tokens": ["War", "der", "Sin\u00b7nen", "so", "be\u00b7raubt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADV", "VVPP", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "Da\u00df es dich in unser Land", "tokens": ["Da\u00df", "es", "dich", "in", "un\u00b7ser", "Land"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.14": {"text": "Durch die weite See gesandt?", "tokens": ["Durch", "die", "wei\u00b7te", "See", "ge\u00b7sandt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.15": {"text": "Kunte nicht Neptun das Schiff/", "tokens": ["Kun\u00b7te", "nicht", "Nep\u00b7tun", "das", "Schiff", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "NN", "ART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.16": {"text": "So durch seine Wellen liff/", "tokens": ["So", "durch", "sei\u00b7ne", "Wel\u00b7len", "liff", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.17": {"text": "Und dergleichen schn\u00f6de Last", "tokens": ["Und", "derg\u00b7lei\u00b7chen", "schn\u00f6\u00b7de", "Last"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PIS", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.18": {"text": "Hielt gefangen um den Mast/", "tokens": ["Hielt", "ge\u00b7fan\u00b7gen", "um", "den", "Mast", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "ART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.19": {"text": "Durch der Winde rasend Heer", "tokens": ["Durch", "der", "Win\u00b7de", "ra\u00b7send", "Heer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVPP", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.20": {"text": "St\u00fcrtzen in das tieffste Meer/", "tokens": ["St\u00fcrt\u00b7zen", "in", "das", "tieffs\u00b7te", "Meer", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.21": {"text": "Treiben auff erh\u00f6hten Sand/", "tokens": ["Trei\u00b7ben", "auff", "er\u00b7h\u00f6h\u00b7ten", "Sand", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.22": {"text": "Schlagen an der Klippen Wand/", "tokens": ["Schla\u00b7gen", "an", "der", "Klip\u00b7pen", "Wand", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.23": {"text": "F\u00fchren auff ein falsches Bay", "tokens": ["F\u00fch\u00b7ren", "auff", "ein", "fal\u00b7sches", "Bay"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "ADJA", "NE"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.24": {"text": "Schmettern in viel St\u00fcck entzwey!", "tokens": ["Schmet\u00b7tern", "in", "viel", "St\u00fcck", "ent\u00b7zwey", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PIAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.25": {"text": "Konte nicht der Jupiter", "tokens": ["Kon\u00b7te", "nicht", "der", "Ju\u00b7pi\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "PTKNEG", "ART", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.26": {"text": "Aller Sternen Ober-Herr/", "tokens": ["Al\u00b7ler", "Ster\u00b7nen", "O\u00b7ber\u00b7Herr", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.27": {"text": "Auff das schwancke Wasser-Hau\u00df", "tokens": ["Auff", "das", "schwan\u00b7cke", "Was\u00b7ser\u00b7Hau\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.28": {"text": "Blitz und Donner