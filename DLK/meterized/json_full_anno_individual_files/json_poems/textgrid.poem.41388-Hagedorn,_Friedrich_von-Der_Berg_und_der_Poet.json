{"textgrid.poem.41388": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Der Berg und der Poet", "genre": "verse", "period": "N.A.", "pub_year": 1731, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr G\u00f6tter, rettet! Menschen, flieht!", "tokens": ["Ihr", "G\u00f6t\u00b7ter", ",", "ret\u00b7tet", "!", "Men\u00b7schen", ",", "flieht", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "$.", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein schwangrer Berg beginnt zu kreisen,", "tokens": ["Ein", "schwang\u00b7rer", "Berg", "be\u00b7ginnt", "zu", "krei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und wird jetzt, eh' man sich's versieht,", "tokens": ["Und", "wird", "jetzt", ",", "eh'", "man", "sich's", "ver\u00b7sieht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "$,", "KOUS", "PIS", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit Sand und Schollen um sich schmei\u00dfen.", "tokens": ["Mit", "Sand", "und", "Schol\u00b7len", "um", "sich", "schmei\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "APPR", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Er br\u00fcllt, er kracht, und Thal und Feld", "tokens": ["Er", "br\u00fcllt", ",", "er", "kracht", ",", "und", "Thal", "und", "Feld"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "w