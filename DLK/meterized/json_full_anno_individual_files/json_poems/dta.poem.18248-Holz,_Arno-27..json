{"dta.poem.18248": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "27.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1886", "urn": "urn:nbn:de:kobv:b4-200905192665", "language": ["de:0.99"], "booktitle": "Holz, Arno: Das Buch der Zeit. Lieder eines Modernen. Z\u00fcrich, 1886."}, "poem": {"stanza.1": {"line.1": {"text": "Ich wei\u00df, dies ist das Haus \u201eZu den drei Nymphen\u201c,", "tokens": ["Ich", "wei\u00df", ",", "dies", "ist", "das", "Haus", "\u201e", "Zu", "den", "drei", "Nym\u00b7phen", "\u201c", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "$,", "PDS", "VAFIN", "ART", "NN", "$(", "APPR", "ART", "CARD", "NN", "$(", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Doch setze dich nicht gleich mir auf den Schoo\u00df", "tokens": ["Doch", "set\u00b7ze", "dich", "nicht", "gleich", "mir", "auf", "den", "Schoo\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "ADV", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und kokettire nicht mit deinen Str\u00fcmpfen!", "tokens": ["Und", "ko\u00b7ket\u00b7ti\u00b7re", "nicht", "mit", "dei\u00b7nen", "Str\u00fcmp\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Dein Wort ist wie ein t\u00f6nendes Geschell,", "tokens": ["Dein", "Wort", "ist", "wie", "ein", "t\u00f6\u00b7nen\u00b7des", "Ge\u00b7schell", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "KOKOM", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Du wirst dies junge Herz mir nicht beschwatzen;", "tokens": ["Du", "wirst", "dies", "jun\u00b7ge", "Herz", "mir", "nicht", "be\u00b7schwat\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PDS", "ADJA", "NN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Du bist ja doch nur eine Biermamsell", "tokens": ["Du", "bist", "ja", "doch", "nur", "ei\u00b7ne", "Bier\u00b7mam\u00b7sell"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und feil und falsch wie alle diese Katzen.", "tokens": ["Und", "feil", "und", "falsch", "wie", "al\u00b7le", "die\u00b7se", "Kat\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KON", "ADJD", "KOKOM", "PIS", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Durch dein Gel\u00e4chter zischt die rothe Lust,", "tokens": ["Durch", "dein", "Ge\u00b7l\u00e4ch\u00b7ter", "zischt", "die", "ro\u00b7the", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Goldgier grub sich tief in deine Z\u00fcge", "tokens": ["Die", "Gold\u00b7gier", "grub", "sich", "tief", "in", "dei\u00b7ne", "Z\u00fc\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PRF", "ADJD", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und luftgepolstert thront auf deiner Brust", "tokens": ["Und", "luft\u00b7ge\u00b7pols\u00b7tert", "thront", "auf", "dei\u00b7ner", "Brust"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die gummifabricirte Doppell\u00fcge.", "tokens": ["Die", "gum\u00b7mi\u00b7fab\u00b7ri\u00b7cir\u00b7te", "Dop\u00b7pel\u00b7l\u00fc\u00b7ge", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Was dir an Locken baumelt um die Stirn,", "tokens": ["Was", "dir", "an", "Lo\u00b7cken", "bau\u00b7melt", "um", "die", "Stirn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ist m\u00fchsam nur gestutzt mit Papilloten", "tokens": ["Ist", "m\u00fch\u00b7sam", "nur", "ge\u00b7stutzt", "mit", "Pa\u00b7pil\u00b7lo\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADJD", "ADV", "VVPP", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und dein vertracktes kleines Weibsgehirn", "tokens": ["Und", "dein", "ver\u00b7track\u00b7tes", "klei\u00b7nes", "Weibs\u00b7ge\u00b7hirn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ist bis zum Platzen vollgepfropft mit Zoten.", "tokens": ["Ist", "bis", "zum", "Plat\u00b7zen", "voll\u00b7ge\u00b7pfropft", "mit", "Zo\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPRART", "NN", "VVPP", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Du machst die Augen zu und schnalzst: Wie sch\u00f6n!", "tokens": ["Du", "machst", "die", "Au\u00b7gen", "zu", "und", "schnalz\u00b7st", ":", "Wie", "sch\u00f6n", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKVZ", "KON", "VVFIN", "$.", "PWAV", "ADJD", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Und nippst beim Nachbargast vom Blut der Reben", "tokens": ["Und", "nippst", "beim", "Nach\u00b7bar\u00b7gast", "vom", "Blut", "der", "Re\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPRART", "NN", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und denkst dabei nur an das Lustgest\u00f6hn,", "tokens": ["Und", "denkst", "da\u00b7bei", "nur", "an", "das", "Lust\u00b7ge\u00b7st\u00f6hn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PAV", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Als du dich gestern Nacht ihm preisgegeben.", "tokens": ["Als", "du", "dich", "ge\u00b7stern", "Nacht", "ihm", "preis\u00b7ge\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Dein Element ist recht die V\u00f6llerei,", "tokens": ["Dein", "E\u00b7le\u00b7ment", "ist", "recht", "die", "V\u00f6l\u00b7le\u00b7rei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Das Austernfressen und Champagnersaufen ....", "tokens": ["Das", "Aus\u00b7tern\u00b7fres\u00b7sen", "und", "Cham\u00b7pag\u00b7ner\u00b7sau\u00b7fen", "...."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "$."], "meter": "-+-+--++-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Doch Teufel! schl\u00e4gt die Stutzuhr dort schon Zwei?", "tokens": ["Doch", "Teu\u00b7fel", "!", "schl\u00e4gt", "die", "Stut\u00b7zuhr", "dort", "schon", "Zwei", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$.", "VVFIN", "ART", "NN", "ADV", "ADV", "CARD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Da, nimm mein Portemonnaie \u2014 und la\u00df mich laufen!", "tokens": ["Da", ",", "nimm", "mein", "Por\u00b7te\u00b7mon\u00b7nai\u00b7e", "und", "la\u00df", "mich", "lau\u00b7fen", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "VVIMP", "PPOSAT", "NN", "$(", "KON", "VVIMP", "PPER", "VVINF", "$."], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}}}}}