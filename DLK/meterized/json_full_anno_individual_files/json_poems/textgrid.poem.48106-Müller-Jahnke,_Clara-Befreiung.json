{"textgrid.poem.48106": {"metadata": {"author": {"name": "M\u00fcller-Jahnke, Clara", "birth": "N.A.", "death": "N.A."}, "title": "Befreiung", "genre": "verse", "period": "N.A.", "pub_year": 1882, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O du, den alle Sterne loben,", "tokens": ["O", "du", ",", "den", "al\u00b7le", "Ster\u00b7ne", "lo\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "ART", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "ich hab dich in des Nordsturms Toben", "tokens": ["ich", "hab", "dich", "in", "des", "Nord\u00b7sturms", "To\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "und in des S\u00fcdwinds Hauch gesucht,", "tokens": ["und", "in", "des", "S\u00fcd\u00b7winds", "Hauch", "ge\u00b7sucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "im unermessenen Wellenscho\u00dfe,", "tokens": ["im", "un\u00b7er\u00b7mes\u00b7se\u00b7nen", "Wel\u00b7len\u00b7scho\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "im Purpurkelch der Junirose", "tokens": ["im", "Pur\u00b7pur\u00b7kelch", "der", "Ju\u00b7ni\u00b7ro\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "und in des Herbstes reifer Frucht.", "tokens": ["und", "in", "des", "Herbs\u00b7tes", "rei\u00b7fer", "Frucht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich suchte dich in Kirchenmauern", "tokens": ["Ich", "such\u00b7te", "dich", "in", "Kir\u00b7chen\u00b7mau\u00b7ern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "und trat mit ahnungsvollen Schauern", "tokens": ["und", "trat", "mit", "ah\u00b7nungs\u00b7vol\u00b7len", "Schau\u00b7ern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "in deines Namens Heiligtum;", "tokens": ["in", "dei\u00b7nes", "Na\u00b7mens", "Hei\u00b7lig\u00b7tum", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "und als der Predigt Wort verklungen,", "tokens": ["und", "als", "der", "Pre\u00b7digt", "Wort", "ver\u00b7klun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "da sangen mir in tausend Zungen", "tokens": ["da", "san\u00b7gen", "mir", "in", "tau\u00b7send", "Zun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "CARD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "ringsum die Steine deinen Ruhm.", "tokens": ["ring\u00b7sum", "die", "Stei\u00b7ne", "dei\u00b7nen", "Ruhm", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.3": {"line.1": {"text": "Ich sah aus knospenden Gewalten", "tokens": ["Ich", "sah", "aus", "knos\u00b7pen\u00b7den", "Ge\u00b7wal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+---+-", "measure": "unknown.measure.tri"}, "line.2": {"text": "zur F\u00fclle sich den Geist gestalten,", "tokens": ["zur", "F\u00fcl\u00b7le", "sich", "den", "Geist", "ge\u00b7stal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PRF", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "mit deines Geistes Kraft gepaart, \u2013", "tokens": ["mit", "dei\u00b7nes", "Geis\u00b7tes", "Kraft", "ge\u00b7paart", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "VVPP", "$,", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "ich sah am Leidenspf\u00fchl des Armen", "tokens": ["ich", "sah", "am", "Lei\u00b7den\u00b7spf\u00fchl", "des", "Ar\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "die Liebe schweigend sich erbarmen", "tokens": ["die", "Lie\u00b7be", "schwei\u00b7gend", "sich", "er\u00b7bar\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "PRF", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "als Zeugin deiner Gegenwart.", "tokens": ["als", "Zeu\u00b7gin", "dei\u00b7ner", "Ge\u00b7gen\u00b7wart", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ich ahnte dich in blauen Weiten;", "tokens": ["Ich", "ahn\u00b7te", "dich", "in", "blau\u00b7en", "Wei\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "im wandellosen Gang der Zeiten", "tokens": ["im", "wan\u00b7del\u00b7lo\u00b7sen", "Gang", "der", "Zei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "verfolg ich deiner Tritte Spur; \u2013", "tokens": ["ver\u00b7folg", "ich", "dei\u00b7ner", "Trit\u00b7te", "Spur", ";", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "und schritt ich bis ans Weltenende,", "tokens": ["und", "schritt", "ich", "bis", "ans", "Wel\u00b7te\u00b7nen\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "das Werk erschaut ich deiner H\u00e4nde,", "tokens": ["das", "Werk", "er\u00b7schaut", "ich", "dei\u00b7ner", "H\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "die Sch\u00f6pfung deiner Allmacht nur!", "tokens": ["die", "Sch\u00f6p\u00b7fung", "dei\u00b7ner", "All\u00b7macht", "nur", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Wer in des Lebens Buch gelesen,", "tokens": ["Wer", "in", "des", "Le\u00b7bens", "Buch", "ge\u00b7le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "ihn trifft ein Hauch von deinem Wesen,", "tokens": ["ihn", "trifft", "ein", "Hauch", "von", "dei\u00b7nem", "We\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "aus jedem Worte, jedem Blatt", "tokens": ["aus", "je\u00b7dem", "Wor\u00b7te", ",", "je\u00b7dem", "Blatt"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PIAT", "NN", "$,", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "lehrt tiefe Weisheit alle Geister \u2013", "tokens": ["lehrt", "tie\u00b7fe", "Weis\u00b7heit", "al\u00b7le", "Geis\u00b7ter", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "PIAT", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "doch meine Seele rief den Meister,", "tokens": ["doch", "mei\u00b7ne", "See\u00b7le", "rief", "den", "Meis\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "der dieses Buch geschrieben hat . . .", "tokens": ["der", "die\u00b7ses", "Buch", "ge\u00b7schrie\u00b7ben", "hat", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ART", "PDAT", "NN", "VVPP", "VAFIN", "$.", "$.", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Und schrie umsonst. Ihr zitternd Rufen", "tokens": ["Und", "schrie", "um\u00b7sonst", ".", "Ihr", "zit\u00b7ternd", "Ru\u00b7fen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "$.", "PPER", "VVPP", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "verhallte an den Altarstufen", "tokens": ["ver\u00b7hall\u00b7te", "an", "den", "Al\u00b7tar\u00b7stu\u00b7fen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "der Gottheit, die mein Haupt gebar.", "tokens": ["der", "Got\u00b7theit", ",", "die", "mein", "Haupt", "ge\u00b7bar", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da rang in Schmerz und Todesschauern", "tokens": ["Da", "rang", "in", "Schmerz", "und", "To\u00b7des\u00b7schau\u00b7ern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "aus festgef\u00fcgten Kerkermauern", "tokens": ["aus", "fest\u00b7ge\u00b7f\u00fcg\u00b7ten", "Ker\u00b7ker\u00b7mau\u00b7ern"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "der Selbstsucht sich mein Wesen klar.", "tokens": ["der", "Selbst\u00b7sucht", "sich", "mein", "We\u00b7sen", "klar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Ich sah bis auf den Grund der Erde;", "tokens": ["Ich", "sah", "bis", "auf", "den", "Grund", "der", "Er\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+