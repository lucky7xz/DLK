{"dta.poem.3263": {"metadata": {"author": {"name": "Uz, Johann Peter", "birth": "N.A.", "death": "N.A."}, "title": "An Herrn Secret\u00e4r Gleim.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1755", "urn": "urn:nbn:de:kobv:b4-20614-6", "language": ["de:0.99"], "booktitle": "Uz, Johann Peter: Lyrische und andere Gedichte. 2. Aufl. Ansbach, 1755."}, "poem": {"stanza.1": {"line.1": {"text": "Mein Gleim, der in begl\u00fccktrer Luft", "tokens": ["Mein", "Gleim", ",", "der", "in", "be\u00b7gl\u00fcck\u00b7trer", "Luft"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mich halben Wilden oft bedauert,", "tokens": ["Mich", "hal\u00b7ben", "Wil\u00b7den", "oft", "be\u00b7dau\u00b7ert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Mich oft aus dieser W\u00fcste ruft,", "tokens": ["Mich", "oft", "aus", "die\u00b7ser", "W\u00fcs\u00b7te", "ruft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "PDAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wo noch mein Saitenspiel an d\u00fcrren", "tokens": ["Wo", "noch", "mein", "Sai\u00b7ten\u00b7spiel", "an", "d\u00fcr\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "PPOSAT", "NN", "APPR", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Str\u00e4uchen trauert!", "tokens": ["Str\u00e4u\u00b7chen", "trau\u00b7ert", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.2": {"line.1": {"text": "Wie reizet mich der Musen Ruhm,", "tokens": ["Wie", "rei\u00b7zet", "mich", "der", "Mu\u00b7sen", "Ruhm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die um die stolze Spree erwachen,", "tokens": ["Die", "um", "die", "stol\u00b7ze", "Spree", "er\u00b7wa\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wo ihr verfallnes Heiligthum", "tokens": ["Wo", "ihr", "ver\u00b7fall\u00b7nes", "Hei\u00b7lig\u00b7thum"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit neuem Glanze strahlt, und Rosen ihnen lachen!", "tokens": ["Mit", "neu\u00b7em", "Glan\u00b7ze", "strahlt", ",", "und", "Ro\u00b7sen", "ih\u00b7nen", "la\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,", "KON", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Denn h\u00f6re, was dein Freund hievon,", "tokens": ["Denn", "h\u00f6\u00b7re", ",", "was", "dein", "Freund", "hie\u00b7von", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PRELS", "PPOSAT", "NN", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bey dieses Gl\u00fcckes Anbruch, h\u00f6rte,", "tokens": ["Bey", "die\u00b7ses", "Gl\u00fc\u00b7ckes", "An\u00b7bruch", ",", "h\u00f6r\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Am bluhmenvollen Helicon,", "tokens": ["Am", "bluh\u00b7men\u00b7vol\u00b7len", "He\u00b7li\u00b7con", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als tief im Lorbeerwald ihn Pindar einsam lehrte.", "tokens": ["Als", "tief", "im", "Lor\u00b7beer\u00b7wald", "ihn", "Pin\u00b7dar", "ein\u00b7sam", "lehr\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "APPRART", "NN", "PPER", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Den Hayn durchflog ein Lustgesang;", "tokens": ["Den", "Hayn", "durch\u00b7flog", "ein", "Lust\u00b7ge\u00b7sang", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die heilge Stille wich von hinnen:", "tokens": ["Die", "heil\u00b7ge", "Stil\u00b7le", "wich", "von", "hin\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ich sah, indem ich n\u00e4her drang,", "tokens": ["Ich", "sah", ",", "in\u00b7dem", "ich", "n\u00e4\u00b7her", "drang", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich sah den Musengott und alle Pierinnen.", "tokens": ["Ich", "sah", "den", "Mu\u00b7sen\u00b7gott", "und", "al\u00b7le", "Pie\u00b7rin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "KON", "PIAT", "NN", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.5": {"line.1": {"text": "Sie sungen voll zufriedner Lust;", "tokens": ["Sie", "sun\u00b7gen", "voll", "zu\u00b7fried\u00b7ner", "Lust", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der necktarvolle Becher gl\u00e4nzte;", "tokens": ["Der", "neck\u00b7tar\u00b7vol\u00b7le", "Be\u00b7cher", "gl\u00e4nz\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Es reichten ihn, mit nackter Brust,", "tokens": ["Es", "reich\u00b7ten", "ihn", ",", "mit", "nack\u00b7ter", "Brust", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die jungen Grazien, die Ros\u2019 und Myrth umkr\u00e4nzte.", "tokens": ["Die", "jun\u00b7gen", "Gra\u00b7zi\u00b7en", ",", "die", "Ros'", "und", "Myrth", "um\u00b7kr\u00e4nz\u00b7te", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Bald schlossen Alle Hand in Hand;", "tokens": ["Bald", "schlos\u00b7sen", "Al\u00b7le", "Hand", "in", "Hand", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Reihentanz ward angefangen:", "tokens": ["Ein", "Rei\u00b7hen\u00b7tanz", "ward", "an\u00b7ge\u00b7fan\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Da flo\u00df ihr unbewahrt Gewand", "tokens": ["Da", "flo\u00df", "ihr", "un\u00b7be\u00b7wahrt", "Ge\u00b7wand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In Thau und Bluhmen hin; es brannten ihre Wangen.", "tokens": ["In", "Thau", "und", "Bluh\u00b7men", "hin", ";", "es", "brann\u00b7ten", "ih\u00b7re", "Wan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "PTKVZ", "$.", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Mit Recht war iede Muse froh:", "tokens": ["Mit", "Recht", "war", "ie\u00b7de", "Mu\u00b7se", "froh", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PIAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dein K\u00f6nig hie\u00df die Waffen schweigen.", "tokens": ["Dein", "K\u00f6\u00b7nig", "hie\u00df", "die", "Waf\u00b7fen", "schwei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wer hoffte nicht, als Mavors floh,", "tokens": ["Wer", "hoff\u00b7te", "nicht", ",", "als", "Ma\u00b7vors", "floh", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PTKNEG", "$,", "KOUS", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nun w\u00fcrde Friedrichs Huld sich zu den Musen neigen?", "tokens": ["Nun", "w\u00fcr\u00b7de", "Fried\u00b7richs", "Huld", "sich", "zu", "den", "Mu\u00b7sen", "nei\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NE", "NN", "PRF", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Und gleich lud Fama, froh erhitzt,", "tokens": ["Und", "gleich", "lud", "Fa\u00b7ma", ",", "froh", "er\u00b7hitzt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "NN", "$,", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie nach Berlins gew\u00fcnschten Auen:", "tokens": ["Sie", "nach", "Ber\u00b7lins", "ge\u00b7w\u00fcnschten", "Au\u00b7en", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NE", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Dort, Musen! sprach sie, sollt ihr itzt", "tokens": ["Dort", ",", "Mu\u00b7sen", "!", "sprach", "sie", ",", "sollt", "ihr", "itzt"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "$,", "NN", "$.", "VVFIN", "PPER", "$,", "VMFIN", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Athen zum andernmal im alten Flore schauen.", "tokens": ["A\u00b7then", "zum", "an\u00b7dern\u00b7mal", "im", "al\u00b7ten", "Flo\u00b7re", "schau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "ADV", "APPRART", "ADJA", "NN", "VVINF", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}}, "stanza.9": {"line.1": {"text": "Sie sprach und floh; und Ph\u00f6bus fiel", "tokens": ["Sie", "sprach", "und", "floh", ";", "und", "Ph\u00f6\u00b7bus", "fiel"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "$.", "KON", "NE", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit rascher Hand in seine Saiten:", "tokens": ["Mit", "ra\u00b7scher", "Hand", "in", "sei\u00b7ne", "Sai\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Er sang und lie\u00df sein Saitenspiel,", "tokens": ["Er", "sang", "und", "lie\u00df", "sein", "Sai\u00b7ten\u00b7spiel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Voll Necktars und voll Lust, sein g\u00f6ttlich Lied begleiten.", "tokens": ["Voll", "Neck\u00b7tars", "und", "voll", "Lust", ",", "sein", "g\u00f6tt\u00b7lich", "Lied", "be\u00b7glei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "KON", "ADJD", "NN", "$,", "PPOSAT", "ADJD", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.10": {"line.1": {"text": "Begl\u00fccktes Reich! der L\u00e4nder Zier!", "tokens": ["Be\u00b7gl\u00fcck\u00b7tes", "Reich", "!", "der", "L\u00e4n\u00b7der", "Zier", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Brach Ph\u00f6bus aus; und alles lauschte:", "tokens": ["Brach", "Ph\u00f6\u00b7bus", "aus", ";", "und", "al\u00b7les", "lauschte", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "PTKVZ", "$.", "KON", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es schwieg das l\u00fcsterne Revier;", "tokens": ["Es", "schwieg", "das", "l\u00fcs\u00b7ter\u00b7ne", "Re\u00b7vier", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Es schwieg der laute West, der in den Lorbeern rauschte.", "tokens": ["Es", "schwieg", "der", "lau\u00b7te", "West", ",", "der", "in", "den", "Lor\u00b7beern", "rauschte", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.11": {"line.1": {"text": "Ja! fuhr er fort, begl\u00fccktes Reich,", "tokens": ["Ja", "!", "fuhr", "er", "fort", ",", "be\u00b7gl\u00fcck\u00b7tes", "Reich", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKANT", "$.", "VVFIN", "PPER", "PTKVZ", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wo Friedrich herrscht, wie V\u00e4ter pflegen,", "tokens": ["Wo", "Fried\u00b7rich", "herrscht", ",", "wie", "V\u00e4\u00b7ter", "pfle\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "VVFIN", "$,", "PWAV", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Gleich gro\u00df und stets Minerven gleich,", "tokens": ["Gleich", "gro\u00df", "und", "stets", "Mi\u00b7ner\u00b7ven", "gleich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADV", "NN", "ADV", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Es schwinge seine Faust den Oelzweig oder Degen!", "tokens": ["Es", "schwin\u00b7ge", "sei\u00b7ne", "Faust", "den", "O\u00b7el\u00b7zweig", "o\u00b7der", "De\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "ART", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}}, "stanza.12": {"line.1": {"text": "Ich seh ihn! welch ein k\u00fchner Held!", "tokens": ["Ich", "seh", "ihn", "!", "welch", "ein", "k\u00fch\u00b7ner", "Held", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$.", "PWAT", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der schnelle Sieg fliegt ihm zur Seite.", "tokens": ["Der", "schnel\u00b7le", "Sieg", "fliegt", "ihm", "zur", "Sei\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "So kommt der Kriegsgott aus dem Feld;", "tokens": ["So", "kommt", "der", "Kriegs\u00b7gott", "aus", "dem", "Feld", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So furchtbar gl\u00fcht sein Blick, entflam\u0303t vom wilden Streite!", "tokens": ["So", "furcht\u00b7bar", "gl\u00fcht", "sein", "Blick", ",", "ent\u00b7flam\u0303t", "vom", "wil\u00b7den", "Strei\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "PPOSAT", "NN", "$,", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.13": {"line.1": {"text": "Doch Friedrich will geliebet seyn:", "tokens": ["Doch", "Fried\u00b7rich", "will", "ge\u00b7lie\u00b7bet", "seyn", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VMFIN", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er wird bald m\u00fcde, stets zu schrecken;", "tokens": ["Er", "wird", "bald", "m\u00fc\u00b7de", ",", "stets", "zu", "schre\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$,", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und h\u00e4ngt im nahen Palmenhayn", "tokens": ["Und", "h\u00e4ngt", "im", "na\u00b7hen", "Pal\u00b7men\u00b7hayn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die g\u00fcldnen Waffen auf, die Staub und Blut dedecken:", "tokens": ["Die", "g\u00fcld\u00b7nen", "Waf\u00b7fen", "auf", ",", "die", "Staub", "und", "Blut", "de\u00b7de\u00b7cken", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$,", "ART", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.14": {"line.1": {"text": "Und wirft sich, da der Sieg ihm lacht,", "tokens": ["Und", "wirft", "sich", ",", "da", "der", "Sieg", "ihm", "lacht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "$,", "KOUS", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dem Frieden in die holden Arme,", "tokens": ["Dem", "Frie\u00b7den", "in", "die", "hol\u00b7den", "Ar\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Da neben ihm die Weisheit lacht,", "tokens": ["Da", "ne\u00b7ben", "ihm", "die", "Weis\u00b7heit", "lacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Voll Glanzes und umringt von kluger Freuden Schwarme.", "tokens": ["Voll", "Glan\u00b7zes", "und", "um\u00b7ringt", "von", "klu\u00b7ger", "Freu\u00b7den", "Schwar\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "KON", "VVFIN", "APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.15": {"line.1": {"text": "Wie wird nunmehr die g\u00fcldne Zeit", "tokens": ["Wie", "wird", "nun\u00b7mehr", "die", "g\u00fcld\u00b7ne", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In seinen Staaten sich verj\u00fcngen,", "tokens": ["In", "sei\u00b7nen", "Staa\u00b7ten", "sich", "ver\u00b7j\u00fcn\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und \u00fcberall Zufriedenheit", "tokens": ["Und", "\u00fc\u00b7be\u00b7rall", "Zu\u00b7frie\u00b7den\u00b7heit"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADV", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und reicher Ueberflu\u00df die sichren Fl\u00fcgel schwingen!", "tokens": ["Und", "rei\u00b7cher", "Ue\u00b7berf\u00b7lu\u00df", "die", "sich\u00b7ren", "Fl\u00fc\u00b7gel", "schwin\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "NN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.16": {"line.1": {"text": "Drum eilt auch ihr an Friedrichs Brust,", "tokens": ["Drum", "eilt", "auch", "ihr", "an", "Fried\u00b7richs", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ADV", "PPER", "APPR", "NE", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Jhr Musen, mit dem \u00e4chten Witze!", "tokens": ["Ihr", "Mu\u00b7sen", ",", "mit", "dem", "\u00e4ch\u00b7ten", "Wit\u00b7ze", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Er winket euch! seyd seine Lust,", "tokens": ["Er", "win\u00b7ket", "euch", "!", "seyd", "sei\u00b7ne", "Lust", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$.", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und weicht hinfort nicht mehr vom k\u00f6niglichen Sitze:", "tokens": ["Und", "weicht", "hin\u00b7fort", "nicht", "mehr", "vom", "k\u00f6\u00b7nig\u00b7li\u00b7chen", "Sit\u00b7ze", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PTKNEG", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.17": {"line.1": {"text": "Und lehrt am ewigen Berlin,", "tokens": ["Und", "lehrt", "am", "e\u00b7wi\u00b7gen", "Ber\u00b7lin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "ADJA", "NE", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Auf das die Welt bewundernd schauet,", "tokens": ["Auf", "das", "die", "Welt", "be\u00b7wun\u00b7dernd", "schau\u00b7et", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "ART", "NN", "VVPP", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wie herrlich alle K\u00fcnste bl\u00fchn,", "tokens": ["Wie", "herr\u00b7lich", "al\u00b7le", "K\u00fcns\u00b7te", "bl\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn ein Monarch sie pflegt, und Gnade sie bethauet.", "tokens": ["Wenn", "ein", "Mon\u00b7arch", "sie", "pflegt", ",", "und", "Gna\u00b7de", "sie", "be\u00b7thau\u00b7et", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "VVFIN", "$,", "KON", "NN", "PPER", "VVFIN", "$."], "meter": "-++--+-+-+-+-", "measure": "iambic.hexa.relaxed"}}}}}