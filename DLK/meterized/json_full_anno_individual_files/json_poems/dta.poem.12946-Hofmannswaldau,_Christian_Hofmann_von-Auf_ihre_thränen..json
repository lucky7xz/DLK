{"dta.poem.12946": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auf ihre thr\u00e4nen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Aurora weint, und Sylvia weint auch:", "tokens": ["Au\u00b7ro\u00b7ra", "weint", ",", "und", "Syl\u00b7via", "weint", "auch", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "KON", "NE", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Doch gehen dieser ihre thr\u00e4nen,", "tokens": ["Doch", "ge\u00b7hen", "die\u00b7ser", "ih\u00b7re", "thr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PDAT", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "So ein verborgner schmertz ihr aus dem hertzen dr\u00fcckt,", "tokens": ["So", "ein", "ver\u00b7borg\u00b7ner", "schmertz", "ihr", "aus", "dem", "hert\u00b7zen", "dr\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "An kostbarkeit weit, weit vor jenen,", "tokens": ["An", "kost\u00b7bar\u00b7keit", "weit", ",", "weit", "vor", "je\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "$,", "ADJD", "APPR", "PDAT", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ob ihre krafft gleich wald und feld erqvickt.", "tokens": ["Ob", "ih\u00b7re", "krafft", "gleich", "wald", "und", "feld", "er\u00b7qvickt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "ADV", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Glaubst du es nicht, Aurora! auf mein wort,", "tokens": ["Glaubst", "du", "es", "nicht", ",", "Au\u00b7ro\u00b7ra", "!", "auf", "mein", "wort", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "PTKNEG", "$,", "NE", "$.", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "So schicke nur dein aug\u2019 an diesen werthen ort,", "tokens": ["So", "schi\u00b7cke", "nur", "dein", "aug'", "an", "die\u00b7sen", "wert\u00b7hen", "ort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PPOSAT", "NN", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So wirst du sehn, da\u00df deine reinen z\u00e4hren", "tokens": ["So", "wirst", "du", "sehn", ",", "da\u00df", "dei\u00b7ne", "rei\u00b7nen", "z\u00e4h\u00b7ren"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "VVINF", "$,", "KOUS", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Sich nur in thau verkehren,", "tokens": ["Sich", "nur", "in", "thau", "ver\u00b7keh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "APPR", "NE", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Da das, was Sylvien aus ihren augen rinnt,", "tokens": ["Da", "das", ",", "was", "Syl\u00b7vi\u00b7en", "aus", "ih\u00b7ren", "au\u00b7gen", "rinnt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "$,", "PRELS", "NE", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Die allersch\u00f6nsten perlen sind.", "tokens": ["Die", "al\u00b7ler\u00b7sch\u00f6ns\u00b7ten", "per\u00b7len", "sind", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}