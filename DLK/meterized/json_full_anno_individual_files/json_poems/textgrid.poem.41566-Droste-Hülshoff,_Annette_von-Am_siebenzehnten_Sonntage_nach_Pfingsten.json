etra"}}, "stanza.13": {"line.1": {"text": "Dann will ich dir aus freier Brust", "tokens": ["Dann", "will", "ich", "dir", "aus", "frei\u00b7er", "Brust"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein \u00fcberselig Loblied singen,", "tokens": ["Ein", "\u00fc\u00b7ber\u00b7se\u00b7lig", "Lob\u00b7lied", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und wieder soll in Gotteslust", "tokens": ["Und", "wie\u00b7der", "soll", "in", "Got\u00b7tes\u00b7lust"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VMFIN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie fr\u00fcher meine Stimme klingen.", "tokens": ["Wie", "fr\u00fc\u00b7her", "mei\u00b7ne", "Stim\u00b7me", "klin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ist sie gebrochen jetzt und matt,", "tokens": ["Ist", "sie", "ge\u00b7bro\u00b7chen", "jetzt", "und", "matt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "ADV", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Du bist es, der die Mittel hat,", "tokens": ["Du", "bist", "es", ",", "der", "die", "Mit\u00b7tel", "hat", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "$,", "PRELS", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "So in die kr\u00e4nksten Adern dringen.", "tokens": ["So", "in", "die", "kr\u00e4nks\u00b7ten", "A\u00b7dern", "drin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.14": {"line.1": {"text": "F\u00fchl' ich doch heut in mir erweckt", "tokens": ["F\u00fchl'", "ich", "doch", "heut", "in", "mir", "er\u00b7weckt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ADV", "ADV", "APPR", "PPER", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein lang entschwundenes Vertrauen,", "tokens": ["Ein", "lang", "ent\u00b7schwun\u00b7de\u00b7nes", "Ver\u00b7trau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df mich nicht Tod noch S\u00fcnde schreckt:", "tokens": ["Da\u00df", "mich", "nicht", "Tod", "noch", "S\u00fcn\u00b7de", "schreckt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "NN", "ADV", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie sollt' ich denn auf dich nicht bauen!", "tokens": ["Wie", "sollt'", "ich", "denn", "auf", "dich", "nicht", "bau\u00b7en", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "ADV", "APPR", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ja, wenn du willst, so kann ich doch", "tokens": ["Ja", ",", "wenn", "du", "willst", ",", "so", "kann", "ich", "doch"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "KOUS", "PPER", "VMFIN", "$,", "ADV", "VMFIN", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mit diesen meinen Augen noch", "tokens": ["Mit", "die\u00b7sen", "mei\u00b7nen", "Au\u00b7gen", "noch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "PPOSAT", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "In diesem meinem Leib dich schauen.", "tokens": ["In", "die\u00b7sem", "mei\u00b7nem", "Leib", "dich", "schau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "PPOSAT", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.15": {"line.1": {"text": "Ich wei\u00df es, da\u00df von mir nicht stammt", "tokens": ["Ich", "wei\u00df", "es", ",", "da\u00df", "von", "mir", "nicht", "stammt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$,", "KOUS", "APPR", "PPER", "PTKNEG", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was mich so freudig mu\u00df durchzittern;", "tokens": ["Was", "mich", "so", "freu\u00b7dig", "mu\u00df", "durch\u00b7zit\u00b7tern", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "ADJD", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Strahl ist es, den du entflammt,", "tokens": ["Ein", "Strahl", "ist", "es", ",", "den", "du", "ent\u00b7flammt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Traum, den Starren zu ersch\u00fcttern.", "tokens": ["Ein", "Traum", ",", "den", "Star\u00b7ren", "zu", "er\u00b7sch\u00fct\u00b7tern", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "O fahre fort, o r\u00fchr' mich an,", "tokens": ["O", "fah\u00b7re", "fort", ",", "o", "r\u00fchr'", "mich", "an", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PTKVZ", "$,", "FM", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "O brich den Todesschlaf, und dann,", "tokens": ["O", "brich", "den", "To\u00b7des\u00b7schlaf", ",", "und", "dann", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "$,", "KON", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Dann werd' ich Morgenl\u00fcfte wittern!", "tokens": ["Dann", "werd'", "ich", "Mor\u00b7gen\u00b7l\u00fcf\u00b7te", "wit\u00b7tern", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.16": {"line.1": {"text": "Hast du gesprochen: \u00bbWeine nicht!\u00ab,", "tokens": ["Hast", "du", "ge\u00b7spro\u00b7chen", ":", "\u00bb", "Wei\u00b7ne", "nicht", "!", "\u00ab", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "punct", "punct", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "$.", "$(", "NN", "PTKNEG", "$.", "$(", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du wei\u00dft, da\u00df nicht die Toten weinen,", "tokens": ["Du", "wei\u00dft", ",", "da\u00df", "nicht", "die", "To\u00b7ten", "wei\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PTKNEG", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ob schier im Traum das Herze bricht,", "tokens": ["Ob", "schier", "im", "Traum", "das", "Her\u00b7ze", "bricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "APPRART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und wohl Gebet' dir Seufzer scheinen,", "tokens": ["Und", "wohl", "Ge\u00b7bet'", "dir", "Seuf\u00b7zer", "schei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "NN", "PPER", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die fl\u00fcstern m\u00f6chten schwach und lind:", "tokens": ["Die", "fl\u00fcs\u00b7tern", "m\u00f6ch\u00b7ten", "schwach", "und", "lind", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VVINF", "VMFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "\u00bbdu hast geweckt der Witwe Kind,", "tokens": ["\u00bb", "du", "hast", "ge\u00b7weckt", "der", "Wit\u00b7we", "Kind", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "VVPP", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ich liege noch in Totenleinen!\u00ab", "tokens": ["Ich", "lie\u00b7ge", "noch", "in", "To\u00b7ten\u00b7lei\u00b7nen", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "NN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}