{"textgrid.poem.41566": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Am siebenzehnten Sonntage nach Pfingsten", "genre": "verse", "period": "N.A.", "pub_year": 1822, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn deine Hand den Sarg ber\u00fchrt,", "tokens": ["Wenn", "dei\u00b7ne", "Hand", "den", "Sarg", "be\u00b7r\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dann mu\u00df der Tote sich beleben,", "tokens": ["Dann", "mu\u00df", "der", "To\u00b7te", "sich", "be\u00b7le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Dein Hauch die Wetterwolke f\u00fchrt,", "tokens": ["Dein", "Hauch", "die", "Wet\u00b7ter\u00b7wol\u00b7ke", "f\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dann mu\u00df sie milden Manna geben.", "tokens": ["Dann", "mu\u00df", "sie", "mil\u00b7den", "Man\u00b7na", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Du der aus Felsen Labung zieht,", "tokens": ["Du", "der", "aus", "Fel\u00b7sen", "La\u00b7bung", "zieht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "APPR", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dem Aarons d\u00fcrrer Stab gebl\u00fcht,", "tokens": ["Dem", "Aa\u00b7rons", "d\u00fcr\u00b7rer", "Stab", "ge\u00b7bl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Des Niles Fluten sich erheben:", "tokens": ["Des", "Ni\u00b7les", "Flu\u00b7ten", "sich", "er\u00b7he\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Der M\u00e4chtige bist du, um auch", "tokens": ["Der", "M\u00e4ch\u00b7ti\u00b7ge", "bist", "du", ",", "um", "auch"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PPER", "$,", "KOUI", "ADV"], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.2": {"text": "Der Seele dumpfen Schlaf zu enden;", "tokens": ["Der", "See\u00b7le", "dum\u00b7pfen", "Schlaf", "zu", "en\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Zu dir darf seinen Sterbehauch", "tokens": ["Zu", "dir", "darf", "sei\u00b7nen", "Ster\u00b7be\u00b7hauch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "VMFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der todeswunde Sch\u00e4cher senden;", "tokens": ["Der", "to\u00b7des\u00b7wun\u00b7de", "Sch\u00e4\u00b7cher", "sen\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Du nimmst den letzten Atemzug,", "tokens": ["Du", "nimmst", "den", "letz\u00b7ten", "A\u00b7tem\u00b7zug", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ein Reuelaut ist dir genug,", "tokens": ["Ein", "Reu\u00b7e\u00b7laut", "ist", "dir", "ge\u00b7nug", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Den Blitz in seinem Flug zu wenden.", "tokens": ["Den", "Blitz", "in", "sei\u00b7nem", "Flug", "zu", "wen\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Du hast dich an das Tor gestellt", "tokens": ["Du", "hast", "dich", "an", "das", "Tor", "ge\u00b7stellt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "APPR", "ART", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den Sohn der Witwe zu erwarten,", "tokens": ["Den", "Sohn", "der", "Wit\u00b7we", "zu", "er\u00b7war\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und hast, ein Herr der ganzen Welt,", "tokens": ["Und", "hast", ",", "ein", "Herr", "der", "gan\u00b7zen", "Welt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "$,", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Beachtet ihren kleinen Garten.", "tokens": ["Be\u00b7ach\u00b7tet", "ih\u00b7ren", "klei\u00b7nen", "Gar\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Du der das kranke Rohr nicht knickt,", "tokens": ["Du", "der", "das", "kran\u00b7ke", "Rohr", "nicht", "knickt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ART", "ADJA", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Am Docht das F\u00fcnkchen nicht zerdr\u00fcckt,", "tokens": ["Am", "Docht", "das", "F\u00fcnk\u00b7chen", "nicht", "zer\u00b7dr\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und nie gebrochen hat die Scharten,", "tokens": ["Und", "nie", "ge\u00b7bro\u00b7chen", "hat", "die", "Schar\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVPP", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ber\u00fchre mich, denn ich bin tot", "tokens": ["Be\u00b7r\u00fch\u00b7re", "mich", ",", "denn", "ich", "bin", "tot"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "KON", "PPER", "VAFIN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und meine Werke sind nur Leichen!", "tokens": ["Und", "mei\u00b7ne", "Wer\u00b7ke", "sind", "nur", "Lei\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ADV", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Hauch' \u00fcber mich; denn blutig rot", "tokens": ["Hauch'", "\u00fc\u00b7ber", "mich", ";", "denn", "blu\u00b7tig", "rot"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "APPR", "PPER", "$.", "KON", "ADJD", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die S\u00fcnde lie\u00df mir ihre Zeichen!", "tokens": ["Die", "S\u00fcn\u00b7de", "lie\u00df", "mir", "ih\u00b7re", "Zei\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "O wende du den Donnerschlag,", "tokens": ["O", "wen\u00b7de", "du", "den", "Don\u00b7ner\u00b7schlag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der \u00fcber meinem Haupte brach,", "tokens": ["Der", "\u00fc\u00b7ber", "mei\u00b7nem", "Haup\u00b7te", "brach", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und la\u00df die dumpfen Nebel weichen!", "tokens": ["Und", "la\u00df", "die", "dum\u00b7pfen", "Ne\u00b7bel", "wei\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Dann will ich dir aus freier Brust", "tokens": ["Dann", "will", "ich", "dir", "aus", "frei\u00b7er", "Brust"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein \u00fcberselig Loblied singen,", "tokens": ["Ein", "\u00fc\u00b7ber\u00b7se\u00b7lig", "Lob\u00b7lied", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und wieder soll in Gotteslust", "tokens": ["Und", "wie\u00b7der", "soll", "in", "Got\u00b7tes\u00b7lust"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VMFIN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie fr\u00fcher meine Stimme klingen.", "tokens": ["Wie", "fr\u00fc\u00b7her", "mei\u00b7ne", "Stim\u00b7me", "klin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ist sie gebrochen jetzt und matt,", "tokens": ["Ist", "sie", "ge\u00b7bro\u00b7chen", "jetzt", "und", "matt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "ADV", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Du bist es, der die Mittel hat,", "tokens": ["Du", "bist", "es", ",", "der", "die", "Mit\u00b7tel", "hat", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "$,", "PRELS", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "So in die kr\u00e4nksten Adern dringen.", "tokens": ["So", "in", "die", "kr\u00e4nks\u00b7ten", "A\u00b7dern", "drin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "F\u00fchl' ich doch heut in mir erweckt", "tokens": ["F\u00fchl'", "ich", "doch", "heut", "in", "mir", "er\u00b7weckt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ADV", "ADV", "APPR", "PPER", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein lang entschwundenes Vertrauen,", "tokens": ["Ein", "lang", "ent\u00b7schwun\u00b7de\u00b7nes", "Ver\u00b7trau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df mich nicht Tod noch S\u00fcnde schreckt:", "tokens": ["Da\u00df", "mich", "nicht", "Tod", "noch", "S\u00fcn\u00b7de", "schreckt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "NN", "ADV", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie sollt' ich denn auf dich nicht bauen!", "tokens": ["Wie", "sollt'", "ich", "denn", "auf", "dich", "nicht", "bau\u00b7en", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "ADV", "APPR", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ja, wenn du willst, so kann ich doch", "tokens": ["Ja", ",", "wenn", "du", "willst", ",", "so", "kann", "ich", "doch"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "KOUS", "PPER", "VMFIN", "$,", "ADV", "VMFIN", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mit diesen meinen Augen noch", "tokens": ["Mit", "die\u00b7sen", "mei\u00b7nen", "Au\u00b7gen", "noch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "PPOSAT", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "In diesem meinem Leib dich schauen.", "tokens": ["In", "die\u00b7sem", "mei\u00b7nem", "Leib", "dich", "schau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "PPOSAT", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Ich wei\u00df es, da\u00df von mir nicht stammt", "tokens": ["Ich", "wei\u00df", "es", ",", "da\u00df", "von", "mir", "nicht", "stammt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$,", "KOUS", "APPR", "PPER", "PTKNEG", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was mich so freudig mu\u00df durchzittern;", "tokens": ["Was", "mich", "so", "freu\u00b7dig", "mu\u00df", "durch\u00b7zit\u00b7tern", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "ADJD", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Strahl ist es, den du entflammt,", "tokens": ["Ein", "Strahl", "ist", "es", ",", "den", "du", "ent\u00b7flammt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Traum, den Starren zu ersch\u00fcttern.", "tokens": ["Ein", "Traum", ",", "den", "Star\u00b7ren", "zu", "er\u00b7sch\u00fct\u00b7tern", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "O fahre fort, o r\u00fchr' mich an,", "tokens": ["O", "fah\u00b7re", "fort", ",", "o", "r\u00fchr'", "mich", "an", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PTKVZ", "$,", "FM", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "O brich den Todesschlaf, und dann,", "tokens": ["O", "brich", "den", "To\u00b7des\u00b7schlaf", ",", "und", "dann", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "$,", "KON", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Dann werd' ich Morgenl\u00fcfte wittern!", "tokens": ["Dann", "werd'", "ich", "Mor\u00b7gen\u00b7l\u00fcf\u00b7te", "wit\u00b7tern", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Hast du gesprochen: \u00bbWeine nicht!\u00ab,", "tokens": ["Hast", "du", "ge\u00b7spro\u00b7chen", ":", "\u00bb", "Wei\u00b7ne", "nicht", "!", "\u00ab", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "punct", "punct", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "$.", "$(", "NN", "PTKNEG", "$.", "$(", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du wei\u00dft, da\u00df nicht die Toten weinen,", "tokens": ["Du", "wei\u00dft", ",", "da\u00df", "nicht", "die", "To\u00b7ten", "wei\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PTKNEG", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ob schier im Traum das Herze bricht,", "tokens": ["Ob", "schier", "im", "Traum", "das", "Her\u00b7ze", "bricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "APPRART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und wohl Gebet' dir Seufzer scheinen,", "tokens": ["Und", "wohl", "Ge\u00b7bet'", "dir", "Seuf\u00b7zer", "schei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "NN", "PPER", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die fl\u00fcstern m\u00f6chten schwach und lind:", 