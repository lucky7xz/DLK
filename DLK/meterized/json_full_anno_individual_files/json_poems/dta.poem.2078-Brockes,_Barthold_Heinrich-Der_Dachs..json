{"dta.poem.2078": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der Dachs.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie viele Thiere steile H\u00f6h'n, so lieben Dachse dunk-", "tokens": ["Wie", "vie\u00b7le", "Thie\u00b7re", "stei\u00b7le", "H\u00f6h'n", ",", "so", "lie\u00b7ben", "Dach\u00b7se", "dunk"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "PIAT", "NN", "ADJA", "NN", "$,", "ADV", "ADJA", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "le Tiefen", "tokens": ["le", "Tie\u00b7fen"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Und finstre L\u00f6cher in der Erde, worinn sie nicht des Tags", "tokens": ["Und", "finst\u00b7re", "L\u00f6\u00b7cher", "in", "der", "Er\u00b7de", ",", "wo\u00b7rinn", "sie", "nicht", "des", "Tags"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "NN", "APPR", "ART", "NN", "$,", "PWAV", "PPER", "PTKNEG", "ART", "NN"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "nur schliefen,", "tokens": ["nur", "schlie\u00b7fen", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVINF", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "Nein, worinn sie die halbe Zeit des Lebens selbst sich fast", "tokens": ["Nein", ",", "wo\u00b7rinn", "sie", "die", "hal\u00b7be", "Zeit", "des", "Le\u00b7bens", "selbst", "sich", "fast"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "PWAV", "PPER", "ART", "ADJA", "NN", "ART", "NN", "ADV", "PRF", "ADV"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.6": {"text": "begraben,", "tokens": ["be\u00b7gra\u00b7ben", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "Und ihre Lagerstatt darinn, so lang der Winter w\u00e4hret,", "tokens": ["Und", "ih\u00b7re", "La\u00b7ger\u00b7statt", "da\u00b7rinn", ",", "so", "lang", "der", "Win\u00b7ter", "w\u00e4h\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "PAV", "$,", "ADV", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.8": {"text": "haben:", "tokens": ["ha\u00b7ben", ":"], "token_info": ["word", "punct"], "pos": ["VAFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.9": {"text": "Dann schlafen sie die ganze Zeit, dann sollen sie von sich", "tokens": ["Dann", "schla\u00b7fen", "sie", "die", "gan\u00b7ze", "Zeit", ",", "dann", "sol\u00b7len", "sie", "von", "sich"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,", "ADV", "VMFIN", "PPER", "APPR", "PRF"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.10": {"text": "sich n\u00e4hren,", "tokens": ["sich", "n\u00e4h\u00b7ren", ","], "token_info": ["word", "word", "punct"], "pos": ["PRF", "VVINF", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.11": {"text": "Und blo\u00df von ihrem eignen K\u00f6rper und ihrem eignen Fet-", "tokens": ["Und", "blo\u00df", "von", "ih\u00b7rem", "eig\u00b7nen", "K\u00f6r\u00b7per", "und", "ih\u00b7rem", "eig\u00b7nen", "Fet"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "PPOSAT", "ADJA", "NN", "KON", "PPOSAT", "ADJA", "TRUNC"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.12": {"text": "te zehren.", "tokens": ["te", "zeh\u00b7ren", "."], "token_info": ["word", "word", "punct"], "pos": ["NE", "VVFIN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.13": {"text": "Sonst n\u00e4hret dieses Thier sich meist von Regenw\u00fcrmern,", "tokens": ["Sonst", "n\u00e4h\u00b7ret", "die\u00b7ses", "Thier", "sich", "meist", "von", "Re\u00b7gen\u00b7w\u00fcr\u00b7mern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PDAT", "NN", "PRF", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "K\u00e4fern, Schnecken,", "tokens": ["K\u00e4\u00b7fern", ",", "Schne\u00b7cken", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.15": {"text": "Von Molchen, Kr\u00f6ten, Fr\u00f6schen, M\u00e4usen und anderm", "tokens": ["Von", "Mol\u00b7chen", ",", "Kr\u00f6\u00b7ten", ",", "Fr\u00f6\u00b7schen", ",", "M\u00e4u\u00b7sen", "und", "an\u00b7derm"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NE", "$,", "NN", "$,", "NN", "$,", "NN", "KON", "PIS"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.16": {"text": "Ungeziefer mehr.", "tokens": ["Un\u00b7ge\u00b7zie\u00b7fer", "mehr", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "ADV", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.17": {"text": "Doch lassen sie auch junge Hasen, zusammt Kaninichen,", "tokens": ["Doch", "las\u00b7sen", "sie", "auch", "jun\u00b7ge", "Ha\u00b7sen", ",", "zu\u00b7sammt", "Ka\u00b7ni\u00b7ni\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADJA", "NN", "$,", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.18": {"text": "sich schmecken,", "tokens": ["sich", "schme\u00b7cken", ","], "token_info": ["word", "word", "punct"], "pos": ["PRF", "VVINF", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.19": {"text": "Auch junge V\u00f6gel sonderlich. Sie sind sehr fett, es wird", "tokens": ["Auch", "jun\u00b7ge", "V\u00f6\u00b7gel", "son\u00b7der\u00b7lich", ".", "Sie", "sind", "sehr", "fett", ",", "es", "wird"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ADJA", "NN", "ADJD", "$.", "PPER", "VAFIN", "ADV", "ADJD", "$,", "PPER", "VAFIN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.20": {"text": "das Schmeer", "tokens": ["das", "Schmeer"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+", "measure": "iambic.single"}, "line.21": {"text": "Als eine Haut herabgezogen, und n\u00fctzet sehr in Arzeneyen.", "tokens": ["Als", "ei\u00b7ne", "Haut", "her\u00b7ab\u00b7ge\u00b7zo\u00b7gen", ",", "und", "n\u00fct\u00b7zet", "sehr", "in", "Ar\u00b7ze\u00b7ne\u00b7yen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVPP", "$,", "KON", "VVFIN", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+--+-+-+--+", "measure": "iambic.octa.plus.relaxed"}, "line.22": {"text": "Des Fleisches hat man sich nicht minder, wenn es ge-", "tokens": ["Des", "Flei\u00b7sches", "hat", "man", "sich", "nicht", "min\u00b7der", ",", "wenn", "es", "ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PIS", "PRF", "PTKNEG", "ADV", "$,", "KOUS", "PPER", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "salzen, zu erfreuen.", "tokens": ["sal\u00b7zen", ",", "zu", "er\u00b7freu\u00b7en", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PTKZU", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.24": {"text": "Die H\u00e4ute, welche denen Sattlern, den T\u00e4schnern auch,", "tokens": ["Die", "H\u00e4u\u00b7te", ",", "wel\u00b7che", "de\u00b7nen", "Satt\u00b7lern", ",", "den", "T\u00e4schnern", "auch", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PDS", "NN", "$,", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.25": {"text": "zu Gute kommen,", "tokens": ["zu", "Gu\u00b7te", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.26": {"text": "Die werden viel zu Reisekasten, zu Pinseln wird das Haar,", "tokens": ["Die", "wer\u00b7den", "viel", "zu", "Rei\u00b7se\u00b7kas\u00b7ten", ",", "zu", "Pin\u00b7seln", "wird", "das", "Haar", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "APPR", "NN", "$,", "APPR", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.27": {"text": "genommen.", "tokens": ["ge\u00b7nom\u00b7men", "."], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.28": {"text": "So da\u00df selbst an dem faulen Dachs ein fromm- und bil-", "tokens": ["So", "da\u00df", "selbst", "an", "dem", "fau\u00b7len", "Dachs", "ein", "from\u00b7m", "und", "bil"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "ADV", "APPR", "ART", "ADJA", "NN", "ART", "TRUNC", "KON", "TRUNC"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.29": {"text": "liges Gem\u00fcth,", "tokens": ["li\u00b7ges", "Ge\u00b7m\u00fcth", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.30": {"text": "Wie sehr auch er den Menschen n\u00fctzt, mit Dank und mit", "tokens": ["Wie", "sehr", "auch", "er", "den", "Men\u00b7schen", "n\u00fctzt", ",", "mit", "Dank", "und", "mit"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ADV", "PPER", "ART", "NN", "VVFIN", "$,", "APPR", "NN", "KON", "APPR"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Bewundrung sieht.", "tokens": ["Be\u00b7wund\u00b7rung", "sieht", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}