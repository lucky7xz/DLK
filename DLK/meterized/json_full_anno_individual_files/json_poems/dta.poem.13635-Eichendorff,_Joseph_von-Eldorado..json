{"dta.poem.13635": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Eldorado.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Nun jeden Morgenschimmer", "tokens": ["Nun", "je\u00b7den", "Mor\u00b7gen\u00b7schim\u00b7mer"], "token_info": ["word", "word", "word"], "pos": ["ADV", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Steig' ich in's Bl\u00fcthenmeer,", "tokens": ["Steig'", "ich", "in's", "Bl\u00fc\u00b7then\u00b7meer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Bis ich Gl\u00fcckseel'ger nimmer", "tokens": ["Bis", "ich", "Gl\u00fcck\u00b7seel'\u00b7ger", "nim\u00b7mer"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "NN", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Von dorten wiederkehr'.", "tokens": ["Von", "dor\u00b7ten", "wie\u00b7der\u00b7kehr'", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}