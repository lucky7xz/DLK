{"textgrid.poem.49625": {"metadata": {"author": {"name": "Thoma, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Reichsrat Freiherr von Soden", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Herr von Soden ist ein bayrischer Pair;", "tokens": ["Der", "Herr", "von", "So\u00b7den", "ist", "ein", "bay\u00b7ri\u00b7scher", "Pair", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Er ist aus der Gegend von Ingolstadt her.", "tokens": ["Er", "ist", "aus", "der", "Ge\u00b7gend", "von", "In\u00b7gol\u00b7stadt", "her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ART", "NN", "APPR", "NE", "PTKVZ", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Papa und Gro\u00df- und Urgro\u00dfpapa,", "tokens": ["Pa\u00b7pa", "und", "Gro\u00df", "und", "Ur\u00b7gro\u00df\u00b7pa\u00b7pa", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "TRUNC", "KON", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Die waren alle von jeher da.", "tokens": ["Die", "wa\u00b7ren", "al\u00b7le", "von", "je\u00b7her", "da", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PIS", "APPR", "ADV", "ADV", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Die s\u00e4mtlichen Ahnen sind in dem Nest", "tokens": ["Die", "s\u00e4mt\u00b7li\u00b7chen", "Ah\u00b7nen", "sind", "in", "dem", "Nest"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN", "APPR", "ART", "NN"], "meter": "-+--+-++-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Als die Herren von Soden bekannt gewest.", "tokens": ["Als", "die", "Her\u00b7ren", "von", "So\u00b7den", "be\u00b7kannt", "ge\u00b7west", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPR", "NN", "ADJD", "VVPP", "$."], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Sonst hat man nichts von keinem gewi\u00dft,", "tokens": ["Sonst", "hat", "man", "nichts", "von", "kei\u00b7nem", "ge\u00b7wi\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "PIS", "APPR", "PIS", "VVPP", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Als da\u00df er Freiherr von Soden ist.", "tokens": ["Als", "da\u00df", "er", "Frei\u00b7herr", "von", "So\u00b7den", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "NN", "APPR", "NN", "VAFIN", "$."], "meter": "-+-++-+-+", "measure": "unknown.measure.penta"}}, "stanza.3": {"line.1": {"text": "Sie k\u00fcmmerten sich um die \u00d6konomie,", "tokens": ["Sie", "k\u00fcm\u00b7mer\u00b7ten", "sich", "um", "die", "\u00d6\u00b7kon\u00b7o\u00b7mie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+---+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Um Rosse, K\u00e4lber, Ochsen und K\u00fch',", "tokens": ["Um", "Ros\u00b7se", ",", "K\u00e4l\u00b7ber", ",", "Och\u00b7sen", "und", "K\u00fch'", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Sie waren recht fromm und sie