{"dta.poem.14759": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Der Gefangene .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Um ihn nun th\u00e4t sie schlagen", "tokens": ["Um", "ihn", "nun", "th\u00e4t", "sie", "schla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "PPER", "ADV", "VVFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Arme weich und blo\u00df,", "tokens": ["Die", "Ar\u00b7me", "weich", "und", "blo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "KON", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er konnte nichts mehr sagen,", "tokens": ["Er", "konn\u00b7te", "nichts", "mehr", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIS", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sie lie\u00df ihn nicht mehr los.", "tokens": ["Sie", "lie\u00df", "ihn", "nicht", "mehr", "los", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}