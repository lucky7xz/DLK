verscheinen,", "tokens": ["Mag", "die\u00b7ser", "Er\u00b7de", "Licht", "ver\u00b7schei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PDAT", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Mag diese Sonne untergehn,", "tokens": ["Mag", "die\u00b7se", "Son\u00b7ne", "un\u00b7ter\u00b7gehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PDAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ich werde selig mit den Deinen", "tokens": ["Ich", "wer\u00b7de", "se\u00b7lig", "mit", "den", "Dei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADJD", "APPR", "ART", "PPOSAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Lobsingend stehn auf h\u00f6hern H\u00f6hn.", "tokens": ["Lob\u00b7sin\u00b7gend", "stehn", "auf", "h\u00f6\u00b7hern", "H\u00f6hn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.14": {"line.1": {"text": "Ja, s\u00fc\u00dfer Heiland, mit den Deinen,", "tokens": ["Ja", ",", "s\u00fc\u00b7\u00dfer", "Hei\u00b7land", ",", "mit", "den", "Dei\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADJA", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sei auch ich unter Kleinsten klein \u2013", "tokens": ["Sei", "auch", "ich", "un\u00b7ter", "Kleins\u00b7ten", "klein", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPER", "APPR", "NN", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dein Licht wird ewig auf mir scheinen,", "tokens": ["Dein", "Licht", "wird", "e\u00b7wig", "auf", "mir", "schei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dein Glanz wird ewig bei mir sein.", "tokens": ["Dein", "Glanz", "wird", "e\u00b7wig", "bei", "mir", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "APPR", "PPER", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Hier gilt kein Zagen und kein Fragen,", "tokens": ["Hier", "gilt", "kein", "Za\u00b7gen", "und", "kein", "Fra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "KON", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Hier gilt: ", "tokens": ["Hier", "gilt", ":"], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}}}}}