{"textgrid.poem.59743": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "Abschiedslied", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schon dunkeln meine Lebenstage", "tokens": ["Schon", "dun\u00b7keln", "mei\u00b7ne", "Le\u00b7bens\u00b7ta\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADJA", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sich tief hinab zum Abendschein,", "tokens": ["Sich", "tief", "hin\u00b7ab", "zum", "A\u00b7bend\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und ernster fragt die gro\u00dfe Frage:", "tokens": ["Und", "erns\u00b7ter", "fragt", "die", "gro\u00b7\u00dfe", "Fra\u00b7ge", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Was bist du? Sprich: Was wirst du sein?", "tokens": ["Was", "bist", "du", "?", "Sprich", ":", "Was", "wirst", "du", "sein", "?"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "$.", "VVIMP", "$.", "PWS", "VAFIN", "PPER", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wie l\u00f6st das R\u00e4tsel deines Lebens", "tokens": ["Wie", "l\u00f6st", "das", "R\u00e4t\u00b7sel", "dei\u00b7nes", "Le\u00b7bens"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Sich hinter deinem Grabe auf?", "tokens": ["Sich", "hin\u00b7ter", "dei\u00b7nem", "Gra\u00b7be", "auf", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "War all dein Streben nicht vergebens?", "tokens": ["War", "all", "dein", "Stre\u00b7ben", "nicht", "ver\u00b7ge\u00b7bens", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "PPOSAT", "NN", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "War eitel Irrlauf nicht dein Lauf?", "tokens": ["War", "ei\u00b7tel", "I\u00b7rrlauf", "nicht", "dein", "Lauf", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "NN", "PTKNEG", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Jawohl, die letzten Glockenschl\u00e4ge,", "tokens": ["Ja\u00b7wohl", ",", "die", "letz\u00b7ten", "Glo\u00b7cken\u00b7schl\u00e4\u00b7ge", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der letzte Strahl des Abendlichts,", "tokens": ["Der", "letz\u00b7te", "Strahl", "des", "A\u00b7bend\u00b7lichts", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Was klingen sie im Busen rege?", "tokens": ["Was", "klin\u00b7gen", "sie", "im", "Bu\u00b7sen", "re\u00b7ge", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Was leuchtet er aus deinem Nichts?", "tokens": ["Was", "leuch\u00b7tet", "er", "aus", "dei\u00b7nem", "Nichts", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Was melden deiner Augen Tr\u00e4nen?", "tokens": ["Was", "mel\u00b7den", "dei\u00b7ner", "Au\u00b7gen", "Tr\u00e4\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Was wird im kranken Herzen wach?", "tokens": ["Was", "wird", "im", "kran\u00b7ken", "Her\u00b7zen", "wach", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "APPRART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "O all dein Irren, Tr\u00e4umen, Sehnen,", "tokens": ["O", "all", "dein", "Ir\u00b7ren", ",", "Tr\u00e4u\u00b7men", ",", "Seh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "PIAT", "PPOSAT", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Des Lebens langes Weh und Ach.", "tokens": ["Des", "Le\u00b7bens", "lan\u00b7ges", "Weh", "und", "Ach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "So ist's: Mit D\u00fcsternis umhangen", "tokens": ["So", "ist's", ":", "Mit", "D\u00fcs\u00b7ter\u00b7nis", "um\u00b7han\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VAFIN", "$.", "APPR", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie oft war dir die wunde Brust,", "tokens": ["Wie", "oft", "war", "dir", "die", "wun\u00b7de", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VAFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Dorn dein Sehnen und Verlangen,", "tokens": ["Ein", "Dorn", "dein", "Seh\u00b7nen", "und", "Ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Gift die S\u00fc\u00dfigkeit der Lust;", "tokens": ["Ein", "Gift", "die", "S\u00fc\u00b7\u00dfig\u00b7keit", "der", "Lust", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wie mochte sich der Blinde h\u00fcten", "tokens": ["Wie", "moch\u00b7te", "sich", "der", "Blin\u00b7de", "h\u00fc\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PRF", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Auf bunter T\u00e4uschung Blumenfeld,", "tokens": ["Auf", "bun\u00b7ter", "T\u00e4u\u00b7schung", "Blu\u00b7men\u00b7feld", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wie oft die Natter unter Bl\u00fcten", "tokens": ["Wie", "oft", "die", "Nat\u00b7ter", "un\u00b7ter", "Bl\u00fc\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Den Bi\u00df auf ihren Pfl\u00fccker schnellt?", "tokens": ["Den", "Bi\u00df", "auf", "ih\u00b7ren", "Pfl\u00fc\u00b7cker", "schnellt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Doch still! Auch lieblich ist verklungen", "tokens": ["Doch", "still", "!", "Auch", "lieb\u00b7lich", "ist", "ver\u00b7klun\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "$.", "ADV", "ADJD", "VAFIN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dir mancher sch\u00f6ne Erdentag,", "tokens": ["Dir", "man\u00b7cher", "sch\u00f6\u00b7ne", "Er\u00b7den\u00b7tag", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Von Gottes Lieb' und Lust durchsungen,", "tokens": ["Von", "Got\u00b7tes", "Lieb'", "und", "Lust", "durch\u00b7sun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die t\u00f6nt Erinnrung fr\u00f6hlich nach.", "tokens": ["Die", "t\u00f6nt", "E\u00b7rinn\u00b7rung", "fr\u00f6h\u00b7lich", "nach", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ja, Gott, ich danke f\u00fcr dein ", "tokens": ["Ja", ",", "Gott", ",", "ich", "dan\u00b7ke", "f\u00fcr", "dein"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "NN", "$,", "PPER", "VVFIN", "APPR", "PPOSAT"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "F\u00fcrs Wonnewort: ", "tokens": ["F\u00fcrs", "Won\u00b7ne\u00b7wort", ":"], "token_info": ["word", "word", "punct"], "pos": ["NN", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "F\u00fcr deine sch\u00f6ne, gr\u00fcne Erde", "tokens": ["F\u00fcr", "dei\u00b7ne", "sch\u00f6\u00b7ne", ",", "gr\u00fc\u00b7ne", "Er\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und all ihr Sonnenangesicht.", "tokens": ["Und", "all", "ihr", "Son\u00b7nen\u00b7an\u00b7ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Ja, Dank dir, Herr, f\u00fcr reiche Freude", "tokens": ["Ja", ",", "Dank", "dir", ",", "Herr", ",", "f\u00fcr", "rei\u00b7che", "Freu\u00b7de"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PTKANT", "$,", "APPR", "PPER", "$,", "NN", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Auf schwerstem, l\u00e4ngstem Pilgergang.", "tokens": ["Auf", "schwers\u00b7tem", ",", "l\u00e4ngs\u00b7tem", "Pil\u00b7ger\u00b7gang", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es macht des Abends Schlafgel\u00e4ute", "tokens": ["Es", "macht", "des", "A\u00b7bends", "Schlaf\u00b7ge\u00b7l\u00e4u\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dem m\u00fcden Wandrer nimmer bang;", "tokens": ["Dem", "m\u00fc\u00b7den", "Wand\u00b7rer", "nim\u00b7mer", "bang", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wie oft er auch auf w\u00fcstem Pfade", "tokens": ["Wie", "oft", "er", "auch", "auf", "w\u00fcs\u00b7tem", "Pfa\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "PPER", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Von deinem Lichte lief verirrt,", "tokens": ["Von", "dei\u00b7nem", "Lich\u00b7te", "lief", "ver\u00b7irrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Er wei\u00df, da\u00df deine Huld und Gnade", "tokens": ["Er", "wei\u00df", ",", "da\u00df", "dei\u00b7ne", "Huld", "und", "Gna\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPOSAT", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ihn nimmermehr verlassen wird.", "tokens": ["Ihn", "nim\u00b7mer\u00b7mehr", "ver\u00b7las\u00b7sen", "wird", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Nein, nimmer! Felsen sind die Worte,", "tokens": ["Nein", ",", "nim\u00b7mer", "!", "Fel\u00b7sen", "sind", "die", "Wor\u00b7te", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADV", "$.", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Worte dein, Herr Jesus Christ,", "tokens": ["Die", "Wor\u00b7te", "dein", ",", "Herr", "Je\u00b7sus", "Christ", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "$,", "NN", "NE", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Durch welche mir die Himmelspforte", "tokens": ["Durch", "wel\u00b7che", "mir", "die", "Him\u00b7mel\u00b7spfor\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "PPER", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Gnade weit ge\u00f6ffnet ist.", "tokens": ["Der", "Gna\u00b7de", "weit", "ge\u00b7\u00f6ff\u00b7net", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mag dieser Erde Licht verscheinen,", "tokens": ["Mag", "die\u00b7ser", "Er\u00b7de", "Licht", "ver\u00b7schei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PDAT", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Mag diese Sonne untergehn,", "tokens": ["Mag", "die\u00b7se", "Son\u00b7ne", "un\u00b7ter\u00b7gehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PDAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ich werde selig mit den Deinen", "tokens": ["Ich", "wer\u00b7de", "se\u00b7lig", "mit", "den", "Dei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADJD", "APPR", "ART", "PPOSAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Lobsingend stehn auf h\u00f6hern H\u00f6hn.", "tokens": ["Lob\u00b7sin\u00b7gend", "stehn", "auf", "h\u00f6\u00b7hern", "H\u00f6hn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Ja, s\u00fc\u00dfer Heiland, mit den Deinen,", "tokens": ["Ja", ",", "s\u00fc\u00b7\u00dfer", "Hei\u00b7land", ",", "mit", "den", "Dei\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADJA", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sei auch ich unter Kleinsten klein \u2013", "tokens": ["Sei", "auch", "ich", "un\u00b7ter", "Kleins\u00b7ten", "klein", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPER", "APPR", "NN", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dein Licht wird ewig auf mir scheinen,", "tokens": ["Dein", "Licht", "wird", "e\u00b7wig", "auf", "mir", "schei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dein Glanz wird ewig bei mir sein.", "tokens": ["Dein", "Glanz", "wird", "e\u00b7wig", "bei", "mir", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "APPR", "PPER", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Hier gilt kein Zagen und kein Fragen,", "tokens": ["Hier", "gilt", "kein", "Za\u00b7gen", "und", "kein", "Fra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "KON", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Hier gilt: ", "tokens": ["Hier", "gilt", ":"], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}}, "stanza.8": {"line.1": {"text": "Schon dunkeln meine Lebenstage", "tokens": ["Schon", "dun\u00b7keln", "mei\u00b7ne", "Le\u00b7bens\u00b7ta\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADJA", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "lin