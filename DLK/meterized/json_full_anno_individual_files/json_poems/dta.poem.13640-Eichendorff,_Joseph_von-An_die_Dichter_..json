{"dta.poem.13640": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "An die Dichter .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Wo treues Wollen, redlich Streben", "tokens": ["Wo", "treu\u00b7es", "Wol\u00b7len", ",", "red\u00b7lich", "Stre\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "ADJA", "NN", "$,", "ADJD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und rechten Sinn der Rechte sp\u00fcrt,", "tokens": ["Und", "rech\u00b7ten", "Sinn", "der", "Rech\u00b7te", "sp\u00fcrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das mu\u00df die Seele ihm erheben,", "tokens": ["Das", "mu\u00df", "die", "See\u00b7le", "ihm", "er\u00b7he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das hat mich jedesmal ger\u00fchrt.", "tokens": ["Das", "hat", "mich", "je\u00b7des\u00b7mal", "ge\u00b7r\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}