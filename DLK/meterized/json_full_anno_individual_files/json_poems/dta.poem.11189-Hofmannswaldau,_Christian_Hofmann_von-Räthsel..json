{"dta.poem.11189": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "R\u00e4thsel.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Neptun war gantz entbrannt, die Ceres zu umschliessen,", "tokens": ["Nep\u00b7tun", "war", "gantz", "ent\u00b7brannt", ",", "die", "Ce\u00b7res", "zu", "um\u00b7schlies\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "VVPP", "$,", "PRELS", "NE", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sie merckte seine gluth, und lie\u00df sich willig k\u00fcssen;", "tokens": ["Sie", "merck\u00b7te", "sei\u00b7ne", "gluth", ",", "und", "lie\u00df", "sich", "wil\u00b7lig", "k\u00fcs\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,", "KON", "VVFIN", "PRF", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sein crystallinen mund sog ihren malvasier,", "tokens": ["Sein", "crys\u00b7tal\u00b7li\u00b7nen", "mund", "sog", "ih\u00b7ren", "mal\u00b7va\u00b7sier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "So zeugten sie ein kind. Wie hie\u00df der name? Bier.", "tokens": ["So", "zeug\u00b7ten", "sie", "ein", "kind", ".", "Wie", "hie\u00df", "der", "na\u00b7me", "?", "Bier", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$.",