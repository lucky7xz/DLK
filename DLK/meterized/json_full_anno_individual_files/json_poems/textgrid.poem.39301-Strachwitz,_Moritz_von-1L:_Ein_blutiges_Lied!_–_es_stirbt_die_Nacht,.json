{"textgrid.poem.39301": {"metadata": {"author": {"name": "Strachwitz, Moritz von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein blutiges Lied! \u2013 es stirbt die Nacht,", "genre": "verse", "period": "N.A.", "pub_year": 1834, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein blutiges Lied! \u2013 es stirbt die Nacht,", "tokens": ["Ein", "blu\u00b7ti\u00b7ges", "Lied", "!", "\u2013", "es", "stirbt", "die", "Nacht", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "$(", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Aus der Heide jubelt der Wind der Schlacht! \u2013", "tokens": ["Aus", "der", "Hei\u00b7de", "ju\u00b7belt", "der", "Wind", "der", "Schlacht", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "ART", "NN", "$.", "$("], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ihre Schwerter sind blau, ihre Banner rot,", "tokens": ["Ih\u00b7re", "Schwer\u00b7ter", "sind", "blau", ",", "ih\u00b7re", "Ban\u00b7ner", "rot", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$,", "PPOSAT", "NN", "ADJD", "$,"], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Und sie reiten hinein in den roten Tod", "tokens": ["Und", "sie", "rei\u00b7ten", "hin\u00b7ein", "in", "den", "ro\u00b7ten", "Tod"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.5": {"text": "Wohl \u00fcber die breite Heide!", "tokens": ["Wohl", "\u00fc\u00b7ber", "die", "brei\u00b7te", "Hei\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Und einer voran, sein Streithengst fliegt,", "tokens": ["Und", "ei\u00b7ner", "vo\u00b7ran", ",", "sein", "Strei\u00b7thengst", "fliegt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "PTKVZ", "$,", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sein Herz sich \u00fcber den Wolken wiegt,", "tokens": ["Sein", "Herz", "sich", "\u00fc\u00b7ber", "den", "Wol\u00b7ken", "wiegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PRF", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Eine Rose bl\u00fcht auf des Feindes Wall,", "tokens": ["Ei\u00b7ne", "Ro\u00b7se", "bl\u00fcht", "auf", "des", "Fein\u00b7des", "Wall", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "NE", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Nach der Rose sieht er all\u00fcberall", "tokens": ["Nach", "der", "Ro\u00b7se", "sieht", "er", "al\u00b7l\u00fc\u00b7be\u00b7rall"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "PIAT"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "Wohl \u00fcber die dunkle Heide!", "tokens": ["Wohl", "\u00fc\u00b7ber", "die", "dunk\u00b7le", "Hei\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Sein Sporn ist scharf, sein Herz ist frisch,", "tokens": ["Sein", "Sporn", "ist", "scharf", ",", "sein", "Herz", "ist", "frisch", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$,", "PPOSAT", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Blume, die bl\u00fcht so zauberisch.", "tokens": ["Die", "Blu\u00b7me", ",", "die", "bl\u00fcht", "so", "zau\u00b7be\u00b7risch", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VVFIN", "ADV", "ADJD", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Einen t\u00fcchtigen Satz und drauf und drein \u2013", "tokens": ["Ei\u00b7nen", "t\u00fcch\u00b7ti\u00b7gen", "Satz", "und", "drauf", "und", "drein", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "PAV", "KON", "ADV", "$("], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "\u00bbo Rose, Du mu\u00dft mein eigen sein!\u00ab \u2013", "tokens": ["\u00bb", "o", "Ro\u00b7se", ",", "Du", "mu\u00dft", "mein", "ei\u00b7gen", "sein", "!", "\u00ab", "\u2013"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["$(", "FM", "FM", "$,", "PPER", "VMFIN", "PPOSAT", "ADJD", "VAINF", "$.", "$(", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Vom Hufschlag donnert die Heide.", "tokens": ["Vom", "Huf\u00b7schlag", "don\u00b7nert", "die", "Hei\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Einen t\u00fcchtigen Satz! Die Kugel pfeift,", "tokens": ["Ei\u00b7nen", "t\u00fcch\u00b7ti\u00b7gen", "Satz", "!", "Die", "Ku\u00b7gel", "pfeift", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "ART", "NN", "VVFIN", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Er greift nach dem Herzen, er wird geschleift.", "tokens": ["Er", "greift", "nach", "dem", "Her\u00b7zen", ",", "er", "wird", "ge\u00b7schleift", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Der Hengst rei\u00dft aus mit dem Ritter wert,", "tokens": ["Der", "Hengst", "rei\u00dft", "aus", "mit", "dem", "Rit\u00b7ter", "wert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Da\u00df blutendes Haar die Heide kehrt", "tokens": ["Da\u00df", "blu\u00b7ten\u00b7des", "Haar", "die", "Hei\u00b7de", "kehrt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJA", "NN", "ART", "NN", "VVFIN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Die Heide, die rote Heide! \u2013", "tokens": ["Die", "Hei\u00b7de", ",", "die", "ro\u00b7te", "Hei\u00b7de", "!", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NE", "$,", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Der Mann bin ich, ich war einst jung,", "tokens": ["Der", "Mann", "bin", "ich", ",", "ich", "war", "einst", "jung", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "$,", "PPER", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So trotzig mein Herz, so hoch sein Schwung.", "tokens": ["So", "trot\u00b7zig", "mein", "Herz", ",", "so", "hoch", "sein", "Schwung", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPOSAT", "NN", "$,", "ADV", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Mein freudiges Leben setzt' ich ein,", "tokens": ["Mein", "freu\u00b7di\u00b7ges", "Le\u00b7ben", "setzt'", "ich", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Denn die Rose sollte gewonnen sein,", "tokens": ["Denn", "die", "Ro\u00b7se", "soll\u00b7te", "ge\u00b7won\u00b7nen", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VMFIN", "VVPP", "VAINF", "$,"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Die herrliche Rose der Heide!", "tokens": ["Die", "herr\u00b7li\u00b7che", "Ro\u00b7se", "der", "Hei\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.6": {"line.1": {"text": "Vorbei, vorbei, der Schu\u00df traf vorn,", "tokens": ["Vor\u00b7bei", ",", "vor\u00b7bei", ",", "der", "Schu\u00df", "traf", "vorn", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PTKVZ", "$,", "ART", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "O s\u00fc\u00dfe Rose, wie brennt Dein Dorn!", "tokens": ["O", "s\u00fc\u00b7\u00dfe", "Ro\u00b7se", ",", "wie", "brennt", "Dein", "Dorn", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "PWAV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Durch das Leben geschleift von Zorn und Qual,", "tokens": ["Durch", "das", "Le\u00b7ben", "ge\u00b7schleift", "von", "Zorn", "und", "Qual", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Wann steht das Ro\u00df, wann ruhst Du einmal,", "tokens": ["Wann", "steht", "das", "Ro\u00df", ",", "wann", "ruhst", "Du", "ein\u00b7mal", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "$,", "PWAV", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Mein Herz, tief unter der Heide?", "tokens": ["Mein", "Herz", ",", "tief", "un\u00b7ter", "der", "Hei\u00b7de", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.7": {"line.1": {"text": "Ein blutiges Lied! \u2013 es stirbt die Nacht,", "tokens": ["Ein", "blu\u00b7ti\u00b7ges", "Lied", "!", "\u2013", "es", "stirbt", "die", "Nacht", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "$(", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Aus der Heide jubelt der Wind der Schlacht! \u2013", "tokens": ["Aus", "der", "Hei\u00b7de",