{"textgrid.poem.45832": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "[so bricht das Herz]", "genre": "verse", "period": "N.A.", "pub_year": 1799, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So bricht das Herz, so mu\u00df ich ewig weinen,", "tokens": ["So", "bricht", "das", "Herz", ",", "so", "mu\u00df", "ich", "e\u00b7wig", "wei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "ADV", "VMFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So tret' ich wankend auf die neue Bahn,", "tokens": ["So", "tret'", "ich", "wan\u00b7kend", "auf", "die", "neu\u00b7e", "Bahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und in dem ersten Schritte schon erscheinen", "tokens": ["Und", "in", "dem", "ers\u00b7ten", "Schrit\u00b7te", "schon", "er\u00b7schei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "ADV", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Hoffnungen, der Lohn ein leerer Wahn.", "tokens": ["Die", "Hoff\u00b7nun\u00b7gen", ",", "der", "Lohn", "ein", "lee\u00b7rer", "Wahn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Mit Pflichten soll ich Liebe binden,", "tokens": ["Mit", "Pflich\u00b7ten", "soll", "ich", "Lie\u00b7be", "bin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VMFIN", "PPER", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die Liebe von der Pflicht getrennt;", "tokens": ["Die", "Lie\u00b7be", "von", "der", "Pflicht", "ge\u00b7trennt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und frohe Kr\u00e4nze soll ich winden,", "tokens": ["Und", "fro\u00b7he", "Kr\u00e4n\u00b7ze", "soll", "ich", "win\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Die keine Blume kennt.", "tokens": ["Die", "kei\u00b7ne", "Blu\u00b7me", "kennt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Der erste Blick mu\u00df schon in Tr\u00e4nen schwimmen,", "tokens": ["Der", "ers\u00b7te", "Blick", "mu\u00df", "schon", "in", "Tr\u00e4\u00b7nen", "schwim\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "ADV", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mir gegen\u00fcber steht das stille Haus,", "tokens": ["Mir", "ge\u00b7gen\u00b7\u00fc\u00b7ber", "steht", "das", "stil\u00b7le", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPO", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der Orgelton schwillt bang um helle Stimmen,", "tokens": ["Der", "Or\u00b7gel\u00b7ton", "schwillt", "bang", "um", "hel\u00b7le", "Stim\u00b7men