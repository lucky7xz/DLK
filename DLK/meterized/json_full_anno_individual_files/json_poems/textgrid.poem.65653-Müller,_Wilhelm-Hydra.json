{"textgrid.poem.65653": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Hydra", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hoher, steiler, fester Felsen, darauf Hellas Freiheit ruht!", "tokens": ["Ho\u00b7her", ",", "stei\u00b7ler", ",", "fes\u00b7ter", "Fel\u00b7sen", ",", "da\u00b7rauf", "Hel\u00b7las", "Frei\u00b7heit", "ruht", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJA", "$,", "ADJA", "NN", "$,", "PAV", "NE", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Seh' ich deine Wolkengipfel, steigt mein Herz, und wallt mein Blut.", "tokens": ["Seh'", "ich", "dei\u00b7ne", "Wol\u00b7ken\u00b7gip\u00b7fel", ",", "steigt", "mein", "Herz", ",", "und", "wallt", "mein", "Blut", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "$,", "VVFIN", "PPOSAT", "NN", "$,", "KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Hoher, steiler, fester Felsen, den des Meeres Wog' umbraust,", "tokens": ["Ho\u00b7her", ",", "stei\u00b7ler", ",", "fes\u00b7ter", "Fel\u00b7sen", ",", "den", "des", "Mee\u00b7res", "Wo\u00b7g'", "um\u00b7braust", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJA", "$,", "ADJA", "NN", "$,", "PRELS", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+--+", "measure": "iambic.octa.plus.octa.plus.chol"}, "line.4": {"text": "\u00dcber dessen kahlem Scheitel wild die Donnerwolke saust!", "tokens": ["\u00dc\u00b7ber", "des\u00b7