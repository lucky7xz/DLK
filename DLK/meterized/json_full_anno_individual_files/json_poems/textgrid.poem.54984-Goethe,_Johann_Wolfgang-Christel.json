"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das geht herum, das geht so scharf,", "tokens": ["Das", "geht", "he\u00b7rum", ",", "das", "geht", "so", "scharf", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PTKVZ", "$,", "PDS", "VVFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da f\u00fchl ich mich so ganz!", "tokens": ["Da", "f\u00fchl", "ich", "mich", "so", "ganz", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und wenn's ihr taumlig wird und warm,", "tokens": ["Und", "wenn's", "ihr", "taum\u00b7lig", "wird", "und", "warm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADJD", "VAFIN", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da wieg ich sie sogleich", "tokens": ["Da", "wieg", "ich", "sie", "sog\u00b7leich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "An meiner Brust, in meinem Arm;", "tokens": ["An", "mei\u00b7ner", "Brust", ",", "in", "mei\u00b7nem", "Arm", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "'s ist mir ein K\u00f6nigreich!", "tokens": ["'s", "ist", "mir", "ein", "K\u00f6\u00b7nig\u00b7reich", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Und wenn sie liebend nach mir blickt", "tokens": ["Und", "wenn", "sie", "lie\u00b7bend", "nach", "mir", "blickt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "ADJD", "APPR", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und alles rund vergi\u00dft,", "tokens": ["Und", "al\u00b7les", "rund", "ver\u00b7gi\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und dann an meine Brust gedr\u00fcckt", "tokens": ["Und", "dann", "an", "mei\u00b7ne", "Brust", "ge\u00b7dr\u00fcckt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "PPOSAT", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und weidlich eins gek\u00fc\u00dft,", "tokens": ["Und", "weid\u00b7lich", "eins", "ge\u00b7k\u00fc\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PIS", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Das l\u00e4uft mir durch das R\u00fcckenmark", "tokens": ["Das", "l\u00e4uft", "mir", "durch", "das", "R\u00fc\u00b7cken\u00b7mark"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Bis in die gro\u00dfe Zeh!", "tokens": ["Bis", "in", "die", "gro\u00b7\u00dfe", "Zeh", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ich bin so schwach, ich bin so stark,", "tokens": ["Ich", "bin", "so", "schwach", ",", "ich", "bin", "so", "stark", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$,", "PPER", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Mir ist so wohl, so weh!", "tokens": ["Mir", "ist", "so", "wohl", ",", "so", "weh", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "$,", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Da m\u00f6cht ich mehr und immer mehr,", "tokens": ["Da", "m\u00f6cht", "ich", "mehr", "und", "im\u00b7mer", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "KON", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Tag wird mir nicht lang;", "tokens": ["Der", "Tag", "wird", "mir", "nicht", "lang", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wenn ich die Nacht auch bei ihr w\u00e4r,", "tokens": ["Wenn", "ich", "die", "Nacht", "auch", "bei", "ihr", "w\u00e4r", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "APPR", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Davor w\u00e4r mir nicht bang.", "tokens": ["Da\u00b7vor", "w\u00e4r", "mir", "nicht", "bang", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ich denk, ich halte sie einmal", "tokens": ["Ich", "denk", ",", "ich", "hal\u00b7te", "sie", "ein\u00b7mal"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und b\u00fc\u00dfe meine Lust;", "tokens": ["Und", "b\u00fc\u00b7\u00dfe", "mei\u00b7ne", "Lust", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und endigt sich nicht meine Qual,", "tokens": ["Und", "en\u00b7digt", "sich", "nicht", "mei\u00b7ne", "Qual", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "PTKNEG", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Sterb ich an ihrer Brust!", "tokens": ["Sterb", "ich", "an", "ih\u00b7rer", "Brust", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "---+-+", "measure": "unknown.measure.di"}}}}}