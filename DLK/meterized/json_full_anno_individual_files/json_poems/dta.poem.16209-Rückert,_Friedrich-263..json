{"dta.poem.16209": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "263.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Und locket wieder dich das Gaukelspiel der Welt,", "tokens": ["Und", "lo\u00b7cket", "wie\u00b7der", "dich", "das", "Gau\u00b7kel\u00b7spiel", "der", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PPER", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was sie dir vorh\u00e4lt stets, und stets dir vorenth\u00e4lt!", "tokens": ["Was", "sie", "dir", "vor\u00b7h\u00e4lt", "stets", ",", "und", "stets", "dir", "vor\u00b7ent\u00b7h\u00e4lt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PPER", "VVFIN", "ADV", "$,", "KON", "VVFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "O nimm in deine Brust nicht diesen harten Stein;", "tokens": ["O", "nimm", "in", "dei\u00b7ne", "Brust", "nicht", "die\u00b7sen", "har\u00b7ten", "Stein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "PPOSAT", "NN", "PTKNEG", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zwei Herzen k\u00f6nnen nicht in Einem Busen seyn.", "tokens": ["Zwei", "Her\u00b7zen", "k\u00f6n\u00b7nen", "nicht", "in", "Ei\u00b7nem", "Bu\u00b7sen", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VMFIN", "PTKNEG", "APPR", "ART", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Er dr\u00fcckt das Herz dir ab, das sich daran will laben;", "tokens": ["Er", "dr\u00fcckt", "das", "Herz", "dir", "ab", ",", "das", "sich", "da\u00b7ran", "will", "la\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PPER", "PTKVZ", "$,", "PRELS", "PRF", "PAV", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "O habe du das Herz, dein Herz f\u00fcr dich zu haben!", "tokens": ["O", "ha\u00b7be", "du", "das", "Herz", ",", "dein", "Herz", "f\u00fcr", "dich", "zu", "ha\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "ART", "NN", "$,", "PPOSAT", "NN", "APPR", "PPER", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "In dir bist du gesund, und f\u00fchlst in ihr dich krank;", "tokens": ["In", "dir", "bist", "du", "ge\u00b7sund", ",", "und", "f\u00fchlst", "in", "ihr", "dich", "krank", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "PPER", "VVPP", "$,", "KON", "VVFIN", "APPR", "PPER", "PRF", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gib, was du hast, der Welt, und nimm nicht ihren Dank!", "tokens": ["Gib", ",", "was", "du", "hast", ",", "der", "Welt", ",", "und", "nimm", "nicht", "ih\u00b7ren", "Dank", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "PWS", "PPER", "VAFIN", "$,", "ART", "NN", "$,", "KON", "VVIMP", "PTKNEG", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}