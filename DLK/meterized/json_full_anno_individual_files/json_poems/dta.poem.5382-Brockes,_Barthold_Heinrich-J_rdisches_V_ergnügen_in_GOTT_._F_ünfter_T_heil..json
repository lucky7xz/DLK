{"dta.poem.5382": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "J rdisches  \n  V ergn\u00fcgen  \n in  \n  GOTT .  \n  F \u00fcnfter  T heil.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Man stelle sich nach diesem Leben", "tokens": ["Man", "stel\u00b7le", "sich", "nach", "die\u00b7sem", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "PRF", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zwo solcher Seelen vor, wovon die eine", "tokens": ["Zwo", "sol\u00b7cher", "See\u00b7len", "vor", ",", "wo\u00b7von", "die", "ei\u00b7ne"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "PIAT", "NN", "PTKVZ", "$,", "PWAV", "ART", "ART"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die gantze Lebens-Zeit dem gelben Scheine", "tokens": ["Die", "gant\u00b7ze", "Le\u00b7bens\u00b7Zeit", "dem", "gel\u00b7ben", "Schei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Des Goldes nachgestrebt; die ander\u2019 Acht gegeben", "tokens": ["Des", "Gol\u00b7des", "nach\u00b7ge\u00b7strebt", ";", "die", "an\u00b7der'", "Acht", "ge\u00b7ge\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$.", "ART", "ADJD", "CARD", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Auf GOttes Wunder, GOtt in seinem Werck\u2019 geehret,", "tokens": ["Auf", "Got\u00b7tes", "Wun\u00b7der", ",", "Gott", "in", "sei\u00b7nem", "Werck", "ge\u00b7eh\u00b7ret", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "$,", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Zu GOttes Ruhm, geschmeckt, gesehen und geh\u00f6ret;", "tokens": ["Zu", "Got\u00b7tes", "Ruhm", ",", "ge\u00b7schmeckt", ",", "ge\u00b7se\u00b7hen", "und", "ge\u00b7h\u00f6\u00b7ret", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "$,", "VVPP", "$,", "VVPP", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Was meinst du, wird sich GOtt daran", "tokens": ["Was", "meinst", "du", ",", "wird", "sich", "Gott", "da\u00b7ran"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "$,", "VAFIN", "PRF", "NN", "PAV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Nicht mehr vergn\u00fcgen, und sie wehlen,", "tokens": ["Nicht", "mehr", "ver\u00b7gn\u00fc\u00b7gen", ",", "und", "sie", "weh\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "VVINF", "$,", "KON", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Als die (wo man so sagen kann)", "tokens": ["Als", "die", "(", "wo", "man", "so", "sa\u00b7gen", "kann", ")"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "$(", "PWAV", "PIS", "ADV", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Fast gantz metallisirte Seelen?", "tokens": ["Fast", "gantz", "me\u00b7tal\u00b7li\u00b7sir\u00b7te", "See\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "mea