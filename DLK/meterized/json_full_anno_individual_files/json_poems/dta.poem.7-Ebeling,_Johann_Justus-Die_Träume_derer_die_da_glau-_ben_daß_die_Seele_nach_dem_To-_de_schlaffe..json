{"dta.poem.7": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "Die Tr\u00e4ume derer die da glau-  \n ben da\u00df die Seele nach dem To-  \n de schlaffe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198797", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Welt wird meist regiert von\neinen falschen Wahn,", "tokens": ["Die", "Welt", "wird", "meist", "re\u00b7giert", "von", "ei\u00b7nen", "fal\u00b7schen", "Wahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Menschen Aberwiz irrt", "tokens": ["Der", "Men\u00b7schen", "A\u00b7ber\u00b7wiz", "irrt"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VVFIN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Indem er meint zu sehn, ist er", "tokens": ["In\u00b7dem", "er", "meint", "zu", "sehn", ",", "ist", "er"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "PTKZU", "VVINF", "$,", "VAFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da er statt Warheits-Schein", "tokens": ["Da", "er", "statt", "Wa\u00b7rheits\u00b7Schein"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Was eitler Wiz erdacht, gef\u00e4llt dem meisten woll;", "tokens": ["Was", "eit\u00b7ler", "Wiz", "er\u00b7dacht", ",", "ge\u00b7f\u00e4llt", "dem", "meis\u00b7ten", "woll", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJA", "NN", "VVPP", "$,", "VVFIN", "ART", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Weil man das sehen will, was man nur glauben", "tokens": ["Weil", "man", "das", "se\u00b7hen", "will", ",", "was", "man", "nur", "glau\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "PDS", "VVINF", "VMFIN", "$,", "PRELS", "PIS", "ADV", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Das menschliche Gehirn von Einbildung betrogen,", "tokens": ["Das", "menschli\u00b7che", "Ge\u00b7hirn", "von", "Ein\u00b7bil\u00b7dung", "be\u00b7tro\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.8": {"text": "Gleicht einer Wageschal, die leicht wird \u00fcberwogen", "tokens": ["Gleicht", "ei\u00b7ner", "Wa\u00b7ge\u00b7schal", ",", "die", "leicht", "wird", "\u00fc\u00b7ber\u00b7wo\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "$,", "PRELS", "ADJD", "VAFIN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Von d\u00fcnner Spreu, von Wind. Ein ungereimt", "tokens": ["Von", "d\u00fcn\u00b7ner", "Spreu", ",", "von", "Wind", ".", "Ein", "un\u00b7ge\u00b7reimt"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "APPR", "NN", "$.", "ART", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Das gegen Warheit liegt, hat ofte mehr Gewicht", "tokens": ["Das", "ge\u00b7gen", "War\u00b7heit", "liegt", ",", "hat", "of\u00b7te", "mehr", "Ge\u00b7wicht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "APPR", "NN", "VVFIN", "$,", "VAFIN", "ADV", "PIAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Als dieses Gr\u00fcnde hat: man kan es kl\u00e4rlich sehen", "tokens": ["Als", "die\u00b7ses", "Gr\u00fcn\u00b7de", "hat", ":", "man", "kan", "es", "kl\u00e4r\u00b7lich", "se\u00b7hen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PDAT", "NN", "VAFIN", "$.", "PIS", "VMFIN", "PPER", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Wenn wir was dieser gl\u00e4ubt, und jener meint,", "tokens": ["Wenn", "wir", "was", "die\u00b7ser", "gl\u00e4ubt", ",", "und", "je\u00b7ner", "meint", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PWS", "PDS", "VVFIN", "$,", "KON", "PDS", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Nur ein Exempel ist, hie zum Beweis genug,", "tokens": ["Nur", "ein", "Ex\u00b7em\u00b7pel", "ist", ",", "hie", "zum", "Be\u00b7weis", "ge\u00b7nug", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VAFIN", "$,", "ADV", "APPRART", "NN", "ADV", "$,"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.14": {"text": "Als ich einst einen Mann von scharfen Wizze frug,", "tokens": ["Als", "ich", "einst", "ei\u00b7nen", "Mann", "von", "schar\u00b7fen", "Wiz\u00b7ze", "frug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "In was vor einen Stand die Seele w\u00fcrde kom-", "tokens": ["In", "was", "vor", "ei\u00b7nen", "Stand", "die", "See\u00b7le", "w\u00fcr\u00b7de", "kom"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "APPR", "ART", "NN", "ART", "NN", "VAFIN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Wenn sie aus ihren Leib in ", "tokens": ["Wenn", "sie", "aus", "ih\u00b7ren", "Leib", "in"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "APPR"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.17": {"text": "Sprach er in einen Schlaf, in eine stille Ruh;", "tokens": ["Sprach", "er", "in", "ei\u00b7nen", "Schlaf", ",", "in", "ei\u00b7ne", "stil\u00b7le", "Ruh", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "ART", "NN", "$,", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Er sezte zum Beweis den albern Grund hinzu:", "tokens": ["Er", "sez\u00b7te", "zum", "Be\u00b7weis", "den", "al\u00b7bern", "Grund", "hin\u00b7zu", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Weil ihr die Sinnen fehln; so kan sie auch nicht", "tokens": ["Weil", "ihr", "die", "Sin\u00b7nen", "fehln", ";", "so", "kan", "sie", "auch", "nicht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "VVINF", "$.", "ADV", "VMFIN", "PPER", "ADV", "PTKNEG"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.20": {"text": "So mu\u00df der Sch\u00f6pfer sie in tieffen Schlaf versen-", "tokens": ["So", "mu\u00df", "der", "Sch\u00f6p\u00b7fer", "sie", "in", "tief\u00b7fen", "Schlaf", "ver\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ART", "NN", "PPER", "APPR", "ADJA", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Wenn sie im K\u00f6rper wohnt; so ist die Denkungs-", "tokens": ["Wenn", "sie", "im", "K\u00f6r\u00b7per", "wohnt", ";", "so", "ist", "die", "Den\u00b7kungs"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPRART", "NN", "VVFIN", "$.", "ADV", "VAFIN", "ART", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "Die richtige Vernunft, der Seelen Eigenschaft.", "tokens": ["Die", "rich\u00b7ti\u00b7ge", "Ver\u00b7nunft", ",", "der", "See\u00b7len", "Ei\u00b7gen\u00b7schaft", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Ich sprach: Mein Freund! kehrts um; so wird", "tokens": ["Ich", "sprach", ":", "Mein", "Freund", "!", "kehrts", "um", ";", "so", "wird"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "PPOSAT", "NN", "$.", "ADV", "PTKVZ", "$.", "ADV", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Denn jezt schl\u00e4ft eure Seel, das merk ich aus den", "tokens": ["Denn", "jezt", "schl\u00e4ft", "eu\u00b7re", "Seel", ",", "das", "merk", "ich", "aus", "den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPOSAT", "NN", "$,", "PDS", "VVFIN", "PPER", "APPR", "ART"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.25": {"text": "Die sie jezt phantasirt; ihr wi\u00dft nicht was ihr", "tokens": ["Die", "sie", "jezt", "phan\u00b7ta\u00b7sirt", ";", "ihr", "wi\u00dft", "nicht", "was", "ihr"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ADV", "VVPP", "$.", "PPER", "VVFIN", "PTKNEG", "PWS", "PPER"], "meter": "---+-+-+--+", "measure": "iambic.tetra.chol"}, "line.26": {"text": "Weil euch als blos im Traum, die Seele schlafend", "tokens": ["Weil", "euch", "als", "blos", "im", "Traum", ",", "die", "See\u00b7le", "schla\u00b7fend"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "KOKOM", "ADV", "APPRART", "NN", "$,", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.27": {"text": "Ich w\u00fcnsche da\u00df ihr bald von euren Traum erwa-", "tokens": ["Ich", "w\u00fcn\u00b7sche", "da\u00df", "ihr", "bald", "von", "eu\u00b7ren", "Traum", "er\u00b7wa"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KOUS", "PPER", "ADV", "APPR", "PPOSAT", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Und wachend \u00fcberdenkt, was ihr vor Grillen ma-", "tokens": ["Und", "wa\u00b7chend", "\u00fc\u00b7ber\u00b7denkt", ",", "was", "ihr", "vor", "Gril\u00b7len", "ma"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVPP", "$,", "PWS", "PPER", "APPR", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}