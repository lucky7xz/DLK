{"dta.poem.14253": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Das Fl\u00fcgelro\u00df .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.85", "nl:0.14"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Ich hab' nicht viel hienieden,", "tokens": ["Ich", "hab'", "nicht", "viel", "hien\u00b7ie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADV", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ich hab' nicht Geld noch Gut;", "tokens": ["Ich", "hab'", "nicht", "Geld", "noch", "Gut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Was vielen nicht beschieden,", "tokens": ["Was", "vie\u00b7len", "nicht", "be\u00b7schie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ist mein: \u2014 der frische Muth.", "tokens": ["Ist", "mein", ":", "der", "fri\u00b7sche", "Muth", "."], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "$.", "$(", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}