 "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Die zerstreuten Elemente", "tokens": ["Die", "zer\u00b7streu\u00b7ten", "E\u00b7le\u00b7men\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.8": {"text": "Deines wahren, bessern Seins.", "tokens": ["Dei\u00b7nes", "wah\u00b7ren", ",", "bes\u00b7sern", "Seins", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Spare nicht das Wort der R\u00fcge,", "tokens": ["Spa\u00b7re", "nicht", "das", "Wort", "der", "R\u00fc\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Willst du offen vor dir sein,", "tokens": ["Willst", "du", "of\u00b7fen", "vor", "dir", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADJD", "APPR", "PPER", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn das eitle Kleid der L\u00fcge", "tokens": ["Denn", "das", "eit\u00b7le", "Kleid", "der", "L\u00fc\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Trugst auch du, den leeren Schein.", "tokens": ["Trugst", "auch", "du", ",", "den", "lee\u00b7ren", "Schein", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PPER", "$,", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Falschheit mit verstecktem H\u00f6hnen", "tokens": ["Falschheit", "mit", "ver\u00b7steck\u00b7tem", "H\u00f6h\u0