 {"text": "Was du sollst und was du darfst.", "tokens": ["Was", "du", "sollst", "und", "was", "du", "darfst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VMFIN", "KON", "PWS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Nie zu fr\u00fche kann entschwinden", "tokens": ["Nie", "zu", "fr\u00fc\u00b7he", "kann", "ent\u00b7schwin\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "VMFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Irrthum, den gehegt dein Herz,", "tokens": ["Irr\u00b7thum", ",", "den", "ge\u00b7hegt", "dein", "Herz", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Machte dich die Lust erblinden,", "tokens": ["Mach\u00b7te", "dich", "die", "Lust", "er\u00b7blin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Mu\u00df erleuchten dich der Schmerz.", "tokens": ["Mu\u00df", "er\u00b7leuch\u00b7ten", "dich", "der", "Schmerz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Doch in tiefstem Schmerzensringen", "tokens": ["Doch", "in", "tiefs\u00b7tem", "Schmer\u00b7zens\u00b7rin\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und im Sturm der Leidenschaft", "tokens": ["Und", "im", "Sturm", "der", "Lei\u00b7den\u00b7schaft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "K\u00e4mpfe sich aus Todesschlingen", "tokens": ["K\u00e4mp\u00b7fe", "sich", "aus", "To\u00b7des\u00b7schlin\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Deines be\u00dfren Wollens Kraft.", "tokens": ["Dei\u00b7nes", "be\u00df\u00b7ren", "Wol\u00b7lens", "Kraft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und sie steigt, zum Licht gezogen,", "tokens": ["Und", "sie", "steigt", ",", "zum", "Licht", "ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wie, von Morgenglanz umwebt,", "tokens": ["Wie", ",", "von", "Mor\u00b7gen\u00b7glanz", "um\u00b7webt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "APPR", "NE", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Aus beruhigt blauen Wogen", "tokens": ["Aus", "be\u00b7ru\u00b7higt", "blau\u00b7en", "Wo\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJD", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Sich der Sch\u00f6nheit G\u00f6ttin hebt.", "tokens": ["Sich", "der", "Sch\u00f6n\u00b7heit", "G\u00f6t\u00b7tin", "hebt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "NN", "NE", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Sch\u00f6nheit sei das heil'ge Streben,", "tokens": ["Sch\u00f6n\u00b7heit", 