{"dta.poem.9649": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Wider die allzu argw\u00f6hnische m\u00e4nner.  \n \u2020 \u2020 \u2020", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Aus furcht/ er werde das/ was er verdient zu seyn/", "tokens": ["Aus", "furcht", "/", "er", "wer\u00b7de", "das", "/", "was", "er", "ver\u00b7dient", "zu", "seyn", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$(", "PPER", "VAFIN", "ART", "$(", "PWS", "PPER", "VVPP", "PTKZU", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sperrt er das arme weib als wie ein hencker ein.", "tokens": ["Sperrt", "er", "das", "ar\u00b7me", "weib", "als", "wie", "ein", "hen\u00b7cker", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "KOUS", "KOKOM", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sein argwohn kommt von nichts/ als seinem b\u00f6sen leben.", "tokens": ["Sein", "arg\u00b7wohn", "kommt", "von", "nichts", "/", "als", "sei\u00b7nem", "b\u00f6\u00b7sen", "le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "PIS", "$(", "KOUS", "PPOSAT", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Er wei\u00df was er gethan; drum kan er achtung geben.", "tokens": ["Er", "wei\u00df", "was", "er", "ge\u00b7than", ";", "drum", "kan", "er", "ach\u00b7tung", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PWS", "PPER", "VVPP", "$.", "PAV", "VMFIN", "PPER", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Sie ist die tugend selbst/ ein engel in dem hau\u00df;", "tokens": ["Sie", "ist", "die", "tu\u00b7gend", "selbst", "/", "ein", "en\u00b7gel", "in", "dem", "hau\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADV", "$(", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Er aber f\u00fcrchtet sich/ die rache bleibt nicht aus.", "tokens": ["Er", "a\u00b7ber", "f\u00fcrch\u00b7tet", "sich", "/", "die", "ra\u00b7che", "bleibt", "nicht", "aus", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PRF", "$(", "ART", "NN", "VVFIN", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Er f\u00fchlt sich alle tag/ ob er es schon geworden/", "tokens": ["Er", "f\u00fchlt", "sich", "al\u00b7le", "tag", "/", "ob", "er", "es", "schon", "ge\u00b7wor\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PIAT", "NN", "$(", "KOUS", "PPER", "PPER", "ADV", "VAPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ob er die zeichen sp\u00fcrt von dem besorgten orden.", "tokens": ["Ob", "er", "die", "zei\u00b7chen", "sp\u00fcrt", "von", "dem", "be\u00b7sorg\u00b7ten", "or\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Was hilfft da ihre treu? sie hat doch keine ruh.", "tokens": ["Was", "hilfft", "da", "ih\u00b7re", "treu", "?", "sie", "hat", "doch", "kei\u00b7ne", "ruh", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "PPOSAT", "ADJD", "$.", "PPER", "VAFIN", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Worzu sie ihn nicht macht/ da machet er sich zu.", "tokens": ["Wor\u00b7zu", "sie", "ihn", "nicht", "macht", "/", "da", "ma\u00b7chet", "er", "sich", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "PTKNEG", "VVFIN", "$(", "ADV", "VVFIN", "PPER", "PRF", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Er machet sich ja selbst zum hanrey in gedancken/", "tokens": ["Er", "ma\u00b7chet", "sich", "ja", "selbst", "zum", "han\u00b7rey", "in", "ge\u00b7dan\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADV", "ADV", "APPRART", "NN", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Wie kan er anders thun/ als mit der frauen zancken?", "tokens": ["Wie", "kan", "er", "an\u00b7ders", "thun", "/", "als", "mit", "der", "frau\u00b7en", "zan\u00b7cken", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "ADV", "VVINF", "$(", "KOKOM", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}