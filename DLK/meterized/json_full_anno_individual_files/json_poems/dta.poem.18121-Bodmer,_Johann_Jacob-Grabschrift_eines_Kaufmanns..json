{"dta.poem.18121": {"metadata": {"author": {"name": "Bodmer, Johann Jacob", "birth": "N.A.", "death": "N.A."}, "title": "Grabschrift eines Kaufmanns.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-200905198432", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich machte Rechnung wohl, es k\u00f6nte mir nicht fehlen,", "tokens": ["Ich", "mach\u00b7te", "Rech\u00b7nung", "wohl", ",", "es", "k\u00f6n\u00b7te", "mir", "nicht", "feh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "ADV", "$,", "PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich wolte richtig Geld f\u00fcr mein Credit auszehlen,", "tokens": ["Ich", "wol\u00b7te", "rich\u00b7tig", "Geld", "f\u00fcr", "mein", "Cre\u00b7dit", "aus\u00b7zeh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADJD", "NN", "APPR", "PPOSAT", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Des Todes ich verga\u00df, der dann ohn all Geduld", "tokens": ["Des", "To\u00b7des", "ich", "ver\u00b7ga\u00df", ",", "der", "dann", "ohn", "all", "Ge\u00b7duld"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PPER", "VVFIN", "$,", "PRELS", "ADV", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mich bald verarrestirt allein um seine Schuld.", "tokens": ["Mich", "bald", "ver\u00b7ar\u00b7res\u00b7tirt", "al\u00b7lein", "um", "sei\u00b7ne", "Schuld", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVPP", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}