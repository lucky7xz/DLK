{"dta.poem.18935": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "14.  \n  An die Fraw M: Stinckerin.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-200905198111", "language": ["de:0.99"], "booktitle": "Weckherlin, Georg Rodolf: Gaistliche und Weltliche Gedichte. Amsterdam, 1641."}, "poem": {"stanza.1": {"line.1": {"text": "Es ist nicht seltzam/ da\u00df dein hund", "tokens": ["Es", "ist", "nicht", "selt\u00b7zam", "/", "da\u00df", "dein", "hund"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADJD", "$(", "KOUS", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So leckerhafft nach allen Dr\u2014", "tokens": ["So", "le\u00b7cker\u00b7hafft", "nach", "al\u00b7len", "Dr"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PIAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dan wer dich kennet/ dem ist kund/", "tokens": ["Dan", "wer", "dich", "ken\u00b7net", "/", "dem", "ist", "kund", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PWS", "PPER", "VVFIN", "$(", "PDS", "VAFIN", "PTKVZ", "$("], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Da\u00df er dein maul selbs offt kan lecken.", "tokens": ["Da\u00df", "er", "dein", "maul", "selbs", "offt", "kan", "le\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "ADV", "ADV", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}