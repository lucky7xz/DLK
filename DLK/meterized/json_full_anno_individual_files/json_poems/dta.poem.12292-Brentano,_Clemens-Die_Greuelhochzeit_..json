{"dta.poem.12292": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Die Greuelhochzeit .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1806", "urn": "urn:nbn:de:kobv:b4-20090519157", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "In Frauenstadt ein harter Mann,               ", "tokens": ["In", "Frau\u00b7en\u00b7stadt", "ein", "har\u00b7ter", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es war ein reicher B\u00fcrgerssohn,", "tokens": ["Es", "war", "ein", "rei\u00b7cher", "B\u00fcr\u00b7gers\u00b7sohn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der hat sich ausersehen", "tokens": ["Der", "hat", "sich", "au\u00b7ser\u00b7se\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "PRF", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein reiches M\u00e4dchen h\u00fcbsch und fein,", "tokens": ["Ein", "rei\u00b7ches", "M\u00e4d\u00b7chen", "h\u00fcbsch", "und", "fein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Er dacht, die sollt sein eigen seyn;", "tokens": ["Er", "dacht", ",", "die", "sollt", "sein", "ei\u00b7gen", "seyn", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PRELS", "VMFIN", "PPOSAT", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der Handschlag war geschehen.", "tokens": ["Der", "Hand\u00b7schlag", "war", "ge\u00b7sche\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Als man bei etlich Wochen Zeit,", "tokens": ["Als", "man", "bei", "et\u00b7lich", "Wo\u00b7chen", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "PIAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Oeffentlich die zwey junge Leut", "tokens": ["Oef\u00b7fent\u00b7lich", "die", "zwey", "jun\u00b7ge", "Leut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "CARD", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dreymal verk\u00fcndigt hatte,", "tokens": ["Drey\u00b7mal", "ver\u00b7k\u00fcn\u00b7digt", "hat\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "VAFIN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Das M\u00e4dchen war betr\u00fcbet sehr,", "tokens": ["Das", "M\u00e4d\u00b7chen", "war", "be\u00b7tr\u00fc\u00b7bet", "sehr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wollt ihren Br\u00e4utigam nicht mehr,", "tokens": ["Wollt", "ih\u00b7ren", "Br\u00e4u\u00b7ti\u00b7gam", "nicht", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Doch kam die Reu zu sp\u00e4te.", "tokens": ["Doch", "kam", "die", "Reu", "zu", "sp\u00e4\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NE", "PTKZU", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ein Schuhknecht that ihr gehen nach,", "tokens": ["Ein", "Schuh\u00b7knecht", "that", "ihr", "ge\u00b7hen", "nach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Welchem sie auch die Eh versprach,", "tokens": ["Wel\u00b7chem", "sie", "auch", "die", "Eh", "ver\u00b7sprach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Und liebet ihn dermassen,", "tokens": ["Und", "lie\u00b7bet", "ihn", "der\u00b7mas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PDS", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Hat ihm versprochen vielmal schon:", "tokens": ["Hat", "ihm", "ver\u00b7spro\u00b7chen", "viel\u00b7mal", "schon", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVFIN", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Eh sie behielt den B\u00fcrgerssohn,", "tokens": ["Eh", "sie", "be\u00b7hielt", "den", "B\u00fcr\u00b7gers\u00b7sohn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wollt sie das Leben lassen.", "tokens": ["Wollt", "sie", "das", "Le\u00b7ben", "las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Zur Hochzeit war nun alles bereit't,", "tokens": ["Zur", "Hoch\u00b7zeit", "war", "nun", "al\u00b7les", "be\u00b7reit't", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "ADV", "PIS", "ADJD", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Da man die zwey verlobte Leut", "tokens": ["Da", "man", "die", "zwey", "ver\u00b7lob\u00b7te", "Leut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "ART", "CARD", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wollte zur Kirche f\u00fchren,", "tokens": ["Woll\u00b7te", "zur", "Kir\u00b7che", "f\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPRART", "NN", "VVINF", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Die Braut zu ihrem Br\u00e4utigam spricht:", "tokens": ["Die", "Braut", "zu", "ih\u00b7rem", "Br\u00e4u\u00b7ti\u00b7gam", "spricht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "\u201edu wei\u00dft ich will dich haben nicht.\u201c", "tokens": ["\u201e", "du", "wei\u00dft", "ich", "will", "dich", "ha\u00b7ben", "nicht", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VVFIN", "PPER", "VMFIN", "PPER", "VAFIN", "PTKNEG", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da war gro\u00df Lamentiren.", "tokens": ["Da", "war", "gro\u00df", "La\u00b7men\u00b7ti\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADJD", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Der Br\u00e4utigam wohl zu ihr sprach:", "tokens": ["Der", "Br\u00e4u\u00b7ti\u00b7gam", "wohl", "zu", "ihr", "sprach", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "ADV", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u201emein liebes Kind! bedenk die Sach,", "tokens": ["\u201e", "mein", "lie\u00b7bes", "Kind", "!", "be\u00b7denk", "die", "Sach", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "ADJA", "NN", "$.", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u201ewas du mir hast versprochen.", "tokens": ["\u201e", "was", "du", "mir", "hast", "ver\u00b7spro\u00b7chen", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "PPER", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "\u201eschick dich mein Schatz, thu mit mir gehn,", "tokens": ["\u201e", "schick", "dich", "mein", "Schatz", ",", "thu", "mit", "mir", "gehn", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "PPER", "PPOSAT", "NN", "$,", "VVFIN", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "\u201el\u00e4\u00dft du mich hier in Schanden stehn,", "tokens": ["\u201e", "l\u00e4\u00dft", "du", "mich", "hier", "in", "Schan\u00b7den", "stehn", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "PRF", "ADV", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "\u201eso bleibts nicht ungerochen.\u201c", "tokens": ["\u201e", "so", "bleibts", "nicht", "un\u00b7ge\u00b7ro\u00b7chen", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "VVFIN", "PTKNEG", "ADJD", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Allein sie wollt nicht folgen ihm,", "tokens": ["Al\u00b7lein", "sie", "wollt", "nicht", "fol\u00b7gen", "ihm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VMFIN", "PTKNEG", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Br\u00e4utigam voll Zorn und Grimm,", "tokens": ["Der", "Br\u00e4u\u00b7ti\u00b7gam", "voll", "Zorn", "und", "Grimm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "ADJD", "NN", "KON", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Th\u00e4t in die Kammer gehen;", "tokens": ["Th\u00e4t", "in", "die", "Kam\u00b7mer", "ge\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Alsbald er th\u00e4te ein Pistol", "tokens": ["Als\u00b7bald", "er", "th\u00e4\u00b7te", "ein", "Pis\u00b7tol"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Mit zweyen Kugeln laden wohl,", "tokens": ["Mit", "zwe\u00b7yen", "Ku\u00b7geln", "la\u00b7den", "wohl", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das niemand th\u00e4te sehen.", "tokens": ["Das", "nie\u00b7mand", "th\u00e4\u00b7te", "se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Indem so ging der Kirchgang an,", "tokens": ["In\u00b7dem", "so", "ging", "der", "Kirch\u00b7gang", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es freute sich ein Jedermann,", "tokens": ["Es", "freu\u00b7te", "sich", "ein", "Je\u00b7der\u00b7mann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ART", "PIS", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und wollte gerne sehen,", "tokens": ["Und", "woll\u00b7te", "ger\u00b7ne", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da\u00df alles m\u00f6chte werden gut,", "tokens": ["Da\u00df", "al\u00b7les", "m\u00f6ch\u00b7te", "wer\u00b7den", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VMFIN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Machten der Braut ein'n guten Muth,", "tokens": ["Mach\u00b7ten", "der", "Braut", "ein'n", "gu\u00b7ten", "Muth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Sie th\u00e4t zur Kirche gehen.", "tokens": ["Sie", "th\u00e4t", "zur", "Kir\u00b7che", "ge\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Als nun die Braut und Br\u00e4utigam,", "tokens": ["Als", "nun", "die", "Braut", "und", "Br\u00e4u\u00b7ti\u00b7gam", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "KON", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und alles Volk zur Kirche kam,", "tokens": ["Und", "al\u00b7les", "Volk", "zur", "Kir\u00b7che", "kam", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Priester th\u00e4te gehen,", "tokens": ["Der", "Pries\u00b7ter", "th\u00e4\u00b7te", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wie sonst gebr\u00e4uchlich, zum Altar,", "tokens": ["Wie", "sonst", "ge\u00b7br\u00e4uch\u00b7lich", ",", "zum", "Al\u00b7tar", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADJD", "$,", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Darauf kam das verlobte Paar,", "tokens": ["Da\u00b7rauf", "kam", "das", "ver\u00b7lob\u00b7te", "Paar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und th\u00e4ten vor ihm stehen.", "tokens": ["Und", "th\u00e4\u00b7ten", "vor", "ihm", "ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Als er die Brant gefraget nun,", "tokens": ["Als", "er", "die", "Brant", "ge\u00b7fra\u00b7get", "nun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ob sie den Junggesellen sch\u00f6n,", "tokens": ["Ob", "sie", "den", "Jung\u00b7ge\u00b7sel\u00b7len", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zu ihrem Mann wollt haben?", "tokens": ["Zu", "ih\u00b7rem", "Mann", "wollt", "ha\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VMFIN", "VAINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Darauf die Braut antwortet bald:", "tokens": ["Da\u00b7rauf", "die", "Braut", "ant\u00b7wor\u00b7tet", "bald", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "\u201eeh ich zum Mann ihn haben wollt,", "tokens": ["\u201e", "eh", "ich", "zum", "Mann", "ihn", "ha\u00b7ben", "wollt", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PPER", "APPRART", "NN", "PPER", "VAINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "\u201eeh geb ich auf mein Leben.\u201c", "tokens": ["\u201e", "eh", "geb", "ich", "auf", "mein", "Le\u00b7ben", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "KOUS", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Kaum sie das Wort geredet wohl,", "tokens": ["Kaum", "sie", "das", "Wort", "ge\u00b7re\u00b7det", "wohl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ART", "NN", "VVPP", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Br\u00e4utigam nahm das Pistol,", "tokens": ["Der", "Br\u00e4u\u00b7ti\u00b7gam", "nahm", "das", "Pis\u00b7tol", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": 