{"dta.poem.1680": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Braut-Gedichte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Gold/ Seide kan nicht Ind und Perse nur gew\u00e4hren/", "tokens": ["Gold", "/", "Sei\u00b7de", "kan", "nicht", "Ind", "und", "Per\u00b7se", "nur", "ge\u00b7w\u00e4h\u00b7ren", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "NE", "VMFIN", "PTKNEG", "NN", "KON", "NN", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Noch Mogols reiches Land sein theures Helffenbein/", "tokens": ["Noch", "Mo\u00b7gols", "rei\u00b7ches", "Land", "sein", "theu\u00b7res", "Helf\u00b7fen\u00b7bein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "ADJA", "NN", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Arabien hat nicht die sch\u00f6nste Ros\u2019 allein/", "tokens": ["A\u00b7ra\u00b7bi\u00b7en", "hat", "nicht", "die", "sch\u00f6ns\u00b7te", "Ros'", "al\u00b7lein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PTKNEG", "ART", "ADJA", "NN", "ADV", "$("], "meter": "+----+-+-+-+", "measure": "dactylic.init"}, "line.4": {"text": "Es kan Golkonda nicht allein Demanten n\u00e4hren/", "tokens": ["Es", "kan", "Gol\u00b7kon\u00b7da", "nicht", "al\u00b7lein", "De\u00b7man\u00b7ten", "n\u00e4h\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "NE", "PTKNEG", "ADV", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Den strahlenden Rubin die Morgen-Welt geb\u00e4hren/", "tokens": ["Den", "strah\u00b7len\u00b7den", "Ru\u00b7bin", "die", "Mor\u00b7gen\u00b7Welt", "ge\u00b7b\u00e4h\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NE", "ART", "NN", "VVPP", "$("], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Die Muschel-reiche See der Perlen Mutter seyn/", "tokens": ["Die", "Mu\u00b7schel\u00b7rei\u00b7che", "See", "der", "Per\u00b7len", "Mut\u00b7ter", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "NN", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und Paros geben her den weisen Marmolstein.", "tokens": ["Und", "Pa\u00b7ros", "ge\u00b7ben", "her", "den", "wei\u00b7sen", "Mar\u00b7mol\u00b7stein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wendt keine Kosten auff/ euch hin und her zu zehren.", "tokens": ["Wendt", "kei\u00b7ne", "Kos\u00b7ten", "auff", "/", "euch", "hin", "und", "her", "zu", "zeh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "APPR", "$(", "PPER", "PTKVZ", "KON", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Zieht unserm Queisse zu/ schaut seine Nimphen an/", "tokens": ["Zieht", "un\u00b7serm", "Que\u00b7is\u00b7se", "zu", "/", "schaut", "sei\u00b7ne", "Nim\u00b7phen", "an", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PTKZU", "$(", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$("], "meter": "-+--+--+--+-+", "measure": "amphibrach.tetra.plus"}, "line.10": {"text": "Beauget unsre Braut: ihr werdet sagen m\u00fcssen/", "tokens": ["Be\u00b7au\u00b7get", "uns\u00b7re", "Braut", ":", "ihr", "wer\u00b7det", "sa\u00b7gen", "m\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$.", "PPER", "VAFIN", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Da\u00df unser Schlesien di\u00df alles zeugen kan.", "tokens": ["Da\u00df", "un\u00b7ser", "Schle\u00b7si\u00b7en", "di\u00df", "al\u00b7les", "zeu\u00b7gen", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PDS", "PIS", "VVFIN", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Wohl dem/ der solchen Schatz in Ruhe kan gen\u00fcssen!", "tokens": ["Wohl", "dem", "/", "der", "sol\u00b7chen", "Schatz", "in", "Ru\u00b7he", "kan", "ge\u00b7n\u00fcs\u00b7sen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "$(", "ART", "PIAT", "NN", "APPR", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Herr Br\u00e4utigam seht zu/ legt die Jubelen an/", "tokens": ["Herr", "Br\u00e4u\u00b7ti\u00b7gam", "seht", "zu", "/", "legt", "die", "Ju\u00b7be\u00b7len", "an", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VVFIN", "PTKZU", "$(", "VVFIN", "ART", "NN", "PTKVZ", "$("], "meter": "-+--+-+-+--+", "measure": "iambic.penta.relaxed"}, "line.14": {"text": "Da\u00df sich die Nachwelt auch/ wie ihr/ erfreuen kan.", "tokens": ["Da\u00df", "sich", "die", "Nach\u00b7welt", "auch", "/", "wie", "ihr", "/", "er\u00b7freu\u00b7en", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "ADV", "$(", "PWAV", "PPER", "$(", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}