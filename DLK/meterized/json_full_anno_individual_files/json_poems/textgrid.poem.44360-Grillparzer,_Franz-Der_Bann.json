{"textgrid.poem.44360": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Der Bann", "genre": "verse", "period": "N.A.", "pub_year": 1819, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Leb wohl, Geliebte, ich mu\u00df scheiden,", "tokens": ["Leb", "wohl", ",", "Ge\u00b7lieb\u00b7te", ",", "ich", "mu\u00df", "schei\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "$,", "NN", "$,", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Es treibt mich fort in Angst und Qual,", "tokens": ["Es", "treibt", "mich", "fort", "in", "Angst", "und", "Qual", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Fort von der Wohnstatt meiner Freuden,", "tokens": ["Fort", "von", "der", "Wohn\u00b7statt", "mei\u00b7ner", "Freu\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Fort von dem Weibe meiner Wahl.", "tokens": ["Fort", "von", "dem", "Wei\u00b7be", "mei\u00b7ner", "Wahl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.2": {"line.1": {"text": "Nicht dieser Blick und diese Z\u00e4hren!", "tokens": ["Nicht", "die\u00b7ser", "Blick", "und", "die\u00b7se", "Z\u00e4h\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PDAT", "NN", "KON", "PDAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Verbirg dein holdes Angesicht!", "tokens": ["Ver\u00b7birg", "dein", "hol\u00b7des", "An\u00b7ge\u00b7sicht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du kannst das Scheiden mir erschweren,", "tokens": ["Du", "kannst", "das", "Schei\u00b7den", "mir", "er\u00b7schwe\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Doch mir ersparen kannst dus nicht.", "tokens": ["Doch", "mir", "er\u00b7spa\u00b7ren", "kannst", "dus", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVINF", "VMFIN", "PIS", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Denn wisse, wenn du mich umschlungen,", "tokens": ["Denn", "wis\u00b7se", ",", "wenn", "du", "mich", "um\u00b7schlun\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Umschlangst du keinen ", "tokens": ["Um\u00b7schlangst", "du", "kei\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPER", "PIAT"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Der Abgott deiner Huldigungen,", "tokens": ["Der", "Ab\u00b7gott", "dei\u00b7ner", "Hul\u00b7di\u00b7gun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Er ist belegt mit Acht und Bann.", "tokens": ["Er", "ist", "be\u00b7legt", "mit", "Acht", "und", "Bann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "APPR", "CARD", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Der F\u00fcrstin, der die Welt zu eigen,", "tokens": ["Der", "F\u00fcrs\u00b7tin", ",", "der", "die", "Welt", "zu", "ei\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der alles huldigt, was da lebt,", "tokens": ["Der", "al\u00b7les", "hul\u00b7digt", ",", "was", "da", "lebt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "$,", "PRELS", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Vor der sich alle Wesen beugen,", "tokens": ["Vor", "der", "sich", "al\u00b7le", "We\u00b7sen", "beu\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PRF", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Hab ich im Wahnsinn widerstrebt;", "tokens": ["Hab", "ich", "im", "Wahn\u00b7sinn", "wi\u00b7der\u00b7strebt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Mit ihrer Schwester, sinnverwirret,", "tokens": ["Mit", "ih\u00b7rer", "Schwes\u00b7ter", ",", "sinn\u00b7ver\u00b7wir\u00b7ret", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die ohne Heimat, ohne Haus,", "tokens": ["Die", "oh\u00b7ne", "Hei\u00b7mat", ",", "oh\u00b7ne", "Haus", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "$,", "KOUI", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Durch Erd und Luft und Wellen irret,", "tokens": ["Durch", "Erd", "und", "Luft", "und", "Wel\u00b7len", "ir\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zog ich in wilder Jagd hinaus.", "tokens": ["Zog", "ich", "in", "wil\u00b7der", "Jagd", "hin\u00b7aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Im Mondenglanz, auf fl\u00fcchtgem Fu\u00dfe,", "tokens": ["Im", "Mon\u00b7den\u00b7glanz", ",", "auf", "fl\u00fccht\u00b7gem", "Fu\u00b7\u00dfe", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Schlang ich mit ihr den Geisterreihn,", "tokens": ["Schlang", "ich", "mit", "ihr", "den", "Geis\u00b7ter\u00b7reihn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und alles Wirklichen Genusse", "tokens": ["Und", "al\u00b7les", "Wirk\u00b7li\u00b7chen", "Ge\u00b7nus\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Entsagt ich um den holden Schein.", "tokens": ["Ent\u00b7sagt", "ich", "um", "den", "hol\u00b7den", "Schein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Da sprach die F\u00fcrstin zornentglommen,", "tokens": ["Da", "sprach", "die", "F\u00fcrs\u00b7tin", "zor\u00b7nent\u00b7glom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "\u00bbverschm\u00e4hst du so, was ich dir bot,", "tokens": ["\u00bb", "ver\u00b7schm\u00e4hst", "du", "so", ",", "was", "ich", "dir", "bot", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "ADV", "$,", "PWS", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So seis auf immer dir genommen,", "tokens": ["So", "seis", "auf", "im\u00b7mer", "dir", "ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ADV", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Du vogelfrei bis an den Tod.", "tokens": ["Du", "vo\u00b7gel\u00b7frei", "bis", "an", "den", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Von Wunsch zu Wunsch in ewger Kette", "tokens": ["Von", "Wunsch", "zu", "Wunsch", "in", "ew\u00b7ger", "Ket\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "APPR", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und rastlos wie du bist, so bleib,", "tokens": ["Und", "rast\u00b7los", "wie", "du", "bist", ",", "so", "bleib", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADJD", "KOKOM", "PPER", "VAFIN", "$,", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dir sei kein Haus und keine St\u00e4tte,", "tokens": ["Dir", "sei", "kein", "Haus", "und", "kei\u00b7ne", "St\u00e4t\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "KON", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Kein Freund, kein Bruder und kein Weib;", "tokens": ["Kein", "Freund", ",", "kein", "Bru\u00b7der", "und", "kein", "Weib", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN", "KON", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Ein B\u00fcttel aber beigegeben,", "tokens": ["Ein", "B\u00fct\u00b7tel", "a\u00b7ber", "bei\u00b7ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit dir, in dir, la\u00df er dich nie,", "tokens": ["Mit", "dir", ",", "in", "dir", ",", "la\u00df", "er", "dich", "nie", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "APPR", "PPER", "$,", "VVIMP", "PPER", "PRF", "ADV", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Der peitsche rastlos dich durchs Leben,", "tokens": ["Der", "peit\u00b7sche", "rast\u00b7los", "dich", "durchs", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der wilde D\u00e4mon Phantasie.", "tokens": ["Der", "wil\u00b7de", "D\u00e4\u00b7mon", "Phan\u00b7ta\u00b7sie", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Er hei\u00dfe dich nach allem fassen,", "tokens": ["Er", "hei\u00b7\u00dfe", "dich", "nach", "al\u00b7lem", "fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Was irdisch sch\u00f6n, mit raschem Geiz,", "tokens": ["Was", "ir\u00b7disch", "sch\u00f6n", ",", "mit", "ra\u00b7schem", "Geiz", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "ADJD", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch h\u00e4ltst dus, m\u00fcssest du es hassen", "tokens": ["Doch", "h\u00e4ltst", "dus", ",", "m\u00fcs\u00b7sest", "du", "es", "has\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "NE", "$,", "VMFIN", "PPER", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und M\u00e4ngel sieh in jedem Reiz;", "tokens": ["Und", "M\u00e4n\u00b7gel", "sieh", "in", "je\u00b7dem", "Reiz", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Verdammet Schatten nachzujagen,", "tokens": ["Ver\u00b7dam\u00b7met", "Schat\u00b7ten", "nach\u00b7zu\u00b7ja\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Buhl doch um Augenblickes Ku\u00df,", "tokens": ["Buhl", "doch", "um", "Au\u00b7gen\u00b7bli\u00b7ckes", "Ku\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "APPR", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es fehle Kraft dir zum Entsagen", "tokens": ["Es", "feh\u00b7le", "Kraft", "dir", "zum", "Ent\u00b7sa\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und Selbstbegrenzung zum Genu\u00df.", "tokens": ["Und", "Selbst\u00b7be\u00b7gren\u00b7zung", "zum", "Ge\u00b7nu\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Die Sprache will ich dir verwandeln,", "tokens": ["Die", "Spra\u00b7che", "will", "ich", "dir", "ver\u00b7wan\u00b7deln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dein H\u00f6rer sei der Mi\u00dfverstand,", "tokens": ["Dein", "H\u00f6\u00b7rer", "sei", "der", "Mi\u00df\u00b7ver\u00b7stand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mi\u00dflingen sei mit deinem Handeln,", "tokens": ["Mi\u00df\u00b7lin\u00b7gen", "sei", "mit", "dei\u00b7nem", "Han\u00b7deln", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Entzweit auf immer Kopf und Hand;", "tokens": ["Ent\u00b7zweit", "auf", "im\u00b7mer", "Kopf", "und", "Hand", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADV", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.13": {"line.1": {"text": "Die ", "tokens": ["Die"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.2": {"text": "Sie schaudere zur\u00fcck vor dir!", "tokens": ["Sie", "schau\u00b7de\u00b7re", "zu\u00b7r\u00fcck", "vor", "dir", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und sagt sie: ja, hat sie gew\u00e4hret,", "tokens": ["Und", "sagt", "sie", ":", "ja", ",", "hat", "sie", "ge\u00b7w\u00e4h\u00b7ret", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$.", "PTKANT", "$,", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-++--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "So t\u00f6t ihr Ja dir die Begier.", "tokens": ["So", "t\u00f6t", "ihr", "Ja", "dir", "die", "Be\u00b7gier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.14": {"line.1": {"text": "Und da\u00df der letzte Trost versaget,", "tokens": ["Und", "da\u00df", "der", "letz\u00b7te", "Trost", "ver\u00b7sa\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Verewigt Rache sei und Leid,", "tokens": ["Ve\u00b7re\u00b7wigt", "Ra\u00b7che", "sei", "und", "Leid", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "NN", "VAFIN", "KON", "NN", "$,"], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "lin