"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Bist du's, so hemme nichts, was in dir wogt und lodert;", "tokens": ["Bist", "du's", ",", "so", "hem\u00b7me", "nichts", ",", "was", "in", "dir", "wogt", "und", "lo\u00b7dert", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "$,", "ADV", "VVFIN", "PIS", "$,", "PRELS", "APPR", "PPER", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Stell's dar, und wandle k\u00fchn auch au\u00dfer Bahn und Spur.", "tokens": ["Stell's", "dar", ",", "und", "wand\u00b7le", "k\u00fchn", "auch", "au\u00b7\u00dfer", "Bahn", "und", "Spur", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$,", "KON", "VVFIN", "ADJD", "ADV", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Doch wenn die Kunst Vollendung fodert,", "tokens": ["Doch", "wenn", "die", "Kunst", "Vol\u00b7len\u00b7dung", "fo\u00b7dert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.9": {"text": "So gieb sie auf: ", "tokens": ["So", "gieb", "sie", "auf", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "Natur ist Eins und Alles. Du erkennest", "tokens": ["Na\u00b7tur", "ist", "Eins", "und", "Al\u00b7les", ".", "Du", "er\u00b7ken\u00b7nest"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "VAFIN", "NN", "KON", "PIS", "$.", "PPER", "VVFIN"], "met