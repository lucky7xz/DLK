{"textgrid.poem.53099": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es ist ja wahr, wir haben nun", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es ist ja wahr, wir haben nun", "tokens": ["Es", "ist", "ja", "wahr", ",", "wir", "ha\u00b7ben", "nun"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$,", "PPER", "VAFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die beste Seel- vnd Augenweide,", "tokens": ["Die", "bes\u00b7te", "Seel", "vnd", "Au\u00b7gen\u00b7wei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "TRUNC", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn auff dem bunten Blumen-Kleide", "tokens": ["Wenn", "auff", "dem", "bun\u00b7ten", "Blu\u00b7men\u00b7Klei\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die\u00df immer dem zuvor wil thun", "tokens": ["Die\u00df", "im\u00b7mer", "dem", "zu\u00b7vor", "wil", "thun"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "ADV", "ART", "ADV", "VMFIN", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Vnd pr\u00e4chtiger sich meint zu machen,", "tokens": ["Vnd", "pr\u00e4ch\u00b7ti\u00b7ger", "sich", "meint", "zu", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "PRF", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Daher man jetzt sieht alles lachen.", "tokens": ["Da\u00b7her", "man", "jetzt", "sieht", "al\u00b7les", "la\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PIS", "ADV", "VVFIN", "PIS", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.2": {"line.1": {"text": "Geht, Kinder, auff das Feld zerstrewt,", "tokens": ["Geht", ",", "Kin\u00b7der", ",", "auff", "das", "Feld", "zer\u00b7strewt", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$,", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vnd pfl\u00fcckt euch von der Frucht des Lentzen,", "tokens": ["Vnd", "pfl\u00fcckt", "euch", "von", "der", "Frucht", "des", "Lent\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Hie gelb vnd blaw, dort gr\u00fcn zu Kr\u00e4ntzen,", "tokens": ["Hie", "gelb", "vnd", "blaw", ",", "dort", "gr\u00fcn", "zu", "Kr\u00e4nt\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "$,", "ADV", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Beraubt das sch\u00f6ne Meyen-Kleid!", "tokens": ["Be\u00b7raubt", "das", "sch\u00f6\u00b7ne", "Meyen\u00b7Kleid", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Geht von Narcissen vnd Violen", "tokens": ["Geht", "von", "Nar\u00b7cis\u00b7sen", "vnd", "Vi\u00b7o\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "So viel euch gut d\u00fcnckt, einzuholen!", "tokens": ["So", "viel", "euch", "gut", "d\u00fcnckt", ",", "ein\u00b7zu\u00b7ho\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "ADJD", "VVFIN", "$,", "VVIZU", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Doch, eh' jhr die\u00df vnd das ber\u00fchrt,", "tokens": ["Doch", ",", "eh'", "jhr", "die\u00df", "vnd", "das", "be\u00b7r\u00fchrt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PPER", "PDS", "KON", "PDS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So schwingt zuvor au\u00df diesen Schrancken", "tokens": ["So", "schwingt", "zu\u00b7vor", "au\u00df", "die\u00b7sen", "Schran\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Hienauff gen Himmel die Gedancken,", "tokens": ["Hien\u00b7auff", "gen", "Him\u00b7mel", "die", "Ge\u00b7dan\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "APPR", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wo zu Gem\u00fcht euch wird gef\u00fchrt,", "tokens": ["Wo", "zu", "Ge\u00b7m\u00fcht", "euch", "wird", "ge\u00b7f\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "NN", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Was dort in jenem Krantz der Ehren", "tokens": ["Was", "dort", "in", "je\u00b7nem", "Krantz", "der", "Eh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "APPR", "PDAT", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "F\u00fcr sch\u00f6ne Blumen doch geh\u00f6ren!", "tokens": ["F\u00fcr", "sch\u00f6\u00b7ne", "Blu\u00b7men", "doch", "ge\u00b7h\u00f6\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Der Lielien Farben-reicher Pracht,", "tokens": ["Der", "Lie\u00b7li\u00b7en", "Fa\u00b7rben\u00b7rei\u00b7cher", "Pracht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die Zier der Tulipan vnd Nelcken", "tokens": ["Die", "Zier", "der", "Tu\u00b7li\u00b7pan", "vnd", "Nel\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Mu\u00df offt de\u00df Abends hie verwelcken,", "tokens": ["Mu\u00df", "offt", "de\u00df", "A\u00b7bends", "hie", "ver\u00b7wel\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie sch\u00f6n sie vns auch angelacht;", "tokens": ["Wie", "sch\u00f6n", "sie", "vns", "auch", "an\u00b7ge\u00b7lacht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Ewig-gr\u00fcne Krantz der Frommen", "tokens": ["Der", "E\u00b7wig\u00b7gr\u00fc\u00b7ne", "Krantz", "der", "From\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wird nie vmb seinen Zieraht kommen.", "tokens": ["Wird", "nie", "vmb", "sei\u00b7nen", "Zier\u00b7aht", "kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Es gr\u00fcnen Bluhmen Ihm zu gut,", "tokens": ["Es", "gr\u00fc\u00b7nen", "Bluh\u00b7men", "Ihm", "zu", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "PPER", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dort an den Silber-klahren Quellen,", "tokens": ["Dort", "an", "den", "Sil\u00b7ber\u00b7klah\u00b7ren", "Quel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Kein Nord ist, der sie wei\u00df zu f\u00e4llen,", "tokens": ["Kein", "Nord", "ist", ",", "der", "sie", "wei\u00df", "zu", "f\u00e4l\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "$,", "PRELS", "PPER", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Kein Brand, der jhnen schaden thut;", "tokens": ["Kein", "Brand", ",", "der", "jh\u00b7nen", "scha\u00b7den", "thut", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PRELS", "PPER", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Taw de\u00df Lebens mu\u00df sie netzen", "tokens": ["Der", "Taw", "de\u00df", "Le\u00b7bens", "mu\u00df", "sie", "net\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Vnd h\u00f6chste Klarheit auff sie setzen.", "tokens": ["Vnd", "h\u00f6chs\u00b7te", "Klar\u00b7heit", "auff", "sie", "set\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Wie seelig werden die doch seyn,", "tokens": ["Wie", "see\u00b7lig", "wer\u00b7den", "die", "doch", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "ART", "ADV", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die dort in eitel Vorjahrs-Tagen", "tokens": ["Die", "dort", "in", "ei\u00b7tel", "Vor\u00b7jahr\u00b7sTa\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "APPR", "ADJD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "So sch\u00f6ne Kr\u00e4ntze werden tragen!", "tokens": ["So", "sch\u00f6\u00b7ne", "Kr\u00e4nt\u00b7ze", "wer\u00b7den", "tra\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Fragt Ihr, ob dieser Blumen-schein", "tokens": ["Fragt", "Ihr", ",", "ob", "die\u00b7ser", "Blu\u00b7men\u00b7schein"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Auch ewer Haar einmahl wird kleiden?", "tokens": ["Auch", "e\u00b7wer", "Haar", "ein\u00b7mahl", "wird", "klei\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "ADV", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ja, wo jhr fromm k\u00f6nnt seyn, vnd