{"dta.poem.18485": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Eine andre.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1703", "urn": "urn:nbn:de:kobv:b4-200905199360", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Weil ich im leben war/ so war bey mir die lade", "tokens": ["Weil", "ich", "im", "le\u00b7ben", "war", "/", "so", "war", "bey", "mir", "die", "la\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPRART", "VVINF", "VAFIN", "$(", "ADV", "VAFIN", "APPR", "PPER", "ART", "NN"], "meter": "-+-+-+-++--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Der hochgekr\u00f6nten zunfft: Sey keiner drum ein thor/", "tokens": ["Der", "hoch\u00b7ge\u00b7kr\u00f6n\u00b7ten", "zunfft", ":", "Sey", "kei\u00b7ner", "drum", "ein", "thor", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "VAFIN", "PIS", "PAV", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und thu mir/ nun ich tod/ mit \u00fcblem urtheil schade/", "tokens": ["Und", "thu", "mir", "/", "nun", "ich", "tod", "/", "mit", "\u00fcb\u00b7lem", "ur\u00b7theil", "scha\u00b7de", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$(", "ADV", "PPER", "NN", "$(", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sonst schl\u00e4gt statt meiner ihn/ mein geist zum \u00e4ltsten vor.", "tokens": ["Sonst", "schl\u00e4gt", "statt", "mei\u00b7ner", "ihn", "/", "mein", "geist", "zum", "\u00e4lts\u00b7ten", "vor", "."], "token_info": ["word", "word", "word", "word", "word", "pu