{"textgrid.poem.24798": {"metadata": {"author": {"name": "Hofmannsthal, Hugo von", "birth": "N.A.", "death": "N.A."}, "title": "Prolog zu dem Buch \u00bbAnatol\u00ab", "genre": "verse", "period": "N.A.", "pub_year": 1901, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hohe Gitter, Taxushecken,", "tokens": ["Ho\u00b7he", "Git\u00b7ter", ",", "Ta\u00b7xus\u00b7he\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wappen nimmermehr vergoldet,", "tokens": ["Wap\u00b7pen", "nim\u00b7mer\u00b7mehr", "ver\u00b7gol\u00b7det", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sphinxe, durch das Dickicht schimmernd ...", "tokens": ["Sphin\u00b7xe", ",", "durch", "das", "Di\u00b7ckicht", "schim\u00b7mernd", "..."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "ART", "NN", "ADJD", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "... Knarrend \u00f6ffnen sich die Tore. \u2013", "tokens": ["...", "Knar\u00b7rend", "\u00f6ff\u00b7nen", "sich", "die", "To\u00b7re", ".", "\u2013"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "NN", "VVFIN", "PRF", "ART", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Mit verschlafenen Kaskaden", "tokens": ["Mit", "ver\u00b7schla\u00b7fe\u00b7nen", "Kas\u00b7ka\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+---+-", "measure": "unknown.measure.tri"}, "line.6": {"text": "Und verschlafenen Tritonen,", "tokens": ["Und", "ver\u00b7schla\u00b7fe\u00b7nen", "Tri\u00b7to\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.7": {"text": "Rokoko, verstaubt und lieblich,", "tokens": ["Ro\u00b7ko\u00b7ko", ",", "ver\u00b7staubt", "und", "lieb\u00b7lich", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVPP", "KON", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Seht ... das Wien des Canaletto,", "tokens": ["Seht", "...", "das", "Wi\u00b7en", "des", "Ca\u00b7na\u00b7let\u00b7to", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$(", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.9": {"text": "Wien von siebzehnhundertsechzig ...", "tokens": ["Wi\u00b7en", "von", "sieb\u00b7zehn\u00b7hun\u00b7dert\u00b7sech\u00b7zig", "..."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "APPR", "CARD", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "... Gr\u00fcne, braune stille Teiche,", "tokens": ["...", "Gr\u00fc\u00b7ne", ",", "brau\u00b7ne", "stil\u00b7le", "Tei\u00b7che", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "NN", "$,", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Glatt und marmorwei\u00df umrandet,", "tokens": ["Glatt", "und", "mar\u00b7mor\u00b7wei\u00df", "um\u00b7ran\u00b7det", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "In dem Spiegelbild der Nixen", "tokens": ["In", "dem", "Spie\u00b7gel\u00b7bild", "der", "Ni\u00b7xen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.13": {"text": "Spielen Gold- und Silberfische ...", "tokens": ["Spie\u00b7len", "Gold", "und", "Sil\u00b7ber\u00b7fi\u00b7sche", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "TRUNC", "KON", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Auf dem glattgeschornen Rasen", "tokens": ["Auf", "dem", "glatt\u00b7ge\u00b7schor\u00b7nen", "Ra\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Liegen zierlich gleiche Schatten", "tokens": ["Lie\u00b7gen", "zier\u00b7lich", "glei\u00b7che", "Schat\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ADJD", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Schlanker Oleanderst\u00e4mme;", "tokens": ["Schlan\u00b7ker", "O\u00b7lean\u00b7der\u00b7st\u00e4m\u00b7me", ";"], "token_info": ["word", "word", "punct"], "pos": ["NN", "NE", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.17": {"text": "Zweige w\u00f6lben sich zur Kuppel,", "tokens": ["Zwei\u00b7ge", "w\u00f6l\u00b7ben", "sich", "zur", "Kup\u00b7pel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PRF", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Zweige neigen sich zur Nische", "tokens": ["Zwei\u00b7ge", "nei\u00b7gen", "sich", "zur", "Ni\u00b7sche"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PRF", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "F\u00fcr die steifen Liebespaare,", "tokens": ["F\u00fcr", "die", "stei\u00b7fen", "Lie\u00b7be\u00b7spaa\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Heroinen und Heroen ...", "tokens": ["He\u00b7roi\u00b7nen", "und", "He\u00b7roen", "..."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.21": {"text": "Drei Delphine gie\u00dfen murmelnd", "tokens": ["Drei", "Del\u00b7phi\u00b7ne", "gie\u00b7\u00dfen", "mur\u00b7melnd"], "token_info": ["word", "word", "word", "word"], "pos": ["CARD", "NN", "VVFIN", "ADJD"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.22": {"text": "Fluten in ein Muschelbecken ...", "tokens": ["Flu\u00b7ten", "in", "ein", "Mu\u00b7schel\u00b7be\u00b7cken", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$("], "meter": "+--+-+--", "measure": "iambic.tri.invert"}, "line.23": {"text": "Duftige Kastanienbl\u00fcten", "tokens": ["Duf\u00b7ti\u00b7ge", "Kas\u00b7ta\u00b7ni\u00b7en\u00b7bl\u00fc\u00b7ten"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.24": {"text": "Gleiten, schwirren leuchtend nieder", "tokens": ["Glei\u00b7ten", ",", "schwir\u00b7ren", "leuch\u00b7tend", "nie\u00b7der"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "VVFIN", "ADJD", "PTKVZ"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.25": {"text": "Und ertrinken in den Becken ...", "tokens": ["Und", "er\u00b7trin\u00b7ken", "in", "den", "Be\u00b7cken", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.26": {"text": "... Hinter einer Taxusmauer", "tokens": ["...", "Hin\u00b7ter", "ei\u00b7ner", "Ta\u00b7xus\u00b7mau\u00b7er"], "token_info": ["punct", "word", "word", "word"], "pos": ["$(", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.27": {"text": "T\u00f6nen Geigen, Klarinetten,", "tokens": ["T\u00f6\u00b7nen", "Gei\u00b7gen", ",", "Kla\u00b7ri\u00b7net\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.28": {"text": "Und sie scheinen den grazi\u00f6sen", "tokens": ["Und", "sie", "schei\u00b7nen", "den", "gra\u00b7zi\u00b7\u00f6\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.29": {"text": "Amoretten zu entstr\u00f6men,", "tokens": ["A\u00b7mo\u00b7ret\u00b7ten", "zu", "ent\u00b7str\u00f6\u00b7men", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.30": {"text": "Die rings auf der Rampe sitzen,", "tokens": ["Die", "rings", "auf", "der", "Ram\u00b7pe", "sit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.31": {"text": "Fiedelnd oder Blumen windend,", "tokens": ["Fie\u00b7delnd", "o\u00b7der", "Blu\u00b7men", "win\u00b7dend", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.32": {"text": "Selbst von Blumen bunt umgeben,", "tokens": ["Selbst", "von", "Blu\u00b7men", "bunt", "um\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.33": {"text": "Die aus Marmorvasen str\u00f6men:", "tokens": ["Die", "aus", "Mar\u00b7mor\u00b7va\u00b7sen", "str\u00f6\u00b7men", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NE", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.34": {"text": "Goldlack und Jasmin und Flieder ...", "tokens": ["Gold\u00b7lack", "und", "Jas\u00b7min", "und", "Flie\u00b7der", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NE", "KON", "NN", "$("], "meter": "++-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.35": {"text": "... Auf der Rampe, zwischen ihnen", "tokens": ["...", "Auf", "der", "Ram\u00b7pe", ",", "zwi\u00b7schen", "ih\u00b7nen"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word"], "pos": ["$(", "APPR", "ART", "NN", "$,", "APPR", "PPER"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.36": {"text": "Sitzen auch kokette Frauen,", "tokens": ["Sit\u00b7zen", "auch", "ko\u00b7ket\u00b7te", "Frau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADJA", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.37": {"text": "Violette Monsignori ...", "tokens": ["Vi\u00b7o\u00b7let\u00b7te", "Mon\u00b7sig\u00b7no\u00b7ri", "..."], "token_info": ["word", "word", "punct"], "pos": ["NE", "NE", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.38": {"text": "Und im Gras, zu ihren F\u00fc\u00dfen", "tokens": ["Und", "im", "Gras", ",", "zu", "ih\u00b7ren", "F\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "$,", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.39": {"text": "Und auf Polstern, auf den Stufen", "tokens": ["Und", "auf", "Pols\u00b7tern", ",", "auf", "den", "Stu\u00b7fen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "$,", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.40": {"text": "Kavaliere und Abbati ...", "tokens": ["Ka\u00b7va\u00b7lie\u00b7re", "und", "Ab\u00b7ba\u00b7ti", "..."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NE", "$("], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.41": {"text": "Andre heben andre Frauen", "tokens": ["And\u00b7re", "he\u00b7ben", "and\u00b7re", "Frau\u00b7en"], "token_info": ["word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.42": {"text": "Aus den parf\u00fcmierten S\u00e4nften ...", "tokens": ["Aus", "den", "par\u00b7f\u00fc\u00b7mier\u00b7ten", "S\u00e4nf\u00b7ten", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.43": {"text": "... Durch die Zweige brechen Lichter,", "tokens": ["...", "Durch", "die", "Zwei\u00b7ge", "bre\u00b7chen", "Lich\u00b7ter", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.44": {"text": "Flimmern auf den blonden K\u00f6pfchen,", "tokens": ["Flim\u00b7mern", "auf", "den", "blon\u00b7den", "K\u00f6pf\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.45": {"text": "Scheinen auf den bunten Polstern,", "tokens": ["Schei\u00b7nen", "auf", "den", "bun\u00b7ten", "Pols\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.46": {"text": "Gleiten \u00fcber Kies und Rasen,", "tokens": ["Glei\u00b7ten", "\u00fc\u00b7ber", "Kies", "und", "Ra\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NE", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.47": {"text": "Gleiten \u00fcber das Ger\u00fcste,", "tokens": ["Glei\u00b7ten", "\u00fc\u00b7ber", "das", "Ge\u00b7r\u00fcs\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.48": {"text": "Das wir fl\u00fcchtig aufgeschlagen.", "tokens": ["Das", "wir", "fl\u00fcch\u00b7tig", "auf\u00b7ge\u00b7schla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.49": {"text": "Wein und Winde klettert aufw\u00e4rts", "tokens": ["Wein", "und", "Win\u00b7de", "klet\u00b7tert", "auf\u00b7w\u00e4rts"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "VVFIN", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.50": {"text": "Und umh\u00fcllt die lichten Balken,", "tokens": ["Und", "um\u00b7h\u00fcllt", "die", "lich\u00b7ten", "Bal\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.51": {"text": "Und dazwischen farben\u00fcppig", "tokens": ["Und", "da\u00b7zwi\u00b7schen", "far\u00b7be\u00b7n\u00fcp\u00b7pig"], "token_info": ["word", "word", "word"], "pos": ["KON", "PAV", "ADJD"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.52": {"text": "Flattert Teppich und Tapete,", "tokens": ["Flat\u00b7tert", "Tep\u00b7pich", "und", "Ta\u00b7pe\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "KON", "NN", "$,"], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.53": {"text": "Sch\u00e4ferszenen, keck gewoben,", "tokens": ["Sch\u00e4\u00b7fers\u00b7ze\u00b7nen", ",", "keck", "ge\u00b7wo\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.54": {"text": "Zierlich von Watteau entworfen ...", "tokens": ["Zier\u00b7lich", "von", "Wat\u00b7tea\u00b7u", "ent\u00b7wor\u00b7fen", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "VVPP", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}}, "stanza.2": {"line.1": {"text": "Eine Laube statt der B\u00fchne,", "tokens": ["Ei\u00b7ne", "Lau\u00b7be", "statt", "der", "B\u00fch\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sommersonne statt der Lampen,", "tokens": ["Som\u00b7mer\u00b7son\u00b7ne", "statt", "der", "Lam\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Also spielen wir Theater,", "tokens": ["Al\u00b7so", "spie\u00b7len", "wir", "The\u00b7a\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Spielen unsre eignen St\u00fccke,", "tokens": ["Spie\u00b7len", "uns\u00b7re", "eig\u00b7nen", "St\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Fr\u00fchgereift und zart und traurig,", "tokens": ["Fr\u00fch\u00b7ge\u00b7reift", "und", "zart", "und", "trau\u00b7rig", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Die Kom\u00f6die unsrer Seele,", "tokens": ["Die", "Ko\u00b7m\u00f6\u00b7die", "uns\u00b7rer", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Unsres F\u00fchlens Heut und Gestern,", "tokens": ["Uns\u00b7res", "F\u00fch\u00b7lens", "Heut", "und", "Ge\u00b7stern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "KON", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.8": {"text": "B\u00f6ser Dinge h\u00fcbsche Formel,", "tokens": ["B\u00f6\u00b7ser", "Din\u00b7ge", "h\u00fcb\u00b7sche", "For\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Glatte Worte, bunte Bilder,", "tokens": ["Glat\u00b7te", "Wor\u00b7te", ",", "bun\u00b7te", "Bil\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Halbes, heimliches Empfinden,", "tokens": ["Hal\u00b7bes", ",", "heim\u00b7li\u00b7ches", "Emp\u00b7fin\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Agonien, Episoden ...", "tokens": ["A\u00b7go\u00b7ni\u00b7en", ",", "E\u00b7pi\u00b7so\u00b7den", "..."], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$,", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Manche h\u00f6ren zu, nicht alle ...", "tokens": ["Man\u00b7che", "h\u00f6\u00b7ren", "zu", ",", "nicht", "al\u00b7le", "..."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PTKVZ", "$,", "PTKNEG", "PIAT", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Manche tr\u00e4umen, manche lachen,", "tokens": ["Man\u00b7che", "tr\u00e4u\u00b7men", ",", "man\u00b7che", "la\u00b7chen", ","], "token_info": ["word", "word", "pu