{"dta.poem.2553": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Hochzeit-Nacht Hn. P. H. d. R. und  \n Jungf. H. A. 1666.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Brich angenehme Nacht mit deinem Schmuck' hervor/", "tokens": ["Brich", "an\u00b7ge\u00b7neh\u00b7me", "Nacht", "mit", "dei\u00b7nem", "Schmuck'", "her\u00b7vor", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "APPR", "PPOSAT", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie sehr du sonst verhast so wirstu hier geliebet/", "tokens": ["Wie", "sehr", "du", "sonst", "ver\u00b7hast", "so", "wirs\u00b7tu", "hier", "ge\u00b7lie\u00b7bet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPER", "ADV", "VVFIN", "ADV", "VAFIN", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Weil deine tieffe still offt mehr Ergetzung giebet/", "tokens": ["Weil", "dei\u00b7ne", "tief\u00b7fe", "still", "offt", "mehr", "Er\u00b7get\u00b7zung", "gie\u00b7bet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADJD", "ADV", "PIAT", "NN", "VVFIN", "$("], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Als wenn der Tag sich weist an seinem Rosen-Thor/", "tokens": ["Als", "wenn", "der", "Tag", "sich", "weist", "an", "sei\u00b7nem", "Ro\u00b7sen\u00b7Thor", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "ART", "NN", "PRF", "VVFIN", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wer jemahls nur geliebt/ hat Opffer dir gebrennet/", "tokens": ["Wer", "je\u00b7mahls", "nur", "ge\u00b7liebt", "/", "hat", "Opf\u00b7fer", "dir", "ge\u00b7bren\u00b7net", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADV", "VVPP", "$(", "VAFIN", "NN", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Damit kein Argus nicht sein heimlich Feur gekennet.", "tokens": ["Da\u00b7mit", "kein", "Ar\u00b7gus", "nicht", "sein", "heim\u00b7lich", "Feur", "ge\u00b7ken\u00b7net", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PIAT", "NN", "PTKNEG", "PPOSAT", "ADJD", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Erschein\u2019 ingleichen auch der Clitemnestern Sohn/", "tokens": ["Er\u00b7schein'", "in\u00b7glei\u00b7chen", "auch", "der", "Cli\u00b7tem\u00b7nes\u00b7tern", "Sohn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "ADV", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Du s\u00fcsser Abendstern mit deinen g\u00fcldnen Br\u00fcdern/", "tokens": ["Du", "s\u00fcs\u00b7ser", "A\u00b7bends\u00b7tern", "mit", "dei\u00b7nen", "g\u00fcld\u00b7nen", "Br\u00fc\u00b7dern", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.9": {"text": "F\u00fchrt euren Reihen-Dantz und singt was von den Liedern/", "tokens": ["F\u00fchrt", "eu\u00b7ren", "Rei\u00b7hen\u00b7Dantz", "und", "singt", "was", "von", "den", "Lie\u00b7dern", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "KON", "VVFIN", "PIS", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "So heisse Lieb\u2019 erdacht/ als vor der Venus Thron", "tokens": ["So", "heis\u00b7se", "Lieb'", "er\u00b7dacht", "/", "als", "vor", "der", "Ve\u00b7nus", "Thron"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJA", "NN", "VVPP", "$(", "KOKOM", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Die Hertzen sich geb\u00fcckt/ das wunderholde wackeln", "tokens": ["Die", "Hert\u00b7zen", "sich", "ge\u00b7b\u00fcckt", "/", "das", "wun\u00b7der\u00b7hol\u00b7de", "wa\u00b7ckeln"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "PRF", "VVPP", "$(", "ART", "ADJA", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Zeugt noch ein lebend Feur in den verliebten Fackeln.", "tokens": ["Zeugt", "noch", "ein", "le\u00b7bend", "Feur", "in", "den", "ver\u00b7lieb\u00b7ten", "Fa\u00b7ckeln", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJD", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.13": {"text": "Du wirst dein Silberlicht Diana nicht entziehn/", "tokens": ["Du", "wirst", "dein", "Sil\u00b7ber\u00b7licht", "Di\u00b7a\u00b7na", "nicht", "ent\u00b7ziehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "NE", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Noch der ", "tokens": ["Noch", "der"], "token_info": ["word", "word"], "pos": ["ADV", "ART"], "meter": "+-", "measure": "trochaic.single"}, "line.15": {"text": "Gedenck\u2019 an jene Zeit und abgestohlne Freuden/", "tokens": ["Ge\u00b7denck", "an", "je\u00b7ne", "Zeit", "und", "ab\u00b7ge\u00b7stohl\u00b7ne", "Freu\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PDAT", "NN", "KON", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Da Latmus Steine dich von Flammen sahen gl\u00fchn/", "tokens": ["Da", "Lat\u00b7mus", "Stei\u00b7ne", "dich", "von", "Flam\u00b7men", "sa\u00b7hen", "gl\u00fchn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "NN", "PRF", "APPR", "NN", "VVFIN", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Als du Endimion dem Himmel vorgezogen/", "tokens": ["Als", "du", "En\u00b7di\u00b7mi\u00b7on", "dem", "Him\u00b7mel", "vor\u00b7ge\u00b7zo\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "ART", "NN", "VVPP", "$("], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}, "line.18": {"text": "Und hiengst an seiner Brust und nicht an deinem Bogen.", "tokens": ["Und", "hiengst", "an", "sei\u00b7ner", "Brust", "und", "nicht", "an", "dei\u00b7nem", "Bo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PPOSAT", "NN", "KON", "PTKNEG", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Jhr Sternen die ihr nun verschwendrisch habt geliebt/", "tokens": ["Ihr", "Ster\u00b7nen", "die", "ihr", "nun", "ver\u00b7schwend\u00b7risch", "habt", "ge\u00b7liebt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "PPER", "ADV", "ADJD", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Geht auf mit eurem Strahl und helfft die Braut-Nacht ziehren/", "tokens": ["Geht", "auf", "mit", "eu\u00b7rem", "Strahl", "und", "helfft", "die", "Braut\u00b7Nacht", "zieh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "APPR", "PPOSAT", "NN", "KON", "VVFIN", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Verdolmetscht ", "tokens": ["Ver\u00b7dol\u00b7metscht"], "token_info": ["word"], "pos": ["NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.22": {"text": "Der schlauhen Unter-Welt/ die sich im Lieben \u00fcbt/", "tokens": ["Der", "schlau\u00b7hen", "Un\u00b7ter\u00b7Welt", "/", "die", "sich", "im", "Lie\u00b7ben", "\u00fcbt", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "PRELS", "PRF", "APPRART", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Und vor der Sonnen-Licht des Mondenschein erwehlet/", "tokens": ["Und", "vor", "der", "Son\u00b7nen\u00b7Licht", "des", "Mon\u00b7den\u00b7schein", "er\u00b7weh\u00b7let", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Wenn sie die Einsamkeit bestimmter Stunden zehlet.", "tokens": ["Wenn", "sie", "die", "Ein\u00b7sam\u00b7keit", "be\u00b7stimm\u00b7ter", "Stun\u00b7den", "zeh\u00b7let", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Leandern ward bey Nacht und nicht bey Tage hei\u00df/", "tokens": ["Le\u00b7an\u00b7dern", "ward", "bey", "Nacht", "und", "nicht", "bey", "Ta\u00b7ge", "hei\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "APPR", "NN", "KON", "PTKNEG", "APPR", "NN", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "In diese Kappe will ein Buhler sich verh\u00fcllen/", "tokens": ["In", "die\u00b7se", "Kap\u00b7pe", "will", "ein", "Buh\u00b7ler", "sich", "ver\u00b7h\u00fcl\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VMFIN", "ART", "NN", "PRF", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Die frische Regung l\u00e4st sich nicht mit Worten stillen/", "tokens": ["Die", "fri\u00b7sche", "Re\u00b7gung", "l\u00e4st", "sich", "nicht", "mit", "Wor\u00b7ten", "stil\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PRF", "PTKNEG", "APPR", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Der Mensch der ist nur Mensch/ und nicht ein Zemblisch Ei\u00df/", "tokens": ["Der", "Mensch", "der", "ist", "nur", "Mensch", "/", "und", "nicht", "ein", "Zem\u00b7blisch", "Ei\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "VAFIN", "ADV", "NN", "$(", "KON", "PTKNEG", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Die kalte Mitternacht starrt nicht in seinen Gliedern/", "tokens": ["Die", "kal\u00b7te", "Mit\u00b7ter\u00b7nacht", "starrt", "nicht", "in", "sei\u00b7nen", "Glie\u00b7dern", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PTKNEG", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Wenn Liebes-Plitze gehn von sch\u00f6nen Augen Liedern.", "tokens": ["Wenn", "Lie\u00b7bes\u00b7P\u00b7lit\u00b7ze", "gehn", "von", "sch\u00f6\u00b7nen", "Au\u00b7gen", "Lie\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVFIN", "APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Ob f\u00fcr Gerichte gleich die Sch\u00f6nheit wird verklagt/", "tokens": ["Ob", "f\u00fcr", "Ge\u00b7rich\u00b7te", "gleich", "die", "Sch\u00f6n\u00b7heit", "wird", "ver\u00b7klagt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "NN", "ADV", "ART", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Kan Phrynens Lilgen-Brust der Richter Augen fangen/", "tokens": ["Kan", "Phry\u00b7nens", "Lil\u00b7gen\u00b7Brust", "der", "Rich\u00b7ter", "Au\u00b7gen", "fan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NE", "NN", "ART", "NN", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Da\u00df sie Barmhertzigkeit/ dem Recht nicht nachgehangen/", "tokens": ["Da\u00df", "sie", "Barm\u00b7hert\u00b7zig\u00b7keit", "/", "dem", "Recht", "nicht", "nach\u00b7ge\u00b7han\u00b7gen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "$(", "ART", "NN", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Da\u00df der verliebte Mund nicht/ was der W\u00fcrffel/ sagt.", "tokens": ["Da\u00df", "der", "ver\u00b7lieb\u00b7te", "Mund", "nicht", "/", "was", "der", "W\u00fcrf\u00b7fel", "/", "sagt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "PTKNEG", "$(", "PWS", "ART", "NN", "$(", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Man wei\u00df die Ursach auch so ihr Gesetz umbtrieben/", "tokens": ["Man", "wei\u00df", "die", "Ur\u00b7sach", "auch", "so", "ihr", "Ge\u00b7setz", "umb\u00b7trie\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "NN", "ADV", "ADV", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Die ", "tokens": ["Die"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.37": {"text": "Pericles den Athen und Grichen-Land geh\u00f6rt/", "tokens": ["Pe\u00b7ri\u00b7cles", "den", "A\u00b7then", "und", "Gri\u00b7chen\u00b7Land", "ge\u00b7h\u00f6rt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NE", "KON", "NN", "VVFIN", "$("], "meter": "+-+-+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.38": {"text": "Hat bey Aspasien was ihn bewegt/ gefunden/", "tokens": ["Hat", "bey", "As\u00b7pa\u00b7si\u00b7en", "was", "ihn", "be\u00b7wegt", "/", "ge\u00b7fun\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "APPR", "NE", "PWS", "PPER", "VVFIN", "$(", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Der Lacon und sein Weib sagt von Epaminonden", "tokens": ["Der", "La\u00b7con", "und", "sein", "Weib", "sagt", "von", "E\u00b7pa\u00b7mi\u00b7non\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "PPOSAT", "NN", "VVFIN", "APPR", "NE"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "(dem Abgott jener Zeit/ der L\u00e4nder hat zerst\u00f6rt)", "tokens": ["(", "dem", "Ab\u00b7gott", "je\u00b7ner", "Zeit", "/", "der", "L\u00e4n\u00b7der", "hat", "zer\u00b7st\u00f6rt", ")"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "PDAT", "NN", "$(", "ART", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Da\u00df Lieb ihn nur ergetzt; der Periander brannte/", "tokens": ["Da\u00df", "Lieb", "ihn", "nur", "er\u00b7getzt", ";", "der", "Pe\u00b7ri\u00b7an\u00b7der", "brann\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPER", "ADV", "VVPP", "$.", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Als in Pelopones\u2019 er einst Melissen kannte.", "tokens": ["Als", "in", "Pe\u00b7lo\u00b7po\u00b7nes'", "er", "einst", "Me\u00b7lis\u00b7sen", "kann\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "NE", "PPER", "ADV", "NE", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Und ists nun wunderns werth/ da\u00df itzt auch Flammen f\u00fchlt", "tokens": ["Und", "ists", "nun", "wun\u00b7derns", "werth", "/", "da\u00df", "itzt", "auch", "Flam\u00b7men", "f\u00fchlt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "ADV", "NN", "ADJD", "$(", "KOUS", "ADV", "ADV", "NN", "VVFIN"], "meter": "-+-+-+-+