{"textgrid.poem.57806": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der rote, der wei\u00dfe und der blutrote Klee,", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der rote, der wei\u00dfe und der blutrote Klee,", "tokens": ["Der", "ro\u00b7te", ",", "der", "wei\u00b7\u00dfe", "und", "der", "blut\u00b7ro\u00b7te", "Klee", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ART", "ADJA", "KON", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Die Liebe, die Treue und das Herz tut mir weh;", "tokens": ["Die", "Lie\u00b7be", ",", "die", "Treu\u00b7e", "und", "das", "Herz", "tut", "mir", "weh", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "KON", "ART", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+--+---+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und mein Herz, das ist traurig,", "tokens": ["Und", "mein", "Herz", ",", "das", "ist", "trau\u00b7rig", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "PDS", "VAFIN", "ADJD", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Und mein Herz, das ist schwer,", "tokens": ["Und", "mein", "Herz", ",", "das", "ist", "schwer", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "PDS", "VAFIN", "ADJD", "$,"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Denn die eine, die ich meine,", "tokens": ["Denn", "die", "ei\u00b7ne", ",", "die", "ich", "mei\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ART", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und die liebt mich nicht mehr.", "tokens": ["Und", "die", "liebt", "mich", "nicht", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "VVFIN", "PPER", "PTKNEG", "ADV", "$."], "meter": "-++--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Narzissen und Nelken und Veilchen sind sch\u00f6n,", "tokens": ["Nar\u00b7zis\u00b7sen", "und", "Nel\u00b7ken", "und", "Veil\u00b7chen", "sind", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "KON", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Ich will in die Fremde, die Fremde jetzt gehn;", "tokens": ["Ich", "will", "in", "die", "Frem\u00b7de", ",", "die", "Frem\u00b7de", "jetzt", "gehn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "ART", "NN", "$,", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "In der Stadt sind die M\u00e4dchen", "tokens": ["In", "der", "Stadt", "sind", "die", "M\u00e4d\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VAFIN", "ART", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Noch einmal so sch\u00f6n,", "tokens": ["Noch", "ein\u00b7mal", "so", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "ADJD", "$,"], "meter": "----+", "measure": "unknown.measure.single"}, "line.5": {"text": "Ich such' mir eine andre", "tokens": ["Ich", "such'", "mir", "ei\u00b7ne", "and\u00b7re"], "token_in