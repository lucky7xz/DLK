{"textgrid.poem.48971": {"metadata": {"author": {"name": "Kretschmann, Karl Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Rhingulphs Ermunterung", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Heil mir! da\u00df solch ein Saitenspiel", "tokens": ["Heil", "mir", "!", "da\u00df", "solch", "ein", "Sai\u00b7ten\u00b7spiel"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PPER", "$.", "KOUS", "PIAT", "ART", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Vom Himmel mir zum Loose fiel,", "tokens": ["Vom", "Him\u00b7mel", "mir", "zum", "Loo\u00b7se", "fiel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Womit ich, tugendhafter Mann,", "tokens": ["Wo\u00b7mit", "ich", ",", "tu\u00b7gend\u00b7haf\u00b7ter", "Mann", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dir Kampf und Arbeit lohnen kan!", "tokens": ["Dir", "Kampf", "und", "Ar\u00b7beit", "loh\u00b7nen", "kan", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "KON", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Zwar sang ich sonst an Irmgards Brust", "tokens": ["Zwar", "sang", "ich", "sonst", "an", "Irm\u00b7gards", "Brust"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "NE", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der G\u00f6ttin Freya Fr\u00fchlingslust,", "tokens": ["Der", "G\u00f6t\u00b7tin", "Frey\u00b7a", "Fr\u00fch\u00b7lings\u00b7lust", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und, warm von Herthas Honigwein,", "tokens": ["Und", ",", "warm", "von", "Her\u00b7thas", "Ho\u00b7nig\u00b7wein", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ADJD", "APPR", "NE", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den frischen bunten Herbst im Hain.", "tokens": ["Den", "fri\u00b7schen", "bun\u00b7ten", "Herbst", "im", "Hain", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Da str\u00f6mte durch den Eichengang", "tokens": ["Da", "str\u00f6m\u00b7te", "durch", "den", "Ei\u00b7chen\u00b7gang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im Walde wonniger Gesang,", "tokens": ["Im", "Wal\u00b7de", "won\u00b7ni\u00b7ger", "Ge\u00b7sang", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bis sich das Lied am Felsen brach;", "tokens": ["Bis", "sich", "das", "Lied", "am", "Fel\u00b7sen", "brach", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da sangen mirs die Elfen nach:", "tokens": ["Da", "san\u00b7gen", "mirs", "die", "El\u00b7fen", "nach", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Und ich war, wie die Biene schw\u00e4rmt,", "tokens": ["Und", "ich", "war", ",", "wie", "die", "Bie\u00b7ne", "schw\u00e4rmt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "$,", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.2": {"text": "Sich an den Sommerl\u00fcften w\u00e4rmt,", "tokens": ["Sich", "an", "den", "Som\u00b7mer\u00b7l\u00fcf\u00b7ten", "w\u00e4rmt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Vom Thaue trinkt, von Blumen i\u00dft,", "tokens": ["Vom", "Thau\u00b7e", "trinkt", ",", "von", "Blu\u00b7men", "i\u00dft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$,", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und mit Gesumme fr\u00f6hlich ist.", "tokens": ["Und", "mit", "Ge\u00b7sum\u00b7me", "fr\u00f6h\u00b7lich", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Doch m\u00e4chtig, wie Allvater schuf,", "tokens": ["Doch", "m\u00e4ch\u00b7tig", ",", "wie", "All\u00b7va\u00b7ter", "schuf", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "PWAV", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Traf meine Seel' ein andrer Ruf:", "tokens": ["Traf", "mei\u00b7ne", "Seel'", "ein", "an\u00b7drer", "Ruf", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u00bbheld Herman sey nun dein Gesang;", "tokens": ["\u00bb", "held", "Her\u00b7man", "sey", "nun", "dein", "Ge\u00b7sang", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "NE", "VAFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "\u00bbdein Lied sey, Varus Untergang!\u00ab", "tokens": ["\u00bb", "dein", "Lied", "sey", ",", "Va\u00b7rus", "Un\u00b7ter\u00b7gang", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["$(", "PPOSAT", "NN", "VAFIN", "$,", "NE", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Gefallen, ja gefallen ist", "tokens": ["Ge\u00b7fal\u00b7len", ",", "ja", "ge\u00b7fal\u00b7len", "ist"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "ADV", "VVPP", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vor uns der klugen R\u00f6mer List:", "tokens": ["Vor", "uns", "der", "klu\u00b7gen", "R\u00f6\u00b7mer", "List", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "ADJA", "NN", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Triumph! Zerdonnert ist die Macht", "tokens": ["Tri\u00b7umph", "!", "Zer\u00b7don\u00b7nert", "ist", "die", "Macht"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$.", "VVPP", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Stolzen, und ihr Ruhm wird Nacht!", "tokens": ["Der", "Stol\u00b7zen", ",", "und", "ihr", "Ruhm", "wird", "Nacht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KON", "PPOSAT", "NN", "VAFIN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Drum itzt von meinem Spiel zur\u00fcck", "tokens": ["Drum", "itzt", "von", "mei\u00b7nem", "Spiel", "zu\u00b7r\u00fcck"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "ADV", "APPR", "PPOSAT", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Freuden und der Liebe Gl\u00fcck!", "tokens": ["Der", "Freu\u00b7den", "und", "der", "Lie\u00b7be", "Gl\u00fcck", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Herab vom zarten Schwalbenton", "tokens": ["Her\u00b7ab", "vom", "zar\u00b7ten", "Schwal\u00b7ben\u00b7ton"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Tobt es in tiefern Saiten schon,", "tokens": ["Tobt", "es", "in", "tie\u00b7fern", "Sai\u00b7ten", "schon", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "ADV", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.8": {"line.1": {"text": "Und stimmt auf Hermans Siegeslied.", "tokens": ["Und", "stimmt", "auf", "Her\u00b7mans", "Sie\u00b7ges\u00b7lied", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NE", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit gro\u00dfer Thaten B\u00fcrde zieht", "tokens": ["Mit", "gro\u00b7\u00dfer", "Tha\u00b7ten", "B\u00fcr\u00b7de", "zieht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Heldenzeit auf ihrer Bahn,", "tokens": ["Die", "Hel\u00b7den\u00b7zeit", "auf", "ih\u00b7rer", "Bahn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und rei\u00dft das Lied mit sich hinan.", "tokens": ["Und", "rei\u00dft", "das", "Lied", "mit", "sich", "hi\u00b7nan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "PRF", "PTKVZ", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.9": {"line.1": {"text": "Dort wecket seines Jubels Schall", "tokens": ["Dort", "we\u00b7cket", "sei\u00b7nes", "Ju\u00b7bels", "Schall"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der g\u00f6ldnen Sterne Wiederhall,", "tokens": ["Der", "g\u00f6ld\u00b7nen", "Ster\u00b7ne", "Wie\u00b7der\u00b7hall", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und Hermans Name t\u00f6nt ins Zelt", "tokens": ["Und", "Her\u00b7mans", "Na\u00b7me", "t\u00f6nt", "ins", "Zelt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "NE", "NN", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der G\u00f6tter, wie hier durch die Welt.", "tokens": ["Der", "G\u00f6t\u00b7ter", ",", "wie", "hier", "durch", "die", "Welt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "\u00bbhorch!\u00ab \u2013 rufte Thuisko: \u2013 \u00bbhorcht, da zieht", "tokens"