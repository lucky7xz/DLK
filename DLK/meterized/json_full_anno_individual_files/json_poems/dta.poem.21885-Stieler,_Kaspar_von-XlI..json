{"dta.poem.21885": {"metadata": {"author": {"name": "Stieler, Kaspar von", "birth": "N.A.", "death": "N.A."}, "title": "XlI.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1660", "urn": "urn:nbn:de:kobv:b4-20586-2", "language": ["de:0.99"], "booktitle": "Filidor der Dorfferer [i. e. Stieler, Kaspar von]: Die Geharnschte Venus. Hamburg, 1660."}, "poem": {"stanza.1": {"line.1": {"text": "Wir singen. Fillis spielt die Fl\u00f6ten", "tokens": ["Wir", "sin\u00b7gen", ".", "Fil\u00b7lis", "spielt", "die", "Fl\u00f6\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "NE", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "den Schall merkt Sie und ich allein.", "tokens": ["den", "Schall", "merkt", "Sie", "und", "ich", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "KON", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "La\u00df/ Fillis/ la\u00df dein Fingern seyn/", "tokens": ["La\u00df", "/", "Fil\u00b7lis", "/", "la\u00df", "dein", "Fin\u00b7gern", "seyn", "/"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$(", "NE", "$(", "VVIMP", "PPOSAT", "NN", "VAINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Soll aber ich die Laute schlagen/", "tokens": ["Soll", "a\u00b7ber", "ich", "die", "Lau\u00b7te", "schla\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PPER", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}