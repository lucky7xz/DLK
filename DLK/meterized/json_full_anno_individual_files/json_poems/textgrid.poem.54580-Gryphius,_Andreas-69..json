{"textgrid.poem.54580": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "69.", "genre": "verse", "period": "N.A.", "pub_year": 1640, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sie dennoch sie/ mein Licht/ sie wil best\u00e4ndig seyn/", "tokens": ["Sie", "den\u00b7noch", "sie", "/", "mein", "Licht", "/", "sie", "wil", "be\u00b7st\u00e4n\u00b7dig", "seyn", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PPER", "$(", "PPOSAT", "NN", "$(", "PPER", "VMFIN", "ADJD", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ob die Zeit sich gleich ver\u00e4ndert und die Sonne sich versteckt/", "tokens": ["Ob", "die", "Zeit", "sich", "gleich", "ver\u00b7\u00e4n\u00b7dert", "und", "die", "Son\u00b7ne", "sich", "ver\u00b7steckt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PRF", "ADV", "VVPP", "KON", "ART", "NN", "PRF", "VVPP", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Und die w\u00fcsten Felder trauren/ und das Feld mit Schnee bedeckt/", "tokens": ["Und", "die", "w\u00fcs\u00b7ten", "Fel\u00b7der", "trau\u00b7ren", "/", "und", "das", "Feld", "mit", "Schnee", "be\u00b7deckt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVINF", "$(", "KON", "ART", "NN", "APPR", "NN", "VVPP", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Sie dennoch (wie sie schreibt) geht kein Ver\u00e4ndern ein", "tokens": ["Sie", "den\u00b7noch", "(", "wie", "sie", "schreibt", ")", "geht", "kein", "Ver\u00b7\u00e4n\u00b7dern", "ein"], "tok