{"dta.poem.3060": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "106.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Zu seinem Ebenbild seit Gott den Menschen schuf,", "tokens": ["Zu", "sei\u00b7nem", "E\u00b7ben\u00b7bild", "seit", "Gott", "den", "Men\u00b7schen", "schuf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie ungehorsam konnt er werden seinem Ruf?", "tokens": ["Wie", "un\u00b7ge\u00b7hor\u00b7sam", "konnt", "er", "wer\u00b7den", "sei\u00b7nem", "Ruf", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VMFIN", "PPER", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Weil er war Gottes Kind, und werden sollt' ein Mann,", "tokens": ["Weil", "er", "war", "Got\u00b7tes", "Kind", ",", "und", "wer\u00b7den", "sollt'", "ein", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VAFIN", "NN", "NN", "$,", "KON", "VAINF", "VMFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein freier Mann, der nur sich selbst gehorchen kann.", "tokens": ["Ein", "frei\u00b7er", "Mann", ",", "der", "nur", "sich", "selbst", "ge\u00b7hor\u00b7chen", "kann", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ADV", "PRF", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Darum den Willen hat sein Vater ihm gegeben,", "tokens": ["Da\u00b7rum", "den", "Wil\u00b7len", "hat", "sein", "Va\u00b7ter", "ihm", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "VAFIN", "PPOSAT", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sich zu gehorchen und ihm selbst zu widerstreben;", "tokens": ["Sich", "zu", "ge\u00b7hor\u00b7chen", "und", "ihm", "selbst", "zu", "wi\u00b7der\u00b7stre\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "PTKZU", "VVINF", "KON", "PPER", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Kraft dessen an sich selbst verzieren und unzieren", "tokens": ["Kraft", "des\u00b7sen", "an", "sich", "selbst", "ver\u00b7zie\u00b7ren", "und", "un\u00b7zie\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "PDS", "APPR", "PRF", "ADV", "VVINF", "KON", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Er nun mag Gottes Bild, und nur nicht ganz verlieren;", "tokens": ["Er", "nun", "mag", "Got\u00b7tes", "Bild", ",", "und", "nur", "nicht", "ganz", "ver\u00b7lie\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VMFIN", "NN", "NN", "$,", "KON", "ADV", "PTKNEG", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Kraft dessen er auch mag das Bild herstellen klar,", "tokens": ["Kraft", "des\u00b7sen", "er", "auch", "mag", "das", "Bild", "her\u00b7stel\u00b7len", "klar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADV", "VMFIN", "ART", "NN", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df er durch sich nun sei, was er durch Gott nur war.", "tokens": ["Da\u00df", "er", "durch", "sich", "nun", "sei", ",", "was", "er", "durch", "Gott", "nur", "war", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PRF", "ADV", "VAFIN", "$,", "PWS", "PPER", "APPR", "NN", "ADV", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}