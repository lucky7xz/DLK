{"dta.poem.23616": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Virtus philologica.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1840", "urn": "urn:nbn:de:kobv:b4-200905192626", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was r\u00fchmt ihr doch an Rom und Griechenland", "tokens": ["Was", "r\u00fchmt", "ihr", "doch", "an", "Rom", "und", "Grie\u00b7chen\u00b7land"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "APPR", "NE", "KON", "NE"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Stets Freiheit, Tapferkeit und Vaterland?", "tokens": ["Stets", "Frei\u00b7heit", ",", "Tap\u00b7fer\u00b7keit", "und", "Va\u00b7ter\u00b7land", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "O w\u00e4ret ihr nur Sklaven dort gewesen,", "tokens": ["O", "w\u00e4\u00b7ret", "ihr", "nur", "Skla\u00b7ven", "dort", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "ADV", "NN", "ADV", "VAPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Von eurem R\u00fchmen w\u00e4rt ihr l\u00e4ngst genesen!", "tokens": ["Von", "eu\u00b7rem", "R\u00fch\u00b7men", "w\u00e4rt", "ihr", "l\u00e4ngst", "ge\u00b7ne\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Zwar Sklaven seid ihr, eurer Wissenschaft,", "tokens": ["Zwar", "Skla\u00b7ven", "seid", "ihr", ",", "eu\u00b7rer", "Wis\u00b7sen\u00b7schaft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "NN", "VAFIN", "PPER", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die euch verzehret euer Mark und Kraft,", "tokens": ["Die", "euch", "ver\u00b7zeh\u00b7ret", "eu\u00b7er", "Mark", "und", "Kraft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df ihr trotz allen alten Herrlichkeiten", "tokens": ["Da\u00df", "ihr", "trotz", "al\u00b7len", "al\u00b7ten", "Herr\u00b7lich\u00b7kei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Schulf\u00fcchse seid und bleibt in unsern Zeiten.", "tokens": ["Schul\u00b7f\u00fcch\u00b7se", "seid", "und", "bleibt", "in", "un\u00b7sern", "Zei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "KON", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "---+-+-+-+-", "measure": "unknown.measure.tetra"}}}}}