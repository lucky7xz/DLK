{"dta.poem.18715": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang von", "birth": "N.A.", "death": "N.A."}, "title": "Wanderers Gem\u00fcthsruhe .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1819", "urn": "urn:nbn:de:kobv:b4-200905191692", "language": ["de:0.99"], "booktitle": "Goethe, Johann Wolfgang von: West-\u00f6stlicher Divan. Stuttgart, 1819."}, "poem": {"stanza.1": {"line.1": {"text": "Ueber\u2019s Niedertr\u00e4chtige", "tokens": ["Ue\u00b7ber's", "Nie\u00b7der\u00b7tr\u00e4ch\u00b7ti\u00b7ge"], "token_info": ["word", "word"], "pos": ["NE", "NE"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Niemand sich beklage;", "tokens": ["Nie\u00b7mand", "sich", "be\u00b7kla\u00b7ge", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PIS", "PRF", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Denn es ist das M\u00e4chtige,", "tokens": ["Denn", "es", "ist", "das", "M\u00e4ch\u00b7ti\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Was man dir auch sage.", "tokens": ["Was", "man", "dir", "auch", "sa\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "In dem Schlechten waltet es", "tokens": ["In", "dem", "Schlech\u00b7ten", "wal\u00b7tet", "es"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sich zu Hochgewinne,", "tokens": ["Sich", "zu", "Hoch\u00b7ge\u00b7win\u00b7ne", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PRF", "APPR", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Und mit Rechtem schaltet es", "tokens": ["Und", "mit", "Rech\u00b7tem", "schal\u00b7tet", "es"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "VVFIN", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ganz nach seinem Sinne.", "tokens": ["Ganz", "nach", "sei\u00b7nem", "Sin\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Wandrer! \u2014 Gegen solche Noth", "tokens": ["Wand\u00b7rer", "!", "Ge\u00b7gen", "sol\u00b7che", "Noth"], "token_info": ["word", "punct", "punct", "word", "word", "word"], "pos": ["NN", "$.", "$(", "APPR", "PIAT", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Wolltest du dich str\u00e4uben?", "tokens": ["Woll\u00b7test", "du", "dich", "str\u00e4u\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Wirbelwind und trocknen Koth", "tokens": ["Wir\u00b7bel\u00b7wind", "und", "trock\u00b7nen", "Koth"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "KON", "ADJA", "NN"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.4": {"text": "Lass sie drehn und st\u00e4uben.", "tokens": ["Lass", "sie", "drehn", "und", "st\u00e4u\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VVINF", "KON", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}