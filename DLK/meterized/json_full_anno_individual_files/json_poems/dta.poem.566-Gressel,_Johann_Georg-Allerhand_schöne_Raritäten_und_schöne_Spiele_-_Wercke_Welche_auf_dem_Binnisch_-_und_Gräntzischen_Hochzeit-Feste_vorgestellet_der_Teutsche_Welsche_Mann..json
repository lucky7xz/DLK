{"dta.poem.566": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Allerhand sch\u00f6ne Rarit\u00e4ten und sch\u00f6ne  \n Spiele - Wercke/  \n Welche auf dem  \n Binnisch - und Gr\u00e4ntzischen  \n Hochzeit-Feste vorgestellet  \n der Teutsche Welsche Mann.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Fragt ihr/ was ich bey euch in euren ", "tokens": ["Fragt", "ihr", "/", "was", "ich", "bey", "euch", "in", "eu\u00b7ren"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$(", "PWS", "PPER", "APPR", "PPER", "APPR", "PPOSAT"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "So sag\u2019 ich/ da\u00df ich blo\u00df darum bin hieher kommen/", "tokens": ["So", "sag'", "ich", "/", "da\u00df", "ich", "blo\u00df", "da\u00b7rum", "bin", "hie\u00b7her", "kom\u00b7men", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$(", "KOUS", "PPER", "ADV", "PAV", "VAFIN", "PAV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Weil ich aus einem Brieff j\u00fcngsthin la\u00df ohne Brill/", "tokens": ["Weil", "ich", "aus", "ei\u00b7nem", "Brieff", "j\u00fcng\u00b7sthin", "la\u00df", "oh\u00b7ne", "Brill", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "ADV", "VVFIN", "APPR", "NE", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da\u00df ihm Herr ", "tokens": ["Da\u00df", "ihm", "Herr"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPER", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Drum wolt ich euch und eure G\u00e4st", "tokens": ["Drum", "wolt", "ich", "euch", "und", "eu\u00b7re", "G\u00e4st"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VMFIN", "PPER", "PPER", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Erlustigen beym Hochzeit-Fest.", "tokens": ["Er\u00b7lus\u00b7ti\u00b7gen", "beym", "Hoch\u00b7zeit\u00b7Fest", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Sch\u00f6ne Rarit\u00e4t/ sch\u00f6ne Spiele-Werckerc.", "tokens": ["Sch\u00f6\u00b7ne", "Ra\u00b7ri\u00b7t\u00e4t", "/", "sch\u00f6\u00b7ne", "Spie\u00b7le\u00b7\u00b7Wer\u00b7ckerc", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$(", "ADJA", "NN", "$."], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}}}}}