{"dta.poem.2430": {"metadata": {"author": {"name": "Neumark, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Ubi abundavit delictum, ibi supera-  \n bundavit, gratia Dei.  Epist. ad Rom. cap.  5.  \n An die S\u00fcnde.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1652", "urn": "urn:nbn:de:kobv:b4-20428-0", "language": ["de:0.99"], "booktitle": "Neumark, Georg: Poetisch- und Musikalisches Lustw\u00e4ldchen. Hamburg, 1652."}, "poem": {"stanza.1": {"line.1": {"text": "La\u00df dich du armes Mensch/ die S\u00fcnde nicht erschrekken/", "tokens": ["La\u00df", "dich", "du", "ar\u00b7mes", "Mensch", "/", "die", "S\u00fcn\u00b7de", "nicht", "er\u00b7schrek\u00b7ken", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPER", "ADJA", "NN", "$(", "ART", "NN", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sie sey gleich wie das Blut aus jenen wehrten Schnekkt", "tokens": ["Sie", "sey", "gleich", "wie", "das", "Blut", "aus", "je\u00b7nen", "wehr\u00b7ten", "Schnekkt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "KOKOM", "ART", "NN", "APPR", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die man in China f\u00e4ngt; Sie sein gleich Purpurroht;", "tokens": ["Die", "man", "in", "Chi\u00b7na", "f\u00e4ngt", ";", "Sie", "sein", "gleich", "Pur\u00b7pur\u00b7roht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "APPR", "NE", "VVFIN", "$.", "PPER", "PPOSAT", "ADV", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sie sey gleich Karmesin/ es hat doch keine Noht", "tokens": ["Sie", "sey", "gleich", "Kar\u00b7me\u00b7sin", "/", "es", "hat", "doch", "kei\u00b7ne", "Noht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "NN", "$(", "PPER", "VAFIN", "ADV", "PIAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Jm fall du Reue tr\u00e4gst. Sie soll der Wolle gleichen;", "tokens": ["Jm", "fall", "du", "Reu\u00b7e", "tr\u00e4gst", ".", "Sie", "soll", "der", "Wol\u00b7le", "glei\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "NN", "VVFIN", "$.", "PPER", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Es sol der frische Schnee vor ihrer Sch\u00f6nheit weichen/", "tokens": ["Es", "sol", "der", "fri\u00b7sche", "Schnee", "vor", "ih\u00b7rer", "Sch\u00f6n\u00b7heit", "wei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Durch jenes wehrte Bad die Purpurrohte Fluht.", "tokens": ["Durch", "je\u00b7nes", "wehr\u00b7te", "Bad", "die", "Pur\u00b7pur\u00b7roh\u00b7te", "Fluht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDS", "VVFIN", "NE", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Durch welche fragestu? durch Christi theures Blut.", "tokens": ["Durch", "wel\u00b7che", "fra\u00b7ge\u00b7stu", "?", "durch", "Chris\u00b7ti", "theu\u00b7res", "Blut", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "VVFIN", "$.", "APPR", "NE", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}