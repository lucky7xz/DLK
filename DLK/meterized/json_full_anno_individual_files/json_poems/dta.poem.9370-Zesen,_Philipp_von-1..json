{"dta.poem.9370": {"metadata": {"author": {"name": "Zesen, Philipp von", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-20684-8", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was mag ich mich unterfangen?", "tokens": ["Was", "mag", "ich", "mich", "un\u00b7ter\u00b7fan\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "PRF", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Ach was untersteh ich mich", "tokens": ["Ach", "was", "un\u00b7ter\u00b7steh", "ich", "mich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ITJ", "PWS", "VVFIN", "PPER", "PRF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "deine rosen-rothe Wangen", "tokens": ["dei\u00b7ne", "ro\u00b7sen\u00b7ro\u00b7the", "Wan\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "anzuschauen und auch dich", "tokens": ["an\u00b7zu\u00b7schau\u00b7en", "und", "auch", "dich"], "token_info": ["word", "word", "word", "word"], "pos": ["VVIZU", "KON", "ADV", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "sch\u00f6nes Bild herf\u00fcr zu streichen/", "tokens": ["sch\u00f6\u00b7nes", "Bild", "her\u00b7f\u00fcr", "zu", "strei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "PTKZU", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "dem die Sterne selbsten weichen?", "tokens": ["dem", "die", "Ster\u00b7ne", "selbs\u00b7ten", "wei\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "K\u00f6nte gleich Apelles mahlen/", "tokens": ["K\u00f6n\u00b7te", "gleich", "A\u00b7pel\u00b7les", "mah\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "NE", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "dich/ ", "tokens": ["dich", "/"], "token_info": ["word", "punct"], "pos": ["PPER", "$("], "meter": "-", "measure": "single.down"}, "line.3": {"text": "und der Augen helle strahlen/", "tokens": ["und", "der", "Au\u00b7gen", "hel\u00b7le", "strah\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "dieser blancken Br\u00fcste schild/", "tokens": ["die\u00b7ser", "blan\u00b7cken", "Br\u00fcs\u00b7te", "schild", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "ADJD", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "kann doch nicht entworffen werden", "tokens": ["kann", "doch", "nicht", "ent\u00b7worf\u00b7fen", "wer\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "PTKNEG", "VVPP", "VAINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "deine tugend und geberden.", "tokens": ["dei\u00b7ne", "tu\u00b7gend", "und", "ge\u00b7ber\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Solche liebligkeit im sprechen/", "tokens": ["Sol\u00b7che", "lieb\u00b7lig\u00b7keit", "im", "spre\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPRART", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "das so milde freundlich seyn", "tokens": ["das", "so", "mil\u00b7de", "freund\u00b7lich", "seyn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "ADV", "ADJD", "ADJD", "VAINF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "kann mir muth und sinnen brechen;", "tokens": ["kann", "mir", "muth", "und", "sin\u00b7nen", "bre\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "NN", "KON", "VVINF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "wenn dein Antlitz bricht herrein/", "tokens": ["wenn", "dein", "Ant\u00b7litz", "bricht", "her\u00b7rein", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VVFIN", "PTKVZ", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "wenn die braunen Augen funckeln/", "tokens": ["wenn", "die", "brau\u00b7nen", "Au\u00b7gen", "fun\u00b7ckeln", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "kann mich keine Nacht verdunckeln.", "tokens": ["kann", "mich", "kei\u00b7ne", "Nacht", "ver\u00b7dun\u00b7ckeln", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "PIAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Deiner hohen Stirne prangen", "tokens": ["Dei\u00b7ner", "ho\u00b7hen", "Stir\u00b7ne", "pran\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "sch\u00f6n und braunlecht anzusehn", "tokens": ["sch\u00f6n", "und", "braun\u00b7lecht", "an\u00b7zu\u00b7sehn"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "KON", "VVFIN", "VVIZU"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "ist mein hoffen und verlangen/", "tokens": ["ist", "mein", "hof\u00b7fen", "und", "ver\u00b7lan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "VVINF", "KON", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "ach! wenn wird es wohl geschehn", "tokens": ["ach", "!", "wenn", "wird", "es", "wohl", "ge\u00b7schehn"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "KOUS", "VAFIN", "PPER", "ADV", "VVINF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "da\u00df da wird in meinen armen", "tokens": ["da\u00df", "da", "wird", "in", "mei\u00b7nen", "ar\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "VAFIN", "APPR", "PPOSAT", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "dem so schlangker Leib erwarmen.", "tokens": ["dem", "so", "schlang\u00b7ker", "Leib", "er\u00b7war\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Ich will mich mit macht bem\u00fchen", "tokens": ["Ich", "will", "mich", "mit", "macht", "be\u00b7m\u00fc\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PRF", "APPR", "VVFIN", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "zuerlangen deine gunst/", "tokens": ["zu\u00b7er\u00b7lan\u00b7gen", "dei\u00b7ne", "gunst", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "wil mit meinem singen zihen", "tokens": ["wil", "mit", "mei\u00b7nem", "sin\u00b7gen", "zi\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "APPR", "PPOSAT", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dich/ zu leschen meine brunst/", "tokens": ["Dich", "/", "zu", "le\u00b7schen", "mei\u00b7ne", "brunst", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$(", "PTKZU", "VVINF", "PPOSAT", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "wenn ich dieses werd\u2019 erlangen/", "tokens": ["wenn", "ich", "die\u00b7ses", "werd'", "er\u00b7lan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PDS", "VAFIN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "will ich gerne seyn gefangen.", "tokens": ["will", "ich", "ger\u00b7ne", "seyn", "ge\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "VAINF", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}