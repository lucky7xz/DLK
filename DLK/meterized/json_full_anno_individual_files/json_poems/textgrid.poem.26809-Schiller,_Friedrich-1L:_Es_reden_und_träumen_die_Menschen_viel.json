{"textgrid.poem.26809": {"metadata": {"author": {"name": "Schiller, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es reden und tr\u00e4umen die Menschen viel", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es reden und tr\u00e4umen die Menschen viel", "tokens": ["Es", "re\u00b7den", "und", "tr\u00e4u\u00b7men", "die", "Men\u00b7schen", "viel"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVINF", "KON", "VVFIN", "ART", "NN", "ADV"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Von bessern k\u00fcnftigen Tagen,", "tokens": ["Von", "bes\u00b7sern", "k\u00fcnf\u00b7ti\u00b7gen", "Ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Nach einem gl\u00fccklichen goldenen Ziel", "tokens": ["Nach", "ei\u00b7nem", "gl\u00fcck\u00b7li\u00b7chen", "gol\u00b7de\u00b7nen", "Ziel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Sieht man sie rennen und jagen.", "tokens": ["Sieht", "man", "sie", "ren\u00b7nen", "und", "ja\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PPER", "VVFIN", "KON", "VVINF", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.5": {"text": "Die Welt wird alt und wird wieder jung,", "tokens": ["Die", "Welt", "wird", "alt", "und", "wird", "wie\u00b7der", "jung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "KON", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+----+", "measure": "unknown.measure.tri"}, "line.6": {"text": "Doch der Mensch hofft immer Verbesserung.", "tokens": ["Doch", "der", "Mensch", "hofft", "im\u00b7mer", "Ver\u00b7bes\u00b7se\u00b7rung", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ADV", "NN", "$."], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Die Hoffnung f\u00fchrt ihn ins Leben ein,", "tokens": ["Die", "Hoff\u00b7nung", "f\u00fchrt", "ihn", "ins", "Le\u00b7ben", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sie umflattert den fr\u00f6hlichen Knaben,", "tokens": ["Sie", "um\u00b7flat\u00b7tert", "den", "fr\u00f6h\u00b7li\u00b7chen", "Kna\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Den J\u00fcngling locket ihr Zauberschein,", "tokens": ["Den", "J\u00fcng\u00b7ling", "lo\u00b7cket", "ihr", "Zau\u00b7ber\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Sie wird mit dem Greis nicht begraben,", "tokens": ["Sie", "wird", "mit", "dem", "Greis", "nicht", "be\u00b7gra\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ART", "NN", "PTKNEG", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Denn beschlie\u00dft er im Grabe den m\u00fcden Lauf,", "tokens": ["Denn", "be\u00b7schlie\u00dft", "er", "im", "Gra\u00b7be", "den", "m\u00fc\u00b7den", "Lauf", ","], "token_info": ["word", "word", "word", "word", "word