{"dta.poem.21714": {"metadata": {"author": {"name": "Kosegarten, Ludwig Gotthard", "birth": "N.A.", "death": "N.A."}, "title": "Narcissus.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1802", "urn": "urn:nbn:de:kobv:b4-200905193310", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Echo, die Zarte, verschm&#228;ht' er verstockt. In", "tokens": ["E\u00b7cho", ",", "die", "Zar\u00b7te", ",", "ver\u00b7schm", "&#228;", "ht'", "er", "ver\u00b7stockt", ".", "In"], "token_info": ["word", "punct", "word", "word", "punct", "word", "XML_entity", "word", "word", "word", "punct", "word"], "pos": ["NN", "$,", "ART", "NN", "$,", "VVFIN", "$(", "VVFIN", "PPER", "VVPP", "$.", "APPR"], "meter": "+--+--+---+-", "measure": "dactylic.tri.plus"}, "line.2": {"text": "sich selber entbrennend,", "tokens": ["sich", "sel\u00b7ber", "ent\u00b7bren\u00b7nend", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PRF", "ADV", "VVPP", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Fasst' ihn beth\u00f6render Wahn, deckt' ihn die", "tokens": ["Fasst'", "ihn", "be\u00b7th\u00f6\u00b7ren\u00b7der", "Wahn", ",", "deckt'", "ihn", "die"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADJA", "NN", "$,", "PRELS", "PPER", "ART"], "meter": "+--+--++-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "stygische Nacht.", "tokens": ["sty\u00b7gi\u00b7sche", "Nacht", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Scheut, ihr Verg\u00f6ttrer des Ich, ", "tokens": ["Scheut", ",", "ihr", "Ver\u00b7g\u00f6tt\u00b7rer", "des", "Ich", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "ART", "PPER", "$,"], "meter": "++-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Echo des Herzens", "tokens": ["E\u00b7cho", "des", "Her\u00b7zens"], "token_info": ["word", "word", "word"], "pos": ["NN", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "D\u00fcrft' euch erstummen, den Geist D\u00fcnkel und", "tokens": ["D\u00fcrft'", "euch", "er\u00b7stum\u00b7men", ",", "den", "Geist", "D\u00fcn\u00b7kel", "und"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "VVPP", "$,", "ART", "NN", "NN", "KON"], "meter": "-+-+---+-+", "measure": "zehnsilber"}, "line.8": {"text": "Dunkel umfahn.", "tokens": ["Dun\u00b7kel", "um\u00b7fahn", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVINF", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}}}}