{"textgrid.poem.43192": {"metadata": {"author": {"name": "Christen, Ada", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sind es Schatten ferner Zeiten,", "tokens": ["Sind", "es", "Schat\u00b7ten", "fer\u00b7ner", "Zei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schatten schon aus Zukunftstagen,", "tokens": ["Schat\u00b7ten", "schon", "aus", "Zu\u00b7kunfts\u00b7ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Die durch meine Seele gleiten,", "tokens": ["Die", "durch", "mei\u00b7ne", "See\u00b7le", "glei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die zu mir her\u00fcberragen?", "tokens": ["Die", "zu", "mir", "her\u00b7\u00fc\u00b7berr\u00b7a\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Denn oft bluten alle Wunden,", "tokens": ["Denn", "oft", "blu\u00b7ten", "al\u00b7le", "Wun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Alle Sterne, sie erblassen \u2013", "tokens": ["Al\u00b7le", "Ster\u00b7ne", ",", "sie", "er\u00b7blas\u00b7sen", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PPER", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ich kann in solchen Stunden", "tokens": ["Und", "ich", "kann", "in", "sol\u00b7chen", "Stun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VMFIN", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nichts mehr lieben \u2013 nichts mehr hassen.", "tokens": ["Nichts", "mehr", "lie\u00b7ben", "\u2013", "nichts", "mehr", "has\u00b7sen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "VVINF", "$(", "PIS", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Sind es Schatten ferner Zeiten,", "tokens": ["Sind", "es", "Schat\u00b7ten", "fer\u00b7ner", "Zei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schatten schon aus Zukunftstagen,", "tokens": ["Schat\u00b7ten", "schon", "aus", "Zu\u00b7kunfts\u00b7ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Die durch meine Seele gleiten,", "tokens": ["Die", "durch", "mei\u00b7ne", "See\u00b7le", "glei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die zu mir her\u00fcberragen?", "tokens": ["Die", "zu", "mir", "her\u00b7\u00fc\u00b7berr\u00b7a\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Denn oft blute