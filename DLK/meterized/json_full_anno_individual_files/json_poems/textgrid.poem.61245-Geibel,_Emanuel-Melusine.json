{"textgrid.poem.61245": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Melusine", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es wohnt das M\u00e4dchen wunderhold", "tokens": ["Es", "wohnt", "das", "M\u00e4d\u00b7chen", "wun\u00b7der\u00b7hold"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mitten im Walde;", "tokens": ["Mit\u00b7ten", "im", "Wal\u00b7de", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Was da webet und gr\u00fcnt und bl\u00fcht,", "tokens": ["Was", "da", "we\u00b7bet", "und", "gr\u00fcnt", "und", "bl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Gehorcht ihr balde.", "tokens": ["Ge\u00b7horcht", "ihr", "bal\u00b7de", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Und tritt sie fr\u00fch aus ihrer T\u00fcr", "tokens": ["Und", "tritt", "sie", "fr\u00fch", "aus", "ih\u00b7rer", "T\u00fcr"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auf leichten F\u00fc\u00dfen,", "tokens": ["Auf", "leich\u00b7ten", "F\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Flattern die V\u00f6gel um sie her,", "tokens": ["Flat\u00b7tern", "die", "V\u00f6\u00b7gel", "um", "sie", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Die blauen Blumen gr\u00fc\u00dfen.", "tokens": ["Die", "blau\u00b7en", "Blu\u00b7men", "gr\u00fc\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Das fleckige Rehlein h\u00e4lt ihr still,", "tokens": ["Das", "flec\u00b7ki\u00b7ge", "Reh\u00b7lein", "h\u00e4lt", "ihr", "still", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "L\u00e4sset sich streicheln mit Nicken;", "tokens": ["L\u00e4s\u00b7set", "sich", "strei\u00b7cheln", "mit", "Ni\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "VVFIN", "APPR", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Sie hat gez\u00e4hmt den jungen Wolf", "tokens": ["Sie", "hat", "ge\u00b7z\u00e4hmt", "den", "jun\u00b7gen", "Wolf"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "VVPP", "ART", "ADJA", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit ihren holdseligen Blicken.", "tokens": ["Mit", "ih\u00b7ren", "hold\u00b7se\u00b7li\u00b7gen", "Bli\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Singend \u00fcber das tauige Moos", "tokens": ["Sin\u00b7gend", "\u00fc\u