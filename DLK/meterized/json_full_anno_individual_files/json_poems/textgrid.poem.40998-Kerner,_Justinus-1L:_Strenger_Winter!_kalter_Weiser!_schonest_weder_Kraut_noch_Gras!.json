{"textgrid.poem.40998": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "1L: Strenger Winter! kalter Weiser! schonest weder Kraut noch Gras!", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Strenger Winter! kalter Weiser! schonest weder Kraut noch Gras!", "tokens": ["Stren\u00b7ger", "Win\u00b7ter", "!", "kal\u00b7ter", "Wei\u00b7ser", "!", "scho\u00b7nest", "we\u00b7der", "Kraut", "noch", "Gras", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "ADJA", "NN", "$.", "VVFIN", "KON", "NN", "ADV", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Was du nur ber\u00fchrst, du Frost'ger! wandelst du in starres Glas.", "tokens": ["Was", "du", "nur", "be\u00b7r\u00fchrst", ",", "du", "Frost'\u00b7ger", "!", "wan\u00b7delst", "du", "in", "star\u00b7res", "Glas", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "ADV", "$,", "PPER", "NN", "$.", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Bunte Bl\u00fcten, gr\u00fcne Bl\u00e4tter, die der milde Sommer gab,", "tokens": ["Bun\u00b7te", "Bl\u00fc\u00b7ten", ",", "gr\u00fc\u00b7ne", "Bl\u00e4t\u00b7ter", ",", "die", "der", "mil\u00b7de", "Som\u00b7mer", "gab", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,", "PRELS", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Schl\u00e4gst du, weil du's nicht geboren, mit den harten F\u00e4usten ab,", "tokens": ["Schl\u00e4gst", "du", ",", "weil", "du's", "nicht", "ge\u00b7bo\u00b7ren", ",", "mit", "den", "har\u00b7ten", "F\u00e4us\u00b7ten", "ab", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "PIS", "PTKNEG", "VVPP", "$,", "APPR", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.5": {"text": "Rufest stolz: \u00bbIch hab' dem Flusse klar gef\u00fchret den Beweis,", "tokens": ["Ru\u00b7fest", "stolz", ":", "\u00bb", "Ich", "hab'", "dem", "Flus\u00b7se", "klar", "ge\u00b7f\u00fch\u00b7ret", "den", "Be\u00b7weis", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "$.", "$(", "PPER", "VAFIN", "ART", "NN", "ADJD", "VVPP", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.6": {"text": "Da\u00df er gar zu w\u00e4\u00dfrig flie\u00dfe, da\u00df er werden soll zu Eis.", "tokens": ["Da\u00df", "er", "gar", "zu", "w\u00e4\u00df\u00b7rig", "flie\u00b7\u00dfe", ",", "da\u00df", "er", "wer\u00b7den", "soll", "zu", "Eis", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PTKA", "ADJD", "VVFIN", "$,", "KOUS", "PPER", "VAINF", "VMFIN", "APPR", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.7": {"text": "Nachtigall, dem l\u00e4pp'schen Vogel, der naiv-dumm sang bei Nacht,", "tokens": ["Nach\u00b7ti\u00b7gall", ",", "dem", "l\u00e4pp'\u00b7schen", "Vo\u00b7gel", ",", "der", "na\u00b7i\u00b7vdumm", "sang", "bei", "Nacht", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "ADJA", "NN", "$,", "PRELS", "ADJD", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+---+-+-+", "measure": "unknown.measure.septa"}, "line.8": {"text": "Rief ich zu: Du Abgeschmackter! hab' zum Schweigen ihn gebracht.", "tokens": ["Rief", "ich", "zu", ":", "Du", "Ab\u00b7ge\u00b7schmack\u00b7ter", "!", "hab'", "zum", "Schwei\u00b7gen", "ihn", "ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "$.", "PPER", "NN", "$.", "VAFIN", "APPRART", "NN", "PPER", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.9": {"text": "Auch der Lerche, die durchs Fliegen himmelhoch das Fleisch verlor,", "tokens": ["Auch", "der", "Ler\u00b7che", ",", "die", "durchs", "Flie\u00b7gen", "him\u00b7mel\u00b7hoch", "das", "Fleisch", "ver\u00b7lor", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "PRELS", "APPRART", "NN", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.10": {"text": "Sagt' ich kalt: La\u00df deinen Wahnwitz! und sie kommt nicht mehr hervor.", "tokens": ["Sagt'", "ich", "kalt", ":", "La\u00df", "dei\u00b7nen", "Wahn\u00b7witz", "!", "und", "sie", "kommt", "nicht", "mehr", "her\u00b7vor", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "$.", "VVIMP", "PPOSAT", "NN", "$.", "KON", "PPER", "VVFIN", "PTKNEG", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.11": {"text": "Und der Sonne, die getrieben tolles Wesen mannigfalt,", "tokens": ["Und", "der", "Son\u00b7ne", ",", "die", "ge\u00b7trie\u00b7ben", "tol\u00b7les", "We\u00b7sen", "man\u00b7nig\u00b7falt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "VVPP", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.12": {"text": "Sah ich streng nur ins Gesichte, und sie ward verst\u00e4ndig kalt,", "tokens": ["Sah", "ich", "streng", "nur", "ins", "Ge\u00b7sich\u00b7te", ",", "und", "sie", "ward", "ver\u00b7st\u00e4n\u00b7dig", "kalt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVFIN", "ADV", "APPRART", "NN", "$,", "KON", "PPER", "VAFIN", "ADJD", "ADJD", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.13": {"text": "L\u00e4\u00dft nicht mehr den Regenbogen, den phantastischen, ergl\u00fchn:", "tokens": ["L\u00e4\u00dft", "nicht", "mehr", "den", "Re\u00b7gen\u00b7bo\u00b7gen", ",", "den", "phan\u00b7tas\u00b7ti\u00b7schen", ",", "er\u00b7gl\u00fchn", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ADV", "ART", "NN", "$,", "ART", "ADJA", "$,", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.14": {"text": "Denn ich hab' ihr klar bewiesen, da\u00df ", "tokens": ["Denn", "ich", "hab'", "ihr", "klar", "be\u00b7wie\u00b7sen", ",", "da\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "PPER", "VAFIN", "PPER", "ADJD", "VVPP", "$,", "KOUS"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.15": {"text": "Auch dem Donner in den Wolken sagt' ich ohne alle Scheu:", "tokens": ["Auch", "dem", "Don\u00b7ner", "in", "den", "Wol\u00b7ken", "sagt'", "ich", "oh\u00b7ne", "al\u00b7le", "Scheu", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "PPER", "APPR", "PIAT", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.16": {"text": "(und er schweigt) \u2013 da\u00df er nichts anders als ein kind'sches Spucken sei.", "tokens": ["(", "und", "er", "schweigt", ")", "\u2013", "da\u00df", "er", "nichts", "an\u00b7ders", "als", "ein", "kin\u00b7d'\u00b7sches", "Spu\u00b7cken", "sei", "."], "token_info": ["punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "PPER", "VVFIN", "$(", "$(", "KOUS", "PPER", "PIS", "ADV", "KOKOM", "ART", "ADJA", "NN", "VAFIN", "$."], "meter": "--+-+-+--+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.17": {"text": "Also kam durch mein Bestreben in die Welt nun Zeit und Ma\u00df,", "tokens": ["Al\u00b7so", "kam", "durch", "mein", "Be\u00b7stre\u00b7ben", "in", "die", "Welt", "nun", "Zeit", "und", "Ma\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "NN", "APPR", "ART", "NN", "ADV", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.18": {"text": "Ha! beim alten tollen Leben w\u00e4r' sie bald erstickt in Gras.\u00ab", "tokens": ["Ha", "!", "beim", "al\u00b7ten", "tol\u00b7len", "Le\u00b7ben", "w\u00e4r'", "sie", "bald", "er\u00b7stickt", "in", "Gras", ".", "\u00ab"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ITJ", "$.", "APPRART", "ADJA", "ADJA", "NN", "VAFIN", "PPER", "ADV", "VVPP", "APPR", "NN", "$.", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.19": {"text": "Strenger Winter! Rezensente! mache dich nicht allzu wei\u00df!", "tokens": ["Stren\u00b7ger", "Win\u00b7ter", "!", "Re\u00b7zen\u00b7sen\u00b7te", "!", "ma\u00b7che", "dich", "nicht", "all\u00b7zu", "wei\u00df", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "NN", "$.", "VVFIN", "PPER", "PTKNEG", "PTKA", "ADJD", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.20": {"text": "Sieh! auch dir wird einstens brechen in der Brust das harte Eis!", "tokens": ["Sieh", "!", "auch", "dir", "wird", "eins\u00b7tens", "bre\u00b7chen", "in", "der", "Brust", "das", "har\u00b7te", "Eis", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "ADV", "PPER", "VAFIN", "ADV", "VVFIN", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.21": {"text": "Flu\u00df wird flie\u00dfen, Vogel singen, Sonne warm und segnend sein,", "tokens": ["Flu\u00df", "wird", "flie\u00b7\u00dfen", ",", "Vo\u00b7gel", "sin\u00b7gen", ",", "Son\u00b7ne", "warm", "und", "seg\u00b7nend", "sein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "VVINF", "$,", "NE", "VVFIN", "$,", "NN", "ADJD", "KON", "VVPP", "VAINF", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.22": {"text": "Luft wird regnen, Donner rollen, aber du wirst nimmer schrein.", "tokens": ["Luft", "wird", "reg\u00b7nen", ",", "Don\u00b7ner", "rol\u00b7len", ",", "a\u00b7ber", "du", "wirst", "nim\u00b7mer", "schrein", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "VVINF", "$,", "NN", "VVINF", "$,", "KON", "PPER", "VAFIN", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.2": {"line.1": {"text": "Strenger Winter! kalter Weiser! schonest weder Kraut noch Gras!", "tokens": ["Stren\u00b7ger", "Win\u00b7ter", "!", "kal\u00b7ter", "We