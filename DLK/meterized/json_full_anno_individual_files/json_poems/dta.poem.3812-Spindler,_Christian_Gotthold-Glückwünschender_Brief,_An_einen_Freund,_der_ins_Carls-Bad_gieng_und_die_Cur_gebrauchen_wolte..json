{"dta.poem.3812": {"metadata": {"author": {"name": "Spindler, Christian Gotthold", "birth": "N.A.", "death": "N.A."}, "title": "Gl\u00fcckw\u00fcnschender Brief,  \n An einen Freund, der ins Carls-Bad gieng/ und die Cur  \n gebrauchen wolte.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-20581-9", "language": ["de:0.99"], "booktitle": "Spindler, Christian Gotthold: Unschuldige Jugend-Fr\u00fcchte. Leipzig, 1745."}, "poem": {"stanza.1": {"line.1": {"text": "So gehst du, liebster Freund, ins Carols-Bad", "tokens": ["So", "gehst", "du", ",", "liebs\u00b7ter", "Freund", ",", "ins", "Ca\u00b7rol\u00b7sBad"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ADJA", "NN", "$,", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "hinein,", "tokens": ["hin\u00b7ein", ","], "token_info": ["word", "punct"], "pos": ["PTKVZ", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "So wird denn deine Cur in GOtt gesegnet seyn", "tokens": ["So", "wird", "denn", "dei\u00b7ne", "Cur", "in", "Gott", "ge\u00b7seg\u00b7net", "seyn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "PPOSAT", "NN", "APPR", "NN", "VVPP", "VAINF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Durch jene heisse Fluth, die durch den Prudel dringet", "tokens": ["Durch", "je\u00b7ne", "heis\u00b7se", "Fluth", ",", "die", "durch", "den", "Pru\u00b7del", "drin\u00b7get"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Da wo die D\u00f6plitz sich in krumme Uffer schlinget,", "tokens": ["Da", "wo", "die", "D\u00f6p\u00b7litz", "sich", "in", "krum\u00b7me", "Uf\u00b7fer", "schlin\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWAV", "ART", "NN", "PRF", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Da wo den Krancken selbst die \u00fcbergrosse Krafft,", "tokens": ["Da", "wo", "den", "Kran\u00b7cken", "selbst", "die", "\u00fc\u00b7berg\u00b7ros\u00b7se", "Krafft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWAV", "ART", "NN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Der angenehmste Ort, Lust und Gesundheit schafft.", "tokens": ["Der", "an\u00b7ge\u00b7nehms\u00b7te", "Ort", ",", "Lust", "und", "Ge\u00b7sund\u00b7heit", "schafft", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-++--+-+", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Wie sch\u00f6n ist diese Stadt, an G\u00e4rten und Alleen,", "tokens": ["Wie", "sch\u00f6n", "ist", "die\u00b7se", "Stadt", ",", "an", "G\u00e4r\u00b7ten", "und", "Al\u00b7leen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PDAT", "NN", "$,", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "An Wiesen, Fluhr, und Wald; du wirst es", "tokens": ["An", "Wie\u00b7sen", ",", "Fluhr", ",", "und", "Wald", ";", "du", "wirst", "es"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "NN", "$,", "KON", "NN", "$.", "PPER", "VAFIN", "PPER"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "selbsten sehen.", "tokens": ["selbs\u00b7ten", "se\u00b7hen", "."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.11": {"text": "Doch, Freund, dich reitzet nicht die angenehmste", "tokens": ["Doch", ",", "Freund", ",", "dich", "reit\u00b7zet", "nicht", "die", "an\u00b7ge\u00b7nehms\u00b7te"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "$,", "NN", "$,", "PPER", "VVFIN", "PTKNEG", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Stadt,", "tokens": ["Stadt", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+", "measure": "single.up"}, "line.13": {"text": "Nein nur der Born; der da die Krafft Bethes-", "tokens": ["Nein", "nur", "der", "Born", ";", "der", "da", "die", "Krafft", "Be\u00b7thes"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "ADV", "ART", "NE", "$.", "ART", "ADV", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "da hat,", "tokens": ["da", "hat", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VAFIN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.15": {"text": "Der so erhitzte Born, die heilsam holde Quelle,", "tokens": ["Der", "so", "er\u00b7hitz\u00b7te", "Born", ",", "die", "heil\u00b7sam", "hol\u00b7de", "Quel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "NE", "$,", "PRELS", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Er springt mit grosser Krafft rein, wunderbahr", "tokens": ["Er", "springt", "mit", "gros\u00b7ser", "Krafft", "rein", ",", "wun\u00b7der\u00b7bahr"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "ADJD", "$,", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "und helle.", "tokens": ["und", "hel\u00b7le", "."], "token_info": ["word", "word", "punct"], "pos": ["KON", "ADJA", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.18": {"text": "Es mindert deine Quaal und \u00fcberh\u00e4uften Schmerz,", "tokens": ["Es", "min\u00b7dert", "dei\u00b7ne", "Qua\u00b7al", "und", "\u00fc\u00b7berh\u00b7\u00e4uf\u00b7ten", "Schmerz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.19": {"text": "Di\u00df angenehmste Bad erquickt dein mattes Hertz.", "tokens": ["Di\u00df", "an\u00b7ge\u00b7nehms\u00b7te", "Bad", "er\u00b7quickt", "dein", "mat\u00b7tes", "Hertz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "NE", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "So dient des Bornes Krafft, des H\u00f6chsten treues", "tokens": ["So", "dient", "des", "Bor\u00b7nes", "Krafft", ",", "des", "H\u00f6chs\u00b7ten", "treu\u00b7es"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,", "ART", "NN", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "F\u00fcgen,", "tokens": ["F\u00fc\u00b7gen", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.22": {"text": "Dir, wenn es nur vorbey, zum irdischen Vergn\u00fcgen.", "tokens": ["Dir", ",", "wenn", "es", "nur", "vor\u00b7bey", ",", "zum", "ir\u00b7di\u00b7schen", "Ver\u00b7gn\u00fc\u00b7gen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "KOUS", "PPER", "ADV", "PTKVZ", "$,", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Die weise Vorsicht selbst thut die Erh\u00f6rung kund.", "tokens": ["Die", "wei\u00b7se", "Vor\u00b7sicht", "selbst", "thut", "die", "Er\u00b7h\u00f6\u00b7rung", "kund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.24": {"text": "Bethesda Wunderwerck macht deinen Leib gesund.", "tokens": ["Be\u00b7thes\u00b7da", "Wun\u00b7der\u00b7werck", "macht", "dei\u00b7nen", "Leib", "ge\u00b7sund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VVFIN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Gesetzt, da\u00df auch sein Saltz dich etwas bitter tr\u00e4ncket,", "tokens": ["Ge\u00b7setzt", ",", "da\u00df", "auch", "sein", "Saltz", "dich", "et\u00b7was", "bit\u00b7ter", "tr\u00e4n\u00b7cket", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "KOUS", "ADV", "PPOSAT", "NN", "PPER", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "So wird dir doch dadurch gesunder Leib geschencket.", "tokens": ["So", "wird", "dir", "doch", "da\u00b7durch", "ge\u00b7sun\u00b7der", "Leib", "ge\u00b7schen\u00b7cket", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "PAV", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Das, was dein Hertz verlangt, und was du immer", "tokens": ["Das", ",", "was", "dein", "Hertz", "ver\u00b7langt", ",", "und", "was", "du", "im\u00b7mer"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "$,", "PRELS", "PPOSAT", "NN", "VVPP", "$,", "KON", "PWS", "PPER", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.28": {"text": "willt,", "tokens": ["willt", ","], "token_info": ["word", "punct"], "pos": ["VMFIN", "$,"], "meter": "-", "measure": "single.down"}, "line.29": {"text": "Giebt dir der reiche Born, wo Segens-Wasser", "tokens": ["Giebt", "dir", "der", "rei\u00b7che", "Born", ",", "wo", "Se\u00b7gens\u00b7Was\u00b7ser"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NE", "$,", "PWAV", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.30": {"text": "quillt.", "tokens": ["quillt", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+", "measure": "single.up"}, "line.31": {"text": "Dein Bad wird dir ins Haus durch R\u00f6hren zuge-", "tokens": ["Dein", "Bad", "wird", "dir", "ins", "Haus", "durch", "R\u00f6h\u00b7ren", "zu\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NE", "VAFIN", "PPER", "APPRART", "NN", "APPR", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.32": {"text": "leitet,", "tokens": ["lei\u00b7tet", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.33": {"text": "Die Kunst hat dieses ja zu deinem Zweck bereitet.", "tokens": ["Die", "Kunst", "hat", "die\u00b7ses", "ja", "zu", "dei\u00b7nem", "Zweck", "be\u00b7rei\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PDS", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Wird der gesaltzne Quell dir \u00f6fftermahls zur Last,", "tokens": ["Wird", "der", "ge\u00b7saltz\u00b7ne", "Quell", "dir", "\u00f6ff\u00b7ter\u00b7mahls", "zur", "Last", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Vertreibe den Geschmack durch einen Bade-Gast,", "tokens": ["Ver\u00b7trei\u00b7be", "den", "Ge\u00b7schmack", "durch", "ei\u00b7nen", "Ba\u00b7de\u00b7Gast", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Der dir das bittre Saltz in Nectar-Safft ver-", "tokens": ["Der", "dir", "das", "bitt\u00b7re", "Saltz", "in", "Nec\u00b7ta\u00b7r\u00b7S\u00b7afft", "ver"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ART", "ADJA", "NN", "APPR", "NN", "TRUNC"], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.37": {"text": "wandelt,", "tokens": ["wan\u00b7delt", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.38": {"text": "Wenn er als Freund mit dir vertraut und redlich", "tokens": ["Wenn", "er", "als", "Freund", "mit", "dir", "ver\u00b7traut", "und", "red\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "KOUS", "NN", "APPR", "PPER", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.39": {"text": "handelt.", "tokens": ["han\u00b7delt", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.40": {"text": "Und wenn man bey der Cur um H\u00fclffe fragen mu\u00df,", "tokens": ["Und", "wenn", "man", "bey", "der", "Cur", "um", "H\u00fclf\u00b7fe", "fra\u00b7gen", "mu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PIS", "APPR", "ART", "NN", "APPR", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "So dienet dir zum Schutz ein ", "tokens": ["So", "die\u00b7net", "dir", "zum", "Schutz", "ein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.42": {"text": "Der wird dir durch Besuch, wie auch durch treue", "tokens": ["Der", "wird", "dir", "durch", "Be\u00b7such", ",", "wie", "auch", "durch", "treu\u00b7e"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "PPER", "APPR", "NN", "$,", "PWAV", "ADV", "APPR", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.43": {"text": "Zeilen,", "tokens": ["Zei\u00b7len", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.44": {"text": "So offt du es bedarffst, den besten Rath ertheilen.", "tokens": ["So", "offt", "du", "es", "be\u00b7darffst", ",", "den", "bes\u00b7ten", "Rath", "er\u00b7thei\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "PPER", "VVFIN", "$,", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Enthaltsam must du seyn bey dieser grossen Cur,", "tokens": ["Ent\u00b7halt\u00b7sam", "must", "du", "seyn", "bey", "die\u00b7ser", "gros\u00b7sen", "Cur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VMFIN", "PPER", "VAINF", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "Den\u0303 findest du dein Wohl, den\u0303 findest du die Spur,", "tokens": ["De\u00f1", "fin\u00b7dest", "du", "dein", "Wohl", ",", "de\u00f1", "fin\u00b7dest", "du", "die", "Spur", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "PPOSAT", "NN", "$,", "PDS", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "Die dich der Sterblichkeit und Sarg und Grufft", "tokens": ["Die", "dich", "der", "Sterb\u00b7lich\u00b7keit", "und", "Sarg", "und", "Grufft"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ART", "NN", "KON", "NN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.48": {"text": "entziehet,", "tokens": ["ent\u00b7zie\u00b7het", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.49": {"text": "Wobey der Deinen Gl\u00fcck, durch deinen Wohl-", "tokens": ["Wo\u00b7bey", "der", "Dei\u00b7nen", "Gl\u00fcck", ",", "durch", "dei\u00b7nen", "Wohl"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "ART", "PPOSAT", "NN", "$,", "APPR", "PPOSAT", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.50": {"text": "stand bl\u00fchet.", "tokens": ["stand", "bl\u00fc\u00b7het", "."], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "VVFIN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.51": {"text": "Doch schone dich, o Freund! verbirge Zorn u. Ha\u00df,", "tokens": ["Doch", "scho\u00b7ne", "dich", ",", "o", "Freund", "!", "ver\u00b7bir\u00b7ge", "Zorn", "u.", "Ha\u00df", ","], "token_info": ["word", "word",