], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADV", "ADJD", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.29": {"text": "Fragt dich wer, was du gelesen,", "tokens": ["Fragt", "dich", "wer", ",", "was", "du", "ge\u00b7le\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "$,", "PWS", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.30": {"text": "Der nicht bald dabey gewesen,", "tokens": ["Der", "nicht", "bald", "da\u00b7bey", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "ADV", "PAV", "VAPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.31": {"text": "Sprich: Von hinnen nicht gar weit", "tokens": ["Sprich", ":", "Von", "hin\u00b7nen", "nicht", "gar", "weit"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "$.", "APPR", "ADV", "PTKNEG", "ADV", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.32": {"text": "Steht ein Sarckvoll Redligkeit.", "tokens": ["Steht", "ein", "Sar\u00b7ck\u00b7voll", "Red\u00b7lig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}}}}