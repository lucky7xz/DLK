{"dta.poem.7384": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Der Mutter Wiederkehr.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-20090519994", "language": ["de:0.99"], "booktitle": "Droste-H\u00fclshoff, Annette von: Gedichte. Stuttgart u. a., 1844."}, "poem": {"stanza.1": {"line.1": {"text": "Du fr\u00e4gst mich immer von neuem, Marie,", "tokens": ["Du", "fr\u00e4gst", "mich", "im\u00b7mer", "von", "neu\u00b7em", ",", "Ma\u00b7rie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPR", "ADJA", "$,", "NE", "$,"], "meter": "-+-+--+---", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Warum ich mein Heimathland", "tokens": ["Wa\u00b7rum", "ich", "mein", "Hei\u00b7mat\u00b7hland"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "PPER", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Die alten lieben Gebilde flieh", "tokens": ["Die", "al\u00b7ten", "lie\u00b7ben", "Ge\u00b7bil\u00b7de", "flieh"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN", "VVFIN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Dem Herzen doch eingebrannt?", "tokens": ["Dem", "Her\u00b7zen", "doch", "ein\u00b7ge\u00b7brannt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVPP", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Nichts soll das Weib dem Manne verhehlen,", "tokens": ["Nichts", "soll", "das", "Weib", "dem", "Man\u00b7ne", "ver\u00b7heh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Und nichts dem treuen Weibe der Mann,", "tokens": ["Und", "nichts", "dem", "treu\u00b7en", "Wei\u00b7be", "der", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.7": {"text": "Drum setz dich her, ich will erz\u00e4hlen,", "tokens": ["Drum", "setz", "dich", "her", ",", "ich", "will", "er\u00b7z\u00e4h\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PTKVZ", "$,", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Doch abw\u00e4rts sitze \u2014 schau mich nicht an.", "tokens": ["Doch", "ab\u00b7w\u00e4rts", "sit\u00b7ze", "schau", "mich", "nicht", "an", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "$(", "VVFIN", "PPER", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Bei meinen Eltern ich war, \u2014 ein Kind,", "tokens": ["Bei", "mei\u00b7nen", "El\u00b7tern", "ich", "war", ",", "ein", "Kind", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PPER", "VAFIN", "$,", "$(", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ein Kind und dessen nicht froh,", "tokens": ["Ein", "Kind", "und", "des\u00b7sen", "nicht", "froh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "PDS", "PTKNEG", "ADJD", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Im Hause wehte ein dr\u00fcckender Wind,", "tokens": ["Im", "Hau\u00b7se", "weh\u00b7te", "ein", "dr\u00fc\u00b7cken\u00b7der", "Wind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Der ehliche Friede floh,", "tokens": ["Der", "eh\u00b7li\u00b7che", "Frie\u00b7de", "floh", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Nicht Zank noch Scheltwort durfte ich h\u00f6ren,", "tokens": ["Nicht", "Zank", "noch", "Schelt\u00b7wort", "durf\u00b7te", "ich", "h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "ADV", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Doch wie ein Fels auf Allen es lag,", "tokens": ["Doch", "wie", "ein", "Fels", "auf", "Al\u00b7len", "es", "lag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "APPR", "NE", "PPER", "VVFIN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.7": {"text": "Sahn wir von Reisen den Vater kehren,", "tokens": ["Sahn", "wir", "von", "Rei\u00b7sen", "den", "Va\u00b7ter", "keh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Das war uns Kindern ein trauriger Tag.", "tokens": ["Das", "war", "uns", "Kin\u00b7dern", "ein", "trau\u00b7ri\u00b7ger", "Tag", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Ein Kaufmann, ernst, sein strenges Gem\u00fcth", "tokens": ["Ein", "Kauf\u00b7mann", ",", "ernst", ",", "sein", "stren\u00b7ges", "Ge\u00b7m\u00fcth"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ADJD", "$,", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Verbittert durch manchen Verlust,", "tokens": ["Ver\u00b7bit\u00b7tert", "durch", "man\u00b7chen", "Ver\u00b7lust", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "PIAT", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Und meine Mutter die war so m\u00fcd,", "tokens": ["Und", "mei\u00b7ne", "Mut\u00b7ter", "die", "war", "so", "m\u00fcd", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ART", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "So keuchend ging ihre Brust!", "tokens": ["So", "keu\u00b7chend", "ging", "ih\u00b7re", "Brust", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Noch seh' ich wie sie, die Augen ger\u00f6thet,", "tokens": ["Noch", "seh'", "ich", "wie", "sie", ",", "die", "Au\u00b7gen", "ge\u00b7r\u00f6\u00b7thet", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PWAV", "PPER", "$,", "ART", "NN", "VVPP", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Ein Bild der still verh\u00e4rmten Geduld,", "tokens": ["Ein", "Bild", "der", "still", "ver\u00b7h\u00e4rm\u00b7ten", "Ge\u00b7duld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.7": {"text": "An unserm Bettchen gekniet und gebetet.", "tokens": ["An", "un\u00b7serm", "Bett\u00b7chen", "ge\u00b7kniet", "und", "ge\u00b7be\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "KON", "VVPP", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Gewi\u00df, meine Mutter war frei von Schuld!", "tokens": ["Ge\u00b7wi\u00df", ",", "mei\u00b7ne", "Mut\u00b7ter", "war", "frei", "von", "Schuld", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPOSAT", "NN", "VAFIN", "ADJD", "APPR", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.4": {"line.1": {"text": "Doch trieb der Vater sich um \u2014 vielleicht", "tokens": ["Doch", "trieb", "der", "Va\u00b7ter", "sich", "um", "viel\u00b7leicht"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "PRF", "APPR", "$(", "ADV"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "In London oder in Wien \u2014", "tokens": ["In", "Lon\u00b7don", "o\u00b7der", "in", "Wi\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "APPR", "NE", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Dann lebten wir auf und athmeten leicht,", "tokens": ["Dann", "leb\u00b7ten", "wir", "auf", "und", "ath\u00b7me\u00b7ten", "leicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "KON", "VVFIN", "ADJD", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und schossen wie Kressen so gr\u00fcn.", "tokens": ["Und", "schos\u00b7sen", "wie", "Kres\u00b7sen", "so", "gr\u00fcn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KOKOM", "NN", "ADV", "ADJD", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "Durch lustige Schw\u00e4nke machte uns lachen", "tokens": ["Durch", "lus\u00b7ti\u00b7ge", "Schw\u00e4n\u00b7ke", "mach\u00b7te", "uns", "la\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "VVINF"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Der gute Me\u00dfner, d\u00fcrr und ergraut,", "tokens": ["Der", "gu\u00b7te", "Me\u00df\u00b7ner", ",", "d\u00fcrr", "und", "er\u00b7graut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.7": {"text": "Der dann uns Alle sollte bewachen,", "tokens": ["Der", "dann", "uns", "Al\u00b7le", "soll\u00b7te", "be\u00b7wa\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PPER", "PIS", "VMFIN", "VVINF", "$,"], "meter": "---+-+--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Denn meiner Mutter ward Nichts vertraut.", "tokens": ["Denn", "mei\u00b7ner", "Mut\u00b7ter", "ward", "Nichts", "ver\u00b7traut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "PIS", "ADJD", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Da schickte der Himmel ein schweres Leid,", "tokens": ["Da", "schick\u00b7te", "der", "Him\u00b7mel", "ein", "schwe\u00b7res", "Leid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Sie schlich so lange umher,", "tokens": ["Sie", "schlich", "so", "lan\u00b7ge", "um\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ADV", "ADV", "PTKVZ", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Und h\u00e4rmte sich sachte in's Sterbekleid,", "tokens": ["Und", "h\u00e4rm\u00b7te", "sich", "sach\u00b7te", "in's", "Ster\u00b7be\u00b7kleid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Wir waren wie irre V\u00f6gel im Haine,", "tokens": ["Wir", "wa\u00b7ren", "wie", "ir\u00b7re", "V\u00f6\u00b7gel", "im", "Hai\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "KOKOM", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Zu fr\u00fch entflattert dem treuen Nest,", "tokens": ["Zu", "fr\u00fch", "ent\u00b7flat\u00b7tert", "dem", "treu\u00b7en", "Nest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Bald tobten wir toll \u00fcber Bl\u00f6cke und Steine,", "tokens": ["Bald", "tob\u00b7ten", "wir", "toll", "\u00fc\u00b7ber", "Bl\u00f6\u00b7cke", "und", "Stei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.7": {"text": "Und duckten bald, in den Winkel gepre\u00dft.", "tokens": ["Und", "duck\u00b7ten", "bald", ",", "in", "den", "Win\u00b7kel", "ge\u00b7pre\u00dft", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "$,", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "Dem alten Manne ward kalt und hei\u00df,", "tokens": ["Dem", "al\u00b7ten", "Man\u00b7ne", "ward", "kalt", "und", "hei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Dem w\u00fcrdigen Sakristan,", "tokens": ["Dem", "w\u00fcr\u00b7di\u00b7gen", "Sak\u00b7ris\u00b7tan", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "NE", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Sah er besudelt mit Staub und Schwei\u00df", "tokens": ["Sah", "er", "be\u00b7su\u00b7delt", "mit", "Staub", "und", "Schwei\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Und gl\u00fchend wie Oefen uns nahn;", "tokens": ["Und", "gl\u00fc\u00b7hend", "wie", "O\u00b7e\u00b7fen", "uns", "nahn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KOKOM", "NN", "PPER", "ADJA", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Doch traten wir in die ver\u00f6dete Kammer,", "tokens": ["Doch", "tra\u00b7ten", "wir", "in", "die", "ver\u00b7\u00f6\u00b7de\u00b7te", "Kam\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Und sahn das Schemelchen am Clavier,", "tokens": ["Und", "sahn", "das", "Sche\u00b7mel\u00b7chen", "am", "Cla\u00b7vier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Dann str\u00f6mte der unb\u00e4ndige Jammer,", "tokens": ["Dann", "str\u00f6m\u00b7te", "der", "un\u00b7b\u00e4n\u00b7di\u00b7ge", "Jam\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+---+--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Und nach der Mutter wimmerten wir.", "tokens": ["Und", "nach", "der", "Mut\u00b7ter", "wim\u00b7mer\u00b7ten", "wir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.7": {"line.1": {"text": "Am sechsten Abend nachdem sie fort", "tokens": ["Am", "sechs\u00b7ten", "A\u00b7bend", "nach\u00b7dem", "sie", "fort"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "KOUS", "PPER", "PTKVZ"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "\u2014 Wir kauerten am Kamin,", "tokens": ["Wir", "kau\u00b7er\u00b7ten", "am", "Ka\u00b7min", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "----+-+", "measure": "unknown.measure.di"}, "line.3": {"text": "Der Alte lehnte am Simse dort", "tokens": ["Der", "Al\u00b7te", "lehn\u00b7te", "am", "Sim\u00b7se", "dort"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "ADV"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und sah die Kohlen vergl\u00fchn,", "tokens": ["Und", "sah", "die", "Koh\u00b7len", "ver\u00b7gl\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "