{"textgrid.poem.39418": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Geht in Frieden nur nach Haus,", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Geht in Frieden nur nach Haus,", "tokens": ["Geht", "in", "Frie\u00b7den", "nur", "nach", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "ADV", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Denn ich komme nicht heraus.", "tokens": ["Denn", "ich", "kom\u00b7me", "nicht", "he\u00b7raus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PTKNEG", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schreit ihr doch bei jedem Plunder", "tokens": ["Schreit", "ihr", "doch", "bei", "je\u00b7dem", "Plun\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "ADV", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Immer gleich: heraus! heraus!", "tokens": ["Im\u00b7mer", "gleich", ":", "he\u00b7raus", "!", "he\u00b7raus", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "ADV", "$.", "PTKVZ", "$.", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Tobt nicht oft das ganze Haus,", "tokens": ["Tobt", "nicht", "oft", "das", "gan\u00b7ze", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Wie ein schwangrer Berg, o Wunder!", "tokens": ["Wie", "ein", "schwang\u00b7rer", "Berg", ",", "o", "Wun\u00b7der", "!"], "token_