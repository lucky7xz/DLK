{"textgrid.poem.48064": {"metadata": {"author": {"name": "M\u00fcller-Jahnke, Clara", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "verse", "period": "N.A.", "pub_year": 1882, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich war ein Kind und er war schon gro\u00df,", "tokens": ["Ich", "war", "ein", "Kind", "und", "er", "war", "schon", "gro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "KON", "PPER", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "und wir liebten uns recht wie Geschwister,", "tokens": ["und", "wir", "lieb\u00b7ten", "uns", "recht", "wie", "Ge\u00b7schwis\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ADJD", "KOKOM", "NN", "$,"], "meter": "+-+-+++-+-", "measure": "unknown.measure.hexa"}, "line.3": {"text": "ich setzte mir auf seinen blanken Helm", "tokens": ["ich", "setz\u00b7te", "mir", "auf", "sei\u00b7nen", "blan\u00b7ken", "Helm"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "und schnallte mir um den Tornister.", "tokens": ["und", "schnall\u00b7te", "mir", "um", "den", "Tor\u00b7nis\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+--+--", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Dann schlich ich verstohlen, da\u00df keiner es sah,", "tokens": ["Dann", "schlich", "ich", "ver\u00b7stoh\u00b7len", ",", "da\u00df", "kei\u00b7ner", "es", "sah", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "w