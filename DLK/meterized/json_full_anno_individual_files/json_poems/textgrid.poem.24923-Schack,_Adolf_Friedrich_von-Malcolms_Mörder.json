{"textgrid.poem.24923": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Malcolms M\u00f6rder", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sie haben des schlummernden K\u00f6nigs Haupt", "tokens": ["Sie", "ha\u00b7ben", "des", "schlum\u00b7mern\u00b7den", "K\u00f6\u00b7nigs", "Haupt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Gef\u00e4llt durch t\u00fcckischen Mord!", "tokens": ["Ge\u00b7f\u00e4llt", "durch", "t\u00fc\u00b7cki\u00b7schen", "Mord", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Mit dem Golde st\u00fcrmen sie, das sie geraubt,", "tokens": ["Mit", "dem", "Gol\u00b7de", "st\u00fcr\u00b7men", "sie", ",", "das", "sie", "ge\u00b7raubt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "Aus dem Schlosse von Glamis fort.", "tokens": ["Aus", "dem", "Schlos\u00b7se", "von", "Gla\u00b7mis", "fort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "NE", "PTKVZ", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.2": {"line.1": {"text": "\u00bbdicht wirbelt, vom Winde gefegt, der Schnee;", "tokens": ["\u00bb", "dicht", "wir\u00b7belt", ",", "vom", "Win\u00b7de", "ge\u00b7fegt", ",", "der", "Schnee", ";"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "ADJD", "VVFIN", "$,", "APPRART", "NN", "VVPP", "$,", "ART", "NN", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Verweht ist jegliche Spur;", "tokens": ["Ver\u00b7weht", "ist", "jeg\u00b7li\u00b7che", "Spur", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "VAFIN", "PIAT", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Durch Nebel und treibende Flocken, weh,", "tokens": ["Durch", "Ne\u00b7bel", "und", "trei\u00b7ben\u00b7de", "Flo\u00b7cken", ",", "weh", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "KON", "ADJA", "NN", "$,", "PTKVZ", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Wie finden den Weg wir nur?\u00ab", "tokens": ["Wie", "fin\u00b7den", "den", "Weg", "wir", "nur", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "PPER", "ADV", "$.", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Ins Antlitz starren sich John und Dick:", "tokens": ["Ins", "Ant\u00b7litz", "star\u00b7ren", "sich", "John", "und", "Dick", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PRF", "NE", "KON", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "\u00bbo w\u00e4r' es nimmer geschehn!", "tokens": ["\u00bb", "o", "w\u00e4r'", "es", "nim\u00b7mer", "ge\u00b7schehn", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM", "VAFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Sahst du, wie zum Himmel den brechenden Blick", "tokens": ["Sahst", "du", ",", "wie", "zum", "Him\u00b7mel", "den", "bre\u00b7chen\u00b7den", "Blick"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "PWAV", "APPRART", "NN", "ART", "ADJA", "NN"], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Er hob, um Rache zu flehn?", "tokens": ["Er", "hob", ",", "um", "Ra\u00b7che", "zu", "flehn", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUI", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.4": {"line.1": {"text": "Sie holt uns ein. Als vom Rumpf ich ihm schlug", "tokens": ["Sie", "holt", "uns", "ein", ".", "Als", "vom", "Rumpf", "ich", "ihm", "schlug"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$.", "KOUS", "APPRART", "NN", "PPER", "PPER", "VVFIN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Das greise Haupt mit dem Schwert,", "tokens": ["Das", "grei\u00b7se", "Haupt", "mit", "dem", "Schwert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Starb stumm auf seinen Lippen ein Fluch,", "tokens": ["Starb", "stumm", "auf", "sei\u00b7nen", "Lip\u00b7pen", "ein", "Fluch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Gott aber hat ihn geh\u00f6rt.\u00ab", "tokens": ["Gott", "a\u00b7ber", "hat", "ihn", "ge\u00b7h\u00f6rt", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "ADV", "VAFIN", "PPER", "VVFIN", "$.", "$("], "meter": "++-+--+", "measure": "iambic.tetra.chol"}}, "stanza.5": {"line.1": {"text": "Die anderen lachen. \u00bbFurcht vor Spuk", "tokens": ["Die", "an\u00b7de\u00b7ren", "la\u00b7chen", ".", "\u00bb", "Furcht", "vor", "Spuk"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "VVINF", "$.", "$(", "NN", "APPR", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Hat euch die Glieder gel\u00e4hmt;", "tokens": ["Hat", "euch", "die", "Glie\u00b7der", "ge\u00b7l\u00e4hmt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "VVPP", "$."], "meter": "++-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Vom Weine des K\u00f6nigs ein t\u00fcchtiger Schluck", "tokens": ["Vom", "Wei\u00b7ne", "des", "K\u00f6\u00b7nigs", "ein", "t\u00fcch\u00b7ti\u00b7ger", "Schluck"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Wird Mut euch geben, da nehmt!\u00ab", "tokens": ["Wird", "Mut", "euch", "ge\u00b7ben", ",", "da", "nehmt", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["VAFIN", "NN", "PPER", "VVINF", "$,", "ADV", "VVFIN", "$.", "$("], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.6": {"line.1": {"text": "Im Kreise lassen sie gehn den Wein,", "tokens": ["Im", "Krei\u00b7se", "las\u00b7sen", "sie", "gehn", "den", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Um das starrende Blut zu taun.", "tokens": ["Um", "das", "star\u00b7ren\u00b7de", "Blut", "zu", "taun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "\u00bbnun schnell! An der Grenze m\u00fcssen wir sein,", "tokens": ["\u00bb", "nun", "schnell", "!", "An", "der", "Gren\u00b7ze", "m\u00fcs\u00b7sen", "wir", "sein", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ADJD", "$.", "APPR", "ART", "NN", "VMFIN", "PPER", "VAINF", "$,"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Eh der Morgen beginnt zu graun.", "tokens": ["Eh", "der", "Mor\u00b7gen", "be\u00b7ginnt", "zu", "graun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.7": {"line.1": {"text": "Grad aus den Weg! nur immer gerad!", "tokens": ["Grad", "aus", "den", "Weg", "!", "nur", "im\u00b7mer", "ge\u00b7rad", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$.", "ADV", "ADV", "ADV", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Rechts liegt die Gespensterheide;", "tokens": ["Rechts", "liegt", "die", "Ge\u00b7spens\u00b7ter\u00b7hei\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Links f\u00fchrt auf den Farfarsee der Pfad;", "tokens": ["Links", "f\u00fchrt", "auf", "den", "Farf\u00b7ar\u00b7see", "der", "Pfad", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Die m\u00fcssen wir fliehen beide.\u00ab", "tokens": ["Die", "m\u00fcs\u00b7sen", "wir", "flie\u00b7hen", "bei\u00b7de", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "VMFIN", "PPER", "VVFIN", "PIS", "$.", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.8": {"line.1": {"text": "Im Sturm, der die Stimmen \u00fcbert\u00e4ubt,", "tokens": ["Im", "Sturm", ",", "der", "die", "Stim\u00b7men", "\u00fc\u00b7bert\u00b7\u00e4ubt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PRELS", "ART", "NN", "VVPP", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Nicht h\u00f6ren einander sie mehr;", "tokens": ["Nicht", "h\u00f6\u00b7ren", "ein\u00b7an\u00b7der", "sie", "mehr", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "PRF", "PPER", "ADV", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Sie sehen sich nicht, so wirbelt und st\u00e4ubt", "tokens": ["Sie", "se\u00b7hen", "sich", "nicht", ",", "so", "wir\u00b7belt", "und", "st\u00e4ubt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "PTKNEG", "$,", "ADV", "VVFIN", "KON", "VVFIN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Der Schnee in den L\u00fcften umher.", "tokens": ["Der", "Schnee", "in", "den", "L\u00fcf\u00b7ten", "um\u00b7her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.9": {"line.1": {"text": "Sieh da! was zuckt durch die Finsternis?", "tokens": ["Sieh", "da", "!", "was", "zuckt", "durch", "die", "Fins\u00b7ter\u00b7nis", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$.", "PWS", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Ein Windsto\u00df bricht herein;", "tokens": ["Ein", "Wind\u00b7sto\u00df", "bricht", "her\u00b7ein", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Wolken zerst\u00e4uben; herab durch den Ri\u00df", "tokens": ["Die", "Wol\u00b7ken", "zer\u00b7st\u00e4u\u00b7ben", ";", "her\u00b7ab", "durch", "den", "Ri\u00df"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVPP", "$.", "ADV", "APPR", "ART", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "F\u00e4llt matt des Mondes Schein.", "tokens": ["F\u00e4llt", "matt", "des", "Mon\u00b7des", "Schein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "\u00bbweh, weh, im Kreise sind wir geirrt!", "tokens": ["\u00bb", "weh", ",", "weh", ",", "im", "Krei\u00b7se", "sind", "wir", "ge\u00b7irrt", "!"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PTKVZ", "$,", "PTKVZ", "$,", "APPRART", "NN", "VAFIN", "PPER", "VVPP", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Von Glamis das Schlo\u00df ragt dort.", "tokens": ["Von", "Gla\u00b7mis", "das", "Schlo\u00df", "ragt", "dort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ART", "NN", "VVFIN", "ADV", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Seht ihr, wie's hell an den Fenstern wird?", "tokens": ["Seht", "ihr", ",", "wie's", "hell", "an", "den", "Fens\u00b7tern", "wird", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "VVFIN", "ADJD", "APPR", "ART", "NN", "VAFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "H\u00f6rt ihr die T\u00f6ne? Fort! fort!\u00ab", "tokens": ["H\u00f6rt", "ihr", "die", "T\u00f6\u00b7ne", "?", "Fort", "!", "fort", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "$.", "NN", "$.", "PTKVZ", "$.", "$("], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.11": {"line.1": {"text": "Klar leuchtet hernieder vom Kirchlein des Turms", "tokens": ["Klar", "leuch\u00b7tet", "her\u00b7nie\u00b7der", "vom", "Kirch\u00b7lein", "des", "Turms"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PTKVZ", "APPRART", "NN", "ART", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Der Altarkerzen Strahl,", "tokens": ["Der", "Al\u00b7tar\u00b7ker\u00b7zen", "Strahl", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Und her\u00fcber hallt durch die Pausen des Sturms", "tokens": ["Und", "her\u00b7\u00fc\u00b7ber", "hallt", "durch", "die", "Pau\u00b7sen", "des", "Sturms"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Der Totenamtchoral.", "tokens": ["Der", "To\u00b7ten\u00b7amt\u00b7cho\u00b7ral", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Da dr\u00f6hnt zu den F\u00fc\u00dfen der M\u00f6rder j\u00e4h", "tokens": ["Da", "dr\u00f6hnt", "zu", "den", "F\u00fc\u00b7\u00dfen", "der", "M\u00f6r\u00b7der", "j\u00e4h"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "ADJD"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Ein Krachen wie Donnerrollen;", "tokens": ["Ein", "Kra\u00b7chen", "wie", "Don\u00b7ner\u00b7rol\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KOKOM", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Sie stehn auf dem Eis; auf rei\u00dft sich der See,", "tokens": ["Sie", "stehn", "auf", "dem", "Eis", ";", "auf", "rei\u00dft", "sich", "der", "See", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$.", "APPR", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Es bersten und knirschen die Schollen.", "tokens": ["Es", "bers\u00b7ten", "und", "knir\u00b7schen", "die", "Schol\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.13": {"line.1": {"text": "Durch g\u00e4hnende Spalten schie\u00dft und quillt", "tokens": ["Durch", "g\u00e4h\u00b7nen\u00b7de", "Spal\u00b7ten", "schie\u00dft", "und", "quillt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "KON", "VVFIN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Das Wasser mit sch\u00e4umenden Wellen;", "tokens": ["Das", "Was\u00b7ser", "mit", "sch\u00e4u\u00b7men\u00b7den", "Wel\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "In den Strude