{"dta.poem.3642": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Erschreckliche Geschichte vom H\u00fcnchen und  \n vom H\u00e4nchen .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519172", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wohin H\u00e4nchen?", "tokens": ["Wo\u00b7hin", "H\u00e4n\u00b7chen", "?"], "token_info": ["word", "word", "punct"], "pos": ["PWAV", "NN", "$."], "meter": "--+-", "measure": "anapaest.init"}, "line.2": {"text": "Mein H\u00fcnchen begraben.", "tokens": ["Mein", "H\u00fcn\u00b7chen", "be\u00b7gra\u00b7ben", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Darf ich aufsitzen?", "tokens": ["Darf", "ich", "auf\u00b7sit\u00b7zen", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Sitz hinten auf den Wagen,", "tokens": ["Sitz", "hin\u00b7ten", "auf", "den", "Wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Forne k\u00f6nnens meine Pferdchen nicht vertragen.", "tokens": ["For\u00b7ne", "k\u00f6n\u00b7nens", "mei\u00b7ne", "Pferd\u00b7chen", "nicht", "ver\u00b7tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPOSAT", "NN", "PTKNEG", "VVFIN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.6": {"text": "Da hat sich der Fuchs aufgesetzt, kam ein Wolf.", "tokens": ["Da", "hat", "sich", "der", "Fuchs", "auf\u00b7ge\u00b7setzt", ",", "kam", "ein", "Wolf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PRF", "ART", "NE", "VVPP", "$,", "VVFIN", "ART", "NE", "$."], "meter": "-+--++-+--+", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Wohin H\u00e4nchen? u. s. w.", "tokens": ["Wo\u00b7hin", "H\u00e4n\u00b7chen", "?", "u.", "s.", "w."], "token_info": ["word", "word", "punct", "abbreviation", "abbreviation", "abbreviation"], "pos": ["PWAV", "NN", "$.", "APPR", "VVIMP", "NN"], "meter": "--+-", "measure": "anapaest.init"}, "line.8": {"text": "kam ein L\u00f6we, kam ein B\u00e4r, u. s. w., alle hinten drauf,", "tokens": ["kam", "ein", "L\u00f6\u00b7we", ",", "kam", "ein", "B\u00e4r", ",", "u.", "s.", "w.", ",", "al\u00b7le", "hin\u00b7ten", "drauf", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "abbreviation", "abbreviation", "abbreviation", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NE", "$,", "VVFIN", "ART", "NN", "$,", "KON", "VVIMP", "NN", "$,", "PIS", "ADV", "PTKVZ", "$,"], "meter": "--+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "endlich kam noch ein Floh,", "tokens": ["end\u00b7lich", "kam", "noch", "ein", "Floh", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.10": {"text": "Wohin H\u00e4nchen? u. s. w.", "tokens": ["Wo\u00b7hin", "H\u00e4n\u00b7chen", "?", "u.", "s.", "w."], "token_info": ["word", "word", "punct", "abbreviation", "abbreviation", "abbreviation"], "pos": ["PWAV", "NN", "$.", "APPR", "VVIMP", "NN"], "meter": "--+-", "measure": "anapaest.init"}}}}}