{"dta.poem.6385": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Des Saveurs.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Regeln zeigen uns den sichern Weg, wir sehn", "tokens": ["Die", "Re\u00b7geln", "zei\u00b7gen", "uns", "den", "si\u00b7chern", "Weg", ",", "wir", "sehn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,", "PPER", "VVINF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die h\u00e4rtsten C\u00f6rper offt zergehn,", "tokens": ["Die", "h\u00e4rts\u00b7ten", "C\u00f6r\u00b7per", "offt", "zer\u00b7gehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und die, so fl\u00fc\u00dfig, feste werden,", "tokens": ["Und", "die", ",", "so", "fl\u00fc\u00b7\u00dfig", ",", "fes\u00b7te", "wer\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "$,", "ADV", "ADJD", "$,", "ADJA", "VAINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Was noch so hefftig brennt, wird Eis,", "tokens": ["Was", "noch", "so", "heff\u00b7tig", "brennt", ",", "wird", "Eis", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADV", "ADJD", "VVFIN", "$,", "VAFIN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und, was am allerk\u00e4ltsten, hei\u00df.", "tokens": ["Und", ",", "was", "am", "al\u00b7ler\u00b7k\u00e4lts\u00b7ten", ",", "hei\u00df", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "$,", "PRELS", "APPRART", "ADJA", "$,", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dies sind best\u00e4ndige Bewegungen auf Erden.", "tokens": ["Dies", "sind", "be\u00b7st\u00e4n\u00b7di\u00b7ge", "Be\u00b7we\u00b7gun\u00b7gen", "auf", "Er\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADJA", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+---+-", "measure": "unknown.measure.penta"}}}}}