{"dta.poem.15911": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "116.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Einmal gethanes soll man nicht noch einmal machen;", "tokens": ["Ein\u00b7mal", "ge\u00b7tha\u00b7nes", "soll", "man", "nicht", "noch", "ein\u00b7mal", "ma\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "VMFIN", "PIS", "PTKNEG", "ADV", "ADV", "VVINF", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "F\u00fcr neue Th\u00e4tigkeit gibts immer neue Sachen.", "tokens": ["F\u00fcr", "neu\u00b7e", "Th\u00e4\u00b7tig\u00b7keit", "gibts", "im\u00b7mer", "neu\u00b7e", "Sa\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Zur Widerholung reizt zwar ein gelungner Ton,", "tokens": ["Zur", "Wi\u00b7der\u00b7ho\u00b7lung", "reizt", "zwar", "ein", "ge\u00b7lung\u00b7ner", "Ton", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch unterm Thema bleibt die Variazion.", "tokens": ["Doch", "un\u00b7term", "The\u00b7ma", "bleibt", "die", "Va\u00b7ria\u00b7zi\u00b7on", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}