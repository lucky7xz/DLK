{"dta.poem.18577": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "A n  S elimenen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1702", "urn": "urn:nbn:de:kobv:b4-200905197766", "language": ["de:0.99"], "booktitle": "Hunold, Christian Friedrich: Die Edle Bem\u00fchung m\u00fcssiger Stunden. Hamburg, 1702."}, "poem": {"stanza.1": {"line.1": {"text": "So wolt ihr mich getreue Sinnen qu\u00e4len/", "tokens": ["So", "wolt", "ihr", "mich", "ge\u00b7treu\u00b7e", "Sin\u00b7nen", "qu\u00e4\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und stellt mir stets ein Englisch Bildni\u00df f\u00fcr?", "tokens": ["Und", "stellt", "mir", "stets", "ein", "Eng\u00b7lisch", "Bild\u00b7ni\u00df", "f\u00fcr", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ART", "NN", "NN", "APPR", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ihr k\u00fcsset zwar die Sch\u00f6nste meiner Seelen/", "tokens": ["Ihr", "k\u00fcs\u00b7set", "zwar", "die", "Sch\u00f6ns\u00b7te", "mei\u00b7ner", "See\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Allein mich reist des Schicksals Macht von ihr:", "tokens": ["Al\u00b7lein", "mich", "reist", "des", "Schick\u00b7sals", "Macht", "von", "ihr", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "ART", "NN", "NN", "APPR", "PPOSAT", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ach k\u00fcsset sie und martert mich dabey/", "tokens": ["Ach", "k\u00fcs\u00b7set", "sie", "und", "mar\u00b7tert", "mich", "da\u00b7bey", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "PPER", "KON", "VVFIN", "PRF", "PAV", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Ihr seyd getreu.", "tokens": ["Ihr", "seyd", "ge\u00b7treu", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Ihr seyd getreu der sch\u00f6nen Selimenen/", "tokens": ["Ihr", "seyd", "ge\u00b7treu", "der", "sch\u00f6\u00b7nen", "Se\u00b7li\u00b7me\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die meine Brust vor ihre G\u00f6ttin h\u00e4lt.", "tokens": ["Die", "mei\u00b7ne", "Brust", "vor", "ih\u00b7re", "G\u00f6t\u00b7tin", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ach mu\u00df ich mich nach dir vergeblich sehnen!", "tokens": ["Ach", "mu\u00df", "ich", "mich", "nach", "dir", "ver\u00b7geb\u00b7lich", "seh\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PRF", "APPR", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Verbanst du mich noch endlich aus der Welt?", "tokens": ["Ver\u00b7banst", "du", "mich", "noch", "end\u00b7lich", "aus", "der", "Welt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "So thu es nur/ du bist mein ander ich/", "tokens": ["So", "thu", "es", "nur", "/", "du", "bist", "mein", "an\u00b7der", "ich", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$(", "PPER", "VAFIN", "PPOSAT", "ADJD", "PPER", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Verbanne mich.", "tokens": ["Ver\u00b7ban\u00b7ne", "mich", "."], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Verbanne mich auch zu den wilden Mohren/", "tokens": ["Ver\u00b7ban\u00b7ne", "mich", "auch", "zu", "den", "wil\u00b7den", "Moh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ja hei\u00df mich gar in \u00f6de W\u00fcsten gehn:", "tokens": ["Ja", "hei\u00df", "mich", "gar", "in", "\u00f6\u00b7de", "W\u00fcs\u00b7ten", "gehn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADJD", "PPER", "ADV", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ich habe mich als Sclave dir verschworen/", "tokens": ["Ich", "ha\u00b7be", "mich", "als", "Scla\u00b7ve", "dir", "ver\u00b7schwo\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "KOUS", "NN", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und bins bereit gehorsamst auszustehn.", "tokens": ["Und", "bins", "be\u00b7reit", "ge\u00b7hor\u00b7samst", "aus\u00b7zu\u00b7stehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADJD", "VVPP", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Doch siehe nur bey meinen Elend an/", "tokens": ["Doch", "sie\u00b7he", "nur", "bey", "mei\u00b7nen", "E\u00b7lend", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "ADV", "APPR", "PPOSAT", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Was Gnade kan.", "tokens": ["Was", "Gna\u00b7de", "kan", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "NN", "VMFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}