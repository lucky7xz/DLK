{"dta.poem.21329": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Caput VII.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-30602-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Drey Todtengerippe, phantastisch geputzt,", "tokens": ["Drey", "Tod\u00b7ten\u00b7ge\u00b7rip\u00b7pe", ",", "phan\u00b7tas\u00b7tisch", "ge\u00b7putzt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["CARD", "NN", "$,", "ADJD", "VVPP", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Mit Kronen auf den elenden", "tokens": ["Mit", "Kro\u00b7nen", "auf", "den", "e\u00b7len\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "APPR", "ART", "ADJA"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Vergilbten Sch\u00e4deln, sie trugen auch", "tokens": ["Ver\u00b7gilb\u00b7ten", "Sch\u00e4\u00b7deln", ",", "sie", "tru\u00b7gen", "auch"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "PPER", "VVFIN", "ADV"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Das Zepter in kn\u00f6chernen H\u00e4nden.", "tokens": ["Das", "Zep\u00b7ter", "in", "kn\u00f6\u00b7cher\u00b7nen", "H\u00e4n\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.2": {"line.1": {"text": "Wie Hampelm\u00e4nner bewegten sie", "tokens": ["Wie", "Ham\u00b7pel\u00b7m\u00e4n\u00b7ner", "be\u00b7weg\u00b7ten", "sie"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "NN", "VVFIN", "PPER"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die l\u00e4ngstverstorbenen Knochen;", "tokens": ["Die", "l\u00e4ngst\u00b7ver\u00b7stor\u00b7be\u00b7nen", "Kno\u00b7chen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Die haben nach Moder und zugleich", "tokens": ["Die", "ha\u00b7ben", "nach", "Mo\u00b7der", "und", "zu\u00b7gleich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "APPR", "NN", "KON", "ADV"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Nach Weihrauchduft gerochen.", "tokens": ["Nach", "Weih\u00b7rauch\u00b7duft", "ge\u00b7ro\u00b7chen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Der Eine bewegte sogar den Mund", "tokens": ["Der", "Ei\u00b7ne", "be\u00b7weg\u00b7te", "so\u00b7gar", "den", "Mund"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PIS", "VVFIN", "ADV", "ART", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Und hielt eine Rede, sehr lange;", "tokens": ["Und", "hielt", "ei\u00b7ne", "Re\u00b7de", ",", "sehr", "lan\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,", "ADV", "ADV", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Er setzte mir auseinander warum", "tokens": ["Er", "setz\u00b7te", "mir", "aus\u00b7ein\u00b7an\u00b7der", "wa\u00b7rum"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PWAV"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Er meinen Respekt verlange.", "tokens": ["Er", "mei\u00b7nen", "Res\u00b7pekt", "ver\u00b7lan\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Zuerst weil er ein Todter sey,", "tokens": ["Zu\u00b7erst", "weil", "er", "ein", "Tod\u00b7ter", "sey", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und zweitens weil er ein K\u00f6nig,", "tokens": ["Und", "zwei\u00b7tens", "weil", "er", "ein", "K\u00f6\u00b7nig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "KOUS", "PPER", "ART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und drittens weil er ein Heil\u2019ger sey, \u2013", "tokens": ["Und", "drit\u00b7tens", "weil", "er", "ein", "Heil'\u00b7ger", "sey", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADV", "KOUS", "PPER", "ART", "NN", "VAFIN", "$,", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Das alles r\u00fchrte mich wenig.", "tokens": ["Das", "al\u00b7les", "r\u00fchr\u00b7te", "mich", "we\u00b7nig", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "VVFIN", "PPER", "PIS", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}}}}