{"dta.poem.5401": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der Himmlische Thau.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "An einer Pflantze feuchter Spitze", "tokens": ["An", "ei\u00b7ner", "Pflant\u00b7ze", "feuch\u00b7ter", "Spit\u00b7ze"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sah ich, in fr\u00fcher Morgen-Zeit,", "tokens": ["Sah", "ich", ",", "in", "fr\u00fc\u00b7her", "Mor\u00b7gen\u00b7Zeit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Als Erd\u2019 und Luft voll reiner Heiterkeit,", "tokens": ["Als", "Erd'", "und", "Luft", "voll", "rei\u00b7ner", "Hei\u00b7ter\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "In einem Tr\u00f6pfgen Thau, viel helle bunte Blitze.", "tokens": ["In", "ei\u00b7nem", "Tr\u00f6pf\u00b7gen", "Thau", ",", "viel", "hel\u00b7le", "bun\u00b7te", "Blit\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,", "PIAT", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ich sprach, als ich vor Lust mich kaum besann,", "tokens": ["Ich", "sprach", ",", "als", "ich", "vor", "Lust", "mich", "kaum", "be\u00b7sann", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "APPR", "NN", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Das bunte Tr\u00f6pfgen folgends an:", "tokens": ["Das", "bun\u00b7te", "Tr\u00f6pf\u00b7gen", "fol\u00b7gends", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PIS", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wie kommt es, da\u00df in deiner R\u00fcnde", "tokens": ["Wie", "kommt", "es", ",", "da\u00df", "in", "dei\u00b7ner", "R\u00fcn\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "$,", "KOUS", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ich ein so herrlich, buntes Licht,", "tokens": ["Ich", "ein", "so", "herr\u00b7lich", ",", "bun\u00b7tes", "Licht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ART", "ADV", "ADJD", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Mit fast geblendetem Gesicht,", "tokens": ["Mit", "fast", "ge\u00b7blen\u00b7de\u00b7tem", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "In solchem hellen Schimmer finde?", "tokens": ["In", "sol\u00b7chem", "hel\u00b7len", "Schim\u00b7mer", "fin\u00b7de", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Drauf deucht mich, da\u00df ich sehend h\u00f6rte,", "tokens": ["Drauf", "deucht", "mich", ",", "da\u00df", "ich", "se\u00b7hend", "h\u00f6r\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "$,", "KOUS", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Wie es, mit klarer Schrift, mich dergestalt belehrte:", "tokens": ["Wie", "es", ",", "mit", "kla\u00b7rer", "Schrift", ",", "mich", "der\u00b7ge\u00b7stalt", "be\u00b7lehr\u00b7te", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "$,", "APPR", "ADJA", "NN", "$,", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Was mich mit solchem Glantz erf\u00fcllt,", "tokens": ["Was", "mich", "mit", "sol\u00b7chem", "Glantz", "er\u00b7f\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Ist das mir eingepr\u00e4gte Bild", "tokens": ["Ist", "das", "mir", "ein\u00b7ge\u00b7pr\u00e4g\u00b7te", "Bild"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PDS", "PPER", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Der Sonne, die so wunder-sch\u00f6n,", "tokens": ["Der", "Son\u00b7ne", ",", "die", "so", "wun\u00b7der\u00b7sch\u00f6n", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Und die ihr, obgleich ihre Pracht", "tokens": ["Und", "die", "ihr", ",", "ob\u00b7gleich", "ih\u00b7re", "Pracht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ART", "PPER", "$,", "KOUS", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Allein die Creatur so sch\u00f6n, so herrlich, macht;", "tokens": ["Al\u00b7lein", "die", "Crea\u00b7tur", "so", "sch\u00f6n", ",", "so", "herr\u00b7lich", ",", "macht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADV", "ADJD", "$,", "ADV", "ADJD", "$,", "VVFIN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.18": {"text": "Dennoch kaum w\u00fcrdigt anzusehn.", "tokens": ["Den\u00b7noch", "kaum", "w\u00fcr\u00b7digt", "an\u00b7zu\u00b7sehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "VVIZU", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Damit ich nun, so viel an mir,", "tokens": ["Da\u00b7mit", "ich", "nun", ",", "so", "viel", "an", "mir", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "$,", "ADV", "ADV", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Die Quell des Lichts und Lebens dir,", "tokens": ["Die", "Quell", "des", "Lichts", "und", "Le\u00b7bens", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "KON", "NN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Zu unsers grossen Sch\u00f6pfers Preise,", "tokens": ["Zu", "un\u00b7sers", "gros\u00b7sen", "Sch\u00f6p\u00b7fers", "Prei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Doch wenigstens im Abdruck weise", "tokens": ["Doch", "we\u00b7nigs\u00b7tens", "im", "Ab\u00b7druck", "wei\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "So stell ich dir ihr herrlich Licht,", "tokens": ["So", "stell", "ich", "dir", "ihr", "herr\u00b7lich", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "PPER", "PPER", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Durch meine Klarheit, ins Gesicht.", "tokens": ["Durch", "mei\u00b7ne", "Klar\u00b7heit", ",", "ins", "Ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Ja ich verricht\u2019 es nicht allein;", "tokens": ["Ja", "ich", "ver\u00b7richt'", "es", "nicht", "al\u00b7lein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PPER", "VVFIN", "PPER", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Viel Millionen an der Zahl", "tokens": ["Viel", "Mil\u00b7lion\u00b7en", "an", "der", "Zahl"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.27": {"text": "Bestreben sich, nebst mir, um auch den Strahl,", "tokens": ["Be\u00b7stre\u00b7ben", "sich", ",", "nebst", "mir", ",", "um", "auch", "den", "Strahl", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "$,", "APPR", "PPER", "$,", "KOUI", "ADV", "ART", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.28": {"text": "Den allbelebenden und Wunder-reichen Schein,", "tokens": ["Den", "all\u00b7be\u00b7le\u00b7ben\u00b7den", "und", "Wun\u00b7der\u00b7rei\u00b7chen", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "KON", "NN", "NN", "$,"], "meter": "-+-+---+-+-+", "measure": "unknown.measure.penta"}}, "stanza.2": {"line.1": {"text": "In deutlicher Copie, zu zeigen;", "tokens": ["In", "deut\u00b7li\u00b7cher", "Co\u00b7pie", ",", "zu", "zei\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Damit ihr zum Original,", "tokens": ["Da\u00b7mit", "ihr", "zum", "O\u00b7rig\u00b7i\u00b7nal", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+---", "measure": "unknown.measure.tri"}, "line.3": {"text": "Durch ihren Glantz ger\u00fchrt, bewundernd m\u00f6get steigen.", "tokens": ["Durch", "ih\u00b7ren", "Glantz", "ge\u00b7r\u00fchrt", ",", "be\u00b7wun\u00b7dernd", "m\u00f6\u00b7get", "stei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$,", "ADJD", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ich ward ger\u00fchret durch die Klarheit", "tokens": ["Ich", "ward", "ge\u00b7r\u00fch\u00b7ret", "durch", "die", "Klar\u00b7heit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "VVPP", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Der von dem Tropffen Thau mir angezeigten Wahrheit;", "tokens": ["Der", "von", "dem", "Tropf\u00b7fen", "Thau", "mir", "an\u00b7ge\u00b7zeig\u00b7ten", "Wahr\u00b7heit", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "NN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ich wendete mein geist- und c\u00f6rperlich Gesicht,", "tokens": ["Ich", "wen\u00b7de\u00b7te", "mein", "geist", "und", "c\u00f6r\u00b7per\u00b7lich", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "TRUNC", "KON", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Voll Lust und Danck, zum hellen Sonnen-Licht,", "tokens": ["Voll", "Lust", "und", "Danck", ",", "zum", "hel\u00b7len", "Son\u00b7nen\u00b7Licht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "KON", "NN", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Und danckte GOtt, da\u00df er derselben Pracht", "tokens": ["Und", "danck\u00b7te", "Gott", ",", "da\u00df", "er", "der\u00b7sel\u00b7ben", "Pracht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "NN", "$,", "KOUS", "PPER", "PDAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "So wunderbar gemacht.", "tokens": ["So", "wun\u00b7der\u00b7bar", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Dem sch\u00f6nen Morgen nun, nach dem der Tag verschwunden,", "tokens": ["Dem", "sch\u00f6\u00b7nen", "Mor\u00b7gen", "nun", ",", "nach", "dem", "der", "Tag", "ver\u00b7schwun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "$,", "APPR", "ART", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Und sich der Abend eingefunden,", "tokens": ["Und", "sich", "der", "A\u00b7bend", "ein\u00b7ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Folgt\u2019 eine ja so sch\u00f6ne Nacht.", "tokens": ["Folgt'", "ei\u00b7ne", "ja", "so", "sch\u00f6\u00b7ne", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADV", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Ich sahe denn, bey heitrer Luft, im Dunckeln,", "tokens": ["Ich", "sa\u00b7he", "denn", ",", "bey", "hei\u00b7trer", "Luft", ",", "im", "Dun\u00b7ckeln", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "APPR", "ADJA", "NN", "$,", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Die ungezehlten Sterne funckeln.", "tokens": ["Die", "un\u00b7ge\u00b7zehl\u00b7ten", "Ster\u00b7ne", "fun\u00b7ckeln", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Wie ich nun fr\u00fch, vor Lust erstaunt, den Thau gesehen;", "tokens": ["Wie", "ich", "nun", "fr\u00fch", ",", "vor", "Lust", "er\u00b7staunt", ",", "den", "Thau", "ge\u00b7se\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ADJD", "$,", "APPR", "NN", "VVPP", "$,", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "So kam bey noch in mir vorhand\u2019nen Thau-Jdeen,", "tokens": ["So", "kam", "bey", "noch", "in", "mir", "vor\u00b7han\u00b7d'\u00b7nen", "Thau\u00b7J\u00b7de\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ADV", "APPR", "PPER", "ADJA", "NN", "$,"], "meter": "--+-+-+-+-+-+", "measure": "anapaest.init"}, "line.17": {"text": "Nun auch der helle Himmel mir", "tokens": ["Nun", "auch", "der", "hel\u00b7le", "Him\u00b7mel", "mir"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Als wie ein weites Feld von gl\u00e4ntzendem Sapphir,", "tokens": ["Als", "wie", "ein", "wei\u00b7tes", "Feld", "von", "gl\u00e4nt\u00b7zen\u00b7dem", "Sap\u00b7phir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.19": {"text": "Und, recht wie Tropfen Thau, die hellen Sterne f\u00fcr.", "tokens": ["Und", ",", "recht", "wie", "Trop\u00b7fen", "Thau", ",", "die", "hel\u00b7len", "Ster\u00b7ne", "f\u00fcr", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ADJD", "KOKOM", "NN", "NN", "$,", "ART", "ADJA", "NN", "APPR", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "O! rieff ich, welch ein Feld! O! welch ein Wunder-Thau,", "tokens": ["O", "!", "rieff", "ich", ",", "welch", "ein", "Feld", "!", "O", "!", "welch", "ein", "Wun\u00b7der\u00b7\u00b7T\u00b7hau", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$.", "VVFIN", "PPER", "$,", "PWAT", "ART", "NN", "$.", "NE", "$.", "PWAT", "ART", "NN", "$,"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.21": {"text": "Womit ich es erf\u00fcllet schau!", "tokens": ["Wo\u00b7mit", "ich", "es", "er\u00b7f\u00fcl\u00b7let", "schau", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "O welche Tropfen! deren Gr\u00f6sse", "tokens": ["O", "wel\u00b7che", "Trop\u00b7fen", "!", "de\u00b7ren", "Gr\u00f6s\u00b7se"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NE", "PWAT", "NN", "$.", "PRELAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Ich kaum mit den Gedancken messe!", "tokens": ["Ich", "kaum", "mit", "den", "Ge\u00b7dan\u00b7cken", "mes\u00b7se", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Und die, wie unser Thau, ihr Prangen", "tokens": ["Und", "die", ",", "wie", "un\u00b7ser", "Thau", ",", "ihr", "Pran\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ART", "$,", "PWAV", "PPOSAT", "NN", "$,", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Nur blos von einer Sonn\u2019 empfangen!", "tokens": ["Nur", "blos", "von", "ei\u00b7ner", "Sonn'", "emp\u00b7fan\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "O! welche Sonne! die nicht nur solch Sonnen-Heer,", "tokens": ["O", "!", "wel\u00b7che", "Son\u00b7ne", "!", "die", "nicht", "nur", "solch", "Son\u00b7nen\u00b7Heer", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PWAT", "NN", "$.", "ART", "PTKNEG", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Aus ihrem ew\u2019gen Lichtes Meer,", "tokens": ["Aus", "ih\u00b7rem", "ew'\u00b7gen", "Lich\u00b7tes", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Als so viel Tropfen schm\u00fcckt und zieret;", "tokens": ["Als", "so", "viel", "Trop\u00b7fen", "schm\u00fcckt", "und", "zie\u00b7ret", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PIAT", "NN", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.29": {"text": "Nein, die derselben Kraft und Pracht,", "tokens": ["Nein", ",", "die", "der\u00b7sel\u00b7ben", "Kraft", "und", "Pracht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PRELS", "PDAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "Durchs Feur der Lieb\u2019, hervorgebracht,", "tokens": ["Durchs", "Feur", "der", "Lieb'", ",", "her\u00b7vor\u00b7ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "Zum Nutz der Creatur formiret!", "tokens": ["Zum", "Nutz", "der", "Crea\u00b7tur", "for\u00b7mi\u00b7ret", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.3": {"line.1": {"text": "Ach la\u00df, o ew\u2019ges Liebes-Licht,", "tokens": ["Ach", "la\u00df", ",", "o", "ew'\u00b7ges", "Lie\u00b7bes\u00b7Licht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "$,", "FM", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So oft ich, in des Himmels H\u00f6he,", "tokens": ["So", "oft", "ich", ",", "in", "des", "Him\u00b7mels", "H\u00f6\u00b7he", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "$,", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der Sonnen grosse Tropffen sehe;", "tokens": ["Der", "Son\u00b7nen", "gros\u00b7se", "Tropf\u00b7fen", "se\u00b7he", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Doch meine Seele, durchs Gesicht,", "tokens": ["Doch", "mei\u00b7ne", "See\u00b7le", ",", "durchs", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Zu dir, in ihrer Meng\u2019, als so viel Staffeln, steigen,", "tokens": ["Zu", "dir", ",", "in", "ih\u00b7rer", "Meng'", ",", "als", "so", "viel", "Staf\u00b7feln", ",", "stei\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PPER", "$,", "APPR", "PPOSAT", "NN", "$,", "KOUS", "ADV", "PIAT", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und, voller Ehrfurcht, nie von deinem Ruhme schweigen!", "tokens": ["Und", ",", "vol\u00b7ler", "Ehr\u00b7furcht", ",", "nie", "von", "dei\u00b7nem", "Ruh\u00b7me", "schwei\u00b7gen", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ADJA", "NN", "$,", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}