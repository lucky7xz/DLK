{"textgrid.poem.25356": {"metadata": {"author": {"name": "Stadler, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du griffst nach Gl\u00fcck.", "genre": "verse", "period": "N.A.", "pub_year": 1898, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du griffst nach Gl\u00fcck.", "tokens": ["Du", "griffst", "nach", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Es schmolz wie Flocken Schnees,", "tokens": ["Es", "schmolz", "wie", "Flo\u00b7cken", "Schnees", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "KOKOM", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "die du in aufgehobnen H\u00e4nden eingefangen.", "tokens": ["die", "du", "in", "auf\u00b7ge\u00b7hob\u00b7nen", "H\u00e4n\u00b7den", "ein\u00b7ge\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Frost fiel auf dich. Du hast Decken", "tokens": ["Frost", "fiel", "auf", "dich", ".", "Du", "hast", "De\u00b7cken"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPR", "PPER", "$.", "PPER", "VAFIN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "\u00fcber dein rot str\u00f6mendes Herz gehangen.", "tokens": ["\u00fc\u00b7ber", "dein", "rot", "str\u00f6\u00b7men\u00b7des", "Herz", "ge\u00b7han\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJD", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.6": {"text": "Traumstarre kam und f\u00fcllte alle Mulden deiner Seele", "tokens": ["Traum\u00b7star\u00b7re", "kam", "und", "f\u00fcll\u00b7te", "al\u00b7le", "Mul\u00b7den", "dei\u00b7ner", "See\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "KON", "VVFIN", "PIAT", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.7": {"text": "wie Gew\u00e4sser aus entsperrten Wehren \u2013", "tokens": ["wie", "Ge\u00b7w\u00e4s\u00b7ser", "aus", "ent\u00b7sperr\u00b7ten", "Weh\u00b7ren", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "ADJA", "NN", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Nun f\u00fchlst du W\u00fcsten um dich wachsen,", "tokens": ["Nun", "f\u00fchlst", "du", "W\u00fcs\u00b7ten", "um", "dich", "wach\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVFIN", "APPR", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "die dein wehes Blut verzehren.", "tokens": ["die", "dein", "we\u00b7hes", "Blut", "ver\u00b7zeh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Nun siehst du dich, mit nachtgebundnen Augen,", "tokens": ["Nun", "siehst", "du", "dich", ",", "mit", "nacht\u00b7ge\u00b7bund\u00b7nen", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "wie im Schlaf, durch tote Gassen schreiten", "tokens": ["wie", "im", "Schlaf", ",", "durch", "to\u00b7te", "Gas\u00b7sen", "schrei\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "APPRART", "NN", "$,", "APPR", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.12": {"text": "Und Schicksal, spukhaft nah und unerreichbar,", "tokens": ["Und", "Schick\u00b7sal", ",", "spuk\u00b7haft", "nah", "und", "un\u00b7er\u00b7reich\u00b7bar", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "ADJD", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.13": {"text": "dir vor\u00fcbergleiten.", "tokens": ["dir", "vor\u00b7\u00fc\u00b7berg\u00b7lei\u00b7ten", "."], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.14": {"text": "Wach auf! Gespenster suchen dich!", "tokens": ["Wach", "auf", "!", "Ge\u00b7spens\u00b7ter", "su\u00b7chen", "dich", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "$.", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Sieh: \u00fcber dir w\u00f6lbt sich s\u00fcdlicher Mittagshimmel,", "tokens": ["Sieh", ":", "\u00fc\u00b7ber", "dir", "w\u00f6lbt", "sich", "s\u00fcd\u00b7li\u00b7cher", "Mit\u00b7tags\u00b7him\u00b7mel", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "APPR", "PPER", "VVFIN", "PRF", "ADJA", "NN", "$,"], "meter": "-+--+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.16": {"text": "buntgefleckt, goldtief und klar!", "tokens": ["bunt\u00b7ge\u00b7fleckt", ",", "gold\u00b7tief", "und", "klar", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "VVFIN", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.17": {"text": "Sieh: der Meerwind deiner Kindheit weht immer noch", "tokens": ["Sieh", ":", "der", "Meer\u00b7wind", "dei\u00b7ner", "Kind\u00b7heit", "weht", "im\u00b7mer", "noch"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$.", "ART", "NN", "PPOSAT", "NN", "VVFIN", "ADV", "ADV"], "meter": "+-+-+-+-+--+", "measure": "iambic.hexa.chol"}, "line.18": {"text": "\u00fcber dein aufgelockertes schwarzes Haar!", "tokens": ["\u00fc\u00b7ber", "dein", "auf\u00b7ge\u00b7lo\u00b7cker\u00b7tes", "schwar\u00b7zes", "Haar", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "ADJA", "NN", "$."], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.19": {"text": "Sieh: deine schlaf bet\u00e4ubten Augen sind", "tokens": ["Sieh", ":", "dei\u00b7ne", "schlaf", "be\u00b7t\u00e4ub\u00b7ten", "Au\u00b7gen", "sind"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$.", "PPOSAT", "NN", "ADJA", "NN", "VAFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "ganz getr\u00e4nkt und vollgesogen", "tokens": ["ganz", "ge\u00b7tr\u00e4nkt", "und", "voll\u00b7ge\u00b7so\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVPP", "KON", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "Mit Gl\u00fcck der Welt, das sie in fr\u00fchen Klostertagen", "tokens": ["Mit", "Gl\u00fcck", "der", "Welt", ",", "das", "sie", "in", "fr\u00fc\u00b7hen", "Klos\u00b7ter\u00b7ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "ART", "NN", "$,", "PRELS", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "d\u00fcrstend auf sich hergezogen.", "tokens": ["d\u00fcrs\u00b7tend", "auf", "sich", "her\u00b7ge\u00b7zo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PRF", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.23": {"text": "Und jeder Hauch,", "tokens": ["Und", "je\u00b7der", "Hauch", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.24": {"text": "der dein erwachend Blut dereinst bewegt,", "tokens": ["der", "dein", "er\u00b7wa\u00b7chend", "Blut", "de\u00b7reinst", "be\u00b7wegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "VVPP", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "Ward nun zum festen Pulsschlag,", "tokens": ["Ward", "nun", "zum", "fes\u00b7ten", "Puls\u00b7schlag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.26": {"text": "der dein Wesen n\u00e4hrt und tr\u00e4gt.", "tokens": ["der", "dein", "We\u00b7sen", "n\u00e4hrt", "und", "tr\u00e4gt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "KON", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.27": {"text": "Tanz b\u00e4umt sich in deinen Gliedern", "tokens": ["Tanz", "b\u00e4umt", "sich", "in", "dei\u00b7nen", "Glie\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PRF", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.28": {"text": "und wartet, aufgereckt,", "tokens": ["und", "war\u00b7tet", ",", "auf\u00b7ge\u00b7reckt", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.29": {"text": "Da\u00df deines Herzens Cymbelschlagen", "tokens": ["Da\u00df", "dei\u00b7nes", "Her\u00b7zens", "Cym\u00b7bel\u00b7schla\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.30": {"text": "seine Lust erweckt.", "tokens": ["sei\u00b7ne", "Lust", "er\u00b7weckt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.31": {"text": "Deines Lebens Stimme steigt,", "tokens": ["Dei\u00b7nes", "Le\u00b7bens", "Stim\u00b7me", "steigt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.32": {"text": "morgendlich \u00fcberschwellend wie Lerchenschlag,", "tokens": ["mor\u00b7gend\u00b7lich", "\u00fc\u00b7bersc\u00b7hwel\u00b7lend", "wie", "Ler\u00b7chen\u00b7schlag", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "KOKOM", "NN", "$,"], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.33": {"text": "\u00dcber das Fr\u00fchlingsland,", "tokens": ["\u00dc\u00b7ber", "das", "Fr\u00fch\u00b7lings\u00b7land", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.34": {"text": "das lauter und jung ergl\u00e4nzt wie am ersten Tag.", "tokens": ["das", "lau\u00b7ter", "und", "jung", "er\u00b7gl\u00e4nzt", "wie", "am", "ers\u00b7ten", "Tag", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ADJD", "VVPP", "KOKOM", "APPRART", "ADJA", "NN", "$."], "meter": "-+--+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.35": {"text": "Vor deiner Schwelle wartet alles Wunder", "tokens": ["Vor", "dei\u00b7ner", "Schwel\u00b7le", "war\u00b7tet", "al\u00b7les", "Wun\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.36": {"text": "und will zu dir herein \u2013", "tokens": ["und", "will", "zu", "dir", "her\u00b7ein", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "APPR", "PPER", "PTKVZ", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.37": {"text": "Sch\u00fcttle die Nacht von dir!", "tokens": ["Sch\u00fctt\u00b7le", "die", "Nacht", "von", "dir", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "PPER", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.2": {"line.1": {"text": "Du griffst nach Gl\u00fcck.", "tokens": ["Du", "griffst", "nach", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Es schmolz wie Flocken Schnees,", "tokens": ["Es", "schmolz", "wie", "Flo\u00b7cken", "Schnees", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "KOKOM", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "die du in aufgehobnen H\u00e4nden eingefangen.", "tokens": ["die", "du", "in", "auf\u00b7ge\u00b7hob\u00b7nen", "H\u00e4n\u00b7den", "ein\u00b7ge\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Frost fiel auf dich. Du hast Decken", "tokens": ["Frost", "fiel", "auf", "dich", ".", "Du", "hast", "De\u00b7cken"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPR", "PPER", "$.", "PPER", "VAFIN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "\u00fcber dein rot str\u00f6mendes Herz gehangen.", "tokens": ["\u00fc\u00b7ber", "dein", "rot", "str\u00f6\u00b7