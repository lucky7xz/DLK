{"dta.poem.12127": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "49.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201ewas hast du nun im Brief f\u00fcr Neuigkeit erhalten?\u201c", "tokens": ["\u201e", "was", "hast", "du", "nun", "im", "Brief", "f\u00fcr", "Neu\u00b7ig\u00b7keit", "er\u00b7hal\u00b7ten", "?", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PWS", "VAFIN", "PPER", "ADV", "APPRART", "NN", "APPR", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gar kein' als da\u00df daheim noch alles ist beim Alten.", "tokens": ["Gar", "kein'", "als", "da\u00df", "da\u00b7heim", "noch", "al\u00b7les", "ist", "beim", "Al\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "KOKOM", "KOUS", "ADV", "ADV", "PIS", "VAFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Und weiter w\u00fcnsch' ich nichts, als da\u00df dort alles bleibe", "tokens": ["Und", "wei\u00b7ter", "w\u00fcn\u00b7sch'", "ich", "nichts", ",", "als", "da\u00df", "dort", "al\u00b7les", "blei\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PIS", "$,", "KOKOM", "KOUS", "ADV", "PIS", "VVFIN"], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Beim Alten, au\u00dfer dem was Neues heim ich schreibe.", "tokens": ["Beim", "Al\u00b7ten", ",", "au\u00b7\u00dfer", "dem", "was", "Neu\u00b7es", "heim", "ich", "schrei\u00b7be", "."], "token_info": ["word", "wor