{"dta.poem.5467": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Vergn\u00fcgen auch bey feuchtem Wetter  \n im Winter.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Da\u00df es an uns allein fast lieget,", "tokens": ["Da\u00df", "es", "an", "uns", "al\u00b7lein", "fast", "lie\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn man sich nicht zu aller Zeit,", "tokens": ["Wenn", "man", "sich", "nicht", "zu", "al\u00b7ler", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PRF", "PTKNEG", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "An der Gesch\u00f6pfe Lieblichkeit,", "tokens": ["An", "der", "Ge\u00b7sch\u00f6p\u00b7fe", "Lieb\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auch gar wenn alles na\u00df, und schlackrig ist, vergn\u00fcget;", "tokens": ["Auch", "gar", "wenn", "al\u00b7les", "na\u00df", ",", "und", "schlack\u00b7rig", "ist", ",", "ver\u00b7gn\u00fc\u00b7get", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADV", "KOUS", "PIS", "ADJD", "$,", "KON", "ADJD", "VAFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Hat mir ein tr\u00fcber Tag im Winter j\u00fcngst gewiesen,", "tokens": ["Hat", "mir", "ein", "tr\u00fc\u00b7ber", "Tag", "im", "Win\u00b7ter", "j\u00fcngst", "ge\u00b7wie\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN", "APPRART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Da ich verschiednes, welches sch\u00f6n,", "tokens": ["Da", "ich", "ver\u00b7schied\u00b7nes", ",", "wel\u00b7ches", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJA", "$,", "PRELS", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Auch im December selbst, gesehn,", "tokens": ["Auch", "im", "De\u00b7cem\u00b7ber", "selbst", ",", "ge\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "ADV", "$,", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Woran ich mich vergn\u00fcgt und GOtt daf\u00fcr gepriesen.", "tokens": ["Wo\u00b7ran", "ich", "mich", "ver\u00b7gn\u00fcgt", "und", "Gott", "da\u00b7f\u00fcr", "ge\u00b7prie\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "VVPP", "KON", "NN", "PAV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ich ging, in einer Morgen-Stunde,", "tokens": ["Ich", "ging", ",", "in", "ei\u00b7ner", "Mor\u00b7gen\u00b7Stun\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit einem Pfeifchen in dem Munde,", "tokens": ["Mit", "ei\u00b7nem", "Pfei\u00b7fchen", "in", "dem", "Mun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "In meinem Garten auf und nieder,", "tokens": ["In", "mei\u00b7nem", "Gar\u00b7ten", "auf", "und", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Von Grillen und Gesch\u00e4ften frey,", "tokens": ["Von", "Gril\u00b7len", "und", "Ge\u00b7sch\u00e4f\u00b7ten", "frey", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und ward gewahr, wie hin und wieder,", "tokens": ["Und", "ward", "ge\u00b7wahr", ",", "wie", "hin", "und", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADJD", "$,", "PWAV", "ADV", "KON", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Auch wenn es schlackrig ist, doch was betr\u00e4chtlichs sey.", "tokens": ["Auch", "wenn", "es", "schlack\u00b7rig", "ist", ",", "doch", "was", "be\u00b7tr\u00e4cht\u00b7lichs", "sey", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "ADJD", "VAFIN", "$,", "KON", "PWS", "PIS", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die B\u00e4ume, die nunmehr entkleidet, kamen mir,", "tokens": ["Die", "B\u00e4u\u00b7me", ",", "die", "nun\u00b7mehr", "ent\u00b7klei\u00b7det", ",", "ka\u00b7men", "mir", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "VVPP", "$,", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Als wenn sie w\u00fcrcklich schlieffen, f\u00fcr;", "tokens": ["Als", "wenn", "sie", "w\u00fcrck\u00b7lich", "schlief\u00b7fen", ",", "f\u00fcr", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "ADJD", "VVINF", "$,", "APPR", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Die Winde schienen oft, durch hin und wieder biegen,", "tokens": ["Die", "Win\u00b7de", "schie\u00b7nen", "oft", ",", "durch", "hin", "und", "wie\u00b7der", "bie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "$,", "APPR", "PTKVZ", "KON", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Sie gleichsam in den Schlaf zu wiegen.", "tokens": ["Sie", "gleich\u00b7sam", "in", "den", "Schlaf", "zu", "wie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Sind nun die Wipfel gleich, da alle Zweig\u2019 entlaubet,", "tokens": ["Sind", "nun", "die", "Wip\u00b7fel", "gleich", ",", "da", "al\u00b7le", "Zweig'", "ent\u00b7lau\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "ADV", "$,", "KOUS", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Von ihrer gr\u00fcnen Pracht beraubet;", "tokens": ["Von", "ih\u00b7rer", "gr\u00fc\u00b7nen", "Pracht", "be\u00b7rau\u00b7bet", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "So scheinen doch, wenn wir es wol erwegen,", "tokens": ["So", "schei\u00b7nen", "doch", ",", "wenn", "wir", "es", "wol", "er\u00b7we\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "$,", "KOUS", "PPER", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Der B\u00e4ume feuchte St\u00e4mm\u2019 hingegen", "tokens": ["Der", "B\u00e4u\u00b7me", "feuch\u00b7te", "St\u00e4mm'", "hin\u00b7ge\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Mehr, als vorhin, geschm\u00fcckt; indem die Dunckelheit", "tokens": ["Mehr", ",", "als", "vor\u00b7hin", ",", "ge\u00b7schm\u00fcckt", ";", "in\u00b7dem", "die", "Dun\u00b7ckel\u00b7heit"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PIS", "$,", "KOUS", "ADV", "$,", "VVPP", "$.", "KOUS", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Die Rinden schw\u00e4rtzt) des Moses gr\u00fcne Pracht,", "tokens": ["Die", "Rin\u00b7den", "schw\u00e4rtzt", ")", "des", "Mo\u00b7ses", "gr\u00fc\u00b7ne", "Pracht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$(", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Die hie und da mit weisser untermischt,", "tokens": ["Die", "hie", "und", "da", "mit", "weis\u00b7ser", "un\u00b7ter\u00b7mischt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "KON", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Nicht durch die N\u00e4sse nur erfrischt;", "tokens": ["Nicht", "durch", "die", "N\u00e4s\u00b7se", "nur", "er\u00b7frischt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "An Farbe noch viel sch\u00f6ner macht.", "tokens": ["An", "Far\u00b7be", "noch", "viel", "sch\u00f6\u00b7ner", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Wenn \u00fcberdem", "tokens": ["Wenn", "\u00fc\u00b7ber\u00b7dem"], "token_info": ["word", "word"], "pos": ["KOUS", "ADJA"], "meter": "-+--", "measure": "dactylic.init"}, "line.2": {"text": "Jetzt \u00fcberall, auch an den kleinsten Zweigen,", "tokens": ["Jetzt", "\u00fc\u00b7be\u00b7rall", ",", "auch", "an", "den", "kleins\u00b7ten", "Zwei\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sich grosse, klare Tropfen zeigen;", "tokens": ["Sich", "gros\u00b7se", ",", "kla\u00b7re", "Trop\u00b7fen", "zei\u00b7gen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PRF", "ADJA", "$,", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So l\u00e4\u00dft auch dieses angenehm.", "tokens": ["So", "l\u00e4\u00dft", "auch", "die\u00b7ses", "an\u00b7ge\u00b7nehm", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PDAT", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Indem sie all\u2019 in reiner Klarheit prangen,", "tokens": ["In\u00b7dem", "sie", "all'", "in", "rei\u00b7ner", "Klar\u00b7heit", "pran\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIS", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Als s\u00e4he man daran erystallne Kugeln hangen,", "tokens": ["Als", "s\u00e4\u00b7he", "man", "da\u00b7ran", "e\u00b7rys\u00b7tall\u00b7ne", "Ku\u00b7geln", "han\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PIS", "PAV", "ADJA", "NN", "VVINF", "$,"], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "Wenn man auf sie sein Aug\u2019 in dieser Absicht lencket,", "tokens": ["Wenn", "man", "auf", "sie", "sein", "Aug'", "in", "die\u00b7ser", "Ab\u00b7sicht", "len\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "PPER", "PPOSAT", "NN", "APPR", "PDAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und, da\u00df es in der That den Augen lieblich, dencket.", "tokens": ["Und", ",", "da\u00df", "es", "in", "der", "That", "den", "Au\u00b7gen", "lieb\u00b7lich", ",", "den\u00b7cket", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PPER", "APPR", "ART", "NN", "ART", "NN", "ADJD", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Die Knospen, welche man jetzt mehr, als sonften, sieht,", "tokens": ["Die", "Knos\u00b7pen", ",", "wel\u00b7che", "man", "jetzt", "mehr", ",", "als", "sonf\u00b7ten", ",", "sieht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PIS", "ADV", "ADV", "$,", "KOUS", "ADJA", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die zeigen uns die Stellen, worin sich", "tokens": ["Die", "zei\u00b7gen", "uns", "die", "Stel\u00b7len", ",", "wo\u00b7rin", "sich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PDS", "VVFIN", "PPER", "ART", "NN", "$,", "PWAV", "PRF"], "meter": "-+-+-+-++-", "measure": "unknown.measure.penta"}, "line.3": {"text": "Die wirckende Natur hier innerlich,", "tokens": ["Die", "wir\u00b7cken\u00b7de", "Na\u00b7tur", "hier", "in\u00b7ner\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Zu unsrer Lust, zu unserm Nutz, bem\u00fcht.", "tokens": ["Zu", "uns\u00b7rer", "Lust", ",", "zu", "un\u00b7serm", "Nutz", ",", "be\u00b7m\u00fcht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "APPR", "PPOSAT", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Es scheint die Luft zwar schwer, und recht auf uns", "tokens": ["Es", "scheint", "die", "Luft", "zwar", "schwer", ",", "und", "recht", "auf", "uns"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "ADJD", "$,", "KON", "ADJD", "APPR", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wenn sie mit feuchtem Duft uns rings umher bezirckt,", "tokens": ["Wenn", "sie", "mit", "feuch\u00b7tem", "Duft", "uns", "rings", "um\u00b7her", "be\u00b7zirckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN", "PPER", "ADV", "PTKVZ", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wodurch sie denn in uns ein\u2019 Art von Schwermuth wirckt,", "tokens": ["Wo\u00b7durch", "sie", "denn", "in", "uns", "ein'", "Art", "von", "Schwer\u00b7muth", "wirckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "APPR", "PPER", "ART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die aber eigentlich kein wahres Unvergn\u00fcgen.", "tokens": ["Die", "a\u00b7ber", "ei\u00b7gent\u00b7lich", "kein", "wah\u00b7res", "Un\u00b7ver\u00b7gn\u00fc\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Es mischt sich eine Luft in diesen Unmuth ein,", "tokens": ["Es", "mischt", "sich", "ei\u00b7ne", "Luft", "in", "die\u00b7sen", "Un\u00b7muth", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ART", "NN", "APPR", "PDAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wodurch man gleichsam kann in Schwermuth fr\u00f6lich seyn.", "tokens": ["Wo\u00b7durch", "man", "gleich\u00b7sam", "kann", "in", "Schwer\u00b7muth", "fr\u00f6\u00b7lich", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ADJD", "VMFIN", "APPR", "NN", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Es scheint des Geistes Kraft, wenn tr\u00fcbe Luft uns dr\u00fcckt,", "tokens": ["Es", "scheint", "des", "Geis\u00b7tes", "Kraft", ",", "wenn", "tr\u00fc\u00b7be", "Luft", "uns", "dr\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "$,", "KOUS", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und unsre Blicke hemmt, sich minder zu zerstreuen,", "tokens": ["Und", "uns\u00b7re", "Bli\u00b7cke", "hemmt", ",", "sich", "min\u00b7der", "zu", "zer\u00b7streu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "$,", "PRF", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Und, gleichsam mehr vereint, zum Dencken mehr geschickt,", "tokens": ["Und", ",", "gleich\u00b7sam", "mehr", "ver\u00b7eint", ",", "zum", "Den\u00b7cken", "mehr", "ge\u00b7schickt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ADJD", "ADV", "VVPP", "$,", "APPRART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Bey \u00e4usserlichem Schaur sich innerlich zu freuen.", "tokens": ["Bey", "\u00e4us\u00b7ser\u00b7li\u00b7chem", "Schaur", "sich", "in\u00b7ner\u00b7lich", "zu", "freu\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PRF", "ADJD", "PTKZU", "VVINF", "$."], "meter": "++-+-+-+-+-+-", "measure": "unknown.measure.septa"}}, "stanza.6": {"line.1": {"text": "Indem ich dieses f\u00fchl\u2019, empfind\u2019 ich doch dabey,", "tokens": ["In\u00b7dem", "ich", "die\u00b7ses", "f\u00fchl'", ",", "emp\u00b7find'", "ich", "doch", "da\u00b7bey", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PDS", "VVFIN", "$,", "VVFIN", "PPER", "ADV", "PAV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df diese tr\u00fcbe Zeit, nur denen leidlich sey,", "tokens": ["Da\u00df", "die\u00b7se", "tr\u00fc\u00b7be", "Zeit", ",", "nur", "de\u00b7nen", "leid\u00b7lich", "sey", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "ADJA", "NN", "$,", "ADV", "PRELS", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die, wenn so K\u00e4lt\u2019 als Feuchtigkeit sich mehren,", "tokens": ["Die", ",", "wenn", "so", "K\u00e4lt'", "als", "Feuch\u00b7tig\u00b7keit", "sich", "meh\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "KOUS", "ADV", "NN", "KOUS", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In ihre warmen Zimmer kehren,", "tokens": ["In", "ih\u00b7re", "war\u00b7men", "Zim\u00b7mer", "keh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und sich mit Recht erfreuen k\u00f6nnen,", "tokens": ["Und", "sich", "mit", "Recht", "er\u00b7freu\u00b7en", "k\u00f6n\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "APPR", "NN", "VVINF", "VMINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df ihnen, in der Winter-Zeit,", "tokens": ["Da\u00df", "ih\u00b7nen", ",", "in", "der", "Win\u00b7ter\u00b7Zeit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der Sch\u00f6pfer die Beqvemlichkeit,", "tokens": ["Der", "Sch\u00f6p\u00b7fer", "die", "