{"textgrid.poem.53506": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Revue", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Weiblichkeit la\u00df ich vor\u00fcberrauschen,", "tokens": ["Die", "Weib\u00b7lich\u00b7keit", "la\u00df", "ich", "vor\u00b7\u00fc\u00b7berr\u00b7au\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVIMP", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Hilfsdienstmutwillige, M\u00e4dchen aus dem Land \u2013", "tokens": ["Hilfs\u00b7dienst\u00b7mut\u00b7wil\u00b7li\u00b7ge", ",", "M\u00e4d\u00b7chen", "aus", "dem", "Land", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "APPR", "ART", "NN", "$("], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "dem Schlagen eines Herzens will ich lauschen \u2013", "tokens": ["dem", "Schla\u00b7gen", "ei\u00b7nes", "Her\u00b7zens", "will", "ich", "lau\u00b7schen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VMFIN", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "gib mir die Hand!", "tokens": ["gib", "mir", "die", "Hand", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Ja, aber wer? In diesen Menschenwogen", "tokens": ["Ja", ",", "a\u00b7ber", "wer", "?", "In", "die\u00b7sen", "Men\u00b7schen\u00b7wo\u00b7gen"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKANT", "$,", "KON", "PWS", "$.", "APPR", "PDAT", "NN"], "meter": "-+-+-+-