"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die ihr dann als Gift verschreit!", "tokens": ["Die", "ihr", "dann", "als", "Gift", "ver\u00b7schreit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "KOUS", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und die K\u00fcsse sind vergessen,", "tokens": ["Und", "die", "K\u00fcs\u00b7se", "sind", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ist ihr Zauber winterkalt,", "tokens": ["Ist", "ihr", "Zau\u00b7ber", "win\u00b7ter\u00b7kalt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Von dem Teufel scheint besessen", "tokens": ["Von", "dem", "Teu\u00b7fel", "scheint", "be\u00b7ses\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Was sonst Amors Allgewalt!", "tokens": ["Was", "sonst", "A\u00b7mors", "All\u00b7ge\u00b7walt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "NE", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}