{"dta.poem.5487": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Sonnen-Schein in der Nacht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich seh, auch mitten in der Nacht,", "tokens": ["Ich", "seh", ",", "auch", "mit\u00b7ten", "in", "der", "Nacht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADV", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Jm hellen Mond der Sonnen Pracht,", "tokens": ["Jm", "hel\u00b7len", "Mond", "der", "Son\u00b7nen", "Pracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ob es die wenigsten gleich meinen,", "tokens": ["Ob", "es", "die", "we\u00b7nigs\u00b7ten", "gleich", "mei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "VVFIN", "ADV", "VVFIN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Jm Wiederschlag auf Erden scheinen.", "tokens": ["Jm", "Wie\u00b7der\u00b7schlag", "auf", "Er\u00b7den", "schei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "So danckt dem, der den Mond gemacht,", "tokens": ["So", "danckt", "dem", ",", "der", "den", "Mond", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "$,", "PRELS", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So da\u00df, auch mitten in der Nacht,", "tokens": ["So", "da\u00df", ",", "auch", "mit\u00b7ten", "in", "der", "Nacht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "$,", "ADV", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Er unsrer Sonnen Licht und Pracht,", "tokens": ["Er", "uns\u00b7rer", "Son\u00b7nen", "Licht", "und", "Pracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Die stets das Firmament erf\u00fcllet", "tokens": ["Die", "stets", "das", "Fir\u00b7ma\u00b7ment", "er\u00b7f\u00fcl\u00b7let"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "(doch wenn sie nicht an etwas f\u00e4llt", "tokens": ["(", "doch", "wenn", "sie", "nicht", "an", "et\u00b7was", "f\u00e4llt"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "ADV", "KOUS", "PPER", "PTKNEG", "APPR", "PIS", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Den Creaturen dieser Welt", "tokens": ["Den", "Crea\u00b7tu\u00b7ren", "die\u00b7ser", "Welt"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PDAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Nicht sichtbar ist) vor uns enth\u00fcllet,", "tokens": ["Nicht", "sicht\u00b7bar", "ist", ")", "vor", "uns", "ent\u00b7h\u00fcl\u00b7let", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "VAFIN", "$(", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-++--+-", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "Und la\u00dft uns, ob der Sonnen Schein,", "tokens": ["Und", "la\u00dft", "uns", ",", "ob", "der", "Son\u00b7nen", "Schein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "$,", "KOUS", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Den uns der Mond zeigt, fr\u00f6lich seyn,", "tokens": ["Den", "uns", "der", "Mond", "zeigt", ",", "fr\u00f6\u00b7lich", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "VVFIN", "$,", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Und unserm GOtt, um ihn zu preisen,", "tokens": ["Und", "un\u00b7serm", "Gott", ",", "um", "ihn", "zu", "prei\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "KOUI", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Doch eine frohe Seele weisen!", "tokens": ["Doch", "ei\u00b7ne", "fro\u00b7he", "See\u00b7le", "wei\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Wie kann der grosse Sch\u00f6pfer wollen,", "tokens": ["Wie", "kann", "der", "gros\u00b7se", "Sch\u00f6p\u00b7fer", "wol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "ART", "ADJA", "NN", "VMFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Da\u00df wir ihm etwas anders zollen,", "tokens": ["Da\u00df", "wir", "ihm", "et\u00b7was", "an\u00b7ders", "zol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Als eine, mit ger\u00fchrter Brust,", "tokens": ["Als", "ei\u00b7ne", ",", "mit", "ge\u00b7r\u00fchr\u00b7ter", "Brust", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Aus seinem Werck empfundne Lust?", "tokens": ["Aus", "sei\u00b7nem", "Werck", "emp\u00b7fund\u00b7ne", "Lust", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}