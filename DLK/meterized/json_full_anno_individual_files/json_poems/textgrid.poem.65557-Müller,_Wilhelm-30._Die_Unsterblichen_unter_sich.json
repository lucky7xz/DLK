{"textgrid.poem.65557": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "30. Die Unsterblichen unter sich", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr schm\u00fcckt euch freundlich und zufrieden mit gr\u00fcnen Lorbeerkr\u00e4nzen aus,", "tokens": ["Ihr", "schm\u00fcckt", "euch", "freund\u00b7lich", "und", "zu\u00b7frie\u00b7den", "mit", "gr\u00fc\u00b7nen", "Lor\u00b7beer\u00b7kr\u00e4n\u00b7zen", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "APPR", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "Und d\u00fcrft ihr drau\u00dfen nicht sie tragen, so bleiben sie f\u00fcr euer Haus.", "tokens": ["Und", "d\u00fcrft", "ihr", "drau\u00b7\u00dfen", "nicht", "sie", "tra\u00b7gen", ",", "so", "blei\u00b7ben", "sie", "f\u00fcr", "eu\u00b7er", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADV", "PTKNEG", "PPER", "VVINF", "$,", "ADV", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.3": {"text": "Wollt ihr auf Erden schon genie\u00dfen den Vorschmack der Unsterblichkeit,", "tokens": ["Wollt", "ihr", "auf", "Er\u00b7den", "schon", "ge\u00b7nie\u00b7\u00dfen", "den", "Vor\u00b7schmack", "der", "U\u00b7nsterb\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "NN", "ADV", "ADV", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+--+--+--+", "measure": "iambic.septa.relaxed"}, "