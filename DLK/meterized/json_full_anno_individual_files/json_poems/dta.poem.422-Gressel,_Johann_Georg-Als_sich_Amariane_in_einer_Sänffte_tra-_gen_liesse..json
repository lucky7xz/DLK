{"dta.poem.422": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Als sich  Amariane  in einer S\u00e4nffte tra-  \n gen liesse.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Des Atlas Schulter kan des Himmels Pallast tragen/", "tokens": ["Des", "At\u00b7las", "Schul\u00b7ter", "kan", "des", "Him\u00b7mels", "Pal\u00b7last", "tra\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VMFIN", "ART", "NN", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Amarianen bringt ein eintzger Kerl nicht fort/", "tokens": ["A\u00b7ma\u00b7ri\u00b7a\u00b7nen", "bringt", "ein", "eintz\u00b7ger", "Kerl", "nicht", "fort", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "ADJA", "NN", "PTKNEG", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Zwey Tr\u00e4ger m\u00fcssen seyn will sie an andern Ort.", "tokens": ["Zwey", "Tr\u00e4\u00b7ger", "m\u00fcs\u00b7sen", "seyn", "will", "sie", "an", "an\u00b7dern", "Ort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VMFIN", "VAINF", "VMFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Denn darf ein Mann sich wol das fortzubringen wagen?", "tokens": ["Denn", "darf", "ein", "Mann", "sich", "wol", "das", "fort\u00b7zu\u00b7brin\u00b7gen", "wa\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "NN", "PRF", "ADV", "ART", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Das schwerer ist als was der grosse ", "tokens": ["Das", "schwe\u00b7rer", "ist", "als", "was", "der", "gros\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "VAFIN", "KOUS", "PIS", "ART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und wo der Himmel sich nebst Gott im Menschen stelt.", "tokens": ["Und", "wo", "der", "Him\u00b7mel", "sich", "nebst", "Gott", "im", "Men\u00b7schen", "stelt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "PRF", "APPR", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}