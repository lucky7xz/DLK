{"textgrid.poem.42689": {"metadata": {"author": {"name": "Ratschky, Joseph Franz", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der Tod, ein alter hagrer Mann,", "genre": "verse", "period": "N.A.", "pub_year": 1783, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Tod, ein alter hagrer Mann,", "tokens": ["Der", "Tod", ",", "ein", "al\u00b7ter", "hag\u00b7rer", "Mann", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Traf einst zur Nachtzeit auf der Reise", "tokens": ["Traf", "einst", "zur", "Nacht\u00b7zeit", "auf", "der", "Rei\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPRART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Den jungen kleinen Amor an.", "tokens": ["Den", "jun\u00b7gen", "klei\u00b7nen", "A\u00b7mor", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NE", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Regenguss, der eimerweise", "tokens": ["Ein", "Re\u00b7gen\u00b7guss", ",", "der", "ei\u00b7mer\u00b7wei\u00b7se"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Aus einer Wetterwolke drang,", "tokens": ["Aus", "ei\u00b7ner", "Wet\u00b7ter\u00b7wol\u00b7ke", "drang", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und Rheens irdenes Geh\u00e4use", "tokens": ["Und", "Rheens", "ir\u00b7de\u00b7nes", "Ge\u00b7h\u00e4u\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NE", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Dem Weltmeer \u00e4hnlich machte, zwang", "tokens": ["Dem", "Welt\u00b7meer", "\u00e4hn\u00b7lich", "mach\u00b7te", ",", "zwang"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "ADJD", "VVFIN", "$,", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Die zween berittnen Bogensch\u00fctzen", "tokens": ["Die", "zween", "be\u00b7ritt\u00b7nen", "Bo\u00b7gen\u00b7sch\u00fct\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Vor einem Gasthof abzusitzen.", "tokens": ["Vor", "ei\u00b7nem", "Gast\u00b7hof", "ab\u00b7zu\u00b7sit\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Weil es kein kl\u00fcgres Mittel gab,", "tokens": ["Weil", "es", "kein", "kl\u00fcg\u00b7res", "Mit\u00b7tel", "gab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Als willig hier zu \u00fcbernachten,", "tokens": ["Als", "wil\u00b7lig", "hier", "zu", "\u00fc\u00b7bern\u00b7ach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "So legten sie die K\u00f6cher ab,", "tokens": ["So", "leg\u00b7ten", "sie", "die", "K\u00f6\u00b7cher", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Und liessen sich ein Ferkel schlachten.", "tokens": ["Und", "lies\u00b7sen", "sich", "ein", "Fer\u00b7kel", "schlach\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Nachdem ihr kleiner Abendschmaus", "tokens": ["Nach\u00b7dem", "ihr", "klei\u00b7ner", "A\u00b7bendsc\u00b7hmaus"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Verzehrt war, zogen die zween G\u00e4ste,", "tokens": ["Ver\u00b7zehrt", "war", ",", "zo\u00b7gen", "die", "zween", "G\u00e4s\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "$,", "VVFIN", "ART", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Vor Schlummer g\u00e4hnend, die durchn\u00e4sste,", "tokens": ["Vor", "Schlum\u00b7mer", "g\u00e4h\u00b7nend", ",", "die", "durch\u00b7n\u00e4ss\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,", "PRELS", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Vom Regen schwere Kleidung aus,", "tokens": ["Vom", "Re\u00b7gen", "schwe\u00b7re", "Klei\u00b7dung", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Versenkten tief sich in ein niedlich", "tokens": ["Ver\u00b7senk\u00b7ten", "tief", "sich", "in", "ein", "nied\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "PRF", "APPR", "ART", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Bepf\u00fclbtes Bett, und pflegten friedlich", "tokens": ["Be\u00b7pf\u00fclb\u00b7tes", "Bett", ",", "und", "pfleg\u00b7ten", "fried\u00b7lich"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "KON", "VVFIN", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Des Schlafes, der mit raschem Flug", "tokens": ["Des", "Schla\u00b7fes", ",", "der", "mit", "ra\u00b7schem", "Flug"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.8": {"text": "Sie bald in's Reich der Tr\u00e4ume trug.", "tokens": ["Sie", "bald", "in's", "Reich", "der", "Tr\u00e4u\u00b7me", "trug", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPRART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Die Wirthinn, der der blinde Bube", "tokens": ["Die", "Wirt\u00b7hinn", ",", "der", "der", "blin\u00b7de", "Bu\u00b7be"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Samt dem verdorrten Greis, der ihn", "tokens": ["Samt", "dem", "ver\u00b7dorr\u00b7ten", "Greis", ",", "der", "ihn"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN", "$,", "PRELS", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Begleitete, verd\u00e4chtig schien,", "tokens": ["Be\u00b7glei\u00b7te\u00b7te", ",", "ver\u00b7d\u00e4ch\u00b7tig", "schien", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Schlich nun aus Neugier in die Stube.", "tokens": ["Schlich", "nun", "aus", "Neu\u00b7gier", "in", "die", "Stu\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "APPR", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Sie steckte bald in Amors Pack,", "tokens": ["Sie", "steck\u00b7te", "bald", "in", "A\u00b7mors", "Pack", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "NE", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Bald in des Todes Mantelsack", "tokens": ["Bald", "in", "des", "To\u00b7des", "Man\u00b7tel\u00b7sack"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die mit dem feinsten Brillenglase", "tokens": ["Die", "mit", "dem", "feins\u00b7ten", "Bril\u00b7len\u00b7gla\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Zu diesem Zweck verseh'ne Nase,", "tokens": ["Zu", "die\u00b7sem", "Zweck", "ver\u00b7seh'\u00b7ne", "Na\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.9": {"text": "Und leert', als sie die K\u00f6cher fand,", "tokens": ["Und", "leert'", ",", "als", "sie", "die", "K\u00f6\u00b7cher", "fand", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Auf's Tischchen, wo die Lampe stand,", "tokens": ["Auf's", "Tischchen", ",", "wo", "die", "Lam\u00b7pe", "stand", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.11": {"text": "Die Pfeile forschend hin, als pl\u00f6tzlich", "tokens": ["Die", "Pfei\u00b7le", "for\u00b7schend", "hin", ",", "als", "pl\u00f6tz\u00b7lich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVPP", "PTKVZ", "$,", "KOUS", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Der schelmische Beelzebub", "tokens": ["Der", "schel\u00b7mi\u00b7sche", "Beel\u00b7ze\u00b7bub"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.13": {"text": "Kupido tr\u00e4umend ein entsetzlich", "tokens": ["Ku\u00b7pi\u00b7do", "tr\u00e4u\u00b7mend", "ein", "ent\u00b7setz\u00b7lich"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVPP", "ART", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Geheul in seinem Bett erhub.", "tokens": ["Ge\u00b7heul", "in", "sei\u00b7nem", "Bett", "er\u00b7hub", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Betroffen las sie nun in Eile", "tokens": ["Be\u00b7trof\u00b7fen", "las", "sie", "nun", "in", "Ei\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Die blindlings ausgeleerten Pfeile", "tokens": ["Die", "blind\u00b7lings", "aus\u00b7ge\u00b7leer\u00b7ten", "Pfei\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Zusammen, die beym matten Schein", "tokens": ["Zu\u00b7sam\u00b7men", ",", "die", "beym", "mat\u00b7ten", "Schein"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "PRELS", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Der Lampe sich so arg verwirrten,", "tokens": ["Der", "Lam\u00b7pe", "sich", "so", "arg", "ver\u00b7wirr\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "ADV", "ADJD", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Dass in Kupidens K\u00f6cherlein", "tokens": ["Dass", "in", "Ku\u00b7pi\u00b7dens", "K\u00f6\u00b7cher\u00b7lein"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPR", "NN", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.20": {"text": "Des Todes Pfeile sich verirrten,", "tokens": ["Des", "To\u00b7des", "Pfei\u00b7le", "sich", "ver\u00b7irr\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Und manches Pfeilchen Amors sich", "tokens": ["Und", "man\u00b7ches", "Pfeil\u00b7chen", "A\u00b7mors", "sich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "NE", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Mit in des Todes K\u00f6cher schlich.", "tokens": ["Mit", "in", "des", "To\u00b7des", "K\u00f6\u00b7cher", "schlich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Seit diesem feinen Abentheuer", "tokens": ["Seit", "die\u00b7sem", "fei\u00b7nen", "A\u00b7bent\u00b7heu\u00b7er"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sieht man, dass, gleich dem j\u00fcngsten Freyer,", "tokens": ["Sieht", "man", ",", "dass", ",", "gleich", "dem", "j\u00fcng\u00b7sten", "Frey\u00b7er", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "$,", "KOUS", "$,", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der Graukopf nun um Liebe wirbt,", "tokens": ["Der", "Grau\u00b7kopf", "nun", "um", "Lie\u00b7be", "wirbt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und oft zu fr\u00fch der J\u00fcngling stirbt,", "tokens": ["Und", "oft", "zu", "fr\u00fch", "der", "J\u00fcng\u00b7ling", "stirbt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PTKA", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Weil itzt der Tod aus seinem K\u00f6cher", "tokens": ["Weil", "itzt", "der", "Tod", "aus", "sei\u00b7nem", "K\u00f6\u00b7cher"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Kupidens Pfeil' auf alte Sch\u00e4cher", "tokens": ["Ku\u00b7pi\u00b7dens", "Pfeil'", "auf", "al\u00b7te", "Sch\u00e4\u00b7cher"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Aus Irrthum oft zu schleudern pflegt,", "tokens": ["Aus", "Irr\u00b7thum", "oft", "zu", "schleu\u00b7dern", "pflegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und mit des Knochenmannes Pfeilen", "tokens": ["Und", "mit", "des", "Kno\u00b7chen\u00b7man\u00b7nes", "Pfei\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Der kleine blinde Gott zuweilen", "tokens": ["Der", "klei\u00b7ne", "blin\u00b7de", "Gott", "zu\u00b7wei\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Dem J\u00fcngling Todeswunden schl\u00e4gt.", "tokens": ["Dem", "J\u00fcng\u00b7ling", "To\u00b7des\u00b7wun\u00b7den", "schl\u00e4gt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Der Tod, ein alter hagrer Mann,", "tokens": ["Der", "Tod", ",", "ein", "al\u00b7ter", "hag\u00b7rer", "Mann", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Traf einst zur Nachtzeit auf der Reise", "tokens": ["Traf", "einst", "zur", "Nacht\u00b7zeit", "auf", "der", "Rei\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPRART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Den jungen kleinen Amor an.", "tokens": ["Den", "jun\u00b7gen", "klei\u00b7nen", "A\u00b7mor", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NE", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Regenguss, der eimerweise", "tokens": ["Ein