{"dta.poem.10259": {"metadata": {"author": {"name": "Liliencron, Detlev von", "birth": "N.A.", "death": "N.A."}, "title": "Unheimlicher Teich.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1883", "urn": "urn:nbn:de:kobv:b4-200905197184", "language": ["de:0.99"], "booktitle": "Liliencron, Detlev von: Adjutantenritte und andere Gedichte. Leipzig, [1883]."}, "poem": {"stanza.1": {"line.1": {"text": "Zwei krause verkr&#252;ppelte Zwergeichen,             ", "tokens": ["Zwei", "krau\u00b7se", "ver\u00b7kr", "&#252;", "ppel\u00b7te", "Zwer\u00b7gei\u00b7chen", ","], "token_info": ["word", "word", "word", "XML_entity", "word", "word", "punct"], "pos": ["CARD", "NN", "NE", "$(", "ADJA", "NN", "$,"], "meter": "-+--+--++-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Weidengestr\u00fcpp, Feldsteine, und", "tokens": ["Wei\u00b7den\u00b7ge\u00b7str\u00fcpp", ",", "Feld\u00b7stei\u00b7ne", ",", "und"], "token_info": ["word", "punct", "word", "punct", "word"], "pos": ["NE", "$,", "NN", "$,", "KON"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Ein alter, weggeworfener, zerrissener,", "tokens": ["Ein", "al\u00b7ter", ",", "weg\u00b7ge\u00b7wor\u00b7fe\u00b7ner", ",", "zer\u00b7ris\u00b7se\u00b7ner", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "$,", "ADJA", "$,"], "meter": "-+-+-+---+--", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Halbverfaulter, verlassener Stiefel.", "tokens": ["Halb\u00b7ver\u00b7faul\u00b7ter", ",", "ver\u00b7las\u00b7se\u00b7ner", "Stie\u00b7fel", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Im Schilf l\u00e4rmt der Rohrspatz", "tokens": ["Im", "Schilf", "l\u00e4rmt", "der", "Rohrs\u00b7patz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN"], "meter": "-+--++", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "In weiter Stille.", "tokens": ["In", "wei\u00b7ter", "Stil\u00b7le", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADV", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Langsam auf Brachfeld und Moor welkt der Tag,", "tokens": ["Lang\u00b7sam", "auf", "Brach\u00b7feld", "und", "Moor", "welkt", "der", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "KON", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.2": {"text": "Und bla\u00df zwinkern drei, vier Sterne,", "tokens": ["Und", "bla\u00df", "zwin\u00b7kern", "drei", ",", "vier", "Ster\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "CARD", "$,", "CARD", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wie K\u00e4tzchen\u00e4ugelchen, die zum ersten Mal in die", "tokens": ["Wie", "K\u00e4tz\u00b7che\u00b7n\u00e4u\u00b7ge\u00b7lchen", ",", "die", "zum", "ers\u00b7ten", "Mal", "in", "die"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "NE", "$,", "PRELS", "APPRART", "ADJA", "NN", "APPR", "ART"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Es schweigt der Wind.", "tokens": ["Es", "schweigt", "der", "Wind", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Eine Kuh br\u00fcllt auf fernen Feldern", "tokens": ["Ei\u00b7ne", "Kuh", "br\u00fcllt", "auf", "fer\u00b7nen", "Fel\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "In weiter Stille.", "tokens": ["In", "wei\u00b7ter", "Stil\u00b7le", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADV", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Still und einsam.", "tokens": ["Still", "und", "ein\u00b7sam", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.4": {"line.1": {"text": "Aus der schwarzen Wasserlache", "tokens": ["Aus", "der", "schwar\u00b7zen", "Was\u00b7ser\u00b7la\u00b7che"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Steigt in lang wei\u00dfem Gewand ein Priester.", "tokens": ["Steigt", "in", "lang", "wei\u00b7\u00dfem", "Ge\u00b7wand", "ein", "Pries\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJD", "ADJA", "NN", "ART", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.3": {"text": "Und in seiner Hand, hoch dem Haupte,", "tokens": ["Und", "in", "sei\u00b7ner", "Hand", ",", "hoch", "dem", "Haup\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "NN", "$,", "ADJD", "ART", "NN", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Gl\u00e4nzt die Monstranz.", "tokens": ["Gl\u00e4nzt", "die", "Mons\u00b7tranz", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.5": {"line.1": {"text": "Die Monstranz?", "tokens": ["Die", "Mons\u00b7tranz", "?"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.6": {"line.1": {"text": "Vor zweihundert und etlichen Jahren", "tokens": ["Vor", "zwei\u00b7hun\u00b7dert", "und", "et\u00b7li\u00b7chen", "Jah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "CARD", "KON", "ADJA", "NN"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Sind die Schweden durchs Land gefahren,", "tokens": ["Sind", "die", "Schwe\u00b7den", "durchs", "Land", "ge\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NE", "APPRART", "NN", "VVPP", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Und ein w\u00fcster Blondgesell", "tokens": ["Und", "ein", "w\u00fcs\u00b7ter", "Blond\u00b7ge\u00b7sell"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.4": {"text": "Stahl aus der Kirche das Heiligste schnell", "tokens": ["Stahl", "aus", "der", "Kir\u00b7che", "das", "Hei\u00b7ligs\u00b7te", "schnell"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "ART", "ADJA", "NN"], "meter": "+--+---+-+", "measure": "dactylic.di.plus"}, "line.5": {"text": "Und steckt in den Sack das St\u00fcck.", "tokens": ["Und", "steckt", "in", "den", "Sack", "das", "St\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Doch hinter ihm her kam der Priester gerannt,", "tokens": ["Doch", "hin\u00b7ter", "ihm", "her", "kam", "der", "Pries\u00b7ter", "ge\u00b7rannt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "APZR", "VVFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.7": {"text": "Ein junger, tapferer Pr\u00e4dikant,", "tokens": ["Ein", "jun\u00b7ger", ",", "tap\u00b7fe\u00b7rer", "Pr\u00e4\u00b7di\u00b7kant", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Und k\u00e4mpft es zur\u00fcck.", "tokens": ["Und", "k\u00e4mpft", "es", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.7": {"line.1": {"text": "Aber wehe, o weh,", "tokens": ["A\u00b7ber", "we\u00b7he", ",", "o", "weh", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "FM", "PTKVZ", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Hinterm Busch im Klee,", "tokens": ["Hin\u00b7term", "Busch", "im", "Klee", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Lag des Schweden Kamerad,", "tokens": ["Lag", "des", "Schwe\u00b7den", "Ka\u00b7me\u00b7rad", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Von Axel Cederstolpe\u2019s Dragonern, Sven Grath", "tokens": ["Von", "A\u00b7xel", "Ce\u00b7der\u00b7stol\u00b7pe's", "Dra\u00b7go\u00b7nern", ",", "Sven", "Grath"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "NE", "NE", "NN", "$,", "ADJA", "NN"], "meter": "-+-+-+-++--+", "measure": "iambic.hexa.chol"}, "line.5": {"text": "Die beiden schlugen den Priester tot,", "tokens": ["Die", "bei\u00b7den", "schlu\u00b7gen", "den", "Pries\u00b7ter", "tot", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "ADJA", "ART", "NN", "ADJD", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Der hat in seiner letzten Not", "tokens": ["Der", "hat", "in", "sei\u00b7ner", "letz\u00b7ten", "Not"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Das Hostiengef\u00e4\u00df gehalten,", "tokens": ["Das", "Hos\u00b7ti\u00b7en\u00b7ge\u00b7f\u00e4\u00df", "ge\u00b7hal\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Da\u00df sich die Finger krallten in Wachs \u2026", "tokens": ["Da\u00df", "sich", "die", "Fin\u00b7ger", "krall\u00b7ten", "in", "Wachs", "\u2026"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "VVFIN", "APPR", "NN", "$("], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.9": {"text": "\u2026 und sie warfen ihn ins Loch.", "tokens": ["\u2026", "und", "sie", "war\u00b7fen", "ihn", "ins", "Loch", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "PPER", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Allabendlich doch,", "tokens": ["Al\u00b7la\u00b7bend\u00b7lich", "doch", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "ADV", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Wenn das letzte Rot verschwommen,", "tokens": ["Wenn", "das", "letz\u00b7te", "Rot", "ver\u00b7schwom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und die ersten Sterne kommen,", "tokens": ["Und", "die", "ers\u00b7ten", "Ster\u00b7ne", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Steht er tieftraurig auf dem Teiche.", "tokens": ["Steht", "er", "tief\u00b7trau\u00b7rig", "auf", "dem", "Tei\u00b7che", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "APPR", "ART", "NN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}}, "stanza.9": {"line.1": {"text": "Gestern kam der alte Kuhhirte Hans", "tokens": ["Ge\u00b7stern", "kam", "der", "al\u00b7te", "Kuh\u00b7hir\u00b7te", "Hans"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN", "NE"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Vom Jahrmarkt etwas schwer des Weges daher,", "tokens": ["Vom", "Jahr\u00b7markt", "et\u00b7was", "schwer", "des", "We\u00b7ges", "da\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "ADJD", "ART", "NN", "PAV", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Der sah den Priester und die Monstranz.", "tokens": ["Der", "sah", "den", "Pries\u00b7ter", "und", "die", "Mons\u00b7tranz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den alten Hans fanden wir heute Morgen", "tokens": ["Den", "al\u00b7ten", "Hans", "fan\u00b7den", "wir", "heu\u00b7te", "Mor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NE", "VVFIN", "PPER", "ADV", "NN"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Als Leiche.", "tokens": ["Als", "Lei\u00b7che", "."], "token_info": ["word", "word", "punct"], "pos": ["KOUS", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}}}}}