{"textgrid.poem.57905": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Das Beste daran", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Bild von unserm neuen Rathaus", "tokens": ["Ein", "Bild", "von", "un\u00b7serm", "neu\u00b7en", "Rat\u00b7haus"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ist in der Kunsthalle ausgestellt;", "tokens": ["Ist", "in", "der", "Kunst\u00b7hal\u00b7le", "aus\u00b7ge\u00b7stellt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ich hab's gesehn; nun raten Sie mal,", "tokens": ["Ich", "hab's", "ge\u00b7sehn", ";", "nun", "ra\u00b7ten", "Sie", "mal", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$.", "ADV", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Was mir am besten dran gef\u00e4llt?", "tokens": ["Was", "mir", "am", "bes\u00b7ten", "dran", "ge\u00b7f\u00e4llt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PTKA", "ADJD", "PAV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "\u00bbdie Kuppel?\u00ab Nee, darauf verzicht' ich;", "tokens": ["\u00bb", "die", "Kup\u00b7pel", "?", "\u00ab", "Nee", ",", "da\u00b7rauf", "ver\u00b7zicht'", "ich", ";"], "token_info": ["punct", "word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "$.", "$(", "NN", "$,", "PAV", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wir sind ja nicht im Orient.", "tokens": ["Wir", "sind", "ja", "nicht", "im", "O\u00b7rient", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PTKNEG", "APPRART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Jawohl, ich h\u00e4tte nichts dagegen,", "tokens": ["Ja\u00b7wohl", ",", "ich", "h\u00e4t\u00b7te", "nichts", "da\u00b7ge\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "VAFIN", "PIS", "PAV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn es im fernen Bagdad st\u00e4nd.", "tokens": ["Wenn", "es", "im", "fer\u00b7nen", "Bag\u00b7dad", "st\u00e4nd", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "\u00bbdas Ganze?\u00ab Nee. \u00bbUnd die Detailchens?\u00ab", "tokens": ["\u00bb", "das", "Gan\u00b7ze", "?", "\u00ab", "Nee", ".", "\u00bb", "Und", "die", "De\u00b7tail\u00b7chens", "?", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "punct", "word", "punct", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ART", "NN", "$.", "$(", "NN", "$.", "$(", "KON", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Auch nicht. \u00bbDas kolossive Dach?\u00ab", "tokens": ["Auch", "nicht", ".", "\u00bb", "Das", "ko\u00b7los\u00b7si\u00b7ve", "Dach", "?", "\u00ab"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "PTKNEG", "$.", "$(", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Erst recht nicht, und Sie finden's nimmer,", "tok