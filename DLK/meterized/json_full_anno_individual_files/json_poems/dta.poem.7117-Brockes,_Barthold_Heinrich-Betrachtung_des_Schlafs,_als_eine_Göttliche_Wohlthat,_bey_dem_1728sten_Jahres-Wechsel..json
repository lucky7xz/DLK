{"dta.poem.7117": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Betrachtung des Schlafs,  \n als  \n eine G\u00f6ttliche Wohlthat,  \n bey dem 1728sten Jahres-Wechsel.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Sie scheinet ferner noch f\u00fcr jedermann,", "tokens": ["Sie", "schei\u00b7net", "fer\u00b7ner", "noch", "f\u00fcr", "je\u00b7der\u00b7mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "APPR", "PIS", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "So Pfad, als Weg\u2019 und Stege zu verstecken,", "tokens": ["So", "Pfad", ",", "als", "Weg'", "und", "Ste\u00b7ge", "zu", "ver\u00b7ste\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "$,", "KOUS", "NN", "KON", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df man nicht gehn noch wandeln kan.", "tokens": ["Da\u00df", "man", "nicht", "gehn", "noch", "wan\u00b7deln", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PTKNEG", "VVFIN", "ADV", "VVFIN", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}