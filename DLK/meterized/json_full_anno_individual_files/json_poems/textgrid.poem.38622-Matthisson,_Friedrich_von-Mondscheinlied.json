{"textgrid.poem.38622": {"metadata": {"author": {"name": "Matthisson, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Mondscheinlied", "genre": "verse", "period": "N.A.", "pub_year": 1789, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Vollmond schwebt in Osten;", "tokens": ["Der", "Voll\u00b7mond", "schwebt", "in", "Os\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Am alten Geisterthurm", "tokens": ["Am", "al\u00b7ten", "Geis\u00b7ter\u00b7thurm"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Flimmt bl\u00e4ulich im bemoosten", "tokens": ["Flimmt", "bl\u00e4u\u00b7lich", "im", "be\u00b7moos\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "APPRART", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Gestein der Feuerwurm.", "tokens": ["Ge\u00b7stein", "der", "Feu\u00b7er\u00b7wurm", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der Linde sch\u00f6ner Sylphe", "tokens": ["Der", "Lin\u00b7de", "sch\u00f6\u00b7ner", "Syl\u00b7phe"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NE", "ADJA", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "Streift scheu in Lunens Glanz,", "tokens": ["Streift", "scheu", "in", "Lu\u00b7nens", "Glanz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "NE", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Im dunklen Uferschilfe", "tokens": ["Im", "dunk\u00b7len", "U\u00b7fer\u00b7schil\u00b7fe"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Webt leichter Irrwischtanz.", "tokens": ["Webt", "leich\u00b7ter", "Irr\u00b7wischtanz", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Die Kirchenfenster schimmern;", "tokens": ["Die", "Kir\u00b7chen\u00b7fens\u00b7ter", "schim\u00b7mern", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In Silber wallt das Korn;", "tokens": ["In", "Sil\u00b7ber", "wallt", "das", "Korn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Bewegte Sternchen flimmern", "tokens": ["Be\u00b7weg\u00b7te", "Stern\u00b7chen", "flim\u00b7mern"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Auf Teich und Wiesenborn;", "tokens": ["Auf", "Teich", "und", "Wie\u00b7sen\u00b7born", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Im Lichte wehn die Ranken", "tokens": ["Im", "Lich\u00b7te", "wehn", "die", "Ran\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der \u00f6den Felsenkluft;", "tokens": ["Der", "\u00f6\u00b7den", "Fel\u00b7sen\u00b7kluft", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Den Berg, wo Tannen wanken,", "tokens": ["Den", "Berg", ",", "wo", "Tan\u00b7nen", "wan\u00b7ken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Umschleiert weisser Duft.", "tokens": ["Um\u00b7schlei\u00b7ert", "weis\u00b7ser", "Duft", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Wie sch\u00f6n der Mond die Wellen", "tokens": ["Wie", "sch\u00f6n", "der", "Mond", "die", "Wel\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Des Erlenbachs bes\u00e4umt,", "tokens": ["Des", "Er\u00b7len\u00b7bachs", "be\u00b7s\u00e4umt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der hier durch Binsenstellen,", "tokens": ["Der", "hier", "durch", "Bin\u00b7sen\u00b7stel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dort unter Blumen sch\u00e4umt,", "tokens": ["Dort", "un\u00b7ter", "Blu\u00b7men", "sch\u00e4umt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Als lodernde Kaskade", "tokens": ["Als", "lo\u00b7dern\u00b7de", "Kas\u00b7ka\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "ADJA", "NN"], "meter": "-+---+-", "measure": "dactylic.init"}, "line.6": {"text": "Des Dorfes M\u00fchle treibt,", "tokens": ["Des", "Dor\u00b7fes", "M\u00fch\u00b7le", "treibt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und wild vom lauten Rade", "tokens": ["Und", "wild", "vom", "lau\u00b7ten", "Ra\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "In Silberfunken st\u00e4ubt.", "tokens": ["In", "Sil\u00b7ber\u00b7fun\u00b7ken", "st\u00e4ubt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Die Pappelweide zittert,", "tokens": ["Die", "Pap\u00b7pel\u00b7wei\u00b7de", "zit\u00b7tert", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Nun d\u00e4mmernd, nun umblinkt,", "tokens": ["Nun", "d\u00e4m\u00b7mernd", ",", "nun", "um\u00b7blinkt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVPP", "$,", "ADV", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wo von Jesmin umgittert", "tokens": ["Wo", "von", "Jes\u00b7min", "um\u00b7git\u00b7tert"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPR", "NE", "VVPP"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Die Sommerlaube winkt,", "tokens": ["Die", "Som\u00b7mer\u00b7lau\u00b7be", "winkt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und mit geflochtnem Pf\u00f6rtchen,", "tokens": ["Und", "mit", "ge\u00b7flocht\u00b7nem", "Pf\u00f6rt\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Das auf den Weiher sieht,", "tokens": ["Das", "auf", "den", "Wei\u00b7her", "sieht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ein l\u00e4ndlich stilles G\u00e4rtchen", "tokens": ["Ein", "l\u00e4nd\u00b7lich", "stil\u00b7les", "G\u00e4rt\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die Fischerh\u00fctt\u2019 umbl\u00fcht.", "tokens": ["Die", "Fi\u00b7scher\u00b7h\u00fctt'", "um\u00b7bl\u00fcht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Durch Fichten senkt der Schimmer,", "tokens": ["Durch", "Fich\u00b7ten", "senkt", "der", "Schim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So bleich und schauerlich,", "tokens": ["So", "bleich", "und", "schau\u00b7er\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Auf die beb\u00fcschten Tr\u00fcmmer", "tokens": ["Auf", "die", "be\u00b7b\u00fcschten", "Tr\u00fcm\u00b7mer"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Der Wasserleitung sich,", "tokens": ["Der", "Was\u00b7ser\u00b7lei\u00b7tung", "sich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Bestralt die d\u00fcstern Eiben", "tokens": ["Be\u00b7stralt", "die", "d\u00fcs\u00b7tern", "Ei\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der kleinen Meierei,", "tokens": ["Der", "klei\u00b7nen", "Mei\u00b7e\u00b7rei", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und hellt die bunten Scheiben", "tokens": ["Und", "hellt", "die", "bun\u00b7ten", "Schei\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Der gothischen Abtei.", "tokens": ["Der", "go\u00b7thi\u00b7schen", "Ab\u00b7tei", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "--+-+-", "measure": "anapaest.init"}}, "stanza.6": {"line.1": {"text": "Wie sanft verschmilzt der blassen", "tokens": ["Wie", "sanft", "ver\u00b7schmilzt", "der", "blas\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "VVFIN", "ART", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Beleuchtung Zauberschein", "tokens": ["Be\u00b7leuch\u00b7tung", "Zau\u00b7ber\u00b7schein"], "token_info": ["word", "word"], "pos": ["NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die ungeheuren Massen", "tokens": ["Die", "un\u00b7ge\u00b7heu\u00b7ren", "Mas\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Gezackter Felsenreih'n,", "tokens": ["Ge\u00b7zack\u00b7ter", "Fel\u00b7sen\u00b7reih'n", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Dort wo, in milder Helle,", "tokens": ["Dort", "wo", ",", "in", "mil\u00b7der", "Hel\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PWAV", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Von Immergr\u00fcn umwebt,", "tokens": ["Von", "Im\u00b7mer\u00b7gr\u00fcn", "um\u00b7webt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Die Eremitenzelle", "tokens": ["Die", "E\u00b7re\u00b7mi\u00b7ten\u00b7zel\u00b7le"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "An grauer Klippe schwebt.", "tokens": ["An", "grau\u00b7er", "Klip\u00b7pe", "schwebt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Der Elfen Heere schweifen", "tokens": ["Der", "El\u00b7fen", "Hee\u00b7re", "schwei\u00b7fen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Durch Feld und Wiesenplan,", "tokens": ["Durch", "Feld", "und", "Wie\u00b7sen\u00b7plan", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es deuten Silberstreifen", "tokens": ["Es", "deu\u00b7ten", "Sil\u00b7ber\u00b7strei\u00b7fen"], "token_info": ["word", "word", "word"], "pos": ["PPER", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dem Sch\u00e4fer ihre Bahn;", "tokens": ["Dem", "Sch\u00e4\u00b7fer", "ih\u00b7re", "Bahn", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Er wei\u00df am Purpurkreise,", "tokens": ["Er", "wei\u00df", "am", "Pur\u00b7pur\u00b7krei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Vom Wollenvieh verschm\u00e4ht,", "tokens": ["Vom", "Wol\u00b7len\u00b7vieh", "ver\u00b7schm\u00e4ht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "In welchem Blumengleise", "tokens": ["In", "wel\u00b7chem", "Blu\u00b7men\u00b7glei\u00b7se"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PWAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ihr Abendreih'n sich dreht.", "tokens": ["Ihr", "A\u00b7bendreih'n", "sich", "dreht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PRF", "VVFIN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.8": {"line.1": {"text": "Bald bergen, bald entfalten,", "tokens": ["Bald", "ber\u00b7gen", ",", "bald", "ent\u00b7fal\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVINF", "$,", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In lieblicher Magie,", "tokens": ["In", "lieb\u00b7li\u00b7cher", "Ma\u00b7gie", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sich wechselnd die Gestalten", "tokens": ["Sich", "wech\u00b7selnd", "die", "Ge\u00b7stal\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["PRF", "ADJD", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der regen Phantasie.", "tokens": ["Der", "re\u00b7gen", "Phan\u00b7ta\u00b7sie", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+--", "measure": "unknown.measure.di"}, "line.5": {"text": "Die zarten Bl\u00fcten keimen,", "tokens": ["Die", "zar\u00b7ten", "Bl\u