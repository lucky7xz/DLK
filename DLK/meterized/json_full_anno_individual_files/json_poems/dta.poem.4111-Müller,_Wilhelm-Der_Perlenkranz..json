{"dta.poem.4111": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Der Perlenkranz.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1821", "urn": "urn:nbn:de:kobv:b4-200905194209", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Gewunden um dein Haupt,", "tokens": ["Ge\u00b7wun\u00b7den", "um", "dein", "Haupt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Nicht bunt von Sommerblumen,", "tokens": ["Nicht", "bunt", "von", "Som\u00b7mer\u00b7blu\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Nicht immergr\u00fcn belaubt.", "tokens": ["Nicht", "im\u00b7mer\u00b7gr\u00fcn", "be\u00b7laubt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Von hellen, wei\u00dfen Perlen", "tokens": ["Von", "hel\u00b7len", ",", "wei\u00b7\u00dfen", "Per\u00b7len"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Soll es geflochten sein:", "tokens": ["Soll", "es", "ge\u00b7floch\u00b7ten", "sein", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVPP", "VAINF", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Durch deine schwarzen Locken", "tokens": ["Durch", "dei\u00b7ne", "schwar\u00b7zen", "Lo\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Flie\u00dft es wie Sternenschein.", "tokens": ["Flie\u00dft", "es", "wie", "Ster\u00b7nen\u00b7schein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KOKOM", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.3": {"line.1": {"text": "Neige dein Haupt, du Liebe,", "tokens": ["Nei\u00b7ge", "dein", "Haupt", ",", "du", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,", "PPER", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "L\u00f6s' auf dein langes Haar!", "tokens": ["L\u00f6s'", "auf", "dein", "lan\u00b7ges", "Haar", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Kennst du die Perlenkrone,", "tokens": ["Kennst", "du", "die", "Per\u00b7len\u00b7kro\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Durchsichtig, wasserklar?", "tokens": ["Durch\u00b7sich\u00b7tig", ",", "was\u00b7ser\u00b7klar", "?"], "token_info": ["word", "punct", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Bebt Ahnung dir im Herzen?", "tokens": ["Bebt", "Ah\u00b7nung", "dir", "im", "Her\u00b7zen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "O glaube, was sie spricht.", "tokens": ["O", "glau\u00b7be", ",", "was", "sie", "spricht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "La\u00df auf dein Haupt mich weinen:", "tokens": ["La\u00df", "auf", "dein", "Haupt", "mich", "wei\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "APPR", "PPOSAT", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Tauft denn die Thr\u00e4ne nicht?", "tokens": ["Tauft", "denn", "die", "Thr\u00e4\u00b7ne", "nicht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "PTKNEG", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}}}}