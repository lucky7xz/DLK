{"dta.poem.4264": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Mon Repos.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Laube, die ich ", "tokens": ["Die", "Lau\u00b7be", ",", "die", "ich"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "PPER"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "nen pflag,", "tokens": ["nen", "pflag", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Liegt in dem Walde rechter Hand. Von Caprifolium", "tokens": ["Liegt", "in", "dem", "Wal\u00b7de", "rech\u00b7ter", "Hand", ".", "Von", "Cap\u00b7ri\u00b7fo\u00b7li\u00b7um"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "ADJA", "NN", "$.", "APPR", "NN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "und Eichen", "tokens": ["und", "Ei\u00b7chen"], "token_info": ["word", "word"], "pos": ["KON", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "Bedecket hier, mit gr\u00fcnen Schatten, ein dicht-geflochtnes", "tokens": ["Be\u00b7de\u00b7cket", "hier", ",", "mit", "gr\u00fc\u00b7nen", "Schat\u00b7ten", ",", "ein", "dicht\u00b7ge\u00b7flocht\u00b7nes"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "ADV", "$,", "APPR", "ADJA", "NN", "$,", "ART", "ADJA"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Bl\u00e4tter-Dach,", "tokens": ["Bl\u00e4t\u00b7ter\u00b7Dach", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Ein ausgeschweiftes Rasen-B\u00e4nkchen. Die\u00df lieget, zwi-", "tokens": ["Ein", "aus\u00b7ge\u00b7schweif\u00b7tes", "Ra\u00b7sen\u00b7B\u00e4nk\u00b7chen", ".", "Die\u00df", "lie\u00b7get", ",", "zwi"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ART", "ADJA", "NN", "$.", "PDS", "VVFIN", "$,", "TRUNC"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "schen gr\u00fcnen Str\u00e4uchen,", "tokens": ["schen", "gr\u00fc\u00b7nen", "Str\u00e4u\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.9": {"text": "An einem jungen Eichen-Baum, an dessen Stamm man", "tokens": ["An", "ei\u00b7nem", "jun\u00b7gen", "Ei\u00b7chen\u00b7Baum", ",", "an", "des\u00b7sen", "Stamm", "man"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "APPR", "PRELAT", "NN", "PIS"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "sanft sich lehnen,", "tokens": ["sanft", "sich", "leh\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PRF", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.11": {"text": "Und recht bequehmlich ruhen kann, um desto mehr, als,", "tokens": ["Und", "recht", "be\u00b7quehm\u00b7lich", "ru\u00b7hen", "kann", ",", "um", "des\u00b7to", "mehr", ",", "als", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "VVINF", "VMFIN", "$,", "KOUI", "ADV", "ADV", "$,", "KOUS", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "rechter Hand,", "tokens": ["rech\u00b7ter", "Hand", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.13": {"text": "Ein ander krumm-gewachs\u2019ner Baum, den Arm bequehm", "tokens": ["Ein", "an\u00b7der", "krum\u00b7mge\u00b7wachs'\u00b7ner", "Baum", ",", "den", "Arm", "be\u00b7quehm"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJD", "ADJA", "NN", "$,", "ART", "NN", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "darauf zu dehnen,", "tokens": ["da\u00b7rauf", "zu", "deh\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PAV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.15": {"text": "Und recht gem\u00e4chlich, als ein Lehnstuhl, auf selbigem zu", "tokens": ["Und", "recht", "ge\u00b7m\u00e4ch\u00b7lich", ",", "als", "ein", "Lehn\u00b7stuhl", ",", "auf", "sel\u00b7bi\u00b7gem", "zu"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADJD", "ADJD", "$,", "KOUS", "ART", "NN", "$,", "APPR", "ADJA", "PTKZU"], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.16": {"text": "ruhen, stand.", "tokens": ["ru\u00b7hen", ",", "stand", "."], "token_info": ["word", "punct", "word", "punct"], "pos": ["VVINF", "$,", "VVFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.2": {"line.1": {"text": "Der Ast, der erst von ungefehr nach uns, denn von uns", "tokens": ["Der", "Ast", ",", "der", "erst", "von", "un\u00b7ge\u00b7fehr", "nach", "uns", ",", "denn", "von", "uns"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "APPR", "ADJD", "APPR", "PPER", "$,", "KON", "APPR", "PPER"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "weg sich strecket,", "tokens": ["weg", "sich", "stre\u00b7cket", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PRF", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Ist noch, zum Ueberflu\u00df, mit Moo\u00df, als wie ein weicher", "tokens": ["Ist", "noch", ",", "zum", "Ue\u00b7berf\u00b7lu\u00df", ",", "mit", "Moo\u00df", ",", "als", "wie", "ein", "wei\u00b7cher"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "$,", "APPRART", "NN", "$,", "APPR", "NN", "$,", "KOUS", "KOKOM", "ART", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sammt, bedecket.", "tokens": ["Sammt", ",", "be\u00b7de\u00b7cket", "."], "token_info": ["word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Zween Eich-B\u00e4um\u2019 stehen an dem Eingang, die sieht man", "tokens": ["Zween", "Eich\u00b7B\u00e4um'", "ste\u00b7hen", "an", "dem", "Ein\u00b7gang", ",", "die", "sieht", "man"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "NN", "VVFIN", "APPR", "ART", "NN", "$,", "PRELS", "VVFIN", "PIS"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "dergestalt umschl\u00e4nget", "tokens": ["der\u00b7ge\u00b7stalt", "um\u00b7schl\u00e4n\u00b7get"], "token_info": ["word", "word"], "pos": ["ADV", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Vom schlanken ", "tokens": ["Vom", "schlan\u00b7ken"], "token_info": ["word", "word"], "pos": ["APPRART", "VVINF"], "meter": "-+-", "measure": "amphibrach.single"}, "line.8": {"text": "kann,", "tokens": ["kann", ","], "token_info": ["word", "punct"], "pos": ["VMFIN", "$,"], "meter": "+", "measure": "single.up"}, "line.9": {"text": "Wie das sonst weich\u2019 und schwache Holz sich in den Stamm", "tokens": ["Wie", "das", "sonst", "weich'", "und", "schwa\u00b7che", "Holz", "sich", "in", "den", "Stamm"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PDS", "ADV", "VVFIN", "KON", "ADJA", "NN", "PRF", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "hineingedr\u00e4nget", "tokens": ["hin\u00b7ein\u00b7ge\u00b7dr\u00e4n\u00b7get"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.11": {"text": "Noch mehr als zwey Quer-Finger tief. Man sieht hier,", "tokens": ["Noch", "mehr", "als", "zwey", "Quer\u00b7Fin\u00b7ger", "tief", ".", "Man", "sieht", "hier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "KOKOM", "CARD", "NN", "ADJD", "$.", "PIS", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "mit Bewundern, an,", "tokens": ["mit", "Be\u00b7wun\u00b7dern", ",", "an", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "$,", "PTKVZ", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.13": {"text": "Wie das viel schw\u00e4chere das starke, zuweilen m\u00e4chtig, zu", "tokens": ["Wie", "das", "viel", "schw\u00e4\u00b7che\u00b7re", "das", "star\u00b7ke", ",", "zu\u00b7wei\u00b7len", "m\u00e4ch\u00b7tig", ",", "zu"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PWAV", "PDS", "ADV", "VVFIN", "ART", "ADJA", "$,", "ADV", "ADJD", "$,", "APPR"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.14": {"text": "bezwingen.", "tokens": ["be\u00b7zwin\u00b7gen", "."], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.15": {"text": "Es kam die\u00df, was ich sahe, mir,", "tokens": ["Es", "kam", "die\u00df", ",", "was", "ich", "sa\u00b7he", ",", "mir", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "PDS", "$,", "PWS", "PPER", "VVFIN", "$,", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Von Handlungen auch unter Menschen, als ein belehrend", "tokens": ["Von", "Hand\u00b7lun\u00b7gen", "auch", "un\u00b7ter", "Men\u00b7schen", ",", "als", "ein", "be\u00b7leh\u00b7rend"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "ADV", "APPR", "NN", "$,", "KOUS", "ART", "VVPP"], "meter": "-++--+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.17": {"text": "Sinnbild f\u00fcr:", "tokens": ["Sinn\u00b7bild", "f\u00fcr", ":"], "token_info": ["word", "word", "punct"], "pos": ["NN", "APPR", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.18": {"text": "Da nemlich \u00f6fters zarte Frauen selbst Helden zum Gehor-", "tokens": ["Da", "nem\u00b7lich", "\u00f6f\u00b7ters", "zar\u00b7te", "Frau\u00b7en", "selbst", "Hel\u00b7den", "zum", "Ge\u00b7hor"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ADV", "ADJA", "NN", "ADV", "NN", "APPRART", "NN"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.19": {"text": "sam bringen.", "tokens": ["sam", "brin\u00b7gen", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVINF", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.3": {"line.1": {"text": "Um mich an diesem holden Ort um desto mehr noch zu", "tokens": ["Um", "mich", "an", "die\u00b7sem", "hol\u00b7den", "Ort", "um", "des\u00b7to", "mehr", "noch", "zu"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "PRF", "APPR", "PDAT", "ADJA", "NN", "APPR", "ADV", "ADV", "ADV", "APPR"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "ergetzen,", "tokens": ["er\u00b7get\u00b7zen", ","], "token_info": ["word", "punct"], "pos": ["VVINF", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Lie\u00df ich hier einen kleinen Tisch, zum Schreiben, in der", "tokens": ["Lie\u00df", "ich", "hier", "ei\u00b7nen", "klei\u00b7nen", "Tisch", ",", "zum", "Schrei\u00b7ben", ",", "in", "der"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$,", "APPRART", "NN", "$,", "APPR", "ART"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mitte setzen,", "tokens": ["Mit\u00b7te", "set\u00b7zen", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Um, in der Einsamkeit, darauf die Sch\u00f6nheit, die wir hier", "tokens": ["Um", ",", "in", "der", "Ein\u00b7sam\u00b7keit", ",", "da\u00b7rauf", "die", "Sch\u00f6n\u00b7heit", ",", "die", "wir", "hier"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUI", "$,", "APPR", "ART", "NN", "$,", "PAV", "ART", "NN", "$,", "PRELS", "PPER", "ADV"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.6": {"text": "erblicken,", "tokens": ["er\u00b7bli\u00b7cken", ","], "token_info": ["word", "punct"], "pos": ["VVINF", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "Mit Lust und Andacht, abzuschildern, und, GOtt zu Ehren,", "tokens": ["Mit", "Lust", "und", "An\u00b7dacht", ",", "ab\u00b7zu\u00b7schil\u00b7dern", ",", "und", ",", "Gott", "zu", "Eh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,", "VVIZU", "$,", "KON", "$,", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "auszudr\u00fccken.", "tokens": ["aus\u00b7zu\u00b7dr\u00fc\u00b7cken", "."], "token_info": ["word", "punct"], "pos": ["VVIZU", "$."], "meter": "+-+-", "measure": "trochaic.di"}}}}}