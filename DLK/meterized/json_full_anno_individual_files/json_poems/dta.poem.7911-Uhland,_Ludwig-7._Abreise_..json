{"dta.poem.7911": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "7.  Abreise .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.99"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "So hab\u2019 ich nun die Stadt verlassen,", "tokens": ["So", "hab'", "ich", "nun", "die", "Stadt", "ver\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wo ich gelebet lange Zeit;", "tokens": ["Wo", "ich", "ge\u00b7le\u00b7bet", "lan\u00b7ge", "Zeit", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVPP", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich ziehe r\u00fcstig meiner Stra\u00dfen,", "tokens": ["Ich", "zie\u00b7he", "r\u00fcs\u00b7tig", "mei\u00b7ner", "Stra\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Es gibt mir Niemand das Geleit.", "tokens": ["Es", "gibt", "mir", "Nie\u00b7mand", "das", "Ge\u00b7leit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIS", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Man hat mir nicht den Rock zerrissen,", "tokens": ["Man", "hat", "mir", "nicht", "den", "Rock", "zer\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PPER", "PTKNEG", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Es w\u00e4r\u2019 auch Schade f\u00fcr das Kleid!", "tokens": ["Es", "w\u00e4r'", "auch", "Scha\u00b7de", "f\u00fcr", "das", "Kleid", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Noch in die Wange mich gebissen", "tokens": ["Noch", "in", "die", "Wan\u00b7ge", "mich", "ge\u00b7bis\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "PPER", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Vor \u00fcbergro\u00dfem Herzeleid.", "tokens": ["Vor", "\u00fc\u00b7ber\u00b7gro\u00b7\u00dfem", "Her\u00b7ze\u00b7leid", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Auch Keinem hat\u2019s den Schlaf vertrieben,", "tokens": ["Auch", "Kei\u00b7nem", "hat's", "den", "Schlaf", "ver\u00b7trie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df ich am Morgen weiter geh\u2019;", "tokens": ["Da\u00df", "ich", "am", "Mor\u00b7gen", "wei\u00b7ter", "geh'", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie konnten\u2019s halten nach Belieben;", "tokens": ["Sie", "konn\u00b7ten's", "hal\u00b7ten", "nach", "Be\u00b7lie\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Von Einer aber thut mir\u2019s weh.", "tokens": ["Von", "Ei\u00b7ner", "a\u00b7ber", "thut", "mir's", "weh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "ADV", "VVFIN", "NE", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}