{"textgrid.poem.45926": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "[du Herrlicher! den kaum die Zeit erkannt]", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du Herrlicher! den kaum die Zeit erkannt,", "tokens": ["Du", "Herr\u00b7li\u00b7cher", "!", "den", "kaum", "die", "Zeit", "er\u00b7kannt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$.", "ART", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der wie ein schuldlos Kind", "tokens": ["Der", "wie", "ein", "schuld\u00b7los", "Kind"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "KOKOM", "ART", "ADJD", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Begeistert fromm die treue keusche Hand", "tokens": ["Be\u00b7geis\u00b7tert", "fromm", "die", "treu\u00b7e", "keu\u00b7sche", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Nach Gottes Flamme streckte,", "tokens": ["Nach", "Got\u00b7tes", "Flam\u00b7me", "streck\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der f\u00fcr das Eitle blind", "tokens": ["Der", "f\u00fcr", "das", "Eit\u00b7le", "blind"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Ohn' umzuschaun zur Wiege alter Kunst", "tokens": ["Ohn'", "um\u00b7zu\u00b7schaun", "zur", "Wie\u00b7ge", "al\u00b7ter", "Kunst"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVIZU", "APPRART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Durch neuer L\u00fcge G\u00f6tzentempel drang,", "tokens": ["Durch", "neu\u00b7er", "L\u00fc\u00b7ge", "G\u00f6t\u00b7zen\u00b7tem\u00b7pel", "drang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Und stillanschaund die G\u00f6ttliche erweckte.", "tokens": ["Und", "stil\u00b7lan\u00b7schaund", "die", "G\u00f6tt\u00b7li\u00b7che", "er\u00b7weck\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Sie l\u00e4chelte und nannte dich den Ihren,", "tokens": ["Sie", "l\u00e4\u00b7chel\u00b7te", "und", "nann\u00b7te", "dich", "den", "Ih\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Der ihr die ird'schen Kr\u00e4nze so bedeutend schlang", "tokens": ["Der", "ihr", "die", "ird'\u00b7schen", "Kr\u00e4n\u00b7ze", "so", "be\u00b7deu\u00b7tend", "schlang"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ART", "ADJA", "NN", "ADV", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Und wollte dich, mit ihr zu triumphieren", "tokens": ["Und", "woll\u00b7te", "dich", ",", "mit", "ihr", "zu", "tri\u00b7um\u00b7phie\u00b7ren"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "$,", "APPR", "PPER", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Zum sel'gen Born von allem Lichte f\u00fchren.", "tokens": ["Zum", "sel'\u00b7gen", "Born", "von", "al\u00b7lem", "Lich\u00b7te", "f\u00fch\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "APPR", "PIS", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wer dich geliebt, verstand den sch\u00f6nen Traum,", "tokens": ["Wer", "dich", "ge\u00b7liebt", ",", "ver\u00b7stand", "den", "sch\u00f6\u00b7nen", "Traum", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "$,", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Den du im Himmel tr\u00e4umtest, dessen Schatten", "tokens": ["Den", "du", "im", "Him\u00b7mel", "tr\u00e4um\u00b7test", ",", "des\u00b7sen", "Schat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "PPER", "APPRART", "NN", "VVFIN", "$,", "PRELAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Auf unsrer dunklen Erde lichten Saum", "tokens": ["Auf", "uns\u00b7rer", "dunk\u00b7len", "Er\u00b7de", "lich\u00b7ten", "Saum"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Weissagend niederfiel. \u2013", "tokens": ["Weis\u00b7sa\u00b7gend", "nie\u00b7der\u00b7fiel", ".", "\u2013"], "token_info": ["word", "word", "punct", "punct"], "pos": ["ADJD", "VVFIN", "$.", "$("], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Dein K\u00fcnstlerwerk, es schien ein zierlich Spiel,", "tokens": ["Dein", "K\u00fcnst\u00b7ler\u00b7werk", ",", "es", "schien", "ein", "zier\u00b7lich", "Spiel", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPER", "VVFIN", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Es rankte blumig auf und betend vor der Sonne", "tokens": ["Es", "rank\u00b7te", "blu\u00b7mig", "auf", "und", "be\u00b7tend", "vor", "der", "Son\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "PTKVZ", "KON", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Setzt fromme Kindlein du in s\u00fc\u00dfer Kelche Wonne;", "tokens": ["Setzt", "from\u00b7me", "Kin\u00b7dlein", "du", "in", "s\u00fc\u00b7\u00dfer", "Kel\u00b7che", "Won\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "PPER", "APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Doch wie im Fr\u00fchlingstaumel fromm ein Herz", "tokens": ["Doch", "wie", "im", "Fr\u00fch\u00b7lings\u00b7tau\u00b7mel", "fromm", "ein", "Herz"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "APPRART", "NN", "ADJD", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Das Siegsgepr\u00e4ng' des ew'gen Gottes liest,", "tokens": ["Das", "Siegs\u00b7ge\u00b7pr\u00e4ng'", "des", "ew'\u00b7gen", "Got\u00b7tes", "liest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Wie in des Lebens ernstem Blumenscherz", "tokens": ["Wie", "in", "des", "Le\u00b7bens", "erns\u00b7tem", "Blu\u00b7men\u00b7scherz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Dem Schauenden die Tiefe sich erschlie\u00dft,", "tokens": ["Dem", "Schau\u00b7en\u00b7den", "die", "Tie\u00b7fe", "sich", "er\u00b7schlie\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "So steht, die Schwester dieser s\u00fcndentrunknen Zeit,", "tokens": ["So", "steht", ",", "die", "Schwes\u00b7ter", "die\u00b7ser", "s\u00fcn\u00b7den\u00b7trunk\u00b7nen", "Zeit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "ART", "NN", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Vor deinen Bildern glaubend, hoffend, liebend, die Beschaulichkeit.", "tokens": ["Vor", "dei\u00b7nen", "Bil\u00b7dern", "glau\u00b7bend", ",", "hof\u00b7fend", ",", "lie\u00b7bend", ",", "die", "Be\u00b7schau\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$,", "VVPP", "$,", "ADJD", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}}, "stanza.3": {"line.1": {"text": "O trauert nicht um seinen fr\u00fchen Tod!", "tokens": ["O", "trau\u00b7ert", "nicht", "um", "sei\u00b7nen", "fr\u00fc\u00b7hen", "Tod", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PTKNEG", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Er lebte nicht, er war ein Morgenrot,", "tokens": ["Er", "leb\u00b7te", "nicht", ",", "er", "war", "ein", "Mor\u00b7gen\u00b7rot", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PPER", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Das in der Zeiten trauriger Verwirrung", "tokens": ["Das", "in", "der", "Zei\u00b7ten", "trau\u00b7ri\u00b7ger", "Ver\u00b7wir\u00b7rung"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zu fr\u00fch uns guter Tage Hoffnung bot,", "tokens": ["Zu", "fr\u00fch", "uns", "gu\u00b7ter", "Ta\u00b7ge", "Hoff\u00b7nung", "bot", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "PPER", "ADJA", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wer dieser Bl\u00fcte Fr\u00fcchte konnte ahnen,", "tokens": ["Wer", "die\u00b7ser", "Bl\u00fc\u00b7te", "Fr\u00fcch\u00b7te", "konn\u00b7te", "ah\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PDAT", "NN", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Der mu\u00dfte tief bewu\u00dft der eigenen Verirrung,", "tokens": ["Der", "mu\u00df\u00b7te", "tief", "be\u00b7wu\u00dft", "der", "ei\u00b7ge\u00b7nen", "Ver\u00b7ir\u00b7rung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ADJD", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Der eignen Armut sich besch\u00e4mend mahnen;", "tokens": ["Der", "eig\u00b7nen", "Ar\u00b7mut", "sich", "be\u00b7sch\u00e4\u00b7mend", "mah\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PRF", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "So mu\u00dft' auch ich, wenn ich sein Werk durchdachte,", "tokens": ["So", "mu\u00dft'", "auch", "ich", ",", "wenn", "ich", "sein", "Werk", "durch\u00b7dach\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ADV", "PPER", "$,", "KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+--+-++-+-", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Das wie ein Gottentz\u00fcckter selig lachte,", "tokens": ["Das", "wie", "ein", "Got\u00b7tent\u00b7z\u00fcck\u00b7ter", "se\u00b7lig", "lach\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "KOKOM", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Zu mir, bewegt in ernster Demut sagen:", "tokens": ["Zu", "mir", ",", "be\u00b7wegt", "in", "erns\u00b7ter", "De\u00b7mut", "sa\u00b7gen", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "VVFIN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Wie sollen die Vollendung wir ertragen?", "tokens": ["Wie", "sol\u00b7len", "die", "Vol\u00b7len\u00b7dung", "wir", "er\u00b7tra\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "ART", "NN", "PPER", "VVINF", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.12": {"text": "Und auf dem Babylon rings sah ich ragen,", "tokens": ["Und", "auf", "dem", "Ba\u00b7by\u00b7lon", "rings", "sah", "ich", "ra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NE", "ADV", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Die Kreuze frech, den Helden dran zu schlagen.", "tokens": ["Die", "Kreu\u00b7ze", "frech", ",", "den", "Hel\u00b7den", "dran", "zu", "schla\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$,", "ART", "NN", "PAV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "O trauert nicht um seinen fr\u00fchen Tod!", "tokens": ["O", "trau\u00b7ert", "nicht", "um", "sei\u00b7nen", "fr\u00fc\u00b7hen", "Tod", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PTKNEG", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Er lebte nicht, er war ein Abendrot,", "tokens": ["Er", "leb\u00b7te", "nicht", ",", "er", "war", "ein", "A\u00b7ben\u00b7drot", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PPER", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Versp\u00e4tet aus verlornen Paradiesen", "tokens": ["Ver\u00b7sp\u00e4\u00b7tet", "aus", "ver\u00b7lor\u00b7nen", "Pa\u00b7ra\u00b7die\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Lie\u00df t\u00e4uschend es in unsrer N\u00e4chte Not", "tokens": ["Lie\u00df", "t\u00e4u\u00b7schend", "es", "in", "uns\u00b7rer", "N\u00e4ch\u00b7te", "Not"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "PPER", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Die ahndungsreichen Schimmer flie\u00dfen.", "tokens": ["Die", "ahn\u00b7dungs\u00b7rei\u00b7chen", "Schim\u00b7mer", "flie\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Und wer an seinem Grabe eine Nacht", "tokens": ["Und", "wer", "an", "sei\u00b7nem", "Gra\u00b7be", "ei\u00b7ne", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "APPR", "PPOSAT", "NN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "In Tr\u00e4nen harrt, bis da\u00df der Tag erwacht,", "tokens": ["In", "Tr\u00e4\u00b7nen", "harrt", ",", "bis", "da\u00df", "der", "Tag", "er\u00b7wacht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$,", "KON", "KOUS", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Den seines Lebens Morgenstern verhie\u00df,", "tokens": ["Den", "sei\u00b7nes", "Le\u00b7bens", "Mor\u00b7gens\u00b7tern", "ver\u00b7hie\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+---+", "measure": "zehnsilber"}, "line.4": {"text": "Der wird, ist er ein Kind, den Morgen kaum erleben,", "tokens": ["Der", "wird", ",", "ist", "er", "ein", "Kind", ",", "den", "Mor\u00b7gen", "kaum", "er\u00b7le\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "$,", "VAFIN", "PPER", "ART", "NN", "$,", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ist er ein frommer Mann, mit ihm, der uns verlie\u00df,", "tokens": ["Ist", "er", "ein", "from\u00b7mer", "Mann", ",", "mit", "ihm", ",", "der", "uns", "ver\u00b7lie\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "N