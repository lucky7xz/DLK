{"textgrid.poem.60654": {"metadata": {"author": {"name": "La Fontaine, Jean de", "birth": "N.A.", "death": "N.A."}, "title": "1L: Von dem, was ihm die eigne Herde trug \u2013", "genre": "verse", "period": "N.A.", "pub_year": 1658, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Von dem, was ihm die eigne Herde trug \u2013", "tokens": ["Von", "dem", ",", "was", "ihm", "die", "eig\u00b7ne", "Her\u00b7de", "trug", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$,", "PWS", "PPER", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Es war nicht viel, doch f\u00fcr Bescheidenheit genug \u2013", "tokens": ["Es", "war", "nicht", "viel", ",", "doch", "f\u00fcr", "Be\u00b7schei\u00b7den\u00b7heit", "ge\u00b7nug", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADV", "$,", "ADV", "APPR", "NN", "ADV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Hat viele Jahre lang und ohne Sorgenplage", "tokens": ["Hat", "vie\u00b7le", "Jah\u00b7re", "lang", "und", "oh\u00b7ne", "Sor\u00b7gen\u00b7pla\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PIAT", "NN", "ADJD", "KON", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ein Hirt gelebt am Meeresstrand.", "tokens": ["Ein", "Hirt", "ge\u00b7lebt", "am", "Mee\u00b7res\u00b7strand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Doch leider sah er dort fast alle Tage", "tokens": ["Doch", "lei\u00b7der", "sah", "er", "dort", "fast", "al\u00b7le", "Ta\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADV", "ADV", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wertvolle Fracht und Sch\u00e4tze allerhand", "tokens": ["Wert\u00b7vol\u00b7le", "Fracht", "und", "Sch\u00e4t\u00b7ze", "al\u00b7ler\u00b7hand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "KON", "NN", "PIAT"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.7": {"text": "Verladen, was ihn schlie\u00dflich trieb,", "tokens": ["Ver\u00b7la\u00b7den", ",", "was", "ihn", "schlie\u00df\u00b7lich", "trieb", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Sein Vieh zu Geld zu machen", "tokens": ["Sein", "Vieh", "zu", "Geld", "zu", "ma\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "APPR", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Und dies in Seegesch\u00e4ften anzulegen.", "tokens": ["Und", "dies", "in", "See\u00b7ge\u00b7sch\u00e4f\u00b7ten", "an\u00b7zu\u00b7le\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "APPR", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wie sehr er sich damit dem Trug des Gl\u00fccks verschrieb,", "tokens": ["Wie", "sehr", "er", "sich", "da\u00b7mit", "dem", "Trug", "des", "Gl\u00fccks", "ver\u00b7schrieb", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPER", "PRF", "PAV", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Erfuhr er bald: Das Geld verschlang der Rachen", "tokens": ["Er\u00b7fuhr", "er", "bald", ":", "Das", "Geld", "ver\u00b7schlang", "der", "Ra\u00b7chen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "$.", "ART", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der See, ausblieb der Segen.", "tokens": ["Der", "See", ",", "aus\u00b7blieb", "der", "Se\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Er mu\u00dfte wieder Hirte werden.", "tokens": ["Er", "mu\u00df\u00b7te", "wie\u00b7der", "Hir\u00b7te", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "NN", "VAINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Jetzt aber nicht wie einst als Herr von eignen Herden:", "tokens": ["Jetzt", "a\u00b7ber", "nicht", "wie", "einst", "als", "Herr", "von", "eig\u00b7nen", "Her\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PTKNEG", "KOKOM", "ADV", "KOUS", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Er, der sich schon als halben Kr\u00f6sus sah,", "tokens": ["Er", ",", "der", "sich", "schon", "als", "hal\u00b7ben", "Kr\u00f6\u00b7sus", "sah", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "PRF", "ADV", "KOUS", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Stand nun als armer Schlucker da.", "tokens": ["Stand", "nun", "als", "ar\u00b7mer", "Schlu\u00b7cker", "da", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "KOUS", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Von neuem kam er schlie\u00dflich hoch, der Schippenheld,", "tokens": ["Von", "neu\u00b7em", "kam", "er", "schlie\u00df\u00b7lich", "hoch", ",", "der", "Schip\u00b7pen\u00b7held", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "VVFIN", "PPER", "ADV", "ADJD", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Und kaufte Vieh. Und als die Winde wieder", "tokens": ["Und", "kauf\u00b7te", "Vieh", ".", "Und", "als", "die", "Win\u00b7de", "wie\u00b7der"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "NN", "$.", "KON", "KOUS", "ART", "NN", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sanft atmeten und Schiffen Gl\u00fcck verhie\u00dfen,", "tokens": ["Sanft", "at\u00b7me\u00b7ten", "und", "Schif\u00b7fen", "Gl\u00fcck", "ver\u00b7hie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "KON", "NN", "NN", "VVINF", "$,"], "meter": "-+---+-+-+-", "measure": "dactylic.init"}, "line.3": {"text": "Da sprach er: \u00bbHa! ihr w\u00fcnschet neues Geld?", "tokens": ["Da", "sprach", "er", ":", "\u00bb", "Ha", "!", "ihr", "w\u00fcn\u00b7schet", "neu\u00b7es", "Geld", "?"], "token_info": ["word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$.", "$(", "ITJ", "$.", "PPER", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ihr sch\u00f6nen Wasser tut so sch\u00f6n und bieder.", "tokens": ["Ihr", "sch\u00f6\u00b7nen", "Was\u00b7ser", "tut", "so", "sch\u00f6n", "und", "bie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Doch wendet niemals mehr euch hier an diesen,", "tokens": ["Doch", "wen\u00b7det", "nie\u00b7mals", "mehr", "euch", "hier", "an", "die\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADV", "PPER", "ADV", "APPR", "PDAT", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Sucht andere Dumme, die ihr prellt!\u00ab", "tokens": ["Sucht", "an\u00b7de\u00b7re", "Dum\u00b7me", ",", "die", "ihr", "prellt", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "ADJA", "NN", "$,", "PRELS", "PPER", "VVFIN", "$.", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "An der Geschichte ist kein Deut erfunden.", "tokens": ["An", "der", "Ge\u00b7schich\u00b7te", "ist", "kein", "Deut", "er\u00b7fun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sie ist ein wahres Beispiel, zu bekunden:", "tokens": ["Sie", "ist", "ein", "wah\u00b7res", "Bei\u00b7spiel", ",", "zu", "be\u00b7kun\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein Sous in Sicherheit ist oft", "tokens": ["Ein", "Sous", "in", "Si\u00b7cher\u00b7heit", "ist", "oft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN", "VAFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mehr wert als f\u00fcnf, die man erhofft.", "tokens": ["Mehr", "wert", "als", "f\u00fcnf", ",", "die", "man", "er\u00b7hofft", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "CARD", "$,", "PRELS", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Man soll mit seinem Los zufrieden sein", "tokens": ["Man", "soll", "mit", "sei\u00b7nem", "Los", "zu\u00b7frie\u00b7den", "sein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VMFIN", "APPR", "PPOSAT", "NN", "ADJD", "VAINF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und nicht dem Wasser und dem Wind", "tokens": ["Und", "nicht", "dem", "Was\u00b7ser", "und", "dem", "Wind"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PTKNEG", "ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ehrgeizig seine Ohren leihn.", "tokens": ["Ehr\u00b7gei\u00b7zig", "sei\u00b7ne", "Oh\u00b7ren", "leihn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Auf einen Gl\u00fccklichen entfallen", "tokens": ["Auf", "ei\u00b7nen", "Gl\u00fcck\u00b7li\u00b7chen", "ent\u00b7fal\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Zehntausend, die betrogen sind.", "tokens": ["Zehn\u00b7tau\u00b7send", ",", "die", "be\u00b7tro\u00b7gen", "sind", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "PRELS", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Reichtum verspricht das Meer euch allen,", "tokens": ["Reich\u00b7tum", "ver\u00b7spricht", "das", "Meer", "euch", "al\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "PPER", "PIS", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.11": {"text": "Doch h\u00fctet euch und denkt daran,", "tokens": ["Doch", "h\u00fc\u00b7tet", "euch", "und", "denkt", "da\u00b7ran", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "KON", "VVFIN", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Da\u00df man vor Sturm und Raub kein Gut bewahren kann.", "tokens": ["Da\u00df", "man", "vor", "Sturm", "und", "Raub", "kein", "Gut", "be\u00b7wah\u00b7ren", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "NN", "KON", "NN", "PIAT", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Von dem, was ihm die eigne Herde trug \u2013", "tokens": ["Von", "dem", ",", "was", "ihm", "die", "eig\u00b7ne", "Her\u00b7de", "trug", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$,", "PWS", "PPER", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Es war nicht viel, doch f\u00fcr Bescheidenheit genug \u2013", "tokens": ["Es", "war", "nicht", "viel", ",", "doch", "f\u00fcr", "Be\u00b7schei\u00b7den\u00b7heit", "ge\u00b7nug", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADV", "$,", "ADV", "APPR", "NN", "ADV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexan