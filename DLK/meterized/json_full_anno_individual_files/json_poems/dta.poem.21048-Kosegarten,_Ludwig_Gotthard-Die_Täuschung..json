{"dta.poem.21048": {"metadata": {"author": {"name": "Kosegarten, Ludwig Gotthard", "birth": "N.A.", "death": "N.A."}, "title": "Die T\u00e4uschung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1798", "urn": "urn:nbn:de:kobv:b4-200905193300", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Im Erlenbusch, im Tannenhayn,               ", "tokens": ["Im", "Er\u00b7len\u00b7busch", ",", "im", "Tan\u00b7nen\u00b7hayn", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In Sonn- und Mond- und Sternenschein", "tokens": ["In", "Sonn", "und", "Mon\u00b7d", "und", "Ster\u00b7nen\u00b7schein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "TRUNC", "KON", "TRUNC", "KON", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Uml\u00e4chelt mich ein Bildniss.", "tokens": ["Um\u00b7l\u00e4\u00b7chelt", "mich", "ein", "Bild\u00b7niss", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Vor seinem L\u00e4cheln kl\u00e4rt sich schnell", "tokens": ["Vor", "sei\u00b7nem", "L\u00e4\u00b7cheln", "kl\u00e4rt", "sich", "schnell"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PRF", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die D\u00e4mmerung in Himmelhell,", "tokens": ["Die", "D\u00e4m\u00b7me\u00b7rung", "in", "Him\u00b7mel\u00b7hell", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "In Paradies die Wildniss.", "tokens": ["In", "Pa\u00b7ra\u00b7dies", "die", "Wild\u00b7niss", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Es s\u00e4uselt in der Abendluft,", "tokens": ["Es", "s\u00e4u\u00b7selt", "in", "der", "A\u00b7ben\u00b7dluft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es d\u00e4mmert in dem Morgenduft,", "tokens": ["Es", "d\u00e4m\u00b7mert", "in", "dem", "Mor\u00b7gen\u00b7duft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es tanzet auf der Aue.", "tokens": ["Es", "tan\u00b7zet", "auf", "der", "Au\u00b7e", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Es fl\u00f6tet in dem Wachtelschlag,", "tokens": ["Es", "fl\u00f6\u00b7tet", "in", "dem", "Wach\u00b7tel\u00b7schlag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und spiegelt sich im klaren Bach,", "tokens": ["Und", "spie\u00b7gelt", "sich", "im", "kla\u00b7ren", "Bach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und badet sich im Thaue.", "tokens": ["Und", "ba\u00b7det", "sich", "im", "Thau\u00b7e", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Es naht in holder Traulichkeit", "tokens": ["Es", "naht", "in", "hol\u00b7der", "Trau\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sich mir in tiefster Dunkelheit", "tokens": ["Sich", "mir", "in", "tiefs\u00b7ter", "Dun\u00b7kel\u00b7heit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PRF", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So sch\u00fcchtern und so leise.", "tokens": ["So", "sch\u00fcch\u00b7tern", "und", "so", "lei\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KON", "ADV", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Es lullt mich wohl in sanfte Ruh,", "tokens": ["Es", "lullt", "mich", "wohl", "in", "sanf\u00b7te", "Ruh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und haucht im Schlaf mir Tr\u00e4ume zu", "tokens": ["Und", "haucht", "im", "Schlaf", "mir", "Tr\u00e4u\u00b7me", "zu"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPRART", "NN", "PPER", "NN", "PTKZU"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Von wunders\u00fcsser Weise.", "tokens": ["Von", "wun\u00b7der\u00b7s\u00fcs\u00b7ser", "Wei\u00b7se", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ich \u00f6ffn' ihm sehnend meinen Arm,", "tokens": ["Ich", "\u00f6ffn'", "ihm", "seh\u00b7nend", "mei\u00b7nen", "Arm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVPP", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und streb', es traut und liebewarm", "tokens": ["Und", "streb'", ",", "es", "traut", "und", "lie\u00b7be\u00b7warm"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "PPER", "VVFIN", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "An meine Brust zu dr\u00fccken.", "tokens": ["An", "mei\u00b7ne", "Brust", "zu", "dr\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ich hasch', und hasche leere Luft.", "tokens": ["Ich", "hasch'", ",", "und", "ha\u00b7sche", "lee\u00b7re", "Luft", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKVZ", "$,", "KON", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und nichtig, wie ein Nebelduft,", "tokens": ["Und", "nich\u00b7tig", ",", "wie", "ein", "Ne\u00b7bel\u00b7duft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "PWAV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Entwallt es meinen Blicken.", "tokens": ["Ent\u00b7wallt", "es", "mei\u00b7nen", "Bli\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}