": "Der solche Rosen bl\u00fchen hie\u00df", "tokens": ["Der", "sol\u00b7che", "Ro\u00b7sen", "bl\u00fc\u00b7hen", "hie\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PIAT", "NN", "VVINF", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und uns, trotz wilder Wetter Toben,", "tokens": ["Und", "uns", ",", "trotz", "wil\u00b7der", "Wet\u00b7ter", "To\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "APPR", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Die sch\u00f6nste dennoch finden lie\u00df.", "tokens": ["Die", "sch\u00f6ns\u00b7te", "den\u00b7noch", "fin\u00b7den", "lie\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADV", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "So la\u00df uns diesen Gott vertrauen,", "tokens": ["So", "la\u00df", "uns", "die\u00b7sen", "Gott", "ver\u00b7trau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "PDAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der an den Blumen Wunder thut,", "tokens": ["Der", "an", "den", "Blu\u00b7men", "Wun\u00b7der", "thut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nicht nur im Blitz ist er zu schauen,", "tokens": ["Nicht", "nur", "im", "Blitz", "ist", "er", "zu", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "APPRART", "NN", "VAFIN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"tex