{"textgrid.poem.41772": {"metadata": {"author": {"name": "Otto, Louise", "birth": "N.A.", "death": "N.A."}, "title": "Moosrose", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die rote, bl\u00e4tterreiche Rose,", "tokens": ["Die", "ro\u00b7te", ",", "bl\u00e4t\u00b7ter\u00b7rei\u00b7che", "Ro\u00b7se", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Voll Duft und tiefverborgner Glut,", "tokens": ["Voll", "Duft", "und", "tief\u00b7ver\u00b7borg\u00b7ner", "Glut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die ohne Dorn im weichen Moose,", "tokens": ["Die", "oh\u00b7ne", "Dorn", "im", "wei\u00b7chen", "Moo\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Auf zartem Stengel tr\u00e4umend ruht':", "tokens": ["Auf", "zar\u00b7tem", "Sten\u00b7gel", "tr\u00e4u\u00b7mend", "ruht'", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Rose gab ich Dir zu eigen \u2013", "tokens": ["Die", "Ro\u00b7se", "gab", "ich", "Dir", "zu", "ei\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PPER", "PTKA", "ADJD", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "O wie verstandest Du mich wohl!", "tokens": ["O", "wie", "ver\u00b7stan\u00b7dest", "Du", "mich", "wohl", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "VVFIN", "PPER", "PRF", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Du weihtest sie zum Bundeszeichen", "tokens": ["Du", "weih\u00b7test", "sie", "zum", "Bun\u00b7des\u00b7zei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Zu unsrer Seligkeit Symbol!", "tokens": ["Zu", "uns\u00b7rer", "Se\u00b7lig\u00b7keit", "Sym\u00b7bol", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Du willst sie unverwelklich wahren", "tokens": ["Du", "willst", "sie", "un\u00b7ver\u00b7wel\u00b7klich", "wah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "ADJD", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In Deiner Hand, an Deiner Brust,", "tokens": ["In", "Dei\u00b7ner", "Hand", ",", "an", "Dei\u00b7ner", "Brust", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Talisman, der in Gefahren", "tokens": ["Ein", "Ta\u00b7lis\u00b7man", ",", "der", "in", "Ge\u00b7fah\u00b7ren"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Zu sch\u00fctzen mich und Dich gewu\u00dft;", "tokens": ["Zu", "sch\u00fct\u00b7zen", "mich", "und", "Dich", "ge\u00b7wu\u00dft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "PPER", "KON", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ein Unterpfand von k\u00fcnft'ger Wonne,", "tokens": ["Ein", "Un\u00b7ter\u00b7pfand", "von", "k\u00fcnft'\u00b7ger", "Won\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wenn hinter uns die finstre Nacht,", "tokens": ["Wenn", "hin\u00b7ter", "uns", "die", "finst\u00b7re", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wenn eine freie, stolze Sonne", "tokens": ["Wenn", "ei\u00b7ne", "frei\u00b7e", ",", "stol\u00b7ze", "Son\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Zugleich auf uns herniederlacht.", "tokens": ["Zu\u00b7gleich", "auf", "uns", "her\u00b7nie\u00b7der\u00b7lacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Viel Dornen sind auf unsern Wegen,", "tokens": ["Viel", "Dor\u00b7nen", "sind", "auf", "un\u00b7sern", "We\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Doch ", "tokens": ["Doch"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Du zogst mit warmen Herzensschl\u00e4gen", "tokens": ["Du", "zogst", "mit", "war\u00b7men", "Her\u00b7zens\u00b7schl\u00e4\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die stille Knospe voll und gro\u00df.", "tokens": ["Die", "stil\u00b7le", "Knos\u00b7pe", "voll", "und", "gro\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Das ist ein Sprossen, ist ein Dr\u00e4ngen \u2013", "tokens": ["Das", "ist", "ein", "Spros\u00b7sen", ",", "ist", "ein", "Dr\u00e4n\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "VAFIN", "ART", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ein ganzer Hain von Rosen bl\u00fcht,", "tokens": ["Ein", "gan\u00b7zer", "Hain", "von", "Ro\u00b7sen", "bl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und zu begeisterten Ges\u00e4ngen", "tokens": ["Und", "zu", "be\u00b7geis\u00b7ter\u00b7ten", "Ge\u00b7s\u00e4n\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ein jeder Kelch sich \u00f6ffnend gl\u00fcht.", "tokens": ["Ein", "je\u00b7der", "Kelch", "sich", "\u00f6ff\u00b7nend", "gl\u00fcht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "PRF", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "So la\u00df uns selig tr\u00e4umend wallen", "tokens": ["So", "la\u00df", "uns", "se\u00b7lig", "tr\u00e4u\u00b7mend", "wal\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVIMP", "PPER", "ADJD", "VVPP", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Im Rosenhain der Poesie,", "tokens": ["Im", "Ro\u00b7sen\u00b7hain", "der", "Poe\u00b7sie", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+---", "measure": "unknown.measure.di"}, "line.3": {"text": "Und Lied um Lied soll preisend schallen", "tokens": ["Und", "Lied", "um", "Lied", "soll", "prei\u00b7send", "schal\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "APPR", "NN", "VMFIN", "ADJD", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In s\u00fc\u00dfer Liebes-Melodie.", "tokens": ["In", "s\u00fc\u00b7\u00dfer", "Lie\u00b7bes\u00b7Me\u00b7lo\u00b7die", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So la\u00df uns Gott im Himmel loben", "tokens": ["So", "la\u00df", "uns", "Gott", "im", "Him\u00b7mel", "lo\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVIMP", "PPER", "NN", "APPRART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der solche Rosen bl\u00fchen hie\u00df", "tokens": ["Der", "sol\u00b7che", "Ro\u00b7sen", "bl\u00fc\u00b7hen", "hie\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PIAT", "NN", "VVINF", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und uns, trotz wilder Wetter Toben,", "tokens": ["Und", "uns", ",", "trotz", "wil\u00b7der", "Wet\u00b7ter", "To\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "APPR", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Die sch\u00f6nste dennoch finden lie\u00df.", "tokens": ["Die", "sch\u00f6ns\u00b7te", "den\u00b7noch", "fin\u00b7den", "lie\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADV", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "So la\u00df uns diesen Gott vertrauen,", "tokens": ["So", "la\u00df", "uns", "die\u00b7sen", "Gott", "ver\u00b7trau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "PDAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der an den Blumen Wunder thut,", "tokens": ["Der", "an", "den", "Blu\u00b7men", "Wun\u00b7der", "thut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nicht nur im Blitz ist er zu schauen,", "tokens": ["Nicht", "nur", "im", "Blitz", "ist", "er", "zu", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "APPRART", "NN", "VAFIN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Er redet auch aus Rosenglut.", "tokens": ["Er", "re\u00b7det", "auch", "aus", "Ro\u00b7seng\u00b7lut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wie uns des Wetters Nacht umdunkelt,", "tokens": ["Wie", "uns", "des", "Wet\u00b7ters", "Nacht", "um\u00b7dun\u00b7kelt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wie Angst und Weh' das Los der Zeit:", "tokens": ["Wie", "Angst", "und", "Weh'", "das", "Los", "der", "Zeit", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ein heil'ger Strahl im Kelche funkelt \u2013", "tokens": ["Ein", "heil'\u00b7ger", "Strahl", "im", "Kel\u00b7che", "fun\u00b7kelt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Die Rose bl\u00fcht in Ewigkeit!", "tokens": ["Die", "Ro\u00b7se", "bl\u00fcht", "in", "E\u00b7wig\u00b7keit", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Die rote, bl\u00e4tterreiche Rose,", "tokens": ["Die", "ro\u00b7te", ",", "bl\u00e4t\u00b7ter\u00b7rei\u00b7che", "Ro\u00b7se", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Voll Duft und tiefverborgner Glut,", "tokens": ["Voll", "Duft", "und", "tief\u00b7ver\u00b7borg\u00b7ner", "Glut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die ohne Dorn im weichen Moose,", "tokens": ["Die", "oh\u00b7ne", "Dorn", "im", "wei\u00b7chen", "Moo\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Auf zartem Stengel tr\u00e4umend ruht':", "tokens": ["Auf", "zar\u00b7tem", "Sten\u00