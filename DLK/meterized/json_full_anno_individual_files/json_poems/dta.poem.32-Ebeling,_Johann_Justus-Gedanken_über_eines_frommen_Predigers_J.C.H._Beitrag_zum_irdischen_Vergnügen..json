{"dta.poem.32": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "Gedanken  \n \u00fcber eines frommen Predigers J.C.H.  \n Beitrag zum irdischen Vergn\u00fcgen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198797", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein Herz das seinen Sch\u00f6pfer liebt,", "tokens": ["Ein", "Herz", "das", "sei\u00b7nen", "Sch\u00f6p\u00b7fer", "liebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das sich in reiner Andacht \u00fcbt,", "tokens": ["Das", "sich", "in", "rei\u00b7ner", "An\u00b7dacht", "\u00fcbt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PRF", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das kan auf Bergen in den Gr\u00fcn-", "tokens": ["Das", "kan", "auf", "Ber\u00b7gen", "in", "den", "Gr\u00fcn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VMFIN", "APPR", "NN", "APPR", "ART", "TRUNC"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In G\u00e4rten, Auen, Wald und Feld,", "tokens": ["In", "G\u00e4r\u00b7ten", ",", "Au\u00b7en", ",", "Wald", "und", "Feld", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "In allen Dingen dieser Welt,", "tokens": ["In", "al\u00b7len", "Din\u00b7gen", "die\u00b7ser", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ein Feur zum Andachtsopfer finden.", "tokens": ["Ein", "Feur", "zum", "An\u00b7dach\u00b7tsop\u00b7fer", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Ich seh dein Buch ", "tokens": ["Ich", "seh", "dein", "Buch"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Davon als den Beweisthum an;", "tokens": ["Da\u00b7von", "als", "den", "Be\u00b7wei\u00b7sthum", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "KOUS", "ART", "NN", "PTKVZ", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.9": {"text": "Ich w\u00fcnsche da\u00df desselben Lehren,", "tokens": ["Ich", "w\u00fcn\u00b7sche", "da\u00df", "des\u00b7sel\u00b7ben", "Leh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOUS", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Des Lesers Herze dahin ziehn,", "tokens": ["Des", "Le\u00b7sers", "Her\u00b7ze", "da\u00b7hin", "ziehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PAV", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Sich so wie du stets zu bem\u00fchn,", "tokens": ["Sich", "so", "wie", "du", "stets", "zu", "be\u00b7m\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "KOKOM", "PPER", "ADV", "PTKZU", "VVINF", "$,"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.12": {"text": "Den Sch\u00f6pfer im Gesch\u00f6pf zu ehren.", "tokens": ["Den", "Sch\u00f6p\u00b7fer", "im", "Ge\u00b7sch\u00f6pf", "zu", "eh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}