{"textgrid.poem.46220": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "An Herren Christian F\u00fcrsten zu Anhalt", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der feind gleichwie der freind wird dich in dieser nacht,", "tokens": ["Der", "feind", "gleich\u00b7wie", "der", "freind", "wird", "dich", "in", "die\u00b7ser", "nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "VAFIN", "PRF", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "da tugend und fromkeit ganz ligen \u00fcberdrungen,", "tokens": ["da", "tu\u00b7gend", "und", "from\u00b7keit", "ganz", "li\u00b7gen", "\u00fc\u00b7berd\u00b7run\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "NN", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+--+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "ein wunder in dem rat, ein dunder in der schlacht,", "tokens": ["ein", "wun\u00b7der", "in", "dem", "rat", ",", "ein", "dun\u00b7der", "in", "der", "schlacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "VVFIN", "$,", "ART", "ADJA", "APPR", "ART", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "mit ehr, verwundrung, lob zu nennen stracks gezwungen.", "tokens": ["mit", "ehr", ",", "ver\u00b7wund\u00b7rung", ",", "lob", "zu", "nen\u00b7nen", "stracks", "ge\u00b7zwun\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "$,", "NN", "PTKZU", "VVINF", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Auch deines geists, leibs, munds weis, stark und s\u00fc\u00df macht", "tokens": ["Auch", "dei\u00b7nes", "geists", ",", "leibs", ",", "munds", "weis", ",", "stark", "und", "s\u00fc\u00df", "macht"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "NN", "$,", "VVFIN", "$,", "ADJD", "PTKVZ", "$,", "ADJD", "KON", "ADJD", "VVFIN"], "meter": "-+---+-+-+-", "measure": "dactylic.init"}, "line.6": {"text": "(was zu der helden lob die Griechen je gesungen)", "tokens": ["(", "was", "zu", "der", "hel\u00b7den", "lob", "die", "Grie\u00b7chen", "je", "ge\u00b7sun\u00b7gen", ")"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "APPR", "ART", "ADJA", "NN", "ART", "NN", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "bereiten f\u00fcr dein haupt mit dreimal reichem pracht", "tokens": ["be\u00b7rei\u00b7ten", "f\u00fcr", "dein", "haupt", "mit", "drei\u00b7mal", "rei\u00b7chem", "pracht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "APPR", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "ein dreifach reichen kranz durch dein schwert, feder, zungen.", "tokens": ["ein", "drei\u00b7fach", "rei\u00b7chen", "kranz", "durch", "dein", "schwert", ",", "fe\u00b7der", ",", "zun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "APPR", "PPOSAT", "VVFIN", "$,", "PTKVZ", "$,", "VVINF", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Wan du