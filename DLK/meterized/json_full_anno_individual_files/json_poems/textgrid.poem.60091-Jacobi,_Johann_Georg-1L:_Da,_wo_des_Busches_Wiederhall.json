{"textgrid.poem.60091": {"metadata": {"author": {"name": "Jacobi, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "1L: Da, wo des Busches Wiederhall", "genre": "verse", "period": "N.A.", "pub_year": 1777, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da, wo des Busches Wiederhall", "tokens": ["Da", ",", "wo", "des", "Bu\u00b7sches", "Wie\u00b7der\u00b7hall"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "PWAV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Am reinsten war, da sa\u00df auf ihrer Eiche,", "tokens": ["Am", "reins\u00b7ten", "war", ",", "da", "sa\u00df", "auf", "ih\u00b7rer", "Ei\u00b7che", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "VAFIN", "$,", "ADV", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Verborgen, eine Nachtigall;", "tokens": ["Ver\u00b7bor\u00b7gen", ",", "ei\u00b7ne", "Nach\u00b7ti\u00b7gall", ";"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und Knaben pl\u00e4tscherten nicht weit davon im Teiche.", "tokens": ["Und", "Kna\u00b7ben", "pl\u00e4t\u00b7scher\u00b7ten", "nicht", "weit", "da\u00b7von", "im", "Tei\u00b7che", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PTKNEG", "ADJD", "PAV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Sie sa\u00df, und sang in Fried' und Ruh;", "tokens": ["Sie", "sa\u00df", ",", "und", "sang", "in", "Fried'", "und", "Ruh", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KON", "VVFIN", "APPR", "NE", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die V\u00f6gel h\u00f6rten ihr auf allen Aesten zu.", "tokens": ["Die", "V\u00f6\u00b7gel", "h\u00f6r\u00b7ten", "ihr", "auf", "al\u00b7len", "A\u00b7es\u00b7ten", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Ach! aber weg vom Bade schlich", "tokens": ["Ach", "!", "a\u00b7ber", "weg", "vom", "Ba\u00b7de", "schlich"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "ADV", "ADV", "APPRART", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Knabe zu dem Baum, und scharrte", "tokens": ["Ein", "Kna\u00b7be", "zu", "dem", "Baum", ",", "und", "scharr\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,", "KON", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Den Boden auf, und stellte sich", "tokens": ["Den", "Bo\u00b7den", "auf", ",", "und", "stell\u00b7te", "sich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "PTKVZ", "$,", "KON", "VVFIN", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Am nahen Schlehdorn auf die Warte.", "tokens": ["Am", "na\u00b7hen", "Schleh\u00b7dorn", "auf", "die", "War\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die Nachtigall ist treu und gut,", "tokens": ["Die", "Nach\u00b7ti\u00b7gall", "ist", "treu", "und", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Man kann sie leichtlich \u00fcberraschen;", "tokens": ["Man", "kann", "sie", "leicht\u00b7lich", "\u00fc\u00b7berr\u00b7a\u00b7schen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Sie glaubte jetzt, ein W\u00fcrmchen zu erhaschen,", "tokens": ["Sie", "glaub\u00b7te", "jetzt", ",", "ein", "W\u00fcrm\u00b7chen", "zu", "er\u00b7ha\u00b7schen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und flog herab mit unbesorgtem Muth,", "tokens": ["Und", "flog", "her\u00b7ab", "mit", "un\u00b7be\u00b7sorg\u00b7tem", "Muth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Als, wie ein Blitz, des Knaben Hut", "tokens": ["Als", ",", "wie", "ein", "Blitz", ",", "des", "Kna\u00b7ben", "Hut"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "$,", "PWAV", "ART", "NN", "$,", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Die arme S\u00e4ngerinn bedeckte,", "tokens": ["Die", "ar\u00b7me", "S\u00e4n\u00b7ge\u00b7rinn", "be\u00b7deck\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Und sie durch ihr Geschrey den Busch zu Klagen weckte.", "tokens": ["Und", "sie", "durch", "ihr", "Ge\u00b7schrey", "den", "Busch", "zu", "Kla\u00b7gen", "weck\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "PPOSAT", "NN", "ART", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Das gr\u00f6\u00dfte Mitleid trug", "tokens": ["Das", "gr\u00f6\u00df\u00b7te", "Mit\u00b7leid", "trug"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Mit ihr ein Stieglitz, alt und klug,", "tokens": ["Mit", "ihr", "ein", "Stieg\u00b7litz", ",", "alt", "und", "klug", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Von jener Art, die nimmer unterlassen,", "tokens": ["Von", "je\u00b7ner", "Art", ",", "die", "nim\u00b7mer", "un\u00b7ter\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,", "PRELS", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ihr Tr\u00f6stungs-Wort als Predigt abzufassen.", "tokens": ["Ihr", "Tr\u00f6s\u00b7tungs\u00b7Wort", "als", "Pre\u00b7digt", "ab\u00b7zu\u00b7fas\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KOUS", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Der sagte: Liebe Nachtigall!", "tokens": ["Der", "sag\u00b7te", ":", "Lie\u00b7be", "Nach\u00b7ti\u00b7gall", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$.", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von Herzen dauert mich dein unverhoffter Fall.", "tokens": ["Von", "Her\u00b7zen", "dau\u00b7ert", "mich", "dein", "un\u00b7ver\u00b7hoff\u00b7ter", "Fall", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Welt ist schlimm; es gibt der losen", "tokens": ["Die", "Welt", "ist", "schlimm", ";", "es", "gibt", "der", "lo\u00b7sen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$.", "PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Gesellen jetzund \u00fcberall.", "tokens": ["Ge\u00b7sel\u00b7len", "je\u00b7tzund", "\u00fc\u00b7be\u00b7rall", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da\u00df aber auch ihr Virtuosen", "tokens": ["Da\u00df", "a\u00b7ber", "auch", "ihr", "Vir\u00b7tu\u00b7o\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "In eurem Thun und Dichten jederzeit", "tokens": ["In", "eu\u00b7rem", "Thun", "und", "Dich\u00b7ten", "je\u00b7der\u00b7zeit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "KON", "NN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "So neu, so albern seyd,", "tokens": ["So", "neu", ",", "so", "al\u00b7bern", "seyd", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADV", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Als w\u00fcchsen euch die Fl\u00fcgel eben!", "tokens": ["Als", "w\u00fcch\u00b7sen", "euch", "die", "Fl\u00fc\u00b7gel", "e\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPER", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Auf eines Kindes Wink sich also Preis zu geben,", "tokens": ["Auf", "ei\u00b7nes", "Kin\u00b7des", "Wink", "sich", "al\u00b7so", "Preis", "zu", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "PRF", "ADV", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "F\u00fcr Meisters\u00e4nger, welche Schmach!", "tokens": ["F\u00fcr", "Meis\u00b7ter\u00b7s\u00e4n\u00b7ger", ",", "wel\u00b7che", "Schmach", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PWAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Die winselnde Gefangne sprach:", "tokens": ["Die", "win\u00b7seln\u00b7de", "Ge\u00b7fang\u00b7ne", "sprach", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.2": {"text": "O Freundinn! soll ich nicht dem Menschen mich vertrauen?", "tokens": ["O", "Freun\u00b7dinn", "!", "soll", "ich", "nicht", "dem", "Men\u00b7schen", "mich", "ver\u00b7trau\u00b7en", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "VMFIN", "PPER", "PTKNEG", "ART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich nehm' in G\u00e4rten, Feldern, Auen", "tokens": ["Ich", "nehm'", "in", "G\u00e4r\u00b7ten", ",", "Fel\u00b7dern", ",", "Au\u00b7en"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$,", "NN", "$,", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ihm nichts, bin keinem je zur Last;", "tokens": ["Ihm", "nichts", ",", "bin", "kei\u00b7nem", "je", "zur", "Last", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "$,", "VAFIN", "PIS", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dem M\u00fcden sing ich gern bey seiner Abend-Rast,", "tokens": ["Dem", "M\u00fc\u00b7den", "sing", "ich", "gern", "bey", "sei\u00b7ner", "A\u00b7ben\u00b7dRast", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und hoffte Schutz und Lohn von ihm, der solche Gruben", "tokens": ["Und", "hoff\u00b7te", "Schutz", "und", "Lohn", "von", "ihm", ",", "der", "sol\u00b7che", "Gru\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "KON", "NN", "APPR", "PPER", "$,", "PRELS", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Mir gr\u00e4bt im unverd\u00e4cht'gen Hain.", "tokens": ["Mir", "gr\u00e4bt", "im", "un\u00b7ver\u00b7d\u00e4cht'\u00b7gen", "Hain", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Du konntest, fiel der Stieglitz ein,", "tokens": ["Du", "konn\u00b7test", ",", "fiel", "der", "Stieg\u00b7litz", "ein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von M\u00e4nnern des gew\u00e4rtig seyn;", "tokens": ["Von", "M\u00e4n\u00b7nern", "des", "ge\u00b7w\u00e4r\u00b7tig", "seyn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch, liebe Nachtigall, von Buben,", "tokens": ["Doch", ",", "lie\u00b7be", "Nach\u00b7ti\u00b7gall", ",", "von", "Bu\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "$,", "VVFIN", "NN", "$,", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Von Lotterbuben, nein!", "tokens": ["Von", "Lot\u00b7ter\u00b7bu\u00b7ben", ",", "nein", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["APPR", "NE", "$,", "PTKANT", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Da, wo des Busches Wiederhall", "tokens": ["Da", ",", "wo", "des", "Bu\u00b7sches", "Wie\u00b7der\u00b7hall"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "PWAV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Am reinsten war, da sa\u00df auf ihrer Eiche,", "tokens": ["Am", "reins\u00b7ten", "war", ",", "da", "sa\u00df", "auf", "ih\u00b7rer", "Ei\u00b7che", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "VAFIN", "$,", "ADV", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Verborgen, eine Nachtigall;", "tokens": ["Ver\u00b7bor\u00b7gen", ",", "ei\u00b7ne", "Nach\u00b7ti\u00b7gall", ";"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "$."], "meter": "-