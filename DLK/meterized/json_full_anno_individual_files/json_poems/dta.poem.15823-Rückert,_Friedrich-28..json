{"dta.poem.15823": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "28.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Freund ist immerfort vor meiner Seele Augen,", "tokens": ["Der", "Freund", "ist", "im\u00b7mer\u00b7fort", "vor", "mei\u00b7ner", "See\u00b7le", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn die des Leibes ihn nicht zu ergreifen taugen.", "tokens": ["Wenn", "die", "des", "Lei\u00b7bes", "ihn", "nicht", "zu", "er\u00b7grei\u00b7fen", "tau\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ART", "NN", "PPER", "PTKNEG", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Er blickt von dort mich an, wo auf die Sonne geht,", "tokens": ["Er", "blickt", "von", "dort", "mich", "an", ",", "wo", "auf", "die", "Son\u00b7ne", "geht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADV", "PPER", "PTKVZ", "$,", "PWAV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und blickt noch einmal her, wo sie im Sinken steht.", "tokens": ["Und", "blickt", "noch", "ein\u00b7mal", "her", ",", "wo", "sie", "im", "Sin\u00b7ken", "steht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADV", "PTKVZ", "$,", "PWAV", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "So wie sie blicket hier, hat Abschied er genommen;", "tokens": ["So", "wie", "sie", "bli\u00b7cket", "hier", ",", "hat", "Ab\u00b7schied", "er", "ge\u00b7nom\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "PPER", "VVFIN", "ADV", "$,", "VAFIN", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und wie sie blicket dort, so wird er wieder kommen.", "tokens": ["Und", "wie", "sie", "bli\u00b7cket", "dort", ",", "so", "wird", "er", "wie\u00b7der", "kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "VVFIN", "ADV", "$,", "ADV", "VAFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}