{"dta.poem.14652": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Die Brautfahrt .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Du mu\u00dft glauben, Du mu\u00dft wagen,", "tokens": ["Du", "mu\u00dft", "glau\u00b7ben", ",", "Du", "mu\u00dft", "wa\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "$,", "PPER", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und, den Argonauten gleich,", "tokens": ["Und", ",", "den", "Ar\u00b7go\u00b7nau\u00b7ten", "gleich", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ART", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wird die Woge fromm Dich tragen", "tokens": ["Wird", "die", "Wo\u00b7ge", "fromm", "Dich", "tra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ART", "NN", "VVFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In das wunderbare Reich;", "tokens": ["In", "das", "wun\u00b7der\u00b7ba\u00b7re", "Reich", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Muthig streitend mit den Winden,", "tokens": ["Mut\u00b7hig", "strei\u00b7tend", "mit", "den", "Win\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Mu\u00df ich meine Heimath finden!", "tokens": ["Mu\u00df", "ich", "mei\u00b7ne", "Hei\u00b7math", "fin\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}