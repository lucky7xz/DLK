{"dta.poem.2763": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Ein anders.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Dein auserw\u00e4hlter Schatz/ vertrauter Perlemuth/", "tokens": ["Dein", "au\u00b7ser\u00b7w\u00e4hl\u00b7ter", "Schatz", "/", "ver\u00b7trau\u00b7ter", "Per\u00b7le\u00b7muth", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$(", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die mir der Himmel itzt schenckt zu dem h\u00f6chsten Gut/", "tokens": ["Die", "mir", "der", "Him\u00b7mel", "itzt", "schenckt", "zu", "dem", "h\u00f6chs\u00b7ten", "Gut", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "ADV", "VVFIN", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und unsern Liebes-Bund geneiget unterschreibt/", "tokens": ["Und", "un\u00b7sern", "Lie\u00b7bes\u00b7Bund", "ge\u00b7nei\u00b7get", "un\u00b7ter\u00b7schreibt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVPP", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der auch trotz Noth und Tod best\u00e4ndig ist und", "tokens": ["Der", "auch", "trotz", "Noth", "und", "Tod", "be\u00b7st\u00e4n\u00b7dig", "ist", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "APPR", "NN", "KON", "NN", "ADJD", "VAFIN", "KON"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Mein Hertze hat ja l\u00e4ngst zum Opffer dir gebrennt/", "tokens": ["Mein", "Hert\u00b7ze", "hat", "ja", "l\u00e4ngst", "zum", "Opf\u00b7fer", "dir", "ge\u00b7brennt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VVFIN", "VAFIN", "ADV", "ADV", "APPRART", "NN", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So hat dein sch\u00f6nes Aug\u2019 auch meine Brunst gekennt/", "tokens": ["So", "hat", "dein", "sch\u00f6\u00b7nes", "Aug'", "auch", "mei\u00b7ne", "Brunst", "ge\u00b7kennt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "ADJA", "NN", "ADV", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Nun hat den Opffer-Tisch die Venus selbst bereit/", "tokens": ["Nun", "hat", "den", "Opf\u00b7fer\u00b7Tisch", "die", "Ve\u00b7nus", "selbst", "be\u00b7reit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ART", "NN", "ADV", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Umb den der Hymen auch die Liebes-Rosen streut.", "tokens": ["Umb", "den", "der", "Hy\u00b7men", "auch", "die", "Lie\u00b7bes\u00b7Ro\u00b7sen", "streut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ART", "NN", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}}, "stanza.3": {"line.1": {"text": "Die Sternen sehn erfreut der Seelen B\u00fcndn\u00fc\u00df an/", "tokens": ["Die", "Ster\u00b7nen", "sehn", "er\u00b7freut", "der", "See\u00b7len", "B\u00fcnd\u00b7n\u00fc\u00df", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VVFIN", "ART", "NN", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und werffen gr\u00f6sser Licht von dem Saphirnen Plan/", "tokens": ["Und", "werf\u00b7fen", "gr\u00f6s\u00b7ser", "Licht", "von", "dem", "Sa\u00b7phir\u00b7nen", "Plan", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die angenehme Nacht sagt neue Freuden zu/", "tokens": ["Die", "an\u00b7ge\u00b7neh\u00b7me", "Nacht", "sagt", "neu\u00b7e", "Freu\u00b7den", "zu", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADJA", "NN", "PTKZU", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und ruffet dir und mir/ Hertzliebste/ zu der Ruh.", "tokens": ["Und", "ruf\u00b7fet", "dir", "und", "mir", "/", "Hertz\u00b7liebs\u00b7te", "/", "zu", "der", "Ruh", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "KON", "PPER", "$(", "NN", "$(", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Dein holdes Angesicht gew\u00e4hrt der Himmel mir/", "tokens": ["Dein", "hol\u00b7des", "An\u00b7ge\u00b7sicht", "ge\u00b7w\u00e4hrt", "der", "Him\u00b7mel", "mir", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "ART", "NN", "PPER", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich achte nicht die Sonn und aller Sternen Zier/", "tokens": ["Ich", "ach\u00b7te", "nicht", "die", "Sonn", "und", "al\u00b7ler", "Ster\u00b7nen", "Zier", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ART", "NN", "KON", "PIAT", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ein recht-verliebter Blick/ den mir dein Hertz entdeckt/", "tokens": ["Ein", "recht\u00b7ver\u00b7lieb\u00b7ter", "Blick", "/", "den", "mir", "dein", "Hertz", "ent\u00b7deckt", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "ART", "PPER", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Hat mehr Empfindlichkeit und Lust bey mir erweckt.", "tokens": ["Hat", "mehr", "Emp\u00b7find\u00b7lich\u00b7keit", "und", "Lust", "bey", "mir", "er\u00b7weckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "KON", "NN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Jhr Buhler/ jener Zeit/ seyd nichtig hier gesch\u00e4tzt/", "tokens": ["Ihr", "Buh\u00b7ler", "/", "je\u00b7ner", "Zeit", "/", "seyd", "nich\u00b7tig", "hier", "ge\u00b7sch\u00e4tzt", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "PDAT", "NN", "$(", "VAFIN", "ADJD", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die offt ein leer er Traum und Sinnen-Bild ergetzt:", "tokens": ["Die", "offt", "ein", "leer", "er", "Traum", "und", "Sin\u00b7nen\u00b7Bild", "er\u00b7getzt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "ADJD", "PPER", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mir legt der Himmel selbst die Perlemuth itzt bey/", "tokens": ["Mir", "legt", "der", "Him\u00b7mel", "selbst", "die", "Per\u00b7le\u00b7muth", "itzt", "bey", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "ART", "NN", "ADV", "APPR", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und flecht umb unser Haupt den Krantz von Lieb und Treu.", "tokens": ["Und", "flecht", "umb", "un\u00b7ser", "Haupt", "den", "Krantz", "von", "Lieb", "und", "Treu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "ART", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Wie seelig acht ich mich/ mein Kind/ bey dir zu seyn/", "tokens": ["Wie", "see\u00b7lig", "acht", "ich", "mich", "/", "mein", "Kind", "/", "bey", "dir", "zu", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "CARD", "PPER", "PRF", "$(", "PPOSAT", "NN", "$(", "APPR", "PPER", "PTKZU", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein Ku\u00df von deinem Mund ist ja mein Nectar-Wein!", "tokens": ["Ein", "Ku\u00df", "von", "dei\u00b7nem", "Mund", "ist", "ja", "mein", "Nec\u00b7ta\u00b7rWein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "VAFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der zarten Armen Band/ so dich und mich verstrickt/", "tokens": ["Der", "zar\u00b7ten", "Ar\u00b7men", "Band", "/", "so", "dich", "und", "mich", "ver\u00b7strickt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$(", "ADV", "PPER", "KON", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Hat Zunder heisser Lust dem Hertzen eingedr\u00fcckt.", "tokens": ["Hat", "Zun\u00b7der", "heis\u00b7ser", "Lust", "dem", "Hert\u00b7zen", "ein\u00b7ge\u00b7dr\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "ADJA", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Die Worte fehlen mir/ mein Engel und mein Licht/", "tokens": ["Die", "Wor\u00b7te", "feh\u00b7len", "mir", "/", "mein", "En\u00b7gel", "und", "mein", "Licht", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$(", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du weist da\u00df Liebenden Beredsamkeit gebricht/", "tokens": ["Du", "weist", "da\u00df", "Lie\u00b7ben\u00b7den", "Be\u00b7red\u00b7sam\u00b7keit", "ge\u00b7bricht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOUS", "NN", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Zu melden diese Glut/ so mir das Blut erhitzt/", "tokens": ["Zu", "mel\u00b7den", "die\u00b7se", "Glut", "/", "so", "mir", "das", "Blut", "er\u00b7hitzt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "PDAT", "NN", "$(", "ADV", "PPER", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Zu nennen diesen Brand/ in dem die Seele schwitzt.", "tokens": ["Zu", "nen\u00b7nen", "die\u00b7sen", "Brand", "/", "in", "dem", "die", "See\u00b7le", "schwitzt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "PDAT", "NN", "$(", "APPR", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Mein Trost auf dieser Welt und Freundin der nichts gleich/", "tokens": ["Mein", "Trost", "auf", "die\u00b7ser", "Welt", "und", "Freun\u00b7din", "der", "nichts", "gleich", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PDAT", "NN", "KON", "NN", "ART", "PIS", "ADV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Venus ladet mich heut in ihr Wollust-Reich/", "tokens": ["Die", "Ve\u00b7nus", "la\u00b7det", "mich", "heut", "in", "ihr", "Wol\u00b7lust\u00b7Reich", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sie wird bey solchem Mahl die beste Wirthin seyn/", "tokens": ["Sie", "wird", "bey", "sol\u00b7chem", "Mahl", "die", "bes\u00b7te", "Wirt\u00b7hin", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PIAT", "NN", "ART", "ADJA", "NN", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und mit liebreicher Kost Hertz und Gem\u00fcth erfreun.", "tokens": ["Und", "mit", "lieb\u00b7rei\u00b7cher", "Kost", "Hertz", "und", "Ge\u00b7m\u00fcth", "er\u00b7freun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "NN", "KON", "NN", "VVINF", "$."], "meter": "--++-++--+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.9": {"line.1": {"text": "Komm sch\u00f6nste Perlemuth/ mein Hertze klopfft nach dir/", "tokens": ["Komm", "sch\u00f6ns\u00b7te", "Per\u00b7le\u00b7muth", "/", "mein", "Hert\u00b7ze", "klopfft", "nach", "dir", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "$(", "PPOSAT", "VVFIN", "VVFIN", "APPR", "PPER", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie eine Uhr sonst schl\u00e4gt/ voll Hoffen/ voll Begier.", "tokens": ["Wie", "ei\u00b7ne", "Uhr", "sonst", "schl\u00e4gt", "/", "voll", "Hof\u00b7fen", "/", "voll", "Be\u00b7gier", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADV", "VVFIN", "$(", "ADJD", "NN", "$(", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Es hat Leander nicht die Hero so umbfast/", "tokens": ["Es", "hat", "Le\u00b7an\u00b7der", "nicht", "die", "He\u00b7ro", "so", "umb\u00b7fast", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NE", "PTKNEG", "ART", "NN", "ADV", "ADV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Als ich dich k\u00fcssen will du h\u00f6chst-verlangter Gast.", "tokens": ["Als", "ich", "dich", "k\u00fcs\u00b7sen", "will", "du", "h\u00f6chst\u00b7ver\u00b7lang\u00b7ter", "Gast", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVINF", "VMFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}