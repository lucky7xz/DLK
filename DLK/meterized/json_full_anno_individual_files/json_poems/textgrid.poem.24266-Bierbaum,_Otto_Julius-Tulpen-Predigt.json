{"textgrid.poem.24266": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Tulpen-Predigt", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Fenster auf! Es hat der Fr\u00fchling", "tokens": ["Fens\u00b7ter", "auf", "!", "Es", "hat", "der", "Fr\u00fch\u00b7ling"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PTKVZ", "$.", "PPER", "VAFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Endlich wieder seine Zeit.", "tokens": ["End\u00b7lich", "wie\u00b7der", "sei\u00b7ne", "Zeit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Alle Blumen m\u00fcssen bl\u00fchen,", "tokens": ["Al\u00b7le", "Blu\u00b7men", "m\u00fcs\u00b7sen", "bl\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Alle V\u00f6gel m\u00fcssen singen,", "tokens": ["Al\u00b7le", "V\u00f6\u00b7gel", "m\u00fcs\u00b7sen", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Alle M\u00e4dchen m\u00fcssen lieben,", "tokens": ["Al\u00b7le", "M\u00e4d\u00b7chen", "m\u00fcs\u00b7sen", "lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Alle Herzen werden weit.", "tokens": ["Al\u00b7le", "Her\u00b7zen", "wer\u00b7den", "weit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "M\u00e4dchen mit den s\u00fc\u00dfen Augen,", "tokens": ["M\u00e4d\u00b7chen", "mit", "den", "s\u00fc\u00b7\u00dfen", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Komm, setz dich auf meinen Scho\u00df!", "tokens": ["Komm", ",", "setz", "dich", "auf", "mei\u00b7nen", "Scho\u00df", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVIMP", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Deine H\u00e4nde mu\u00df ich k\u00fcssen,", "tokens": ["Dei\u00b7ne", "H\u00e4n\u00b7de", "mu\u00df", "ich", "k\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Deine Augen mu\u00df ich k\u00fcssen,", "tokens": ["Dei\u00b7ne", "Au\u00b7gen", "mu\u00df", "ich", "k\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Deine Lippen mu\u00df ich k\u00fcssen,", "tokens": ["Dei\u00b7ne", "Lip\u00b7pen", "mu\u00df", "ich", "k\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Denn die Freude ist zu gro\u00df.", "tokens": ["Denn", "die", "Freu\u00b7de", "ist", "zu", "gro\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "PTKA", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Sieh doch, Kind, die Tulpen haben", "tokens": ["Sieh", "doch", ",", "Kind", ",", "die", "Tul\u00b7pen", "ha\u00b7ben"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NE", "ADV", "$,", "NN", "$,", "ART", "NN", "VAFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ihre Kelche aufgemacht:", "tokens": ["Ih\u00b7re", "Kel\u00b7che", "auf\u00b7ge\u00b7macht", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Rote, gelbe und gescheckte:", "tokens": ["Ro\u00b7te", ",", "gel\u00b7be", "und", "ge\u00b7scheck\u00b7te", ":"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "KON", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Tiefe Kelche voller Gluten,", "tokens": ["Tie\u00b7fe", "Kel\u00b7che", "vol\u00b7ler", "Glu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Nichts als Sch\u00f6nheit, nichts als Liebe,", "tokens": ["Nichts", "als", "Sch\u00f6n\u00b7heit", ",", "nichts", "als", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "KOKOM", "NN", "$,", "PIS", "KOKOM", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Eine ungeheure Pracht.", "tokens": ["Ei\u00b7ne", "un\u00b7ge\u00b7heu\u00b7re", "Pracht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Kann denn irgendeiner traurig", "tokens": ["Kann", "denn", "ir\u00b7gend\u00b7ei\u00b7ner", "trau\u00b7rig"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "ADV", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Unter diesen Flammen sein?", "tokens": ["Un\u00b7ter", "die\u00b7sen", "Flam\u00b7men", "sein", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sieh: das kam aus schwarzer Erde!", "tokens": ["Sieh", ":", "das", "kam", "aus", "schwar\u00b7zer", "Er\u00b7de", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PDS", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Denke: solche Flammen schlafen", "tokens": ["Den\u00b7ke", ":", "sol\u00b7che", "Flam\u00b7men", "schla\u00b7fen"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["VVIMP", "$.", "PIAT", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Winters unter unsern F\u00fc\u00dfen!", "tokens": ["Win\u00b7ters", "un\u00b7ter", "un\u00b7sern", "F\u00fc\u00b7\u00dfen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Nur die Liebe schl\u00e4ft nie ein.", "tokens": ["Nur", "die", "Lie\u00b7be", "schl\u00e4ft", "nie", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Glaube, M\u00e4dchen, an die Erde,", "tokens": ["Glau\u00b7be", ",", "M\u00e4d\u00b7chen", ",", "an", "die", "Er\u00b7de", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Weil sie voller Liebe ist,", "tokens": ["Weil", "sie", "vol\u00b7ler", "Lie\u00b7be", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJA", "NN", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sind wir doch aus ihr geboren,", "tokens": ["Sind", "wir", "doch", "aus", "ihr", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "APPR", "PPER", "VVPP", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Wie die Blumen aus dem Beete.", "tokens": ["Wie", "die", "Blu\u00b7men", "aus", "dem", "Bee\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Schlechtes Kind, das seiner Mutter", "tokens": ["Schlech\u00b7tes", "Kind", ",", "das", "sei\u00b7ner", "Mut\u00b7ter"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "PRELS", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wunderreichen Scho\u00df vergi\u00dft.", "tokens": ["Wun\u00b7der\u00b7rei\u00b7chen", "Scho\u00df", "ver\u00b7gi\u00dft", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "La\u00df die Blinden ihre Augen", "tokens": ["La\u00df", "die", "Blin\u00b7den", "ih\u00b7re", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "ART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In das Himmlische verdrehn.", "tokens": ["In", "das", "Himm\u00b7li\u00b7sche", "ver\u00b7drehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Du bewu\u00dftes Kind der Erde,", "tokens": ["Du", "be\u00b7wu\u00df\u00b7tes", "Kind", "der", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Reich wie sie an Saft und Kr\u00e4ften,", "tokens": ["Reich", "wie", "sie", "an", "Saft", "und", "Kr\u00e4f\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "PPER", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wohlgetane, Starke, Sch\u00f6ne,", "tokens": ["Wohl\u00b7ge\u00b7ta\u00b7ne", ",", "Star\u00b7ke", ",", "Sch\u00f6\u00b7ne", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Du sollst in die Blumen sehn.", "tokens": ["Du", "sollst", "in", "die", "Blu\u00b7men", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.7": {"line.1": {"text": "Alles, was das reiche Leben", "tokens": ["Al\u00b7les", ",", "was", "das", "rei\u00b7che", "Le\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PIS", "$,", "PRELS", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dir bestimmt hat, M\u00e4dchen, ruht", "tokens": ["Dir", "be\u00b7stimmt", "hat", ",", "M\u00e4d\u00b7chen", ",", "ruht"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word"], "pos": ["PPER", "VVPP", "VAFIN", "$,", "NN", "$,", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Auch in diesen Glutenkelchen,", "tokens": ["Auch", "in", "die\u00b7sen", "Glu\u00b7ten\u00b7kel\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und es meints die Mutter Erde", "tokens": ["Und", "es", "meints", "die", "Mut\u00b7ter", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "ADV", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Mit den liebetreuen Kindern", "tokens": ["Mit", "den", "lie\u00b7be\u00b7treu\u00b7en", "Kin\u00b7dern"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Immer, M\u00e4dchen, immer gut.", "tokens": ["Im\u00b7mer", ",", "M\u00e4d\u00b7chen", ",", "im\u00b7mer", "gut", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "$,", "NN", "$,", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Liebe ist das Wort der Worte,", "tokens": ["Lie\u00b7be", "ist", "das", "Wort", "der", "Wor\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Liebe ist des Lebens Wort;", "tokens": ["Lie\u00b7be", "ist", "des", "Le\u00b7bens", "Wort", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wei\u00dft du das in deinem Herzen,", "tokens": ["Wei\u00dft", "du", "das", "in", "dei\u00b7nem", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PDS", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wei\u00dft du das in deinen Sinnen,", "tokens": ["Wei\u00dft", "du", "das", "in", "dei\u00b7nen", "Sin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PDS", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Dann kann nichts dich \u00fcberwinden,", "tokens": ["Dann", "kann", "nichts", "dich", "\u00fc\u00b7berw\u00b7in\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIS", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Deine Mutter hilft dir fort.", "tokens": ["Dei\u00b7ne", "Mut\u00b7ter", "hilft", "dir", "fort", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Lacht mein M\u00e4dchen? Lache, lache,", "tokens": ["Lacht", "mein", "M\u00e4d\u00b7chen", "?", "La\u00b7che", ",", "la\u00b7che", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$.", "NN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Liebes M\u00e4dchen, lach mich aus!", "tokens": ["Lie\u00b7bes", "M\u00e4d\u00b7chen", ",", "lach", "mich", "aus", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Weiser ist dein klares Lachen", "tokens": ["Wei\u00b7ser", "ist", "dein", "kla\u00b7res", "La\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Als mein Predigen und Dichten,", "tokens": ["Als", "mein", "Pre\u00b7di\u00b7gen", "und", "Dich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "--+---+-", "measure": "anapaest.init"}, "line.5": {"text": "Sch\u00f6ner ist dein liebes Lachen", "tokens": ["Sch\u00f6\u00b7ner", "ist", "dein", "lie\u00b7bes", "La\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Als ein ganzer Tulpenstrau\u00df.", "tokens": ["Als", "ein", "gan\u00b7zer", "Tul\u00b7pen\u00b7strau\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Einen Ku\u00df! Dann in den Garten,", "tokens": ["Ei\u00b7nen", "Ku\u00df", "!", "Dann", "in", "den", "Gar\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In die Flammen gelb und rot!", "tokens": ["In", "die", "Flam\u00b7men", "gelb", "und", "rot", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dankbar treue Erdenkinder", "tokens": ["Dank\u00b7bar", "treu\u00b7e", "Er\u00b7den\u00b7kin\u00b7der"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wollen wir den Tag genie\u00dfen:", "tokens": ["Wol\u00b7len", "wir", "den", "Tag", "ge\u00b7nie\u00b7\u00dfen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct