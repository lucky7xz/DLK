{"dta.poem.18338": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "\u201e\u1f14\u03c3\u03c3\u03b5\u03c4\u03b1\u03b9 \u1f23\u03bc\u03b1\u03f1!\u201c", "genre": "Lyrik", "period": "N.A.", "pub_year": "1886", "urn": "urn:nbn:de:kobv:b4-200905192665", "language": ["de:0.99"], "booktitle": "Holz, Arno: Das Buch der Zeit. Lieder eines Modernen. Z\u00fcrich, 1886."}, "poem": {"stanza.1": {"line.1": {"text": "Wir dienen dir und du belohnst uns nie,", "tokens": ["Wir", "die\u00b7nen", "dir", "und", "du", "be\u00b7lohnst", "uns", "nie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "KON", "PPER", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Denn auch noch heut ist unsre deutsche Freiheit", "tokens": ["Denn", "auch", "noch", "heut", "ist", "uns\u00b7re", "deut\u00b7sche", "Frei\u00b7heit"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "ADV", "VAFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nur eine schwarzrothgoldne Phantasie!", "tokens": ["Nur", "ei\u00b7ne", "schwarz\u00b7roth\u00b7gold\u00b7ne", "Phan\u00b7ta\u00b7sie", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}