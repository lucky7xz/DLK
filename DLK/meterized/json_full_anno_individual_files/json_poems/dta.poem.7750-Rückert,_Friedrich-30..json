{"dta.poem.7750": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "30.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die beiden Palmen, die dort alternd stehn beisammen,", "tokens": ["Die", "bei\u00b7den", "Pal\u00b7men", ",", "die", "dort", "al\u00b7ternd", "stehn", "bei\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "$,", "PRELS", "ADV", "ADJD", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sie danken nicht ihr Heil dem Grund aus dem sie stammen;", "tokens": ["Sie", "dan\u00b7ken", "nicht", "ihr", "Heil", "dem", "Grund", "aus", "dem", "sie", "stam\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "PPOSAT", "NN", "ART", "NN", "APPR", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Sie danken es dem Hauch des Himmels, Poesie;", "tokens": ["Sie", "dan\u00b7ken", "es", "dem", "Hauch", "des", "Him\u00b7mels", ",", "Poe\u00b7sie", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "ART", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sie stehn, weil einmal sprach ein Dichter scheidend hie:", "tokens": ["Sie", "stehn", ",", "weil", "ein\u00b7mal", "sprach", "ein", "Dich\u00b7ter", "schei\u00b7dend", "hie", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "ADV", "VVFIN", "ART", "NN", "ADJD", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ihr beiden Palmen, gebt mir euern Abschiedsgru\u00df,", "tokens": ["Ihr", "bei\u00b7den", "Pal\u00b7men", ",", "gebt", "mir", "eu\u00b7ern", "Ab\u00b7schieds\u00b7gru\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "NN", "$,", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil ich von allem, was mir lieb ist, scheiden mu\u00df.", "tokens": ["Weil", "ich", "von", "al\u00b7lem", ",", "was", "mir", "lieb", "ist", ",", "schei\u00b7den", "mu\u00df", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PIS", "$,", "PWS", "PPER", "ADJD", "VAFIN", "$,", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Nie rastet das Geschick, zu scheiden und zu trennen", "tokens": ["Nie", "ras\u00b7tet", "das", "Ge\u00b7schick", ",", "zu", "schei\u00b7den", "und", "zu", "tren\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "PTKZU", "VVINF", "KON", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Auf Erden alle, die sich lieben und sich kennen.", "tokens": ["Auf", "Er\u00b7den", "al\u00b7le", ",", "die", "sich", "lie\u00b7ben", "und", "sich", "ken\u00b7nen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PIS", "$,", "PRELS", "PRF", "VVINF", "KON", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Ihr aber bleibet ungeschieden mir, ihr beiden!", "tokens": ["Ihr", "a\u00b7ber", "blei\u00b7bet", "un\u00b7ge\u00b7schie\u00b7den", "mir", ",", "ihr", "bei\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "ADJD", "PPER", "$,", "PPER", "PIAT", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch wird das Ungl\u00fcck auch einst kommen, euch zu scheiden.", "tokens": ["Doch", "wird", "das", "Un\u00b7gl\u00fcck", "auch", "einst", "kom\u00b7men", ",", "euch", "zu", "schei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "ADV", "ADV", "VVINF", "$,", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Der Dichter sprachs, und gieng den schweren Abschiedsgang,", "tokens": ["Der", "Dich\u00b7ter", "sprachs", ",", "und", "gieng", "den", "schwe\u00b7ren", "Ab\u00b7schieds\u00b7gang", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "KON", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch in den L\u00fcften hier blieb seines Liedes Klang.", "tokens": ["Doch", "in", "den", "L\u00fcf\u00b7ten", "hier", "blieb", "sei\u00b7nes", "Lie\u00b7des", "Klang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADV", "VVFIN", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Es gieng von Ohr zu Ohr das Lied, von Mund zu Munde,", "tokens": ["Es", "gieng", "von", "Ohr", "zu", "Ohr", "das", "Lied", ",", "von", "Mund", "zu", "Mun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "APPR", "NN", "ART", "NN", "$,", "APPR", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und nie droht' Axt und Beil dem heil'gen Palmenbunde.", "tokens": ["Und", "nie", "droht'", "Axt", "und", "Beil", "dem", "heil'\u00b7gen", "Pal\u00b7men\u00b7bun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "NN", "KON", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Da kam der K\u00f6nig her auf seinem Siegeszug,", "tokens": ["Da", "kam", "der", "K\u00f6\u00b7nig", "her", "auf", "sei\u00b7nem", "Sie\u00b7ges\u00b7zug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APZR", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Palme stand im Weg dem Wagen, der ihn trug.", "tokens": ["Die", "Pal\u00b7me", "stand", "im", "Weg", "dem", "Wa\u00b7gen", ",", "der", "ihn", "trug", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Des Beiles Sch\u00e4rfe war schon angelegt dem Fu\u00df;", "tokens": ["Des", "Bei\u00b7les", "Sch\u00e4r\u00b7fe", "war", "schon", "an\u00b7ge\u00b7legt", "dem", "Fu\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VAFIN", "ADV", "VVPP", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Fuhrmann aber sprach des Dichters Abschiedsgru\u00df:", "tokens": ["Der", "Fuhr\u00b7mann", "a\u00b7ber", "sprach", "des", "Dich\u00b7ters", "Ab\u00b7schieds\u00b7gru\u00df", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.10": {"line.1": {"text": "Ihr Palmen bleibet ungeschieden mir, ihr beiden!", "tokens": ["Ihr", "Pal\u00b7men", "blei\u00b7bet", "un\u00b7ge\u00b7schie\u00b7den", "mir", ",", "ihr", "bei\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJD", "PPER", "$,", "PPER", "PIAT", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch wird das Ungl\u00fcck auch schon kommen euch zu scheiden.", "tokens": ["Doch", "wird", "das", "Un\u00b7gl\u00fcck", "auch", "schon", "kom\u00b7men", "euch", "zu", "schei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "ADV", "ADV", "VVFIN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.11": {"line.1": {"text": "Das war der beiden Heil; der K\u00f6nig rief: halt ein!", "tokens": ["Das", "war", "der", "bei\u00b7den", "Heil", ";", "der", "K\u00f6\u00b7nig", "rief", ":", "halt", "ein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "PIAT", "NN", "$.", "ART", "NN", "VVFIN", "$.", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich will das Ungl\u00fcck, das sie scheiden soll, nicht seyn.", "tokens": ["Ich", "will", "das", "Un\u00b7gl\u00fcck", ",", "das", "sie", "schei\u00b7den", "soll", ",", "nicht", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "$,", "PRELS", "PPER", "VVINF", "VMFIN", "$,", "PTKNEG", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.12": {"line.1": {"text": "Dem Dichterworte mag zur Ehre sich bequemen", "tokens": ["Dem", "Dich\u00b7ter\u00b7wor\u00b7te", "mag", "zur", "Eh\u00b7re", "sich", "be\u00b7que\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "APPRART", "NN", "PRF", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mein Siegeswagen wol, den Umweg hier zu nehmen.", "tokens": ["Mein", "Sie\u00b7ges\u00b7wa\u00b7gen", "wol", ",", "den", "Um\u00b7weg", "hier", "zu", "neh\u00b7men", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "$,", "ART", "NN", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.13": {"line.1": {"text": "Ihr aber steht, bis euch Sturm oder Alter bricht!", "tokens": ["Ihr", "a\u00b7ber", "steht", ",", "bis", "euch", "Sturm", "o\u00b7der", "Al\u00b7ter", "bricht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "$,", "KOUS", "PPER", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Das mag das Ungl\u00fcck seyn, von dem der Dichter spricht.", "tokens": ["Das", "mag", "das", "Un\u00b7gl\u00fcck", "seyn", ",", "von", "dem", "der", "Dich\u00b7ter", "spricht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ART", "NN", "VAINF", "$,", "APPR", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}