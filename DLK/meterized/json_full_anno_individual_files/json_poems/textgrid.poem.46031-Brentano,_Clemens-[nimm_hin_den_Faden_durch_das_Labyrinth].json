{"textgrid.poem.46031": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "[nimm hin den Faden durch das Labyrinth]", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nimm hin den Faden durch das Labyrinth,", "tokens": ["Nimm", "hin", "den", "Fa\u00b7den", "durch", "das", "La\u00b7by\u00b7rinth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Das schrecklicher als jenes alte ist,", "tokens": ["Das", "schreck\u00b7li\u00b7cher", "als", "je\u00b7nes", "al\u00b7te", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "KOKOM", "PDAT", "ADJA", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "In dessen ausweglosem Pfadgewind'", "tokens": ["In", "des\u00b7sen", "aus\u00b7weg\u00b7lo\u00b7sem", "Pfad\u00b7ge\u00b7wind'"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PRELAT", "ADJA", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Ein scheu\u00dflich Ungeheur den Wandrer fri\u00dft,", "tokens": ["Ein", "scheu\u00df\u00b7lich", "Un\u00b7ge\u00b7heur", "den", "Wand\u00b7rer", "fri\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Denn hier mein Freund! schreckt dich kein greulich Tier,", "tokens": ["Denn", "hier", "mein", "Freund", "!", "schreckt", "dich", "kein", "greu\u00b7lich", "Tier", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPOS