{"dta.poem.2444": {"metadata": {"author": {"name": "Neumark, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Si Deus pro nobis, quis contra nos?", "genre": "Lyrik", "period": "N.A.", "pub_year": "1652", "urn": "urn:nbn:de:kobv:b4-20428-0", "language": ["de:0.99"], "booktitle": "Neumark, Georg: Poetisch- und Musikalisches Lustw\u00e4ldchen. Hamburg, 1652."}, "poem": {"stanza.1": {"line.1": {"text": "Gleich wie ein matter Hirsch ge\u00e4ngstigt von den", "tokens": ["Gleich", "wie", "ein", "mat\u00b7ter", "Hirsch", "ge\u00b7\u00e4ngs\u00b7tigt", "von", "den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "ART", "ADJA", "NN", "VVPP", "APPR", "ART"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Schlangen/", "tokens": ["Schlan\u00b7gen", "/"], "token_info": ["word", "punct"], "pos": ["NN", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Nach einem Bronnen eylt mit Wind-gleich-schnellem", "tokens": ["Nach", "ei\u00b7nem", "Bron\u00b7nen", "eylt", "mit", "Win\u00b7dgleich\u00b7schnel\u00b7lem"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Lauff':", "tokens": ["Lauff'", ":"], "token_info": ["word", "punct"], "pos": ["VVIMP", "$."], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Also auch liebster Gott erseufftz\u2019 ich Himmel auff/", "tokens": ["Al\u00b7so", "auch", "liebs\u00b7ter", "Gott", "er\u00b7seufft\u00b7z'", "ich", "Him\u00b7mel", "auff", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJA", "NN", "VVFIN", "PPER", "NN", "APPR", "$("], "meter": "+--+-+--+-+-+", "measure": "iambic.hexa.invert"}, "line.6": {"text": "Und hab\u2019 in meiner Noht nach dir mein gro\u00df Verlangen", "tokens": ["Und", "hab'", "in", "mei\u00b7ner", "N