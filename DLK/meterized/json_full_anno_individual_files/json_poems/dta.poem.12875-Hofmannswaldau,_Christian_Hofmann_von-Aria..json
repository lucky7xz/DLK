{"dta.poem.12875": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Aria.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mu\u00df ich denn nichts, als klage-lieder singen,", "tokens": ["Mu\u00df", "ich", "denn", "nichts", ",", "als", "kla\u00b7ge\u00b7lie\u00b7der", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "PIS", "$,", "KOUS", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und nur ein spiel der sternen seyn?", "tokens": ["Und", "nur", "ein", "spiel", "der", "ster\u00b7nen", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "VVFIN", "ART", "ADJA", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Soll meine ruh mir selber unruh bringen?", "tokens": ["Soll", "mei\u00b7ne", "ruh", "mir", "sel\u00b7ber", "un\u00b7ruh", "brin\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "PPER", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Erndt ich bey rosen dornen ein?", "tokens": ["Erndt", "ich", "bey", "ro\u00b7sen", "dor\u00b7nen", "ein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ach, verh\u00e4ngni\u00df! \u00e4ndre dich,", "tokens": ["Ach", ",", "ver\u00b7h\u00e4ng\u00b7ni\u00df", "!", "\u00e4nd\u00b7re", "dich", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$,", "VVFIN", "$.", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Sonst wird dein schlu\u00df Melindens todes-stich.", "tokens": ["Sonst", "wird", "dein", "schlu\u00df", "Me\u00b7lin\u00b7dens", "to\u00b7des\u00b7stich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "NE", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Es zeigt sich zwar die angenehmste sonne;", "tokens": ["Es", "zeigt", "sich", "zwar", "die", "an\u00b7ge\u00b7nehms\u00b7te", "son\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Doch stehn die wolcken stets dabey:", "tokens": ["Doch", "stehn", "die", "wol\u00b7cken", "stets", "da\u00b7bey", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADV", "PAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die nacht der angst steht immer bey der wonne.", "tokens": ["Die", "nacht", "der", "angst", "steht", "im\u00b7mer", "bey", "der", "won\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mein gl\u00fcck ist lauter tyranney.", "tokens": ["Mein", "gl\u00fcck", "ist", "lau\u00b7ter", "ty\u00b7ran\u00b7ney", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ach, verh\u00e4ngni\u00df! \u00e4ndre dich,", "tokens": ["Ach", ",", "ver\u00b7h\u00e4ng\u00b7ni\u00df", "!", "\u00e4nd\u00b7re", "dich", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$,", "VVFIN", "$.", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Sonst wird dein schlu\u00df Melindens todes-stich.", "tokens": ["Sonst", "wird", "dein", "schlu\u00df", "Me\u00b7lin\u00b7dens", "to\u00b7des\u00b7stich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "NE", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Kan ich das licht nicht ohne nebel haben,", "tokens": ["Kan", "ich", "das", "licht", "nicht", "oh\u00b7ne", "ne\u00b7bel", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "PTKNEG", "APPR", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So la\u00df mich in dem schatten stehn.", "tokens": ["So", "la\u00df", "mich", "in", "dem", "schat\u00b7ten", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "APPR", "ART", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Will deine gunst nicht die gefahr begraben,", "tokens": ["Will", "dei\u00b7ne", "gunst", "nicht", "die", "ge\u00b7fahr", "be\u00b7gra\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "PTKNEG", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "So mag ich nicht auf marmel gehn?", "tokens": ["So", "mag", "ich", "nicht", "auf", "mar\u00b7mel", "gehn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PTKNEG", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+"