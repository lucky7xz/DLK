{"textgrid.poem.32946": {"metadata": {"author": {"name": "Flaischlen, C\u00e4sar", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich hab's genug! ich mag nicht mehr!", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich hab's genug! ich mag nicht mehr!", "tokens": ["Ich", "hab's", "ge\u00b7nug", "!", "ich", "mag", "nicht", "mehr", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "$.", "PPER", "VMFIN", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "zum Teufel mit all dem Plunder!", "tokens": ["zum", "Teu\u00b7fel", "mit", "all", "dem", "Plun\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "PIAT", "ART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "ein Gott im Himmel allenfalls", "tokens": ["ein", "Gott", "im", "Him\u00b7mel", "al\u00b7len\u00b7falls"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "h\u00fclf noch mit einem Wunder!", "tokens": ["h\u00fclf", "noch", "mit", "ei\u00b7nem", "Wun\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ich hab gek\u00e4mpft und ich h\u00e4tt gesiegt ...", "tokens": ["Ich", "hab", "ge\u00b7k\u00e4mpft", "und", "ich", "h\u00e4tt", "ge\u00b7siegt", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "KON", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "doch