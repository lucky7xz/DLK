{"textgrid.poem.32899": {"metadata": {"author": {"name": "Wei\u00dfe, Christian Felix", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wenn sich ein Narr im Pompe zeiget,", "genre": "verse", "period": "N.A.", "pub_year": 1765, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn sich ein Narr im Pompe zeiget,", "tokens": ["Wenn", "sich", "ein", "Narr", "im", "Pom\u00b7pe", "zei\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der P\u00f6bel sich dem\u00fcthig beuget,", "tokens": ["Der", "P\u00f6\u00b7bel", "sich", "de\u00b7m\u00fct\u00b7hig", "beu\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sich niemals glaubet satt zu sehn:", "tokens": ["Sich", "nie\u00b7mals", "glau\u00b7bet", "satt", "zu", "sehn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "VVFIN", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das la\u00df ich gern geschehn.", "tokens": ["Das", "la\u00df", "ich", "gern", "ge\u00b7schehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Doch wenn bey magern plumpen Scherzen", "tokens": ["Doch", "wenn", "bey", "ma\u00b7gern", "plum\u00b7pen", "Scher\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "APPR", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ein lauter Hof von ganzem Herzen", "tokens": ["Ein", "lau\u00b7ter", "Hof", "von", "gan\u00b7zem", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PIAT", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Sein Bravo! schreyt, schreyt: das war sch\u00f6n!", "tokens": ["Sein", "Bra\u00b7vo", "!", "schreyt", ",", "schreyt", ":", "das", "war", "sch\u00f6n", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "VVFIN", "$,", "VVFIN", "$.", "PDS", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Das ist nicht auszustehn!", "tokens": ["Das", "ist", "nicht", "aus\u00b7zu\u00b7stehn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wenn Fr\u00e4ulein, bey der Gans gebohren,", "tokens": ["Wenn", "Fr\u00e4u\u00b7lein", ",", "bey", "der", "Gans", "ge\u00b7boh\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "$,", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Im st\u00e4dtischen galanten Thoren", "tokens": ["Im", "st\u00e4d\u00b7ti\u00b7schen", "ga\u00b7lan\u00b7ten", "Tho\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Den Ph\u00f6nix aller Junker sehn,", "tokens": ["Den", "Ph\u00f6\u00b7nix", "al\u00b7ler", "Jun\u00b7ker", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das la\u00df ich gern geschehn!", "tokens": ["Das", "la\u00df", "ich", "gern", "ge\u00b7schehn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Doch wenn hochweis erfahrne Damen", "tokens": ["Doch", "wenn", "hoch\u00b7weis", "er\u00b7fahr\u00b7ne", "Da\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Das beste Herz um Westen, Namen", "tokens": ["Das", "bes\u00b7te", "Herz", "um", "Wes\u00b7ten", ",", "Na\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "$,", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und fein fresiertes Haar verschm\u00e4hn,", "tokens": ["Und", "fein", "fre\u00b7sier\u00b7tes", "Haar", "ver\u00b7schm\u00e4hn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Das ist nicht auszustehn!", "tokens": ["Das", "ist", "nicht", "aus\u00b7zu\u00b7stehn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Wenn junge Krieger vor ihr Leben,", "tokens": ["Wenn", "jun\u00b7ge", "Krie\u00b7ger", "vor", "ihr", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das sie zu sehr empfinden, beben,", "tokens": ["Das", "sie", "zu", "sehr", "emp\u00b7fin\u00b7den", ",", "be\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PDS", "PPER", "PTKA", "ADV", "VVFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Mit blasser Stirn zu Felde gehn:", "tokens": ["Mit", "blas\u00b7ser", "Stirn", "zu", "Fel\u00b7de", "gehn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das la\u00df ich gern geschehn.", "tokens": ["Das", "la\u00df", "ich", "gern", "ge\u00b7schehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Doch wenn sie ohne Graun und Zagen", "tokens": ["Doch", "wenn", "sie", "oh\u00b7ne", "Graun", "und", "Za\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Sich wild um Concubinen schlagen,", "tokens": ["Sich", "wild", "um", "Con\u00b7cu\u00b7bi\u00b7nen", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Durch Brunst und Wein den Tod erflehn:", "tokens": ["Durch", "Brunst", "und", "Wein", "den", "Tod", "er\u00b7flehn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Das ist nicht auszustehn!", "tokens": ["Das", "ist", "nicht", "aus\u00b7zu\u00b7stehn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Wenn oft ein Mann beym z\u00e4nkschen Weibe", "tokens": ["Wenn", "oft", "ein", "Mann", "beym", "z\u00e4nk\u00b7schen", "Wei\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "NN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr M\u00e4dchen sich zum Zeitvertreibe,", "tokens": ["Ihr", "M\u00e4d\u00b7chen", "sich", "zum", "Zeit\u00b7ver\u00b7trei\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PRF", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "So ungerecht es ist, ersehn:", "tokens": ["So", "un\u00b7ge\u00b7recht", "es", "ist", ",", "er\u00b7sehn", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "VAFIN", "$,", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das la\u00df ich noch geschehn.", "tokens": ["Das", "la\u00df", "ich", "noch", "ge\u00b7schehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Doch wenn ein Mann bey einer Sch\u00f6nen,", "tokens": ["Doch", "wenn", "ein", "Mann", "bey", "ei\u00b7ner", "Sch\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der Tugend Zier, des J\u00fcnglings Sehnen,", "tokens": ["Der", "Tu\u00b7gend", "Zier", ",", "des", "J\u00fcng\u00b7lings", "Seh\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Sucht plumpe Dirnen auszusp\u00e4hn,", "tokens": ["Sucht", "plum\u00b7pe", "Dir\u00b7nen", "aus\u00b7zu\u00b7sp\u00e4hn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Das ist nicht auszustehn!", "tokens": ["Das", "ist", "nicht", "aus\u00b7zu\u00b7stehn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Wenn M\u00e4dchen, Herzen zu ber\u00fccken,", "tokens": ["Wenn", "M\u00e4d\u00b7chen", ",", "Her\u00b7zen", "zu", "be\u00b7r\u00fc\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "$,", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sich tagelang vorm Spiegel schm\u00fccken,", "tokens": ["Sich", "ta\u00b7ge\u00b7lang", "vorm", "Spie\u00b7gel", "schm\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und auch was menschliches versehn,", "tokens": ["Und", "auch", "was", "menschli\u00b7ches", "ver\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PWS", "PIS", "VVINF", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Das la\u00df ich gern geschehn.", "tokens": ["Das", "la\u00df", "ich", "gern", "ge\u00b7schehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Doch wenn es alte Jungfern wagen,", "tokens": ["Doch", "wenn", "es", "al\u00b7te", "Jung\u00b7fern", "wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Den ganzen Lenz an sich zu tragen,", "tokens": ["Den", "gan\u00b7zen", "Lenz", "an", "sich", "zu", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PRF", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Zu siegen sich noch unterstehn,", "tokens": ["Zu", "sie\u00b7gen", "sich", "noch", "un\u00b7ter\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PRF", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Das ist nicht auszustehn!", "tokens": ["Das", "ist", "nicht", "aus\u00b7zu\u00b7stehn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Wenn Juden niedertr\u00e4chtig sinnen,", "tokens": ["Wenn", "Ju\u00b7den", "nie\u00b7der\u00b7tr\u00e4ch\u00b7tig", "sin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Durch schlauen Wucher zu gewinnen,", "tokens": ["Durch", "schlau\u00b7en", "Wu\u00b7cher", "zu", "ge\u00b7win\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Auf Vortheil, nicht auf Ehre sehn,", "tokens": ["Auf", "Vor\u00b7theil", ",", "nicht", "auf", "Eh\u00b7re", "sehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PTKNEG", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das la\u00df 