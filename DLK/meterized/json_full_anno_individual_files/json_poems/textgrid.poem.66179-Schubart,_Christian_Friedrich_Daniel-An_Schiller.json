{"textgrid.poem.66179": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "An Schiller", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.71", "af:0.28"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dank dir, ", "tokens": ["Dank", "dir", ","], "token_info": ["word", "word", "punct"], "pos": ["APPR", "PPER", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.2": {"text": "Die deinem Gesang entquoll!", "tokens": ["Die", "dei\u00b7nem", "Ge\u00b7sang", "ent\u00b7quoll", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Meines Berges Genius, der Riese,", "tokens": ["Mei\u00b7nes", "Ber\u00b7ges", "Ge\u00b7nius", ",", "der", "Rie\u00b7se", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NE", "$,", "ART", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Ein Sch\u00e4tzer hohen Sangs,", "tokens": ["Ein", "Sch\u00e4t\u00b7zer", "ho\u00b7hen", "Sangs", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Lauscht' dir, da\u00df der Kolbe von Stahl", "tokens": ["Lauscht'", "dir", ",", "da\u00df", "der", "Kol\u00b7be", "von", "Stahl"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "ART", "NN", "APPR", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Entsank seiner wolkigen Rechten!", "tokens": ["Ent\u00b7sank", "sei\u00b7ner", "wol\u00b7ki\u00b7gen", "Rech\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.2": {"line.1": {"text": "Auch ich schlang deinen Gesang,", "tokens": ["Auch", "ich", "schlang", "dei\u00b7nen", "Ge\u00b7sang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Wie der Langdurstende", "tokens": ["Wie", "der", "Lang\u00b7durs\u00b7ten\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "ART", "NN"], "meter": "+--+--", "measure": "dactylic.di.plus"}, "line.3": {"text": "Mit woll\u00fcstig geschlo\u00dfnem Auge", "tokens": ["Mit", "wol\u00b7l\u00fcs\u00b7tig", "ge\u00b7schlo\u00df\u00b7nem", "Au\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJD", "ADJA", "NN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Schl\u00fcrft aus des Baches Frische.", "tokens": ["Schl\u00fcrft", "aus", "des", "Ba\u00b7ches", "Fri\u00b7sche", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.3": {"line.1": {"text": "Sah nicht des eisernen Gitters Schatten,", "tokens": ["Sah", "nicht", "des", "ei\u00b7ser\u00b7nen", "Git\u00b7ters", "Schat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Den die Sonne malt", "tokens": ["Den", "die", "Son\u00b7ne", "malt"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ART", "NN", "VVFIN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Auf meines Kerkers Boden!", "tokens": ["Auf", "mei\u00b7nes", "Ker\u00b7kers", "Bo\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "H\u00f6rte nicht Fesselgeklirr am wunden Arm.", "tokens": ["H\u00f6r\u00b7te", "nicht", "Fes\u00b7sel\u00b7ge\u00b7klirr", "am", "wun\u00b7den", "Arm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Denn du sangst!", "tokens": ["Denn", "du", "sangst", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.5": {"line.1": {"text": "Deiner Lieder Feuerstrom", "tokens": ["Dei\u00b7ner", "Lie\u00b7der", "Feu\u00b7er\u00b7strom"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "St\u00fcrzte t\u00f6nend nieder vor mir;", "tokens": ["St\u00fcrz\u00b7te", "t\u00f6\u00b7nend", "nie\u00b7der", "vor", "mir", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PTKVZ", "APPR", "PPER", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Und ich horchte seinem Wogensturze;", "tokens": ["Und", "ich", "horch\u00b7te", "sei\u00b7nem", "Wo\u00b7gen\u00b7stur\u00b7ze", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Hoch empor stieg meine Seele", "tokens": ["Hoch", "em\u00b7por", "stieg", "mei\u00b7ne", "See\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "PTKVZ", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Mit dem Funkengest\u00e4ube", "tokens": ["Mit", "dem", "Fun\u00b7ken\u00b7ge\u00b7st\u00e4u\u00b7be"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "Seiner Flut.", "tokens": ["Sei\u00b7ner", "Flut", "."], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.6": {"line.1": {"text": "Da trat vor mich ein Bote des Himmels,", "tokens": ["Da", "trat", "vor", "mich", "ein", "Bo\u00b7te", "des", "Him\u00b7mels", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PRF", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "L\u00e4chelte mir sanft und sprach:", "tokens": ["L\u00e4\u00b7chel\u00b7te", "mir", "sanft", "und", "sprach", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "KON", "VVFIN", "$."], "meter": "+---+-+", "measure": "dactylic.init"}, "line.3": {"text": "\u00bbein Bote des Himmels bin ich", "tokens": ["\u00bb", "ein", "Bo\u00b7te", "des", "Him\u00b7mels", "bin", "ich"], "token_info": ["punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "ART", "NN", "ART", "NN", "VAFIN", "PPER"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Und bringe deinem trauten ", "tokens": ["Und", "brin\u00b7ge", "dei\u00b7nem", "trau\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Den du so hei\u00df und br\u00fcderlich liebst,", "tokens": ["Den", "du", "so", "hei\u00df", "und", "br\u00fc\u00b7der\u00b7lich", "liebst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADJD", "KON", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "An dessen Feuerbusen du j\u00fcngst lagst,", "tokens": ["An", "des\u00b7sen", "Feu\u00b7er\u00b7bu\u00b7sen", "du", "j\u00fcngst", "lagst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und lange d'ran weintest,", "tokens": ["Und", "lan\u00b7ge", "d'\u00b7ran", "wein\u00b7test", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PAV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ja deinem trauten ", "tokens": ["Ja", "dei\u00b7nem", "trau\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["PTKANT", "PPOSAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.9": {"text": "Gottes ", "tokens": ["Got\u00b7tes"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.10": {"text": "Da\u00df ihn Laura's Zauberblick", "tokens": ["Da\u00df", "ihn", "Lau\u00b7ra's", "Zau\u00b7berb\u00b7lick"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "NE", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Nicht lockt' in der Wollust Lache;", "tokens": ["Nicht", "lockt'", "in", "der", "Wol\u00b7lust", "La\u00b7che", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.12": {"text": "Da\u00df er in Laura's flimmendem Auge", "tokens": ["Da\u00df", "er", "in", "Lau\u00b7ra's", "flim\u00b7men\u00b7dem", "Au\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "NE", "ADJA", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.13": {"text": "\u2013 Gott sah!", "tokens": ["\u2013", "Gott", "sah", "!"], "token_info": ["punct", "word", "word", "punct"], "pos": ["$(", "NN", "VVFIN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.14": {"text": "Da\u00df er muthig z\u00fcrnt", "tokens": ["Da\u00df", "er", "mut\u00b7hig", "z\u00fcrnt"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "VVFIN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.15": {"text": "Dem gekr\u00f6nten Laster!", "tokens": ["Dem", "ge\u00b7kr\u00f6n\u00b7ten", "Las\u00b7ter", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.16": {"text": "Da\u00df er's k\u00f6stlicher h\u00e4lt", "tokens": ["Da\u00df", "er's", "k\u00f6st\u00b7li\u00b7cher", "h\u00e4lt"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PIS", "ADJD", "VVFIN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.17": {"text": "Menschen zu ", "tokens": ["Men\u00b7schen", "zu"], "token_info": ["word", "word"], "pos": ["NN", "PTKZU"], "meter": "+-+", "measure": "trochaic.di"}, "line.18": {"text": "Als zu ", "tokens": ["Als", "zu"], "token_info": ["word", "word"], "pos": ["KOUS", "APPR"], "meter": "+-", "measure": "trochaic.single"}, "line.19": {"text": "Da\u00df er h\u00f6rte des Weltalls Symphonie,", "tokens": ["Da\u00df", "er", "h\u00f6r\u00b7te", "des", "Welt\u00b7alls", "Sym\u00b7pho\u00b7nie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.20": {"text": "Beginnend im tausendstimmigen Einklang der Liebe,", "tokens": ["Be\u00b7gin\u00b7nend", "im", "tau\u00b7sends\u00b7tim\u00b7mi\u00b7gen", "Ein\u00b7klang", "der", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPRART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.21": {"text": "Endend im allstimmigen Einklang der Liebe!", "tokens": ["En\u00b7dend", "im", "alls\u00b7tim\u00b7mi\u00b7gen", "Ein\u00b7klang", "der", "Lie\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPRART", "ADJA", "NN", "ART", "NN", "$."], "meter": "+-+-+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.22": {"text": "Da\u00df er von seines Felsen Zacken", "tokens": ["Da\u00df", "er", "von", "sei\u00b7nes", "Fel\u00b7sen", "Za\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Die Sprache des Sturms der Natur", "tokens": ["Die", "Spra\u00b7che", "des", "Sturms", "der", "Na\u00b7tur"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "ART", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.24": {"text": "Hinunter ins menschenwogende Thal h\u00f6rte:", "tokens": ["Hin\u00b7un\u00b7ter", "ins", "men\u00b7schen\u00b7wo\u00b7gen\u00b7de", "Thal", "h\u00f6r\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+--+--+--", "measure": "iambic.tetra.relaxed"}, "line.25": {"text": "\u00bbkreaturen, erkennt ihr Gott?", "tokens": ["\u00bb", "kre\u00b7a\u00b7tu\u00b7ren", ",", "er\u00b7kennt", "ihr", "Gott", "?"], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VVINF", "$,", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.26": {"text": "Kreaturen, erkennt ihr Gott?", "tokens": ["Kre\u00b7a\u00b7tu\u00b7ren", ",", "er\u00b7kennt", "ihr", "Gott", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.27": {"text": "Da\u00df er's f\u00fcr Thorheit h\u00e4lt,", "tokens": ["Da\u00df", "er's", "f\u00fcr", "Thor\u00b7heit", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.28": {"text": "Mit hektischem Menschenodem", "tokens": ["Mit", "hek\u00b7ti\u00b7schem", "Men\u00b7sche\u00b7no\u00b7dem"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.29": {"text": "Zu hauchen in Gottes", "tokens": ["Zu", "hau\u00b7chen", "in", "Got\u00b7tes"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "VVFIN", "APPR", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.30": {"text": "Lebenden Sturmwind;", "tokens": ["Le\u00b7ben\u00b7den", "Sturm\u00b7wind", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.31": {"text": "Zu befl\u00fcgeln den ewigen Kreislauf", "tokens": ["Zu", "be\u00b7fl\u00fc\u00b7geln", "den", "e\u00b7wi\u00b7gen", "Kreis\u00b7lauf"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKZU", "VVINF", "ART", "ADJA", "NN"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.32": {"text": "Der beaugten R\u00e4der!", "tokens": ["Der", "be\u00b7aug\u00b7ten", "R\u00e4\u00b7der", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.33": {"text": "Da\u00df er beim ", "tokens": ["Da\u00df", "er", "beim"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPER", "APPRART"], "meter": "+-+", "measure": "trochaic.di"}, "line.34": {"text": "Den ", "tokens": ["Den"], "token_info": ["word"], "pos": ["ART"], "meter": "+", "measure": "single.up"}, "line.35": {"text": "Dies dank' ich deinem ", "tokens": ["Dies", "dank'", "ich", "dei\u00b7nem"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPER", "PPOSAT"], "meter": "-+-+-", "measure": "iambic.di"}, "line.36": {"text": "Und bring' ihm ", "tokens": ["Und", "bring'", "ihm"], "token_info": ["word", "word", "word"], "pos": ["KON", "VVFIN", "PPER"], "meter": "-+-", "measure": "amphibrach.single"}, "line.37": {"text": "Auch bring' ich ihm ", "tokens": ["Auch", "bring'", "ich", "ihm"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPER"], "meter": "-+-+", "measure": "iambic.di"}, "line.38": {"text": "Den Aetherstrahl des Genius zu brauchen", "tokens": ["Den", "A\u00b7e\u00b7ther\u00b7strahl", "des", "Ge\u00b7nius", "zu", "brau\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "PTKZU", "VVINF"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.39": {"text": "F\u00fcr Gott!", "tokens": ["F\u00fcr", "Gott", "!"], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.40": {"text": "F\u00fcr den Gesalbten Gottes!", "tokens": ["F\u00fcr", "den", "Ge\u00b7salb\u00b7ten", "Got\u00b7tes", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.41": {"text": "F\u00fcr's Vaterland!", "tokens": ["F\u00fcr's", "Va\u00b7ter\u00b7land", "!"], "token_info": ["word", "word", "punct"], "pos": ["NE", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.42": {"text": "Zu st\u00e4hlen seiner Br\u00fcder milchzerflo\u00dfnen Muth;", "tokens": ["Zu", "st\u00e4h\u00b7len", "sei\u00b7ner", "Br\u00fc\u00b7der", "milch\u00b7zer\u00b7flo\u00df\u00b7nen", "Muth", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Zu sprechen jenes Lebens Hoffnung", "tokens": ["Zu", "spre\u00b7chen", "je\u00b7nes