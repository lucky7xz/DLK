{"textgrid.poem.41660": {"metadata": {"author": {"name": "Baudelaire, Charles", "birth": "N.A.", "death": "N.A."}, "title": "1L: Pascal sah eine Kluft, wo er auch ging und stand.", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Pascal sah eine Kluft, wo er auch ging und stand.", "tokens": ["Pas\u00b7cal", "sah", "ei\u00b7ne", "Kluft", ",", "wo", "er", "auch", "ging", "und", "stand", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "$,", "PWAV", "PPER", "ADV", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein Abgrund ist das All: Traum, Handlung, Wort, Verlangen!", "tokens": ["Ein", "Ab\u00b7grund", "ist", "das", "All", ":", "Traum", ",", "Hand\u00b7lung", ",", "Wort", ",", "Ver\u00b7lan\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$.", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wie oft ist \u00fcber mich der Wind des Schrecks gegangen,", "tokens": ["Wie", "oft", "ist", "\u00fc\u00b7ber", "mich", "der", "Wind", "des", "Schrecks", "ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VAFIN", "APPR", "PPER", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da\u00df sich mein Haar erhob, von eisger Furcht gebannt.", "tokens": ["Da\u00df", "sich", "mein", "Haar", "er\u00b7hob", ",", "von", "eis\u00b7ger", "Furcht", "ge\u00b7bannt", "."], "token_info": ["word", 