{"textgrid.poem.55180": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Der neue Amor", "genre": "verse", "period": "N.A.", "pub_year": 1792, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Amor, nicht das Kind, der J\u00fcngling, der Psychen verf\u00fchrte,", "tokens": ["A\u00b7mor", ",", "nicht", "das", "Kind", ",", "der", "J\u00fcng\u00b7ling", ",", "der", "Psy\u00b7chen", "ver\u00b7f\u00fchr\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PTKNEG", "ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Sah im Olympus sich um, frech und der Siege gewohnt;", "tokens": ["Sah", "im", "O\u00b7lym\u00b7pus", "sich", "um", ",", "frech", "und", "der", "Sie\u00b7ge", "ge\u00b7wohnt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NE", "PRF", "PTKVZ", "$,", "ADJD", "KON", "ART", "NN", "VVPP", "$."], "meter": "----+--+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Eine G\u00f6ttin erblickt' er, vor allen die herrlichste Sch\u00f6ne,", "tokens": ["Ei\u00b7ne", "G\u00f6t\u00b7tin", "er\u00b7blickt'", "er", ",", "vor", "al\u00b7len", "die", "herr\u00b7lichs\u00b7te", "Sch\u00f6\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,", "APPR", "PIAT", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Venus Urania war's, und er entbrannte f\u00fcr sie.", "tokens": ["Ve\u00b7nus", "U\u00b7ra\u00b7nia", "wa\u00b7r's", ",", "und", "er", "ent\u00b7brann\u00b7te", "f\u00fcr", "sie", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VAFIN", "$,", "KON", "PPER", "VVFIN", "APPR", "PPER", "$."], "meter": "+-+-+-+-+-+--+", "measure": "iambic.septa.chol"}, "line.5": {"text": "Ach, die Heilige selbst, sie widerstand nicht dem Werben,", "tokens": ["Ach", ",", "die", "Hei\u00b7li\u00b7ge", "selbst", ",", "sie", "wi\u00b7der\u00b7stand", "nicht", "dem", "Wer\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ART", "ADJA", "ADV", "$,", "PPER", "VVFIN", "PTKNEG", "ART", "NN", "$,"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.6": {"text": "Und der Verwegene hielt fest sie im Arme bestrickt.", "tokens": ["Und", "der", "Ver\u00b7we\u00b7ge\u00b7ne", "hielt", "fest", "sie", "im", "Ar\u00b7me", "be\u00b7strickt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ADJD", "PPER", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+--+---+--+", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Da entstand aus ihnen ein neuer lieblicher Amor,", "tokens": ["Da", "ent\u00b7stand", "aus", "ih\u00b7nen", "ein", "neu\u00b7er", "lieb\u00b7li\u00b7cher", "A\u00b7mor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPER", "ART", "ADJA", "ADJA", "NE", "$,"], "meter": "--+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Der dem Vater den Sinn, Sitte der Mutter verdankt.", "tokens": ["Der", "dem", "Va\u00b7ter", "den", "Sinn", ",", "Sit\u00b7te", "der", "Mut\u00b7ter", "ver\u00b7dankt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ART", "NN", "$,", "NN", "ART", "NN", "VVFIN", "$."], "meter": "--+--++--+--+", "me