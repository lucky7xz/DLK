{"textgrid.poem.48376": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "4.", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein Liebster ist kommen von Aberdeen,", "tokens": ["Mein", "Liebs\u00b7ter", "ist", "kom\u00b7men", "von", "A\u00b7berd\u00b7een", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVINF", "APPR", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Ach, \u00fcber die Ma\u00dfen lieb' ich ihn,", "tokens": ["Ach", ",", "\u00fc\u00b7ber", "die", "Ma\u00b7\u00dfen", "lieb'", "ich", "ihn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "APPR", "ART", "NN", "VVFIN", "PPER", "PPER", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und hat mich doch betr\u00fcbt und erschreckt", "tokens": ["Und", "hat", "mich", "doch", "be\u00b7tr\u00fcbt", "und", "er\u00b7schreckt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "ADV", "VVPP", "KON", "VVFIN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Und die wei\u00dfe Kokarde aufgesteckt;", "tokens": ["Und", "die", "wei\u00b7\u00dfe", "Ko\u00b7kar\u00b7de", "auf\u00b7ge\u00b7steckt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Er ist ein \u00fcberm\u00fctiger Bursch,", "tokens": ["Er", "ist", "ein", "\u00fc\u00b7berm\u00b7\u00fc\u00b7ti\u00b7ger", "Bursch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Und doch ein lieber, g\u00fctiger Bursch,", "tokens": ["Und", "doch", "ein", "lie\u00b7ber", ",", "g\u00fc\u00b7ti\u00b7ger", "Bursch", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "ADV", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.7": {"text": "Und ich lieb' ihn und will mit ihm gehn", "tokens": ["Und", "ich", "lieb'", "ihn", "und", "will", "mit", "ihm", "gehn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPER", "KON", "VMFIN", "APPR", "PPER", "VVINF"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.8": {"text": "Und immer die wei\u00dfe Kokarde sehn.", "tokens": ["Und", "im\u00b7mer", "die", "wei\u00b7\u00dfe", "Ko\u00b7kar\u00b7de", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.2": {"line.1": {"text": "Ich will verkaufen Gei\u00df und Kuh", "tokens": ["Ich", "will", "ver\u00b7kau\u00b7fen", "Gei\u00df", "und", "Kuh"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "VVINF", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und Spindel und Flachs und Garn dazu", "tokens": ["Und", "Spin\u00b7del", "und", "Flachs", "und", "Garn", "da\u00b7zu"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "KON", "NN", "KON", "NN", "PAV"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und will mir kaufen ein Tartankleid", "tokens": ["Und", "will", "mir", "kau\u00b7fen", "ein", "Tar\u00b7tan\u00b7kleid"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "VVFIN"