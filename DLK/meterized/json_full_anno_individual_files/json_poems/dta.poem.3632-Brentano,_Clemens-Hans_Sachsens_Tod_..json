{"dta.poem.3632": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Hans Sachsens Tod .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519172", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Als man schrieb um Weihnachten               ", "tokens": ["Als", "man", "schrieb", "um", "Weih\u00b7nach\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "VVFIN", "APPR", "NN"], "meter": "+-+-++-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Gleich Sechs und Siebenzig,", "tokens": ["Gleich", "Sechs", "und", "Sie\u00b7ben\u00b7zig", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "CARD", "KON", "NE", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mich da aufwachen machten", "tokens": ["Mich", "da", "auf\u00b7wa\u00b7chen", "mach\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVIZU", "VVFIN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Die Nachtraben frostig,", "tokens": ["Die", "Nach\u00b7tra\u00b7ben", "fros\u00b7tig", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Da\u00df ich nicht mehr konnt schlafen,", "tokens": ["Da\u00df", "ich", "nicht", "mehr", "konnt", "schla\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ADV", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Mich trafen", "tokens": ["Mich", "tra\u00b7fen"], "token_info": ["word", "word"], "pos": ["PPER", "VVFIN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "Gedanken allzuviel.", "tokens": ["Ge\u00b7dan\u00b7ken", "all\u00b7zu\u00b7viel", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "PIAT", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Da kam mir vor mein Wandern,", "tokens": ["Da", "kam", "mir", "vor", "mein", "Wan\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Und was ich trieb darin,", "tokens": ["Und", "was", "ich", "trieb", "da\u00b7rin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VVFIN", "PAV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Mir fiel ein unter andern,", "tokens": ["Mir", "fiel", "ein", "un\u00b7ter", "an\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "APPR", "PIS", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Wie viel Hans Sachs vorhin", "tokens": ["Wie", "viel", "Hans", "Sachs", "vor\u00b7hin"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "NE", "NE", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "Macht Lieder, geistlich Geschichte,", "tokens": ["Macht", "Lie\u00b7der", ",", "geist\u00b7lich", "Ge\u00b7schich\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "ADJD", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.13": {"text": "Gedichte,", "tokens": ["Ge\u00b7dich\u00b7te", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.14": {"text": "Fabeln, Gespr\u00e4ch und Spiel,", "tokens": ["Fa\u00b7beln", ",", "Ge\u00b7spr\u00e4ch", "und", "Spiel", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.15": {"text": "Und wie es fromm',", "tokens": ["Und", "wie", "es", "fromm'", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.16": {"text": "Und Nutz draus komm',", "tokens": ["Und", "Nutz", "draus", "komm'", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PAV", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.17": {"text": "Wohl jedem, der sich des annomm'.", "tokens": ["Wohl", "je\u00b7dem", ",", "der", "sich", "des", "an\u00b7nomm'", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "$,", "PRELS", "PRF", "ART", "VVFIN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.18": {"text": "Indem entschlief ich wiederum,", "tokens": ["In\u00b7dem", "ent\u00b7schlief", "ich", "wie\u00b7de\u00b7rum", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Und Morgens drauf mir in den Sinn", "tokens": ["Und", "Mor\u00b7gens", "drauf", "mir", "in", "den", "Sinn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "PAV", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Ein fr\u00f6hlich Traum da fiel.", "tokens": ["Ein", "fr\u00f6h\u00b7lich", "Traum", "da", "fiel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Mich d\u00e4ucht, ich reist' aus r\u00fcstig,", "tokens": ["Mich", "d\u00e4ucht", ",", "ich", "reist'", "aus", "r\u00fcs\u00b7tig", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "APPR", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und kam zur Mayenzeit,", "tokens": ["Und", "kam", "zur", "Ma\u00b7yen\u00b7zeit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "In eine Stadt gro\u00df, lustig,", "tokens": ["In", "ei\u00b7ne", "Stadt", "gro\u00df", ",", "lus\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "$,", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Von H\u00e4usern sch\u00f6n bereit,", "tokens": ["Von", "H\u00e4u\u00b7sern", "sch\u00f6n", "be\u00b7reit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die Wohnung der ged\u00fcrsten (k\u00fchnen)", "tokens": ["Die", "Woh\u00b7nung", "der", "ge\u00b7d\u00fcrs\u00b7ten", "(", "k\u00fch\u00b7nen", ")"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "$(", "ADJA", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Reichsf\u00fcrsten", "tokens": ["Reichs\u00b7f\u00fcrs\u00b7ten"], "token_info": ["word"], "pos": ["NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "War mitten in der Stadt.", "tokens": ["War", "mit\u00b7ten", "in", "der", "Stadt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Und auch ein Berg hoch, gr\u00fcne,", "tokens": ["Und", "auch", "ein", "Berg", "hoch", ",", "gr\u00fc\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "ADJD", "$,", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Darauf ein sch\u00f6ner Gart,", "tokens": ["Da\u00b7rauf", "ein", "sch\u00f6\u00b7ner", "Gart", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "In Freuden war ich k\u00fchne,", "tokens": ["In", "Freu\u00b7den", "war", "ich", "k\u00fch\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Weil drin gepflanzet ward", "tokens": ["Weil", "drin", "ge\u00b7pflan\u00b7zet", "ward"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ADV", "VVPP", "VAFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "Wohl mancher Baum voll Fr\u00fcchte,", "tokens": ["Wohl", "man\u00b7cher", "Baum", "voll", "Fr\u00fcch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "ADJD", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Gez\u00fcchte,", "tokens": ["Ge\u00b7z\u00fcch\u00b7te", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.14": {"text": "Pomranzen und Muskat,", "tokens": ["Pom\u00b7ran\u00b7zen", "und", "Mus\u00b7kat", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.15": {"text": "Mehr fand ich drein", "tokens": ["Mehr", "fand", "ich", "drein"], "token_info": ["word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "PPER", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.16": {"text": "Rosinlein fein,", "tokens": ["Ro\u00b7sin\u00b7lein", "fein", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "ADJD", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.17": {"text": "Mandlen, Feigen, allerlei rein", "tokens": ["Mand\u00b7len", ",", "Fei\u00b7gen", ",", "al\u00b7ler\u00b7lei", "rein"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Wohlschmeckend Fr\u00fcchte, gro\u00df und klein,", "tokens": ["Wohl\u00b7schme\u00b7ckend", "Fr\u00fcch\u00b7te", ",", "gro\u00df", "und", "klein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Geno\u00df viel Volk da insgemein,", "tokens": ["Ge\u00b7no\u00df", "viel", "Volk", "da", "ins\u00b7ge\u00b7mein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Das drin spatzieret hat.", "tokens": ["Das", "drin", "spat\u00b7zie\u00b7ret", "hat", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "VVFIN", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Mitten im Garten stande", "tokens": ["Mit\u00b7ten", "im", "Gar\u00b7ten", "stan\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "ADJA"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Ein sch\u00f6nes Lusth\u00e4u\u00dflein,", "tokens": ["Ein", "sch\u00f6\u00b7nes", "Lust\u00b7h\u00e4u\u00df\u00b7lein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Darin ein Saal sich fande,", "tokens": ["Da\u00b7rin", "ein", "Saal", "sich", "fan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mit Marmor pflastert fein,", "tokens": ["Mit", "Mar\u00b7mor", "pflas\u00b7tert", "fein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Mit sch\u00f6n lieblichen Schilden", "tokens": ["Mit", "sch\u00f6n", "lieb\u00b7li\u00b7chen", "Schil\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJD", "ADJA", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "Und Bilden,", "tokens": ["Und", "Bil\u00b7den", ","], "token_info": ["word", "word", "punct"], "pos": ["KON", "NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "Figuren frech und k\u00fchn.", "tokens": ["Fi\u00b7gu\u00b7ren", "frech", "und", "k\u00fchn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Ringsum der Saal auch hatte", "tokens": ["Ring\u00b7sum", "der", "Saal", "auch", "hat\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "ADV", "VAFIN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.9": {"text": "Fenster geschnitzet aus,", "tokens": ["Fens\u00b7ter", "ge\u00b7schnit\u00b7zet", "aus", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVPP", "PTKVZ", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.10": {"text": "Durch die man all' Frucht thate", "tokens": ["Durch", "die", "man", "all'", "Frucht", "tha\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "PIS", "PIAT", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Im Garten sehen draus.", "tokens": ["Im", "Gar\u00b7ten", "se\u00b7hen", "draus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVINF", "PAV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "Im Saal stand auch ohnecket", "tokens": ["Im", "Saal", "stand", "auch", "oh\u00b7nec\u00b7ket"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "ADV", "NE"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Bedecket", "tokens": ["Be\u00b7de\u00b7cket"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.14": {"text": "Ein Tisch mit Seiden gr\u00fcn", "tokens": ["Ein", "Tisch", "mit", "Sei\u00b7den", "gr\u00fcn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.15": {"text": "An selbem sa\u00df", "tokens": ["An", "sel\u00b7bem", "sa\u00df"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.16": {"text": "Ein Altmann bla\u00df,", "tokens": ["Ein", "Alt\u00b7mann", "bla\u00df", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.17": {"text": "In einem gro\u00dfen Buch er las,", "tokens": ["In", "ei\u00b7nem", "gro\u00b7\u00dfen", "Buch", "er", "las", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "H\u00e4tt einen langen Bart f\u00fcrbas", "tokens": ["H\u00e4tt", "ei\u00b7nen", "lan\u00b7gen", "Bart", "f\u00fcr\u00b7bas"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Grauweis, wie eine Taub er sa\u00df", "tokens": ["Grau\u00b7weis", ",", "wie", "ei\u00b7ne", "Taub", "er", "sa\u00df"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PWAV", "ART", "NN", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Auf einem Blatte gr\u00fcn.", "tokens": ["Auf", "ei\u00b7nem", "Blat\u00b7te", "gr\u00fcn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Das Buch lag auf dem Pulte", "tokens": ["Das", "Buch", "lag", "auf", "dem", "Pul\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Auf seinem Tisch allein,", "tokens": ["Auf", "sei\u00b7nem", "Tisch", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und auf den B\u00e4nken, gulden,", "tokens": ["Und", "auf", "den", "B\u00e4n\u00b7ken", ",", "gul\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mehr andre B\u00fccher fein,", "tokens": ["Mehr", "and\u00b7re", "B\u00fc\u00b7cher", "fein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die alle wohl beschlagen", "tokens": ["Die", "al\u00b7le", "wohl", "be\u00b7schla\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PIS", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Da lagen,", "tokens": ["Da", "la\u00b7gen", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,"], "meter":