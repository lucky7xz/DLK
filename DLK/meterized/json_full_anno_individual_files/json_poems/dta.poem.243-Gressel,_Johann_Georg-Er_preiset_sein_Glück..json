{"dta.poem.243": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Er preiset sein Gl\u00fcck.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Jhr Auen/ B\u00e4ch und B\u00fcsche", "tokens": ["Ihr", "Au\u00b7en", "/", "B\u00e4ch", "und", "B\u00fc\u00b7sche"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$(", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Du stiller Felder-Ruh/", "tokens": ["Du", "stil\u00b7ler", "Fel\u00b7der\u00b7Ruh", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und auch ihr stummen Fische/", "tokens": ["Und", "auch", "ihr", "stum\u00b7men", "Fi\u00b7sche", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "H\u00f6rt meine Freude zu.", "tokens": ["H\u00f6rt", "mei\u00b7ne", "Freu\u00b7de", "zu", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ich sa\u00df vor wenig Tagen", "tokens": ["Ich", "sa\u00df", "vor", "we\u00b7nig", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Bey meiner ", "tokens": ["Bey", "mei\u00b7ner"], "token_info": ["word", "word"], "pos": ["APPR", "PPOSAT"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Und was ich nur mucht fragen/", "tokens": ["Und", "was", "ich", "nur", "mucht", "fra\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "ADV", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "War alles bey ihr Ja.", "tokens": ["War", "al\u00b7les", "bey", "ihr", "Ja", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ich sprach: Mein Kind von Hertzen", "tokens": ["Ich", "sprach", ":", "Mein", "Kind", "von", "Hert\u00b7zen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "PPOSAT", "NN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Verehr\u2019 ich eure Pracht", "tokens": ["Ver\u00b7ehr'", "ich", "eu\u00b7re", "Pracht"], "token_info": ["word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So lang die Sternen schertzen", "tokens": ["So", "lang", "die", "Ster\u00b7nen", "schert\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Am Himmel bey der Nacht.", "tokens": ["Am", "Him\u00b7mel", "bey", "der", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ja in der letzten Stunde/", "tokens": ["Ja", "in", "der", "letz\u00b7ten", "Stun\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Leib und Seele trennt/", "tokens": ["Die", "Leib", "und", "See\u00b7le", "trennt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ob schon mit schwachem Munde/", "tokens": ["Ob", "schon", "mit", "schwa\u00b7chem", "Mun\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Werdt ihr mein Licht genennt.", "tokens": ["Werdt", "ihr", "mein", "Licht", "ge\u00b7nennt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Drauf schlo\u00df sie mich gebunden", "tokens": ["Drauf", "schlo\u00df", "sie", "mich", "ge\u00b7bun\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "PRF", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In ihre Arme ein/", "tokens": ["In", "ih\u00b7re", "Ar\u00b7me", "ein", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ach! da\u00df der s\u00fcssen Stunden", "tokens": ["Ach", "!", "da\u00df", "der", "s\u00fcs\u00b7sen", "Stun\u00b7den"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Noch tausend m\u00f6chten seyn.", "tokens": ["Noch", "tau\u00b7send", "m\u00f6ch\u00b7ten", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "CARD", "VMFIN", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Und mit verliebten Blicken", "tokens": ["Und", "mit", "ver\u00b7lieb\u00b7ten", "Bli\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sah sie mich freundlich an/", "tokens": ["Sah", "sie", "mich", "freund\u00b7lich", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADJD", "PTKVZ", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ein sanfftes H\u00e4nde dr\u00fccken", "tokens": ["Ein", "sanff\u00b7tes", "H\u00e4n\u00b7de", "dr\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wurd\u2019 auch hinzugethan.", "tokens": ["Wurd'", "auch", "hin\u00b7zu\u00b7ge\u00b7than", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Bis endlich meine Sch\u00f6ne", "tokens": ["Bis", "end\u00b7lich", "mei\u00b7ne", "Sch\u00f6\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Worte bracht herf\u00fcr/", "tokens": ["Die", "Wor\u00b7te", "bracht", "her\u00b7f\u00fcr", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mein Liebster/ ach ich sehne", "tokens": ["Mein", "Liebs\u00b7ter", "/", "ach", "ich", "seh\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$(", "XY", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mich immerfort nach dir.", "tokens": ["Mich", "im\u00b7mer\u00b7fort", "nach", "dir", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Ich stelle dir das K\u00fcssen", "tokens": ["Ich", "stel\u00b7le", "dir", "das", "K\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und beste Lieben frey/", "tokens": ["Und", "bes\u00b7te", "Lie\u00b7ben", "frey", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "ADJA", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch keiner soll es wissen/", "tokens": ["Doch", "kei\u00b7ner", "soll", "es", "wis\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VMFIN", "PPER", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da\u00df ich dein eigen sey.", "tokens": ["Da\u00df", "ich", "dein", "ei\u00b7gen", "sey", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "ADJD", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Wir sassen voller Freuden", "tokens": ["Wir", "sas\u00b7sen", "vol\u00b7ler", "Freu\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In dem geb\u00fcckten Klee/", "tokens": ["In", "dem", "ge\u00b7b\u00fcck\u00b7ten", "Klee", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Entfernt von allen Leiden/", "tokens": ["Ent\u00b7fernt", "von", "al\u00b7len", "Lei\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "PIAT", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Von Kummer/ Furcht und Weh.", "tokens": ["Von", "Kum\u00b7mer", "/", "Furcht", "und", "Weh", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$(", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Jhr Mund lag auf dem meinen", "tokens": ["Ihr", "Mund", "lag", "auf", "dem", "mei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "ART", "PPOSAT"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Von ihr selbst angelegt/", "tokens": ["Von", "ihr", "selbst", "an\u00b7ge\u00b7legt", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "VVPP", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und ihre Brust lie\u00df scheinen", "tokens": ["Und", "ih\u00b7re", "Brust", "lie\u00df", "schei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Was sie verborgen tr\u00e4gt.", "tokens": ["Was", "sie", "ver\u00b7bor\u00b7gen", "tr\u00e4gt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Zwo sch\u00f6ne ", "tokens": ["Zwo", "sch\u00f6\u00b7ne"], "token_info": ["word", "word"], "pos": ["CARD", "ADJA"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Die quollen in die H\u00f6h/", "tokens": ["Die", "quol\u00b7len", "in", "die", "H\u00f6h", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es wuchsen auf Corallen", "tokens": ["Es", "wuch\u00b7sen", "auf", "Co\u00b7ral\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In Flammen und im Schnee.", "tokens": ["In", "Flam\u00b7men", "und", "im", "Schnee", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Es daurte unser Freude", "tokens": ["Es", "daur\u00b7te", "un\u00b7ser", "Freu\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Bis in die sp\u00e4hte Nacht/", "tokens": ["Bis", "in", "die", "sp\u00e4h\u00b7te", "Nacht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Worauf wir alle beyde", "tokens": ["Wo\u00b7rauf", "wir", "al\u00b7le", "bey\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "PPER", "PIAT", "PIS"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Uns auf dem Weg gemacht.", "tokens": ["Uns", "auf", "dem", "Weg", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Dis wil ich euch vertrauen", "tokens": ["Dis", "wil", "ich", "euch", "ver\u00b7trau\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VMFIN", "PPER", "PPER", "VVINF"], "meter": "++-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Jhr Felder weit und breit/", "tokens": ["Ihr", "Fel\u00b7der", "weit", "und", "breit", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "KON", "ADJD", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Jhr aber m\u00fcsset schauen/", "tokens": ["Ihr", "a\u00b7ber", "m\u00fcs\u00b7set", "schau\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da\u00df ihr verschwiegen seyd.", "tokens": ["Da\u00df", "ihr", "ver\u00b7schwie\u00b7gen", "seyd", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Drum sag\u2019 ich mein Ver\u00fcben", "tokens": ["Drum", "sag'", "ich", "mein", "Ver\u00b7\u00fc\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Den stummen Felsen an/", "tokens": ["Den", "stum\u00b7men", "Fel\u00b7sen", "an", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die beste Kunst im Lieben", "tokens": ["Die", "bes\u00b7te", "Kunst", "im", "Lie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPRART", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ist/ da\u00df man schweigen kan.", "tokens": ["Ist", "/", "da\u00df", "man", "schwei\u00b7gen", "kan", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$(", "KOUS", "PIS", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}