{"textgrid.poem.61720": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Mittagszauber", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im Garten wandelt hohe Mittagszeit,", "tokens": ["Im", "Gar\u00b7ten", "wan\u00b7delt", "ho\u00b7he", "Mit\u00b7tags\u00b7zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der Rasen gl\u00e4nzt, die Wipfel schatten breit;", "tokens": ["Der", "Ra\u00b7sen", "gl\u00e4nzt", ",", "die", "Wip\u00b7fel", "schat\u00b7ten", "breit", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Von oben sieht, getaucht in Sonnenschein", "tokens": ["Von", "o\u00b7ben", "sieht", ",", "ge\u00b7taucht", "in", "Son\u00b7nen\u00b7schein"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADV", "VVFIN", "$,", "VVPP", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und leuchtend Blau, der alte Dom herein.", "tokens": ["Und", "leuch\u00b7tend", "Blau", ",", "der", "al\u00b7te", "Dom", "her\u00b7ein", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "NN", "$,", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Am Birnbaum sitzt mein T\u00f6chterchen im Gras;", "tokens": ["Am", "Birn\u00b7baum", "sitzt", "mein", "T\u00f6ch\u00b7ter\u00b7chen", "im", "Gras", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPOSAT", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die M\u00e4rchen liest sie, die als Kind ich las;", "tokens": ["Die", "M\u00e4r\u00b7chen", "liest", "sie", ",", "die", "als", "Kind", "ich", "las", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,", "PRELS", "KOUS", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ihr Antlitz gl\u00fcht, es ziehn durch ihren Sinn", "tokens": ["Ihr", "Ant\u00b7litz", "gl\u00fcht", ",", "es", "ziehn", "durch", "ih\u00b7ren", "Sinn"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "$,", "PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Schneewittchen, D\u00e4umling, Schlangenk\u00f6nigin.", "tokens": ["Schnee\u00b7witt\u00b7chen", ",", "D\u00e4um\u00b7ling", ",", "Schlan\u00b7gen\u00b7k\u00f6\u00b7ni\u00b7gin", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}, "stanza.3": {"line.1": {"text": "Kein Laut von au\u00dfen st\u00f6rt; 's ist Feiertag \u2013", "tokens": ["Kein", "Laut", "von", "au\u00b7\u00dfen", "st\u00f6rt", ";", "'s", "ist", "Fei\u00b7er\u00b7tag", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "APPR", "APPR", "ADV", "VVFIN", "$.", "PPER", "VAFIN", "NN", "$("], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Nur dann und wann vom Turm ein Glockenschlag!", "tokens": ["Nur", "dann", "und", "wann", "vom", "Turm", "ein", "Glo\u00b7cken\u00b7schlag", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "KON", "PWAV", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Nur dann und wann der mattged\u00e4mpfte Schall", "tokens": ["Nur", "dann", "und", "wann", "der", "matt\u00b7ge\u00b7d\u00e4mpf\u00b7te", "Schall"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "KON", "PWAV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Im hohen Gras von eines Apfels Fall!", "tokens": ["Im", "ho\u00b7hen", "Gras", "von", "ei\u00b7nes", "Ap\u00b7fels", "Fall", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Da kommt auf mich ein D\u00e4mmern wunderbar;", "tokens": ["Da", "kommt", "auf", "mich", "ein", "D\u00e4m\u00b7mern", "wun\u00b7der\u00b7bar", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PRF", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Gleichwie im Traum verschmilzt, was ist und war:", "tokens": ["Gleich\u00b7wie", "im", "Traum", "ver\u00b7schmilzt", ",", "was", "ist", "und", "war", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VVPP", "$,", "PWS", "VAFIN", "KON", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Seele l\u00f6st sich und verliert sich weit", "tokens": ["Die", "See\u00b7le", "l\u00f6st", "sich", "und", "ver\u00b7liert", "sich", "weit"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PRF", "KON", "VVFIN", "PRF", "ADJD"], "meter": "-+-+-+-+-+", "measure"