{"dta.poem.1791": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Ubersezte Spruch-Reimen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Schaue da\u00df du nicht zu sehr dich die Hoffart zwingen l\u00e4st/", "tokens": ["Schau\u00b7e", "da\u00df", "du", "nicht", "zu", "sehr", "dich", "die", "Hof\u00b7fart", "zwin\u00b7gen", "l\u00e4st", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOUS", "PPER", "PTKNEG", "PTKA", "ADV", "PPER", "ART", "NN", "VVINF", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "Wenn des gutten Gl\u00fcckes Wind dir in volle Segel bl\u00e4st/", "tokens": ["Wenn", "des", "gut\u00b7ten", "Gl\u00fc\u00b7ckes", "Wind", "dir", "in", "vol\u00b7le", "Se\u00b7gel", "bl\u00e4st", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "NN", "PPER", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "chau auch/ da\u00df du nicht zu sehr den gefallnen Mutt l\u00e4st sin-", "tokens": ["chau", "auch", "/", "da\u00df", "du", "nicht", "zu", "sehr", "den", "ge\u00b7fall\u00b7nen", "Mutt", "l\u00e4st", "sin"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "$(", "KOUS", "PPER", "PTKNEG", "PTKA", "ADV", "ART", "ADJA", "NN", "VVFIN", "TRUNC"], "meter": "+---+-+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "cken/", "tokens": ["cken", "/"], "token_info": ["word", "punct"], "pos": ["VVINF", "$("], "meter": "-", "measure": "single.down"}, "line.5": {"text": "enn dein lekkes Schiff beginnt die gesaltzne Flutt zu trincken.", "tokens": ["enn", "dein", "lek\u00b7kes", "Schiff", "be\u00b7ginnt", "die", "ge\u00b7saltz\u00b7ne", "Flutt", "zu", "trin\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}}}}}