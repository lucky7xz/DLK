{"dta.poem.9562": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auff ihre grausamkeit.  \n C. E.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Dorinde/ ists nun aus mit meinem armen leben?", "tokens": ["Do\u00b7rin\u00b7de", "/", "ists", "nun", "aus", "mit", "mei\u00b7nem", "ar\u00b7men", "le\u00b7ben", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "VAFIN", "ADV", "APPR", "APPR", "PPOSAT", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hab\u2019 ich nun einst verzehrt das d\u00fcrre kummer-brod/", "tokens": ["Hab'", "ich", "nun", "einst", "ver\u00b7zehrt", "das", "d\u00fcr\u00b7re", "kum\u00b7mer\u00b7brod", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "ADV", "VVPP", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und den verg\u00e4llten tranck/ den deine hand mit bot/", "tokens": ["Und", "den", "ver\u00b7g\u00e4ll\u00b7ten", "tranck", "/", "den", "dei\u00b7ne", "hand", "mit", "bot", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$(", "ART", "PPOSAT", "NN", "APPR", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Vor dessen bittrigkeit noch geist und blut erbeben?", "tokens": ["Vor", "des\u00b7sen", "bitt\u00b7rig\u00b7keit", "noch", "geist", "und", "blut", "er\u00b7be\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDS", "ADV", "ADV", "ADJD", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ists m\u00f6glich/ wiltu mich des jammers \u00fcberheben/", "tokens": ["Ists", "m\u00f6g\u00b7lich", "/", "wil\u00b7tu", "mich", "des", "jam\u00b7mers", "\u00fc\u00b7ber\u00b7he\u00b7ben", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$(", "VMFIN", "PRF", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und seh\u2019 ich denn einmahl die endschafft meiner noth?", "tokens": ["Und", "seh'", "ich", "denn", "ein\u00b7mahl", "die", "end\u00b7schafft", "mei\u00b7ner", "noth", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADV", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ach! ja/ dein grausam seyn bringt endlich mir den todt/", "tokens": ["Ach", "!", "ja", "/", "dein", "grau\u00b7sam", "seyn", "bringt", "end\u00b7lich", "mir", "den", "todt", "/"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "ADV", "$(", "PPOSAT", "ADJD", "VAINF", "VVFIN", "ADV", "PPER", "ART", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und will ein schwartzes grab mir zur belohnung geben.", "tokens": ["Und", "will", "ein", "schwart\u00b7zes", "grab", "mir", "zur", "be\u00b7loh\u00b7nung", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "ADJA", "NN", "PPER", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Weil dann mein leben nichts in deinen augen gilt/", "tokens": ["Weil", "dann", "mein", "le\u00b7ben", "nichts", "in", "dei\u00b7nen", "au\u00b7gen", "gilt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPOSAT", "VVFIN", "PIS", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "So s\u00e4ume nur nicht lang/ wann du mich t\u00f6dten wilt/", "tokens": ["So", "s\u00e4u\u00b7me", "nur", "nicht", "lang", "/", "wann", "du", "mich", "t\u00f6d\u00b7ten", "wilt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PTKNEG", "ADJD", "$(", "PWAV", "PPER", "PRF", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Und \u00fcberhebe mich nur zeitig meiner plage:", "tokens": ["Und", "\u00fc\u00b7ber\u00b7he\u00b7be", "mich", "nur", "zei\u00b7tig", "mei\u00b7ner", "pla\u00b7ge", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Doch glaube sicherlich/ du t\u00f6dt\u2019st mich nicht allein;", "tokens": ["Doch", "glau\u00b7be", "si\u00b7cher\u00b7lich", "/", "du", "t\u00f6dt'st", "mich", "nicht", "al\u00b7lein", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "$(", "PPER", "VVFIN", "PPER", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Mein todt wird auch dein todt/ und dein verderben seyn/", "tokens": ["Mein", "todt", "wird", "auch", "dein", "todt", "/", "und", "dein", "ver\u00b7der\u00b7ben", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "VAFIN", "ADV", "PPOSAT", "ADJD", "$(", "KON", "PPOSAT", "VVFIN", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Weil ich dich lebendig in meinem hertzen trage.", "tokens": ["Weil", "ich", "dich", "le\u00b7ben\u00b7dig", "in", "mei\u00b7nem", "hert\u00b7zen", "tra\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADJD", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}