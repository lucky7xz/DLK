{"textgrid.poem.31643": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "15. Herr / warum trittestu so ferne? etc", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da\u00df ich meine Christen probe / ob sie gute Schiffer sind /", "tokens": ["Da\u00df", "ich", "mei\u00b7ne", "Chris\u00b7ten", "pro\u00b7be", "/", "ob", "sie", "gu\u00b7te", "Schif\u00b7fer", "sind", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$(", "KOUS", "PPER", "ADJA", "NN", "VAFIN", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "so / bey St\u00fcrmen / sau\u00df-und brausen / fahren wie bey gutem Wind /", "tokens": ["so", "/", "bey", "St\u00fcr\u00b7men", "/", "sau\u00df\u00b7\u00b7und", "brau\u00b7sen", "/", "fah\u00b7ren", "wie", "bey", "gu\u00b7tem", "Wind", "/"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "APPR", "NN", "$(", "ADJD", "VVFIN", "$(", "VVINF", "KOKOM", "APPR", "ADJA", "NN", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "da\u00df der Hochmut-Segel fall / und die H\u00e4nde sich erheben;", "tokens": ["da\u00df", "der", "Hoch\u00b7mut\u00b7Se\u00b7gel", "fall", "/", "und", "die", "H\u00e4n\u00b7de", "sich", "er\u00b7he\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "NN", "$(", "KON", "ART", "NN", "PRF", "VVINF", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "da\u00df man in der Todes-Noht Geistlich recht beginn zu leben;", "tokens": ["da\u00df", "man", "in", "der", "To\u00b7des\u00b7Noht", "Geist\u00b7lich", "recht", "be\u00b7ginn", "zu", "le\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "ART", "NN", "ADJD", "ADJD", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.5": {"text": "Ja / da\u00df man verlangen trage nach des Himmels festen Land;", "tokens": ["Ja", "/", "da\u00df", "man", "ver\u00b7lan\u00b7gen", "tra\u00b7ge", "nach", "des", "Him\u00b7mels", "fes\u00b7ten", "Land", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$(", "KOUS", "PIS", "VVINF", "VVFIN", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.6": {"text": "meinsttheils aber / da\u00df mein' Allmacht / G\u00fct und H\u00fclffe werd erkandt.", "tokens": ["meinst\u00b7theils", "a\u00b7ber", "/", "da\u00df", "mein'", "All\u00b7macht", "/", "G\u00fct", "und", "H\u00fclf\u00b7fe", "werd", "er\u00b7kandt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$(", "KOUS", "PPOSAT", "NN", "$(", "NN", "KON", "NN", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.2": {"line.1": {"text": "Da\u00df ich meine Christen probe / ob sie gute Schiffer sind /", "tokens": ["Da\u00df", "ich", "mei\u00b7ne", "Chris\u00b7ten", "pro\u00b7be", "/", "ob", "sie", "gu\u00b7te", "Schif\u00b7fer", "sind", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$(", "KOUS", "PPER", "ADJA", "NN", "VAFIN", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "so / bey 