{"textgrid.poem.37935": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "N\u00e4chte", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "N\u00e4chten, da ich bei ihr was,", "tokens": ["N\u00e4ch\u00b7ten", ",", "da", "ich", "bei", "ihr", "was", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "APPR", "PPOSAT", "PWS", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Schwazten wir, dann dies, dann das,", "tokens": ["Schwaz\u00b7ten", "wir", ",", "dann", "dies", ",", "dann", "das", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ADV", "PDS", "$,", "ADV", "PDS", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Auch sehr freundlich zu mir sa\u00df,", "tokens": ["Auch", "sehr", "freund\u00b7lich", "zu", "mir", "sa\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sagt', sie liebt' mich ohn' all Maa\u00df.", "tokens": ["Sagt'", ",", "sie", "liebt'", "mich", "ohn'", "all", "Maa\u00df", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPER", "VVFIN", "PPER", "APPR", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "N\u00e4chten, da ich von ihr scheid,", "tokens": ["N\u00e4ch\u00b7ten", ",", "da", "ich", "von", "ihr", "scheid", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "me