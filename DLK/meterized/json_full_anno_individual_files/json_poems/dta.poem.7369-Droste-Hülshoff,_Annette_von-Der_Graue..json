{"dta.poem.7369": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Der Graue.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-20090519994", "language": ["de:0.99"], "booktitle": "Droste-H\u00fclshoff, Annette von: Gedichte. Stuttgart u. a., 1844."}, "poem": {"stanza.1": {"line.1": {"text": "Im Walde steht die kleine Burg,", "tokens": ["Im", "Wal\u00b7de", "steht", "die", "klei\u00b7ne", "Burg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Aus rohem Quaderstein gefugt,", "tokens": ["Aus", "ro\u00b7hem", "Qua\u00b7der\u00b7stein", "ge\u00b7fugt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit Schart' und Fensterlein, wodurch", "tokens": ["Mit", "Schart'", "und", "Fens\u00b7ter\u00b7lein", ",", "wo\u00b7durch"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["APPR", "NN", "KON", "NN", "$,", "PWAV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Doppelhaken einst gelugt;", "tokens": ["Der", "Dop\u00b7pel\u00b7ha\u00b7ken", "einst", "ge\u00b7lugt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Am Teiche rauscht des Rohres Speer,", "tokens": ["Am", "Tei\u00b7che", "rauscht", "des", "Roh\u00b7res", "Speer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Br\u00fccke wiegt und knarrt im Sturm,", "tokens": ["Die", "Br\u00fc\u00b7cke", "wiegt", "und", "knarrt", "im", "Sturm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und in des Hofes Mitte, schwer,", "tokens": ["Und", "in", "des", "Ho\u00b7fes", "Mit\u00b7te", ",", "schwer", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "NN", "$,", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Plump wie ein M\u00f6rser, steht der Thurm.", "tokens": ["Plump", "wie", "ein", "M\u00f6r\u00b7ser", ",", "steht", "der", "Thurm", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ART", "NN", "$,", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Da siehst du jetzt umher gestellt", "tokens": ["Da", "siehst", "du", "jetzt", "um\u00b7her", "ge\u00b7stellt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PTKVZ", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Manch' feuerrothes Ziegeldach,", "tokens": ["Man\u00b7ch'", "feu\u00b7er\u00b7ro\u00b7thes", "Zie\u00b7gel\u00b7dach", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Und wie der Stempel steigt und f\u00e4llt,", "tokens": ["Und", "wie", "der", "Stem\u00b7pel", "steigt", "und", "f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So pfeift die Dampfmaschine nach;", "tokens": ["So", "pfeift", "die", "Dampf\u00b7ma\u00b7schi\u00b7ne", "nach", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Es knackt die Form, der Bogen schrillt,", "tokens": ["Es", "knackt", "die", "Form", ",", "der", "Bo\u00b7gen", "schrillt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Es dunstet Scheidewassers N\u00e4h',", "tokens": ["Es", "duns\u00b7tet", "Schei\u00b7de\u00b7was\u00b7sers", "N\u00e4h'", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und \u00fcber'm grauen Wappenschild", "tokens": ["Und", "\u00fc\u00b7ber'm", "grau\u00b7en", "Wap\u00b7pen\u00b7schild"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Liest man: ", "tokens": ["Liest", "man", ":"], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "PIS", "$."], "meter": "+-", "measure": "trochaic.single"}}, "stanza.3": {"line.1": {"text": "Doch wie der Kessel quillt und sch\u00e4umt,", "tokens": ["Doch", "wie", "der", "Kes\u00b7sel", "quillt", "und", "sch\u00e4umt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den Br\u00fc\u00df'ler Kaufherrn freut es kaum,", "tokens": ["Den", "Br\u00fc\u00df'\u00b7ler", "Kauf\u00b7herrn", "freut", "es", "kaum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der hatte einmal sich getr\u00e4umt", "tokens": ["Der", "hat\u00b7te", "ein\u00b7mal", "sich", "ge\u00b7tr\u00e4umt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ADV", "PRF", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von Land und Luft den feinsten Traum;", "tokens": ["Von", "Land", "und", "Luft", "den", "feins\u00b7ten", "Traum", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Das war so recht ein Fleckchen, sich", "tokens": ["Das", "war", "so", "recht", "ein", "Fleck\u00b7chen", ",", "sich"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["PDS", "VAFIN", "ADV", "ADJD", "ART", "NN", "$,", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Zu retten aus der Zahlen Haft!", "tokens": ["Zu", "ret\u00b7ten", "aus", "der", "Zah\u00b7len", "Haft", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Nicht gro\u00df, und doch ganz adelich,", "tokens": ["Nicht", "gro\u00df", ",", "und", "doch", "ganz", "a\u00b7de\u00b7lich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "$,", "KON", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und brauchte wenig Dienerschaft.", "tokens": ["Und", "brauch\u00b7te", "we\u00b7nig", "Die\u00b7ner\u00b7schaft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Doch eine Nacht nur macht er sich", "tokens": ["Doch", "ei\u00b7ne", "Nacht", "nur", "macht", "er", "sich"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADV", "VVFIN", "PPER", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bequem es \u2014 oder unbequem \u2014", "tokens": ["Be\u00b7quem", "es", "o\u00b7der", "un\u00b7be\u00b7quem"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPER", "$(", "KON", "ADJD", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "In seinem Schl\u00f6\u00dfchen, und er strich", "tokens": ["In", "sei\u00b7nem", "Schl\u00f6\u00df\u00b7chen", ",", "und", "er", "strich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "KON", "PPER", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nur wie ein Vogel dran seitdem.", "tokens": ["Nur", "wie", "ein", "Vo\u00b7gel", "dran", "seit\u00b7dem", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ART", "NN", "PAV", "PPOSAT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sah dann er zu den Fenstern auf,", "tokens": ["Sah", "dann", "er", "zu", "den", "Fens\u00b7tern", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPER", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Verschlossen wie die Sakristei'n,", "tokens": ["Ver\u00b7schlos\u00b7sen", "wie", "die", "Sa\u00b7kris\u00b7tei'n", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "So zog er wohl die Schultern auf,", "tokens": ["So", "zog", "er", "wohl", "die", "Schul\u00b7tern", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Mit einem Seufzer, oder zwei'n.", "tokens": ["Mit", "ei\u00b7nem", "Seuf\u00b7zer", ",", "o\u00b7der", "zwei'", "n."], "token_info": ["word", "word", "word", "punct", "word", "word", "abbreviation"], "pos": ["APPR", "ART", "NN", "$,", "KON", "XY", "XY"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}