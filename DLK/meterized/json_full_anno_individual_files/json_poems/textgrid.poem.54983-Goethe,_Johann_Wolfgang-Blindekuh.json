{"textgrid.poem.54983": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Blindekuh", "genre": "verse", "period": "N.A.", "pub_year": 1770, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O liebliche Therese!", "tokens": ["O", "lieb\u00b7li\u00b7che", "The\u00b7re\u00b7se", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$."], "meter": "-+--+--", "measure": "iambic.di.relaxed"}, "line.2": {"text": "Wie wandelt gleich ins B\u00f6se", "tokens": ["Wie", "wan\u00b7delt", "gleich", "ins", "B\u00f6\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "ADV", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Dein offnes Auge sich!", "tokens": ["Dein", "off\u00b7nes", "Au\u00b7ge", "sich", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PRF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Die Augen zugebunden,", "tokens": ["Die", "Au\u00b7gen", "zu\u00b7ge\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Hast du mich schnell gefunden,", "tokens": ["Hast", "du", "mich", "schnell", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PRF", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und warum fingst du eben mich?", "tokens": ["Und", "wa\u00b7rum", "fingst", "du", "e\u00b7ben", "mich", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "VVFIN", "PPER", "ADV", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Du fa\u00dftest mich aufs beste", "