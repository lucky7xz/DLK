{"textgrid.poem.66364": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "Serafina's Weihgesang", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Herrlich in des Himmels Maienbl\u00fcthe,", "tokens": ["Herr\u00b7lich", "in", "des", "Him\u00b7mels", "Mai\u00b7en\u00b7bl\u00fct\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Serafina, kam dein Wiegenfest.", "tokens": ["Se\u00b7ra\u00b7fi\u00b7na", ",", "kam", "dein", "Wie\u00b7gen\u00b7fest", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-----+-+", "measure": "dactylic.init"}, "line.3": {"text": "Deines Engels Fr\u00fchlingswange gl\u00fchte", "tokens": ["Dei\u00b7nes", "En\u00b7gels", "Fr\u00fch\u00b7lings\u00b7wan\u00b7ge", "gl\u00fch\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "NN", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Sanft gek\u00fchlt vom Paradieseswest.", "tokens": ["Sanft", "ge\u00b7k\u00fchlt", "vom", "Pa\u00b7ra\u00b7die\u00b7ses\u00b7west", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "APPRART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Sechzehnmal bl\u00e4ht' sich die goldne Traube,", "tokens": ["Sech\u00b7zehn\u00b7mal", "bl\u00e4ht'", "sich", "die", "gold\u00b7ne", "Trau\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Von der Gluth der Sonne ausgekocht,", "tokens": ["Von", "der", "Gluth", "der", "Son\u00b7ne", "aus\u00b7ge\u00b7kocht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Seit der Zeit, da\u00df dir, du Fr\u00fchlingstraube,", "tokens": ["Seit", "der", "Zeit", ",", "da\u00df", "dir", ",", "du", "Fr\u00fch\u00b7lings\u00b7trau\u00b7be", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "KOUS", "PPER", "$,", "PPER", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Purpurblut in blauen Adern pocht.", "tokens": ["Pur\u00b7pur\u00b7blut", "in", "blau\u00b7en", "A\u00b7dern", "pocht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Wie die Bl\u00e4tter gelb am Weinstock hangen,", "tokens": ["Wie", "die", "Bl\u00e4t\u00b7ter", "gelb", "am", "Wein\u00b7stock", "han\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADJD", "APPRART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wenn die St\u00fcrme rauh vor\u00fcberziehn;", "tokens": ["Wenn", "die", "St\u00fcr\u00b7me", "rauh", "vor\u00b7\u00fc\u00b7ber\u00b7ziehn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Ach, so mu\u00df der Reiz auf deinen Wangen,", "tokens": ["Ach", ",", "so", "mu\u00df", "der", "Reiz", "auf", "dei\u00b7nen", "Wan\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADV", "VMFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Deiner Jugend Morgenroth verbl\u00fchn.", "tokens": ["Dei\u00b7ner", "Ju\u00b7gend", "Mor\u00b7gen\u00b7roth", "ver\u00b7bl\u00fchn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "VVINF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Aber Unschuld, die die schlanken Glieder", "tokens": ["A\u00b7ber", "Un\u00b7schuld", ",", "die", "die", "schlan\u00b7ken", "Glie\u00b7der"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "NN", "$,", "PRELS", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Kleidet in des Himmels Rosenduft,", "tokens": ["Klei\u00b7det", "in", "des", "Him\u00b7mels", "Ro\u00b7sen\u00b7duft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Kr\u00fcmmt kein Alter, wie den K\u00f6rper nieder,", "tokens": ["Kr\u00fcmmt", "kein", "Al\u00b7ter", ",", "wie", "den", "K\u00f6r\u00b7per", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "$,", "PWAV", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Siegend schwebt sie \u00fcber Zeit und Gruft.", "tokens": ["Sie\u00b7gend", "schwebt", "sie", "\u00fc\u00b7ber", "Zeit", "und", "Gruft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Serafina, la\u00df uns heute beten!", "tokens": ["Se\u00b7ra\u00b7fi\u00b7na", ",", "la\u00df", "uns", "heu\u00b7te", "be\u00b7ten", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVIMP", "PPER", "ADV", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Gro\u00dfes hat dein Gott an dir gethan;", "tokens": ["Gro\u00b7\u00dfes", "hat", "dein", "Gott", "an", "dir", "ge\u00b7than", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "APPR", "PPER", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Andacht m\u00fcsse deine Wange r\u00f6then,", "tokens": ["An\u00b7dacht", "m\u00fcs\u00b7se", "dei\u00b7ne", "Wan\u00b7ge", "r\u00f6\u00b7then", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Siehst du heut den sch\u00f6nern Himmel an.", "tokens": ["Siehst", "du", "heut", "den", "sch\u00f6\u00b7nern", "Him\u00b7mel", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "Die Gesundheit, die in Purpurfluthen", "tokens": ["Die", "Ge\u00b7sund\u00b7heit", ",", "die", "in", "Pur\u00b7pur\u00b7flut\u00b7hen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Sch\u00e4umend sich durch deine Adern geu\u00dft!", "tokens": ["Sch\u00e4u\u00b7mend", "sich", "durch", "dei\u00b7ne", "A\u00b7dern", "geu\u00dft", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PRF", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Ach, dein Herz, so voll Gef\u00fchl des Guten,", "tokens": ["Ach", ",", "dein", "Herz", ",", "so", "voll", "Ge\u00b7f\u00fchl", "des", "Gu\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPOSAT", "NN", "$,", "ADV", "ADJD", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und den hohen gottgeschaffnen Geist;", "tokens": ["Und", "den", "ho\u00b7hen", "gott\u00b7ge\u00b7schaff\u00b7nen", "Geist", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "Deine Finger, die dem Spiel gebieten,", "tokens": ["Dei\u00b7ne", "Fin\u00b7ger", ",", "die", "dem", "Spiel", "ge\u00b7bie\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Leichtgefl\u00fcgelt, wie der frohe Scherz,", "tokens": ["Leicht\u00b7ge\u00b7fl\u00fc\u00b7gelt", ",", "wie", "der", "fro\u00b7he", "Scherz", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Da\u00df die T\u00f6ne Donnerst\u00fcrme w\u00fcthen,", "tokens": ["Da\u00df", "die", "T\u00f6\u00b7ne", "Don\u00b7ner\u00b7st\u00fcr\u00b7me", "w\u00fct\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Oder sanfter athmen, wie dein Herz;", "tokens": ["O\u00b7der", "sanf\u00b7ter", "ath\u00b7men", ",", "wie", "dein", "Herz", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVINF", "$,", "PWAV", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.8": {"line.1": {"text": "Deinen Vater, deiner Mutter Leben,", "tokens": ["Dei\u00b7nen", "Va\u00b7ter", ",", "dei\u00b7ner", "Mut\u00b7ter", "Le\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Deinen F\u00fchrer, diesen weisen Mann,", "tokens": ["Dei\u00b7nen", "F\u00fch\u00b7rer", ",", "die\u00b7sen", "wei\u00b7sen", "Mann", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PDAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Alles dies hat dir dein Gott gegeben!", "tokens": ["Al\u00b7les", "dies", "hat", "dir", "dein", "Gott", "ge\u00b7ge\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "PDS", "VAFIN", "PPER", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Hat er Gro\u00dfes nicht an dir gethan?", "tokens": ["Hat", "er", "Gro\u00b7\u00dfes", "nicht", "an", "dir", "ge\u00b7than", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "NN", "PTKNEG", "APPR", "PPER", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.9": {"line.1": {"text": "Dank' ihm nur durch Unschuld deiner Sitte", "tokens": ["Dank'", "ihm", "nur", "durch", "Un\u00b7schuld", "dei\u00b7ner", "Sit\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Und mit jeder sanften Weiblichkeit:", "tokens": ["Und", "mit", "je\u00b7der", "sanf\u00b7ten", "Weib\u00b7lich\u00b7keit", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PIAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Zittre vor dem ersten falschen Schritte,", "tokens": ["Zitt\u00b7re", "vor", "dem", "ers\u00b7ten", "fal\u00b7schen", "Schrit\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Den dein gutes Herz so bald bereut.", "tokens": ["Den", "dein", "gu\u00b7tes", "Herz", "so", "bald", "be\u00b7reut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJA", "NN", "ADV", "ADV", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.10": {"line.1": {"text": "Fluch' dem frechen Schattenungeheuer,", "tokens": ["Fluch'", "dem", "fre\u00b7chen", "Schat\u00b7ten\u00b7un\u00b7ge\u00b7heu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Fluch' der Wollust, wenn sie dich beschleicht,", "tokens": ["Fluch'", "der", "Wol\u00b7lust", ",", "wenn", "sie", "dich", "be\u00b7schleicht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "KOUS", "PPER", "PRF", "ADJD", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Und dir h\u00f6hnisch sch\u00e4umend Gift und Feuer", "tokens": ["Und", "dir", "h\u00f6h\u00b7nisch", "sch\u00e4u\u00b7mend", "Gift", "und", "Feu\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "ADJD", "ADJD", "NN", "KON", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Im bekr\u00e4nzten Goldpokale reicht.", "tokens": ["Im", "be\u00b7kr\u00e4nz\u00b7ten", "Gold\u00b7po\u00b7ka\u00b7le", "reicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.11": {"line.1": {"text": "Augenflammen gab dir Gott zu Waffen,", "tokens": ["Au\u00b7gen\u00b7flam\u00b7men", "gab", "dir", "Gott", "zu", "Waf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wenn ein Feind der Tugend sich dir naht.", "tokens": ["Wenn", "ein", "Feind", "der", "Tu\u00b7gend", "sich", "dir", "naht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "NN", "PRF", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Brauche sie, den Frevler zu bestrafen,", "tokens": ["Brau\u00b7che", "sie", ",", "den", "Frev\u00b7ler", "zu", "be\u00b7stra\u00b7fen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Der dich locken will vom Tugendpfad.", "tokens": ["Der", "dich", "lo\u00b7cken", "will", "vom", "Tu\u00b7gend\u00b7pfad", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVINF", "VMFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.12": {"line.1": {"text": "Gottes Ruhe wird dein Herz durchs\u00e4useln,", "tokens": ["Got\u00b7tes", "Ru\u00b7he", "wird", "dein", "Herz", "durch\u00b7s\u00e4u\u00b7seln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VAFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Bleibst du nur der keuschen Tugend hold;", "tokens": ["Bleibst", "du", "nur", "der", "keu\u00b7schen", "Tu\u00b7gend", "hold", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Wenn auch St\u00fcrme deine Stirne kr\u00e4useln,", "tokens": ["Wenn", "auch", "St\u00fcr\u00b7me", "dei\u00b7ne", "Stir\u00b7ne", "kr\u00e4u\u00b7seln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "NN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Hinter ihnen strahlt der Sonne Gold.", "tokens": ["Hin\u00b7ter", "ih\u00b7nen", "strahlt", "der", "Son\u00b7ne", "Gold", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.13": {"line.1": {"text": "Alle Freuden, die die Tugend kr\u00f6nen,", "tokens": ["Al\u00b7le", "Freu\u00b7den", ",", "die", "die", "Tu\u00b7gend", "kr\u00f6\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PRELS", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "W\u00fcnsch' ich dir, du Herzenssiegerin;", "tokens": ["W\u00fcn\u00b7sch'", "ich", "dir", ",", "du", "Her\u00b7zens\u00b7sie\u00b7ge\u00b7rin", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "$,", "PPER", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"te