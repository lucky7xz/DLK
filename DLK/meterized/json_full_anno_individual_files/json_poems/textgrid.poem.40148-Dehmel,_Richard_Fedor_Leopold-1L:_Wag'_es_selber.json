fen,", "tokens": ["Fort", "!", "ich", "will", "aufs", "Pferd", "mich", "wer\u00b7fen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPER", "VMFIN", "APPRART", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "in die ", "tokens": ["in", "die"], "token_info": ["word", "word"], "pos": ["APPR", "ART"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "will zu meiner ", "tokens": ["will", "zu", "mei\u00b7ner"], "token_info": ["word", "word", "word"], "pos": ["VMFIN", "APPR", "PPOSAT"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "ganz in Reinheit Dein zu denken.", "tokens": ["ganz", "in", "Rein\u00b7heit", "Dein", "zu", "den\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "PPOSAT", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Wag' es selber ", "tokens": ["Wag'", "es", "sel\u00b7ber"], "token_info": ["word", "word", "word"], "pos": ["VVIMP", "PPER", "ADV"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "deinen Namen mir zu stammeln;", "tokens": ["dei\u00b7nen", "Na\u00b7men", "mir", "zu", "stam\u00b7meln", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "ist mir immer doch, als m\u00fc\u00dft' ich", "tokens": ["ist", "mir", "im\u00b7mer", "doch", ",", "als", "m\u00fc\u00dft'", "ich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "$,", "KOUS", "VMFIN", "PPER"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "still mich erst zur Andacht sammeln.", "tokens": ["still", "mich", "erst", "zur", "An\u00b7dacht", "sam\u00b7meln", ".