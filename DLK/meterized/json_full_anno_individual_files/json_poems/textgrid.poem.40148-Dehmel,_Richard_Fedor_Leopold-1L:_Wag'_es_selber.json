{"textgrid.poem.40148": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wag' es selber ", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wag' es selber ", "tokens": ["Wag'", "es", "sel\u00b7ber"], "token_info": ["word", "word", "word"], "pos": ["VVIMP", "PPER", "ADV"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "deinen Namen mir zu stammeln;", "tokens": ["dei\u00b7nen", "Na\u00b7men", "mir", "zu", "stam\u00b7meln", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "ist mir immer doch, als m\u00fc\u00dft' ich", "tokens": ["ist", "mir", "im\u00b7mer", "doch", ",", "als", "m\u00fc\u00dft'", "ich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "$,", "KOUS", "VMFIN", "PPER"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "still mich erst zur Andacht sammeln.", "tokens": ["still", "mich", "erst", "zur", "An\u00b7dacht", "sam\u00b7meln", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "ADV", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Und ich mu\u00df es schweigend leiden,", "tokens": ["Und", "ich", "mu\u00df", "es", "schwei\u00b7gend", "lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "darf nicht heil'gen Zorns entbrennen", "tokens": ["darf", "nicht", "heil'\u00b7gen", "Zorns", "ent\u00b7bren\u00b7nen"], "token_info": [