{"dta.poem.7886": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Nachts .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.99"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "Dem stillen Hause blick\u2019 ich zu,", "tokens": ["Dem", "stil\u00b7len", "Hau\u00b7se", "blick'", "ich", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gelehnt an einen Baum;", "tokens": ["Ge\u00b7lehnt", "an", "ei\u00b7nen", "Baum", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dort liegt sie wohl in sch\u00f6ner Ruh", "tokens": ["Dort", "liegt", "sie", "wohl", "in", "sch\u00f6\u00b7ner", "Ruh"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und gl\u00fcht in s\u00fc\u00dfem Traum.", "tokens": ["Und", "gl\u00fcht", "in", "s\u00fc\u00b7\u00dfem", "Traum", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Zum Himmel blick\u2019 ich dann empor,", "tokens": ["Zum", "Him\u00b7mel", "blick'", "ich", "dann", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er h\u00e4ngt mit Wolken dicht.", "tokens": ["Er", "h\u00e4ngt", "mit", "Wol\u00b7ken", "dicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ach! hinter schwarzem Wolkenflor,", "tokens": ["Ach", "!", "hin\u00b7ter", "schwar\u00b7zem", "Wol\u00b7ken\u00b7flor", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da gl\u00e4nzt des Vollmonds Licht.", "tokens": ["Da", "gl\u00e4nzt", "des", "Voll\u00b7monds", "Licht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}