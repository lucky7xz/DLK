{"textgrid.poem.41605": {"metadata": {"author": {"name": "Baudelaire, Charles", "birth": "N.A.", "death": "N.A."}, "title": "1L: Enthaucht im Herbsttag mir, der m\u00fcd sein Aug' geschlossen,", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Enthaucht im Herbsttag mir, der m\u00fcd sein Aug' geschlossen,", "tokens": ["Ent\u00b7haucht", "im", "Herbst\u00b7tag", "mir", ",", "der", "m\u00fcd", "sein", "Aug'", "ge\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "PPER", "$,", "PRELS", "ADJD", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dein Busen warmen Duft, so f\u00fchl ich mich entrafft", "tokens": ["Dein", "Bu\u00b7sen", "war\u00b7men", "Duft", ",", "so", "f\u00fchl", "ich", "mich", "en\u00b7trafft"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$,", "ADV", "VVFIN", "PPER", "PRF", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Zu seligem Gestad, begl\u00fcckt und m\u00e4rchenhaft,", "tokens": ["Zu", "se\u00b7li\u00b7gem", "Ge\u00b7stad", ",", "be\u00b7gl\u00fcckt", "und", "m\u00e4r\u00b7chen\u00b7haft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "VVPP", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Von ewgem Sonnenglanz einf\u00f6rmig \u00fcbergossen.", "tokens": ["Von", "ew\u00b7gem", "Son\u00b7nen\u00b7glanz", "ein\u00b7f\u00f6r\u00b7mig", "\u00fc\u00b7ber\u00b7gos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD",