{"dta.poem.3345": {"metadata": {"author": {"name": "Uz, Johann Peter", "birth": "N.A.", "death": "N.A."}, "title": "An Herrn Secret\u00e4r G*.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1755", "urn": "urn:nbn:de:kobv:b4-20614-6", "language": ["de:0.99"], "booktitle": "Uz, Johann Peter: Lyrische und andere Gedichte. 2. Aufl. Ansbach, 1755."}, "poem": {"stanza.1": {"line.1": {"text": "Doch Amor lacht bey meinem k\u00fchnen Schwur,", "tokens": ["Doch", "A\u00b7mor", "lacht", "bey", "mei\u00b7nem", "k\u00fch\u00b7nen", "Schwur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VVFIN", "APPR", "PPOSAT", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und rauscht mit gl\u00e4nzendem Gefieder", "tokens": ["Und", "rauscht", "mit", "gl\u00e4n\u00b7zen\u00b7dem", "Ge\u00b7fie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Vor meiner Leyer hin, und fordert meine Lieder.", "tokens": ["Vor", "mei\u00b7ner", "Le\u00b7yer", "hin", ",", "und", "for\u00b7dert", "mei\u00b7ne", "Lie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "$,", "KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Es fesselt mich die herrschende Natur", "tokens": ["Es", "fes\u00b7selt", "mich", "die", "herr\u00b7schen\u00b7de", "Na\u00b7tur"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Zu fest an seinen Sieges-Wagen:", "tokens": ["Zu", "fest", "an", "sei\u00b7nen", "Sie\u00b7ges\u00b7Wa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wer widerstrebt, verdoppelt seine Plagen.", "tokens": ["Wer", "wi\u00b7der\u00b7strebt", ",", "ver\u00b7dop\u00b7pelt", "sei\u00b7ne", "Pla\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Die Nacht, wer kennt sie nicht, die Freundinn holder", "tokens": ["Die", "Nacht", ",", "wer", "kennt", "sie", "nicht", ",", "die", "Freun\u00b7dinn", "hol\u00b7der"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PWS", "VVFIN", "PPER", "PTKNEG", "$,", "ART", "NN", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Glut?", "tokens": ["Glut", "?"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}, "line.9": {"text": "Verfolgt, wenn alles ruht,", "tokens": ["Ver\u00b7folgt", ",", "wenn", "al\u00b7les", "ruht", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "KOUS", "PIS", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Mich mit Erscheinungen und flammenreichen Bildern,", "tokens": ["Mich", "mit", "Er\u00b7schei\u00b7nun\u00b7gen", "und", "flam\u00b7men\u00b7rei\u00b7chen", "Bil\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Die mir die Liebe reizend schildern.", "tokens": ["Die", "mir", "die", "Lie\u00b7be", "rei\u00b7zend", "schil\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "VVPP", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Wer gleichet nicht dem Wuchrer Alfius?", "tokens": ["Wer", "glei\u00b7chet", "nicht", "dem", "Wuch\u00b7rer", "Al\u00b7fi\u00b7us", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PTKNEG", "ART", "NN", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Wie rauscht sein Mund von weisen Sittenspr\u00fcchen!", "tokens": ["Wie", "rauscht", "sein", "Mund", "von", "wei\u00b7sen", "Sit\u00b7ten\u00b7spr\u00fc\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPOSAT", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Die Landlust wird herausgestrichen:", "tokens": ["Die", "Land\u00b7lust", "wird", "her\u00b7aus\u00b7ge\u00b7stri\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Sie ist das h\u00f6chste Gut, das ieder suchen mu\u00df.", "tokens": ["Sie", "ist", "das", "h\u00f6chs\u00b7te", "Gut", ",", "das", "ie\u00b7der", "su\u00b7chen", "mu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,", "PRELS", "PIS", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "O heldenm\u00fcthiger Entschlu\u00df!", "tokens": ["O", "hel\u00b7den\u00b7m\u00fct\u00b7hi\u00b7ger", "Ent\u00b7schlu\u00df", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Er handelt schon um Wies und Felder;", "tokens": ["Er", "han\u00b7delt", "schon", "um", "Wies", "und", "Fel\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "NE", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Er k\u00fcndigt Gelder auf: wie? zeigt sich ein Gewinn?", "tokens": ["Er", "k\u00fcn\u00b7digt", "Gel\u00b7der", "auf", ":", "wie", "?", "zeigt", "sich", "ein", "Ge\u00b7winn", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "PTKVZ", "$.", "PWAV", "$.", "VVFIN", "PRF", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Er wankt und leihet seine Gelder", "tokens": ["Er", "wankt", "und", "lei\u00b7het", "sei\u00b7ne", "Gel\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Auf neuen Wucher hin.", "tokens": ["Auf", "neu\u00b7en", "Wu\u00b7cher", "hin", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.21": {"text": "So sind wir Menschen miteinander!", "tokens": ["So", "sind", "wir", "Men\u00b7schen", "mi\u00b7tein\u00b7an\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Wir prahlen, wie die Alexander;", "tokens": ["Wir", "prah\u00b7len", ",", "wie", "die", "A\u00b7lex\u00b7an\u00b7der", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "ART", "NE", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Und kommt ein holdes M\u00e4dchen, ach!", "tokens": ["Und", "kommt", "ein", "hol\u00b7des", "M\u00e4d\u00b7chen", ",", "ach", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$,", "ITJ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Wer ist nicht schwach?", "tokens": ["Wer", "ist", "nicht", "schwach", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PTKNEG", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.25": {"text": "Wer widersteht erobernden Geberden?", "tokens": ["Wer", "wi\u00b7der\u00b7steht", "er\u00b7o\u00b7bern\u00b7den", "Ge\u00b7ber\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-++-+--", "measure": "unknown.measure.penta"}, "line.26": {"text": "Der gestern, wie ein Almanach,", "tokens": ["Der", "ge\u00b7stern", ",", "wie", "ein", "Al\u00b7ma\u00b7nach", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "PWAV", "ART", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.27": {"text": "Von Eh und Weibern sprach,", "tokens": ["Von", "Eh", "und", "Wei\u00b7bern", "sprach", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.28": {"text": "Kann heute Mann und morgen Hahnrey werden.", "tokens": ["Kann", "heu\u00b7te", "Mann", "und", "mor\u00b7gen", "Hahn\u00b7rey", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "NN", "KON", "ADV", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.29": {"text": "Denn ieder schilt und ieder wagt,", "tokens": ["Denn", "ie\u00b7der", "schilt", "und", "ie\u00b7der", "wagt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "KON", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "Was tausenden mislung, was tausend schon beklagt.", "tokens": ["Was", "tau\u00b7sen\u00b7den", "mis\u00b7lung", ",", "was", "tau\u00b7send", "schon", "be\u00b7klagt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJA", "NN", "$,", "PRELS", "CARD", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Die Wollust einer guten Ehe", "tokens": ["Die", "Wol\u00b7lust", "ei\u00b7ner", "gu\u00b7ten", "E\u00b7he"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.32": {"text": "Verdunkelt iedes Gut, verdunkelt alles Wehe", "tokens": ["Ver\u00b7dun\u00b7kelt", "ie\u00b7des", "Gut", ",", "ver\u00b7dun\u00b7kelt", "al\u00b7les", "We\u00b7he"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PIAT", "NN", "$,", "VVFIN", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Vor unserm trunknen Blick;", "tokens": ["Vor", "un\u00b7serm", "trunk\u00b7nen", "Blick", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.34": {"text": "Und ieder hofft ein gleiches Gl\u00fcck.", "tokens": ["Und", "ie\u00b7der", "hofft", "ein", "glei\u00b7ches", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.35": {"text": "Soll, nach des Himmels Rath, ich endlich mich ver-", "tokens": ["Soll", ",", "nach", "des", "Him\u00b7mels", "Rath", ",", "ich", "end\u00b7lich", "mich", "ver"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VMFIN", "$,", "APPR", "ART", "NN", "NN", "$,", "PPER", "ADV", "PPER", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.36": {"text": "m\u00e4hlen;", "tokens": ["m\u00e4h\u00b7len", ";"], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.37": {"text": "So w\u00e4hl er selbst f\u00fcr mich: kein Sterblicher kann w\u00e4h-", "tokens": ["So", "w\u00e4hl", "er", "selbst", "f\u00fcr", "mich", ":", "kein", "Sterb\u00b7li\u00b7cher", "kann", "w\u00e4h"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "PPER", "ADV", "APPR", "PPER", "$.", "PIAT", "NN", "VMFIN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "len,", "tokens": ["len", ","], "token_info": ["word", "punct"], "pos": ["VVINF", "$,"], "meter": "-", "measure": "single.down"}, "line.39": {"text": "Da\u00df diese Wahl ihm nie gereut.", "tokens": ["Da\u00df", "die\u00b7se", "Wahl", "ihm", "nie", "ge\u00b7reut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.40": {"text": "Liebt mich ein gutes Kind mit wahrer Z\u00e4rtlichkeit;", "tokens": ["Liebt", "mich", "ein", "gu\u00b7tes", "Kind", "mit", "wah\u00b7rer", "Z\u00e4rt\u00b7lich\u00b7keit", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "So hat sie die Vollkommenheit,", "tokens": ["So", "hat", "sie", "die", "Voll\u00b7kom\u00b7men\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.42": {"text": "Die mich entz\u00fcckt, die ich begehre:", "tokens": ["Die", "mich", "ent\u00b7z\u00fcckt", ",", "die", "ich", "be\u00b7geh\u00b7re", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVPP", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.43": {"text": "Sie ist mir Pallas und Cythere.", "tokens": ["Sie", "ist", "mir", "Pal\u00b7las", "und", "Cy\u00b7the\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "NN", "KON", "NE", "$."], "meter": "-+-+--+--", "measure": "iambic.tri.relaxed"}, "line.44": {"text": "Das, Freund! ist meine Sittenlehre!", "tokens": ["Das", ",", "Freund", "!", "ist", "mei\u00b7ne", "Sit\u00b7ten\u00b7leh\u00b7re", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "NN", "$.", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}