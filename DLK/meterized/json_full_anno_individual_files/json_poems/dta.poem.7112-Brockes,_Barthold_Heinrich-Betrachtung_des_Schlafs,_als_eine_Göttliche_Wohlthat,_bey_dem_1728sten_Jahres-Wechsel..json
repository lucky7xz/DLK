{"dta.poem.7112": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Betrachtung des Schlafs,  \n als  \n eine G\u00f6ttliche Wohlthat,  \n bey dem 1728sten Jahres-Wechsel.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wofern uns eine Zeit zu GOTTES Ruhm verpflichtet,", "tokens": ["Wo\u00b7fern", "uns", "ei\u00b7ne", "Zeit", "zu", "GoT\u00b7TES", "Ruhm", "ver\u00b7pflich\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "NE", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So ist es die ja wol, wenn ein verneu\u2019tes Jahr", "tokens": ["So", "ist", "es", "die", "ja", "wol", ",", "wenn", "ein", "ver\u00b7neu'\u00b7tes", "Jahr"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ART", "ADV", "ADV", "$,", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Den Theil von unsrer Welt zur Sonnen wieder richtet,", "tokens": ["Den", "Theil", "von", "uns\u00b7rer", "Welt", "zur", "Son\u00b7nen", "wie\u00b7der", "rich\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "APPRART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wovon im vorigen sie weit gewichen war.", "tokens": ["Wo\u00b7von", "im", "vo\u00b7ri\u00b7gen", "sie", "weit", "ge\u00b7wi\u00b7chen", "war", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPRART", "ADJA", "PPER", "ADJD", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wolan denn, weil die Zeit nun abermal erschienen,", "tokens": ["Wo\u00b7lan", "denn", ",", "weil", "die", "Zeit", "nun", "a\u00b7ber\u00b7mal", "er\u00b7schie\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "KOUS", "ART", "NN", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Da sich des Sch\u00f6pffers Macht, in der sich dreh\u2019nden Welt,", "tokens": ["Da", "sich", "des", "Sch\u00f6pf\u00b7fers", "Macht", ",", "in", "der", "sich", "dreh'n\u00b7den", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "NN", "$,", "APPR", "PRELS", "PRF", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Zum Nutz der Creatur, aufs neu\u2019 vor Augen stellt;", "tokens": ["Zum", "Nutz", "der", "Crea\u00b7tur", ",", "aufs", "neu'", "vor", "Au\u00b7gen", "stellt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,", "APPRART", "ADJA", "APPR", "NN", "VVFIN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "So lasst uns Jhn nicht nur mit solchen Dancken dienen,", "tokens": ["So", "lasst", "uns", "Jhn", "nicht", "nur", "mit", "sol\u00b7chen", "Dan\u00b7cken", "die\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "PTKNEG", "ADV", "APPR", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Das blo\u00df nach Eigen-Lieb\u2019 und eig\u2019nen Vortheil schmeckt,", "tokens": ["Das", "blo\u00df", "nach", "Ei\u00b7gen\u00b7Lieb'", "und", "eig'\u00b7nen", "Vor\u00b7theil", "schmeckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "APPR", "NN", "KON", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Da man sich selbst aus Ernst; mit eitlem Wort-Gepr\u00e4nge", "tokens": ["Da", "man", "sich", "selbst", "aus", "Ernst", ";", "mit", "eit\u00b7lem", "Wor\u00b7tGe\u00b7pr\u00e4n\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PIS", "PRF", "ADV", "APPR", "NE", "$.", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Hingegen andern offt, worinnen sonst nichts steckt,", "tokens": ["Hin\u00b7ge\u00b7gen", "an\u00b7dern", "offt", ",", "wo\u00b7rin\u00b7nen", "sonst", "nichts", "steckt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "ADV", "$,", "PWAV", "ADV", "PIS", "VVFIN", "$,"], "meter": "---+-+-+-+-+", "measure": "unknown.measure.penta"}, "line.12": {"text": "Als blo\u00df ein Schall und leerer T\u00f6ne Menge,", "tokens": ["Als", "blo\u00df", "ein", "Schall", "und", "lee\u00b7rer", "T\u00f6\u00b7ne", "Men\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "KON", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Ein fr\u00f6lichs Neu-Jahr w\u00fcnscht. Nein, lasst uns uns er-", "tokens": ["Ein", "fr\u00f6\u00b7lichs", "Neu\u00b7Jahr", "w\u00fcnscht", ".", "Nein", ",", "lasst", "uns", "uns", "er"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$.", "PTKANT", "$,", "VVFIN", "PPER", "PRF", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "wecken,", "tokens": ["we\u00b7cken", ","], "token_info": ["word", "punct"], "pos": ["VVINF", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.15": {"text": "Aus dem Gewohnheits-Schlaf! Auf! lasst uns, GOTT", "tokens": ["Aus", "dem", "Ge\u00b7wohn\u00b7heits\u00b7Schlaf", "!", "Auf", "!", "lasst", "uns", ",", "GoTT"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word"], "pos": ["APPR", "ART", "NN", "$.", "APPR", "$.", "VVFIN", "PPER", "$,", "NE"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "zum Preise,", "tokens": ["zum", "Prei\u00b7se", ","], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.17": {"text": "Ein herrlichs Gut, das er, auf wunderbarer Weise,", "tokens": ["Ein", "herr\u00b7lichs", "Gut", ",", "das", "er", ",", "auf", "wun\u00b7der\u00b7ba\u00b7rer", "Wei\u00b7se", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPER", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "In unserm Leben uns so wunderbar zu schencken,", "tokens": ["In", "un\u00b7serm", "Le\u00b7ben", "uns", "so", "wun\u00b7der\u00b7bar", "zu", "schen\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PPER", "ADV", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "So gn\u00e4dig w\u00fcrdiget; betrachten, \u00fcberdencken", "tokens": ["So", "gn\u00e4\u00b7dig", "w\u00fcr\u00b7di\u00b7get", ";", "be\u00b7trach\u00b7ten", ",", "\u00fc\u00b7ber\u00b7den\u00b7cken"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word"], "pos": ["ADV", "ADJD", "VVFIN", "$.", "VVFIN", "$,", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Und wohl behertzigen. Denn es ist eins der gr\u00f6sten,", "tokens": ["Und", "wohl", "be\u00b7hert\u00b7zi\u00b7gen", ".", "Denn", "es", "ist", "eins", "der", "gr\u00f6s\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVINF", "$.", "KON", "PPER", "VAFIN", "PIS", "ART", "ADJA", "$,"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.21": {"text": "Der wunderw\u00fcrdigsten und besten,", "tokens": ["Der", "wun\u00b7der\u00b7w\u00fcr\u00b7digs\u00b7ten", "und", "bes\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "KON", "VVFIN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.22": {"text": "Wenn man es recht betrachtet und erwegt,", "tokens": ["Wenn", "man", "es", "recht", "be\u00b7trach\u00b7tet", "und", "er\u00b7wegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "ADJD", "VVPP", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "Was unser GOTT darinn f\u00fcr Nutz und Lust gelegt.", "tokens": ["Was", "un\u00b7ser", "GoTT", "da\u00b7rinn", "f\u00fcr", "Nutz", "und", "Lust", "ge\u00b7legt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "PAV", "APPR", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}