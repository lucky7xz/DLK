{"dta.poem.2365": {"metadata": {"author": {"name": "Neumark, Georg", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1652", "urn": "urn:nbn:de:kobv:b4-20428-0", "language": ["de:0.99"], "booktitle": "Neumark, Georg: Poetisch- und Musikalisches Lustw\u00e4ldchen. Hamburg, 1652."}, "poem": {"stanza.1": {"line.1": {"text": "Mein Gott wenn soll es doch geschehen/", "tokens": ["Mein", "Gott", "wenn", "soll", "es", "doch", "ge\u00b7sche\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KOUS", "VMFIN", "PPER", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df ich mag endlich wieder sehen/", "tokens": ["Da\u00df", "ich", "mag", "end\u00b7lich", "wie\u00b7der", "se\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VMFIN", "ADV", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Das hoch beliebte ", "tokens": ["Das", "hoch", "be\u00b7lieb\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["PDS", "ADJD", "ADJA"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Und meinen wehrten Unsterstrand?", "tokens": ["Und", "mei\u00b7nen", "wehr\u00b7ten", "Uns\u00b7ter\u00b7strand", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wie lange soll ich noch so reisen/", "tokens": ["Wie", "lan\u00b7ge", "soll", "ich", "noch", "so", "rei\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VMFIN", "PPER", "ADV", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und mich mit blo\u00dfex Hoffnung speisen?", "tokens": ["Und", "mich", "mit", "blo\u00df\u00b7ex", "Hoff\u00b7nung", "spei\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Neun Jahre sind vorbey geschossen", "tokens": ["Neun", "Jah\u00b7re", "sind", "vor\u00b7bey", "ge\u00b7schos\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["CARD", "NN", "VAFIN", "ADV", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und wie ein Strohm dahin geflossen/", "tokens": ["Und", "wie", "ein", "Strohm", "da\u00b7hin", "ge\u00b7flos\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "PAV", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df ich die liebe ", "tokens": ["Da\u00df", "ich", "die", "lie\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADJA"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Die mich der ", "tokens": ["Die", "mich", "der"], "token_info": ["word", "word", "word"], "pos": ["ART", "PPER", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "Nicht habe/ wie ich soll/ gegr\u00fcsset", "tokens": ["Nicht", "ha\u00b7be", "/", "wie", "ich", "soll", "/", "ge\u00b7gr\u00fcs\u00b7set"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["PTKNEG", "VAFIN", "$(", "PWAV", "PPER", "VMFIN", "$(", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Nochmeines ", "tokens": ["Noch\u00b7mei\u00b7nes"], "token_info": ["word"], "pos": ["NE"], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.3": {"line.1": {"text": "Hier leb' ich zwar in Friedensfreuden/", "tokens": ["Hier", "leb'", "ich", "zwar", "in", "Frie\u00b7dens\u00b7freu\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "NE", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und wei\u00df von keinem Kriegesleiden/", "tokens": ["Und", "wei\u00df", "von", "kei\u00b7nem", "Krie\u00b7ge\u00b7slei\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PIAT", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wie leider dort so mancher Mann/", "tokens": ["Wie", "lei\u00b7der", "dort", "so", "man\u00b7cher", "Mann", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADV", "ADV", "PIAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der seine Noht kaum z\u00e4hlen kan.", "tokens": ["Der", "sei\u00b7ne", "Noht", "kaum", "z\u00e4h\u00b7len", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der kaum was anders wei\u00df zu sagen/", "tokens": ["Der", "kaum", "was", "an\u00b7ders", "wei\u00df", "zu", "sa\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PWS", "ADV", "VVFIN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Als von betr\u00fcbten ", "tokens": ["Als", "von", "be\u00b7tr\u00fcb\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "APPR", "ADJA"], "meter": "++-+-", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Ich bin/ ", "tokens": ["Ich", "bin", "/"], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VAFIN", "$("], "meter": "-+", "measure": "iambic.single"}, "line.2": {"text": "Alhier bey gro\u00df-und weisen ", "tokens": ["Al\u00b7hier", "bey", "gro\u00df\u00b7\u00b7und", "wei\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJD", "VVINF"], "meter": "---+-+-", "measure": "unknown.measure.di"}, "line.3": {"text": "Wol angesehn und sehr beliebt/", "tokens": ["Wol", "an\u00b7ge\u00b7sehn", "und", "sehr", "be\u00b7liebt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "KON", "ADV", "ADJD", "$("], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Ich bin gar selten auch betr\u00fcbt/", "tokens": ["Ich", "bin", "gar", "sel\u00b7ten", "auch", "be\u00b7tr\u00fcbt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "ADV", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich sp\u00fchr\u2019 und merk\u2019 auf allen wegen/", "tokens": ["Ich", "sp\u00fchr'", "und", "merk'", "auf", "al\u00b7len", "we\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "APPR", "PIAT", "APPR", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Den hertzgew\u00fcnschten Himmelssegen.", "tokens": ["Den", "hertz\u00b7ge\u00b7w\u00fcnschten", "Him\u00b7mels\u00b7se\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Es hat nicht vielen so gegl\u00fckket/", "tokens": ["Es", "hat", "nicht", "vie\u00b7len", "so", "ge\u00b7gl\u00fck\u00b7ket", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADV", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie Gott es hat mit mir geschikket;", "tokens": ["Wie", "Gott", "es", "hat", "mit", "mir", "ge\u00b7schik\u00b7ket", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "PPER", "VAFIN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wie Mancher sieht mich scheeldr\u00fcm an", "tokens": ["Wie", "Man\u00b7cher", "sieht", "mich", "scheel\u00b7dr\u00fcm", "an"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PIS", "VVFIN", "PPER", "VVFIN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df mir das Gl\u00fckk so zugethan/", "tokens": ["Da\u00df", "mir", "das", "Gl\u00fckk", "so", "zu\u00b7ge\u00b7than", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Doch acht\u2019 ich nicht der Sp\u00f6tter Neiden", "tokens": ["Doch", "acht'", "ich", "nicht", "der", "Sp\u00f6t\u00b7ter", "Nei\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ich kan es alles lachend leideu.", "tokens": ["Ich", "kan", "es", "al\u00b7les", "la\u00b7chend", "lei\u00b7deu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PIS", "ADJD", "ADJD", "$."], "meter": "-+-+-+-++", "measure": "unknown.measure.penta"}}, "stanza.6": {"line.1": {"text": "Dennoch wie gut es mit mir gehet/", "tokens": ["Den\u00b7noch", "wie", "gut", "es", "mit", "mir", "ge\u00b7het", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ADJD", "PPER", "APPR", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie k\u00f6stlich meine Wolfahrt stehet/", "tokens": ["Wie", "k\u00f6st\u00b7lich", "mei\u00b7ne", "Wol\u00b7fahrt", "ste\u00b7het", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wie freundlich mir das Gl\u00fckke lacht/", "tokens": ["Wie", "freund\u00b7lich", "mir", "das", "Gl\u00fck\u00b7ke", "lacht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und was es mir vor Freude macht/", "tokens": ["Und", "was", "es", "mir", "vor", "Freu\u00b7de", "macht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "PRF", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So sag\u2019 ich doch da\u00df ichs nicht achte/", "tokens": ["So", "sag'", "ich", "doch", "da\u00df", "ichs", "nicht", "ach\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "KOUS", "PIS", "PTKNEG", "ADJA", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wenn ich mein Vaterland betrachte.", "tokens": ["Wenn", "ich", "mein", "Va\u00b7ter\u00b7land", "be\u00b7trach\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Ich wei\u00df nicht was f\u00fcr eine St\u00e4rke/", "tokens": ["Ich", "wei\u00df", "nicht", "was", "f\u00fcr", "ei\u00b7ne", "St\u00e4r\u00b7ke", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "PRELS", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ich bey mir im Gem\u00fchte merke/", "tokens": ["Ich", "bey", "mir", "im", "Ge\u00b7m\u00fch\u00b7te", "mer\u00b7ke", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPER", "APPRART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die mich so sehr begierig macht/", "tokens": ["Die", "mich", "so", "sehr", "be\u00b7gie\u00b7rig", "macht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADV", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df ich numehr fast Tag und Nacht", "tokens": ["Da\u00df", "ich", "nu\u00b7mehr", "fast", "Tag", "und", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADV", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die lieben Meinigen bed\u00e4nke/", "tokens": ["Die", "lie\u00b7ben", "Mei\u00b7ni\u00b7gen", "be\u00b7d\u00e4n\u00b7ke", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und mich/ blo\u00df Sie zu spr\u00e4chen/ kr\u00e4nke.", "tokens": ["Und", "mich", "/", "blo\u00df", "Sie", "zu", "spr\u00e4\u00b7chen", "/", "kr\u00e4n\u00b7ke", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PPER", "$(", "ADV", "PPER", "PTKZU", "VVINF", "$(", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Mein\u2019 h\u00f6chste ", "tokens": ["Mein'", "h\u00f6chs\u00b7te"], "token_info": ["word", "word"], "pos": ["PPOSAT", "ADJA"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Es w\u00fcnschen t\u00e4glich meine Sinnen/", "tokens": ["Es", "w\u00fcn\u00b7schen", "t\u00e4g\u00b7lich", "mei\u00b7ne", "Sin\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df ich doch bald den lieben Tag/", "tokens": ["Da\u00df", "ich", "doch", "bald", "den", "lie\u00b7ben", "Tag", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sie frisch zu sehn/ erleben mag.", "tokens": ["Sie", "frisch", "zu", "sehn", "/", "er\u00b7le\u00b7ben", "mag."], "token_info": ["word", "word", "word", "word", "punct", "word", "abbreviation"], "pos": ["PPER", "ADJD", "PTKZU", "VVINF", "$(", "VVFIN", "NE"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Nim auf/ O ", "tokens": ["Nim", "auf", "/", "O"], "token_info": ["word", "word", "punct", "word"], "pos": ["NE", "APPR", "$(", "NE"], "meter": "+++", "measure": "unknown.measure.tri"}, "line.6": {"text": "So wird es/ hoff\u2019 ich/ bald geschehen.", "tokens": ["So", "wird", "es", "/", "hoff'", "ich", "/", "bald", "ge\u00b7sche\u00b7hen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "$(", "VVFIN", "PPER", "$(", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}