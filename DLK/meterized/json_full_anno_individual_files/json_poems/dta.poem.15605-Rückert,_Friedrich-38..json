{"dta.poem.15605": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "38.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wo mit der Dumpfheit sich die Wissenschaft verb\u00fcndet,", "tokens": ["Wo", "mit", "der", "Dum\u00b7pf\u00b7heit", "sich", "die", "Wis\u00b7sen\u00b7schaft", "ver\u00b7b\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "PRF", "ART", "NN", "VVPP", "$,"], "meter": "---++-+-+-+-+-", "measure": "unknown.measure.hexa"}, "line.2": {"text": "Wird Unerfreuliches kunstm\u00e4\u00dfig fest gegr\u00fcndet.", "tokens": ["Wird", "Un\u00b7er\u00b7freu\u00b7li\u00b7ches", "kunst\u00b7m\u00e4\u00b7\u00dfig", "fest", "ge\u00b7gr\u00fcn\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "ADJD", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Und eh'r nicht wieder wird der Zwingbau eingerissen,", "tokens": ["Und", "eh'r", "nicht", "wie\u00b7der", "wird", "der", "Zwing\u00b7bau", "ein\u00b7ge\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PTKNEG", "ADV", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bis gegen knechtisches aufsteht ein freies Wissen.", "tokens": ["Bis", "ge\u00b7gen", "knech\u00b7ti\u00b7sches", "auf\u00b7steht", "ein", "frei\u00b7es", "Wis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ADJA", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+--+-+-", "measure": "dactylic.tri.plus"}}, "stanza.3": {"line.1": {"text": "Dann w\u00e4chst der Freiheit Haus selbst aus der Knechtschaft Tr\u00fcmmern,", "tokens": ["Dann", "w\u00e4chst", "der", "Frei\u00b7heit", "Haus", "selbst", "aus", "der", "Knecht\u00b7schaft", "Tr\u00fcm\u00b7mern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN", "ADV", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "F\u00fcr alle die zuvor im Kerker nicht verk\u00fcmmern.", "tokens": ["F\u00fcr", "al\u00b7le", "die", "zu\u00b7vor", "im", "Ker\u00b7ker", "nicht", "ver\u00b7k\u00fcm\u00b7mern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "ART", "ADV", "APPRART", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}