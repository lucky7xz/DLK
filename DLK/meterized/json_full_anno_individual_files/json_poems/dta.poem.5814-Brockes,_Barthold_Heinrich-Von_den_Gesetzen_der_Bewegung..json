{"dta.poem.5814": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den Gesetzen der Bewegung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wir aber, die der Sinn verf\u00fchret,", "tokens": ["Wir", "a\u00b7ber", ",", "die", "der", "Sinn", "ver\u00b7f\u00fch\u00b7ret", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die wir gewohnt zu sehn, von Jugend auf", "tokens": ["Die", "wir", "ge\u00b7wohnt", "zu", "sehn", ",", "von", "Ju\u00b7gend", "auf"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "PPER", "VVPP", "PTKZU", "VVINF", "$,", "APPR", "NN", "APPR"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df sich ein C\u00f6rper nicht mehr r\u00fchret:", "tokens": ["Da\u00df", "sich", "ein", "C\u00f6r\u00b7per", "nicht", "mehr", "r\u00fch\u00b7ret", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "PTKNEG", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wir schliessen: da\u00df er selbst den Lauff", "tokens": ["Wir", "schlies\u00b7sen", ":", "da\u00df", "er", "selbst", "den", "Lauff"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "KOUS", "PPER", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und die Bewegung hemm\u2019 und da\u00df zur Ruh allein", "tokens": ["Und", "die", "Be\u00b7we\u00b7gung", "hemm'", "und", "da\u00df", "zur", "Ruh", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "KON", "KOUS", "APPRART", "NN", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die Kr\u00e4ffte, die er zeigt, von ihm gebrauchet seyn.", "tokens": ["Die", "Kr\u00e4ff\u00b7te", ",", "die", "er", "zeigt", ",", "von", "ihm", "ge\u00b7brau\u00b7chet", "seyn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,", "APPR", "PPER", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wir sollten aber billig dencken,", "tokens": ["Wir", "soll\u00b7ten", "a\u00b7ber", "bil\u00b7lig", "den\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Da\u00df sich der C\u00f6rper Lauff nicht selber unterbreche,", "tokens": ["Da\u00df", "sich", "der", "C\u00f6r\u00b7per", "Lauff", "nicht", "sel\u00b7ber", "un\u00b7ter\u00b7bre\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "NN", "PTKNEG", "ADV", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Da\u00df er sich selber nie verringere noch schw\u00e4che,", "tokens": ["Da\u00df", "er", "sich", "sel\u00b7ber", "nie", "ver\u00b7rin\u00b7ge\u00b7re", "noch", "schw\u00e4\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "ADV", "VVFIN", "ADV", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Wofern ihm nichts im Wege st\u00fcnde", "tokens": ["Wo\u00b7fern", "ihm", "nichts", "im", "We\u00b7ge", "st\u00fcn\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PIS", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Und er nicht seine Krafft dadurch verringert f\u00fcnde.", "tokens": ["Und", "er", "nicht", "sei\u00b7ne", "Krafft", "da\u00b7durch", "ver\u00b7rin\u00b7gert", "f\u00fcn\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PTKNEG", "PPOSAT", "NN", "PAV", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Man mu\u00df hiebey zugleich auch dies erwegen:", "tokens": ["Man", "mu\u00df", "hie\u00b7bey", "zu\u00b7gleich", "auch", "dies", "er\u00b7we\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "ADV", "ADV", "ADV", "PDS", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Es mu\u00df ein jedes Ding in seinem Stande bleiben,", "tokens": ["Es", "mu\u00df", "ein", "je\u00b7des", "Ding", "in", "sei\u00b7nem", "Stan\u00b7de", "blei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "PIAT", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Wofern ihn andre nicht aus selben treiben.", "tokens": ["Wo\u00b7fern", "ihn", "and\u00b7re", "nicht", "aus", "sel\u00b7ben", "trei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIS", "PTKNEG", "APPR", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Es kan ja die Natur selbst keinem auferlegen", "tokens": ["Es", "kan", "ja", "die", "Na\u00b7tur", "selbst", "kei\u00b7nem", "auf\u00b7er\u00b7le\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "ART", "NN", "ADV", "PIS", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Da\u00df es sich selbst durch sich zerst\u00f6hre.", "tokens": ["Da\u00df", "es", "sich", "selbst", "durch", "sich", "zer\u00b7st\u00f6h\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "APPR", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Und folgt dahero ganz gewis,", "tokens": ["Und", "folgt", "da\u00b7he\u00b7ro", "ganz", "ge\u00b7wis", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PAV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Da\u00df eine fremde Hindernis,", "tokens": ["Da\u00df", "ei\u00b7ne", "frem\u00b7de", "Hin\u00b7der\u00b7nis", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Die jedes \u00e4ndere, dazu geh\u00f6re.", "tokens": ["Die", "je\u00b7des", "\u00e4n\u00b7de\u00b7re", ",", "da\u00b7zu", "ge\u00b7h\u00f6\u00b7re", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PIAT", "ADJA", "$,", "PAV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}