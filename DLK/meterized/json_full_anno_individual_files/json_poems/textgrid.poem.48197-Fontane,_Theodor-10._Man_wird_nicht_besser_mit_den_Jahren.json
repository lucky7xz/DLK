{"textgrid.poem.48197": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "10. Man wird nicht besser mit den Jahren", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Man wird nicht besser mit den Jahren,", "tokens": ["Man", "wird", "nicht", "bes\u00b7ser", "mit", "den", "Jah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PTKNEG", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie sollt' es auch, man wird bequem", "tokens": ["Wie", "sollt'", "es", "auch", ",", "man", "wird", "be\u00b7quem"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "VMFIN", "PPER", "ADV", "$,", "PIS", "VAFIN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und bringt, um sich die Reu' zu sparen,", "tokens": ["Und", "bringt", ",", "um", "sich", "die", "Reu'", "zu", "spa\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUI", "PRF", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Fehler all in ein System.", "tokens": ["Die", "Feh\u00b7ler", "all", "in", "ein", "Sys\u00b7tem", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "APPR", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Das gibt dann eine glatte Fl\u00e4che,", "tokens": ["Das", "gibt", "dann", "ei\u00b7ne", "glat\u00b7te", "Fl\u00e4\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "me