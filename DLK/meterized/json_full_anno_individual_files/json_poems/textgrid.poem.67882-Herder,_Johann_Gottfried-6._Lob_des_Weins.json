_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wie dann der Flu\u00df", "tokens": ["Wie", "dann", "der", "Flu\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ART", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Des Pegasus", "tokens": ["Des", "Pe\u00b7ga\u00b7sus"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Kommt auf uns zugeschossen:", "tokens": ["Kommt", "auf", "uns", "zu\u00b7ge\u00b7schos\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Der will dann ein", "tokens": ["Der", "will", "dann", "ein"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "VMFIN", "ADV", "ART"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Poete seyn;", "tokens": ["Poe\u00b7te", "seyn", ";"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VAINF", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.9": {"text": "Der kann viel Streitens machen", "tokens": ["Der", "kann", "viel", "Strei\u00b7tens", "ma\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VMFIN", "PIAT", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Von der Natur;", "tokens": ["Von", "der", "Na\u00b7tur", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.11": {"text": "Der redet nur", "tokens": ["Der", "re\u00b7det", "nur"], "token_info": ["word", "word", "word"], "pos": ["PDS", "VVFIN", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.12": {"text": "Von Gottes hohen Sachen.", "tokens": ["Von", "Got\u00b7tes", "ho\u00b7hen", "Sa\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Auch mir wird itzt", "tokens": ["Auch", "mir", "wird", "itzt"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "PPER", "VAFIN", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Der Kopf erhitzt,", "tokens": ["Der", "Kopf", "er\u00b7hitzt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "O Wein, von deinen Gaben", "tokens": ["O", "Wein", ",", "von", "dei\u00b7nen", "Ga\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NN", "$,", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Zunge singt,", "tokens": ["Die", "Zun\u00b7ge", "singt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Die Seele springt,", "tokens": ["Die", "See\u00b7le", "springt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Die F\u00fcsse wollen traben,", "tokens": ["Die", "F\u00fcs\u00b7se", "wol\u00b7len", "tra\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Wohlan! noch ba\u00df", "tokens": ["Wo\u00b7hlan", "!", "noch", "ba\u00df"], "token_info": ["word", "punct", "word", "word"], "pos": ["ADV", "$.", "ADV", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Durch dieses Glas", "tokens": ["Durch", "die\u00b7ses", "Glas"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PDAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Will ich auf dich jezt zielen,", "tokens": ["Will", "ich", "auf", "dich", "jezt", "zie\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Du deutsches Blut", "tokens": ["Du", "deut\u00b7sches", "Blut"], "token_info": ["word", "word", "word"], "pos": ["PPER", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.11": {"text": "Treu, fest und gut!", "tokens": ["Treu", ",", "fest", "und", "gut", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.12": {"text": "La\u00dft Eins zum Tanz mir spielen!", "tokens": ["La\u00dft", "Eins", "zum", "Tanz", "mir", "spie\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "NN", "APPRART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}