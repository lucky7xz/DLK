": ["Macht", "uns", "be\u00b7reit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJD", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Barmherzigkeit", "tokens": ["Barm\u00b7her\u00b7zig\u00b7keit"], "token_info": ["word"], "pos": ["NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Dem Armuth zu erzeigen;", "tokens": ["Dem", "Ar\u00b7muth", "zu", "er\u00b7zei\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Wie auch beherzt,", "tokens": ["Wie", "auch", "be\u00b7herzt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADJD", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "Das was uns schmerzt,", "tokens": ["Das", "was", "uns", "schmerzt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Zu eifern und zu l\u00e4stern:", "tokens": ["Zu", "ei\u00b7fern", "und", "zu", "l\u00e4s\u00b7tern", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "KON", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Ertheilt die Kunst", "tokens": ["Er\u00b7theilt", "die", "Kunst"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.11": {"text": "Und alle Gunst", "tokens": ["Und", "al\u00b7le", "Gunst"], "token_info": ["word", "word", "word"], "pos": ["KON", "PIAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.12": {"text": "Der dreimal dreien Schwestern.", "tokens": ["Der", "drei\u00b7mal", "drei\u00b7en", "Schwes\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "CARD", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Daher man sieht,", "tokens": ["Da\u00b7her", "man", "sieht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PAV", "PIS", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Wann wir hiemit", "tokens": ["Wann", "wir", "hie\u00b7mit"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "PPER", "PAV"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Das Herz uns kaum begossen,", "tokens": ["Das", "Herz", "uns", "kaum", "be\u00b7gos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wie dann der Flu\u00df", "tokens": ["Wie", "dann", "der", "Flu\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ART", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Des Pegasus", "tokens": ["Des", "Pe\u00b7ga\u00b7sus"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Kommt auf uns zugeschossen:", "tokens": ["Kommt", "auf", "uns", "zu\u00b7ge\u00b7schos\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Der will dann ein", "tokens": ["Der", "will", "dann", "ein"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "VMFIN", "ADV", "ART"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Poete seyn;", "tokens": ["Poe\u00b7te", "seyn", ";"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VAINF", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.9": {"text": "Der kann viel Streitens machen", "tokens": ["Der", "kann", "viel", "Strei\u00b7tens", "ma\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": 