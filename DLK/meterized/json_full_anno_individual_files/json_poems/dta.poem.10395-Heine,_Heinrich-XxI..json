{"dta.poem.10395": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "XxI.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "So hast du ganz und gar vergessen,", "tokens": ["So", "hast", "du", "ganz", "und", "gar", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "KON", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df ich so lang dein Herz besessen,", "tokens": ["Da\u00df", "ich", "so", "lang", "dein", "Herz", "be\u00b7ses\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Dein Herzchen so s\u00fc\u00df und so falsch und so klein,", "tokens": ["Dein", "Herz\u00b7chen", "so", "s\u00fc\u00df", "und", "so", "falsch", "und", "so", "klein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "ADJD", "KON", "ADV", "ADJD", "KON", "ADV", "ADJD", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "'s kann nirgend was s\u00fc\u00df'res und falscheres seyn.", "tokens": ["'s", "kann", "nir\u00b7gend", "was", "s\u00fc\u00df'\u00b7res", "und", "fal\u00b7sche\u00b7res", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PWS", "ADJA", "KON", "ADJA", "VAINF", "$."], "meter": "+-+--+--+--+", "measure": "trochaic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "So hast du die Lieb' und das Leid vergessen,", "tokens": ["So", "hast", "du", "die", "Lieb'", "und", "das", "Leid", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "KON", "ART", "NN", "VVPP", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Die's Herz mir th\u00e4ten zusammen pressen.", "tokens": ["Die's", "Herz", "mir", "th\u00e4\u00b7ten", "zu\u00b7sam\u00b7men", "pres\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "PPER", "VVFIN", "ADV", "VVFIN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ich wei\u00df nicht war Liebe gr\u00f6\u00dfer als Leid?", "tokens": ["Ich", "wei\u00df", "nicht", "war", "Lie\u00b7be", "gr\u00f6\u00b7\u00dfer", "als", "Leid", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "VAFIN", "NN", "ADJD", "KOKOM", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ich wei\u00df nur sie waren gro\u00df allebeid'!", "tokens": ["Ich", "wei\u00df", "nur", "sie", "wa\u00b7ren", "gro\u00df", "al\u00b7le\u00b7beid'", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PPER", "VAFIN", "ADJD", "PTKVZ", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}}}}