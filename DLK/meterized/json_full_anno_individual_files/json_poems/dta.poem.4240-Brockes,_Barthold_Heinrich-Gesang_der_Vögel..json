{"dta.poem.4240": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Gesang der V\u00f6gel.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich sah, auf einem schwanken Ast, fast auf desselben \u00e4ussern", "tokens": ["Ich", "sah", ",", "auf", "ei\u00b7nem", "schwan\u00b7ken", "Ast", ",", "fast", "auf", "des\u00b7sel\u00b7ben", "\u00e4us\u00b7sern"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "APPR", "ART", "ADJA", "NN", "$,", "ADV", "APPR", "PDS", "VVINF"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "Spitzen,", "tokens": ["Spit\u00b7zen", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Ein kleines buntes V\u00f6gelein, bequem und recht geruhig,", "tokens": ["Ein", "klei\u00b7nes", "bun\u00b7tes", "V\u00f6\u00b7ge\u00b7lein", ",", "be\u00b7quem", "und", "recht", "ge\u00b7ru\u00b7hig", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,", "ADJD", "KON", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.4": {"text": "sitzen;", "tokens": ["sit\u00b7zen", ";"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Es regt\u2019 an seinem ganzen C\u00f6rper sich nichts, sein kleiner", "tokens": ["Es", "regt'", "an", "sei\u00b7nem", "gan\u00b7zen", "C\u00f6r\u00b7per", "sich", "nichts", ",", "sein", "klei\u00b7ner"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "PRF", "PIS", "$,", "PPOSAT", "ADJA"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Kopf allein", "tokens": ["Kopf", "al\u00b7lein"], "token_info": ["word", "word"], "pos": ["NN", "ADV"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Schien des bebl\u00fchmten Fr\u00fchlings Pracht, zumahl der", "tokens": ["Schien", "des", "be\u00b7bl\u00fchm\u00b7ten", "Fr\u00fch\u00b7lings", "Pracht", ",", "zu\u00b7mahl", "der"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN", "NN", "$,", "KOUS", "ART"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Sonnen hellen Schein,", "tokens": ["Son\u00b7nen", "hel\u00b7len", "Schein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.9": {"text": "Durch stetes Drehen, zu bewundern. Bald bog er es zu", "tokens": ["Durch", "ste\u00b7tes", "Dre\u00b7hen", ",", "zu", "be\u00b7wun\u00b7dern", ".", "Bald", "bog", "er", "es", "zu"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "PTKZU", "VVINF", "$.", "ADV", "VVFIN", "PPER", "PPER", "PTKZU"], "meter": "+--+-+-+--+--+", "measure": "iambic.hexa.invert"}, "line.10": {"text": "beyden Seiten,", "tokens": ["bey\u00b7den", "Sei\u00b7ten", ","], "token_info": ["word", "word", "punct"], "pos": ["PIAT", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.11": {"text": "Bald wendet er es in die R\u00fcnde, entz\u00fcckt ob allen Lieblich-", "tokens": ["Bald", "wen\u00b7det", "er", "es", "in", "die", "R\u00fcn\u00b7de", ",", "ent\u00b7z\u00fcckt", "ob", "al\u00b7len", "Lieb\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "APPR", "ART", "NN", "$,", "VVFIN", "KOUS", "PIAT", "NN"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.12": {"text": "keiten.", "tokens": ["kei\u00b7ten", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.13": {"text": "Jtzt kehrt sein Blick sich Himmel- werts, itzt wieder auf die", "tokens": ["Jtzt", "kehrt", "sein", "Blick", "sich", "Him\u00b7mel", "werts", ",", "itzt", "wie\u00b7der", "auf", "die"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "PRF", "TRUNC", "VAFIN", "$,", "ADV", "ADV", "APPR", "ART"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Welt hernieder,", "tokens": ["Welt", "her\u00b7nie\u00b7der", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.15": {"text": "Und gurgelt, in so muntrer Wendung, aus Lieb\u2019 und Lob", "tokens": ["Und", "gur\u00b7gelt", ",", "in", "so", "mun\u00b7trer", "Wen\u00b7dung", ",", "aus", "Lieb'", "und", "Lob"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "APPR", "ADV", "ADJA", "NN", "$,", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.16": {"text": "erzeugte Lieder.", "tokens": ["er\u00b7zeug\u00b7te", "Lie\u00b7der", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Man konnt\u2019 aus seiner regen Kehle, bey nimmer unter-", "tokens": ["Man", "konnt'", "aus", "sei\u00b7ner", "re\u00b7gen", "Keh\u00b7le", ",", "bey", "nim\u00b7mer", "un\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PIS", "VMFIN", "APPR", "PPOSAT", "ADJA", "NN", "$,", "APPR", "ADV", "TRUNC"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "brochnem Drehn,", "tokens": ["broch\u00b7nem", "Drehn", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Die klingenden Ges\u00e4nge dringen und T\u00f6ne gleichsam quillen", "tokens": ["Die", "klin\u00b7gen\u00b7den", "Ge\u00b7s\u00e4n\u00b7ge", "drin\u00b7gen", "und", "T\u00f6\u00b7ne", "gleich\u00b7sam", "quil\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVINF", "KON", "NN", "ADJD", "VVINF"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "sehn.", "tokens": ["sehn", "."], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Ich setzte mich, von einem Busch bedeckt, bedachtsam zuzu-", "tokens": ["Ich", "setz\u00b7te", "mich", ",", "von", "ei\u00b7nem", "Busch", "be\u00b7deckt", ",", "be\u00b7dacht\u00b7sam", "zu\u00b7zu"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$,", "APPR", "ART", "NN", "VVPP", "$,", "ADJD", "TRUNC"], "meter": "-+-+-+-+-+-+--+", "measure": "iambic.septa.chol"}, "line.6": {"text": "h\u00f6ren,", "tokens": ["h\u00f6\u00b7ren", ","], "token_info": ["word", "punct"], "pos": ["VVINF", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.7": {"text": "Und, in den tausendfachen Liedern, so mich als andre zu beleh-", "tokens": ["Und", ",", "in", "den", "tau\u00b7send\u00b7fa\u00b7chen", "Lie\u00b7dern", ",", "so", "mich", "als", "and\u00b7re", "zu", "be\u00b7leh"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "$,", "APPR", "ART", "ADJA", "NN", "$,", "ADV", "PPER", "KOUS", "PIS", "APPR", "TRUNC"], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.8": {"text": "ren:", "tokens": ["ren", ":"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-", "measure": "single.down"}, "line.9": {"text": "Wie in des Sch\u00f6pfers Creaturen die nicht bemerkten", "tokens": ["Wie", "in", "des", "Sch\u00f6p\u00b7fers", "Crea\u00b7tu\u00b7ren", "die", "nicht", "be\u00b7merk\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN", "NN", "ART", "PTKNEG", "ADJA"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "Kleinigkeiten", "tokens": ["Klei\u00b7nig\u00b7kei\u00b7ten"], "token_info": ["word"], "pos": ["NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.11": {"text": "Der Menschen Achtsamkeit verdienen, und alle zur", "tokens": ["Der", "Men\u00b7schen", "Acht\u00b7sam\u00b7keit", "ver\u00b7die\u00b7nen", ",", "und", "al\u00b7le", "zur"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VVINF", "$,", "KON", "PIS", "APPRART"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "Bewundrung leiten;", "tokens": ["Be\u00b7wund\u00b7rung", "lei\u00b7ten", ";"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Ja wie, da alle grossen Werke aus Kleinigkeiten blo\u00df", "tokens": ["Ja", "wie", ",", "da", "al\u00b7le", "gros\u00b7sen", "Wer\u00b7ke", "aus", "Klei\u00b7nig\u00b7kei\u00b7ten", "blo\u00df"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "PWAV", "$,", "KOUS", "PIAT", "ADJA", "NN", "APPR", "NN", "ADV"], "meter": "-+-+-+--+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "besteh'n,", "tokens": ["be\u00b7steh'n", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Wir, in Betrachtungen der Kleinheit, fast Gott am", "tokens": ["Wir", ",", "in", "Be\u00b7trach\u00b7tun\u00b7gen", "der", "Klein\u00b7heit", ",", "fast", "Gott", "am"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "$,", "APPR", "NN", "ART", "NN", "$,", "ADV", "NN", "APPRART"], "meter": "-+-+---+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "w\u00fcrdigsten erh\u00f6h'n.", "tokens": ["w\u00fcr\u00b7digs\u00b7ten", "er\u00b7h\u00f6h'", "n."], "token_info": ["word", "word", "abbreviation"], "pos": ["VAFIN", "XY", "XY"], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.4": {"line.1": {"text": "Was alle K\u00fcnstler der Music mit allen ihren Instru-", "tokens": ["Was", "al\u00b7le", "K\u00fcnst\u00b7ler", "der", "Mu\u00b7sic", "mit", "al\u00b7len", "ih\u00b7ren", "In\u00b7stru"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "PIAT", "NN", "ART", "NN", "APPR", "PIAT", "PPOSAT", "TRUNC"], "meter": "-+-+-+-+-+-+--+", "measure": "iambic.septa.chol"}, "line.2": {"text": "menten,", "tokens": ["men\u00b7ten", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Zu des Geh\u00f6rs Belustigung, bisher f\u00fcr uns erfinden k\u00f6nnten,", "tokens": ["Zu", "des", "Ge\u00b7h\u00f6rs", "Be\u00b7lus\u00b7ti\u00b7gung", ",", "bis\u00b7her", "f\u00fcr", "uns", "er\u00b7fin\u00b7den", "k\u00f6nn\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,", "ADV", "APPR", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.4": {"text": "Reicht an die reine S\u00fc\u00dfigkeit, die aus der V\u00f6gel Schn\u00e4beln", "tokens": ["Reicht", "an", "die", "rei\u00b7ne", "S\u00fc\u00b7\u00dfig\u00b7keit", ",", "die", "aus", "der", "V\u00f6\u00b7gel", "Schn\u00e4\u00b7beln"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,", "PRELS", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.5": {"text": "bricht,", "tokens": ["bricht", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "-", "measure": "single.down"}, "line.6": {"text": "An Z\u00e4rtlichkeit, an scharfer Sch\u00f6nheit, Ver\u00e4nderung und", "tokens": ["An", "Z\u00e4rt\u00b7lich\u00b7keit", ",", "an", "schar\u00b7fer", "Sch\u00f6n\u00b7heit", ",", "Ver\u00b7\u00e4n\u00b7de\u00b7rung", "und"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "$,", "APPR", "ADJA", "NN", "$,", "NN", "KON"], "meter": "-+-+++-+--+--+", "measure": "iambic.septa.relaxed"}, "line.7": {"text": "Liebreiz, nicht.", "tokens": ["Lieb\u00b7reiz", ",", "nicht", "."], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$,", "PTKNEG", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.8": {"text": "Es wirbeld oft ein klingend ", "tokens": ["Es", "wir\u00b7beld", "oft", "ein", "klin\u00b7gend"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ART", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Singen,", "tokens": ["Sin\u00b7gen", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.10": {"text": "Ein fl\u00f6tend Schmatzen wissen sie vereint zugleich hervorzu-", "tokens": ["Ein", "fl\u00f6\u00b7tend", "Schmat\u00b7zen", "wis\u00b7sen", "sie", "ver\u00b7eint", "zu\u00b7gleich", "her\u00b7vor\u00b7zu"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "NN", "VVFIN", "PPER", "VVFIN", "ADV", "TRUNC"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.11": {"text": "bringen.", "tokens": ["brin\u00b7gen", "."], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.12": {"text": "Sie k\u00f6nnen, bey gesenkten, hohlen und tiefen T\u00f6nen, helle", "tokens": ["Sie", "k\u00f6n\u00b7nen", ",", "bey", "ge\u00b7senk\u00b7ten", ",", "hoh\u00b7len", "und", "tie\u00b7fen", "T\u00f6\u00b7nen", ",", "hel\u00b7le"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["PPER", "VMFIN", "$,", "APPR", "ADJA", "$,", "ADJA", "KON", "ADJA", "NN", "$,", "ADJA"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.13": {"text": "pfeifen.", "tokens": ["pfei\u00b7fen", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.14": {"text": "Sie sind geschickt, fast unbegreiflich, in runden abgesetzten", "tokens": ["Sie", "sind", "ge\u00b7schickt", ",", "fast", "un\u00b7be\u00b7greif\u00b7lich", ",", "in", "run\u00b7den", "ab\u00b7ge\u00b7setz\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "VVPP", "$,", "ADV", "ADJD", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.15": {"text": "L\u00e4ufen,", "tokens": ["L\u00e4u\u00b7fen", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.16": {"text": "Die rollende geschwinde T\u00f6ne zugleich zu dehnen und zu", "tokens": ["Die", "rol\u00b7len\u00b7de", "ge\u00b7schwin\u00b7de", "T\u00f6\u00b7ne", "zu\u00b7gleich", "zu", "deh\u00b7nen", "und", "zu"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN", "ADV", "PTKZU", "VVINF", "KON", "APPR"], "meter": "-+-+-+-+--+-+--+", "measure": "iambic.septa.relaxed"}, "line.17": {"text": "schleifen.", "tokens": ["schlei\u00b7fen", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.18": {"text": "Es locket, zwitschert, schl\u00e4gt und gurgelt, bald steigend, bald", "tokens": ["Es", "lo\u00b7cket", ",", "zwit\u00b7schert", ",", "schl\u00e4gt", "und", "gur\u00b7gelt", ",", "bald", "stei\u00b7gend", ",", "bald"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "KON", "VVFIN", "$,", "ADV", "ADJD", "$,", "ADV"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.19": {"text": "im holden Fall,", "tokens": ["im", "hol\u00b7den", "Fall", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.20": {"text": "Mit tausendf\u00e4ltigen Manieren, der tausendfach formierte", "tokens": ["Mit", "tau\u00b7send\u00b7f\u00e4l\u00b7ti\u00b7gen", "Ma\u00b7nie\u00b7ren", ",", "der", "tau\u00b7send\u00b7fach", "for\u00b7mier\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "PRELS", "ADV", "VVFIN"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.21": {"text": "Schall.", "tokens": ["Schall", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}, "line.22": {"text": "Wenn wir demnach, wie Menschen, denken, und unsre Pflicht", "tokens": ["Wenn", "wir", "dem\u00b7nach", ",", "wie", "Men\u00b7schen", ",", "den\u00b7ken", ",", "und", "uns\u00b7re", "Pflicht"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "PAV", "$,", "PWAV", "NN", "$,", "VVINF", 