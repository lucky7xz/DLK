{"textgrid.poem.24489": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Als Sie andere verliebt anblickte/ sahe er sie vern\u00fcnfftig an", "genre": "verse", "period": "N.A.", "pub_year": 1701, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du bist zwar gut genug/ doch warlich noch nicht sch\u00f6n/", "tokens": ["Du", "bist", "zwar", "gut", "ge\u00b7nug", "/", "doch", "war\u00b7lich", "noch", "nicht", "sch\u00f6n", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "ADV", "$(", "ADV", "ADV", "ADV", "PTKNEG", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "De\u00dfwegen wird die Welt dir nicht zu Diensten stehn.", "tokens": ["De\u00df\u00b7we\u00b7gen", "wird", "die", "Welt", "dir", "nicht", "zu", "Diens\u00b7ten", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "ART", "NN", "PPER", "PTKNEG", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Dein Kopf ist zwar geputzt/ doch mangeln ihm die Augen/", "tokens": ["Dein", "Kopf", "ist", "zwar", "ge\u00b7putzt", "/", "doch", "man\u00b7geln", "ihm", "die", "Au\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "VVPP", "$(", "ADV", "VVFIN", "PPER", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die den Verstand und Geist uns zu entz\u00fcnden taugen.", "tokens": ["Die", "den", "Ver\u00b7stand", "und", "Geist", "uns", "zu", "ent\u00b7z\u00fcn\u00b7den", "tau\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "KON", "NN", "PPER", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die Tugend f\u00fchret dir auch keinen Mann ins Hau\u00df/", "tokens": ["Die", "Tu\u00b7gend", "f\u00fch\u00b7ret", "dir", "auch", "kei\u00b7nen", "Mann", "ins", "Hau\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "PIAT", "NN", "APPRART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Um Geld und Hau\u00df-Rath siehts auch ziemlich mager aus/", "tokens": ["Um", "Geld", "und", "Hau\u00df\u00b7Rath", "siehts", "auch", "ziem\u00b7lich", "ma\u00b7ger", "aus", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "KON", "NN", "VVFIN", "ADV", "ADV", "ADJD", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Drum hab ich gantz vergn\u00fcgt die neue Wahl vernommen:", "tokens": ["Drum", "hab", "ich", "gantz", "ver\u00b7gn\u00fcgt", "die", "neu\u00b7e", "Wahl", "ver\u00b7nom\u00b7men", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "ADV", "VVFIN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Denn der verlieret nichts/ der nichts durch dich bekommen.", "tokens": ["Denn", "der", "ver\u00b7lie\u00b7ret", "nichts", "/", "der", "nichts", "durch", "dich", "be\u00b7kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "VVFIN", "PIS", "$(", "ART", "PIS", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine