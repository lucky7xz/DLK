{"dta.poem.9857": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Lippen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Den k\u00f6cher voller pfeil hat Venus uns geschencket/", "tokens": ["Den", "k\u00f6\u00b7cher", "vol\u00b7ler", "pfeil", "hat", "Ve\u00b7nus", "uns", "ge\u00b7schen\u00b7cket", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "VAFIN", "NN", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und ist es wunderns werth/ da\u00df unsre glut sich schwencket", "tokens": ["Und", "ist", "es", "wun\u00b7derns", "werth", "/", "da\u00df", "uns\u00b7re", "glut", "sich", "schwen\u00b7cket"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "NN", "ADJD", "$(", "KOUS", "PPOSAT", "NN", "PRF", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Bi\u00df an das sternen-dach? Hier liegt ein brand versencket/", "tokens": ["Bi\u00df", "an", "das", "ster\u00b7nen\u00b7dach", "?", "Hier", "liegt", "ein", "brand", "ver\u00b7sen\u00b7cket", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "$.", "ADV", "VVFIN", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der ewig zunder gibt/ der mit rubin umschrencket/", "tokens": ["Der", "e\u00b7wig", "zun\u00b7der", "gibt", "/", "der", "mit", "ru\u00b7bin", "um\u00b7schren\u00b7cket", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADV", "VVFIN", "$(", "ART", "APPR", "NE", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die feuchte s\u00fc\u00dfigkeit/ wenn mund am munde hencket/", "tokens": ["Die", "feuch\u00b7te", "s\u00fc\u00b7\u00dfig\u00b7keit", "/", "wenn", "mund", "am", "mun\u00b7de", "hen\u00b7cket", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "KOUS", "NN", "APPRART", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und die vergn\u00fcgte seel mit zimmet-s\u00e4fften tr\u00e4ncket.", "tokens": ["Und", "die", "ver\u00b7gn\u00fcg\u00b7te", "seel", "mit", "zim\u00b7met\u00b7s\u00e4ff\u00b7ten", "tr\u00e4n\u00b7cket", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}