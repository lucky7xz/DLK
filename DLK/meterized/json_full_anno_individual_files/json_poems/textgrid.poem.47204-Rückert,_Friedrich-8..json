{"textgrid.poem.47204": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "8.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Jahr ist nun geschwunden,", "tokens": ["Ein", "Jahr", "ist", "nun", "ge\u00b7schwun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Seit du geschieden bist,", "tokens": ["Seit", "du", "ge\u00b7schie\u00b7den", "bist", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und wie zwei tr\u00fcbe Stunden", "tokens": ["Und", "wie", "zwei", "tr\u00fc\u00b7be", "Stun\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "CARD", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Gemahnt mich diese Frist.", "tokens": ["Ge\u00b7mahnt", "mich", "die\u00b7se", "Frist", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "PPER", "PDAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Und h\u00e4ttest du gelebet,", "tokens": ["Und", "h\u00e4t\u00b7test", "du", "ge\u00b7le\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mein Kindchen, dieses Jahr,", "tokens": ["Mein", "Kind\u00b7chen", ",", "die\u00b7ses", "Jahr", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PDAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So w\u00e4r' die Frist entschwebet", "tokens": ["So", "w\u00e4r'", "die", "Frist", "ent\u00b7schwe\u00b7bet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein helles Stundenpaar.", "tokens": ["Ein", "hel\u00b7les", "Stun\u00b7den\u00b7paar", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Nun, seit ich auf der Bahre", "tokens": ["Nun", ",", "seit", "ich", "auf", "der", "Bah\u00b7re"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "KOUS", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dich mu\u00dfte sehn, mein Kind,", "tokens": ["Dich", "mu\u00df\u00b7te", "sehn", ",", "mein", "Kind", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Denk' ich, wie wenig Jahre,", "tokens": ["Denk'", "ich", ",", "wie", "we\u00b7nig", "Jah\u00b7re", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "PWAV", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Verliehn dem Menschen sind.", "tokens": ["Ver\u00b7liehn", "dem", "Men\u00b7schen", "sind", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ob tr\u00fcber oder heller,", "tokens": ["Ob", "tr\u00fc\u00b7ber", "o\u00b7der", "hel\u00b7ler", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wie Stunden sind sie nur,", "token