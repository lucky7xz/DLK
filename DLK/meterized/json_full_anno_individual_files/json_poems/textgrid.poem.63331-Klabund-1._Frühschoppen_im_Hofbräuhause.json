{"textgrid.poem.63331": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "1. Fr\u00fchschoppen im Hofbr\u00e4uhause", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hier steht ein Fa\u00df \u2013 und an das Fa\u00df geschwei\u00dft,", "tokens": ["Hier", "steht", "ein", "Fa\u00df", "\u2013", "und", "an", "das", "Fa\u00df", "ge\u00b7schwei\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$(", "KON", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Dem Fasse \u00e4hnlich, dick und rund gerollt:", "tokens": ["Dem", "Fas\u00b7se", "\u00e4hn\u00b7lich", ",", "dick", "und", "rund", "ge\u00b7rollt", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$,", "ADJD", "KON", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ein k.b. Rat... ein Dienstmann... und ein Bold,", "tokens": ["Ein", "k.", "b.", "Rat", "...", "ein", "Dienst\u00b7mann", "...", "und", "ein", "Bold", ","], "token_info": ["word", "abbreviation", "abbreviation", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$(", "ART", "NN", "$(", "KON", "ART", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der sich (mit Gamsbart) als ein Preu\u00df' erweist.", "tokens": ["Der", "sich", "(", "mit", "Gams\u00b7bart", ")", "als", "ein", "Preu\u00df'", "er\u00b7weist", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "$(", "APPR", "NN", "$(", "KOUS", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "m