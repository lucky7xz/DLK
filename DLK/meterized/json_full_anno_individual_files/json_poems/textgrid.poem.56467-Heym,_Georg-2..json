{"textgrid.poem.56467": {"metadata": {"author": {"name": "Heym, Georg", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1899, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Des Abends Trauer sinkt. Sie hocken stumpf", "tokens": ["Des", "A\u00b7bends", "Trau\u00b7er", "sinkt", ".", "Sie", "ho\u00b7cken", "stumpf"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VVFIN", "$.", "PPER", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "In ihrer Kissen Schatten. Und herein", "tokens": ["In", "ih\u00b7rer", "Kis\u00b7sen", "Schat\u00b7ten", ".", "Und", "her\u00b7ein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$.", "KON", "PTKVZ"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Kriecht Wassernebel kalt. Sie h\u00f6ren dumpf", "tokens": ["Kriecht", "Was\u00b7ser\u00b7ne\u00b7bel", "kalt", ".", "Sie", "h\u00f6\u00b7ren", "dumpf"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "NN", "ADJD", "$.", "PPER", "VVFIN", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Durch ihren Saal der Qualen Litanein.", "tokens": ["Durch", "ih\u00b7ren", "Saal", "der", "Qua\u00b7len", "Li\u00b7tan\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Das Fieber kriecht in ihren Lagern um,", "tokens": ["Das", "Fie\u00b7ber", "kriecht", "in", "ih\u00b7ren", "La\u00b7gern", "um", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Langsam, ein gro\u00dfer, gelblicher Polyp.", "tokens": ["Lang\u00b7sam", ",", "ein", "gro\u00b7\u00dfer", ",", "gelb\u00b7li\u00b7cher", "Po\u00b7lyp", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "$,", "ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Sie schaun ihm zu, von dem Entsetzen stumm.", "tokens": ["Sie", "schaun", "ihm", "zu", ",", "von", "dem", "Ent\u00b7set\u00b7zen", "stumm", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$,", "APPR", "ART", "NN", "ADJD", "$."], "meter": "-+--++-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Und ihre Augen werden wei\u00df und tr\u00fcb.", "tokens": ["Und", "ih\u00b7re", "Au\u00b7gen", "wer\u00b7den", "wei\u00df", "und", "tr\u00fcb", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAINF", "VVFIN", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Die Sonne qu\u00e4lt sich auf dem Rand der Nacht.", "tokens": ["Die", "Son\u00b7ne", "qu\u00e4lt", "sich", "auf", "dem", "Rand", "der", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sie bl\u00e4hn die Nasen. Es wird furchtbar hei\u00df.", "tokens": ["Sie", "bl\u00e4hn", "die", "Na\u00b7sen", ".", "Es", "wird", "furcht\u00b7bar", "hei\u00df", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$.", "PPER", "VAFIN", "ADJD", "ADJD", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ein gro\u00dfes Feuer hat sie angefacht,", "tokens": ["Ein", "gro\u00b7\u00dfes", "Feu\u00b7er", "hat", "sie", "an\u00b7ge\u00b7facht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wie eine Blase schwankt ihr roter Kreis.", "tokens": ["Wie", "ei\u00b7ne", "Bla\u00b7se", "schwankt", "ihr", "ro\u00b7ter", "Kreis", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Auf ihrem Dache sitzt ein Mann im Stuhl", "tokens": ["Auf", "ih\u00b7rem", "Da\u00b7che", "sitzt", "ein", "Mann", "im", "Stuhl"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und droht den Kranken mit dem Eisenstab.", "tokens": ["Und", "droht", "den", "Kran\u00b7ken", "mit", "dem", "Ei\u00b7sen\u00b7stab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Darunter schaufeln in dem hei\u00dfen Pfuhl", "tokens": ["Da\u00b7run\u00b7ter", "schau\u00b7feln", "in", "dem", "hei\u00b7\u00dfen", "Pfuhl"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die Nigger schon ihr tiefes, wei\u00dfes Grab.", "tokens": ["Die", "Nig\u00b7ger", "schon", "ihr", "tie\u00b7fes", ",", "wei\u00b7\u00dfes", "Grab", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PPOSAT", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Die Leichentr\u00e4ger gehen durch die Reihen", "tokens": ["Die", "Lei\u00b7chen\u00b7tr\u00e4\u00b7ger", "ge\u00b7hen", "durch", "die", "Rei\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und rei\u00dfen schnell die Toten aus dem Bett.", "tokens": ["Und", "rei\u00b7\u00dfen", "schnell", "die", "To\u00b7ten", "aus", "dem", "Bett", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die andern drehn sich nach der Wand mit Schreien", "tokens": ["Die", "an\u00b7dern", "drehn", "sich", "nach", "der", "Wand", "mit", "Schrei\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "CARD", "PRF", "APPR", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Angst, der Toten gr\u00e4\u00dflichem Valet.", "tokens": ["Der", "Angst", ",", "der", "To\u00b7ten", "gr\u00e4\u00df\u00b7li\u00b7chem", "Va\u00b7let", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "Moskitos summen. Und die Luft beginnt", "tokens": ["Mos\u00b7ki\u00b7tos", "sum\u00b7men", ".", "Und", "die", "Luft", "be\u00b7ginnt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "VVINF", "$.", "KON", "ART", "NN", "VVFIN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Vor Glut zu schmelzen. Wie ein roter Kropf", "tokens": ["Vor", "Glut", "zu", "schmel\u00b7zen", ".", "Wie", "ein", "ro\u00b7ter", "Kropf"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NN", "PTKZU", "VVINF", "$.", "PWAV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Schwillt auf ihr Hals, darinnen Lava rinnt.", "tokens": ["Schwillt", "auf", "ihr", "Hals", ",", "da\u00b7rin\u00b7nen", "La\u00b7va", "rinnt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "$,", "ADV", "NE", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und wie ein Ball von Feuer dr\u00f6hnt ihr Kopf.", "tokens": ["Und", "wie", "ein", "Ball", "von", "Feu\u00b7er", "dr\u00f6hnt", "ihr", "Kopf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "APPR", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Sie machen sich von ihren Hemden los", "tokens": ["Sie", "ma\u00b7chen", "sich", "von", "ih\u00b7ren", "Hem\u00b7den", "los"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "PTKVZ"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und ihren Decken, die sie na\u00df umziehn.", "tokens": ["Und", "ih\u00b7ren", "De\u00b7cken", ",", "die", "sie", "na\u00df", "um\u00b7ziehn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "PRELS", "PPER", "ADJD", "VVIZU", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ihr magrer Leib, bis auf den Nabel blo\u00df,", "tokens": ["Ihr", "mag\u00b7rer", "Leib", ",", "bis", "auf", "den", "Na\u00b7bel", "blo\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "KOUS", "APPR", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wiegt hin und her im Takt der Phantasien.", "tokens": ["Wiegt", "hin", "und", "her", "im", "Takt", "der", "Phan\u00b7ta\u00b7si\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "KON", "ADV", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.8": {"line.1": {"text": "Das Flo\u00df des Todes steuert durch die Nacht", "tokens": ["Das", "Flo\u00df", "des", "To\u00b7des", "steu\u00b7ert", "durch", "die", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Heran durch Meere Schlamms und dunkles Moor.", "tokens": ["He\u00b7ran", "durch", "Mee\u00b7re", "Schlamms", "und", "dunk\u00b7les", "Moor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPOSAT", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sie h\u00f6ren bang, wie seine Stange kracht", "tokens": ["Sie", "h\u00f6\u00b7ren", "bang", ",", "wie", "sei\u00b7ne", "Stan\u00b7ge", "kracht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "$,", "PWAV", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Lauthallend unten am Barackentor.", "tokens": ["Laut\u00b7hal\u00b7lend", "un\u00b7ten", "am", "Ba\u00b7ra\u00b7cken\u00b7tor", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "APPRART", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}, "stanza.9": {"line.1": {"text": "Zu einem Bette kommt das Sakrament.", "tokens": ["Zu", "ei\u00b7nem", "Bet\u00b7te", "kommt", "das", "Sak\u00b7ra\u00b7ment", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der Priester salbt dem Kranken Stirn und Mund.", "tokens": ["Der", "Pries\u00b7ter", "salbt", "dem", "Kran\u00b7ken", "Stirn", "und", "Mund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der Gaumen, der wie rotes Feuer brennt,", "tokens": ["Der", "Gau\u00b7men", ",", "der", "wie", "ro\u00b7tes", "Feu\u00b7er", "brennt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "KOKOM", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "W\u00fcrgt m\u00fchsam die Oblate in den Schlund.", "tokens": ["W\u00fcrgt", "m\u00fch\u00b7sam", "die", "Ob\u00b7la\u00b7te", "in", "den", "Schlund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Die Kranken horchen auf der Lagerstatt", "tokens": ["Die", "Kran\u00b7ken", "hor\u00b7chen", "auf", "der", "La\u00b7ger\u00b7statt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wie Kr\u00f6ten, von dem Lichte rot gefleckt.", "tokens": ["Wie", "Kr\u00f6\u00b7ten", ",", "von", "dem", "Lich\u00b7te", "rot", "ge\u00b7fleckt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "$,", "APPR", "ART", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Betten sind wie eine gro\u00dfe Stadt,", "tokens": ["Die", "Bet\u00b7ten", "sind", "wie", "ei\u00b7ne", "gro\u00b7\u00dfe", "Stadt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "KOKOM", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die eines schwarzen Himmels R\u00e4tsel deckt.", "tokens": ["Die", "ei\u00b7nes", "schwar\u00b7zen", "Him\u00b7mels", "R\u00e4t\u00b7sel", "deckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.11": {"line.1": {"text": "Der Priester singt. In grauser Parodie", "tokens": ["Der", "Pries\u00b7ter", "singt", ".", "In", "grau\u00b7ser", "Pa\u00b7ro\u00b7die"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$.", "APPR", "ADJA", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Kr\u00e4hn sie die Worte nach in dem Gebet.", "tokens": ["Kr\u00e4hn", "sie", "die", "Wor\u00b7te", "nach", "in", "dem", "Ge\u00b7bet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "APPR", "APPR", "ART", "NN", "$."], "meter": "+--+-++-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Sie lachen laut, die Freude sch\u00fcttelt sie.", "tokens": ["Sie", "la\u00b7chen", "laut", ",", "die", "Freu\u00b7de", "sch\u00fct\u00b7telt", "sie", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$,", "ART", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Sie halten sich den Bauch, den Lachen bl\u00e4ht.", "tokens": ["Sie", "hal\u00b7ten", "sich", "den", "Bauch", ",", "den", "La\u00b7chen", "bl\u00e4ht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ART", "NN", "$,", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.12": {"line.1": {"text": "Der Priester kniet sich an der Bettstatt Rand.", "tokens": ["Der", "Pries\u00b7ter", "kniet", "sich", "an", "der", "Bett\u00b7statt", "Rand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "In das Brevier taucht er die Schultern ein.", "tokens": ["In", "das", "Bre\u00b7vier", "taucht", "er", "die", "Schul\u00b7tern", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Der Kranke setzt sich auf. In seiner Hand", "tokens": ["Der", "Kran\u00b7ke", "setzt", "sich", "auf", ".", "In", "sei\u00b7ner", "Hand"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PRF", "PTKVZ", "$.", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Dreht er im Kreise einen spitzen Stein.", "tokens": ["Dreht", "er", "im", "Krei\u00b7se", "ei\u00b7nen", "spit\u00b7zen", "Stein", "."], "token_info": ["word", "word", "word", "wo