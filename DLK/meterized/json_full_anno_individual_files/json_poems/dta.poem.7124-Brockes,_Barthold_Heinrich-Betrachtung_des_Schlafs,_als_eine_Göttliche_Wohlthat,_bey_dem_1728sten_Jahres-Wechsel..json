{"dta.poem.7124": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Betrachtung des Schlafs,  \n als  \n eine G\u00f6ttliche Wohlthat,  \n bey dem 1728sten Jahres-Wechsel.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Man stelle sich doch einst die Welt und unser Leben,", "tokens": ["Man", "stel\u00b7le", "sich", "doch", "einst", "die", "Welt", "und", "un\u00b7ser", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PRF", "ADV", "ADV", "ART", "NN", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als ohne Nacht, als sonder Ruhe, f\u00fcr.", "tokens": ["Als", "oh\u00b7ne", "Nacht", ",", "als", "son\u00b7der", "Ru\u00b7he", ",", "f\u00fcr", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "APPR", "NN", "$,", "KOUS", "ADJA", "NN", "$,", "APPR", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Welch elend Einerley w\u00fcrd\u2019 alle Ding\u2019 umgeben,", "tokens": ["Welch", "e\u00b7lend", "Ei\u00b7ner\u00b7ley", "w\u00fcrd'", "al\u00b7le", "Ding'", "um\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJD", "NN", "VAFIN", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Welch eckle Lange-Weil und Stunden w\u00fcrden wir,", "tokens": ["Welch", "ec\u00b7kle", "Lan\u00b7ge\u00b7Weil", "und", "Stun\u00b7den", "w\u00fcr\u00b7den", "wir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "KON", "NN", "VAFIN", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Bey einem steten Mittag, finden?", "tokens": ["Bey", "ei\u00b7nem", "ste\u00b7ten", "Mit\u00b7tag", ",", "fin\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "W\u00fcrd\u2019 uns ein solches Lust-und Wechsel-loses Ein", "tokens": ["W\u00fcrd'", "uns", "ein", "sol\u00b7ches", "Lust\u00b7\u00b7und", "Wech\u00b7sel\u00b7lo\u00b7ses", "Ein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ART", "PIAT", "NN", "NN", "ART"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Richt unertr\u00e4glich seyn?", "tokens": ["Richt", "un\u00b7er\u00b7tr\u00e4g\u00b7lich", "seyn", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJD", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Nie w\u00e4r\u2019 es spath, nie fr\u00fch,", "tokens": ["Nie", "w\u00e4r'", "es", "spath", ",", "nie", "fr\u00fch", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVFIN", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Nie w\u00e4r\u2019 ein Morgen-Gold an den Saphiernen H\u00f6hen,", "tokens": ["Nie", "w\u00e4r'", "ein", "Mor\u00b7gen\u00b7Gold", "an", "den", "Sa\u00b7phier\u00b7nen", "H\u00f6\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Kein lieblichs Abend-Roth am Horizont zu sehen.", "tokens": ["Kein", "lieb\u00b7lichs", "A\u00b7ben\u00b7dRoth", "am", "Ho\u00b7ri\u00b7zont", "zu", "se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "APPRART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Man s\u00e4he nimmermehr, im Dunckeln,", "tokens": ["Man", "s\u00e4\u00b7he", "nim\u00b7mer\u00b7mehr", ",", "im", "Dun\u00b7ckeln", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "$,", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "So vieler Sonnen Heer\u2019 in hellen Sternen funckeln.", "tokens": ["So", "vie\u00b7ler", "Son\u00b7nen", "Heer'", "in", "hel\u00b7len", "Ster\u00b7nen", "fun\u00b7ckeln", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "NN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Ein unaufh\u00f6rliches Get\u00f6se w\u00fcrde nie,", "tokens": ["Ein", "un\u00b7auf\u00b7h\u00f6r\u00b7li\u00b7ches", "Ge\u00b7t\u00f6\u00b7se", "w\u00fcr\u00b7de", "nie", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Durch eine sanfft\u2019 und holde Stille,", "tokens": ["Durch", "ei\u00b7ne", "sanf\u00b7ft'", "und", "hol\u00b7de", "Stil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJD", "KON", "ADJA", "NN", "$,"], "meter": "-+-++-+-+-", "measure": "unknown.measure.penta"}, "line.15": {"text": "Wie itzt durch Nacht und Schlaf gemindert,", "tokens": ["Wie", "itzt", "durch", "Nacht", "und", "Schlaf", "ge\u00b7min\u00b7dert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "APPR", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Der Menschen Gram und Arbeit nie gelindert,", "tokens": ["Der", "Men\u00b7schen", "Gram", "und", "Ar\u00b7beit", "nie", "ge\u00b7lin\u00b7dert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Und kurtz, es w\u00fcrd\u2019 uns auf der Erden", "tokens": ["Und", "kurtz", ",", "es", "w\u00fcrd'", "uns", "auf", "der", "Er\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "$,", "PPER", "VAFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Das Leben, sonder Schlaf, und Nacht, verdr\u00fc\u00dflich werden.", "tokens": ["Das", "Le\u00b7ben", ",", "son\u00b7der", "Schlaf", ",", "und", "Nacht", ",", "ver\u00b7dr\u00fc\u00df\u00b7lich", "wer\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KON", "NN", "$,", "KON", "NN", "$,", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Wer diesen uns von GOTT geschenckten Schatz die Ruh,", "tokens": ["Wer", "die\u00b7sen", "uns", "von", "GoTT", "ge\u00b7schenck\u00b7ten", "Schatz", "die", "Ruh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PDAT", "PPER", "APPR", "NE", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Nicht recht zu sch\u00e4tzen wei\u00df; der h\u00f6r\u2019 einst denen zu,", "tokens": ["Nicht", "recht", "zu", "sch\u00e4t\u00b7zen", "wei\u00df", ";", "der", "h\u00f6r'", "einst", "de\u00b7nen", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "PTKZU", "VVINF", "VVFIN", "$.", "ART", "VVFIN", "ADV", "PDS", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "F\u00fcr die das sanffte Kind der stillen Schatten flieht,", "tokens": ["F\u00fcr", "die", "das", "sanff\u00b7te", "Kind", "der", "stil\u00b7len", "Schat\u00b7ten", "flieht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Und ihnen blo\u00df dadurch Gesundheit, Muth, Vergn\u00fcgen,", "tokens": ["Und", "ih\u00b7nen", "blo\u00df", "da\u00b7durch", "Ge\u00b7sund\u00b7heit", ",", "Muth", ",", "Ver\u00b7gn\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "PPER", "ADV", "PAV", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Ja offt Verstand und Witz entzieht:", "tokens": ["Ja", "offt", "Ver\u00b7stand", "und", "Witz", "ent\u00b7zieht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADV", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Wie sie auf ihren Bett\u2019, als wie auf Dornen, liegen,", "tokens": ["Wie", "sie", "auf", "ih\u00b7ren", "Bett'", ",", "als", "wie", "auf", "Dor\u00b7nen", ",", "lie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "PPOSAT", "NN", "$,", "KOUS", "KOKOM", "APPR", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Wie Unruh, Bitterkeit und Unzufriedenheit,", "tokens": ["Wie", "Un\u00b7ruh", ",", "Bit\u00b7ter\u00b7keit", "und", "Un\u00b7zu\u00b7frie\u00b7den\u00b7heit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Verwirrung, Grimm und Gram, Angst und Verdr\u00fc\u00dflich-", "tokens": ["Ver\u00b7wir\u00b7rung", ",", "Grimm", "und", "Gram", ",", "Angst", "und", "Ver\u00b7dr\u00fc\u00df\u00b7lich"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "NE", "KON", "NN", "$,", "NN", "KON", "TRUNC"], "meter": "-+-+-+++-+-", "measure": "unknown.measure.hexa"}, "line.27": {"text": "keit,", "tokens": ["keit", ","], "token_info": ["word", "punct"], "pos": ["ADV", "$,"], "meter": "+", "measure": "single.up"}, "line.28": {"text": "Ohn Aufschub sie best\u00fcrmt, sie wechselsweise plaget,", "tokens": ["Ohn", "Auf\u00b7schub", "sie", "be\u00b7st\u00fcrmt", ",", "sie", "wech\u00b7sels\u00b7wei\u00b7se", "pla\u00b7get", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "VVPP", "$,", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Jhr Eingeweide kneipt, als wie ein Wurm sie naget,", "tokens": ["Ihr", "Ein\u00b7ge\u00b7wei\u00b7de", "kneipt", ",", "als", "wie", "ein", "Wurm", "sie", "na\u00b7get", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,", "KOUS", "KOKOM", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Und als ein Feur sie brennt. Es w\u00fcrckt ihr siedend Blut,", "tokens": ["Und", "als", "ein", "Feur", "sie", "brennt", ".", "Es", "w\u00fcrckt", "ihr", "sie\u00b7dend", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "PPER", "VVFIN", "$.", "PPER", "VVFIN", "PPER", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Da es best\u00e4ndig wallt, gedrengt wird und gepresst,", "tokens": ["Da", "es", "be\u00b7st\u00e4n\u00b7dig", "wallt", ",", "ge\u00b7drengt", "wird", "und", "ge\u00b7presst", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VVFIN", "$,", "VVPP", "VAFIN", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Da\u00df, selber im Gehirn, der Seelen reine Gluht", "tokens": ["Da\u00df", ",", "sel\u00b7ber", "im", "Ge\u00b7hirn", ",", "der", "See\u00b7len", "rei\u00b7ne", "Gluht"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "$,", "ADV", "APPRART", "NN", "$,", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Ged\u00e4mpfft, verstreu\u2019t, sich kaum erkennen l\u00e4sst,", "tokens": ["Ge\u00b7d\u00e4mpfft", ",", "ver\u00b7streu't", ",", "sich", "kaum", "er\u00b7ken\u00b7nen", "l\u00e4sst", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,", "PRF", "ADV", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.34": {"text": "Und zu verleschen droht. Es ist f\u00fcrwahr ein Jammer", "tokens": ["Und", "zu", "ver\u00b7le\u00b7schen", "droht", ".", "Es", "ist", "f\u00fcr\u00b7wahr", "ein", "Jam\u00b7mer"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PTKZU", "VVINF", "VVFIN", "$.", "PPER", "VAFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Der unbeschreiblich ist, es leidet Leib und Geist,", "tokens": ["Der", "un\u00b7be\u00b7schreib\u00b7lich", "ist", ",", "es", "lei\u00b7det", "Leib", "und", "Geist", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VAFIN", "$,", "PPER", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Als die die Unruh beyd\u2019 aus ihren Schrancken reisst:", "tokens": ["Als", "die", "die", "Un\u00b7ruh", "beyd'", "aus", "ih\u00b7ren", "Schran\u00b7cken", "reisst", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ART", "NN", "PTKVZ", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Daher, wenn alle Welt der s\u00fcssen Ruh sich freuet,", "tokens": ["Da\u00b7her", ",", "wenn", "al\u00b7le", "Welt", "der", "s\u00fcs\u00b7sen", "Ruh", "sich", "freu\u00b7et", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "KOUS", "PIAT", "NN", "ART", "ADJA", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Ein solcher jede Nacht, als seinen Hencker, scheuet.", "tokens": ["Ein", "sol\u00b7cher", "je\u00b7de", "Nacht", ",", "als", "sei\u00b7nen", "Hen\u00b7cker", ",", "scheu\u00b7et", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "PIAT", "PIAT", "NN", "$,", "KOUS", "PPOSAT", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Es wird ihr m\u00fcrber Geist geplagt, verwirrt, geschreckt,", "tokens": ["Es", "wird", "ihr", "m\u00fcr\u00b7ber", "Geist", "ge\u00b7plagt", ",", "ver\u00b7wirrt", ",", "ge\u00b7schreckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "ADJA", "NN", "VVPP", "$,", "ADJD", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Wenn ihn voll Phantasey ein tr\u00e4ger Schlummer weckt,", "tokens": ["Wenn", "ihn", "voll", "Phan\u00b7ta\u00b7sey", "ein", "tr\u00e4\u00b7ger", "Schlum\u00b7mer", "weckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Der leicht und schwer zugleich.", "tokens": ["Der", "leicht", "und", "schwer", "zu\u00b7gleich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "KON", "ADJD", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.42": {"text": "Zur Folter wird das Bett, zum Kercker seine Kammer,", "tokens": ["Zur", "Fol\u00b7ter", "wird", "das", "Bett", ",", "zum", "Ker\u00b7cker", "sei\u00b7ne", "Kam\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "ART", "NN", "$,", "APPRART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Wodurch ihm dann die gantze Welt,", "tokens": ["Wo\u00b7durch", "ihm", "dann", "die", "gant\u00b7ze", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.44": {"text": "Und alles, was darinn, bi\u00df auf den Tod mi\u00dff\u00e4llt,", "tokens": ["Und", "al\u00b7les", ",", "was", "da\u00b7rinn", ",", "bi\u00df", "auf", "den", "Tod", "mi\u00df\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "$,", "PRELS", "PAV", "$,", "KOUS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}