{"textgrid.poem.52168": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Schutzgeister", "genre": "verse", "period": "N.A.", "pub_year": 1861, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nahe wieder sah ich gl\u00e4nzen", "tokens": ["Na\u00b7he", "wie\u00b7der", "sah", "ich", "gl\u00e4n\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Meiner Firne scharfe Grenzen,", "tokens": ["Mei\u00b7ner", "Fir\u00b7ne", "schar\u00b7fe", "Gren\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Meiner Alpen wei\u00dfe B\u00fcnde,", "tokens": ["Mei\u00b7ner", "Al\u00b7pen", "wei\u00b7\u00dfe", "B\u00fcn\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wurzelnd tief im Kern der Schweiz;", "tokens": ["Wur\u00b7zelnd", "tief", "im", "Kern", "der", "Schweiz", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "APPRART", "NN", "ART", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wieder bin ich dort gegangen,", "tokens": ["Wie\u00b7der", "bin", "ich", "dort", "ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wo die graden W\u00e4nde hangen", "tokens": ["Wo", "die", "gra\u00b7den", "W\u00e4n\u00b7de", "han\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "In des Sees geheime Gr\u00fcnde", "tokens": ["In", "des", "Sees", "ge\u00b7hei\u00b7me", "Gr\u00fcn\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Mit dem dunkelgr\u00fcnen Reiz.", "tokens": ["Mit", "dem", "dun\u00b7kel\u00b7gr\u00fc\u00b7nen", "Reiz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Nimmer war der Tag so helle,", "tokens": ["Nim\u00b7mer", "war", "der", "Tag", "so", "hel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADV", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Niemals reiner meine Augen,", "tokens": ["Nie\u00b7mals", "rei\u00b7ner", "mei\u00b7ne", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Erd und Himmel einzusaugen,", "tokens": ["Erd", "und", "Him\u00b7mel", "ein\u00b7zu\u00b7sau\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Meine Schritte gingen sacht;", "tokens": ["Mei\u00b7ne", "Schrit\u00b7te", "gin\u00b7gen", "sacht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Schauend pilgert ich und lauschte,", "tokens": ["Schau\u00b7end", "pil\u00b7gert", "ich", "und", "lauschte", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "KON", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Weil ein guter Weggeselle", "tokens": ["Weil", "ein", "gu\u00b7ter", "Weg\u00b7ge\u00b7sel\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Heimlich Worte mit mir tauschte", "tokens": ["Heim\u00b7lich", "Wor\u00b7te", "mit", "mir", "tauschte"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "NN", "APPR", "PPER", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Von der Berge Herzensmacht.", "tokens": ["Von", "der", "Ber\u00b7ge", "Her\u00b7zens\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Traulich f\u00fchlt ich seine N\u00e4he", "tokens": ["Trau\u00b7lich", "f\u00fchlt", "ich", "sei\u00b7ne", "N\u00e4\u00b7he"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und mir ward, ob ich ihn s\u00e4he,", "tokens": ["Und", "mir", "ward", ",", "ob", "ich", "ihn", "s\u00e4\u00b7he", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "$,", "KOUS", "PPER", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und er sprach: \u00bbVor manchen Jahren", "tokens": ["Und", "er", "sprach", ":", "\u00bb", "Vor", "man\u00b7chen", "Jah\u00b7ren"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "$.", "$(", "APPR", "PIAT", "NN"], "meter": "+-+---+-", "measure": "unknown.measure.tri"}, "line.4": {"text": "Bin ich r\u00fcstig hier gereist,", "tokens": ["Bin", "ich", "r\u00fcs\u00b7tig", "hier", "ge\u00b7reist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "ADV", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Hier geschritten, dort gefahren!\u00ab", "tokens": ["Hier", "ge\u00b7schrit\u00b7ten", ",", "dort", "ge\u00b7fah\u00b7ren", "!", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["ADV", "VVPP", "$,", "ADV", "VVPP", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und er lobte Land und Leute,", "tokens": ["Und", "er", "lob\u00b7te", "Land", "und", "Leu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Da\u00df sich meine Seele freute", "tokens": ["Da\u00df", "sich", "mei\u00b7ne", "See\u00b7le", "freu\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "PPOSAT", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "An dem liebevollen Geist.", "tokens": ["An", "dem", "lie\u00b7be\u00b7vol\u00b7len", "Geist", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Und er wies auf ein Gel\u00e4nde:", "tokens": ["Und", "er", "wies", "auf", "ein", "Ge\u00b7l\u00e4n\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "\u00bbhier an einem lichten Tage", "tokens": ["\u00bb", "hier", "an", "ei\u00b7nem", "lich\u00b7ten", "Ta\u00b7ge"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Fand ich eure sch\u00f6nste Sage", "tokens": ["Fand", "ich", "eu\u00b7re", "sch\u00f6ns\u00b7te", "Sa\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und ich nahm sie mit mir fort.", "tokens": ["Und", "ich", "nahm", "sie", "mit", "mir", "fort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "APPR", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wandernd hab ich dran gesonnen;", "tokens": ["Wan\u00b7dernd", "hab", "ich", "dran", "ge\u00b7son\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "PAV", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Was zu bilden ich begonnen,", "tokens": ["Was", "zu", "bil\u00b7den", "ich", "be\u00b7gon\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PTKZU", "VVINF", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Legt in Schillers edle H\u00e4nde", "tokens": ["Legt", "in", "Schil\u00b7lers", "ed\u00b7le", "H\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "NE", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Nieder ich als reichen Hort.\u00ab", "tokens": ["Nie\u00b7der", "ich", "als", "rei\u00b7chen", "Hort", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "PPER", "KOUS", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Da er seinen Bruder nannte", "tokens": ["Da", "er", "sei\u00b7nen", "Bru\u00b7der", "nann\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und mir drob das Herz entbrannte,", "tokens": ["Und", "mir", "drob", "das", "Herz", "ent\u00b7brann\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "War's, als schl\u00fcgen weite Fl\u00fcgel", "tokens": ["Wa\u00b7r's", ",", "als", "schl\u00fc\u00b7gen", "wei\u00b7te", "Fl\u00fc\u00b7gel"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "$,", "KOUS", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sausend \u00fcber mir die Luft,", "tokens": ["Sau\u00b7send", "\u00fc\u00b7ber", "mir", "die", "Luft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Schwingen, die den Raum besiegen,", "tokens": ["Schwin\u00b7gen", ",", "die", "den", "Raum", "be\u00b7sie\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wie sie nicht um niedre H\u00fcgel", "tokens": ["Wie", "sie", "nicht", "um", "nied\u00b7re", "H\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "PTKNEG", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Flattern, Schwingen, die sich wiegen,", "tokens": ["Flat\u00b7tern", ",", "Schwin\u00b7gen", ",", "die", "sich", "wie\u00b7gen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "PRELS", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Herrschend \u00fcber Berg und Kluft.", "tokens": ["Herr\u00b7schend", "\u00fc\u00b7ber", "Berg", "und", "Kluft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Selig war ich mit den beiden,", "tokens": ["Se\u00b7lig", "war", "ich", "mit", "den", "bei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "APPR", "ART", "PIAT", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "D\u00e4mmerung verwob die Weiden", "tokens": ["D\u00e4m\u00b7me\u00b7rung", "ver\u00b7wob", "die", "Wei\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN"], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.3": {"text": "Und ich sah zwei treue Sterne", "tokens": ["Und", "ich", "sah", "zwei", "treu\u00b7e", "Ster\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "CARD", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "\u00dcber meiner Heimat gehn.", "tokens": ["\u00dc\u00b7ber", "mei\u00b7ner", "Hei\u00b7mat", "gehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Leben wird mein Volk und dauern", "tokens": ["Le\u00b7ben", "wird", "mein", "Volk", "und", "dau\u00b7ern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "KON", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Zwischen seinen Felsenmauern,", "tokens": ["Zwi\u00b7schen", "sei\u00b7nen", "Fel\u00b7sen\u00b7mau\u00b7ern", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Wenn die Dioskuren gerne", "tokens": ["Wenn", "die", "Dios\u00b7ku\u00b7ren", "ger\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Segnend ihm zu Haupte stehn.", "tokens": ["Seg\u00b7nend", "ihm", "zu", "Haup\u00b7te", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Nahe wieder sah ich gl\u00e4nzen", "tokens": ["Na\u00b7he", "wie\u00b7der", "sah", "ich", "gl\u00e4n\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Meiner Firne scharfe Grenzen,", "tokens": ["Mei\u00b7ner", "Fir\u00b7ne", "schar\u00b7fe", "Gren\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Meiner Alpen wei\u00dfe B\u00fcnde,", "tokens": ["Mei\u00b7ner", "Al\u00b7pen", "wei\u00b7\u00dfe", "B\u00fcn\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wurzelnd tief im Kern der Schweiz;", "tokens": ["Wur\u00b7zelnd", "tief", "im", "Kern", "der", "Schweiz", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "APPRART", "NN", "ART", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wieder bin ich dort gega