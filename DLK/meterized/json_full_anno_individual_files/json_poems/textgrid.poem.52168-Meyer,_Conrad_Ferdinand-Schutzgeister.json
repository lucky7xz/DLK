4": {"text": "Meine Schritte gingen sacht;", "tokens": ["Mei\u00b7ne", "Schrit\u00b7te", "gin\u00b7gen", "sacht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Schauend pilgert ich und lauschte,", "tokens": ["Schau\u00b7end", "pil\u00b7gert", "ich", "und", "lauschte", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "KON", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Weil ein guter Weggeselle", "tokens": ["Weil", "ein", "gu\u00b7ter", "Weg\u00b7ge\u00b7sel\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Heimlich Worte mit mir tauschte", "tokens": ["Heim\u00b7lich", "Wor\u00b7te", "mit", "mir", "tauschte"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "NN", "APPR", "PPER", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Von der Berge Herzensmacht.", "tokens": ["Von", "der", "Ber\u00b7ge", "Her\u00b7zens\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Traulich f\u00fchlt ich seine N\u00e4he", "tokens": ["Trau\u00b7lich", "f\u00fchlt", "ich", "sei\u00b7ne", "N\u00e4\u00b7he"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und mir ward, ob ich ihn s\u00e4he,", "tokens": ["Und", "mir", "ward", ",", "ob", "ich", "ihn", "s\u00e4\u00b7he", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "$,", "KOUS", "PPER", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und er sprach: \u00bbVor manchen Jahren", "tokens": ["Und", "er", "sprach", ":", "\u00bb", "Vor", "man\u00b7chen", "Jah\u00b7ren"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "$.", "$(", "APPR", "PIAT", "NN"], "meter": "+-+---+-", "measure": "unknown.measure.tri"}, "line.4": {"text": "Bin ich r\u00fcstig hier gereist,", "tokens": ["Bin", "ich", "r\u00fcs\u00b7tig", "hier", "ge\u00b7reist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "ADV", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Hier geschritten, dort gefahren!\u00ab", "tokens": ["Hier", "ge\u00b7schrit\u00b7ten", ",", "dort", "ge\u00b7fah\u00b7ren", "!", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["ADV", "VVPP", "$,", "ADV", "VVPP", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und er lobte Land und Leute,", "tokens": ["Und", "er", "lob\u00b7te", "Land", "und", "Leu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Da\u00df sich meine Seele freute", "tokens": ["Da\u00df", "sich", "mei\u00b7ne", "See\u00b7le", "freu\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "PPOSAT", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "An dem liebevollen Geist.", "tokens": ["An", "dem", "lie\u00b7be\u00b7vol\u00b7len", "Geist", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Und er wies auf ein Gel\u0