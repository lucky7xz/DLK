{"dta.poem.5579": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den Weltweisen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es hebet meinen Geist durch einen g\u00fcnst'gen Blick", "tokens": ["Es", "he\u00b7bet", "mei\u00b7nen", "Geist", "durch", "ei\u00b7nen", "g\u00fcnst'\u00b7gen", "Blick"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Urania empor nach jenen heil\u2019gen H\u00f6hen.", "tokens": ["U\u00b7ra\u00b7nia", "em\u00b7por", "nach", "je\u00b7nen", "heil'\u00b7gen", "H\u00f6\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "APPR", "PDAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "Ich will, so viel es kan geschehen,", "tokens": ["Ich", "will", ",", "so", "viel", "es", "kan", "ge\u00b7sche\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "ADV", "ADV", "PPER", "VMFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Jm reinsten Thon, ein neues St\u00fcck", "tokens": ["Jm", "reins\u00b7ten", "Thon", ",", "ein", "neu\u00b7es", "St\u00fcck"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Auf der gestimmten Leyer spielen;", "tokens": ["Auf", "der", "ge\u00b7stimm\u00b7ten", "Le\u00b7yer", "spie\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Auf einen neuen Preis, der ewig dauret, zielen,", "tokens": ["Auf", "ei\u00b7nen", "neu\u00b7en", "Preis", ",", "der", "e\u00b7wig", "dau\u00b7ret", ",", "zie\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "PRELS", "ADJD", "VVFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}