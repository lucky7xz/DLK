{"dta.poem.3574": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Tanzreime .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519172", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Schwimmen zwei Fischle im Wasser herum,               ", "tokens": ["Schwim\u00b7men", "zwei", "Fischle", "im", "Was\u00b7ser", "he\u00b7rum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "CARD", "NN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Strecken die Schw\u00e4nzerl in die H\u00f6h;", "tokens": ["Stre\u00b7cken", "die", "Schw\u00e4n\u00b7zerl", "in", "die", "H\u00f6h", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Liegt es mein Schatzerl im Federbett,", "tokens": ["Liegt", "es", "mein", "Schat\u00b7zerl", "im", "Fe\u00b7der\u00b7bett", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "APPRART", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Thut ihm sein K\u00f6pfle so weh.", "tokens": ["Thut", "ihm", "sein", "K\u00f6pf\u00b7le", "so", "weh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "PPOSAT", "NN", "ADV", "PTKVZ", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.2": {"line.1": {"text": "Komm ich bei Mitternacht,", "tokens": ["Komm", "ich", "bei", "Mit\u00b7ter\u00b7nacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Wird mir gleich aufgemacht;", "tokens": ["Wird", "mir", "gleich", "auf\u00b7ge\u00b7macht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "HabemHaben sein K\u00f6pfle vollgeschw\u00e4tzt,               ", "tokens": ["Ha\u00b7bem", "Ha\u00b7ben", "sein", "K\u00f6pf\u00b7le", "voll\u00b7ge\u00b7schw\u00e4tzt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Hab' ihms voll gelacht!", "tokens": ["Hab'", "ihms", "voll", "ge\u00b7lacht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "VVPP", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}}}}