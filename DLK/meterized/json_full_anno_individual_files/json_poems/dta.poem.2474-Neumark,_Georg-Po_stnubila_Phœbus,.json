{"dta.poem.2474": {"metadata": {"author": {"name": "Neumark, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Po stnubila Ph\u0153bus,", "genre": "Lyrik", "period": "N.A.", "pub_year": "1652", "urn": "urn:nbn:de:kobv:b4-20428-0", "language": ["de:0.85", "no:0.14"], "booktitle": "Neumark, Georg: Poetisch- und Musikalisches Lustw\u00e4ldchen. Hamburg, 1652."}, "poem": {"stanza.1": {"line.1": {"text": "Kein Wetter ist so gro\u00df/ kein Donner kan so knallen/", "tokens": ["Kein", "Wet\u00b7ter", "ist", "so", "gro\u00df", "/", "kein", "Don\u00b7ner", "kan", "so", "knal\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "ADV", "ADJD", "$(", "PIAT", "NN", "VMFIN", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df endlich nicht ein sch\u00f6n und warmer Sonnen-", "tokens": ["Da\u00df", "end\u00b7lich", "nicht", "ein", "sch\u00f6n", "und", "war\u00b7mer", "Son\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "PTKNEG", "ART", "ADJD", "KON", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "schein", "tokens": ["schein"], "token_info": ["word"], "pos": ["ADJD"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Darauf erfolgen solt\u2019; Es geht so in gemein", "tokens": ["Da\u00b7rauf", "er\u00b7fol\u00b7gen", "solt'", ";", "Es", "geht", "so", "in", "ge\u00b7mein"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVINF", "VMFIN", "$.", "PPER", "VVFIN", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Mit allen Menschen zu: Es kan nicht auf sie fallen", "tokens": ["Mit", "al\u00b7len", "Men\u00b7schen", "zu", ":", "Es", "kan", "nicht", "auf", "sie", "fal\u00b7len"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "PTKVZ", "$.", "PPER", "VMFIN", "PTKNEG", "APPR", "PPER", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Des Ungl\u00fckks schwere Last so endlich mit der Zeit/", "tokens": ["Des", "Un\u00b7gl\u00fckks", "schwe\u00b7re", "Last", "so", "end\u00b7lich", "mit", "der", "Zeit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "ADV", "ADV", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Sich nicht verkehren solt? in s\u00fcsse Fr\u00f6ligkeit.", "tokens": ["Sich", "nicht", "ver\u00b7keh\u00b7ren", "solt", "?", "in", "s\u00fcs\u00b7se", "Fr\u00f6\u00b7lig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PRF", "PTKNEG", "VVINF", "VMFIN", "$.", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}