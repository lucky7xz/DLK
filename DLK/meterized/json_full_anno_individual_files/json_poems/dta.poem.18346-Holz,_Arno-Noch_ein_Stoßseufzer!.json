{"dta.poem.18346": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "Noch   ein Sto\u00dfseufzer!", "genre": "Lyrik", "period": "N.A.", "pub_year": "1886", "urn": "urn:nbn:de:kobv:b4-200905192665", "language": ["de:0.85", "af:0.14"], "booktitle": "Holz, Arno: Das Buch der Zeit. Lieder eines Modernen. Z\u00fcrich, 1886."}, "poem": {"stanza.1": {"line.1": {"text": "Die Welt ist bla\u00df, bla\u00df wie Louise,", "tokens": ["Die", "Welt", "ist", "bla\u00df", ",", "bla\u00df", "wie", "Lou\u00b7i\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "ADJD", "KOKOM", "NE", "$,"], "meter": "-+-++-+-+", "measure": "unknown.measure.penta"}, "line.2": {"text": "Das Grundgesetz der neuen Zeit", "tokens": ["Das", "Grund\u00b7ge\u00b7setz", "der", "neu\u00b7en", "Zeit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sei drum das Buch von Adam Riese.", "tokens": ["Sei", "drum", "das", "Buch", "von", "A\u00b7dam", "Rie\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PAV", "ART", "NN", "APPR", "NE", "NE", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Denn wenn die V\u00f6lker nicht mehr fackeln", "tokens": ["Denn", "wenn", "die", "V\u00f6l\u00b7ker", "nicht", "mehr", "fa\u00b7ckeln"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "PTKNEG", "ADV", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und \u00fcber ihm die Throne wackeln,", "tokens": ["Und", "\u00fc\u00b7ber", "ihm", "die", "Thro\u00b7ne", "wa\u00b7ckeln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Dann lupft der Weltwitz sein Visir", "tokens": ["Dann", "lupft", "der", "Welt\u00b7witz", "sein", "Vi\u00b7sir"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und donnert: \u201eZwei mal zwei macht vier!\u201c", "tokens": ["Und", "don\u00b7nert", ":", "\u201e", "Zwei", "mal", "zwei", "macht", "vier", "!", "\u201c"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "$.", "$(", "CARD", "ADV", "CARD", "VVFIN", "CARD", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}