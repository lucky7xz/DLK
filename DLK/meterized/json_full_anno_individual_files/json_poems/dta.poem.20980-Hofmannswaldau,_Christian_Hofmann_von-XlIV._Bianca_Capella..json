{"dta.poem.20980": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "XlIV.  \n Bianca Capella.", "genre": "Lyrik; Prosa; Drama", "period": "N.A.", "pub_year": "1679", "urn": "urn:nbn:de:kobv:b4-20289-1", "language": ["de:0.99"], "booktitle": "Hofmann von Hofmannswaldau, Christian: Deutsche Ubersetzungen und Gedichte. Breslau, 1679."}, "poem": {"stanza.1": {"line.1": {"text": "Mein Bette machte mir das schmeichlende Gel\u00fccke", "tokens": ["Mein", "Bet\u00b7te", "mach\u00b7te", "mir", "das", "schmeich\u00b7len\u00b7de", "Ge\u00b7l\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und peitschte mich hernach mit Dornen seiner T\u00fc-", "tokens": ["Und", "peitschte", "mich", "her\u00b7nach", "mit", "Dor\u00b7nen", "sei\u00b7ner", "T\u00fc"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPR", "NN", "PPOSAT", "TRUNC"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "cke.", "tokens": ["cke", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-", "measure": "single.down"}, "line.4": {"text": "Das Licht von meinem Ruhm gerieth\u2019 in schn\u00f6de", "tokens": ["Das", "Licht", "von", "mei\u00b7nem", "Ruhm", "ge\u00b7ri\u00b7e\u00b7th'", "in", "schn\u00f6\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "ADJD", "APPR", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Nacht/", "tokens": ["Nacht", "/"], "token_info": ["word", "punct"], "pos": ["NN", "$("], "meter": "+", "measure": "single.up"}, "line.6": {"text": "Da\u00df Lieben gifftig ist/ hab\u2019 ich bekand gemacht.", "tokens": ["Da\u00df", "Lie\u00b7ben", "giff\u00b7tig", "ist", "/", "hab'", "ich", "be\u00b7kand", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "ADJD", "VAFIN", "$(", "VAFIN", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}