{"dta.poem.1904": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Ubersezte Spruch-Reimen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn du deine Thaten/ die nichts taugen/", "tokens": ["Wenn", "du", "dei\u00b7ne", "Tha\u00b7ten", "/", "die", "nichts", "tau\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "$(", "ART", "PIS", "VVFIN", "$("], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Uberschielst mit eingeschmierten Augen/", "tokens": ["U\u00b7ber\u00b7schielst", "mit", "ein\u00b7ge\u00b7schmier\u00b7ten", "Au\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Wiltu noch wie Falck und Luchs besehn", "tokens": ["Wil\u00b7tu", "noch", "wie", "Falck", "und", "Luchs", "be\u00b7sehn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "KOKOM", "NN", "KON", "NE", "VVINF"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Was bey deinem Nachbar ist geschehn.", "tokens": ["Was", "bey", "dei\u00b7nem", "Nach\u00b7bar", "ist", "ge\u00b7schehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "PPOSAT", "NN", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}