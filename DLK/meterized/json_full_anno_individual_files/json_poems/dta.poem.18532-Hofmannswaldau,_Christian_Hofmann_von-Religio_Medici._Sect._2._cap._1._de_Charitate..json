{"dta.poem.18532": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Religio Medici. Sect. 2. cap. 1.  \n de Charitate.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1703", "urn": "urn:nbn:de:kobv:b4-200905199360", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die tugend/ so mit recht der menschen ancker heisset/", "tokens": ["Die", "tu\u00b7gend", "/", "so", "mit", "recht", "der", "men\u00b7schen", "an\u00b7cker", "heis\u00b7set", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ADV", "APPR", "ADJD", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die GOttes finger selbst die reine liebe nennt/", "tokens": ["Die", "Got\u00b7tes", "fin\u00b7ger", "selbst", "die", "rei\u00b7ne", "lie\u00b7be", "nennt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "ADV", "ART", "ADJA", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und derer schatten offt die gr\u00f6\u00dfte macht umbreisset/", "tokens": ["Und", "de\u00b7rer", "schat\u00b7ten", "offt", "die", "gr\u00f6\u00df\u00b7te", "macht", "um\u00b7breis\u00b7set", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "ADV", "ART", "ADJA", "VVFIN", "NE", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ists/ derer r\u00f6tel ich nicht selten hab erkennt.", "tokens": ["Ists", "/", "de\u00b7rer", "r\u00f6\u00b7tel", "ich", "nicht", "sel\u00b7ten", "hab", "er\u00b7kennt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "PDS", "NN", "PPER", "PTKNEG", "ADJD", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Di\u00df was des vaters sinn in meinen hat gepflantzet/", "tokens": ["Di\u00df", "was", "des", "va\u00b7ters", "sinn", "in", "mei\u00b7nen", "hat", "ge\u00b7pflant\u00b7zet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PRELS", "ART", "ADJA", "NN", "APPR", "PPOSAT", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ist dieses/ was mir stets nach ihrem zucker schmeckt.", "tokens": ["Ist", "die\u00b7ses", "/", "was", "mir", "stets", "nach", "ih\u00b7rem", "zu\u00b7cker", "schmeckt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDAT", "$(", "PWS", "PPER", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Es hatte sich mein sinn in ihren wall versehantzet/", "tokens": ["Es", "hat\u00b7te", "sich", "mein", "sinn", "in", "ih\u00b7ren", "wall", "ver\u00b7se\u00b7hant\u00b7zet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PRF", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+---+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Und ihre flacken stets zu meinem ziel gesteckt.", "tokens": ["Und", "ih\u00b7re", "fla\u00b7cken", "stets", "zu", "mei\u00b7nem", "ziel", "ge\u00b7steckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Wo die zergliederung der eignen sinnen weiset", "tokens": ["Wo", "die", "zer\u00b7glie\u00b7de\u00b7rung", "der", "eig\u00b7nen", "sin\u00b7nen", "wei\u00b7set"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Ein etwas/ das man wol mit rechte glauben mag;", "tokens": ["Ein", "et\u00b7was", "/", "das", "man", "wol", "mit", "rech\u00b7te", "glau\u00b7ben", "mag", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "$(", "PRELS", "PIS", "ADV", "APPR", "ADJA", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "So scheints/ als w\u00e4r ich nur in ihrem land gereiset/", "tokens": ["So", "scheints", "/", "als", "w\u00e4r", "ich", "nur", "in", "ih\u00b7rem", "land", "ge\u00b7rei\u00b7set", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$(", "KOKOM", "VAFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und anckerte nur blo\u00df bey ihrer niederlag.", "tokens": ["Und", "an\u00b7cker\u00b7te", "nur", "blo\u00df", "bey", "ih\u00b7rer", "nie\u00b7der\u00b7lag", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Es scheint/ al\u00df w\u00e4r ich gantz von ihrer hand gebohren/", "tokens": ["Es", "scheint", "/", "al\u00df", "w\u00e4r", "ich", "gantz", "von", "ih\u00b7rer", "hand", "ge\u00b7boh\u00b7ren", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "KOKOM", "VAFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Al\u00df w\u00e4re jedes glied nach ihrem sinn gemacht/", "tokens": ["Al\u00df", "w\u00e4\u00b7re", "je\u00b7des", "glied", "nach", "ih\u00b7rem", "sinn", "ge\u00b7macht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PIAT", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Al\u00df h\u00e4tte die natur zum muster sie erkohren/", "tokens": ["Al\u00df", "h\u00e4t\u00b7te", "die", "na\u00b7tur", "zum", "mus\u00b7ter", "sie", "er\u00b7koh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "NN", "APPRART", "ADJA", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Und jede zierath uns aus ihrer schoo\u00df gebracht.", "tokens": ["Und", "je\u00b7de", "zie\u00b7rath", "uns", "aus", "ih\u00b7rer", "schoo\u00df", "ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "PPER", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Denn ich kan jede art der menschen wohl vertragen/", "tokens": ["Denn", "ich", "kan", "je\u00b7de", "art", "der", "men\u00b7schen", "wohl", "ver\u00b7tra\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PIAT", "NN", "ART", "ADJA", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Mein auge siehet den und jenen freundlich an.", "tokens": ["Mein", "au\u00b7ge", "sie\u00b7het", "den", "und", "je\u00b7nen", "freund\u00b7lich", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "KON", "PDS", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Ich kan auch keinem nicht ein schlechtes wort versagen/", "tokens": ["Ich", "kan", "auch", "kei\u00b7nem", "nicht", "ein", "schlech\u00b7tes", "wort", "ver\u00b7sa\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PIS", "PTKNEG", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Und schwere ungescheut zu treuer freunde fahn.", "tokens": ["Und", "schwe\u00b7re", "un\u00b7ge\u00b7scheut", "zu", "treu\u00b7er", "freun\u00b7de", "fahn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Mein auge/ zung und mund kan keinen eckel haben/", "tokens": ["Mein", "au\u00b7ge", "/", "zung", "und", "mund", "kan", "kei\u00b7nen", "ec\u00b7kel", "ha\u00b7ben", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "NN", "KON", "NN", "VMFIN", "PIAT", "NN", "VAFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Ich f\u00fchle keine gall in meinen adern nicht/", "tokens": ["Ich", "f\u00fch\u00b7le", "kei\u00b7ne", "gall", "in", "mei\u00b7nen", "a\u00b7dern", "nicht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "APPR", "PPOSAT", "NN", "PTKNEG", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Es kan mich frosch und schneck so wie lampreten laben/", "tokens": ["Es", "kan", "mich", "frosch", "und", "schneck", "so", "wie", "lam\u00b7pre\u00b7ten", "la\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "ADJD", "KON", "ADJD", "ADV", "KOKOM", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Mein mund i\u00dft beydes doch als edelstes gericht.", "tokens": ["Mein", "mund", "i\u00dft", "bey\u00b7des", "doch", "als", "e\u00b7dels\u00b7tes", "ge\u00b7richt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PIS", "ADV", "KOUS", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Wenn beydes mir ein tisch zugleiche tr\u00e4gt und weiset/", "tokens": ["Wenn", "bey\u00b7des", "mir", "ein", "tisch", "zu\u00b7glei\u00b7che", "tr\u00e4gt", "und", "wei\u00b7set", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "ART", "ADJD", "ADJA", "VVFIN", "KON", "VVFIN", "$("], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.26": {"text": "So greiffet meine hand von beyden gleiche zu/", "tokens": ["So", "greif\u00b7fet", "mei\u00b7ne", "hand", "von", "bey\u00b7den", "glei\u00b7che", "zu", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "APPR", "PIAT", "ADJA", "PTKZU", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Wenn einer has\u2019 und reh/ der dort phasanen speiset/", "tokens": ["Wenn", "ei\u00b7ner", "has'", "und", "reh", "/", "der", "dort", "pha\u00b7sa\u00b7nen", "spei\u00b7set", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "KON", "VVFIN", "$(", "ART", "ADV", "VVINF", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "J\u00df\u2019t dort mein mund so wohl/ als hier von einer kuh.", "tokens": ["J\u00df't", "dort", "mein", "mund", "so", "wohl", "/", "als", "hier", "von", "ei\u00b7ner", "kuh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "ADV", "ADV", "$(", "KOKOM", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Das kraut/ das bey dem grab auf todten beinen w\u00e4chset/", "tokens": ["Das", "kraut", "/", "das", "bey", "dem", "grab", "auf", "tod\u00b7ten", "bei\u00b7nen", "w\u00e4ch\u00b7set", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PDS", "APPR", "ART", "NN", "APPR", "ADJA", "VVINF", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Schmeckt mir so wohl als das was in den g\u00e4rten steht;", "tokens": ["Schmeckt", "mir", "so", "wohl", "als", "das", "was", "in", "den", "g\u00e4r\u00b7ten", "steht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "KOKOM", "PDS", "PRELS", "APPR", "ART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Vor dem ein zarter mund au\u00df durst und d\u00fcrre l\u00e4chzet/", "tokens": ["Vor", "dem", "ein", "zar\u00b7ter", "mund", "au\u00df", "durst", "und", "d\u00fcr\u00b7re", "l\u00e4ch\u00b7zet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "ADJA", "NN", "APPR", "ADV", "KON", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Das schlucket meine kehl/ eh krafft und geist vergeht.", "tokens": ["Das", "schluc\u00b7ket", "mei\u00b7ne", "kehl", "/", "eh", "krafft", "und", "geist", "ver\u00b7geht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPOSAT", "NN", "$(", "KOUS", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Seh\u2019 ich gleich schlang und spinn und Basilisken kommen/", "tokens": ["Seh'", "ich", "gleich", "schlang", "und", "spinn", "und", "Ba\u00b7si\u00b7lis\u00b7ken", "kom\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADJD", "KON", "NN", "KON", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "So greiffet meine hand nach keinem steine nicht/", "tokens": ["So", "greif\u00b7fet", "mei\u00b7ne", "hand", "nach", "kei\u00b7nem", "stei\u00b7ne", "nicht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "APPR", "PIS", "VVFIN", "PTKNEG", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Vor kr\u00f6ten habe ich die flucht noch nie genommen/", "tokens": ["Vor", "kr\u00f6\u00b7ten", "ha\u00b7be", "ich", "die", "flucht", "noch", "nie", "ge\u00b7nom\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PPER", "ART", "VVFIN", "ADV", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Vor denen dieser doch und jener sich verkricht.", "tokens": ["Vor", "de\u00b7nen", "die\u00b7ser", "doch", "und", "je\u00b7ner", "sich", "ver\u00b7kricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PDS", "ADV", "KON", "PDS", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Die feindschafft/ so man sonst in andrer adern sindet/", "tokens": ["Die", "feind\u00b7schafft", "/", "so", "man", "sonst", "in", "an\u00b7drer", "a\u00b7dern", "sin\u00b7det", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ADV", "PIS", "ADV", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Hat niemahls bey mir wolln auch nur beherbergt seyn/", "tokens": ["Hat", "nie\u00b7mahls", "bey", "mir", "wolln", "auch", "nur", "be\u00b7her\u00b7bergt", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "PPER", "VMFIN", "ADV", "ADV", "VVPP", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Denn wo man ha\u00df und neid mit einem bande bindet/", "tokens": ["Denn", "wo", "man", "ha\u00df", "und", "neid", "mit", "ei\u00b7nem", "ban\u00b7de", "bin\u00b7det", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PIS", "VVFIN", "KON", "NN", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Da schl\u00e4fft die liebe selbst in solchen windlen ein.", "tokens": ["Da", "schl\u00e4fft", "die", "lie\u00b7be", "selbst", "in", "sol\u00b7chen", "wind\u00b7len", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "VVFIN", "ADV", "APPR", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Frantzosen k\u00f6nnen nicht die Spanier vertragen/", "tokens": ["Frant\u00b7zo\u00b7sen", "k\u00f6n\u00b7nen", "nicht", "die", "Spa\u00b7nier", "ver\u00b7tra\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PTKNEG", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.42": {"text": "Doch schau ich beyde stets mit gleichen augen an/", "tokens": ["Doch", "schau", "ich", "bey\u00b7de", "stets", "mit", "glei\u00b7chen", "au\u00b7gen", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PIS", "ADV", "APPR", "ADJA", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Was diese von der list/ und die von hoffart klagen/", "tokens": ["Was", "die\u00b7se", "von", "der", "list", "/", "und", "die", "von", "hof\u00b7fart", "kla\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PDAT", "APPR", "ART", "NN", "$(", "KON", "ART", "APPR", "NE", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Das hat die feindschafft offt auch ohne recht gethan.", "tokens": ["Das", "hat", "die", "feind\u00b7schafft", "offt", "auch", "oh\u00b7ne", "recht", "ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "ADV", "ADV", "APPR", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Ich kan in beyden ja die klugen geister r\u00fchmen/", "tokens": ["Ich", "kan", "in", "bey\u00b7den", "ja", "die", "klu\u00b7gen", "geis\u00b7ter", "r\u00fch\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PIAT", "ADV", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "Di\u00df was die h\u00f6ffligkeit an beyden vorgestellt/", "tokens": ["Di\u00df", "was", "die", "h\u00f6ff\u00b7lig\u00b7keit", "an", "bey\u00b7den", "vor\u00b7ge\u00b7stellt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PRELS", "ART", "NN", "APPR", "PIS", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "Wie jeder seinen fehl mit klugheit kan verbl\u00fchmen/", "tokens": ["Wie", "je\u00b7der", "sei\u00b7nen", "fehl", "mit", "klug\u00b7heit", "kan", "ver\u00b7bl\u00fch\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "PPOSAT", "NN", "APPR", "NN", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.48": {"text": "Ist was an jedem mir mit rechte wohlgef\u00e4llt.", "tokens": ["Ist", "was", "an", "je\u00b7dem", "mir", "mit", "rech\u00b7te", "wohl\u00b7ge\u00b7f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "APPR", "PIS", "PPER", "APPR", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.49": {"text": "Das hertze Schlesiens hat mich zur welt gebo hren/", "tokens": ["Das", "hert\u00b7ze", "Schle\u00b7si\u00b7ens", "hat", "mich", "zur", "welt", "ge\u00b7bo", "hren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPER", "APPRART", "NN", "NE", "VVINF", "$("], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.50": {"text": "Doch scheints/ a