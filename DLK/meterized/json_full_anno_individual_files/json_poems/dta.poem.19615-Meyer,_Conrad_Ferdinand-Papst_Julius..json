{"dta.poem.19615": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Papst Julius.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-200905193933", "language": ["de:0.99"], "booktitle": "Meyer, Conrad Ferdinand: Gedichte. Leipzig, 1882."}, "poem": {"stanza.1": {"line.1": {"text": "Halb vom Hades schon bezwungen,", "tokens": ["Halb", "vom", "Ha\u00b7des", "schon", "be\u00b7zwun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Von Lemuren schon umschwebt,", "tokens": ["Von", "Le\u00b7mu\u00b7ren", "schon", "um\u00b7schwebt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hat er doch sich losgerungen \u2014", "tokens": ["Hat", "er", "doch", "sich", "los\u00b7ge\u00b7run\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "PRF", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sieh er athmet! Sieh er lebt!", "tokens": ["Sieh", "er", "ath\u00b7met", "!", "Sieh", "er", "lebt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "VVFIN", "$.", "NE", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Hinter seinen greisen Brauen", "tokens": ["Hin\u00b7ter", "sei\u00b7nen", "grei\u00b7sen", "Brau\u00b7en"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Flammt's! Jetzt langt er nach dem Bart,", "tokens": ["Flammt's", "!", "Jetzt", "langt", "er", "nach", "dem", "Bart", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Z\u00fcrnt und schilt den Tod mit rauhen,", "tokens": ["Z\u00fcrnt", "und", "schilt", "den", "Tod", "mit", "rau\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "VVFIN", "ART", "NN", "APPR", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Ungest\u00fcmen Worten hart:", "tokens": ["Un\u00b7ge\u00b7st\u00fc\u00b7men", "Wor\u00b7ten", "hart", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "\u201eweg mir aus dem Angesichte,", "tokens": ["\u201e", "weg", "mir", "aus", "dem", "An\u00b7ge\u00b7sich\u00b7te", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Larven, die mir bleich gedroht!", "tokens": ["Lar\u00b7ven", ",", "die", "mir", "bleich", "ge\u00b7droht", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPER", "ADJD", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Charon, aus dem Sonnenlichte", "tokens": ["Cha\u00b7ron", ",", "aus", "dem", "Son\u00b7nen\u00b7lich\u00b7te"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "APPR", "ART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Weg ins Schilf mit deinem Boot!", "tokens": ["Weg", "ins", "Schilf", "mit", "dei\u00b7nem", "Boot", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Keine Macht ist dir gegeben,", "tokens": ["Kei\u00b7ne", "Macht", "ist", "dir", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Bis ich selbst dich rufen mag!", "tokens": ["Bis", "ich", "selbst", "dich", "ru\u00b7fen", "mag", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PPER", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Heute hab' ich noch zu leben", "tokens": ["Heu\u00b7te", "hab'", "ich", "noch", "zu", "le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Einen vollgedr\u00e4ngten Tag!", "tokens": ["Ei\u00b7nen", "voll\u00b7ge\u00b7dr\u00e4ng\u00b7ten", "Tag", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Arzt, statt deiner faden Tropfen", "tokens": ["Arzt", ",", "statt", "dei\u00b7ner", "fa\u00b7den", "Trop\u00b7fen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "KOUI", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gieb mir des Falerners Glut!", "tokens": ["Gieb", "mir", "des", "Fa\u00b7ler\u00b7ners", "Glut", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Lasse meine Pulse klopfen,", "tokens": ["Las\u00b7se", "mei\u00b7ne", "Pul\u00b7se", "klop\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wirf mir Feuer in das Blut!", "tokens": ["Wirf", "mir", "Feu\u00b7er", "in", "das", "Blut", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Auf die Th\u00fcren! Weg die Kissen!", "tokens": ["Auf", "die", "Th\u00fc\u00b7ren", "!", "Weg", "die", "Kis\u00b7sen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$.", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Meine Feldherrn, tretet ein!", "tokens": ["Mei\u00b7ne", "Feld\u00b7herrn", ",", "tre\u00b7tet", "ein", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Meine Meister, la\u00dft sie wissen,", "tokens": ["Mei\u00b7ne", "Meis\u00b7ter", ",", "la\u00dft", "sie", "wis\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVIMP", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Da\u00df sie dreifach emsig sei'n!", "tokens": ["Da\u00df", "sie", "drei\u00b7fach", "em\u00b7sig", "sei'n", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Regst, Bramante, die geschickten", "tokens": ["Regst", ",", "Bra\u00b7man\u00b7te", ",", "die", "ge\u00b7schick\u00b7ten"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["NN", "$,", "VVFIN", "$,", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "H\u00e4nde du? Vollende doch!", "tokens": ["H\u00e4n\u00b7de", "du", "?", "Voll\u00b7en\u00b7de", "doch", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPER", "$.", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Diese Augen, sie erblickten", "tokens": ["Die\u00b7se", "Au\u00b7gen", ",", "sie", "er\u00b7blick\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PDAT", "NN", "$,", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gerne deine Kuppel noch!", "tokens": ["Ger\u00b7ne", "dei\u00b7ne", "Kup\u00b7pel", "noch", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Michel Angelo, willkommen!", "tokens": ["Mi\u00b7chel", "An\u00b7ge\u00b7lo", ",", "will\u00b7kom\u00b7men", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["NE", "NE", "$,", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Warum schaust du wieder scheel?", "tokens": ["Wa\u00b7rum", "schaust", "du", "wie\u00b7der", "scheel", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Dort erblick' ich meinen frommen,", "tokens": ["Dort", "er\u00b7blick'", "ich", "mei\u00b7nen", "from\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Meinen s\u00fc\u00dfen Raphael!", "tokens": ["Mei\u00b7nen", "s\u00fc\u00b7\u00dfen", "Ra\u00b7phael", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "NE", "$."], "meter": "+-+---", "measure": "unknown.measure.di"}}, "stanza.5": {"line.1": {"text": "Als den Hirten nicht des Lammes,", "tokens": ["Als", "den", "Hir\u00b7ten", "nicht", "des", "Lam\u00b7mes", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PTKNEG", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bildet mich als Mosen ab,", "tokens": ["Bil\u00b7det", "mich", "als", "Mo\u00b7sen", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KOUS", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Der den Dr\u00e4nger seines Stammes", "tokens": ["Der", "den", "Dr\u00e4n\u00b7ger", "sei\u00b7nes", "Stam\u00b7mes"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Niederschlug mit wucht'gem Stab \u2014", "tokens": ["Nie\u00b7der\u00b7schlug", "mit", "wucht'\u00b7gem", "Stab"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wo die Wasserst\u00fcrze tosen", "tokens": ["Wo", "die", "Was\u00b7ser\u00b7st\u00fcr\u00b7ze", "to\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "In die Brunnenschale jach,", "tokens": ["In", "die", "Brun\u00b7nen\u00b7scha\u00b7le", "jach", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Setzet, Meister, mich als Mosen,", "tokens": ["Set\u00b7zet", ",", "Meis\u00b7ter", ",", "mich", "als", "Mo\u00b7sen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$,", "PPER", "KOUS", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Der die Felsenwand zerbrach!", "tokens": ["Der", "die", "Fel\u00b7sen\u00b7wand", "zer\u00b7brach", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Moses bin ich, in dem Blitze", "tokens": ["Mo\u00b7ses", "bin", "ich", ",", "in", "dem", "Blit\u00b7ze"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "VAFIN", "PPER", "$,", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sinai's, in Rauch und Dampf:", "tokens": ["Si\u00b7nai's", ",", "in", "Rauch", "und", "Dampf", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Meine donnernden Gesch\u00fctze", "tokens": ["Mei\u00b7ne", "don\u00b7nern\u00b7den", "Ge\u00b7sch\u00fct\u00b7ze"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Enden flammend jeden Kampf!", "tokens": ["En\u00b7den", "flam\u00b7mend", "je\u00b7den", "Kampf", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Mit den neugego\u00dfnen St\u00fccken", "tokens": ["Mit", "den", "neu\u00b7ge\u00b7go\u00df\u00b7nen", "St\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Bring' ich Burg und Stadt zu Fall,", "tokens": ["Bring'", "ich", "Burg", "und", "Stadt", "zu", "Fall", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "NN", "KON", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Schmettre Breschen, breche L\u00fccken", "tokens": ["Schmett\u00b7re", "Bre\u00b7schen", ",", "bre\u00b7che", "L\u00fc\u00b7cken"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "In den st\u00e4rksten Mauerwall!", "tokens": ["In", "den", "st\u00e4rks\u00b7ten", "Mau\u00b7er\u00b7wall", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}, "stanza.7": {"line.1": {"text": "Falkner, sprich, was macht mein Sperber,", "tokens": ["Falk\u00b7ner", ",", "sprich", ",", "was", "macht", "mein", "Sper\u00b7ber", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "$,", "PWS", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der die Klaue sich zerstie\u00df?", "tokens": ["Der", "die", "Klau\u00b7e", "sich", "zer\u00b7stie\u00df", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "PRF", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Marschalk, sag, wie lebt mein Berber,", "tokens": ["Mar\u00b7schalk", ",", "sag", ",", "wie", "lebt", "mein", "Ber\u00b7ber", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "$,", "PWAV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Den zu scharf ich jagen lie\u00df?", "tokens": ["Den", "zu", "scharf", "ich", "ja\u00b7gen", "lie\u00df", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKZU", "VVFIN", "PPER", "VVINF", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Tummelt, Diener, zum Erg\u00f6tzen", "tokens": ["Tum\u00b7melt", ",", "Die\u00b7ner", ",", "zum", "Er\u00b7g\u00f6t\u00b7zen"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Mir im Hof ein feurig Thier!", "tokens": ["Mir", "im", "Hof", "ein", "feu\u00b7rig", "Thier", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "ART", "ADJD", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "La\u00dft es springen, la\u00dft es setzen", "tokens": ["La\u00dft", "es", "sprin\u00b7gen", ",", "la\u00dft", "es", "set\u00b7zen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVIMP", "PPER", "VVINF", "$,", "VVIMP", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Vor den alten Augen mir!", "tokens": ["Vor", "den", "al\u00b7ten", "Au\u00b7gen", "mir", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Helmt mir die gefurchte Stirne!", "tokens": ["Helmt", "mir", "die", "ge\u00b7furch\u00b7te", "Stir\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Harnischt mir die welke Hand!", "tokens": ["Har\u00b7nischt", "mir", "die", "wel\u00b7ke", "Hand", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+---+-+", "measure": "dactylic.init"}, "line.3": {"text": "Der Italien 