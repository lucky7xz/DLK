{"textgrid.poem.46943": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "13.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der ich gebot von Jericho den Mauern:", "tokens": ["Der", "ich", "ge\u00b7bot", "von", "Je\u00b7ri\u00b7cho", "den", "Mau\u00b7ern", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "APPR", "NE", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "St\u00fcrzt ein! und sie gedachten nicht zu stehen;", "tokens": ["St\u00fcrzt", "ein", "!", "und", "sie", "ge\u00b7dach\u00b7ten", "nicht", "zu", "ste\u00b7hen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$.", "KON", "PPER", "VVFIN", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Meint ihr, wenn meines Odems St\u00fcrme gehen,", "tokens": ["Meint", "ihr", ",", "wenn", "mei\u00b7nes", "O\u00b7dems", "St\u00fcr\u00b7me", "ge\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "PPOSAT", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Burgen eurer Feinde werden dauern?", "tokens": ["Die", "Bur\u00b7gen", "eu\u00b7rer", "Fein\u00b7de", "wer\u00b7den", "dau\u00b7ern", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "\u00bbder ich lie\u00df \u00fcber den erstaunten Schauern", "tokens": ["\u00bb", "der", "ich", "lie\u00df", "\u00fc\u00b7ber", "den", "er\u00b7staun\u00b7te