{"dta.poem.11455": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Vermischte Getichte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wundre dich nicht, da\u00df ein narr seinen weg so schnelle gehet,", "tokens": ["Wund\u00b7re", "dich", "nicht", ",", "da\u00df", "ein", "narr", "sei\u00b7nen", "weg", "so", "schnel\u00b7le", "ge\u00b7het", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "$,", "KOUS", "ART", "ADJD", "PPOSAT", "ADV", "ADV", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "Da ein meister langsam ist, und nicht selten stille stehet.", "tokens": ["Da", "ein", "meis\u00b7ter", "lang\u00b7sam", "ist", ",", "und", "nicht", "sel\u00b7ten", "stil\u00b7le", "ste\u00b7het", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADJD", "VAFIN", "$,", "KON", "PTKNEG", "ADJD", "ADJA", "VVFIN", "$."], "meter": "--+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Jenen leitet wahn und bo\u00dfheit, die nur rennt und nicht ver-", "tokens": ["Je\u00b7nen", "lei\u00b7tet", "wahn", "und", "bo\u00df\u00b7heit", ",", "die", "nur", "rennt", "und", "nicht", "ver"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PTKVZ", "KON", "NN", "$,", "PRELS", "ADV", "VVFIN", "KON", "PTKNEG", "TRUNC"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.4": {"text": "weilt;", "tokens": ["weilt", ";"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-", "measure": "single.down"}, "line.5": {"text": "Diesen aber witz und tugend, die sich niemahls \u00fcbereilt.", "tokens": ["Die\u00b7sen", "a\u00b7ber", "witz", "und", "tu\u00b7gend", ",", "die", "sich", "nie\u00b7mahls", "\u00fc\u00b7be\u00b7reilt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ADJD", "KON", "NN", "$,", "PRELS", "PRF", "ADV", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}}}}