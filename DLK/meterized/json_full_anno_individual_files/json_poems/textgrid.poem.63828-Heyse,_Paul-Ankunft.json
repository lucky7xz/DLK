{"textgrid.poem.63828": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "Ankunft", "genre": "verse", "period": "N.A.", "pub_year": 1901, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun seid gegr\u00fc\u00dft mir, Land und See und hoch am Berg du altes Nest,", "tokens": ["Nun", "seid", "ge\u00b7gr\u00fc\u00dft", "mir", ",", "Land", "und", "See", "und", "hoch", "am", "Berg", "du", "al\u00b7tes", "Nest", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "VVPP", "PPER", "$,", "NN", "KON", "NN", "KON", "ADJD", "APPRART", "NN", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.2": {"text": "Ihr Vignen, l\u00e4ngst nicht mehr geschm\u00fcckt mit rothem Herbstlaub wie zum Fest,", "tokens": ["Ihr", "Vig\u00b7nen", ",", "l\u00e4ngst", "nicht", "mehr", "ge\u00b7schm\u00fcckt", "mit", "ro\u00b7them", "Herbst\u00b7laub", "wie", "zum", "Fest", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADV", "PTKNEG", "ADV", "VVPP", "APPR", "ADJA", "NN", "KOKOM", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.3": {"text": "Olivenhalden, Lorbeerweg, du Bach, der wild zu Tale schie\u00dft,", "tokens": ["O\u00b7li\u00b7ven\u00b7hal\u00b7den", ",", "Lor\u00b7beer\u00b7weg", ",", "du", "Bach", ",", "der", "wild", "zu", "Ta\u00b7le", "schie\u00dft", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "PPER", "NE", "$,", "PRELS", "ADJD", "APPR", "NE", "VVFIN", "$,"], "meter": "+--+-+-+-+-+-+-+", "measure": "iambic.octa.plus.invert"}, "line.4": {"text": "Und ihr zumal am Seegestad, mein Haus und Garten, seid gegr\u00fc\u00dft!", "tokens": ["Und", "ihr", "zu\u00b7mal", "am", "See\u00b7ge\u00b7stad", ",", "mein", "Haus", "und", "Gar\u00b7ten", ",", "seid", "ge\u00b7gr\u00fc\u00dft", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "APPRART", "NN", "$,", "PPOSAT", "NN", "KON", "NN", "$,", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}}, "stanza.2": {"line.1": {"text": "Ich find euch nach der Sommerglut noch frisch und gr\u00fcn am alten Fleck.", "tokens": ["Ich", "find", "euch", "nach", "der", "Som\u00b7mer\u00b7glut", "noch", "frisch", "und", "gr\u00fcn", "am", "al\u00b7ten", "Fleck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "ADV", "ADJD", "KON", "ADJD", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.2": {"text": "Die Erd' hat in Sal\u00f2 gebebt, ihr kamt davon mit blo\u00dfem Schreck,", "tokens": ["Die", "Erd'", "hat", "in", "Sa\u00b7l\u00f2", "ge\u00b7bebt", ",", "ihr", "kamt", "da\u00b7von", "mit", "blo\u00b7\u00dfem", "Schreck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "NE", "VVPP", "$,", "PPER", "VVFIN", "PAV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.3": {"text": "Und dir, o mein Villino, wuchs inzwischen noch ein Fl\u00fcgel an,", "tokens": ["Und", "dir", ",", "o", "mein", "Vil\u00b7li\u00b7no", ",", "wuchs", "in\u00b7zwi\u00b7schen", "noch", "ein", "Fl\u00fc\u00b7gel", "an", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "FM", "PPOSAT", "NN", "$,", "VVFIN", "ADV", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "-+--+--+-+-+-+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "So da\u00df ich mich zum Winterschlaf in dir bequemer strecken kann.", "tokens": ["So", "da\u00df", "ich", "mich", "zum", "Win\u00b7ter\u00b7schlaf", "in", "dir", "be\u00b7que\u00b7mer", "stre\u00b7cken", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "PRF", "APPRART", "NN", "APPR", "PPER", "ADJD", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}}, "stanza.3": {"line.1": {"text": "Und diesen st\u00f6rt auch selten nur von drau\u00dfen ein lebend'ger Klang,", "tokens": ["Und", "die\u00b7sen", "st\u00f6rt", "auch", "sel\u00b7ten", "nur", "von", "drau\u00b7\u00dfen", "ein", "le\u00b7ben\u00b7d'\u00b7ger", "Klang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "ADV", "ADJD", "ADV", "APPR", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-+--+", "measure": "iambic.octa.plus.octa.plus.chol"}, "line.2": {"text": "Ged\u00e4mpft ert\u00f6nt das Glockenspiel zu mir herab vom Bergeshang,", "tokens": ["Ge\u00b7d\u00e4mpft", "er\u00b7t\u00f6nt", "das", "Glo\u00b7cken\u00b7spiel", "zu", "mir", "her\u00b7ab", "vom", "Ber\u00b7ges\u00b7hang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "APPR", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.3": {"text": "Vom See nur eines Dampfers Pfiff, ein Eselchen schreit dann und wann,", "tokens": ["Vom", "See", "nur", "ei\u00b7nes", "Damp\u00b7fers", "Pfiff", ",", "ein", "E\u00b7sel\u00b7chen", "schreit", "dann", "und", "wann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "ART", "NN", "NN", "$,", "ART", "NN", "VVFIN", "ADV", "KON", "PWAV", "$,"], "meter": "-+-+-+-+--+-+--+", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "Und alle St\u00fccke kenn' ich l\u00e4ngst, die drunten spielt der Orgelmann.", "tokens": ["Und", "al\u00b7le", "St\u00fc\u00b7cke", "kenn'", "ich", "l\u00e4ngst", ",", "die", "drun\u00b7ten", "spielt", "der", "Or\u00b7gel\u00b7mann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "PPER", "ADV", "$,", "PRELS", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}}, "stanza.4": {"line.1": {"text": "Und wenn am Haus die Glocke t\u00f6nt, sie meldet lahme Bettler nur;", "tokens": ["Und", "wenn", "am", "Haus", "die", "Glo\u00b7cke", "t\u00f6nt", ",", "sie", "mel\u00b7det", "lah\u00b7me", "Bett\u00b7ler", "nur", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "APPRART", "NN", "ART", "NN", "VVFIN", "$,", "PPER", "VVFIN", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.2": {"text": "Ein Nachbar, der zu plaudern kommt, tritt selten ein in meinen Flur.", "tokens": ["Ein", "Nach\u00b7bar", ",", "der", "zu", "plau\u00b7dern", "kommt", ",", "tritt", "sel\u00b7ten", "ein", "in", "mei\u00b7nen", "Flur", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PTKZU", "VVINF", "VVFIN", "$,", "VVFIN", "ADJD", "ART", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.3": {"text": "Es lockt uns kein Konzert hinaus, ein Schauspiel bietet nur der See,", "tokens": ["Es", "lockt", "uns", "kein", "Kon\u00b7zert", "hin\u00b7aus", ",", "ein", "Schau\u00b7spiel", "bie\u00b7tet", "nur", "der", "See", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIAT", "NN", "APZR", "$,", "ART", "NN", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.4": {"text": "Und Langweil h\u00fcllt uns dichter ein, als hoch im Norden Eis und Schnee.", "tokens": ["Und", "Lang\u00b7weil", "h\u00fcllt", "uns", "dich\u00b7ter", "ein", ",", "als", "hoch", "im", "Nor\u00b7den", "Eis", "und", "Schnee", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PPER", "ADJD", "PTKVZ", "$,", "KOUS", "ADJD", "APPRART", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}}, "stanza.5": {"line.1": {"text": "Ein hochverehrlicher Kurvorstand sorgt eifrig und gewissenhaft,", "tokens": ["Ein", "hoch\u00b7ver\u00b7ehr\u00b7li\u00b7cher", "Kur\u00b7vor\u00b7stand", "sorgt", "eif\u00b7rig", "und", "ge\u00b7wis\u00b7sen\u00b7haft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+--+-+-+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "Da\u00df ja nur keine Lustbarkeit den Winterg\u00e4sten Fieber schafft,", "tokens": ["Da\u00df", "ja", "nur", "kei\u00b7ne", "Lust\u00b7bar\u00b7keit", "den", "Win\u00b7ter\u00b7g\u00e4s\u00b7ten", "Fie\u00b7ber", "schafft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADV", "PIAT", "NN", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.3": {"text": "Da\u00df Keinem, der zu sterben kam, hier, wo ihm letzte Frist gew\u00e4hrt,", "tokens": ["Da\u00df", "Kei\u00b7nem", ",", "der", "zu", "ster\u00b7ben", "kam", ",", "hier", ",", "wo", "ihm", "letz\u00b7te", "Frist", "ge\u00b7w\u00e4hrt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "$,", "PRELS", "PTKZU", "VVINF", "VVFIN", "$,", "ADV", "$,", "PWAV", "PPER", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.4": {"text": "Der Abschied von der sch\u00f6nen Welt durch Lebensfreuden werd' erschwert.", "tokens": ["Der", "Ab\u00b7schied", "von", "der", "sch\u00f6\u00b7nen", "Welt", "durch", "Le\u00b7bens\u00b7freu\u00b7den", "werd'", "er\u00b7schwert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "APPR", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}}, "stanza.6": {"line.1": {"text": "Doch da's einmal nicht anders ist, so f\u00fcg' ich mich gefa\u00dft darein,", "tokens": ["Doch", "da's", "ein\u00b7mal", "nicht", "an\u00b7ders", "ist", ",", "so", "f\u00fcg'", "ich", "mich", "ge\u00b7fa\u00dft", "da\u00b7rein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "ADV", "PTKNEG", "ADV", "VAFIN", "$,", "ADV", "VVFIN", "PPER", "PRF", "VVPP", "PAV", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.2": {"text": "Die Welt zu meiden winterlang, um l\u00e4nger auf der Welt zu sein.", "tokens": ["Die", "Welt", "zu", "mei\u00b7den", "win\u00b7ter\u00b7lang", ",", "um", "l\u00e4n\u00b7ger", "auf", "der", "Welt", "zu", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "VVFIN", "$,", "KOUI", "ADJD", "APPR", "ART", "NN", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.3": {"text": "Und \u00fcberdies \u2013 ein Winterschlaf hier unter Palmen \u2013 in der Tat,", "tokens": ["Und", "\u00fc\u00b7ber\u00b7dies", "\u2013", "ein", "Win\u00b7ter\u00b7schlaf", "hier", "un\u00b7ter", "Pal\u00b7men", "\u2013", "in", "der", "Tat", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$(", "ART", "NN", "ADV", "APPR", "NN", "$(", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.4": {"text": "Ich finde, da\u00df trotz alledem er seine stillen Reize hat.", "tokens": ["Ich", "fin\u00b7de", ",", "da\u00df", "trotz", "al\u00b7le\u00b7dem", "er", "sei\u00b7ne", "stil\u00b7len", "Rei\u00b7ze", "hat", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "APPR", "PIS", "PPER", "PPOSAT", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}}, "stanza.7": {"line.1": {"text": "Sagt Hamlet doch: \u00bbWas uns im Schlaf f\u00fcr Tr\u00e4ume kommen, ja, da liegt's!\u00ab", "tokens": ["Sagt", "Ham\u00b7let", "doch", ":", "\u00bb", "Was", "uns", "im", "Schlaf", "f\u00fcr", "Tr\u00e4u\u00b7me", "kom\u00b7men", ",", "ja", ",", "da", "liegt's", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "punct"], "pos": ["VVFIN", "NE", "ADV", "$.", "$(", "PWS", "PPER", "APPRART", "NN", "APPR", "NN", "VVINF", "$,", "PTKANT", "$,", "KOUS", "NE", "$.", "$("], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.2": {"text": "Wohl Manches, was mir hier getr\u00e4umt, nicht mit dem Morgenrot verfliegt's.", "tokens": ["Wohl", "Man\u00b7ches", ",", "was", "mir", "hier", "ge\u00b7tr\u00e4umt", ",", "nicht", "mit", "dem", "Mor\u00b7gen\u00b7rot", "ver\u00b7fliegt'", "s."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["ADV", "PIS", "$,", "PWS", "PPER", "ADV", "VVPP", "$,", "PTKNEG", "APPR", "ART", "NN", "VVFIN", "NE"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.3": {"text": "Einstweilen sei's im Tagebuch hier zwanglos zu Papier gebracht,", "tokens": ["Einst\u00b7wei\u00b7len", "sei's", "im", "Ta\u00b7ge\u00b7buch", "hier", "zwang\u00b7los", "zu", "Pa\u00b7pier", "ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "APPRART", "NN", "ADV", "ADJD", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.4": {"text": "Und somit, halb im Traume schon, w\u00fcnsch' ich mir selber gute Nacht!", "tokens": ["Und", "so\u00b7mit", ",", "halb", "im", "Trau\u00b7me", "schon", ",", "w\u00fcn\u00b7sch'", "ich", "mir", "sel\u00b7ber", "gu\u00b7te", "Nacht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "ADJD", "APPRART", "NN", "ADV", "$,", "VVFIN", "PPER", "PPER", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+--+-+-+", "measure": "iambic.octa.plus.relaxed"}}, "stanza.8": {"line.1": {"text": "Nun seid gegr\u00fc\u00dft mir, Land und See und hoch am Berg du altes Nest,", "tokens": ["Nun", "seid", "ge\u00b7gr\u00fc\u00dft", "mir", ",", "Land", "und", "See", "und", "hoch", "am", "Berg", "du", "al\u00b7tes", "Nest", ","], "token_info": ["word", "word", 