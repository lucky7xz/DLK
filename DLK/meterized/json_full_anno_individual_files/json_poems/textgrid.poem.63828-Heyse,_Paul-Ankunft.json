: {"line.1": {"text": "Ich find euch nach der Sommerglut noch frisch und gr\u00fcn am alten Fleck.", "tokens": ["Ich", "find", "euch", "nach", "der", "Som\u00b7mer\u00b7glut", "noch", "frisch", "und", "gr\u00fcn", "am", "al\u00b7ten", "Fleck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "ADV", "ADJD", "KON", "ADJD", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.2": {"text": "Die Erd' hat in Sal\u00f2 gebebt, ihr kamt davon mit blo\u00dfem Schreck,", "tokens": ["Die", "Erd'", "hat", "in", "Sa\u00b7l\u00f2", "ge\u00b7bebt", ",", "ihr", "kamt", "da\u00b7von", "mit", "blo\u00b7\u00dfem", "Schreck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "NE", "VVPP", "$,", "PPER", "VVFIN", "PAV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.3": {"text": "Und dir, o mein Villino, wuchs inzwischen noch ein Fl\u00fcgel an,", "tokens": ["Und", "dir", ",", "o", "mein", "Vil\u00b7li\u00b7no", ",", "wuchs", "in\u00b7zwi\u00b7schen", "noch", "ein", "Fl\u00fc\u00b7gel", "an", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "FM", "PPOSAT", "NN", "$,", "VVFIN", "ADV", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "-+--+--+-+-+-+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "So da\u00df ich mich zum Winterschlaf in dir bequemer strecken kann.", "tokens": ["So", "da\u00df", "ich", "mich", "zum", "Win\u00b7ter\u00b7schlaf", "in", "dir", "be\u00b7que\u00b7mer", "stre\u00b7cken", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "pu