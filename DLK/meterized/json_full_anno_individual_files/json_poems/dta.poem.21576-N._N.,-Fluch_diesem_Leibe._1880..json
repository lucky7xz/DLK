{"dta.poem.21576": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Fluch diesem Leibe.  \n 1880.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Fluch diesem Leibe,", "tokens": ["Fluch", "die\u00b7sem", "Lei\u00b7be", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PDAT", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Dem uners\u00e4ttlich l\u00fcsternen,", "tokens": ["Dem", "un\u00b7er\u00b7s\u00e4tt\u00b7lich", "l\u00fcs\u00b7ter\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVINF", "$,"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.3": {"text": "Mit seinen Banden", "tokens": ["Mit", "sei\u00b7nen", "Ban\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Schn\u00fcrt er die Seele ein", "tokens": ["Schn\u00fcrt", "er", "die", "See\u00b7le", "ein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "ART"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Und rei\u00dft in den Koth", "tokens": ["Und", "rei\u00dft", "in", "den", "Koth"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Die Sonnendurstige.", "tokens": ["Die", "Son\u00b7nen\u00b7durs\u00b7ti\u00b7ge", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Aus allen Poren", "tokens": ["Aus", "al\u00b7len", "Po\u00b7ren"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PIAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "Schrei ich nach Freiheit,", "tokens": ["Schrei", "ich", "nach", "Frei\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "In alle Himmel m\u00f6cht\u2019 ich mich recken, \u2014", "tokens": ["In", "al\u00b7le", "Him\u00b7mel", "m\u00f6cht'", "ich", "mich", "re\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PIAT", "NN", "VMFIN", "PPER", "PRF", "VVINF", "$,", "$("], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Aber erbarmungslos", "tokens": ["A\u00b7ber", "er\u00b7bar\u00b7mungs\u00b7los"], "token_info": ["word", "word"], "pos": ["KON", "ADJD"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.11": {"text": "Pre\u00dft mich das Elend", "tokens": ["Pre\u00dft", "mich", "das", "E\u00b7lend"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.12": {"text": "Meiner Sinne", "tokens": ["Mei\u00b7ner", "Sin\u00b7ne"], "token_info": ["word", "word"], "pos": ["PPOSAT", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.13": {"text": "Zur\u00fcck in die Dienstbarkeit.", "tokens": ["Zu\u00b7r\u00fcck", "in", "die", "Dienst\u00b7bar\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.14": {"text": "O Hunger", "tokens": ["O", "Hun\u00b7ger"], "token_info": ["word", "word"], "pos": ["NE", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.15": {"text": "Nach dem Ewigen \u2014", "tokens": ["Nach", "dem", "E\u00b7wi\u00b7gen"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "$("], "meter": "+-+--", "measure": "unknown.measure.di"}, "line.16": {"text": "O Hunger!", "tokens": ["O", "Hun\u00b7ger", "!"], "token_info": ["word", "word", "punct"], "pos": ["NE", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.17": {"text": "Wann kommt die Stunde,", "tokens": ["Wann", "kommt", "die", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.18": {"text": "Wo ich Alles vergessen,", "tokens": ["Wo", "ich", "Al\u00b7les", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PIS", "VVPP", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.19": {"text": "Alles hinschleudern darf", "tokens": ["Al\u00b7les", "hin\u00b7schleu\u00b7dern", "darf"], "token_info": ["word", "word", "word"], "pos": ["PIS", "VVINF", "VMFIN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.20": {"text": "Und nur dich, einzig dich", "tokens": ["Und", "nur", "dich", ",", "ein\u00b7zig", "dich"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ADV", "PPER", "$,", "ADJD", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.21": {"text": "Zu stillen vermag?", "tokens": ["Zu", "stil\u00b7len", "ver\u00b7mag", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.22": {"text": "Weh, wenn die Flamme,", "tokens": ["Weh", ",", "wenn", "die", "Flam\u00b7me", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "ART", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.23": {"text": "Die in mir lodert,", "tokens": ["Die", "in", "mir", "lo\u00b7dert", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.24": {"text": "Mich brennend verzehrte,", "tokens": ["Mich", "bren\u00b7nend", "ver\u00b7zehr\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVFIN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.25": {"text": "Und nicht emporschl\u00fcg\u2019", "tokens": ["Und", "nicht", "em\u00b7por\u00b7schl\u00fcg'"], "token_info": ["word", "word", "word"], "pos": ["KON", "PTKNEG", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.26": {"text": "Wetterleuchtend,", "tokens": ["Wet\u00b7ter\u00b7leuch\u00b7tend", ","], "token_info": ["word", "punct"], "pos": ["ADJD", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.27": {"text": "Herzenentz\u00fcndend.", "tokens": ["Her\u00b7ze\u00b7nent\u00b7z\u00fcn\u00b7dend", "."], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.28": {"text": "Fort, fort, ihr Bilder", "tokens": ["Fort", ",", "fort", ",", "ihr", "Bil\u00b7der"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["NE", "$,", "PTKVZ", "$,", "PPOSAT", "NN"], "meter": "++-+-", "measure": "iambic.di"}, "line.29": {"text": "Lockender L\u00fcste!", "tokens": ["Lo\u00b7cken\u00b7der", "L\u00fcs\u00b7te", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.30": {"text": "Ich will keinen Platz", "tokens": ["Ich", "will", "kei\u00b7nen", "Platz"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PIAT", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.31": {"text": "Am Mahle der Lebenden,", "tokens": ["Am", "Mah\u00b7le", "der", "Le\u00b7ben\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,"], "meter": "-+--+--", "measure": "iambic.di.relaxed"}, "line.32": {"text": "Wo, im glitzernden Licht,", "tokens": ["Wo", ",", "im", "glit\u00b7zern\u00b7den", "Licht", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.33": {"text": "Schwarz\u00e4ugiger Frauen", "tokens": ["Schwarz\u00b7\u00e4u\u00b7gi\u00b7ger", "Frau\u00b7en"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.34": {"text": "Hei\u00dfe, lodernde Blicke", "tokens": ["Hei\u00b7\u00dfe", ",", "lo\u00b7dern\u00b7de", "Bli\u00b7cke"], "token_info": ["word", "punct", "word", "word"], "pos": ["ADJA", "$,", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.35": {"text": "Die Seele versengen.", "tokens": ["Die", "See\u00b7le", "ver\u00b7sen\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.36": {"text": "Ich lausche den Todten", "tokens": ["Ich", "lau\u00b7sche", "den", "Tod\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.37": {"text": "Und horche, was sie verk\u00fcnden,", "tokens": ["Und", "hor\u00b7che", ",", "was", "sie", "ver\u00b7k\u00fcn\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.38": {"text": "Und ich suche die Ungebornen,", "tokens": ["Und", "ich", "su\u00b7che", "die", "Un\u00b7ge\u00b7bor\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.39": {"text": "Da\u00df ich wisse,", "tokens": ["Da\u00df", "ich", "wis\u00b7se", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.40": {"text": "Was war und was sein wird.", "tokens": ["Was", "war", "und", "was", "sein", "wird", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "KON", "PWS", "VAINF", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.41": {"text": "Einsam, einsam", "tokens": ["Ein\u00b7sam", ",", "ein\u00b7sam"], "token_info": ["word", "punct", "word"], "pos": ["ADJD", "$,", "ADJD"], "meter": "+-+-", "measure": "trochaic.di"}, "line.42": {"text": "Will ich wandeln und ziehen,", "tokens": ["Will", "ich", "wan\u00b7deln", "und", "zie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVFIN", "KON", "VVINF", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.43": {"text": "Ob fiebernde Brunst auch", "tokens": ["Ob", "fie\u00b7bern\u00b7de", "Brunst", "auch"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ADJA", "NN", "ADV"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.44": {"text": "Die Adern emporschwellt, \u2014", "tokens": ["Die", "A\u00b7dern", "em\u00b7por\u00b7schwellt", ","], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "$("], "meter": "-+---+", "measure": "dactylic.init"}, "line.45": {"text": "Doch eines verg\u00f6nn\u2019 mir,", "tokens": ["Doch", "ei\u00b7nes", "ver\u00b7g\u00f6nn'", "mir", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "PPER", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.46": {"text": "Allwaltende Weltmacht,", "tokens": ["All\u00b7wal\u00b7ten\u00b7de", "Welt\u00b7macht", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.47": {"text": "Jedes Wort, das ich schmiede,", "tokens": ["Je\u00b7des", "Wort", ",", "das", "ich", "schmie\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.48": {"text": "Es werde zum Glied,", "tokens": ["Es", "wer\u00b7de", "zum", "Glied", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPRART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.49": {"text": "Das die Menschheit verkettet,", "tokens": ["Das", "die", "Menschheit", "ver\u00b7ket\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.50": {"text": "Jedes Lied, das ich singe,", "tokens": ["Je\u00b7des", "Lied", ",", "das", "ich", "sin\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.51": {"text": "Wie Thau la\u00df es fallen", "tokens": ["Wie", "Thau", "la\u00df", "es", "fal\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "VVFIN", "PPER", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.52": {"text": "Auf die Herzen der Armen,", "tokens": ["Auf", "die", "Her\u00b7zen", "der", "Ar\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.53": {"text": "Der S\u00fcnder und Buhlen \u2014", "tokens": ["Der", "S\u00fcn\u00b7der", "und", "Buh\u00b7len"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.54": {"text": "Dann finde ich Frieden.", "tokens": ["Dann", "fin\u00b7de", "ich", "Frie\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}}}}