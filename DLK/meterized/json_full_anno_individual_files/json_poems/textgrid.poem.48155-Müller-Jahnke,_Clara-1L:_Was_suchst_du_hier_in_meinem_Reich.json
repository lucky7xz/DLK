{"textgrid.poem.48155": {"metadata": {"author": {"name": "M\u00fcller-Jahnke, Clara", "birth": "N.A.", "death": "N.A."}, "title": "1L: Was suchst du hier in meinem Reich", "genre": "verse", "period": "N.A.", "pub_year": 1882, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was suchst du hier in meinem Reich", "tokens": ["Was", "suchst", "du", "hier", "in", "mei\u00b7nem", "Reich"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "und w\u00fchlst in unerforschten Tiefen,", "tokens": ["und", "w\u00fchlst", "in", "un\u00b7er\u00b7forschten", "Tie\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "weckst meine Tr\u00e4ume, stumm und bleich,", "tokens": ["weckst", "mei\u00b7ne", "Tr\u00e4u\u00b7me", ",", "stumm", "und", "bleich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "ertrunken, die im Moore schliefen?", "tokens": ["er\u00b7trun\u00b7ken", ",", "die", "im", "Moo\u00b7re", "schlie\u00b7fen", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVINF", "$,", "PRELS", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Aus S\u00fcden kam ein hei\u00dfer Hauch:", "tokens": ["Aus", "S\u00fc\u00b7den", "kam", "ein", "hei\u00b7\u00dfer", "Hauch", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "des Abgrunds blasse Lilien blinken \u2013", "tokens": ["des", "Ab\u00b7grunds", "blas\u00b7se", "Li\u00b7li\u00b7en", "blin\u00b7ken", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "wenn sie dich locken, wirst du auch,", "tokens": ["wenn", "sie", "dich", "lo\u00b7cken", ",", "wirst", "du", "auch", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVINF", "$,", "VAFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "wie hundert andre, j\u00e4h versinken.", "tokens": ["wie", "hun\u00b7dert", "and\u00b7re", ",", "j\u00e4h", "ver\u00b7sin\u00b7ken", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOKOM", "CARD", "PIS", "$,", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Seit einst der Prinz von Samarkand", "tokens": ["Seit", "einst", "der", "Prinz", "von", "Sa\u00b7mar\u00b7kand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "ART", "NN", "APPR", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "sein Blut verspritzt zu meinen F\u00fc\u00dfen,", "tokens": ["sein", "Blut", "ver\u00b7spritzt", "zu", "mei\u00b7nen", "F\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "bin ich in ewiges Graun gebannt,", "tokens": ["bin", "ich", "in", "e\u00b7wi\u00b7ges", "Graun", "ge\u00b7bannt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "ADJA", "NN", "VVPP", "$,"],