penta"}, "line.55": {"text": "Doch horch! es rauscht in ungeduldgen Geigen!", "tokens": ["Doch", "horch", "!", "es", "rauscht", "in", "un\u00b7ge\u00b7duld\u00b7gen", "Gei\u00b7gen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "$.", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.56": {"text": "Das sind die Elfen selbst. Da mu\u00df der Redner schweigen.", "tokens": ["Das", "sind", "die", "El\u00b7fen", "selbst", ".", "Da", "mu\u00df", "der", "Red\u00b7ner", "schwei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "ADV", "$.", "ADV", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ihr seid versammelt hier und seid gespannt,", "tokens": ["Ihr", "seid", "ver\u00b7sam\u00b7melt", "hier", "und", "seid", "ge\u00b7spannt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "ADV", "KON", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ein Tonwerk anzuh\u00f6ren, weit bekannt,", "tokens": ["Ein", "Ton\u00b7werk", "an\u00b7zu\u00b7h\u00f6\u00b7ren", ",", "weit", "be\u00b7kannt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVIZU", "$,", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Das hoch und tief, wie heutzutag der Brauch,", "tokens": ["Das", "hoch", "und", "tief", ",", "wie", "heut\u00b7zu\u00b7tag", "der", "Brauch", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "KON", "ADJD", "$,", "PWAV", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "m