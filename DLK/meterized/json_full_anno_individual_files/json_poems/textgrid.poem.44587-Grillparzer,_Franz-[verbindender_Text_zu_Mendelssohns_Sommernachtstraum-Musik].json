er": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.23": {"text": "Allein das B\u00f6se, schwarz, in vollem Grimme,", "tokens": ["Al\u00b7lein", "das", "B\u00f6\u00b7se", ",", "schwarz", ",", "in", "vol\u00b7lem", "Grim\u00b7me", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "ADJD", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "Ist lange noch nicht alles Schief und Schlimme,", "tokens": ["Ist", "lan\u00b7ge", "noch", "nicht", "al\u00b7les", "Schief", "und", "Schlim\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "PTKNEG", "PIAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.25": {"text": "Die ", "tokens": ["Die"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.26": {"text": "Die halb von ihm und halb von Oben stammt.", "tokens": ["Die", "halb", "von", "ihm", "und", "halb", "von", "O\u00b7ben", "stammt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "PPER", "KON", "ADJD", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.27": {"text": "Ihr liebt, da ist die Huldin eine Fee,", "tokens": ["Ihr", "liebt", ",", "da", "ist", "die", "Hul\u00b7din", "ei\u00b7ne", "Fee", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADV", "VAFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.28": {"text": "Z\u00fcrnt ihr, steht euch ein Kobold in der N\u00e4h.", "tokens": ["Z\u00fcrnt", "ihr", ",", "steht", "euch", "ein", "Ko\u00b7bold", "in", "der", "N\u00e4h", "."], "token_info": ["word", "word", "punct", "word", "wo