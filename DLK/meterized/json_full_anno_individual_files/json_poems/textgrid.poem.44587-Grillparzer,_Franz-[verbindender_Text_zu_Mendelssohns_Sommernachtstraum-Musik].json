{"textgrid.poem.44587": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "[verbindender Text zu Mendelssohns Sommernachtstraum-Musik]", "genre": "verse", "period": "N.A.", "pub_year": 1852, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr seid versammelt hier und seid gespannt,", "tokens": ["Ihr", "seid", "ver\u00b7sam\u00b7melt", "hier", "und", "seid", "ge\u00b7spannt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "ADV", "KON", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ein Tonwerk anzuh\u00f6ren, weit bekannt,", "tokens": ["Ein", "Ton\u00b7werk", "an\u00b7zu\u00b7h\u00f6\u00b7ren", ",", "weit", "be\u00b7kannt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVIZU", "$,", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Das hoch und tief, wie heutzutag der Brauch,", "tokens": ["Das", "hoch", "und", "tief", ",", "wie", "heut\u00b7zu\u00b7tag", "der", "Brauch", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "KON", "ADJD", "$,", "PWAV", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Sich \u00fcbrall Platz gemacht und so bei uns denn auch.", "tokens": ["Sich", "\u00fcb\u00b7rall", "Platz", "ge\u00b7macht", "und", "so", "bei", "uns", "denn", "auch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "NN", "VVPP", "KON", "ADV", "APPR", "PPER", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ihr werdets h\u00f6ren jetzt und zwar im Reich der T\u00f6ne,", "tokens": ["Ihr", "wer\u00b7dets", "h\u00f6\u00b7ren", "jetzt", "und", "zwar", "im", "Reich", "der", "T\u00f6\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVINF", "ADV", "KON", "ADV", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So gut als irgendwo: wir sind noch Mozarts S\u00f6hne.", "tokens": ["So", "gut", "als", "ir\u00b7gend\u00b7wo", ":", "wir", "sind", "noch", "Mo\u00b7zarts", "S\u00f6h\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "ADV", "$.", "PPER", "VAFIN", "ADV", "NE", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Beethoven, Haydn, Meister edler Art,", "tokens": ["Beet\u00b7ho\u00b7ven", ",", "Haydn", ",", "Meis\u00b7ter", "ed\u00b7ler", "Art", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+---+-+-+", "measure": "dactylic.init"}, "line.8": {"text": "Sie wirken, obgleich schwach, noch in die Gegenwart.", "tokens": ["Sie", "wir\u00b7ken", ",", "ob\u00b7gleich", "schwach", ",", "noch", "in", "die", "Ge\u00b7gen\u00b7wart", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "ADJD", "$,", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Doch heut gen\u00fcgt das nicht, denn Mendelssohns Musik", "tokens": ["Doch", "heut", "ge\u00b7n\u00fcgt", "das", "nicht", ",", "denn", "Men\u00b7dels\u00b7sohns", "Mu\u00b7sik"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PDS", "PTKNEG", "$,", "KON", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Lehnt sich dramatisch an ein B\u00fchnenst\u00fcck,", "tokens": ["Lehnt", "sich", "dra\u00b7ma\u00b7tisch", "an", "ein", "B\u00fch\u00b7nen\u00b7st\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Das St\u00fcck nun k\u00f6nnen wir euch vor nicht f\u00fchren,", "tokens": ["Das", "St\u00fcck", "nun", "k\u00f6n\u00b7nen", "wir", "euch", "vor", "nicht", "f\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VMFIN", "PPER", "PRF", "APPR", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Deshalb ward ich gesandt, es euch zu explizieren.", "tokens": ["Des\u00b7halb", "ward", "ich", "ge\u00b7sandt", ",", "es", "euch", "zu", "ex\u00b7pli\u00b7zie\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "VVPP", "$,", "PPER", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Das f\u00e4llt mir schwer. Shakespeare ist Proteus-gleich:", "tokens": ["Das", "f\u00e4llt", "mir", "schwer", ".", "Sha\u00b7ke\u00b7spe\u00b7a\u00b7re", "ist", "Pro\u00b7teus\u00b7gleich", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADJD", "$.", "NE", "VAFIN", "ADJD", "$."], "meter": "-+-++--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.14": {"text": "Glaubt ihr zu halten ihn, so lacht er fern von euch.", "tokens": ["Glaubt", "ihr", "zu", "hal\u00b7ten", "ihn", ",", "so", "lacht", "er", "fern", "von", "euch", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKZU", "VVINF", "PPER", "$,", "ADV", "VVFIN", "PPER", "ADJD", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Doch mu\u00df es, so geschehs. Wir fassens mutig an,", "tokens": ["Doch", "mu\u00df", "es", ",", "so", "ge\u00b7schehs", ".", "Wir", "fas\u00b7sens", "mu\u00b7tig", "an", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "$,", "ADV", "ADJD", "$.", "PPER", "ADV", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Ein Schelm tut mehr, als er nun eben kann.", "tokens": ["Ein", "Schelm", "tut", "mehr", ",", "als", "er", "nun", "e\u00b7ben", "kann", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "$,", "KOUS", "PPER", "ADV", "ADV", "VMFIN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.17": {"text": "Doch zum Beginn und eh wir weiter gehen,", "tokens": ["Doch", "zum", "Be\u00b7ginn", "und", "eh", "wir", "wei\u00b7ter", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "KON", "KOUS", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Sagt mir: glaubt ihr an Elfen oder Feen?", "tokens": ["Sagt", "mir", ":", "glaubt", "ihr", "an", "El\u00b7fen", "o\u00b7der", "Feen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$.", "VVFIN", "PPER", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.19": {"text": "Glaubt ihr? dann gut; wenn aber nicht,", "tokens": ["Glaubt", "ihr", "?", "dann", "gut", ";", "wenn", "a\u00b7ber", "nicht", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$.", "ADV", "ADJD", "$.", "KOUS", "ADV", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Dann geht ihr fehl im Leben und Gedicht.", "tokens": ["Dann", "geht", "ihr", "fehl", "im", "Le\u00b7ben", "und", "Ge\u00b7dicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPRART", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Der Teufel ist der Vater alles B\u00f6sen,", "tokens": ["Der", "Teu\u00b7fel", "ist", "der", "Va\u00b7ter", "al\u00b7les", "B\u00f6\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "Wir beten drum, von ihm uns zu erl\u00f6sen.", "tokens": ["Wir", "be\u00b7ten", "drum", ",", "von", "ihm", "uns", "zu", "er\u00b7l\u00f6\u00b7sen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PAV", "$,", "APPR", "PPER", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.23": {"text": "Allein das B\u00f6se, schwarz, in vollem Grimme,", "tokens": ["Al\u00b7lein", "das", "B\u00f6\u00b7se", ",", "schwarz", ",", "in", "vol\u00b7lem", "Grim\u00b7me", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "ADJD", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "Ist lange noch nicht alles Schief und Schlimme,", "tokens": ["Ist", "lan\u00b7ge", "noch", "nicht", "al\u00b7les", "Schief", "und", "Schlim\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "PTKNEG", "PIAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.25": {"text": "Die ", "tokens": ["Die"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.26": {"text": "Die halb von ihm und halb von Oben stammt.", "tokens": ["Die", "halb", "von", "ihm", "und", "halb", "von", "O\u00b7ben", "stammt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "PPER", "KON", "ADJD", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.27": {"text": "Ihr liebt, da ist die Huldin eine Fee,", "tokens": ["Ihr", "liebt", ",", "da", "ist", "die", "Hul\u00b7din", "ei\u00b7ne", "Fee", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADV", "VAFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.28": {"text": "Z\u00fcrnt ihr, steht euch ein Kobold in der N\u00e4h.", "tokens": ["Z\u00fcrnt", "ihr", ",", "steht", "euch", "ein", "Ko\u00b7bold", "in", "der", "N\u00e4h", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "VVFIN", "PPER", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.29": {"text": "Ihr w\u00fcnscht, ihr hofft, ihr seid begeistert \u2013", "tokens": ["Ihr", "w\u00fcnscht", ",", "ihr", "hofft", ",", "ihr", "seid", "be\u00b7geis\u00b7tert", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "$,", "PPER", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.30": {"text": "Wie mans nun eben ist, nicht meisternd, nur bemeistert \u2013", "tokens": ["Wie", "mans", "nun", "e\u00b7ben", "ist", ",", "nicht", "meis\u00b7ternd", ",", "nur", "be\u00b7meis\u00b7tert", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ADV", "ADV", "VAFIN", "$,", "PTKNEG", "VVPP", "$,", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Da seid ihr denn, ich kann nicht helfen,", "tokens": ["Da", "seid", "ihr", "denn", ",", "ich", "kann", "nicht", "hel\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "$,", "PPER", "VMFIN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.32": {"text": "Besessen, nicht vom Teufel, doch von Elfen,", "tokens": ["Be\u00b7ses\u00b7sen", ",", "nicht", "vom", "Teu\u00b7fel", ",", "doch", "von", "El\u00b7fen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "PTKNEG", "APPRART", "NN", "$,", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.33": {"text": "Und da\u00df sies sind, zeigt schon das \u00f6de Nichts,", "tokens": ["Und", "da\u00df", "sies", "sind", ",", "zeigt", "schon", "das", "\u00f6\u00b7de", "Nichts", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VAFIN", "$,", "VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.34": {"text": "In das der Wahn zerrinnt beim ersten Strahl des Lichts.", "tokens": ["In", "das", "der", "Wahn", "zer\u00b7rinnt", "beim", "ers\u00b7ten", "Strahl", "des", "Lichts", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "ART", "NN", "VVFIN", "APPRART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Doch auch was sch\u00f6n und anmutsvoll im Leben,", "tokens": ["Doch", "auch", "was", "sch\u00f6n", "und", "an\u00b7muts\u00b7voll", "im", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PWS", "ADJD", "KON", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.36": {"text": "Ist diesen M\u00e4chten in die Hand gegeben,", "tokens": ["Ist", "die\u00b7sen", "M\u00e4ch\u00b7ten", "in", "die", "Hand", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDAT", "NN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.37": {"text": "Die Neigung, das Vertraun, die Feindesliebe,", "tokens": ["Die", "Nei\u00b7gung", ",", "das", "Ver\u00b7traun", ",", "die", "Fein\u00b7des\u00b7lie\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.38": {"text": "Was n\u00fctzlicher vielleicht, wenns unterbliebe,", "tokens": ["Was", "n\u00fctz\u00b7li\u00b7cher", "viel\u00b7leicht", ",", "wenns", "un\u00b7ter\u00b7blie\u00b7be", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "ADJD", "ADV", "$,", "KOUS", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.39": {"text": "Und doch, indems der Klugheit Bann entschl\u00fcpft,", "tokens": ["Und", "doch", ",", "in\u00b7dems", "der", "Klug\u00b7heit", "Bann", "ent\u00b7schl\u00fcpft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "ADV", "ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.40": {"text": "Die Bande zwischen Mensch und Menschen kn\u00fcpft:", "tokens": ["Die", "Ban\u00b7de", "zwi\u00b7schen", "Mensch", "und", "Men\u00b7schen", "kn\u00fcpft", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.41": {"text": "Des Dichters Lied, des Malers Meisterst\u00fcck,", "tokens": ["Des", "Dich\u00b7ters", "Lied", ",", "des", "Ma\u00b7lers", "Meis\u00b7ter\u00b7st\u00fcck", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.42": {"text": "Wenn ihr, erfa\u00dft vom Zauber der Musik,", "tokens": ["Wenn", "ihr", ",", "er\u00b7fa\u00dft", "vom", "Zau\u00b7ber", "der", "Mu\u00b7sik", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "VVFIN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.43": {"text": "Euch besser f\u00fchlt, und habt doch nichts getan,", "tokens": ["Euch", "bes\u00b7ser", "f\u00fchlt", ",", "und", "habt", "doch", "nichts", "ge\u00b7tan", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVFIN", "$,", "KON", "VAFIN", "ADV", "PIS", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.44": {"text": "Und reicher, obgleich keiner was gewann,", "tokens": ["Und", "rei\u00b7cher", ",", "ob\u00b7gleich", "kei\u00b7ner", "was", "ge\u00b7wann", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "p