 "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "PRELS", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.54": {"text": "Die Torheit, die der Weisheit sich verm\u00e4hlt.", "tokens": ["Die", "Tor\u00b7heit", ",", "die", "der", "Weis\u00b7heit", "sich", "ver\u00b7m\u00e4hlt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.55": {"text": "Doch horch! es rauscht in ungeduldgen Geigen!", "tokens": ["Doch", "horch", "!", "es", "rauscht", "in", "un\u00b7ge\u00b7duld\u00b7gen", "Gei\u00b7gen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "$.", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.56": {"text": "Das sind die Elfen selbst. Da mu\u00df der Redner schweigen.", "tokens": ["Das", "sind", "die", "El\u00b7fen", "selbst", ".", "Da", "mu\u00df", "der", "Red\u00b7ner", "schwei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "ADV", "$.", "ADV", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}