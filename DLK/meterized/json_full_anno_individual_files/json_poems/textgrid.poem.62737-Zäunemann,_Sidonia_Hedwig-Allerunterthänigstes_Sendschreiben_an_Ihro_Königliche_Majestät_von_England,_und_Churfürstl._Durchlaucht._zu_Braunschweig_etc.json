{"textgrid.poem.62737": {"metadata": {"author": {"name": "Z\u00e4unemann, Sidonia Hedwig", "birth": "N.A.", "death": "N.A."}, "title": "Allerunterth\u00e4nigstes Sendschreiben an Ihro K\u00f6nigliche Majest\u00e4t von England, und Churf\u00fcrstl. Durchlaucht. zu Braunschweig etc", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich schreibe jetzt \u2013 \u2013 doch nein!", "tokens": ["Ich", "schrei\u00b7be", "jetzt", "\u2013", "\u2013", "doch", "nein", "!"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$(", "$(", "ADV", "PTKANT", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Bey einem Scepter darf kein Kiel vom Schreiber seyn;", "tokens": ["Bey", "ei\u00b7nem", "Scep\u00b7ter", "darf", "kein", "Kiel", "vom", "Schrei\u00b7ber", "seyn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VMFIN", "PIAT", "NN", "APPRART", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wo eine Majest\u00e4t in Gold und Purpur thronet,", "tokens": ["Wo", "ei\u00b7ne", "Ma\u00b7jes\u00b7t\u00e4t", "in", "Gold", "und", "Pur\u00b7pur", "thro\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da ists kein solcher Ort, wo ein Poete wohnet.", "tokens": ["Da", "ists", "kein", "sol\u00b7cher", "Ort", ",", "wo", "ein", "Poe\u00b7te", "woh\u00b7net", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIAT", "PIAT", "NN", "$,", "PWAV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Legt man den Kieselstein zu einen Diamant?", "tokens": ["Legt", "man", "den", "Kie\u00b7sel\u00b7stein", "zu", "ei\u00b7nen", "Di\u00b7a\u00b7mant", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wie schickt sich also wohl dein hoher K\u00f6nigs-Stand", "tokens": ["Wie", "schickt", "sich", "al\u00b7so", "wohl", "dein", "ho\u00b7her", "K\u00f6\u00b7nigs\u00b7Stand"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PRF", "ADV", "ADV", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Zu meiner Niedrigkeit? so mu\u00df ich billig schweigen,", "tokens": ["Zu", "mei\u00b7ner", "Nied\u00b7rig\u00b7keit", "?", "so", "mu\u00df", "ich", "bil\u00b7lig", "schwei\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$.", "ADV", "VMFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und meine Ehrfurcht dir mit stillem Geist bezeugen.", "tokens": ["Und", "mei\u00b7ne", "Ehr\u00b7furcht", "dir", "mit", "stil\u00b7lem", "Geist", "be\u00b7zeu\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "PPER", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Doch nein! die Schuldigkeit ruft mich vom Helicon", "tokens": ["Doch", "nein", "!", "die", "Schul\u00b7dig\u00b7keit", "ruft", "mich", "vom", "He\u00b7li\u00b7con"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PTKANT", "$.", "ART", "NN", "VVFIN", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Die Dichter m\u00fcssen sich in stets darzu verpflichten,", "tokens": ["Die", "Dich\u00b7ter", "m\u00fcs\u00b7sen", "sich", "in", "stets", "dar\u00b7zu", "ver\u00b7pflich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PRF", "APPR", "ADV", "PAV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Den F\u00fcrsten dieser Erd ihr Opfer zu entrichten.", "tokens": ["Den", "F\u00fcrs\u00b7ten", "die\u00b7ser", "Erd", "ihr", "Op\u00b7fer", "zu", "ent\u00b7rich\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PDAT", "NN", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Nun diese schuldge Pflicht, ruft meine Niedrigkeit", "tokens": ["Nun", "die\u00b7se", "schuld\u00b7ge", "Pflicht", ",", "ruft", "mei\u00b7ne", "Nied\u00b7rig\u00b7keit"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "PDAT", "ADJA", "NN", "$,", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Da du der Musen-Schaar ein neues Haus geschenket,", "tokens": ["Da", "du", "der", "Mu\u00b7sen\u00b7Schaar", "ein", "neu\u00b7es", "Haus", "ge\u00b7schen\u00b7ket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Woran die ganze Welt stets mit Erstaunen denket,", "tokens": ["Wo\u00b7ran", "die", "gan\u00b7ze", "Welt", "stets", "mit", "Er\u00b7stau\u00b7nen", "den\u00b7ket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Vor dein gesalbtes Haupt. Doch geht bey jedem Schrit", "tokens": ["Vor", "dein", "ge\u00b7salb\u00b7tes", "Haupt", ".", "Doch", "geht", "bey", "je\u00b7dem", "Schrit"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$.", "KON", "VVFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Die Demuth und zugleich die schuldge Ehrfurcht mit.", "tokens": ["Die", "De\u00b7muth", "und", "zu\u00b7gleich", "die", "schuld\u00b7ge", "Ehr\u00b7furcht", "mit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ADV", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Dein neues Lein-Athen nach Schuldigkeit besungen;", "tokens": ["Dein", "neu\u00b7es", "Lein\u00b7A\u00b7then", "nach", "Schul\u00b7dig\u00b7keit", "be\u00b7sun\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.18": {"text": "Doch nicht nach W\u00fcrdigkeit; darzu bin ich zu mat,", "tokens": ["Doch", "nicht", "nach", "W\u00fcr\u00b7dig\u00b7keit", ";", "dar\u00b7zu", "bin", "ich", "zu", "mat", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "APPR", "NN", "$.", "PAV", "VAFIN", "PPER", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Das sch\u00f6nste, was mein Lied in seinen Zeilen hat,", "tokens": ["Das", "sch\u00f6ns\u00b7te", ",", "was", "mein", "Lied", "in", "sei\u00b7nen", "Zei\u00b7len", "hat", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "PRELS", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Ist ", "tokens": ["Ist"], "token_info": ["word"], "pos": ["VAFIN"], "meter": "+", "measure": "single.up"}, "line.21": {"text": "Und auf das herrlichste zu meinen Gl\u00fcck erheben.", "tokens": ["Und", "auf", "das", "herr\u00b7lichs\u00b7te", "zu", "mei\u00b7nen", "Gl\u00fcck", "er\u00b7he\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Ich wage mich damit vor dein geheiligt Knie.", "tokens": ["Ich", "wa\u00b7ge", "mich", "da\u00b7mit", "vor", "dein", "ge\u00b7hei\u00b7ligt", "Knie", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PAV", "APPR", "PPOSAT", "VVPP", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Den Abtrag meiner Pflicht, mit Gnadenvollen Blicken", "tokens": ["Den", "Ab\u00b7trag", "mei\u00b7ner", "Pflicht", ",", "mit", "Gna\u00b7den\u00b7vol\u00b7len", "Bli\u00b7cken"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "NN", "$,", "APPR", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Und holden Augen an, so wirst du mich erquicken.", "tokens": ["Und", "hol\u00b7den", "Au\u00b7gen", "an", ",", "so", "wirst", "du", "mich", "er\u00b7qui\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "PTKVZ", "$,", "ADV", "VAFIN", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Erh\u00f6re meine Bitt, die ich in Demuth thu.", "tokens": ["Er\u00b7h\u00f6\u00b7re", "mei\u00b7ne", "Bitt", ",", "die", "ich", "in", "De\u00b7muth", "thu", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "PRELS", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Die Sonne wirst den Strahl nicht nur den Eichen zu;", "tokens": ["Die", "Son\u00b7ne", "wirst", "den", "Strahl", "nicht", "nur", "den", "Ei\u00b7chen", "zu", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "PTKNEG", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Sie will den Hecken auch ihr Licht und Feuer g\u00f6nnen.", "tokens": ["Sie", "will", "den", "He\u00b7cken", "auch", "ihr", "Licht", "und", "Feu\u00b7er", "g\u00f6n\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "ADV", "PPOSAT", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "So sind Regenten auch, ihr Licht mu\u00df jeden brennen.", "tokens": ["So", "sind", "Re\u00b7gen\u00b7ten", "auch", ",", "ihr", "Licht", "mu\u00df", "je\u00b7den", "bren\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NN", "ADV", "$,", "PPOSAT", "NN", "VMFIN", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Sie sehen nicht allein den hohen Unterthan;", "tokens": ["Sie", "se\u00b7hen", "nicht", "al\u00b7lein", "den", "ho\u00b7hen", "Un\u00b7ter\u00b7than", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Nein auch den fremde Knecht mit Gnaden-Augen an.", "tokens": ["Nein", "auch", "den", "frem\u00b7de", "Knecht", "mit", "Gna\u00b7den\u00b7Au\u00b7gen", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADV", "ART", "ADJA", "NN", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "So wirf, erh\u00f6re mich, ", "tokens": ["So", "wirf", ",", "er\u00b7h\u00f6\u00b7re", "mich", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "VVFIN", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.32": {"text": "Auf mein so Ehrfurchtsvoll und unterth\u00e4nigst Bitten.", "tokens": ["Auf", "mein", "so", "Ehr\u00b7furchts\u00b7voll", "und", "un\u00b7ter\u00b7th\u00e4\u00b7nigst", "Bit\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADV", "ADJD", "KON", "VVFIN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Auch einen Gnadenstrahl auf mein geringes Blat.", "tokens": ["Auch", "ei\u00b7nen", "Gna\u00b7den\u00b7strahl", "auf", "mein", "ge\u00b7rin\u00b7ges", "Blat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Die Allmacht, die ihr Aug mit Seegen auf dich hat,", "tokens": ["Die", "All\u00b7macht", ",", "die", "ihr", "Aug", "mit", "See\u00b7gen", "auf", "dich", "hat", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "APPR", "NN", "APPR", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Erhalte deinen Thron und ", "tokens": ["Er\u00b7hal\u00b7te", "dei\u00b7nen", "Thron", "und"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "KON"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ich schreibe jetzt \u2013 \u2013 doch nein!", "tokens": ["Ich", "schrei\u00b7be", "jetzt", "\u2013", "\u2013", "doch", "nein", "!"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$(", "$(", "ADV", "PTKANT", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Bey einem Scepter darf kein Kiel vom Schreiber seyn;", "tokens": ["Bey", "ei\u00b7nem", "Scep\u00b7ter", "darf", "kein", "Kiel", "vom", "Schrei\u00b7ber", "seyn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VMFIN", "PIAT", "NN", "APPRART", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wo eine Majest\u00e4t in Gold und Purpur thronet,", "tokens": ["Wo", "ei\u00b7ne", "Ma\u00b7jes\u00b7t\u00e4t", "in", "Gold", "und", "Pur\u00b7pur", "thro\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da ists kein solcher Ort, wo ein Poete wohnet.", "tokens": ["Da", "ists", "kein", "sol\u00b7cher", "Ort", ",", "wo", "ein", "Poe\u00b7te", "woh\u00b7net", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIAT", "PIAT", "NN", "$,", "PWAV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Legt man den Kieselstein zu einen Diamant?", "tokens": ["Legt", "man", "den", "Kie\u00b7sel\u00b7stein", "zu", "ei\u00b7nen", "Di\u00b7a\u00b7mant", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wie schickt sich also wohl dein hoher K\u00f6nigs-Stand", "tokens": ["Wie", "schickt", "sich", "al\u00b7so", "wohl", "dein", "ho\u00b7her", "K\u00f6\u00b7nigs\u00b7Stand"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PRF", "ADV", "ADV", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Zu meiner Niedrigkeit? so mu\u00df ich billig schweigen,", "tokens": ["Zu", "mei\u00b7ner", "Nied\u00b7rig\u00b7keit", "?", "so", "mu\u00df", "ich", "bil\u00b7lig", "schwei\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$.", "ADV", "VMFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und meine Ehrfurcht dir mit stillem Geist bezeugen.", "tokens": ["Und", "mei\u00b7ne", "Ehr\u00b7furcht", "dir", "mit", "stil\u00b7lem", "Geist", "be\u00b7zeu\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "PPER", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": 