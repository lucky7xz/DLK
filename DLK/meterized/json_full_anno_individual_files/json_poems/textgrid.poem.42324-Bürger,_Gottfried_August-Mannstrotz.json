{"textgrid.poem.42324": {"metadata": {"author": {"name": "B\u00fcrger, Gottfried August", "birth": "N.A.", "death": "N.A."}, "title": "Mannstrotz", "genre": "verse", "period": "N.A.", "pub_year": 1770, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So lang' ein edler Biedermann", "tokens": ["So", "lang'", "ein", "ed\u00b7ler", "Bie\u00b7der\u00b7mann"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit einem Glied sein Brot verdienen kann,", "tokens": ["Mit", "ei\u00b7nem", "Glied", "sein", "Brot", "ver\u00b7die\u00b7nen", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "So lange sch\u00e4m' er sich nach Gnadenbrot zu lungern!", "tokens": ["So", "lan\u00b7ge", "sch\u00e4m'", "er", "sich", "nach", "Gna\u00b7den\u00b7brot", "zu", "lun\u00b7gern", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "PRF", "APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Doch thut ihm endlich keins mehr gut:", "tokens": ["Doch", "thut", "ihm", "end\u00b7lich", "keins", "mehr", "gut", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "PIAT", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So hab' er Stolz genug und Mut,", "tokens": ["So", "hab'", "er", "Stolz", "ge\u00b7nug", "und", "Mut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "ADV", "KON", "NN", "$,