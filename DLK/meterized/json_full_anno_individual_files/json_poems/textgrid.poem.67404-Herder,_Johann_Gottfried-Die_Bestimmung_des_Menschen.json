measure": "trochaic.tetra"}, "line.7": {"text": "\u00bbmeinem Liebling, wie wenn ich ihm schenkte", "tokens": ["\u00bb", "mei\u00b7nem", "Lieb\u00b7ling", ",", "wie", "wenn", "ich", "ihm", "schenk\u00b7te"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["$(", "PPOSAT", "NN", "$,", "KOKOM", "KOUS", "PPER", "PPER", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Aller meiner Kinder Lust und Schmerz?\u00ab", "tokens": ["Al\u00b7ler", "mei\u00b7ner", "Kin\u00b7der", "Lust", "und", "Schmerz", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "PPOSAT", "NN", "NN", "KON", "NN", "$.", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.11": {"line.1": {"text": "Und sie sann. Auf ", "tokens": ["Und", "sie", "sann", ".", "Auf"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["KON", "PPER", "VVFIN", "$.", "APPR"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Ward aus Allem Sympathie.", "tokens": ["Ward", "aus", "Al\u00b7lem", "Sym\u00b7pa\u00b7thie", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00bbferne,\u00ab sprach sie, \u00bbsei von ihm die Tr\u00e4ge;", "tokens": ["\u00bb", "fer\u00b7ne", ",", "\u00ab", "sprach", "sie", ",", "\u00bb", "sei", "von", "ihm", "die", "Tr\u00e4\u00b7ge", ";"], "token_info": ["punct", "word", "punct", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "$,", "$(", "VVFIN", "PPER", "$,", "$(", "VAFIN", "APPR", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Seine Lust sei ewig-s\u00fc\u00dfe M\u00fch!", "tokens": ["Sei\u00b7ne", "Lust", "sei", "e\u00b7wig\u00b7s\u00fc\u00b7\u00dfe", "M\u00fch", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Angeboren werd' ihm nichts; geboren", "tokens": ["An\u00b7ge\u00b7bo\u00b7ren", "werd'", "ihm", "nichts", ";", "ge\u00b7bo\u00b7ren"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["NN", "VAFIN", "PPER", "PIS", "$.", "VVPP"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Werd' in ihm ein ew'ger Trieb;", "tokens": ["Werd'", "in", "ihm", "ein", "ew'\u00b7ger", "Trieb", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPER", "ART", "ADJA", "NN", "$."], "meter": "----+-+", "measure": "unknown.measure.di"}, "line.7": {"text": "Und auch jedes Gl\u00fcck, durch Schuld verloren,", "tokens": ["Und", "auch", "je\u00b7des", "Gl\u00fcck", ",", "durch", "Schuld", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PIAT", "NN", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Werd' ihm tausendfach durch Reue lieb!", "tokens": ["Werd'", "ihm", "tau\u00b7send\u00b7fach", "durch", "Reu\u00b7e", "lieb", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVFIN", "APPR", "NN", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.12": {"line.1": {"text": "Nur in Andern sei sein Leben,", "tokens": ["Nur", "in", "An\u00b7dern", "sei", "sein", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wirksamkeit sein sch\u00f6nster Lohn!", "tokens": ["Wirk\u00b7sam\u00b7keit", "sein", "sch\u00f6ns\u00b7ter", "Lohn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Enkel, die ihm Dank und Ehre geben,", "tokens": ["En\u00b7kel", ",", "die", "ihm", "Dank", "und", "Eh\u00b7re", "ge\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPER", "NN", "KON", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Lohnen ihn f\u00fcr seiner Br\u00fcder Hohn.", "tokens": ["Loh\u00b7nen", "ihn", "f\u00fcr", "sei\u00b7ner", "Br\u00fc\u00b7der", "Hohn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "So vereint durch alle Folgezeiten", "tokens": ["So", "ver\u00b7eint", "durch", "al\u00b7le", "Fol\u00b7ge\u00b7zei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Strebe seine s\u00fc\u00dfe M\u00fch;", "tokens": ["Stre\u00b7be", "sei\u00b7ne", "s\u00fc\u00b7\u00dfe", "M\u00fch", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Neu gest\u00e4rkt durch Widerw\u00e4rtigkeiten,", "tokens": ["Neu", "ge\u00b7st\u00e4rkt", "durch", "Wi\u00b7der\u00b7w\u00e4r\u00b7tig\u00b7kei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "APPR", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Steige mehr und mehr umfassend sie!", "tokens": ["Stei\u00b7ge", "mehr", "und", "mehr", "um\u00b7fas\u00b7send", "sie", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "KON", "ADV", "VVPP", "PPER", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.13": {"line.1": {"text": "Auch im Kleinsten werd' ums Ganze", "tokens": ["Auch", "im", "Kleins\u00b7ten", "werd'", "ums", "Gan\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "VAFIN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ewig dies Geschlecht verdient;", "tokens": ["E\u00b7wig", "dies", "Ge\u00b7schlecht", "ver\u00b7dient", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PDS", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nur am Ziel, im sch\u00f6nsten Abendglanze,", "tokens": ["Nur", "am", "Ziel", ",", "im", "sch\u00f6ns\u00b7ten", "A\u00b7bend\u00b7glan\u00b7ze", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "H\u00e4ngt der Kranz, der f\u00fcr den Menschen gr\u00fcnt.", "tokens": ["H\u00e4ngt", "der", "Kranz", ",", "der", "f\u00fcr", "den", "Men\u00b7schen", "gr\u00fcnt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "F\u00fcr die Leidenden, die ihn umringen,", "tokens": ["F\u00fcr", "die", "Lei\u00b7den\u00b7den", ",", "die", "ihn", "um\u00b7rin\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Weih' ich ihn der Menschlichkeit,", "tokens": ["Wei