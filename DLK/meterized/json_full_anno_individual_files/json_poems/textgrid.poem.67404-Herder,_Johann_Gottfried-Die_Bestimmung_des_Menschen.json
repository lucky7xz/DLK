{"textgrid.poem.67404": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Die Bestimmung des Menschen", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als die K\u00f6nigin der Dinge,", "tokens": ["Als", "die", "K\u00f6\u00b7ni\u00b7gin", "der", "Din\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Reich an unersch\u00f6pftem Reiz,", "tokens": ["Reich", "an", "un\u00b7er\u00b7sch\u00f6pf\u00b7tem", "Reiz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wesen schuf, war nichts ihr zu geringe;", "tokens": ["We\u00b7sen", "schuf", ",", "war", "nichts", "ihr", "zu", "ge\u00b7rin\u00b7ge", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "VAFIN", "PIS", "PPER", "APPR", "ADJA", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Sie begabete mit mildem Geiz;", "tokens": ["Sie", "be\u00b7ga\u00b7be\u00b7te", "mit", "mil\u00b7dem", "Geiz", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Denn das F\u00fcllhorn aller Trefflichkeiten", "tokens": ["Denn", "das", "F\u00fcll\u00b7horn", "al\u00b7ler", "Treff\u00b7lich\u00b7kei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "PIAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "War in ihrer Mutterhand,", "tokens": ["War", "in", "ih\u00b7rer", "Mut\u00b7ter\u00b7hand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und sie paarte, was an Lieblichkeiten,", "tokens": ["Und", "sie", "paar\u00b7te", ",", "was", "an", "Lieb\u00b7lich\u00b7kei\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "PRELS", "APPR", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Wechselnd auch, zusammen je bestand.", "tokens": ["Wech\u00b7selnd", "auch", ",", "zu\u00b7sam\u00b7men", "je", "be\u00b7stand", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "$,", "ADV", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Einen Schmuck von tausend Farben", "tokens": ["Ei\u00b7nen", "Schmuck", "von", "tau\u00b7send", "Far\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "CARD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Webte sie um Florens Brust;", "tokens": ["Web\u00b7te", "sie", "um", "Flo\u00b7rens", "Brust", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NE", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Neu verj\u00fcnget, wenn die Schwestern starben,", "tokens": ["Neu", "ver\u00b7j\u00fcn\u00b7get", ",", "wenn", "die", "Schwes\u00b7tern", "star\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$,", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Treten Schwestern auf mit Siegeslust.", "tokens": ["Tre\u00b7ten", "Schwes\u00b7tern", "auf", "mit", "Sie\u00b7ges\u00b7lust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "APPR", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "In ein Chor von tausend s\u00fc\u00dfen Liedern", "tokens": ["In", "ein", "Chor", "von", "tau\u00b7send", "s\u00fc\u00b7\u00dfen", "Lie\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "CARD", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Theilte sich ihr m\u00e4cht'ger Klang,", "tokens": ["Theil\u00b7te", "sich", "ihr", "m\u00e4cht'\u00b7ger", "Klang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Der auf bunten schwebenden Gefiedern", "tokens": ["Der", "auf", "bun\u00b7ten", "schwe\u00b7ben\u00b7den", "Ge\u00b7fie\u00b7dern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ADJA", "ADJA", "NN"], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.8": {"text": "Disharmonisch-sch\u00f6n zum Himmel drang.", "tokens": ["Dis\u00b7har\u00b7mo\u00b7nischsch\u00f6n", "zum", "Him\u00b7mel", "drang", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "VVFIN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.3": {"line.1": {"text": "St\u00e4rke, Klugheit, sanfte Triebe,", "tokens": ["St\u00e4r\u00b7ke", ",", "Klug\u00b7heit", ",", "sanf\u00b7te", "Trie\u00b7be", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sch\u00f6nheit in jedweder Art,", "tokens": ["Sch\u00f6n\u00b7heit", "in", "jed\u00b7we\u00b7der", "Art", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PIAT", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Und in tausend der Gestalten Liebe", "tokens": ["Und", "in", "tau\u00b7send", "der", "Ge\u00b7stal\u00b7ten", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "CARD", "ART", "NN", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Ward umhergegossen ungespart.", "tokens": ["Ward", "um\u00b7her\u00b7ge\u00b7gos\u00b7sen", "un\u00b7ge\u00b7spart", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Endlich trat sie in sich selbst und senkte", "tokens": ["End\u00b7lich", "trat", "sie", "in", "sich", "selbst", "und", "senk\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PRF", "ADV", "KON", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Tief sich in ihr Mutterherz:", "tokens": ["Tief", "sich", "in", "ihr", "Mut\u00b7ter\u00b7herz", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PRF", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "\u00bbmeinem Liebling, wie wenn ich ihm schenkte", "tokens": ["\u00bb", "mei\u00b7nem", "Lieb\u00b7ling", ",", "wie", "wenn", "ich", "ihm", "schenk\u00b7te"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["$(", "PPOSAT", "NN", "$,", "KOKOM", "KOUS", "PPER", "PPER", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Aller meiner Kinder Lust und Schmerz?\u00ab", "tokens": ["Al\u00b7ler", "mei\u00b7ner", "Kin\u00b7der", "Lust", "und", "Schmerz", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "PPOSAT", "NN", "NN", "KON", "NN", "$.", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Und sie sann. Auf ", "tokens": ["Und", "sie", "sann", ".", "Auf"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["KON", "PPER", "VVFIN", "$.", "APPR"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Ward aus Allem Sympathie.", "tokens": ["Ward", "aus", "Al\u00b7lem", "Sym\u00b7pa\u00b7thie", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00bbferne,\u00ab sprach sie, \u00bbsei von ihm die Tr\u00e4ge;", "tokens": ["\u00bb", "fer\u00b7ne", ",", "\u00ab", "sprach", "sie", ",", "\u00bb", "sei", "von", "ihm", "die", "Tr\u00e4\u00b7ge", ";"], "token_info": ["punct", "word", "punct", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "$,", "$(", "VVFIN", "PPER", "$,", "$(", "VAFIN", "APPR", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Seine Lust sei ewig-s\u00fc\u00dfe M\u00fch!", "tokens": ["Sei\u00b7ne", "Lust", "sei", "e\u00b7wig\u00b7s\u00fc\u00b7\u00dfe", "M\u00fch", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Angeboren werd' ihm nichts; geboren", "tokens": ["An\u00b7ge\u00b7bo\u00b7ren", "werd'", "ihm", "nichts", ";", "ge\u00b7bo\u00b7ren"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["NN", "VAFIN", "PPER", "PIS", "$.", "VVPP"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Werd' in ihm ein ew'ger Trieb;", "tokens": ["Werd'", "in", "ihm", "ein", "ew'\u00b7ger", "Trieb", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPER", "ART", "ADJA", "NN", "$."], "meter": "----+-+", "measure": "unknown.measure.di"}, "line.7": {"text": "Und auch jedes Gl\u00fcck, durch Schuld verloren,", "tokens": ["Und", "auch", "je\u00b7des", "Gl\u00fcck", ",", "durch", "Schuld", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PIAT", "NN", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Werd' ihm tausendfach durch Reue lieb!", "tokens": ["Werd'", "ihm", "tau\u00b7send\u00b7fach", "durch", "Reu\u00b7e", "lieb", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVFIN", "APPR", "NN", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Nur in Andern sei sein Leben,", "tokens": ["Nur", "in", "An\u00b7dern", "sei", "sein", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wirksamkeit sein sch\u00f6nster Lohn!", "tokens": ["Wirk\u00b7sam\u00b7keit", "sein", "sch\u00f6ns\u00b7ter", "Lohn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Enkel, die ihm Dank und Ehre geben,", "tokens": ["En\u00b7kel", ",", "die", "ihm", "Dank", "und", "Eh\u00b7re", "ge\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPER", "NN", "KON", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Lohnen ihn f\u00fcr seiner Br\u00fcder Hohn.", "tokens": ["Loh\u00b7nen", "ihn", "f\u00fcr", "sei\u00b7ner", "Br\u00fc\u00b7der", "Hohn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "So vereint durch alle Folgezeiten", "tokens": ["So", "ver\u00b7eint", "durch", "al\u00b7le", "Fol\u00b7ge\u00b7zei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Strebe seine s\u00fc\u00dfe M\u00fch;", "tokens": ["Stre\u00b7be", "sei\u00b7ne", "s\u00fc\u00b7\u00dfe", "M\u00fch", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Neu gest\u00e4rkt durch Widerw\u00e4rtigkeiten,", "tokens": ["Neu", "ge\u00b7st\u00e4rkt", "durch", "Wi\u00b7der\u00b7w\u00e4r\u00b7tig\u00b7kei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "APPR", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Steige mehr und mehr umfassend sie!", "tokens": ["Stei\u00b7ge", "mehr", "und", "mehr", "um\u00b7fas\u00b7send", "sie", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "KON", "ADV", "VVPP", "PPER", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "Auch im Kleinsten werd' ums Ganze", "tokens": ["Auch", "im", "Kleins\u00b7ten", "werd'", "ums", "Gan\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "VAFIN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ewig dies Geschlecht verdient;", "tokens": ["E\u00b7wig", "dies", "Ge\u00b7schlecht", "ver\u00b7dient", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PDS", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nur am Ziel, im sch\u00f6nsten Abendglanze,", "tokens": ["Nur", "am", "Ziel", ",", "im", "sch\u00f6ns\u00b7ten", "A\u00b7bend\u00b7glan\u00b7ze", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "H\u00e4ngt der Kranz, der f\u00fcr den Menschen gr\u00fcnt.", "tokens": ["H\u00e4ngt", "der", "Kranz", ",", "der", "f\u00fcr", "den", "Men\u00b7schen", "gr\u00fcnt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "F\u00fcr die Leidenden, die ihn umringen,", "tokens": ["F\u00fcr", "die", "Lei\u00b7den\u00b7den", ",", "die", "ihn", "um\u00b7rin\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Weih' ich ihn der Menschlichkeit,", "tokens": ["Weih'", "ich", "ihn", "der", "Menschlich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Und sein Herz, wenn Seufzer auf ihn dringen,", "tokens": ["Und", "sein", "Herz", ",", "wenn", "Seuf\u00b7zer", "auf", "ihn", "drin\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "KOUS", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Zum Altare der Barmherzigkeit.\u00ab", "tokens": ["Zum", "Al\u00b7ta\u00b7re", "der", "Barm\u00b7her\u00b7zig\u00b7keit", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$.", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "Mutterk\u00f6nigin! das schw\u00e4chste Wesen,", "tokens": ["Mut\u00b7ter\u00b7k\u00f6\u00b7ni\u00b7gin", "!", "das", "schw\u00e4chs\u00b7te", "We\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Das man einzeln nur beweint,", "tokens": ["Das", "man", "ein\u00b7zeln", "nur", "be\u00b7weint", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "ADJD", "ADV", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hast Du Dir im Ganzen auserlesen", "tokens": ["Hast", "Du", "Dir", "im", "Gan\u00b7zen", "au\u00b7ser\u00b7le\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "PPER", "APPRART", "NN", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und gesammt durch Lieb' und Noth vereint.", "tokens": ["Und", "ge\u00b7sammt", "durch", "Lieb'", "und", "Noth", "ver\u00b7eint", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "APPR", "NN", "KON", "NN", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Deinen Sinn f\u00fcrs Gr\u00f6\u00dfere und Gr\u00f6\u00dfte", "tokens": ["Dei\u00b7nen", "Sinn", "f\u00fcrs", "Gr\u00f6\u00b7\u00dfe\u00b7re", "und", "Gr\u00f6\u00df\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "APPRART", "NN", "KON", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Und Dein Mutter