{"textgrid.poem.57707": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es steht eine Rose im Garten,", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es steht eine Rose im Garten,", "tokens": ["Es", "steht", "ei\u00b7ne", "Ro\u00b7se", "im", "Gar\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Ein rotes R\u00f6selein;", "tokens": ["Ein", "ro\u00b7tes", "R\u00f6\u00b7se\u00b7lein", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Allersch\u00f6nste im Dorfe,", "tokens": ["Die", "Al\u00b7ler\u00b7sch\u00f6ns\u00b7te", "im", "Dor\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "APPRART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Das ist die Liebste mein.", "tokens": ["Das", "ist", "die", "Liebs\u00b7te", "mein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "PPOSAT", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Vollmeier ist ihr Vater,", "tokens": ["Voll\u00b7mei\u00b7er", "ist", "ihr", "Va\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Im Dorfe der reichste Mann;", "tokens": ["Im", "Dor\u00b7fe", "der", "reichs\u00b7te", "Mann", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Geht er \u00fcber die Stra\u00dfe,", "tokens": ["Geht", "er", "\u00fc\u00b7ber", "die", "Stra\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Seinen Tritt man h\u00f6ren kann.", "tokens": ["Sei\u00b7nen", "Tritt", "man", "h\u00f6\u00b7ren", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PIS", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Ich pfeife auf seine Taler,", "tokens": ["Ich", "pfei\u00b7fe", "auf", "sei\u00b7ne", "Ta\u00b7ler", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Ich fl\u00f6te auf seinen Stolz,", "tokens": ["Ich", "fl\u00f6\u00b7te", "auf", "sei\u00b7nen", "Stolz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ich k\u00fcsse seine Dorette", "tokens": ["Ich", "k\u00fcs\u00b7se", "sei\u00b7ne", "Do\u00b7ret\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Jedweden Abend im Holz.", "tokens": ["Jed\u00b7we\u00b7den", "A\u00b7bend", "im", "Holz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPRART", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.4": {"line.1": {"text": "Und ist der Mond nicht helle", "tokens": ["Und", "ist", "der", "Mond", "nicht", "hel\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON