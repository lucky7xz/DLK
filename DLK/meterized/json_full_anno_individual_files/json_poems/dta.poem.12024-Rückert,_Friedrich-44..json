{"dta.poem.12024": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "44.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ob wirklich selber du ergreifst die Gegenst\u00e4nde,", "tokens": ["Ob", "wirk\u00b7lich", "sel\u00b7ber", "du", "er\u00b7greifst", "die", "Ge\u00b7gen\u00b7st\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "ADV", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es sei durch den Begriff, es sei durch deine H\u00e4nde;", "tokens": ["Es", "sei", "durch", "den", "Be\u00b7griff", ",", "es", "sei", "durch", "dei\u00b7ne", "H\u00e4n\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ART", "NN", "$,", "PPER", "VAFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ob ihren Eindruck nur, von ihnen einen Schein", "tokens": ["Ob", "ih\u00b7ren", "Ein\u00b7druck", "nur", ",", "von", "ih\u00b7nen", "ei\u00b7nen", "Schein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "$,", "APPR", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In H\u00e4nden habest, scheint unwichtig mir zu seyn.", "tokens": ["In", "H\u00e4n\u00b7den", "ha\u00b7best", ",", "scheint", "un\u00b7wich\u00b7tig", "mir", "zu", "seyn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "$,", "VVFIN", "ADJD", "PPER", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Du bist, die Dinge sind. Dir gn\u00fcge dies zu wissen,", "tokens": ["Du", "bist", ",", "die", "Din\u00b7ge", "sind", ".", "Dir", "gn\u00fc\u00b7ge", "dies", "zu", "wis\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "ART", "NN", "VAFIN", "$.", "PPER", "VVFIN", "PDS", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df und was dir sie sind; das andre magst du missen.", "tokens": ["Da\u00df", "und", "was", "dir", "sie", "sind", ";", "das", "and\u00b7re", "magst", "du", "mis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KON", "PWS", "PPER", "PPER", "VAFIN", "$.", "ART", "PIS", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Du machest deine Welt und deine Welt macht dich;", "tokens": ["Du", "ma\u00b7chest", "dei\u00b7ne", "Welt", "und", "dei\u00b7ne", "Welt", "macht", "dich", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie ihr einander macht, so seid ihr sicherlich.", "tokens": ["Wie", "ihr", "ein\u00b7an\u00b7der", "macht", ",", "so", "seid", "ihr", "si\u00b7cher\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "VVFIN", "$,", "ADV", "VAFIN", "PPER", "PRF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}