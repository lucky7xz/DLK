{"dta.poem.19590": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Der Pilger und die Sarazenin.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-200905193933", "language": ["de:0.99"], "booktitle": "Meyer, Conrad Ferdinand: Gedichte. Leipzig, 1882."}, "poem": {"stanza.1": {"line.1": {"text": "J\u00fcngst am Libanon in einem Kloster,", "tokens": ["J\u00fcngst", "am", "Li\u00b7ba\u00b7non", "in", "ei\u00b7nem", "Klos\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Drin ich eine kurze Reiserast hielt,", "tokens": ["Drin", "ich", "ei\u00b7ne", "kur\u00b7ze", "Rei\u00b7se\u00b7rast", "hielt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Langsam durch die k\u00fchlen Hallen wandelnd,", "tokens": ["Lang\u00b7sam", "durch", "die", "k\u00fch\u00b7len", "Hal\u00b7len", "wan\u00b7delnd", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Blieb ich stehn vor einem alten Bilde,", "tokens": ["Blieb", "ich", "stehn", "vor", "ei\u00b7nem", "al\u00b7ten", "Bil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Wohlbewahrt in eigener Capelle.", "tokens": ["Wohl\u00b7be\u00b7wahrt", "in", "ei\u00b7ge\u00b7ner", "Ca\u00b7pel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Es ber\u00fchrte mich mit leisem Zauber", "tokens": ["Es", "be\u00b7r\u00fchr\u00b7te", "mich", "mit", "lei\u00b7sem", "Zau\u00b7ber"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Trotz der byzantinischen Gestalten,", "tokens": ["Trotz", "der", "by\u00b7zan\u00b7ti\u00b7ni\u00b7schen", "Ge\u00b7stal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.8": {"text": "Denn dar\u00fcber lag ein Glanz der Liebe:", "tokens": ["Denn", "da\u00b7r\u00fc\u00b7ber", "lag", "ein", "Glanz", "der", "Lie\u00b7be", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Durch das Thor des Paradieses schritten", "tokens": ["Durch", "das", "Thor", "des", "Pa\u00b7ra\u00b7die\u00b7ses", "schrit\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "Eine Sarazenin und ein Pilger,", "tokens": ["Ei\u00b7ne", "Sa\u00b7ra\u00b7ze\u00b7nin", "und", "ein", "Pil\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.11": {"text": "Hand in Hand versenkt und Blick in Blick auch.", "tokens": ["Hand", "in", "Hand", "ver\u00b7senkt", "und", "Blick", "in", "Blick", "auch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "VVPP", "KON", "NN", "APPR", "NN", "ADV", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.12": {"text": "\u201ewas bedeutet dieses s\u00fc\u00dfe M\u00e4rchen?\u201c", "tokens": ["\u201e", "was", "be\u00b7deu\u00b7tet", "die\u00b7ses", "s\u00fc\u00b7\u00dfe", "M\u00e4r\u00b7chen", "?", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PWS", "VVFIN", "PDAT", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.13": {"text": "Frug ich Anaklet, den Klosterbruder,", "tokens": ["Frug", "ich", "An\u00b7ak\u00b7let", ",", "den", "Klos\u00b7ter\u00b7bru\u00b7der", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPER", "NE", "$,", "ART", "NN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.14": {"text": "Der mich schleichend \u00fcberall begleitet.", "tokens": ["Der", "mich", "schlei\u00b7chend", "\u00fc\u00b7be\u00b7rall", "be\u00b7glei\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "ADV", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.15": {"text": "Mit gesenkten Augen gab er Antwort:", "tokens": ["Mit", "ge\u00b7senk\u00b7ten", "Au\u00b7gen", "gab", "er", "Ant\u00b7wort", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.16": {"text": "\u201eguter Herr, kein s\u00fc\u00dfes M\u00e4rchen ist es,", "tokens": ["\u201e", "gu\u00b7ter", "Herr", ",", "kein", "s\u00fc\u00b7\u00dfes", "M\u00e4r\u00b7chen", "ist", "es", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJA", "NN", "$,", "PIAT", "ADJA", "NN", "VAFIN", "PPER", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.17": {"text": "Sondern eine tr\u00f6stliche Legende,", "tokens": ["Son\u00b7dern", "ei\u00b7ne", "tr\u00f6st\u00b7li\u00b7che", "Le\u00b7gen\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.18": {"text": "Auf ein altes Pergament verzeichnet", "tokens": ["Auf", "ein", "al\u00b7tes", "Per\u00b7ga\u00b7ment", "ver\u00b7zeich\u00b7net"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.19": {"text": "Zur Erbauung aller gl\u00e4ub'gen Christen.", "tokens": ["Zur", "Er\u00b7bau\u00b7ung", "al\u00b7ler", "gl\u00e4ub'\u00b7gen", "Chris\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PIAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.20": {"text": "Dieser Pilger ist ein heilger M\u00e4rtrer,", "tokens": ["Die\u00b7ser", "Pil\u00b7ger", "ist", "ein", "heil\u00b7ger", "M\u00e4r\u00b7trer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.21": {"text": "Eine M\u00e4rtrin ist die Sarazenin,", "tokens": ["Ei\u00b7ne", "M\u00e4r\u00b7trin", "ist", "die", "Sa\u00b7ra\u00b7ze\u00b7nin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.22": {"text": "Er verschied, gesteinigt und gepeinigt,", "tokens": ["Er", "ver\u00b7schied", ",", "ge\u00b7stei\u00b7nigt", "und", "ge\u00b7pei\u00b7nigt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVPP", "KON", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.23": {"text": "Sie verblich, umarmend eine Schwelle!\u201c", "tokens": ["Sie", "ver\u00b7blich", ",", "um\u00b7ar\u00b7mend", "ei\u00b7ne", "Schwel\u00b7le", "!", "\u201c"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "ADJD", "$,", "VVPP", "ART", "NN", "$.", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "M\u00e4rchenlustig bin ich wie Scheherban,", "tokens": ["M\u00e4r\u00b7chen\u00b7lus\u00b7tig", "bin", "ich", "wie", "Sche\u00b7her\u00b7ban", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "KOKOM", "NE", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Wie die plaudernde Scheherezade!", "tokens": ["Wie", "die", "plau\u00b7dern\u00b7de", "Sche\u00b7he\u00b7re\u00b7za\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Und ich bat den M\u00f6nch: \u201eErz\u00e4hle, Vater,", "tokens": ["Und", "ich", "bat", "den", "M\u00f6nch", ":", "\u201e", "Er\u00b7z\u00e4h\u00b7le", ",", "Va\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "$.", "$(", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Deinem Sohn die tr\u00f6stliche Legende.\u201c", "tokens": ["Dei\u00b7nem", "Sohn", "die", "tr\u00f6st\u00b7li\u00b7che", "Le\u00b7gen\u00b7de", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "ART", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Bruder Anaklet willfahrte sprechend:", "tokens": ["Bru\u00b7der", "An\u00b7ak\u00b7let", "will\u00b7fahr\u00b7te", "spre\u00b7chend", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "ADJA", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "\u201eeinst, vor ungez\u00e4hlten vielen Jahren \u2014", "tokens": ["\u201e", "einst", ",", "vor", "un\u00b7ge\u00b7z\u00e4hl\u00b7ten", "vie\u00b7len", "Jah\u00b7ren"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "$,", "APPR", "ADJA", "PIAT", "NN", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Also steht's im Pergament verzeichnet,", "tokens": ["Al\u00b7so", "steht's", "im", "Per\u00b7ga\u00b7ment", "ver\u00b7zeich\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Das ich gr\u00fcndlich lernte schon als Knabe \u2014", "tokens": ["Das", "ich", "gr\u00fcnd\u00b7lich", "lern\u00b7te", "schon", "als", "Kna\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ADJD", "VVFIN", "ADV", "KOUS", "NN", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Zogen Pilger nach dem Grab vor\u00fcber", "tokens": ["Zo\u00b7gen", "Pil\u00b7ger", "nach", "dem", "Grab", "vor\u00b7\u00fc\u00b7ber"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "APPR", "ART", "NN", "PTKVZ"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Ohne Rast und ohne Trunk und Speise", "tokens": ["Oh\u00b7ne", "Rast", "und", "oh\u00b7ne", "Trunk", "und", "Spei\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Scheuen Fu\u00dfes an der Stadt Damaskus,", "tokens": ["Scheu\u00b7en", "Fu\u00b7\u00dfes", "an", "der", "Stadt", "Da\u00b7mas\u00b7kus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "NN", "NE", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Denn verha\u00dft ist Christus in Damaskus!", "tokens": ["Denn", "ver\u00b7ha\u00dft", "ist", "Chris\u00b7tus", "in", "Da\u00b7mas\u00b7kus", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "NE", "APPR", "NE", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Vor der Stadt Damaskus rauscht ein Brunnen,", "tokens": ["Vor", "der", "Stadt", "Da\u00b7mas\u00b7kus", "rauscht", "ein", "Brun\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NE", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "Wo ein L\u00f6wenkopf aus seines Maules", "tokens": ["Wo", "ein", "L\u00f6\u00b7wen\u00b7kopf", "aus", "sei\u00b7nes", "Mau\u00b7les"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "Tief herabgezognen Winkeln sprudelt", "tokens": ["Tief", "her\u00b7ab\u00b7ge\u00b7zog\u00b7nen", "Win\u00b7keln", "spru\u00b7delt"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.11": {"text": "Ein begehrtes k\u00f6stlich k\u00fchles Wasser.", "tokens": ["Ein", "be\u00b7gehr\u00b7tes", "k\u00f6st\u00b7lich", "k\u00fch\u00b7les", "Was\u00b7ser", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJD", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.12": {"text": "Dort am Brunnen stand die Sarazenin.", "tokens": ["Dort", "am", "Brun\u00b7nen", "stand", "die", "Sa\u00b7ra\u00b7ze\u00b7nin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Schleierlos, die jungen warmen Augen", "tokens": ["Schlei\u00b7er\u00b7los", ",", "die", "jun\u00b7gen", "war\u00b7men", "Au\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "$,", "ART", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "F\u00fcnfzehnj\u00e4hrig oder sechszehnj\u00e4hrig,", "tokens": ["F\u00fcnf\u00b7zehn\u00b7j\u00e4h\u00b7rig", "o\u00b7der", "sechs\u00b7zehn\u00b7j\u00e4h\u00b7rig", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Stand am Brunnen eine Sarazenin,", "tokens": ["Stand", "am", "Brun\u00b7nen", "ei\u00b7ne", "Sa\u00b7ra\u00b7ze\u00b7nin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Die den schlanken Krug gelassen f\u00fcllte.", "tokens": ["Die", "den", "schlan\u00b7ken", "Krug", "ge\u00b7las\u00b7sen", "f\u00fcll\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "VVPP", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Alle Pilger zogen ihr vor\u00fcber", "tokens": ["Al\u00b7le", "Pil\u00b7ger", "zo\u00b7gen", "ihr", "vor\u00b7\u00fc\u00b7ber"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "PTKVZ"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Mit gesenktem Haupte niederblickend,", "tokens": ["Mit", "ge\u00b7senk\u00b7tem", "Haup\u00b7te", "nie\u00b7der\u00b7bli\u00b7ckend", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Denn die Moslimweiber treiben K\u00fcnste.", "tokens": ["Denn", "die", "Mos\u00b7lim\u00b7wei\u00b7ber", "trei\u00b7ben", "K\u00fcns\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "(aber \u00fcberwunden hat sie Christus!)", "tokens": ["(", "a\u00b7ber", "\u00fc\u00b7berw\u00b7un\u00b7den", "hat", "sie", "Chris\u00b7tus", "!", ")"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "VVPP", "VAFIN", "PPER", "NE", "$.", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Nur ein zarter J\u00fcngling, fast ein Knabe", "tokens": ["Nur", "ein", "zar\u00b7ter", "J\u00fcng\u00b7ling", ",", "fast", "ein", "Kna\u00b7be"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN", "$,", "ADV", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Noch, entwich der Pilgerreihe durstig,", "tokens": ["Noch", ",", "ent\u00b7wich", "der", "Pil\u00b7ger\u00b7rei\u00b7he", "durs\u00b7tig", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADJD", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Nahte sich der jungen Sarazenin", "tokens": ["Nah\u00b7te", "sich", "der", "jun\u00b7gen", "Sa\u00b7ra\u00b7ze\u00b7nin"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "ART", "ADJA"