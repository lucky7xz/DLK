{"textgrid.poem.57592": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "1L: Hoch oben auf dem Lebensfelsen,", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hoch oben auf dem Lebensfelsen,", "tokens": ["Hoch", "o\u00b7ben", "auf", "dem", "Le\u00b7bens\u00b7fel\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Noch viele Jahresreisen weit,", "tokens": ["Noch", "vie\u00b7le", "Jah\u00b7res\u00b7rei\u00b7sen", "weit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da bl\u00fcht die unbekannte Blume,", "tokens": ["Da", "bl\u00fcht", "die", "un\u00b7be\u00b7kann\u00b7te", "Blu\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der ich mein Leben hab' geweiht", "tokens": ["Der", "ich", "mein", "Le\u00b7ben", "hab'", "ge\u00b7weiht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "PPOSAT", "NN", "VAFIN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Der Weg ist steil, es brennt die Sonne,", "tokens": ["Der", "Weg", "ist", "steil", ",", "es", "brennt", "die", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Es schmerzt das Knie, der Fu\u00df ist wund,", "tokens": ["Es", "schmerzt", "das", "Knie", ",", "der", "Fu\u00df", "ist", "wund", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Kein Quell, nur wenig bunte Bl\u00fcten,", "tokens": ["Kein", "Quell", ",", "nur", "we\u00b7nig", "bun\u00b7te", "Bl\u00fc\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "ADV", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nach Labung lechzt der trockne Mund.", "tokens": ["Nach", "La\u00b7bung", "lechzt", "der", "trock\u00b7ne", "Mund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ich b\u00fcck' mich nieder zu den Bl\u00fcten", "tokens": ["Ich", "b\u00fcck", "mich", "nie\u00b7der", "zu", "den", "Bl\u00fc\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und st\u00e4rke mich mit Tau und Duft \u2013", "tokens": ["Und", "st\u00e4r\u00b7ke", "mich", "mit", "Tau", "und", "Duft", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Deswegen sollst du mir nicht z\u00fcrnen,", "tokens": ["Des\u00b7we\u00b7gen", "sollst", "du", "mir", "nicht", "z\u00fcr\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-