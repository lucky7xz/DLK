{"dta.poem.8931": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "LxIV.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Wer hat die H\u00e4lfte nicht davon verloren", "tokens": ["Wer", "hat", "die", "H\u00e4lf\u00b7te", "nicht", "da\u00b7von", "ver\u00b7lo\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "ART", "NN", "PTKNEG", "PAV", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Im Traum, im Fieber, im Gespr\u00e4ch mit Thoren,", "tokens": ["Im", "Traum", ",", "im", "Fie\u00b7ber", ",", "im", "Ge\u00b7spr\u00e4ch", "mit", "Tho\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPRART", "NN", "$,", "APPRART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "In Liebesqual, im leeren Zeitverprassen?", "tokens": ["In", "Lie\u00b7bes\u00b7qual", ",", "im", "lee\u00b7ren", "Zeit\u00b7ver\u00b7pras\u00b7sen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ja, der sogar, der ruhig und gelassen,", "tokens": ["Ja", ",", "der", "so\u00b7gar", ",", "der", "ru\u00b7hig", "und", "ge\u00b7las\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PRELS", "ADV", "$,", "PRELS", "ADJD", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit dem Bewu\u00dftseyn, was er soll, geboren,", "tokens": ["Mit", "dem", "Be\u00b7wu\u00df\u00b7tseyn", ",", "was", "er", "soll", ",", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PWS", "PPER", "VMFIN", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Fr\u00fchzeitig einen Lebensgang erkoren,", "tokens": ["Fr\u00fch\u00b7zei\u00b7tig", "ei\u00b7nen", "Le\u00b7bens\u00b7gang", "er\u00b7ko\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "VVINF", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Mu\u00df vor des Lebens Widerspruch erblassen.", "tokens": ["Mu\u00df", "vor", "des", "Le\u00b7bens", "Wi\u00b7der\u00b7spruch", "er\u00b7blas\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Denn Jeder hofft doch, da\u00df das Gl\u00fcck ihm lache,", "tokens": ["Denn", "Je\u00b7der", "hofft", "doch", ",", "da\u00df", "das", "Gl\u00fcck", "ihm", "la\u00b7che", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "ADV", "$,", "KOUS", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Allein das Gl\u00fcck, wenn's wirklich kommt, ertragen,", "tokens": ["Al\u00b7lein", "das", "Gl\u00fcck", ",", "wenn's", "wirk\u00b7lich", "kommt", ",", "er\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "KOUS", "ADJD", "VVFIN", "$,", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ist keines Menschen, w\u00e4re Gottes Sache.", "tokens": ["Ist", "kei\u00b7nes", "Men\u00b7schen", ",", "w\u00e4\u00b7re", "Got\u00b7tes", "Sa\u00b7che", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "$,", "VAFIN", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Auch kommt es nie, wir w\u00fcnschen blos und wagen:", "tokens": ["Auch", "kommt", "es", "nie", ",", "wir", "w\u00fcn\u00b7schen", "blos", "und", "wa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "PPER", "VVFIN", "ADV", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dem Schl\u00e4fer f\u00e4llt es nimmermehr vom Dache,", "tokens": ["Dem", "Schl\u00e4\u00b7fer", "f\u00e4llt", "es", "nim\u00b7mer\u00b7mehr", "vom", "Da\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und auch der L\u00e4ufer wird es nicht erjagen.", "tokens": ["Und", "auch", "der", "L\u00e4u\u00b7fer", "wird", "es", "nicht", "er\u00b7ja\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VAFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}