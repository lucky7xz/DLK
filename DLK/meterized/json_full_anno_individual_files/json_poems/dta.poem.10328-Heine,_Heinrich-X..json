{"dta.poem.10328": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "X.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Da hab' ich viel blasse Leichen", "tokens": ["Da", "hab'", "ich", "viel", "blas\u00b7se", "Lei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PIAT", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Beschworen mit Wortesmacht;", "tokens": ["Be\u00b7schwo\u00b7ren", "mit", "Wor\u00b7tes\u00b7macht", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Die wollen nun nicht mehr weichen", "tokens": ["Die", "wol\u00b7len", "nun", "nicht", "mehr", "wei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VMFIN", "ADV", "PTKNEG", "ADV", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Zur\u00fcck in die alte Nacht.", "tokens": ["Zu\u00b7r\u00fcck", "in", "die", "al\u00b7te", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Das z\u00e4hmende Spr\u00fcchlein vom Meister", "tokens": ["Das", "z\u00e4h\u00b7men\u00b7de", "Spr\u00fcch\u00b7lein", "vom", "Meis\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Verga\u00df ich vor Schauer und Graus,", "tokens": ["Ver\u00b7ga\u00df", "ich", "vor", "Schau\u00b7er", "und", "Graus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NE", "KON", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Nun zieh'n die eig'nen Geister", "tokens": ["Nun", "zieh'n", "die", "eig'\u00b7nen", "Geis\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mich selber in's neblichte Haus.", "tokens": ["Mich", "sel\u00b7ber", "in's", "neb\u00b7lich\u00b7te", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.3": {"line.1": {"text": "La\u00dft ab, ihr finstren D\u00e4monen!", "tokens": ["La\u00dft", "ab", ",", "ihr", "finst\u00b7ren", "D\u00e4\u00b7mo\u00b7nen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "La\u00dft ab, und dr\u00e4ngt mich nicht!", "tokens": ["La\u00dft", "ab", ",", "und", "dr\u00e4ngt", "mich", "nicht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "$,", "KON", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Noch manche Freude mag wohnen", "tokens": ["Noch", "man\u00b7che", "Freu\u00b7de", "mag", "woh\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PIAT", "NN", "VMFIN", "VVINF"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Hier oben im Rosenlicht.", "tokens": ["Hier", "o\u00b7ben", "im", "Ro\u00b7sen\u00b7licht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPRART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Ich mu\u00df ja immer streben", "tokens": ["Ich", "mu\u00df", "ja", "im\u00b7mer", "stre\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Nach der Blume wunderhold;", "tokens": ["Nach", "der", "Blu\u00b7me", "wun\u00b7der\u00b7hold", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Was bedeutet' mein ganzes Leben,", "tokens": ["Was", "be\u00b7deu\u00b7tet'", "mein", "gan\u00b7zes", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Wenn ich Sie nicht lieben gesollt?", "tokens": ["Wenn", "ich", "Sie", "nicht", "lie\u00b7ben", "ge\u00b7sollt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "PTKNEG", "VVINF", "VVPP", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.5": {"line.1": {"text": "Ich m\u00f6cht sie nur einmal umfangen,", "tokens": ["Ich", "m\u00f6cht", "sie", "nur", "ein\u00b7mal", "um\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+---+-", "measure": "unknown.measure.tri"}, "line.2": {"text": "Und pressen an's gl\u00fchende Herz!", "tokens": ["Und", "pres\u00b7sen", "an's", "gl\u00fc\u00b7hen\u00b7de", "Herz", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Nur einmal die Lippen und Wangen", "tokens": ["Nur", "ein\u00b7mal", "die", "Lip\u00b7pen", "und", "Wan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "NN", "KON", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "K\u00fcssen mit sel'gem Schmerz.", "tokens": ["K\u00fcs\u00b7sen", "mit", "sel'\u00b7gem", "Schmerz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.6": {"line.1": {"text": "Nur einmal aus ihrem Munde", "tokens": ["Nur", "ein\u00b7mal", "aus", "ih\u00b7rem", "Mun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "M\u00f6cht' ich h\u00f6ren ein liebendes Wort, \u2014", "tokens": ["M\u00f6cht'", "ich", "h\u00f6\u00b7ren", "ein", "lie\u00b7ben\u00b7des", "Wort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VMFIN", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,", "$("], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Alsdann wollt' ich folgen zur Stunde", "tokens": ["Als\u00b7dann", "wollt'", "ich", "fol\u00b7gen", "zur", "Stun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "VVFIN", "APPRART", "NN"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Euch, Geister, zum finstern Ort.", "tokens": ["Euch", ",", "Geis\u00b7ter", ",", "zum", "fins\u00b7tern", "Ort", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "$,", "APPRART", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.7": {"line.1": {"text": "Die Geister haben's vernommen,", "tokens": ["Die", "Geis\u00b7ter", "ha\u00b7ben's", "ver\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und nicken grausiglich.", "tokens": ["Und", "ni\u00b7cken", "grau\u00b7sig\u00b7lich", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Feins Liebchen, nun bin ich gekommen;", "tokens": ["Feins", "Lieb\u00b7chen", ",", "nun", "bin", "ich", "ge\u00b7kom\u00b7men", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "ADV", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Feins Liebchen, liebst du mich?", "tokens": ["Feins", "Lieb\u00b7chen", ",", "liebst", "du", "mich", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "VVFIN", "PPER", "PRF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}