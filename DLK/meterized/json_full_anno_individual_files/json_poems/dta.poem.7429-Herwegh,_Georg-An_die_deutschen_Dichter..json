{"dta.poem.7429": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "An die deutschen Dichter.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192430", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Seid stolz! es klingt kein Gold der Welt,", "tokens": ["Seid", "stolz", "!", "es", "klingt", "kein", "Gold", "der", "Welt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "$.", "PPER", "VVFIN", "PIAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie Eurer Saiten Gold;", "tokens": ["Wie", "Eu\u00b7rer", "Sai\u00b7ten", "Gold", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es ist kein F\u00fcrst so hoch gestellt,", "tokens": ["Es", "ist", "kein", "F\u00fcrst", "so", "hoch", "ge\u00b7stellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df Ihr ihm dienen sollt!", "tokens": ["Da\u00df", "Ihr", "ihm", "die\u00b7nen", "sollt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Trotz Erz und Marmor st\u00fcrb' er doch,", "tokens": ["Trotz", "Erz", "und", "Mar\u00b7mor", "st\u00fcrb'", "er", "doch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wenn Ihr ihn sterben lie\u00dfet;", "tokens": ["Wenn", "Ihr", "ihn", "ster\u00b7ben", "lie\u00b7\u00dfet", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Der sch\u00f6nste Purpur ist annoch", "tokens": ["Der", "sch\u00f6ns\u00b7te", "Pur\u00b7pur", "ist", "an\u00b7noch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Das Blut, das Ihr als Lied vergie\u00dfet!", "tokens": ["Das", "Blut", ",", "das", "Ihr", "als", "Lied", "ver\u00b7gie\u00b7\u00dfet", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "KOUS", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Der Ruhm der Herrscher wird verweht \u2014", "tokens": ["Der", "Ruhm", "der", "Herr\u00b7scher", "wird", "ver\u00b7weht"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VAFIN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Lobpreis' ihn, wer da will!", "tokens": ["Lob\u00b7preis'", "ihn", ",", "wer", "da", "will", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "PWS", "ADV", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Man jagt und spornt ihn, doch er steht", "tokens": ["Man", "jagt", "und", "spornt", "ihn", ",", "doch", "er", "steht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PIS", "VVFIN", "KON", "VVFIN", "PPER", "$,", "KON", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit ihrem Herzen still.", "tokens": ["Mit", "ih\u00b7rem", "Her\u00b7zen", "still", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "O la\u00dft sie donnern fort und fort!", "tokens": ["O", "la\u00dft", "sie", "don\u00b7nern", "fort", "und", "fort", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADV", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "An ihrem Grab verhallt es.", "tokens": ["An", "ih\u00b7rem", "Grab", "ver\u00b7hallt", "es", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ihr Dichter, sprecht Ein grollend Wort,", "tokens": ["Ihr", "Dich\u00b7ter", ",", "sprecht", "Ein", "grol\u00b7lend", "Wort", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und zu dem ew'gen Gotte schallt es!", "tokens": ["Und", "zu", "dem", "ew'\u00b7gen", "Got\u00b7te", "schallt", "es", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Es hat dem Vogel in dem Nest", "tokens": ["Es", "hat", "dem", "Vo\u00b7gel", "in", "dem", "Nest"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Himmel nie gewankt;", "tokens": ["Der", "Him\u00b7mel", "nie", "ge\u00b7wankt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Den M\u00e4chtigen d\u00fcnkt er nur fest,", "tokens": ["Den", "M\u00e4ch\u00b7ti\u00b7gen", "d\u00fcnkt", "er", "nur", "fest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "PTKVZ", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "So lang' der Thron nicht schwankt!", "tokens": ["So", "lang'", "der", "Thron", "nicht", "schwankt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Palast und Purpur hin und her,", "tokens": ["Pa\u00b7last", "und", "Pur\u00b7pur", "hin", "und", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ob Glanz sie \u00fcbersch\u00fctte \u2014", "tokens": ["Ob", "Glanz", "sie", "\u00fc\u00b7ber\u00b7sch\u00fct\u00b7te"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPER", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Seid stolz, seid stolz, Ihr seid ja mehr;", "tokens": ["Seid", "stolz", ",", "seid", "stolz", ",", "Ihr", "seid", "ja", "mehr", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "$,", "VAFIN", "ADJD", "$,", "PPER", "VAFIN", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Seid Ihr nicht K\u00f6nige der H\u00fctte?", "tokens": ["Seid", "Ihr", "nicht", "K\u00f6\u00b7ni\u00b7ge", "der", "H\u00fct\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "PPER", "PTKNEG", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Blitzt ewig nicht der Thau im Feld,", "tokens": ["Blitzt", "e\u00b7wig", "nicht", "der", "Thau", "im", "Feld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PTKNEG", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gleich wie der Diamant?", "tokens": ["Gleich", "wie", "der", "Di\u00b7a\u00b7mant", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ist nicht ob dieser ganzen Welt", "tokens": ["Ist", "nicht", "ob", "die\u00b7ser", "gan\u00b7zen", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PTKNEG", "KOUS", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Baldachin gespannt?", "tokens": ["Ein", "Bal\u00b7da\u00b7ch\u00b7in", "ge\u00b7spannt", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Wiegt nicht die Rebe, die hinauf", "tokens": ["Wiegt", "nicht", "die", "Re\u00b7be", ",", "die", "hin\u00b7auf"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PTKNEG", "ART", "NN", "$,", "PRELS", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "An einem Strohdach gleitet,", "tokens": ["An", "ei\u00b7nem", "Stroh\u00b7dach", "glei\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Den unfruchtbaren Epheu auf,", "tokens": ["Den", "un\u00b7frucht\u00b7ba\u00b7ren", "E\u00b7pheu", "auf", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der sich um Zwingherrnburgen breitet?", "tokens": ["Der", "sich", "um", "Zwing\u00b7herrn\u00b7bur\u00b7gen", "brei\u00b7tet", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Hoch, S\u00e4nger, schlage Euer Herz,", "tokens": ["Hoch", ",", "S\u00e4n\u00b7ger", ",", "schla\u00b7ge", "Eu\u00b7er", "Herz", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "NN", "$,", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie Lerchen in der Luft!", "tokens": ["Wie", "Ler\u00b7chen", "in", "der", "Luft", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es ruht sich besser allerw\u00e4rts,", "tokens": ["Es", "ruht", "sich", "bes\u00b7ser", "al\u00b7ler\u00b7w\u00e4rts", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADJD", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als in der F\u00fcrstengruft.", "tokens": ["Als", "in", "der", "F\u00fcrs\u00b7ten\u00b7gruft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ein Liebchen, das die Treue bricht,", "tokens": ["Ein", "Lieb\u00b7chen", ",", "das", "die", "Treu\u00b7e", "bricht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ist \u00fcberall zu finden;", "tokens": ["Ist", "\u00fc\u00b7be\u00b7rall", "zu", "fin\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Verschm\u00e4het mir die Ringe nicht,", "tokens": ["Ver\u00b7schm\u00e4\u00b7het", "mir", "die", "Rin\u00b7ge", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Doch la\u00dft Euch nie an Ketten binden!", "tokens": ["Doch", "la\u00dft", "Euch", "nie", "an", "Ket\u00b7ten", "bin\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "ADV", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Dem Volke nur seid zugethan,", "tokens": ["Dem", "Vol\u00b7ke", "nur", "seid", "zu\u00b7ge\u00b7than", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VAFIN", "ADJD", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Jauchzt ihm voran zur Schlacht,", "tokens": ["Jauchzt", "ihm", "vo\u00b7ran", "zur", "Schlacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und liegt's verwundet auf dem Plan,", "tokens": ["Und", "liegt's", "ver\u00b7wun\u00b7det", "auf", "dem", "Plan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So pfleget sein und wacht!", "tokens": ["So", "pfle\u00b7get", "sein", "und", "wacht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "VAINF", "KON", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und so man ihm den letzten Rest", "tokens": ["Und", "so", "man", "ihm", "den", "letz\u00b7ten", "Rest"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "PIS", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der Freiheit will verk\u00fcmmern,", "tokens": ["Der", "Frei\u00b7heit", "will", "ver\u00b7k\u00fcm\u00b7mern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "So haltet nur am Schwerte fest,", "tokens": ["So", "hal\u00b7tet", "nur", "am", "Schwer\u00b7te", "fest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und la\u00dft die Harfen uns zertr\u00fcmmern!", "tokens": ["Und", "la\u00dft", "die", "Har\u00b7fen", "uns", "zer\u00b7tr\u00fcm\u00b7mern", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "ART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}