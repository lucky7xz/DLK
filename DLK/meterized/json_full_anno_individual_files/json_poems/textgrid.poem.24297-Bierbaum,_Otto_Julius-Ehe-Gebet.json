{"textgrid.poem.24297": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Ehe-Gebet", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gib mir deine Hand: ich k\u00fcsse sie.", "tokens": ["Gib", "mir", "dei\u00b7ne", "Hand", ":", "ich", "k\u00fcs\u00b7se", "sie", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPOSAT", "NN", "$.", "PPER", "VVFIN", "PPER", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Schenk mir deinen Blick: ich f\u00fchle ihn.", "tokens": ["Schenk", "mir", "dei\u00b7nen", "Blick", ":", "ich", "f\u00fch\u00b7le", "ihn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPOSAT", "NN", "$.", "PPER", "VVFIN", "PPER", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Gib mir deine Wahrheit, Frau, die ganze,", "tokens": ["Gib", "mir", "dei\u00b7ne", "Wahr\u00b7heit", ",", "Frau", ",", "die", "gan\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPOSAT", "NN", "$,", "NN", "$,", "ART", "ADJA", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Bis zur letzten Nacktheit gib sie mir: ich heilige sie.", "tokens": ["Bis", "zur", "letz\u00b7ten", "Nackt\u00b7heit", "gib", "sie", "mir", ":", "ich", "hei\u00b7li\u00b7ge", "sie", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "APPRART", "ADJA", "NN", "VVIMP", "PPER", "PPER", "$.", "PPER", "VVFIN", "PPER", "$."], "meter": "+-+-+-+-+-+--+", "measure": "iambic.septa.chol"}, "line.5": {"text": "Denn ich mu\u00df G\u00f6ttliches in meinem Herzen haben und wieder einen Glauben,", "tokens": ["Denn", "ich", "mu\u00df", "G\u00f6tt\u00b7li\u00b7ches", "in", "mei\u00b7nem", "Her\u00b7zen", "ha\u00b7ben", "und", "wie\u00b7der", "ei\u00b7nen", "Glau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "NN", "APPR", "PPOSAT", "NN", "VAFIN", "KON", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+--+-+-+-", "measure": "iambic.octa.plus.relaxed"}, "line.6": {"text": "Ein Heiligtum mu\u00df ich haben und einen Altar,", "tokens": ["Ein", "Hei\u00b7lig\u00b7tum", "mu\u00df", "ich", "ha\u00b7ben", "und", "ei\u00b7nen", "Al\u00b7tar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "VAFIN", "KON", "ART", "NN", "$,"], "meter": "-+-+--+--+--+", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Kr\u00e4nze darauf zu legen, Weihrauch meiner Seele,", "tokens": ["Kr\u00e4n\u00b7ze", "da\u00b7rauf", "zu", "le\u00b7gen", ",", "Weih\u00b7rauch", "mei\u00b7ner", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PAV", "PTKZU", "VVINF", "$,", "NN", "PPOSAT", "NN", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.8": {"text": "Und mein Herz selber als Flamme, die sich gerne verzehrt.", "tokens": ["Und", "mein", "Herz", "sel\u00b7ber", "als", "Flam\u00b7me", ",", "die", "sich", "ger\u00b7ne", "ver\u00b7zehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADV", "KOUS", "NN", "$,", "PRELS", "PRF", "ADV", "VVPP", "$."], "meter": "--++--+-+-+--+", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Gib mir deine Hand: ich k\u00fcsse sie.", "tokens": ["Gib", "mir", "dei\u00b7ne", "Hand", ":", "ich", "k\u00fcs\u