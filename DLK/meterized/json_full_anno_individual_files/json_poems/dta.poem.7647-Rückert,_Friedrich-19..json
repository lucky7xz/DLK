{"dta.poem.7647": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "19.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Von Lob und Tadel h\u00e4ngt mitnichten ab dein Adel,", "tokens": ["Von", "Lob", "und", "Ta\u00b7del", "h\u00e4ngt", "mit\u00b7nich\u00b7ten", "ab", "dein", "A\u00b7del", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch ehr als halbes Lob w\u00fcnsch' ich dir ganzen Tadel.", "tokens": ["Doch", "ehr", "als", "hal\u00b7bes", "Lob", "w\u00fcn\u00b7sch'", "ich", "dir", "gan\u00b7zen", "Ta\u00b7del", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KOUS", "ADJA", "NN", "VVFIN", "PPER", "PPER", "ADJA", "NN", "$."], "meter": "-+-+----+-+-+-", "measure": "unknown.measure.penta"}}, "stanza.2": {"line.1": {"text": "Der Tadel spornet dich, den du gerecht erachtest,", "tokens": ["Der", "Ta\u00b7del", "spor\u00b7net", "dich", ",", "den", "du", "ge\u00b7recht", "e\u00b7rach\u00b7test", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,", "PRELS", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und ungerechter kr\u00e4nkt dich nicht, den du verachtest.", "tokens": ["Und", "un\u00b7ge\u00b7rech\u00b7ter", "kr\u00e4nkt", "dich", "nicht", ",", "den", "du", "ver\u00b7ach\u00b7test", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "PTKNEG", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Doch kahles Lob, wie zur Abspeisung nur bestimmt,", "tokens": ["Doch", "kah\u00b7les", "Lob", ",", "wie", "zur", "Ab\u00b7spei\u00b7sung", "nur", "be\u00b7stimmt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,", "PWAV", "APPRART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+--++-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Ein Brocken ists, womit vorlieb ein Bettler nimmt.", "tokens": ["Ein", "Bro\u00b7cken", "ists", ",", "wo\u00b7mit", "vor\u00b7lieb", "ein", "Bett\u00b7ler", "nimmt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$,", "PWAV", "VVFIN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}