{"dta.poem.12648": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auf ihre Sch\u00f6nheit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wje! soll ich, Sch\u00f6nes kind! dich einen menschen nennen?", "tokens": ["Wie", "!", "soll", "ich", ",", "Sch\u00f6\u00b7nes", "kind", "!", "dich", "ei\u00b7nen", "men\u00b7schen", "nen\u00b7nen", "?"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "VMFIN", "PPER", "$,", "ADJA", "NN", "$.", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dich ziert des himmels schmuck; nicht falsche pralerey;", "tokens": ["Dich", "ziert", "des", "him\u00b7mels", "schmuck", ";", "nicht", "fal\u00b7sche", "pra\u00b7le\u00b7rey", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJD", "$.", "PTKNEG", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Dein holder tugend-glantz heift endlich mich bekennen,", "tokens": ["Dein", "hol\u00b7der", "tu\u00b7gen\u00b7dglantz", "heift", "end\u00b7lich", "mich", "be\u00b7ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "ADV", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da\u00df, Edles kind! bey dir, was mehr als irrdisch sey.", "tokens": ["Da\u00df", ",", "Ed\u00b7les", "kind", "!", "bey", "dir", ",", "was", "mehr", "als", "irr\u00b7disch", "sey", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "ADJA", "NN", "$.", "APPR", "PPER", "$,", "PRELS", "PIS", "KOKOM", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der g\u00f6tter angesicht hat dich gantz eingenommen,", "tokens": ["Der", "g\u00f6t\u00b7ter", "an\u00b7ge\u00b7sicht", "hat", "dich", "gantz", "ein\u00b7ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "In deiner brust zeigt sich des himmels hoher schein,", "tokens": ["In", "dei\u00b7ner", "brust", "zeigt", "sich", "des", "him\u00b7mels", "ho\u00b7her", "schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PRF", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Du bist entweder nur zu uns vom himmel kommen;", "tokens": ["Du", "bist", "ent\u00b7we\u00b7der", "nur", "zu", "uns", "vom", "him\u00b7mel", "kom\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "APPR", "PPER", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wo nicht? so mu\u00df allhier der g\u00f6tter wohnung seyn.", "tokens": ["Wo", "nicht", "?", "so", "mu\u00df", "all\u00b7hier", "der", "g\u00f6t\u00b7ter", "woh\u00b7nung", "seyn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PTKNEG", "$.", "ADV", "VMFIN", "ADV", "ART", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}