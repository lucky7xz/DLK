{"textgrid.poem.56018": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "Die Bettler", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du wu\u00dftest nicht, was den Haufen", "tokens": ["Du", "wu\u00df\u00b7test", "nicht", ",", "was", "den", "Hau\u00b7fen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PRELS", "ART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "ausmacht. Ein Fremder fand", "tokens": ["aus\u00b7macht", ".", "Ein", "Frem\u00b7der", "fand"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$.", "ART", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Bettler darin. Sie verkaufen", "tokens": ["Bett\u00b7ler", "da\u00b7rin", ".", "Sie", "ver\u00b7kau\u00b7fen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NN", "PAV", "$.", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "das Hohle aus ihrer Hand.", "tokens": ["das", "Hoh\u00b7le", "aus", "ih\u00b7rer", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Sie zeigen dem Hergereisten", "tokens": ["Sie", "zei\u00b7gen", "dem", "Her\u00b7ge\u00b7reis\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "ihren Mund voll Mist,", "tokens": ["ih\u00b7ren", "Mund", "voll", "Mist", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "und er darf (er kann es sich leisten)", "tokens": ["und", "er", "darf", "(", "er", "kann", "es", "sich", "leis\u00b7ten", ")"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "$(", "PPER", "VMFIN", "PPER", "PRF", "VVINF", "$("], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "sehn, wie ihr Aussatz fri\u00dft.", "tokens": ["sehn", ",", "wie", "ihr", "Aus\u00b7satz", "fri\u00dft", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVINF", "$,", "PWAV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.3": {"line.1": {"text": "Es zergeht in ihren zerr\u00fchrten", "tokens": ["Es", "zer\u00b7geht", "in", "ih\u00b7ren", "zer\u00b7r\u00fchr\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "ADJA"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Augen sein fremdes Gesicht;", "tokens": ["Au\u00b7gen", "sein", "frem\u00b7des", "Ge\u00b7sicht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "und sie freuen sich des Verf\u00fchrten", "tokens": ["und", "sie", "freu\u00b7en", "sich", "des", "Ver\u00b7f\u00fchr\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PRF", "ART", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "und speien, wenn er spricht.", "tokens": ["und", "spei\u00b7en", ",", "wenn", "er", "spricht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Du wu\u00dftest nicht, was den Hau