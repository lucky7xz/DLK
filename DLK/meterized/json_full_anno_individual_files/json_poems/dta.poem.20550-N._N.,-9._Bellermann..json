{"dta.poem.20550": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "9.  \n  Bellermann.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1848", "urn": "urn:nbn:de:kobv:b4-25129-1", "language": ["de:0.99"], "booktitle": "[N. N.]: Erinnerungen eines freiwilligen reitenden J\u00e4gers aus den Kriegsjahren 1813\u20131815. Berlin, 1848."}, "poem": {"stanza.1": {"line.1": {"text": "Ein ber&#252;hmter Schuldirektor", "tokens": ["Ein", "ber", "&#252;", "hm\u00b7ter", "Schul\u00b7di\u00b7rek\u00b7tor"], "token_info": ["word", "word", "XML_entity", "word", "word"], "pos": ["ART", "NE", "$(", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Rief zusammen den Prorektor,", "tokens": ["Rief", "zu\u00b7sam\u00b7men", "den", "Pro\u00b7rek\u00b7tor", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Subrektor und Conrektor;", "tokens": ["Sub\u00b7rek\u00b7tor", "und", "Con\u00b7rek\u00b7tor", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Trug den dreien Patriarchen,", "tokens": ["Trug", "den", "drei\u00b7en", "Pat\u00b7ri\u00b7ar\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "CARD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Zugesellt ihm dem Monarchen,", "tokens": ["Zu\u00b7ge\u00b7sellt", "ihm", "dem", "Mon\u00b7ar\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Haude-Speners Zeitung vor.", "tokens": ["Hau\u00b7de\u00b7S\u00b7pe\u00b7ners", "Zei\u00b7tung", "vor", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "In selb\u2019ger Zeitung stand geschrieben:", "tokens": ["In", "selb'\u00b7ger", "Zei\u00b7tung", "stand", "ge\u00b7schrie\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Alle, welche Ehre lieben,", "tokens": ["Al\u00b7le", ",", "wel\u00b7che", "Eh\u00b7re", "lie\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "PWAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sollten kommen frisch und froh.", "tokens": ["Soll\u00b7ten", "kom\u00b7men", "frisch", "und", "froh", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVINF", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Die es treu und redlich meinen,", "tokens": ["Die", "es", "treu", "und", "red\u00b7lich", "mei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "KON", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Sollten schnell zum Kampf erscheinen,", "tokens": ["Soll\u00b7ten", "schnell", "zum", "Kampf", "er\u00b7schei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADJD", "APPRART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Unser K\u00f6nig woll\u2019 es so!", "tokens": ["Un\u00b7ser", "K\u00f6\u00b7nig", "woll'", "es", "so", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Primus omnium Primanorum", "tokens": ["Pri\u00b7mus", "om\u00b7ni\u00b7um", "Pri\u00b7ma\u00b7no\u00b7rum"], "token_info": ["word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Gegen alles Schuldecorum", "tokens": ["Ge\u00b7gen", "al\u00b7les", "Schul\u00b7de\u00b7co\u00b7rum"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PIAT", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Jauchzte: Rektor, ich geh\u2019 mit! \u2014", "tokens": ["Jauchz\u00b7te", ":", "Rek\u00b7tor", ",", "ich", "geh'", "mit", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["NN", "$.", "NN", "$,", "PPER", "VVFIN", "PTKVZ", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Rektor sprach: Man lern\u2019 noch flei\u00dfig!", "tokens": ["Rek\u00b7tor", "sprach", ":", "Man", "lern'", "noch", "flei\u00b7\u00dfig", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$.", "PIS", "VVFIN", "ADV", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Primus rief: Schon Alles wei\u00df ich!", "tokens": ["Pri\u00b7mus", "rief", ":", "Schon", "Al\u00b7les", "wei\u00df", "ich", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$.", "ADV", "PIS", "VVFIN", "PPER", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wandelt fort. \u2014 Stolz war sein Schritt.", "tokens": ["Wan\u00b7delt", "fort", ".", "Stolz", "war", "sein", "Schritt", "."], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$.", "$(", "NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.4": {"line.1": {"text": "Rektor sinnt: ", "tokens": ["Rek\u00b7tor", "sinnt", ":"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Par, (Cornelius Nepos fatur,)", "tokens": ["Par", ",", "(", "Cor\u00b7ne\u00b7li\u00b7us", "Ne\u00b7pos", "fa\u00b7tur", ",", ")"], "token_info": ["word", "punct", "punct", "word", "word", "word", "punct", "punct"], "pos": ["NE", "$,", "$(", "FM.la", "FM.la", "FM.la", "$,", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Apud Graecos: Marathon!", "tokens": ["A\u00b7pud", "Grae\u00b7cos", ":", "Ma\u00b7ra\u00b7thon", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["FM.la", "FM.la", "$.", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Volksgeist w\u00fcrd\u2019 es ", "tokens": ["Volks\u00b7geist", "w\u00fcrd'", "es"], "token_info": ["word", "word", "word"], "pos": ["NN", "VAFIN", "PPER"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Der schlug ", "tokens": ["Der", "schlug"], "token_info": ["word", "word"], "pos": ["PDS", "VVFIN"], "meter": "-+", "measure": "iambic.single"}, "line.6": {"text": "Sicher auch ", "tokens": ["Si\u00b7cher", "auch"], "token_info": ["word", "word"], "pos": ["ADV", "ADV"], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.5": {"line.1": {"text": "Bellermann that sich gew\u00f6hnen,", "tokens": ["Bel\u00b7ler\u00b7mann", "that", "sich", "ge\u00b7w\u00f6h\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und in zwei geliebten S\u00f6hnen", "tokens": ["Und", "in", "zwei", "ge\u00b7lieb\u00b7ten", "S\u00f6h\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "CARD", "ADJA", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.3": {"text": "Zeigt\u2019 sich, wie er\u2019s meinte, klar.", "tokens": ["Zeigt'", "sich", ",", "wie", "er's", "mein\u00b7te", ",", "klar", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PRF", "$,", "PWAV", "PIS", "VVFIN", "$,", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Trotz Erynnien, trotz Parzen", "tokens": ["Trotz", "E\u00b7ryn\u00b7ni\u00b7en", ",", "trotz", "Par\u00b7zen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "$,", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "K\u00e4mpfte bald bei ", "tokens": ["K\u00e4mpf\u00b7te", "bald", "bei"], "token_info": ["word", "word", "word"], "pos": ["NN", "ADV", "APPR"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Das getreue Br\u00fcderpaar.", "tokens": ["Das", "ge\u00b7treu\u00b7e", "Br\u00fc\u00b7der\u00b7paar", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}