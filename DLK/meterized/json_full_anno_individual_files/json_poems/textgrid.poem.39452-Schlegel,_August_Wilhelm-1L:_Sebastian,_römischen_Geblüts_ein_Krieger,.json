{"textgrid.poem.39452": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sebastian, r\u00f6mischen Gebl\u00fcts ein Krieger,", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sebastian, r\u00f6mischen Gebl\u00fcts ein Krieger,", "tokens": ["Se\u00b7bas\u00b7ti\u00b7an", ",", "r\u00f6\u00b7mi\u00b7schen", "Ge\u00b7bl\u00fcts", "ein", "Krie\u00b7ger", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Schwur zu den Fahnen, die unsterblich lohnen.", "tokens": ["Schwur", "zu", "den", "Fah\u00b7nen", ",", "die", "uns\u00b7terb\u00b7lich", "loh\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,", "PRELS", "PRF", "VVINF", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Den M\u00e4rtyrern wies er die lichten Kronen,", "tokens": ["Den", "M\u00e4r\u00b7ty\u00b7rern", "wies", "er", "die", "lich\u00b7ten", "Kro\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Und mancher ward, von ihm ermuthigt, Sieger.", "tokens": ["Und", "man\u00b7cher", "ward", ",", "von", "ihm", "er\u00b7mut\u00b7higt", ",", "Sie\u00b7ger", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "$,", "APPR", "PPER", "VVFIN", "$,", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Der Imperator h\u00f6rt's ergrimmt. Betrieger!", "tokens": ["Der", "Im\u00b7pe\u00b7ra\u00b7tor", "h\u00f6rt's", "er\u00b7grimmt", ".", "Be\u00b7trie\u00b7ger", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "VVPP", "$.", "NN", "$."], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "So willst du mir und unsern G\u00f6ttern lohnen?", "tokens": ["So", "willst", "du", "mir", "und", "un\u00b7sern", "G\u00f6t\u00b7tern", "loh\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPER", "KON", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ergreift ihn augenblicklich, Centurionen!", "tokens": ["Er\u00b7greift", "ihn", "au\u00b7gen\u00b7blick\u00b7lich", ",", "Cen\u00b7tu\u00b7ri\u00b7o\u00b7nen", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "$,", "NN", "$."], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Als Wurfziel seiner eignen Schaar erlieg' er.", "tokens": ["Als", "Wurf\u00b7ziel", "sei\u00b7ner", "eig\u00b7nen", "Schaar", "er\u00b7lieg'", "er", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPOSAT", "ADJA", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Vom Pferd geri\u00dfen, aller Waffenzierde", "tokens": ["Vom", "Pferd", "ge\u00b7ri\u00b7\u00dfen", ",", "al\u00b7ler", "Waf\u00b7fen\u00b7zier\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPRART", "NN", "VVPP", "$,", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Entkleidet, steht er still dem Kampf entgegen,", "tokens": ["Ent\u00b7klei\u00b7det", ",", "steht", "er", "still", "dem", "Kampf", "ent\u00b7ge\u00b7gen", ","]