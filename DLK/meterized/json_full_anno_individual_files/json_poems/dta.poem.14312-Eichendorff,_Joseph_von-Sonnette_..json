{"dta.poem.14312": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Sonnette .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Ich schweif' umsonst, so weit der Erde Runde,", "tokens": ["Ich", "schweif'", "um\u00b7sonst", ",", "so", "weit", "der", "Er\u00b7de", "Run\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "ADV", "ADJD", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und stieg' ich hoch auch \u00fcber alle Kl\u00fcfte,", "tokens": ["Und", "stieg'", "ich", "hoch", "auch", "\u00fc\u00b7ber", "al\u00b7le", "Kl\u00fcf\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "ADV", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dein Haus ist h\u00f6her noch als diese L\u00fcfte,", "tokens": ["Dein", "Haus", "ist", "h\u00f6\u00b7her", "noch", "als", "die\u00b7se", "L\u00fcf\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "ADV", "KOUS", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da reicht kein Laut hin, noch zur\u00fcck zum Grunde.", "tokens": ["Da", "reicht", "kein", "Laut", "hin", ",", "noch", "zu\u00b7r\u00fcck", "zum", "Grun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "APPR", "PTKVZ", "$,", "ADV", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}