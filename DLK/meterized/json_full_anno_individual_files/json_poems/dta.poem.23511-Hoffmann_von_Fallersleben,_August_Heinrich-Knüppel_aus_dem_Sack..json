{"dta.poem.23511": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Kn\u00fcppel aus dem Sack.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1840", "urn": "urn:nbn:de:kobv:b4-200905192626", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Von allen W\u00fcnschen in der Welt", "tokens": ["Von", "al\u00b7len", "W\u00fcn\u00b7schen", "in", "der", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nur Einer mir anjetzt gef\u00e4llt,", "tokens": ["Nur", "Ei\u00b7ner", "mir", "an\u00b7jetzt", "ge\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "PPER", "ADV", "VVPP", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Nur: Kn\u00fcppel aus dem Sack!", "tokens": ["Nur", ":", "Kn\u00fcp\u00b7pel", "aus", "dem", "Sack", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und g\u00e4be Gott mir Wunschesmacht,", "tokens": ["Und", "g\u00e4\u00b7be", "Gott", "mir", "Wun\u00b7sches\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "PPER", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich d\u00e4chte nur bei Tag und Nacht,", "tokens": ["Ich", "d\u00e4ch\u00b7te", "nur", "bei", "Tag", "und", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Nur: Kn\u00fcppel aus dem Sack!", "tokens": ["Nur", ":", "Kn\u00fcp\u00b7pel", "aus", "dem", "Sack", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Dann braucht' ich weder Gut noch Gold,", "tokens": ["Dann", "braucht'", "ich", "we\u00b7der", "Gut", "noch", "Gold", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "KON", "ADJD", "ADV", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich machte mir die Welt schon hold", "tokens": ["Ich", "mach\u00b7te", "mir", "die", "Welt", "schon", "hold"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit: Kn\u00fcppel aus dem Sack!", "tokens": ["Mit", ":", "Kn\u00fcp\u00b7pel", "aus", "dem", "Sack", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$.", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ich w\u00e4r' ein Sieger, w\u00e4r' ein Held,", "tokens": ["Ich", "w\u00e4r'", "ein", "Sie\u00b7ger", ",", "w\u00e4r'", "ein", "Held", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der erst' und beste Mann der Welt", "tokens": ["Der", "er\u00b7st'", "und", "bes\u00b7te", "Mann", "der", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "KON", "ADJA", "NN", "ART", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Mit: Kn\u00fcppel aus dem Sack!", "tokens": ["Mit", ":", "Kn\u00fcp\u00b7pel", "aus", "dem", "Sack", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$.", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ich schaffte Freiheit, Recht und Ruh", "tokens": ["Ich", "schaff\u00b7te", "Frei\u00b7heit", ",", "Recht", "und", "Ruh"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "ADJA", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und frohes Leben noch dazu", "tokens": ["Und", "fro\u00b7hes", "Le\u00b7ben", "noch", "da\u00b7zu"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "ADV", "PAV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Beim: Kn\u00fcppel aus dem Sack!", "tokens": ["Beim", ":", "Kn\u00fcp\u00b7pel", "aus", "dem", "Sack", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "$.", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und wollt' ich selbst recht lustig sein,", "tokens": ["Und", "wollt'", "ich", "selbst", "recht", "lus\u00b7tig", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADV", "ADJD", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So lie\u00df' ich tanzen Gro\u00df und Klein", "tokens": ["So", "lie\u00df'", "ich", "tan\u00b7zen", "Gro\u00df", "und", "Klein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "VVFIN", "NE", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Beim: Kn\u00fcppel aus dem Sack!", "tokens": ["Beim", ":", "Kn\u00fcp\u00b7pel", "aus", "dem", "Sack", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "$.", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "O M\u00e4rchen, w\u00fcrdest du doch wahr", "tokens": ["O", "M\u00e4r\u00b7chen", ",", "w\u00fcr\u00b7dest", "du", "doch", "wahr"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NN", "$,", "VAFIN", "PPER", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nur Einen einz'gen Tag im Jahr,", "tokens": ["Nur", "Ei\u00b7nen", "einz'\u00b7gen", "Tag", "im", "Jahr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "O Kn\u00fcppel aus dem Sack!", "tokens": ["O", "Kn\u00fcp\u00b7pel", "aus", "dem", "Sack", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ich g\u00e4be drum, ich wei\u00df nicht was,", "tokens": ["Ich", "g\u00e4\u00b7be", "drum", ",", "ich", "wei\u00df", "nicht", "was", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PAV", "$,", "PPER", "VVFIN", "PTKNEG", "PIS", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und schl\u00fcge drein ohn' Unterlass:", "tokens": ["Und", "schl\u00fc\u00b7ge", "drein", "ohn'", "Un\u00b7ter\u00b7lass", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Frisch! Kn\u00fcppel aus dem Sack", "tokens": ["Frisch", "!", "Kn\u00fcp\u00b7pel", "aus", "dem", "Sack"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$.", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Auf's Lumpenpack!", "tokens": ["Auf's", "Lum\u00b7pen\u00b7pack", "!"], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Auf's Hundepack!", "tokens": ["Auf's", "Hun\u00b7de\u00b7pack", "!"], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}