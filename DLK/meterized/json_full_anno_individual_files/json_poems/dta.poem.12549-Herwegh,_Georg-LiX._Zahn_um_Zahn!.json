{"dta.poem.12549": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "LiX.  \n  Zahn um Zahn!", "genre": "Lyrik", "period": "N.A.", "pub_year": "1843", "urn": "urn:nbn:de:kobv:b4-200905192440", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201elange genug erhob ich zum Adel eueren", "tokens": ["\u201e", "lan\u00b7ge", "ge\u00b7nug", "er\u00b7hob", "ich", "zum", "A\u00b7del", "eu\u00b7e\u00b7ren"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "ADV", "ADV", "VVFIN", "PPER", "APPRART", "NN", "VVINF"], "meter": "+--+-+--+--+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Abschaum:", "tokens": ["Ab\u00b7schaum", ":"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Nehmt jetzt, B\u00fcrger, daf\u00fcr adligen", "tokens": ["Nehmt", "jetzt", ",", "B\u00fcr\u00b7ger", ",", "da\u00b7f\u00fcr", "ad\u00b7li\u00b7gen"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["VVFIN", "ADV", "$,", "NN", "$,", "PAV", "VVINF"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Kehricht zur\u00fcck!\u201c", "tokens": ["Keh\u00b7richt", "zu\u00b7r\u00fcck", "!", "\u201c"], "token_info": ["word", "word", "punct", "punct"], "pos": ["NN", "PTKVZ", "$.", "$("], "meter": "+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "\u2014 Andres erz\u00e4hlt die Geschichte vom", "tokens": ["And\u00b7res", "er\u00b7z\u00e4hlt", "die", "Ge\u00b7schich\u00b7te", "vom"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "PIS", "VVFIN", "ART", "NN", "APPRART"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Florentinischen Volke,", "tokens": ["Flo\u00b7ren\u00b7ti\u00b7ni\u00b7schen", "Vol\u00b7ke", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Das mit dem Adelsdiplom seine ", "tokens": ["Das", "mit", "dem", "A\u00b7dels\u00b7dip\u00b7lom", "sei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "APPR", "ART", "NN", "PPOSAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}