{"textgrid.poem.43346": {"metadata": {"author": {"name": "Leopardi, Giacomo", "birth": "N.A.", "death": "N.A."}, "title": "1L: So mild und hell und windstill ist die Nacht,", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So mild und hell und windstill ist die Nacht,", "tokens": ["So", "mild", "und", "hell", "und", "wind\u00b7still", "ist", "die", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "KON", "ADJD", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und ruhig \u00fcber D\u00e4cher hin und G\u00e4rten", "tokens": ["Und", "ru\u00b7hig", "\u00fc\u00b7ber", "D\u00e4\u00b7cher", "hin", "und", "G\u00e4r\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "NN", "PTKVZ", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Schwebt dort der Mond und zeigt auch in der Ferne", "tokens": ["Schwebt", "dort", "der", "Mond", "und", "zeigt", "auch", "in", "der", "Fer\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "NN", "KON", "VVFIN", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Klar jeden Bergesgipfel. O Geliebte,", "tokens": ["Klar", "je\u00b7den", "Ber\u00b7ges\u00b7gip\u00b7fel", ".", "O", "Ge\u00b7lieb\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "PIAT", "NN", "$.", "NE", "NN", "$,"], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}, "line.5": {"text": "Nun sind die Gassen stumm, nur aus den Fenstern", "tokens": ["Nun", "sind", "die", "Gas\u00b7sen", "stumm", ",", "nur", "aus", "den", "Fens\u00b7tern"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADJD", "$,", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Schimmert noch hie und da die n\u00e4cht'ge Lampe.", "tokens": ["Schim\u00b7mert", "noch", "hie", "und", "da", "die", "n\u00e4cht'\u00b7ge", "Lam\u00b7pe", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "KON", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.7": {"text": "Du schl\u00e4fst; denn deiner harrt' ein leichter Schlummer", "tokens": ["Du", "schl\u00e4fst", ";", "denn", "dei\u00b7ner", "harrt'", "ein", "leich\u00b7ter", "Schlum\u00b7mer"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "KON", "PPOSAT", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Im lauschigen Gemach, und keine Sorge", "tokens": ["Im", "lau\u00b7schi\u00b7gen", "Ge\u00b7mach", ",", "und", "kei\u00b7ne", "Sor\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "$,", "KON", "PIAT", "NN"], "meter": "-+---+-+-+-", "measure": "dactylic.init"}, "line.9": {"text": "Nagt dir am Herzen. Ach, du wei\u00dft, du ahnst nicht,", "tokens": ["Nagt", "dir", "am", "Her\u00b7zen", ".", "Ach", ",", "du", "wei\u00dft", ",", "du", "ahnst", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "$.", "ITJ", "$,", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "PTKNEG", "$,"], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.10": {"text": "Welch eine Wunde meiner Brust du schlugst.", "tokens": ["Welch", "ei\u00b7ne", "Wun\u00b7de", "mei\u00b7ner", "Brust", "du", "schlugst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ART", "NN", "PPOSAT", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Du schl\u00e4fst; ich tret' ans Fenster, diesen Himmel,", "tokens": ["Du", "schl\u00e4fst", ";", "ich", "tret'", "ans", "Fens\u00b7ter", ",", "die\u00b7sen", "Him\u00b7mel", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PPER", "VVFIN", "APPRART", "NN", "$,", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Der mir so g\u00fctig l\u00e4chelt, zu begr\u00fc\u00dfen", "tokens": ["Der", "mir", "so", "g\u00fc\u00b7tig", "l\u00e4\u00b7chelt", ",", "zu", "be\u00b7gr\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "PPER", "ADV", "ADJD", "VVFIN", "$,", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Und die Natur, die alte, allgewalt'ge,", "tokens": ["Und", "die", "Na\u00b7tur", ",", "die", "al\u00b7te", ",", "all\u00b7ge\u00b7walt'\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "ART", "ADJA", "$,", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Die mich erschuf zum Leiden. Dir versag' ich", "tokens": ["Die", "mich", "er\u00b7schuf", "zum", "Lei\u00b7den", ".", "Dir", "ver\u00b7sag'", "ich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "PPER", "VVFIN", "APPRART", "NN", "$.", "PPER", "VVFIN", "PPER"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Die Hoffnung, sprach sie, selbst die Hoffnung. Dir", "tokens": ["Die", "Hoff\u00b7nung", ",", "sprach", "sie", ",", "selbst", "die", "Hoff\u00b7nung", ".", "Dir"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "$,", "VVFIN", "PPER", "$,", "ADV", "ART", "NN", "$.", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Soll nie das Auge gl\u00e4nzen, als von Thr\u00e4nen. \u2013", "tokens": ["Soll", "nie", "das", "Au\u00b7ge", "gl\u00e4n\u00b7zen", ",", "als", "von", "Thr\u00e4\u00b7nen", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["VMFIN", "ADV", "ART", "NN", "VVINF", "$,", "KOUS", "APPR", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Dies war ein Feiertag; von Spiel und Kurzweil", "tokens": ["Dies", "war", "ein", "Fei\u00b7er\u00b7tag", ";", "von", "Spiel", "und", "Kurz\u00b7weil"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "NN", "$.", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Ruhst du nun aus und denkst vielleicht im Traum", "tokens": ["Ruhst", "du", "nun", "aus", "und", "denkst", "viel\u00b7leicht", "im", "Traum"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "PTKVZ", "KON", "VVFIN", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "An Alle, denen heute du gefielst", "tokens": ["An", "Al\u00b7le", ",", "de\u00b7nen", "heu\u00b7te", "du", "ge\u00b7fielst"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PIS", "$,", "PRELS", "ADV", "PPER", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Und die dir selbst gefielen. Ich \u2013 nie hofft' ich's \u2013", "tokens": ["Und", "die", "dir", "selbst", "ge\u00b7fie\u00b7len", ".", "Ich", "\u2013", "nie", "hofft'", "ich's", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PPER", "ADV", "VVPP", "$.", "PPER", "$(", "ADV", "VVFIN", "PIS", "$("], "meter": "-+-+-+--++-", "measure": "iambic.penta.relaxed"}, "line.21": {"text": "Bin unter Diesen nicht. Indessen frag' ich,", "tokens": ["Bin", "un\u00b7ter", "Die\u00b7sen", "nicht", ".", "In\u00b7des\u00b7sen", "frag'", "ich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "NN", "PTKNEG", "$.", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "Wie lang dies Leben w\u00e4hrt, und hier zu Boden", "tokens": ["Wie", "lang", "dies", "Le\u00b7ben", "w\u00e4hrt", ",", "und", "hier", "zu", "Bo\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "PDS", "NN", "VVFIN", "$,", "KON", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.23": {"text": "Werf' ich mich st\u00f6hnend. F\u00fcrchterliche Tage", "tokens": ["Wer\u00b7f'", "ich", "mich", "st\u00f6h\u00b7nend", ".", "F\u00fcrch\u00b7ter\u00b7li\u00b7che", "Ta\u00b7ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWS", "PPER", "PRF", "ADJD", "$.", "ADJA", "NN"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.24": {"text": "In solcher Jugend! Unfern auf der Stra\u00dfe", "tokens": ["In", "sol\u00b7cher", "Ju\u00b7gend", "!", "Un\u00b7fern", "auf", "der", "Stra\u00b7\u00dfe"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "$.", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.25": {"text": "Kann ich den einsamen Gesang vernehmen", "tokens": ["Kann", "ich", "den", "ein\u00b7sa\u00b7men", "Ge\u00b7sang", "ver\u00b7neh\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ART", "ADJA", "NN", "VVINF"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.26": {"text": "Des Tagel\u00f6hners, der in sp\u00e4ter Nacht", "tokens": ["Des", "Ta\u00b7ge\u00b7l\u00f6h\u00b7ners", ",", "der", "in", "sp\u00e4\u00b7ter", "Nacht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.27": {"text": "Heimkehrt vom Fest in seine arme H\u00fctte,", "tokens": ["Heim\u00b7kehrt", "vom", "Fest", "in", "sei\u00b7ne", "ar\u00b7me", "H\u00fct\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.28": {"text": "Und heftig schn\u00fcrt sich mir das Herz zusammen,", "tokens": ["Und", "hef\u00b7tig", "schn\u00fcrt", "sich", "mir", "das", "Herz", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PRF", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.29": {"text": "Denk' ich, wie Alles in der Welt vergeht", "tokens": ["Denk'", "ich", ",", "wie", "Al\u00b7les", "in", "der", "Welt", "ver\u00b7geht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "$,", "PWAV", "PIS", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.30": {"text": "Und kaum noch Spuren l\u00e4\u00dft. Verflogen ist", "tokens": ["Und", "kaum", "noch", "Spu\u00b7ren", "l\u00e4\u00dft", ".", "Ver\u00b7flo\u00b7gen", "ist"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ADV", "ADV", "NN", "VVFIN", "$.", "VVPP", "VAFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.31": {"text": "Der Festtag, und dem Feiertage folgt", "tokens": ["Der", "Fest\u00b7tag", ",", "und", "dem", "Fei\u00b7er\u00b7ta\u00b7ge", "folgt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "KON", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.32": {"text": "Der Werkeltag, und so entf\u00fchrt die Zeit", "tokens": ["Der", "Wer\u00b7kel\u00b7tag", ",", "und", "so", "ent\u00b7f\u00fchrt", "die", "Zeit"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "KON", "ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.33": {"text": "Ein jedes Menschenloos. Wo ist nun hin", "tokens": ["Ein", "je\u00b7des", "Men\u00b7schen\u00b7loos", ".", "Wo", "ist", "nun", "hin"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "PIAT", "NN", "$.", "PWAV", "VAFIN", "ADV", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.34": {"text": "Der Ruf der alten V\u00f6lker? Wo die Stimme", "tokens": ["Der", "Ruf", "der", "al\u00b7ten", "V\u00f6l\u00b7ker", "?", "Wo", "die", "Stim\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$.", "PWAV", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.35": {"text": "Unsrer erlauchten Ahnen und das Weltreich", "tokens": ["Uns\u00b7rer", "er\u00b7lauch\u00b7ten", "Ah\u00b7nen", "und", "das", "Welt\u00b7reich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.36": {"text": "Des gro\u00dfen Rom, die Waffen und das Tosen,", "tokens": ["Des", "gro\u00b7\u00dfen", "Rom", ",", "die", "Waf\u00b7fen", "und", "das", "To\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NE", "$,", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.37": {"text": "Das einst erschollen \u00fcber Land und Meer?", "tokens": ["Das", "einst", "er\u00b7schol\u00b7len", "\u00fc\u00b7ber", "Land", "und", "Meer", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.38": {"text": "Alles ist Ruh' und Frieden, stille liegt", "tokens": ["Al\u00b7les", "ist", "Ruh'", "und", "Frie\u00b7den", ",", "stil\u00b7le", "liegt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PIS", "VAFIN", "NN", "KON", "NN", "$,", "ADJA", "VVFIN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.39": {"text": "Die weite Welt, und Niemand spricht von Jenen.", "tokens": ["Die", "wei\u00b7te", "Welt", ",", "und", "Nie\u00b7mand", "spricht", "von", "Je\u00b7nen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "KON", "PIS", "VVFIN", "APPR", "PDAT", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.40": {"text": "In meiner Jugendzeit, da noch mit Sehnsucht", "tokens": ["In", "mei\u00b7ner", "Ju\u00b7gend\u00b7zeit", ",", "da", "noch", "mit", "Sehn\u00b7sucht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "KOUS", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.41": {"text": "Den Festtag ich erharrte, wenn er dann", "tokens": ["Den", "Fest\u00b7tag", "ich", "er\u00b7harr\u00b7te", ",", "wenn", "er", "dann"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "PPER", "VVFIN", "$,", "KOUS", "PPER", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.42": {"text": "Vergangen war, lag ich in Schmerzen wach", "tokens": ["Ver\u00b7gan\u00b7gen", "war", ",", "lag", "ich", "in", "Schmer\u00b7zen", "wach"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "$,", "VVFIN", "PPER", "APPR", "NN", "VVFIN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.43": {"text": "Auf meinem Bette; und in sp\u00e4ter Nacht", "tokens": ["Auf", "mei\u00b7nem", "Bet\u00b7te", ";", "und", "in", "sp\u00e4\u00b7ter", "Nacht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$.", "KON", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.44": {"text": "Ein Lied, das mir heraufklang von der Stra\u00dfe", "tokens": ["Ein", "Lied", ",", "das", "mir", "her\u00b7auf\u00b7klang", "von", "der", "Stra\u00b7\u00dfe"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.45": {"text": "Und sich entfernend nach und nach erstarb \u2013", "tokens": ["Und", "sich", "ent\u00b7fer\u00b7nend", "nach", "und", "nach", "er\u00b7starb", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "VVPP", "APPR", "KON", "APPR", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.46": {"text": "Ganz so wie heut beklemmte mir's das Herz!", "tokens": ["Ganz", "so", "wie", "heut", "be\u00b7klemm\u00b7te", "mir's", "das", "Herz", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"