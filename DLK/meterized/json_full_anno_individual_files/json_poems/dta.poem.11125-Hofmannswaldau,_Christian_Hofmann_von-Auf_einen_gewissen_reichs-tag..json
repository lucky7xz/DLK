{"dta.poem.11125": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auf einen gewissen reichs-tag.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Hjer ist der beste rath, des andern rath zu schw\u00e4chen.", "tokens": ["Hjer", "ist", "der", "bes\u00b7te", "rath", ",", "des", "an\u00b7dern", "rath", "zu", "schw\u00e4\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hier ist der beste spruch, einander widersprechen.", "tokens": ["Hier", "ist", "der", "bes\u00b7te", "spruch", ",", "ein\u00b7an\u00b7der", "wi\u00b7der\u00b7spre\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "$,", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Was ", "tokens": ["Was"], "token_info": ["word"], "pos": ["PWS"], "meter": "-", "measure": "single.down"}, "line.4": {"text": "Wie soll der reichs-tag nicht zuletzt ein rei\u00df-tag seyn?", "tokens": ["Wie", "soll", "der", "reichs\u00b7tag", "nicht", "zu\u00b7letzt", "ein", "rei\u00df\u00b7tag", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "ART", "NN", "PTKNEG", "ADV", "ART", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}