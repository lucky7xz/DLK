{"dta.poem.18757": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang von", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1819", "urn": "urn:nbn:de:kobv:b4-200905191692", "language": ["de:0.99"], "booktitle": "Goethe, Johann Wolfgang von: West-\u00f6stlicher Divan. Stuttgart, 1819."}, "poem": {"stanza.1": {"line.1": {"text": "\u201ewas schm\u00fcckst du die eine Hand denn nun", "tokens": ["\u201e", "was", "schm\u00fcckst", "du", "die", "ei\u00b7ne", "Hand", "denn", "nun"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "PWS", "VVFIN", "PPER", "ART", "ART", "NN", "KON", "ADV"], "meter": "++--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Weit mehr als ihr geb\u00fchrte.\u201c", "tokens": ["Weit", "mehr", "als", "ihr", "ge\u00b7b\u00fchr\u00b7te", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADJD", "ADV", "KOUS", "PPER", "VVFIN", "$.", "$("], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Was sollte denn die linke thun,", "tokens": ["Was", "soll\u00b7te", "denn", "die", "lin\u00b7ke", "thun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ADV", "ART", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn sie die rechte nicht zierte?", "tokens": ["Wenn", "sie", "die", "rech\u00b7te", "nicht", "zier\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "PTKNEG", "VVFIN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}}}}