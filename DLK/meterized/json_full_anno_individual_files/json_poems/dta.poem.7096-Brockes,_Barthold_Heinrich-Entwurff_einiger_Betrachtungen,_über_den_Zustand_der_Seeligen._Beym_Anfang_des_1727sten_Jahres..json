{"dta.poem.7096": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Entwurff einiger Betrachtungen,  \n \u00fcber den  \n  Zustand der Seeligen.  \n Beym Anfang des 1727sten Jahres.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Sie wird durch Danckbarkeit und durch Verwundrungs-", "tokens": ["Sie", "wird", "durch", "Dan\u00b7ck\u00b7bar\u00b7keit", "und", "durch", "Ver\u00b7wun\u00b7drungs"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "NN", "KON", "APPR", "TRUNC"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Triebe,", "tokens": ["Trie\u00b7be", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "In stetig brennender und br\u00fcnst\u2019ger GOTTES Liebe,", "tokens": ["In", "ste\u00b7tig", "bren\u00b7nen\u00b7der", "und", "br\u00fcnst'\u00b7ger", "GoT\u00b7TES", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "KON", "ADJA", "NN", "NN", "$,"], "meter": "---+-+-+-+-+-", "measure": "unknown.measure.penta"}, "line.4": {"text": "Am allerseeligsten und recht verhimmelt seyn.", "tokens": ["Am", "al\u00b7ler\u00b7see\u00b7ligs\u00b7ten", "und", "recht", "ver\u00b7him\u00b7melt", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "KON", "ADJD", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "In einem br\u00fcnstigen Vertrauen,", "tokens": ["In", "ei\u00b7nem", "br\u00fcns\u00b7ti\u00b7gen", "Ver\u00b7trau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und in der Lieb allein,", "tokens": ["Und", "in", "der", "Lieb", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Vermag man in der GOTTHEIT hellen Schein", "tokens": ["Ver\u00b7mag", "man", "in", "der", "GoT\u00b7T\u00b7HEIT", "hel\u00b7len", "Schein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Am allertieffsten einzuschauen.", "tokens": ["Am", "al\u00b7ler\u00b7tieffs\u00b7ten", "ein\u00b7zu\u00b7schau\u00b7en", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}