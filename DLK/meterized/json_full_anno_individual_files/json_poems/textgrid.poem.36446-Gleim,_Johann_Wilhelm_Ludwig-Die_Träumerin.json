{"textgrid.poem.36446": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Die Tr\u00e4umerin", "genre": "verse", "period": "N.A.", "pub_year": 1761, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein kleines schwarzes M\u00e4dchen,", "tokens": ["Ein", "klei\u00b7nes", "schwar\u00b7zes", "M\u00e4d\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Hielt auf dem weichsten Bette,", "tokens": ["Hielt", "auf", "dem", "weichs\u00b7ten", "Bet\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die sanfte Mittagsruhe.", "tokens": ["Die", "sanf\u00b7te", "Mit\u00b7tags\u00b7ru\u00b7he", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Es schlief, wie M\u00e4dchen schlafen,", "tokens": ["Es", "schlief", ",", "wie", "M\u00e4d\u00b7chen", "schla\u00b7fen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Es l\u00e4chelte im Schlafe;", "tokens": ["Es", "l\u00e4\u00b7chel\u00b7te", "im", "Schla\u00b7fe", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Es regte sich der Busen,", "tokens": ["Es", "reg\u00b7te", "sich", "der", "Bu\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "So oft es Athem holte.", "tokens": ["So", "oft", "es", "A\u00b7them", "hol\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Es that, als wolt es wachen;", "tokens": ["Es", "that", ",", "als", "wolt", "es", "wa\u00b7chen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Es warf sich hin und wieder,", "tokens": ["Es", "warf", "sich", "hin", "und", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PTKVZ", "KON", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Und l\u00e4chelte noch zweimal;", "tokens": ["Und", "l\u00e4\u00b7chel\u00b7te", "noch", "zwei\u00b7mal", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Es stekkte bey dem L\u00e4cheln,", "tokens": ["Es", "stekk\u00b7te", "bey", "dem", "L\u00e4\u00b7cheln", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Die rechte Hand im Busen.", "tokens": ["Die", "rech\u00b7te", "Hand", "im", "Bu\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Ich b\u00fckkte mich und lauschte", "tokens": ["Ich", "b\u00fckk\u00b7te", "mich", "und", "lauschte"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "KON", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.14": {"text": "Die Linke zu erblikken;", "tokens": ["Die", "Lin\u00b7ke", "zu", "er\u00b7blik\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.15": {"text": "Allein sie war verborgen.", "tokens": ["Al\u00b7lein", "sie", "war", "ver\u00b7bor\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.16": {"text": "Doch, als ich nicht mehr lauschte,", "tokens": ["Doch", ",", "als", "ich", "nicht", "mehr", "lauschte", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PPER", "PTKNEG", "ADV", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.17": {"text": "Zog es sie schnell zur\u00fckke,", "tokens": ["Zog", "es", "sie", "schnell", "zu\u00b7r\u00fck\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.18": {"text": "Und warf sie zu der Rechten,", "tokens": ["Und", "warf", "sie", "zu", "der", "Rech\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.19": {"text": "Und faltete die H\u00e4nde,", "tokens": ["Und", "fal\u00b7te\u00b7te", "die", "H\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.20": {"text": "Wie fromme Beterinnen,", "tokens": ["Wie", "from\u00b7me", "Be\u00b7te\u00b7rin\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.21": {"text": "Die H\u00e4nd aus Andacht falten.", "tokens": ["Die", "H\u00e4nd", "aus", "An\u00b7dacht", "fal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.22": {"text": "Ach! sprach ich zu den Br\u00fcdern,", "tokens": ["Ach", "!", "sprach", "ich", "zu", "den", "Br\u00fc\u00b7dern", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.23": {"text": "Ach seht, das M\u00e4dchen betet!", "tokens": ["Ach", "seht", ",", "das", "M\u00e4d\u00b7chen", "be\u00b7tet", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "$,", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.24": {"text": "Warum mag doch das M\u00e4dchen,", "tokens": ["Wa\u00b7rum", "mag", "doch", "das", "M\u00e4d\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.25": {"text": "Den harten Himmel bitten?", "tokens": ["Den", "har\u00b7ten", "Him\u00b7mel", "bit\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.26": {"text": "Vernimm es, sprach ein Bruder:", "tokens": ["Ver\u00b7nimm", "es", ",", "sprach", "ein", "Bru\u00b7der", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.27": {"text": "Ich wei\u00df, da\u00df fromme M\u00e4dchen", "tokens": ["Ich", "wei\u00df", ",", "da\u00df", "from\u00b7me", "M\u00e4d\u00b7chen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.28": {"text": "Gott oft um M\u00e4nner bitten,", "tokens": ["Gott", "oft", "um", "M\u00e4n\u00b7ner", "bit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.29": {"text": "Und da\u00df sie oft, in Tr\u00e4umen,", "tokens": ["Und", "da\u00df", "sie", "oft", ",", "in", "Tr\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "$,", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.30": {"text": "Die Bitten wiederholen,", "tokens": ["Die", "Bit\u00b7ten", "wie\u00b7der\u00b7ho\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.31": {"text": "In Tr\u00e4umen M\u00e4nner haben,", "tokens": ["In", "Tr\u00e4u\u00b7men", "M\u00e4n\u00b7ner", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "VAFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.32": {"text": "Und glauben sie zu k\u00fcssen.", "tokens": ["Und", "glau\u00b7ben", "sie", "zu", "k\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.33": {"text": "Dis glaub es, lieber Bruder,", "tokens": ["Dis", "glaub", "es", ",", "lie\u00b7ber", "Bru\u00b7der", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "$,", "ADV", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.34": {"text": "Dis glaubet auch das M\u00e4dchen.", "tokens": ["Dis", "glau\u00b7bet", "auch", "das", "M\u00e4d\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.35": {"text": "Gleich schlich ich zu dem M\u00e4dchen,", "tokens": ["Gleich", "schlich", "ich", "zu", "dem", "M\u00e4d\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.36": {"text": "Und fragt es: Wilst du k\u00fcssen?", "tokens": ["Und", "fragt", "es", ":", "Wilst", "du", "k\u00fcs\u00b7sen", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$.", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.37": {"text": "Da strekkte mir das M\u00e4dchen", "tokens": ["Da", "strekk\u00b7te", "mir", "das", "M\u00e4d\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.38": {"text": "Die Lippen schnell entgegen,", "tokens": ["Die", "Lip\u00b7pen", "schnell", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.39": {"text": "Und eh ich sie ber\u00fchrte,", "tokens": ["Und", "eh", "ich", "sie", "be\u00b7r\u00fchr\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.40": {"text": "Ert\u00f6nten schon die Schm\u00e4tzgen.", "tokens": ["Er\u00b7t\u00f6n\u00b7ten", "schon", "die", "Schm\u00e4tz\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Nun sagt einmal, ihr Sch\u00f6nen,", "tokens": ["Nun", "sagt", "ein\u00b7mal", ",", "ihr", "Sch\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zu mir und meinen Br\u00fcdern:", "tokens": ["Zu", "mir", "und", "mei\u00b7nen", "Br\u00fc\u00b7dern", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ihr wolt nur immer k\u00fcssen.", "tokens": ["Ihr", "wolt", "nur", "im\u00b7mer", "k\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ein kleines schwarzes M\u00e4dchen,", "tokens": ["Ein", "klei\u00b7nes", "schwar\u00b7zes", "M\u00e4d\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Hielt auf dem weichsten Bette,", "tokens": ["Hielt", "auf", "dem", "weichs\u00b7ten", "Bet\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die sanfte Mittagsruhe.", "tokens": ["Die", "sanf\u00b7te", "Mit\u00b7tags\u00b7ru\u00b7he", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Es schlief, wie M\u00e4dchen schlafen,", "tokens": ["Es", "schlief", ",", "wie", "M\u00e4d\u00b7chen", "schla\u00b7fen", ","], "token_info": ["word", "word", "punc