{"dta.poem.21614": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Erwachen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Einen wei\u00dfen Federflaum", "tokens": ["Ei\u00b7nen", "wei\u00b7\u00dfen", "Fe\u00b7der\u00b7flaum"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Fand am Fenster ich den Morgen,", "tokens": ["Fand", "am", "Fens\u00b7ter", "ich", "den", "Mor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Als der Tag aus wirrem Traum", "tokens": ["Als", "der", "Tag", "aus", "wir\u00b7rem", "Traum"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Mich erweckt zu s\u00fc\u00dfen Sorgen.", "tokens": ["Mich", "er\u00b7weckt", "zu", "s\u00fc\u00b7\u00dfen", "Sor\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Und ich blick\u2019 erstaunt hinauf", "tokens": ["Und", "ich", "blick'", "er\u00b7staunt", "hin\u00b7auf"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ADJD", "PTKVZ"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "An den frischen Morgenhimmel,", "tokens": ["An", "den", "fri\u00b7schen", "Mor\u00b7gen\u00b7him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sehe dort in leisem Lauf", "tokens": ["Se\u00b7he", "dort", "in", "lei\u00b7sem", "Lauf"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ziehn der Wolken leicht Gewimmel.", "tokens": ["Ziehn", "der", "Wol\u00b7ken", "leicht", "Ge\u00b7wim\u00b7mel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJD", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Ja, sie ziehn in breitem Zug", "tokens": ["Ja", ",", "sie", "ziehn", "in", "brei\u00b7tem", "Zug"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Zwischen mir und jener Gegend. \u2014", "tokens": ["Zwi\u00b7schen", "mir", "und", "je\u00b7ner", "Ge\u00b7gend", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PPER", "KON", "PDAT", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ist es Wahrheit? Ist es Trug?", "tokens": ["Ist", "es", "Wahr\u00b7heit", "?", "Ist", "es", "Trug", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "NN", "$.", "VAFIN", "PPER", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sind\u2019s nicht Schw\u00e4ne fl\u00fcgelregend?", "tokens": ["Sin\u00b7d's", "nicht", "Schw\u00e4\u00b7ne", "fl\u00fc\u00b7gel\u00b7re\u00b7gend", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "NN", "VVPP", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}}, "stanza.4": {"line.1": {"text": "Ist mein Liebchen gar vielleicht", "tokens": ["Ist", "mein", "Lieb\u00b7chen", "gar", "viel\u00b7leicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPOSAT", "NN", "ADV", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Solch ein heimlich Zauberwesen,", "tokens": ["Solch", "ein", "heim\u00b7lich", "Zau\u00b7ber\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "ART", "ADJD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Das als Schwan die Luft durchstreicht,", "tokens": ["Das", "als", "Schwan", "die", "Luft", "durch\u00b7streicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "KOUS", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie in M\u00e4rchen ich gelesen?", "tokens": ["Wie", "in", "M\u00e4r\u00b7chen", "ich", "ge\u00b7le\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "NN", "PPER", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Sch\u00f6n in menschlicher Gestalt,", "tokens": ["Sch\u00f6n", "in", "menschli\u00b7cher", "Ge\u00b7stalt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ADJA", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Hat sie traut besucht mich gestern,", "tokens": ["Hat", "sie", "traut", "be\u00b7sucht", "mich", "ge\u00b7stern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVFIN", "VVFIN", "PPER", "ADV", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Nachts in Zauberbanns Gewalt", "tokens": ["Nachts", "in", "Zau\u00b7ber\u00b7banns", "Ge\u00b7walt"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Schw\u00e4rmt sie mit den Schwanenschwestern.", "tokens": ["Schw\u00e4rmt", "sie", "mit", "den", "Schwa\u00b7nen\u00b7schwes\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Fliegt bis an mein Fensterbrett,", "tokens": ["Fliegt", "bis", "an", "mein", "Fens\u00b7ter\u00b7brett", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Putzt das wei\u00dfe Schwangefieder,", "tokens": ["Putzt", "das", "wei\u00b7\u00dfe", "Schwan\u00b7ge\u00b7fie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "W\u00e4hrend einsam ich im Bett,", "tokens": ["W\u00e4h\u00b7rend", "ein\u00b7sam", "ich", "im", "Bett", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "W\u00e4lze sonder Ruh die Glieder.", "tokens": ["W\u00e4l\u00b7ze", "son\u00b7der", "Ruh", "die", "Glie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}