meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Ach! ich selbst kann's nicht vollbringen,", "tokens": ["Ach", "!", "ich", "selbst", "kann's", "nicht", "voll\u00b7brin\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PPER", "ADV", "VMFIN", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und ich mu\u00df doch zu Dir hin;", "tokens": ["Und", "ich", "mu\u00df", "doch", "zu", "Dir", "hin", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "ADV", "APPR", "PPER", "PTKVZ", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Du, mein Gott, Du selbst mu\u00dft zwingen", "tokens": ["Du", ",", "mein", "Gott", ",", "Du", "selbst", "mu\u00dft", "zwin\u00b7gen"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "$,", "PPOSAT", "NN", "$,", "PPER", "ADV", "VMFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Den verkehrten, eiteln Sinn,", "tokens": ["Den", "ver\u00b7kehr\u00b7ten", ",", "ei\u00b7teln", "Sinn", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Bis ich Dir geheiligt bin.", "tokens": ["Bis", "ich", "Dir", "ge\u00b7hei\u00b7ligt", "bin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVPP", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Amen, Amen,", "tokens": ["A\u00b7men", ",", "A\u00b7men", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "In Jesu Namen!", "tokens": ["In", "Je\u00b7su", "Na\u00b7men", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}}}}