{"textgrid.poem.57793": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Ulaneneinmaleins", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Eins, zwei, drei und vier,", "tokens": ["Eins", ",", "zwei", ",", "drei", "und", "vier", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "CARD", "$,", "CARD", "KON", "CARD", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Ulanen und die hei\u00dfen wir;", "tokens": ["U\u00b7la\u00b7nen", "und", "die", "hei\u00b7\u00dfen", "wir", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ART", "ADJA", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ulanen die sind blau und wei\u00df,", "tokens": ["U\u00b7la\u00b7nen", "die", "sind", "blau", "und", "wei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "VAFIN", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ulanen lieben treu und hei\u00df,", "tokens": ["U\u00b7la\u00b7nen", "lie\u00b7ben", "treu", "und", "hei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ja treu und hei\u00df.", "tokens": ["Ja", "treu", "und", "hei\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "F\u00fcnf, sechs, sieben und acht,", "tokens": ["F\u00fcnf", ",", "sechs", ",", "sie\u00b7ben", "und", "acht", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "