{"dta.poem.13226": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "E ilftes  B uch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Bey diesem Ernst-Gespr\u00e4ch und theuern Kinder-", "tokens": ["Bey", "die\u00b7sem", "Ernst\u00b7Ge\u00b7spr\u00e4ch", "und", "theu\u00b7ern", "Kin\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "KON", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Scherz", "tokens": ["Scherz"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Verrieth der Ehgemahl durch seinen Blick das", "tokens": ["Ver\u00b7rieth", "der", "Eh\u00b7ge\u00b7mahl", "durch", "sei\u00b7nen", "Blick", "das"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "ART"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Herz;", "tokens": ["Herz", ";"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Des Augs Lebhaftigkeit war dergestallt beflissen,", "tokens": ["Des", "Augs", "Leb\u00b7haf\u00b7tig\u00b7keit", "war", "der\u00b7ge\u00b7stallt", "be\u00b7flis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Als wollt\u2019 es so den Sinn, wie die Geb\u00e4rden, wissen.", "tokens": ["Als", "wollt'", "es", "so", "den", "Sinn", ",", "wie", "die", "Ge\u00b7b\u00e4r\u00b7den", ",", "wis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "VMFIN", "PPER", "ADV", "ART", "NN", "$,", "PWAV", "ART", "NN", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Kein Strahl der Majest\u00e4t, kein Wort blieb ungef\u00fchlt;", "tokens": ["Kein", "Strahl", "der", "Ma\u00b7jes\u00b7t\u00e4t", ",", "kein", "Wort", "blieb", "un\u00b7ge\u00b7f\u00fchlt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ART", "NN", "$,", "PIAT", "NN", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Er schien als ob er still dar\u00fcber Rathschlag hielt.", "tokens": ["Er", "schien", "als", "ob", "er", "still", "da\u00b7r\u00fc\u00b7ber", "Rath\u00b7schlag", "hielt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "KOUS", "PPER", "ADJD", "PAV", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Je mehr ", "tokens": ["Je", "mehr"], "token_info": ["word", "word"], "pos": ["ADV", "ADV"], "meter": "-+", "measure": "iambic.single"}, "line.10": {"text": "Je mehr ward ich gewahr, da\u00df sich sein Herz vergn\u00fcge.", "tokens": ["Je", "mehr", "ward", "ich", "ge\u00b7wahr", ",", "da\u00df", "sich", "sein", "Herz", "ver\u00b7gn\u00fc\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "ADJD", "$,", "KOUS", "PRF", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+---+-+-+-+-", "measure": "dactylic.init"}}}}}