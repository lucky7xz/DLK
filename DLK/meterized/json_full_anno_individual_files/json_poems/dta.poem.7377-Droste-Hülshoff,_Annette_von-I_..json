{"dta.poem.7377": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "I .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-20090519994", "language": ["de:0.99"], "booktitle": "Droste-H\u00fclshoff, Annette von: Gedichte. Stuttgart u. a., 1844."}, "poem": {"stanza.1": {"line.1": {"text": "Sacht pochet der K\u00e4fer im morschen Schrein,", "tokens": ["Sacht", "po\u00b7chet", "der", "K\u00e4\u00b7fer", "im", "mor\u00b7schen", "Schrein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Der Mond steht \u00fcber den Fichten.", "tokens": ["Der", "Mond", "steht", "\u00fc\u00b7ber", "den", "Fich\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "\u201ejesus Maria, wo mag sie seyn!", "tokens": ["\u201e", "je\u00b7sus", "Ma\u00b7ria", ",", "wo", "mag", "sie", "seyn", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "NE", "NE", "$,", "PWAV", "VMFIN", "PPER", "VAINF", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Hin will meine Angst mich richten.", "tokens": ["Hin", "will", "mei\u00b7ne", "Angst", "mich", "rich\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPOSAT", "NN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Helene, Helene, was lie\u00df ich dich gehn", "tokens": ["He\u00b7le\u00b7ne", ",", "He\u00b7le\u00b7ne", ",", "was", "lie\u00df", "ich", "dich", "gehn"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "NE", "$,", "PWS", "VVFIN", "PPER", "PRF", "VVINF"], "meter": "-+--+-++--+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Allein zur Stadt mit den Hunden,", "tokens": ["Al\u00b7lein", "zur", "Stadt", "mit", "den", "Hun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Du armes Kind, das sterbend mir", "tokens": ["Du", "ar\u00b7mes", "Kind", ",", "das", "ster\u00b7bend", "mir"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "ADJA", "NN", "$,", "PRELS", "ADJD", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Auf die Seele die Mutter gebunden!\u201c", "tokens": ["Auf", "die", "See\u00b7le", "die", "Mut\u00b7ter", "ge\u00b7bun\u00b7den", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVPP", "$.", "$("], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Und wieder rennt Gertrude den Weg", "tokens": ["Und", "wie\u00b7der", "rennt", "Ger\u00b7tru\u00b7de", "den", "Weg"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "NN", "ART", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Hinauf bis \u00fcber die Steige.", "tokens": ["Hin\u00b7auf", "bis", "\u00fc\u00b7ber", "die", "Stei\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Hier ist ein Tobel \u2014 sie lauscht am Steg,", "tokens": ["Hier", "ist", "ein", "To\u00b7bel", "sie", "lauscht", "am", "Steg", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "$(", "PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Strauch \u2014 sie r\u00fcttelt am Zweige.", "tokens": ["Ein", "Strauch", "sie", "r\u00fct\u00b7telt", "am", "Zwei\u00b7ge", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PPER", "VVFIN", "APPRART", "NN", "$."], "meter": "-++--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Da drunten summet es Elf im Thurm,", "tokens": ["Da", "drun\u00b7ten", "sum\u00b7met", "es", "Elf", "im", "Thurm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "NE", "APPRART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Gertrude kniet an der Halde:", "tokens": ["Ger\u00b7tru\u00b7de", "kniet", "an", "der", "Hal\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "\u201edu armes Blut, du verlassener Wurm!", "tokens": ["\u201e", "du", "ar\u00b7mes", "Blut", ",", "du", "ver\u00b7las\u00b7se\u00b7ner", "Wurm", "!"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "ADJA", "NN", "$,", "PPER", "ADJA", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Wo magst du irren im Walde!\u201c", "tokens": ["Wo", "magst", "du", "ir\u00b7ren", "im", "Wal\u00b7de", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "VVFIN", "APPRART", "NN", "$.", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Und zitternd l\u00f6st sie den Rosenkranz", "tokens": ["Und", "zit\u00b7ternd", "l\u00f6st", "sie", "den", "Ro\u00b7sen\u00b7kranz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Von ihres G\u00fcrtels Geh\u00e4nge,", "tokens": ["Von", "ih\u00b7res", "G\u00fcr\u00b7tels", "Ge\u00b7h\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ihr Auge starret in tr\u00fcbem Glanz,", "tokens": ["Ihr", "Au\u00b7ge", "star\u00b7ret", "in", "tr\u00fc\u00b7bem", "Glanz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ob es die D\u00e4mmerung sprenge.", "tokens": ["Ob", "es", "die", "D\u00e4m\u00b7me\u00b7rung", "spren\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.5": {"text": "\u201eave Maria \u2014 ein Licht, ein Licht!", "tokens": ["\u201e", "a\u00b7ve", "Ma\u00b7ria", "ein", "Licht", ",", "ein", "Licht", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "NE", "NE", "$(", "ART", "NN", "$,", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sie k\u00f6mmt, 's ist ihre Laterne!", "tokens": ["Sie", "k\u00f6mmt", ",", "'s", "ist", "ih\u00b7re", "La\u00b7ter\u00b7ne", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-++--+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "\u2014 Ach Gott, es ist nur ein Hirtenfeur,", "tokens": ["Ach", "Gott", ",", "es", "ist", "nur", "ein", "Hir\u00b7ten\u00b7feur", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ITJ", "NN", "$,", "PPER", "VAFIN", "ADV", "ART", "NN", "$,"], "meter": "---+-+--+", "measure": "iambic.tri.chol"}, "line.8": {"text": "Jetzt wirft es flatternde Sterne.", "tokens": ["Jetzt", "wirft", "es", "flat\u00b7tern\u00b7de", "Ster\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Vater unser, der du im Himmel bist", "tokens": ["Va\u00b7ter", "un\u00b7ser", ",", "der", "du", "im", "Him\u00b7mel", "bist"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "PPOSAT", "$,", "PRELS", "PPER", "APPRART", "NN", "VAFIN"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Geheiliget werde dein Name\u201c \u2014", "tokens": ["Ge\u00b7hei\u00b7li\u00b7get", "wer\u00b7de", "dein", "Na\u00b7me", "\u201c"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["VVPP", "VAFIN", "PPOSAT", "NN", "$(", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Es rauscht am Hange, \u201eheiliger Christ!\u201c", "tokens": ["Es", "rauscht", "am", "Han\u00b7ge", ",", "\u201e", "hei\u00b7li\u00b7ger", "Christ", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$,", "$(", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Es bricht und knistert im Brahme,", "tokens": ["Es", "bricht", "und", "knis\u00b7tert", "im", "Brah\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Und dr\u00fcber streckt sich ein schlanker Hals,", "tokens": ["Und", "dr\u00fc\u00b7ber", "streckt", "sich", "ein", "schlan\u00b7ker", "Hals", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "VVFIN", "PRF", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Zwei gl\u00e4nzende Augen starren.", "tokens": ["Zwei", "gl\u00e4n\u00b7zen\u00b7de", "Au\u00b7gen", "star\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "ADJA", "NN", "VVFIN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "\u201each Gott, es ist eine Hinde nur,", "tokens": ["\u201e", "ach", "Gott", ",", "es", "ist", "ei\u00b7ne", "Hin\u00b7de", "nur", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "XY", "NN", "$,", "PPER", "VAFIN", "ART", "NN", "ADV", "$,"], "meter": "-+-++-+-+", "measure": "unknown.measure.penta"}, "line.8": {"text": "Jetzt setzt sie \u00fcber die Farren.\u201c", "tokens": ["Jetzt", "setzt", "sie", "\u00fc\u00b7ber", "die", "Far\u00b7ren", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$.", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Gertrude klimmt die Halde hinauf,", "tokens": ["Ger\u00b7tru\u00b7de", "klimmt", "die", "Hal\u00b7de", "hin\u00b7auf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Sie steht an des Raines Mitte.", "tokens": ["Sie", "steht", "an", "des", "Rai\u00b7nes", "Mit\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Da \u2014 t\u00e4uscht ihr Ohr? \u2014 ein fl\u00fcchtiger Lauf,", "tokens": ["Da", "t\u00e4uscht", "ihr", "Ohr", "?", "ein", "fl\u00fcch\u00b7ti\u00b7ger", "Lauf", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "$(", "VVFIN", "PPOSAT", "NN", "$.", "$(", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Behend galoppirende Tritte \u2014", "tokens": ["Be\u00b7hend", "ga\u00b7lop\u00b7pi\u00b7ren\u00b7de", "Trit\u00b7te"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Und um sie springt es in w\u00fcstem Kreis,", "tokens": ["Und", "um", "sie", "springt", "es", "in", "w\u00fcs\u00b7tem", "Kreis", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Und funkelt mit freud'gem Gest\u00f6hne.", "tokens": ["Und", "fun\u00b7kelt", "mit", "freu\u00b7d'\u00b7gem", "Ge\u00b7st\u00f6h\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+--+---+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "\u201efidel, Fidel!\u201c so fl\u00fcstert sie leis,", "tokens": ["\u201e", "fi\u00b7del", ",", "Fi\u00b7del", "!", "\u201c", "so", "fl\u00fcs\u00b7tert", "sie", "leis", ","], "token_info": ["punct", "word", "punct", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "$,", "NE", "$.", "$(", "ADV", "VVFIN", "PPER", "ADJD", "$,"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.8": {"text": "Dann ruft sie schluchzend: \u201eHelene!\u201c", "tokens": ["Dann", "ruft", "sie", "schluch\u00b7zend", ":", "\u201e", "He\u00b7le\u00b7ne", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "$.", "$(", "NE", "$.", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "\u201ehelene!\u201c schallt es am Felsenhang,", "tokens": ["\u201e", "he\u00b7le\u00b7ne", "!", "\u201c", "schallt", "es", "am", "Fel\u00b7sen\u00b7hang", ","], "token_info": ["punct", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJA", "$.", "$(", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "\u201ehelen'!\u201c von des Waldes Kante,", "tokens": ["\u201e", "he\u00b7len'", "!", "\u201c", "von", "des", "Wal\u00b7des", "Kan\u00b7te", ","], "token_info": ["punct", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PTKVZ", "$.", "$(", "APPR", "ART", "NN", "VMFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Es war ein einsamer trauriger Klang,", "tokens": ["Es", "war", "ein", "ein\u00b7sa\u00b7mer", "trau\u00b7ri\u00b7ger", "Klang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Den heimw\u00e4rts die Echo sandte.", "tokens": ["Den", "heim\u00b7w\u00e4rts", "die", "E\u00b7cho", "sand\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Wo drunten im Tobel das M\u00fchlrad wacht,", "tokens": ["Wo", "drun\u00b7ten", "im", "To\u00b7bel", "das", "M\u00fchl\u00b7rad", "wacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "APPRART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Die staubigen Knecht' an der Wanne", "tokens": ["Die", "stau\u00b7bi\u00b7gen", "Knecht'", "an", "der", "Wan\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.7": {"text": "Die haben gehorcht die ganze Nacht", "tokens": ["Die", "ha\u00b7ben", "ge\u00b7horcht", "die", "gan\u00b7ze", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "VVPP", "ART", "ADJA", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Auf das irre Gespenst im Tanne.", "tokens": ["Auf", "das", "ir\u00b7re", "Ge\u00b7spenst", "im", "Tan\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}}, "stanza.7": {"line.1": {"text": "Sie h\u00f6rten sein Rufen von Stund' zu Stund',", "tokens": ["Sie", "h\u00f6r\u00b7ten", "sein", "Ru\u00b7fen", "von", "Stund'", "zu", "Stund'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR", "NN", "APPR", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Sahn seiner Laterne Geflimmer,", "tokens": ["Sahn", "sei\u00b7ner", "La\u00b7ter\u00b7ne", "Ge\u00b7flim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": 