{"dta.poem.10089": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Natur und Kunst.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20086-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Liebster Mensch, gebrauche doch den erforschenden\nVerstand!", "tokens": ["Liebs\u00b7ter", "Mensch", ",", "ge\u00b7brau\u00b7che", "doch", "den", "er\u00b7for\u00b7schen\u00b7den", "Ver\u00b7stand", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Nimm nur einst von einer Bluhme eine Zwiebel in die", "tokens": ["Nimm", "nur", "einst", "von", "ei\u00b7ner", "Bluh\u00b7me", "ei\u00b7ne", "Zwie\u00b7bel", "in", "die"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "ADV", "APPR", "ART", "NN", "ART", "NN", "APPR", "ART"], "meter": "+-+-+-+-+-+--+", "measure": "iambic.septa.chol"}, "line.3": {"text": "Schau sie an! Sprich, kannst du wol die verborgne Krafft", "tokens": ["Schau", "sie", "an", "!", "Sprich", ",", "kannst", "du", "wol", "die", "ver\u00b7borg\u00b7ne", "Krafft"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "PTKVZ", "$.", "VVIMP", "$,", "VMFIN", "PPER", "ADV", "ART", "ADJA", "NN"], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}, "line.4": {"text": "Und den unbekannten Geist, der darin verborgen, finden?", "tokens": ["Und", "den", "un\u00b7be\u00b7kann\u00b7ten", "Geist", ",", "der", "da\u00b7rin", "ver\u00b7bor\u00b7gen", ",", "fin\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$,", "PRELS", "PAV", "VVPP", "$,", "VVINF", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}}, "stanza.2": {"line.1": {"text": "Von sich selber scheint sie schwanger, sie empf\u00e4ngt auch", "tokens": ["Von", "sich", "sel\u00b7ber", "scheint", "sie", "schwan\u00b7ger", ",", "sie", "emp\u00b7f\u00e4ngt", "auch"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PRF", "ADV", "VVFIN", "PPER", "ADJD", "$,", "PPER", "VVFIN", "ADV"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Ein recht Wunder-sch\u00f6nes Kind; da sie selbst doch \u00e4usserlich", "tokens": ["Ein", "recht", "Wun\u00b7der\u00b7sch\u00f6\u00b7nes", "Kind", ";", "da", "sie", "selbst", "doch", "\u00e4us\u00b7ser\u00b7lich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "ADJA", "NN", "$.", "KOUS", "PPER", "ADV", "ADV", "ADJD"], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Aller Sch\u00f6nheit, Form und Farben fast beraubet. Nichts", "tokens": ["Al\u00b7ler", "Sch\u00f6n\u00b7heit", ",", "Form", "und", "Far\u00b7ben", "fast", "be\u00b7rau\u00b7bet", ".", "Nichts"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ADJA", "NN", "$,", "NN", "KON", "NN", "ADV", "VVFIN", "$.", "NN"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.4": {"text": "Welche Schuppen-weise sitzet, wird an ihr von uns ge-", "tokens": ["Wel\u00b7che", "Schup\u00b7pen\u00b7wei\u00b7se", "sit\u00b7zet", ",", "wird", "an", "ihr", "von", "uns", "ge"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PWAT", "NN", "VVFIN", "$,", "VAFIN", "APPR", "PPER", "APPR", "PPER", "TRUNC"], "meter": "+-+-+-+--+--+-", "measure": "hexameter"}, "line.5": {"text": "Und doch ist in ihr verborgen, und zwar in so engem Platz,", "tokens": ["Und", "doch", "ist", "in", "ihr", "ver\u00b7bor\u00b7gen", ",", "und", "zwar", "in", "so", "en\u00b7gem", "Platz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "APPR", "PPER", "VVPP", "$,", "KON", "ADV", "APPR", "ADV", "ADJA", "NN", "$,"], "meter": "-+--+-+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Ein nicht gnug zu sch\u00e4tzender und bewunderns wehrter", "tokens": ["Ein", "nicht", "gnug", "zu", "sch\u00e4t\u00b7zen\u00b7der", "und", "be\u00b7wun\u00b7derns", "wehr\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PTKNEG", "ADV", "PTKA", "ADJD", "KON", "ADJA", "NN"], "meter": "+-+-+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.7": {"text": "Von Geruch, von Farb\u2019 und Sch\u00f6nheit. Kann wol aus", "tokens": ["Von", "Ge\u00b7ruch", ",", "von", "Fa\u00b7rb'", "und", "Sch\u00f6n\u00b7heit", ".", "Kann", "wol", "aus"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "APPR", "NN", "KON", "NN", "$.", "VMFIN", "ADV", "APPR"], "meter": "+-+-+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.8": {"text": "Jemand solche sch\u00f6ne Arbeit, ausser einer Gottheit, machen?", "tokens": ["Je\u00b7mand", "sol\u00b7che", "sch\u00f6\u00b7ne", "Ar\u00b7beit", ",", "aus\u00b7ser", "ei\u00b7ner", "Got\u00b7theit", ",", "ma\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PIS", "PIAT", "ADJA", "NN", "$,", "APPR", "ART", "NN", "$,", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}, "stanza.3": {"line.1": {"text": "Wer ist, der nicht erstaunt, wann er mit Ernst be-", "tokens": ["Wer", "ist", ",", "der", "nicht", "er\u00b7staunt", ",", "wann", "er", "mit", "Ernst", "be"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "$,", "PRELS", "PTKNEG", "VVPP", "$,", "PWAV", "PPER", "APPR", "NE", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df die bewegende, die bildende Natur", "tokens": ["Da\u00df", "die", "be\u00b7we\u00b7gen\u00b7de", ",", "die", "bil\u00b7den\u00b7de", "Na\u00b7tur"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "$,", "ART", "ADJA", "NN"], "meter": "-+-+---+-+-+", "measure": "unknown.measure.penta"}, "line.3": {"text": "Solch auserlesene vollkommene Figur,", "tokens": ["Solch", "au\u00b7ser\u00b7le\u00b7se\u00b7ne", "voll\u00b7kom\u00b7me\u00b7ne", "Fi\u00b7gur", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "ADJA", "NN", "$,"], "meter": "-+--+-+-+--+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Gantz sonder Hand, formir\u2019t; wie sie die Z\u00e4ser lencket,", "tokens": ["Gantz", "son\u00b7der", "Hand", ",", "for\u00b7mir't", ";", "wie", "sie", "die", "Z\u00e4\u00b7ser", "len\u00b7cket", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$,", "VVFIN", "$.", "PWAV", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So Kunst-reich, sonder Kunst, und, ohne Regel, recht.", "tokens": ["So", "Kunst\u00b7reich", ",", "son\u00b7der", "Kunst", ",", "und", ",", "oh\u00b7ne", "Re\u00b7gel", ",", "recht", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "KON", "NN", "$,", "KON", "$,", "KOUI", "NN", "$,", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Welch ein unsichtbarer Euclides stellt sich mir", "tokens": ["Welch", "ein", "un\u00b7sicht\u00b7ba\u00b7rer", "Eu\u00b7cli\u00b7des", "stellt", "sich", "mir"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "ART", "ADJA", "NN", "VVFIN", "PRF", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In diesem Wunder-Bau der Samen-Zwiebel f\u00fcr!", "tokens": ["In", "die\u00b7sem", "Wun\u00b7der\u00b7Bau", "der", "Sa\u00b7men\u00b7Zwie\u00b7bel", "f\u00fcr", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "ART", "NN", "APPR", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der nach so netter Maa\u00df sein k\u00fcnstlich Werck bezirckt,", "tokens": ["Der", "nach", "so", "net\u00b7ter", "Maa\u00df", "sein", "k\u00fcnst\u00b7lich", "Werck", "be\u00b7zirckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADV", "ADJA", "NN", "PPOSAT", "ADJD", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und der, nach Linien, so nicht verhanden, wirckt.", "tokens": ["Und", "der", ",", "nach", "Li\u00b7ni\u00b7en", ",", "so", "nicht", "ver\u00b7han\u00b7den", ",", "wirckt", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ART", "$,", "APPR", "NE", "$,", "ADV", "PTKNEG", "ADJD", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Die\u00df f\u00fchret uns gewi\u00df viel tieffer, als es scheinet.", "tokens": ["Die\u00df", "f\u00fch\u00b7ret", "uns", "ge\u00b7wi\u00df", "viel", "tief\u00b7fer", ",", "als", "es", "schei\u00b7net", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "ADV", "ADJD", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es zeigt uns eine Krafft, die gantz auf andre Weise,", "tokens": ["Es", "zeigt", "uns", "ei\u00b7ne", "Krafft", ",", "die", "gantz", "auf", "and\u00b7re", "Wei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "$,", "PRELS", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Zu des allm\u00e4chtigen Lieb-reichen Sch\u00f6pfers Preise,", "tokens": ["Zu", "des", "all\u00b7m\u00e4ch\u00b7ti\u00b7gen", "Lie\u00b7brei\u00b7chen", "Sch\u00f6p\u00b7fers", "Prei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "NN", "$,"], "meter": "-+-+--++-+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Verf\u00e4hrt und wirckt, als wir. Wir werden \u00fcberf\u00fchret,", "tokens": ["Ver\u00b7f\u00e4hrt", "und", "wirckt", ",", "als", "wir", ".", "Wir", "wer\u00b7den", "\u00fc\u00b7berf\u00b7\u00fch\u00b7ret", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "VVFIN", "$,", "KOUS", "PPER", "$.", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Da\u00df, vor der unsrigen, ihr weit der Preis geb\u00fchret.", "tokens": ["Da\u00df", ",", "vor", "der", "uns\u00b7ri\u00b7gen", ",", "ihr", "weit", "der", "Preis", "ge\u00b7b\u00fch\u00b7ret", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "APPR", "ART", "PPOSS", "$,", "PPER", "ADJD", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Wir thun dadurch zugleich", "tokens": ["Wir", "thun", "da\u00b7durch", "zu\u00b7gleich"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PAV", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "In der besch\u00e4fftigten Natur", "tokens": ["In", "der", "be\u00b7sch\u00e4ff\u00b7tig\u00b7ten", "Na\u00b7tur"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sonst unerkanntes Reich", "tokens": ["Sonst", "un\u00b7er\u00b7kann\u00b7tes", "Reich"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Gar einen scharffen Blick. Wir kommen auf die Spur,", "tokens": ["Gar", "ei\u00b7nen", "scharf\u00b7fen", "Blick", ".", "Wir", "kom\u00b7men", "auf", "die", "Spur", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$.", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Da\u00df wir uns k\u00fcnfftighin nicht mehr so sehr vergessen,", "tokens": ["Da\u00df", "wir", "uns", "k\u00fcnff\u00b7tig\u00b7hin", "nicht", "mehr", "so", "sehr", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "PTKNEG", "ADV", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und alle Kr\u00e4ffte blo\u00df nach unserm Leisten messen.", "tokens": ["Und", "al\u00b7le", "Kr\u00e4ff\u00b7te", "blo\u00df", "nach", "un\u00b7serm", "Leis\u00b7ten", "mes\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Es ist vielmehr, was Menschen hier verrichten,", "tokens": ["Es", "ist", "viel\u00b7mehr", ",", "was", "Men\u00b7schen", "hier", "ver\u00b7rich\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "$,", "PWS", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Allein nur eine Art von Kr\u00e4fften, der die Welt", "tokens": ["Al\u00b7lein", "nur", "ei\u00b7ne", "Art", "von", "Kr\u00e4ff\u00b7ten", ",", "der", "die", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "NN", "APPR", "NN", "$,", "PRELS", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Vielleicht viel tausend in sich h\u00e4lt,", "tokens": ["Viel\u00b7leicht", "viel", "tau\u00b7send", "in", "sich", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "APPR", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Und kann die unsrige mit nichten", "tokens": ["Und", "kann", "die", "uns\u00b7ri\u00b7ge", "mit", "nich\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "ART", "PPOSS", "APPR", "PIS"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Der andern Richtschnur seyn.", "tokens": ["Der", "an\u00b7dern", "Richt\u00b7schnur", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Was der Menschen Seele wircket, heisst man Kunst,", "tokens": ["Was", "der", "Men\u00b7schen", "See\u00b7le", "wir\u00b7cket", ",", "heisst", "man", "Kunst", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "NN", "VVFIN", "$,", "VVFIN", "PIS", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Sie von dem, was die Natur wirckt, bereitet, ziert und", "tokens": ["Sie", "von", "dem", ",", "was", "die", "Na\u00b7tur", "wirckt", ",", "be\u00b7rei\u00b7tet", ",", "ziert", "und"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PPER", "APPR", "ART", "$,", "PRELS", "ART", "NN", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "KON"], "meter": "---+--+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Als wenn, sonder alle Kunst, sonder Zweck, ein Ungefehr", "tokens": ["Als", "wenn", ",", "son\u00b7der", "al\u00b7le", "Kunst", ",", "son\u00b7der", "Zweck", ",", "ein", "Un\u00b7ge\u00b7fehr"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "KOUS", "$,", "KON", "PIAT", "NN", "$,", "KON", "NN", "$,", "ART", "NN"], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Von den Wercken der Natur blo\u00df ein blinder Leiter w\u00e4r.", "tokens": ["Von", "den", "Wer\u00b7cken", "der", "Na\u00b7tur", "blo\u00df", "ein", "blin\u00b7der", "Lei\u00b7ter", "w\u00e4r", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "ADV", "ART", "ADJA", "NN", "VAFIN", "$."], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.5": {"text": "Dieser vorgefasste Wahn schadet uns mehr, als man meinet,", "tokens": ["Die\u00b7ser", "vor\u00b7ge\u00b7fass\u00b7te", "Wahn", "scha\u00b7det", "