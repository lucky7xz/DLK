{"textgrid.poem.67954": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "22. Pallast des Fr\u00fchlings", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Alle T\u00f6chter der Aurora,", "tokens": ["Al\u00b7le", "T\u00f6ch\u00b7ter", "der", "Au\u00b7ro\u00b7ra", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ART", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Alle Blumen in dem Garten,", "tokens": ["Al\u00b7le", "Blu\u00b7men", "in", "dem", "Gar\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Standen hoffend, standen wartend", "tokens": ["Stan\u00b7den", "hof\u00b7fend", ",", "stan\u00b7den", "war\u00b7tend"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NN", "VVPP", "$,", "VVFIN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Auf die k\u00f6nigliche Rose.", "tokens": ["Auf", "die", "k\u00f6\u00b7nig\u00b7li\u00b7che", "Ro\u00b7se", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Und da ging sie majest\u00e4tisch", "tokens": ["Und", "da", "ging", "sie", "ma\u00b7jes\u00b7t\u00e4\u00b7tisch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADJD"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Auf, auf ihrem gr\u00fcnen Throne.", "tokens": ["Auf", ",", "auf", "ih\u00b7rem", "gr\u00fc\u00b7nen", "Thro\u00b7ne", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$,", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Rings um ihren K\u00f6nigspurpur", "tokens": ["Rings", "um", "ih\u00b7ren", "K\u00f6\u00b7nigs\u00b7pur\u00b7pur"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Stand der Dornen scharfe Wache.", "tokens": ["Stand", "der", "Dor\u00b7nen", "schar\u00b7fe", "Wa\u00b7che", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Und sie blickte liebreich nieder,", "tokens": ["Und", "sie", "blick\u00b7te", "lieb\u00b7reich", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADJD", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sie gebildet von der Liebe,", "tokens": ["Sie", "ge\u00b7bil\u00b7det", "von", "der", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und die Blumen alle neigend", "tokens": ["Und", "die", "Blu\u00b7men", "al\u00b7le", "nei\u00b7gend"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gr\u00fcssen sie mit stummer Ehrfurcht.", "tokens": ["Gr\u00fcs\u00b7sen", "sie", "mit", "stum\u00b7mer", "Ehr\u00b7furcht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Die bewundert ihre Sch\u00f6nheit,", "tokens": ["Die", "be\u00b7wun\u00b7dert", "ih\u00b7re", "Sch\u00f6n\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Jene liebet ihre G\u00fcte,", "tokens": ["Je\u00b7ne", "lie\u00b7bet", "ih\u00b7re", "G\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Diese buhlt um ihre Gnade,", "tokens": ["Die\u00b7se", "buhlt", "um", "ih\u00b7re", "Gna\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hundert neiden ihre Reize.", "tokens": ["Hun\u00b7dert", "nei\u00b7den", "ih\u00b7re", "Rei\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Und der Amor ihrer aller,", "tokens": ["Und", "der", "A\u00b7mor", "ih\u00b7rer", "al\u00b7ler", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NE", "PPOSAT", "PIAT", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der sie alle liebgewinnet,", "tokens": ["Der", "sie", "al\u00b7le", "lieb\u00b7ge\u00b7win\u00b7net", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PIS", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Allen ihre S\u00fcsse raubet,", "tokens": ["Al\u00b7len", "ih\u00b7re", "S\u00fcs\u00b7se", "rau\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und nur mit dem Stachel lohnet,", "tokens": ["Und", "nur", "mit", "dem", "Sta\u00b7chel", "loh\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Summend kam die freche Biene,", "tokens": ["Sum\u00b7mend", "kam", "die", "fre\u00b7che", "Bie\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "L\u00fcstend auch nach ihrem Busen;", "tokens": ["L\u00fcs\u00b7tend", "auch", "nach", "ih\u00b7rem", "Bu\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch Ein Blick verjagt den R\u00e4uber,", "tokens": ["Doch", "Ein", "Blick", "ver\u00b7jagt", "den", "R\u00e4u\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und verschlo\u00df den keuschen Busen.", "tokens": ["Und", "ver\u00b7schlo\u00df", "den", "keu\u00b7schen", "Bu\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Und die Nelken stehen neidig,", "tokens": ["Und", "die", "Nel\u00b7ken", "ste\u00b7hen", "nei\u00b7dig", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "(prinzessinnen von Gebl\u00fcte.)", "tokens": ["(", "prin\u00b7zes\u00b7sin\u00b7nen", "von", "Ge\u00b7bl\u00fc\u00b7te", ".", ")"], "token_info": ["punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "APPR", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Die Jasmine, deren weisse", "tokens": ["Die", "Jas\u00b7mi\u00b7ne", ",", "de\u00b7ren", "weis\u00b7se"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "PRELAT", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Frische selbst die Venus heuchelt,", "tokens": ["Fri\u00b7sche", "selbst", "die", "Ve\u00b7nus", "heu\u00b7chelt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Die Narcisse bei der Quelle,", "tokens": ["Die", "Nar\u00b7cis\u00b7se", "bei", "der", "Quel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die nur sie, nicht sich mehr siehet;", "tokens": ["Die", "nur", "sie", ",", "nicht", "sich", "mehr", "sie\u00b7het", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PPER", "$,", "PTKNEG", "PRF", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und die Lilie der Unschuld,", "tokens": ["Und", "die", "Li\u00b7lie", "der", "Un\u00b7schuld", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Schmachtend in der Liebe Thr\u00e4nen.", "tokens": ["Schmach\u00b7tend", "in", "der", "Lie\u00b7be", "Thr\u00e4\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Hyacinthen, Anemonen,", "tokens": ["Hya\u00b7cin\u00b7then", ",", "A\u00b7ne\u00b7mo\u00b7nen", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und die Damen ihres Hofes", "tokens": ["Und", "die", "Da\u00b7men", "ih\u00b7res", "Ho\u00b7fes"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Spr\u00f6de Tulpen, die nicht duften,", "tokens": ["Spr\u00f6\u00b7de", "Tul\u00b7pen", ",", "die", "nicht", "duf\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "PRELS", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Aber prangen und stolzieren \u2013", "tokens": ["A\u00b7ber", "pran\u00b7gen", "und", "stol\u00b7zie\u00b7ren", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Alle stehen, alle warten,", "tokens": ["Al\u00b7le", "ste\u00b7hen", ",", "al\u00b7le", "war\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "PIS", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Welche Freundin sie erw\u00e4hle?", "tokens": ["Wel\u00b7che", "Freun\u00b7din", "sie", "er\u00b7w\u00e4h\u00b7le", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAT", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und sie w\u00e4hlt das stille Veilchen,", "tokens": ["Und", "sie", "w\u00e4hlt", "das", "stil\u00b7le", "Veil\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Aller Blumen Erstgebohrne,", "tokens": ["Al\u00b7ler", "Blu\u00b7men", "Erst\u00b7ge\u00b7bohr\u00b7ne", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Das im Grase sich verh\u00fcllet,", "tokens": ["Das", "im", "Gra\u00b7se", "sich", "ver\u00b7h\u00fcl\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPRART", "NN", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und schon, eh es da ist, duftet,", "tokens": ["Und", "schon", ",", "eh", "es", "da", "ist", ",", "duf\u00b7tet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ADV", "$,", "KOUS", "PPER", "ADV", "VAFIN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Duftet fr\u00fche Lenzerquickung,", "tokens": ["Duf\u00b7tet", "fr\u00fc\u00b7he", "Len\u00b7zer\u00b7quic\u00b7kung", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und die Hoffnung aller Schwestern.", "tokens": ["Und", "die", "Hoff\u00b7nung", "al\u00b7ler", "Schwes\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PIAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Alsobald im Lorbeerwalde", "tokens": ["Al\u00b7so\u00b7bald", "im", "Lor\u00b7beer\u00b7wal\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["ADV", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ihres K\u00f6nigsparadieses", "tokens": ["Ih\u00b7res", "K\u00f6\u00b7nigs\u00b7pa\u00b7ra\u00b7die\u00b7ses"], "token_info": ["word", "word"], "pos": ["PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Fangen jauchzend vor Entz\u00fcckung", "tokens": ["Fan\u00b7gen", "jauch\u00b7zend", "vor", "Ent\u00b7z\u00fc\u00b7ckung"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVPP", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nachtigallen an zu schlagen;", "tokens": ["Nach\u00b7ti\u00b7gal\u00b7len", "an", "zu", "schla\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Und so oft im gr\u00fcnen Fr\u00fchling", "tokens": ["Und", "so", "oft", "im", "gr\u00fc\u00b7nen", "Fr\u00fch\u00b7ling"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dieser Pallast wiederkehret,", "tokens": ["Die\u00b7ser", "Pal\u00b7last", "wie\u00b7der\u00b7keh\u00b7ret", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Singen Sch\u00e4ferin und Sch\u00e4fer", "tokens": ["Sin\u00b7gen", "Sch\u00e4\u00b7fe\u00b7rin", "und", "Sch\u00e4\u00b7fer"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "KON", "NE"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nur das Veilchen und die Rose.", "tokens": ["Nur", "das", "Veil\u00b7chen", "und", "die", "Ro\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "Alle T\u00f6chter der Aurora,", "tokens": ["Al\u00b7le", "T\u00f6ch\u00b7ter", "der", "Au\u00b7ro\u00b7ra", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ART", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Alle Blumen in dem Garten,", "tokens": ["Al\u00b7le", "Blu\u00b7men", "in", "dem", "Gar\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Standen hoffend, standen wartend", "tokens": ["Stan\u00b7den", "hof\u00b7fend", ",", "stan\u00b7den", "war\u00b7tend"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NN", "VVPP", "$,", "VVFIN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Auf die k\u00f6nigliche Rose.", "tokens": ["Auf", "die", "k\u00f6\u00b7nig\u00b7li\u00b7che", "Ro\u00b7se", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.15": {"line.1": {"text": "Und da ging sie majest\u00e4tisch", "tokens": ["Und", "da", "ging", "sie", "ma\u00b7jes\u00b7t\u00e4\u00b7tisch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADJD"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Auf, auf ihrem gr\u00fcnen Throne.", "tokens": ["Auf", ",", "auf", "ih\u00b7rem", "gr\u00fc\u00b7nen", "Thro\u00b7ne", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$,", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Rings um ihren K\u00f6nigspurpur", "tokens": ["Rings", "um", "ih\u00b7ren", "K\u00f6\u00b7nigs\u00b7pur\u00b7pur"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+--+", "measure"