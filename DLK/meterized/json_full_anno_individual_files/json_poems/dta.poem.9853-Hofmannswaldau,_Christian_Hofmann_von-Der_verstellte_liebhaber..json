{"dta.poem.9853": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Der verstellte liebhaber.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mein kind/ la\u00df uns fein heimlich lieben/", "tokens": ["Mein", "kind", "/", "la\u00df", "uns", "fein", "heim\u00b7lich", "lie\u00b7ben", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "VVIMP", "PPER", "ADJD", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nicht wie es sonst pflegt zu geschehn;", "tokens": ["Nicht", "wie", "es", "sonst", "pflegt", "zu", "ge\u00b7schehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PWAV", "PPER", "ADV", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wir m\u00fcssen unsre lust verschieben/", "tokens": ["Wir", "m\u00fcs\u00b7sen", "uns\u00b7re", "lust", "ver\u00b7schie\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So offt es andre leute sehn;", "tokens": ["So", "offt", "es", "and\u00b7re", "leu\u00b7te", "sehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wir m\u00fcssen uns ein wenig dr\u00fccken", "tokens": ["Wir", "m\u00fcs\u00b7sen", "uns", "ein", "we\u00b7nig", "dr\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "ART", "PIS", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und lernen in die leute schicken.", "tokens": ["Und", "ler\u00b7nen", "in", "die", "leu\u00b7te", "schi\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wir wollen so zusammen halten/", "tokens": ["Wir", "wol\u00b7len", "so", "zu\u00b7sam\u00b7men", "hal\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df niemand uns verrathen kan;", "tokens": ["Da\u00df", "nie\u00b7mand", "uns", "ver\u00b7ra\u00b7then", "kan", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn du mich fiehst die h\u00e4nde falten/", "tokens": ["Wenn", "du", "mich", "fiehst", "die", "h\u00e4n\u00b7de", "fal\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVFIN", "ART", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So bet ich deine sch\u00f6nheit an;", "tokens": ["So", "bet", "ich", "dei\u00b7ne", "sch\u00f6n\u00b7heit", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wenn meine arme sich bewegen/", "tokens": ["Wenn", "mei\u00b7ne", "ar\u00b7me", "sich", "be\u00b7we\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "PRF", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "So w\u00fcnsch ich dich herein zu legen.", "tokens": ["So", "w\u00fcnsch", "ich", "dich", "her\u00b7ein", "zu", "le\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "PTKVZ", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Schlag\u2019 ich die augen in die h\u00f6he/", "tokens": ["Schlag'", "ich", "die", "au\u00b7gen", "in", "die", "h\u00f6\u00b7he", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So gehn die seuftzer \u00fcber sich;", "tokens": ["So", "gehn", "die", "seuft\u00b7zer", "\u00fc\u00b7ber", "sich", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "APPR", "PRF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und wenn ich f\u00fcr mich niedersehe/", "tokens": ["Und", "wenn", "ich", "f\u00fcr", "mich", "nie\u00b7der\u00b7se\u00b7he", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "APPR", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So gr\u00fcsset mein gehorsam dich.", "tokens": ["So", "gr\u00fcs\u00b7set", "mein", "ge\u00b7hor\u00b7sam", "dich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "ADJD", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Merck/ wenn ich an die lippen r\u00fchre/", "tokens": ["Merck", "/", "wenn", "ich", "an", "die", "lip\u00b7pen", "r\u00fch\u00b7re", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df durch die lufft ich k\u00fcsse f\u00fchre.", "tokens": ["Da\u00df", "durch", "die", "lufft", "ich", "k\u00fcs\u00b7se", "f\u00fch\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "PPER", "VVFIN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Wenn ich mit meinen fingern spiele/", "tokens": ["Wenn", "ich", "mit", "mei\u00b7nen", "fin\u00b7gern", "spie\u00b7le", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "VVINF", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So dr\u00fcck ich gleichsam deine hand:", "tokens": ["So", "dr\u00fcck", "ich", "gleich\u00b7sam", "dei\u00b7ne", "hand", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und wenn ich an die stirne f\u00fchle/", "tokens": ["Und", "wenn", "ich", "an", "die", "stir\u00b7ne", "f\u00fch\u00b7le", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "APPR", "ART", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Bedeut es heimlichen verstand/", "tokens": ["Be\u00b7deut", "es", "heim\u00b7li\u00b7chen", "ver\u00b7stand", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVINF", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ja ihre stellung vor den leuten", "tokens": ["Ja", "ih\u00b7re", "stel\u00b7lung", "vor", "den", "leu\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "PPOSAT", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Mu\u00df etwas sonderlichs bedeuten.", "tokens": ["Mu\u00df", "et\u00b7was", "son\u00b7der\u00b7lichs", "be\u00b7deu\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Kein mensch soll mercken/ was ich mache/", "tokens": ["Kein", "mensch", "soll", "mer\u00b7cken", "/", "was", "ich", "ma\u00b7che", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "VVINF", "$(", "PWS", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und wie es um uns beyde steh\u2019/", "tokens": ["Und", "wie", "es", "um", "uns", "bey\u00b7de", "steh'", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "APPR", "PPER", "PIS", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich gehe traurig wenn ich lache/", "tokens": ["Ich", "ge\u00b7he", "trau\u00b7rig", "wenn", "ich", "la\u00b7che", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "KOUS", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und lache wenn ich traurig geh\u2019:", "tokens": ["Und", "la\u00b7che", "wenn", "ich", "trau\u00b7rig", "geh'", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KOUS", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Aus mir kan keinem was erhellen/", "tokens": ["Aus", "mir", "kan", "kei\u00b7nem", "was", "er\u00b7hel\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VMFIN", "PIS", "PWS", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ich kan mich stellen und verstellen.", "tokens": ["Ich", "kan", "mich", "stel\u00b7len", "und", "ver\u00b7stel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "VVINF", "KON", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Wir beyde reden ohne zungen/", "tokens": ["Wir", "bey\u00b7de", "re\u00b7den", "oh\u00b7ne", "zun\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "VVINF", "APPR", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vernehmen uns auch ohngefehr;", "tokens": ["Ver\u00b7neh\u00b7men", "uns", "auch", "ohn\u00b7ge\u00b7fehr", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wirstu zu tadeln mich gezwungen/", "tokens": ["Wirs\u00b7tu", "zu", "ta\u00b7deln", "mich", "ge\u00b7zwun\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VVINF", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Halt ich es doch f\u00fcr eine ehr;", "tokens": ["Halt", "ich", "es", "doch", "f\u00fcr", "ei\u00b7ne", "ehr", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPER", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Du wirst es auch nicht \u00fcbel nehmen/", "tokens": ["Du", "wirst", "es", "auch", "nicht", "\u00fc\u00b7bel", "neh\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "PTKNEG", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wenn ich aus noth dich mu\u00df besch\u00e4men.", "tokens": ["Wenn", "ich", "aus", "noth", "dich", "mu\u00df", "be\u00b7sch\u00e4\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "H\u00f6rst du mich/ oder ich dich/ nennen/", "tokens": ["H\u00f6rst", "du", "mich", "/", "o\u00b7der", "ich", "dich", "/", "nen\u00b7nen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "$(", "KON", "PPER", "PRF", "$(", "VVINF", "$("], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Wird keine r\u00f6th uns abgejagt;", "tokens": ["Wird", "kei\u00b7ne", "r\u00f6th", "uns", "ab\u00b7ge\u00b7jagt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wir thun als wenn wir uns nicht kennen/", "tokens": ["Wir", "thun", "als", "wenn", "wir", "uns", "nicht", "ken\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "KOUS", "PPER", "PRF", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und wissen nicht was jener sagt:", "tokens": ["Und", "wis\u00b7sen", "nicht", "was", "je\u00b7ner", "sagt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "PWS", "PDS", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Vexirt man uns/ so braucht man l\u00fcgen", "tokens": ["Ve\u00b7xirt", "man", "uns", "/", "so", "braucht", "man", "l\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "PIS", "PPER", "$(", "ADV", "VVFIN", "PIS", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Sich mit der warheit zu begn\u00fcgen.", "tokens": ["Sich", "mit", "der", "war\u00b7heit", "zu", "be\u00b7gn\u00fc\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Nun dieses wollen wir so treiben", "tokens": ["Nun", "die\u00b7ses", "wol\u00b7len", "wir", "so", "trei\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PDS", "VMFIN", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und uns so lieben unvermerckt", "tokens": ["Und", "uns", "so", "lie\u00b7ben", "un\u00b7ver\u00b7merckt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und immer bey dem l\u00e4ugnen bleiben/", "tokens": ["Und", "im\u00b7mer", "bey", "dem", "l\u00e4ug\u00b7nen", "blei\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ART", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Bi\u00df unser bl\u00f6digkeit sich st\u00e4rckt;", "tokens": ["Bi\u00df", "un\u00b7ser", "bl\u00f6\u00b7dig\u00b7keit", "sich", "st\u00e4rckt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Das aber kan so offt geschehen/", "tokens": ["Das", "a\u00b7ber", "kan", "so", "offt", "ge\u00b7sche\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "VMFIN", "ADV", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "So offt wir uns alleine sehen.", "tokens": ["So", "offt", "wir", "uns", "al\u00b7lei\u00b7ne", "se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "PRF", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}