{"dta.poem.18455": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auf den Stax.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1703", "urn": "urn:nbn:de:kobv:b4-200905199360", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Stax/ ein pennal/ wolt auch mit andern Burschen reiten/", "tokens": ["Stax", "/", "ein", "pen\u00b7nal", "/", "wolt", "auch", "mit", "an\u00b7dern", "Bur\u00b7schen", "rei\u00b7ten", "/"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "ART", "NN", "$(", "VMFIN", "ADV", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und wuste doch nicht recht den wallach zu beschreiten/", "tokens": ["Und", "wus\u00b7te", "doch", "nicht", "recht", "den", "wal\u00b7lach", "zu", "be\u00b7schrei\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PTKNEG", "ADJD", "ART", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der etwas k\u00fctzlich war/ und lief den andern vor.", "tokens": ["Der", "et\u00b7was", "k\u00fctz\u00b7lich", "war", "/", "und", "lief", "den", "an\u00b7dern", "vor", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "ADJD", "VAFIN", "$(", "KON", "VVFIN", "ART", "ADJA", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sacht/ sacht? rief man ihm zu; wie kan ich/ sprach der thor/", "tokens": ["Sacht", "/", "sacht", "?", "rief", "man", "ihm", "zu", ";", "wie", "kan", "ich", "/", "sprach", "der", "thor", "/"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$(", "VVFIN", "$.", "VVFIN", "PIS", "PPER", "PTKVZ", "$.", "PWAV", "VMFIN", "PPER", "$(", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der teufel/ glaub ich/ ist im pferd/ und hilft ihm gehen/", "tokens": ["Der", "teu\u00b7fel", "/", "glaub", "ich", "/", "ist", "im", "pferd", "/", "und", "hilft", "ihm", "ge\u00b7hen", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "VVFIN", "PPER", "$(", "VAFIN", "APPRART", "NN", "$(", "KON", "VVFIN", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ich stech es/ was ich kan/ das aas wil doch nicht stehen.", "tokens": ["Ich", "stech", "es", "/", "was", "ich", "kan", "/", "das", "aas", "wil", "doch", "nicht", "ste\u00b7hen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$(", "PWS", "PPER", "VMFIN", "$(", "ART", "NN", "VMFIN", "ADV", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}