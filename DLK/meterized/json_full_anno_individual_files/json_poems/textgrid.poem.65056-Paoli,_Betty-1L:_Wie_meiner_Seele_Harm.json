{"textgrid.poem.65056": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie meiner Seele Harm", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie meiner Seele Harm", "tokens": ["Wie", "mei\u00b7ner", "See\u00b7le", "Harm"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "PPOSAT", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Vermittelnd zu besiegen?", "tokens": ["Ver\u00b7mit\u00b7telnd", "zu", "be\u00b7sie\u00b7gen", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "O la\u00df' in deinem Arm", "tokens": ["O", "la\u00df'", "in", "dei\u00b7nem", "Arm"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Vergessensfroh mich liegen.", "tokens": ["Ver\u00b7ges\u00b7sens\u00b7froh", "mich", "lie\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "O la\u00df', wenn neu erwacht", "tokens": ["O", "la\u00df'", ",", "wenn", "neu", "er\u00b7wacht"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "PTKVZ", "$,", "KOUS", "ADJD", "VVPP"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Ein schmerzliches Gedenken,", "tokens": ["Ein", "schmerz\u00b7li\u00b7ches", "Ge\u00b7den\u00b7ken", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "In deines Auges Nacht", "tokens": ["In", "dei\u00b7nes", "Au\u00b7ges", "Nacht"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Die Seele mich versenken.", "tokens": ["Die", "See\u00b7le", "mich", "ver\u00b7sen\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und will in seiner Pein", "tokens": ["Und", "will", "in", "sei\u00b7ner", "Pein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Mein Herz ersch\u00f6pft verbluten,", "tokens": ["Mein", "Herz", "er\u00b7sch\u00f6pft", "ver\u00b7blu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Dann hauch' ihm Leben ein,", "tokens": ["Dann", "hauch'", "ihm", "Le\u00b7ben", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Mit deines Kusses Gluthen.", "tokens": ["Mit", "dei\u00b7nes", "Kus\u00b7ses", "Glu\u00b7then", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Doch keinen, keinen Schwur!", "tokens": ["Doch", "kei\u00b7nen", ",", "kei\u00b7nen", "Schwur", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PIAT", "$,", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Meinst du, da\u00df ich ihm traute?", "tokens": ["Meinst", "du", ",", "da\u00df", "ich", "ihm", "trau\u00b7te", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "PPER", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Er mahnte mich doch nur", "tokens": ["Er", "mahn\u00b7te", "mich", "doch", "nur"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "An hingeschwund'ne Laute.", "tokens": ["An", "hin\u00b7ge\u00b7schwun\u00b7d'\u00b7ne", "Lau\u00b7te", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.5": {"line.1": {"text": "An Laute, die vom Strand", "tokens": ["An", "Lau\u00b7te", ",", "die", "vom", "Strand"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "PRELS", "APPRART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Mich lockten auf die Wellen,", "tokens": ["Mich", "lock\u00b7ten", "auf", "die", "Wel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Bis ich am Klippenrand", "tokens": ["Bis", "ich", "am", "Klip\u00b7pen\u00b7rand"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPRART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Mein Fahrzeug sah zerschellen.", "tokens": ["Mein", "Fahr\u00b7zeug", "sah", "zer\u00b7schel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Die wild emp\u00f6rte Fluth", "tokens": ["Die", "wild", "em\u00b7p\u00f6r\u00b7te", "Fluth"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJD", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Kannst du zur Ruh' nicht sprechen;", "tokens": ["Kannst", "du", "zur", "Ruh'", "nicht", "spre\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPRART", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich wei\u00df zu gut, zu gut,", "tokens": ["Ich", "wei\u00df", "zu", "gut", ",", "zu", "gut", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKA", "ADJD", "$,", "PTKA", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Wie leicht ein Schwur zu brechen.", "tokens": ["Wie", "leicht", "ein", "Schwur", "zu", "bre\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Was gl\u00fchend du verneinst,", "tokens": ["Was", "gl\u00fc\u00b7hend", "du", "ver\u00b7neinst", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Schon keimt's in dunkeln Saaten;", "tokens": ["Schon", "keimt's", "in", "dun\u00b7keln", "Saa\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich wei\u00df, du wirst mich einst", "tokens": ["Ich", "wei\u00df", ",", "du", "wirst", "mich", "einst"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VAFIN", "PPER", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Verlassen und verrathen.", "tokens": ["Ver\u00b7las\u00b7sen", "und", "ver\u00b7ra\u00b7then", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "KON", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Das wei\u00df ich, ach! und kann", "tokens": ["Das", "wei\u00df", "ich", ",", "ach", "!", "und", "kann"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PDS", "VVFIN", "PPER", "$,", "ITJ", "$.", "KON", "VMFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Ihn nimmermehr doch heben", "tokens": ["Ihn", "nim\u00b7mer\u00b7mehr", "doch", "he\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "ADV", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Den m\u00e4cht'gen Zauberbann,", "tokens": ["Den", "m\u00e4cht'\u00b7gen", "Zau\u00b7ber\u00b7bann", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Der mich dir hingegeben. \u2013", "tokens": ["Der", "mich", "dir", "hin\u00b7ge\u00b7ge\u00b7ben", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "PPER", "PPER", "VVPP", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Ein W\u00fcstenwand'rer trifft", "tokens": ["Ein", "W\u00fcs\u00b7ten\u00b7wan\u00b7d'\u00b7rer", "trifft"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVFIN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Im Sand auf eine Quelle;", "tokens": ["Im", "Sand", "auf", "ei\u00b7ne", "Quel\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und b\u00f6the sie ihm Gift,", "tokens": ["Und", "b\u00f6\u00b7the", "sie", "ihm", "Gift", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPER", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Er tr\u00e4nk' die gift'ge Welle.", "tokens": ["Er", "tr\u00e4nk'", "die", "gift'\u00b7ge", "Wel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Denn leichter ist zumahl", "tokens": ["Denn", "leich\u00b7ter", "ist", "zu\u00b7mahl"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJD", "VAFIN", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Ein rasch hinlodernd Sterben,", "tokens": ["Ein", "rasch", "hin\u00b7lo\u00b7dernd", "Ster\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVPP", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Als in des Durstes Qual", "tokens": ["Als", "in", "des", "Durs\u00b7tes", "Qual"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Vorkommend zu verderben.", "tokens": ["Vor\u00b7kom\u00b7mend", "zu", "ver\u00b7der\u00b7ben", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PTKZU", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "So bin in dunkler Stund'", "tokens": ["So", "bin", "in", "dunk\u00b7ler", "Stund'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Ich an dein Herz gesunken,", "tokens": ["Ich", "an", "dein", "Herz", "ge\u00b7sun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und hab' von deinem Mund'", "tokens": ["Und", "hab'", "von", "dei\u00b7nem", "Mund'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Den Untergang getrunken.", "tokens": ["Den", "Un\u00b7ter\u00b7gang", "ge\u00b7trun\u00b7ken", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "In unermess'ner Noth", "tokens": ["In", "un\u00b7er\u00b7mess'\u00b7ner", "Noth"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Bist du mir so begegnet,", "tokens": ["Bist", "du", "mir", "so", "be\u00b7geg\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und giebst du mir den Tod,", "tokens": ["Und", "giebst", "du", "mir", "den", "Tod", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Sei dennoch mir gesegnet!", "tokens": ["Sei", "den\u00b7noch", "mir", "ge\u00b7seg\u00b7net", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPER", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Das Gl\u00fcck der Welt erbla\u00dft", "tokens": ["Das", "Gl\u00fcck", "der", "Welt", "er\u00b7bla\u00dft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Von dem, das mir erglommen \u2013", "tokens": ["Von", "dem", ",", "das", "mir", "er\u00b7glom\u00b7men", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$,", "PRELS", "PPER", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Jetzt harr' ich, stumm gefa\u00dft", "tokens": ["Jetzt", "harr'", "ich", ",", "stumm", "ge\u00b7fa\u00dft"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ADJD", "VVPP"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Der Schmerzen, die da kommen! \u2013", "tokens": ["Der", "Schmer\u00b7zen", ",", "die", "da", "kom\u00b7men", "!", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Wie meiner Seele Harm", "tokens": ["Wie", "mei\u00b7ner", "See\u00b7le", "Harm"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "PPOSAT", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Vermittelnd zu besiegen?", "tokens": ["Ver\u00b7mit\u00b7telnd", "zu", "be\u00b7sie\u00b7gen", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "O la\u00df' in deinem Arm", "tokens": ["O", "la\u00df'", "in", "dei\u00b7nem", "Arm"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Vergessensfroh mich liegen.", "tokens": ["Ver\u00b7ges\u00b7sens\u00b7froh", "mich", "lie\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "O la\u00df', wenn neu erwacht", "tokens": ["O", "la\u00df'", ",", "wenn", "neu", "er\u00b7wacht"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "PTKVZ", "$,", "KOUS"