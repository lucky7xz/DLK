{"textgrid.poem.43490": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich habe nicht umsonst gerungen,", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich habe nicht umsonst gerungen,", "tokens": ["Ich", "ha\u00b7be", "nicht", "um\u00b7sonst", "ge\u00b7run\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Umsonst gedichtet und gestrebt,", "tokens": ["Um\u00b7sonst", "ge\u00b7dich\u00b7tet", "und", "ge\u00b7strebt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "KON", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich habe Etwas mir ersungen", "tokens": ["Ich", "ha\u00b7be", "Et\u00b7was", "mir", "er\u00b7sun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Was noch den Dichter \u00fcberlebt.", "tokens": ["Was", "noch", "den", "Dich\u00b7ter", "\u00fc\u00b7ber\u00b7lebt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Und wenn ich nichts behalten werde,", "tokens": ["Und", "wenn", "ich", "nichts", "be\u00b7hal\u00b7ten", "wer\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PIS", "VVINF", "VAFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Bleibt mir der kleinen Lieder Ruhm;", "tokens": ["Bleibt", "mir", "der", "klei\u00b7nen", "Lie\u00b