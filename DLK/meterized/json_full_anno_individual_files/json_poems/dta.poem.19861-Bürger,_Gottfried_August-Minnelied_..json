{"dta.poem.19861": {"metadata": {"author": {"name": "B\u00fcrger, Gottfried August", "birth": "N.A.", "death": "N.A."}, "title": "Minnelied .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1778", "urn": "urn:nbn:de:kobv:b4-20090519672", "language": ["de:0.99"], "booktitle": "B\u00fcrger, Gottfried August: Gedichte. G\u00f6ttingen, 1778."}, "poem": {"stanza.1": {"line.1": {"text": "O wie sch\u00f6n ist, die ich minne,               ", "tokens": ["O", "wie", "sch\u00f6n", "ist", ",", "die", "ich", "min\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "ADJD", "VAFIN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "O wie sch\u00f6n, an Seel\u2019 und Leib!", "tokens": ["O", "wie", "sch\u00f6n", ",", "an", "Seel'", "und", "Leib", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "ADJD", "$,", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Oefters ahndet meinem Sinne,", "tokens": ["Oef\u00b7ters", "ahn\u00b7det", "mei\u00b7nem", "Sin\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mira sey kein sterblich Weib.", "tokens": ["Mi\u00b7ra", "sey", "kein", "sterb\u00b7lich", "Weib", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PIAT", "ADJD", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Schier verkl\u00e4rt, wie Himmelsbr\u00e4ute,", "tokens": ["Schier", "ver\u00b7kl\u00e4rt", ",", "wie", "Him\u00b7mels\u00b7br\u00e4u\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVPP", "$,", "PWAV", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ist sie aller Flecken baar.", "tokens": ["Ist", "sie", "al\u00b7ler", "Fle\u00b7cken", "baar", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PIAT", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Heiliger und sch\u00f6ner war", "tokens": ["Hei\u00b7li\u00b7ger", "und", "sch\u00f6\u00b7ner", "war"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "KON", "ADJD", "VAFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Nur die Hochgebenedeite,", "tokens": ["Nur", "die", "Hoch\u00b7ge\u00b7be\u00b7ne\u00b7dei\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Die den Heiland uns gebar.", "tokens": ["Die", "den", "Hei\u00b7land", "uns", "ge\u00b7bar", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "PPER", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}