{"textgrid.poem.58667": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "99.", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Denk, Seele, dies ist Gottes Wort, der Mund des Herren schw\u00f6ret,", "tokens": ["Denk", ",", "See\u00b7le", ",", "dies", "ist", "Got\u00b7tes", "Wort", ",", "der", "Mund", "des", "Her\u00b7ren", "schw\u00f6\u00b7ret", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "PDS", "VAFIN", "NN", "NN", "$,", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "Da\u00df er, so wahr er selber lebt, nicht deinen Tod begehret!", "tokens": ["Da\u00df", "er", ",", "so", "wahr", "er", "sel\u00b7ber", "lebt", ",", "nicht", "dei\u00b7nen", "Tod", "be\u00b7ge\u00b7hret", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "ADV", "ADJD", "PPER", "ADV", "VVFIN", "$,", "PTKNEG", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+--+", "measure": "iambic.septa.chol"}, "line.3": {"text": "W\u00e4r' dein Verderben noch so gro\u00df, trau nur, so hat's nicht Not,", "tokens": ["W\u00e4r'", "dein", "Ver\u00b7der\u00b7ben", "noch", "so", "gro\u00df", ",", "trau", "nur", ",", "so", "hat's", "nicht", "Not", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ADV", "ADV", "ADJD", "$,", "VVFIN", "ADV", "$,", "ADV", "VAFIN", "PTKNEG", "NN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "H\u00f6r, Gott hat kein'n Gefallen auch an des Gottlosen Tod,", "tokens": ["H\u00f6r", ",", "Gott", "hat", "kein'n", "Ge\u00b7fal\u00b7len", "auch", "an", "des", "Gott\u00b7lo\u00b7sen", "Tod", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "VAFIN", "PIAT", "NN", "ADV", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+--+-+--+", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Bekehre dich von Herzen nur, la\u00df deinen Weg und Willen,", "tokens": ["Be\u00b7keh\u00b7re", "dich", "von", "Her\u00b7zen", "nur", ",", "la\u00df", "dei\u00b7nen", "Weg", "und", "Wil\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "NN", "ADV", "$,", "VVIMP", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.6": {"text": "Gott liebet dich, er hat's so gern, er will's auch selbst erf\u00fcllen! \u2013", "tokens": ["Gott", "lie\u00b7bet", "dich", ",", "er", "hat's", "so", "gern", ",", "er", "will's", "auch", "selbst", "er\u00b7f\u00fcl\u00b7len", "!", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "VVFIN", "PPER", "$,", "PPER", "VAFIN", "ADV", "ADV", "$,", "PPER", "VMFIN", "ADV", "ADV", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.7": {"text": "Nein, Jesu, ich will sterben nicht,", "tokens": ["Nein", ",", "Je\u00b7su", ",", "ich", "will", "ster\u00b7ben", "nicht", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "NE", "$,", "PPER", "VMFIN", "VVINF", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ich glaube, was dein Wort verspricht;", "tokens": ["Ich", "glau\u00b7be", ",", "was", "dein", "Wort", "ver\u00b7spricht", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PRELS", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Nur, was du forderst, gib mir dann,", "tokens": ["Nur", ",", "was", "du", "for\u00b7derst", ",", "gib", "mir", "dann", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWS", "PPER", "VVFIN", "$,", "VVIMP", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Leb in mir, da\u00df ich leben kann!", "tokens": ["Leb", "in", "mir", ",", "da\u00df", "ich", "le\u00b7ben", "kann", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "APPR", "PPER", "$,", "KOUS", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Denk, Seele, dies ist Gottes Wort, der Mund des Herren schw\u00f6ret,", "tokens": ["Denk", ",", "See\u00b7le", ",", "dies", "ist", "Got\u00b7tes", "Wort", ",", "der", "Mund", "des", "Her\u00b7ren", "schw\u00f6\u00b7ret", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "PDS", "VAFIN", "NN", "NN", "$,", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "Da\u00df er, so wahr er selber lebt, nicht deinen Tod begehret!", "tokens": ["Da\u00df", "er", ",", "so", "wahr", "er", "sel\u00b7ber", "lebt", ",", "nicht", "dei\u00b7nen", "Tod", "be\u00b7ge\u00b7hret", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "ADV", "ADJD", "PPER", "ADV", "VVFIN", "$,", "PTKNEG", "PPOSAT", "NN", "VVFIN", "$."],