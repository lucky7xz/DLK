{"textgrid.poem.50828": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Meergedanken", "genre": "verse", "period": "N.A.", "pub_year": 1845, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O w\u00e4r mein Herz das tiefe Meer", "tokens": ["O", "w\u00e4r", "mein", "Herz", "das", "tie\u00b7fe", "Meer"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VAFIN", "PPOSAT", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und meine Feinde die Schiffe:", "tokens": ["Und", "mei\u00b7ne", "Fein\u00b7de", "die", "Schif\u00b7fe", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wie schleudert' es sie hin und her", "tokens": ["Wie", "schleu\u00b7dert'", "es", "sie", "hin", "und", "her"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "PPER", "PTKVZ", "KON", "ADV"], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.4": {"text": "An meines Hasses Riffe!", "tokens": ["An", "mei\u00b7nes", "Has\u00b7ses", "Rif\u00b7fe", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Und endlich schl\u00e4ng es unter sie,", "tokens": ["Und", "end\u00b7lich", "schl\u00e4ng", "es", "un\u00b7ter", "sie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hinunter in die Tiefe,", "tokens": ["Hin\u00b7un\u00b7ter", "in", "die", "Tie\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df dr\u00fcber gl\u00e4nzend sp\u00e4t und fr\u00fch", "tokens": ["Da\u00df", "dr\u00fc\u00b7ber", "gl\u00e4n\u00b7zend", "sp\u00e4t", "und", "fr\u00fch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PAV", "ADJD", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Meeresfrieden schliefe!", "tokens": ["Der", "Mee\u00b7res\u00b7frie\u00b7den", "schlie\u00b7fe", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "So aber ist's 'ne Welle kaum,", "tokens": ["So", "a\u00b7ber", "ist's", "'ne", "Wel\u00b7le", "kaum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von tausenden nur eine,", "tokens": ["Von", "tau\u00b7sen\u00b7den", "nur", "ei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADV", "ART", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Doch nagt und w\u00e4scht ihr leichter Schaum", "tokens": ["Doch", "nagt", "und", "w\u00e4scht", "ihr", "leich\u00b7ter", "Schaum"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Am morschen Schiffsgebeine!", "tokens": ["Am", "mor\u00b7schen", "Schiffs\u00b7ge\u00b7bei\u00b7ne", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Wir Wellen brausen treu vereint,", "tokens": ["Wir", "Wel\u00b7len", "brau\u0