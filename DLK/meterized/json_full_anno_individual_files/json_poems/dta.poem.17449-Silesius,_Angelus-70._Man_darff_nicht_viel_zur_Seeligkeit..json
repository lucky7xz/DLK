{"dta.poem.17449": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "70.  Man darff nicht viel zur Seeligkeit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Christ du bedarffst/ nicht viel zur ewgen Seeligkeit:", "tokens": ["Christ", "du", "be\u00b7darffst", "/", "nicht", "viel", "zur", "ew\u00b7gen", "See\u00b7lig\u00b7keit", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVFIN", "$(", "PTKNEG", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es h\u00fclfft ein eintzigs Kraut das heist gelassenheit.", "tokens": ["Es", "h\u00fclfft", "ein", "eint\u00b7zigs", "Kraut", "das", "heist", "ge\u00b7las\u00b7sen\u00b7heit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}