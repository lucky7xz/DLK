{"textgrid.poem.47137": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "14.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn du fragst nach jenen Liedern,", "tokens": ["Wenn", "du", "fragst", "nach", "je\u00b7nen", "Lie\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "APPR", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die ich einer Toten sang,", "tokens": ["Die", "ich", "ei\u00b7ner", "To\u00b7ten", "sang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "K\u00f6nnt' ich, Liebste, dir erwidern:", "tokens": ["K\u00f6nnt'", "ich", ",", "Liebs\u00b7te", ",", "dir", "er\u00b7wi\u00b7dern", ":"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "$,", "NN", "$,", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Macht dir eine Tote bang?", "tokens": ["Macht", "dir", "ei\u00b7ne", "To\u00b7te", "bang", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Jene Lieder sind ein Rahmen,", "tokens": ["Je\u00b7ne", "Lie\u00b7der", "sind", "ein", "Rah\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Drein zu fassen einen Schmerz,", "tokens": ["Drein", "zu", "fas\u00b7sen", "ei\u00b7nen", "Schmerz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKZU", "VVINF", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dem ich wu\u00dfte keinen Namen,", "tokens": ["Dem", "ich", "wu\u00df\u00b7te", "kei\u00b7nen", "Na\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und den doch gef\u00fchlt mein Herz.", "tokens": ["Und", "den", "doch", "ge\u00b7f\u00fchlt", "mein", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADV", "VVPP", "PPOSAT", "NN", "$."], "meter": "----+-+", "measure": "unknown.measure.di"}}, "stanza.3": {"line.1": {"text": "Ach, das Gl\u00fcck war nicht gestorben,", "tokens": ["Ach", ",", "das", "Gl\u00fcck", "war", "nicht", "ge\u00b7stor\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ART", "NN", "VAFIN", "PTKNEG", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Es war ungeboren mir;", "tokens": ["Es", "war", "un\u00b7ge\u00b7bo\u00b7ren", "mir", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "PPER", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und nun ist's in dir erworben,", "tokens": ["Und", "nun", "ist's", "in", "dir", "er\u00b7wor\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "APPR", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ewig unverloren mir.", "tokens": ["E\u00b7wig", "un\u00b7ver\u00b7lo\u00b7ren", "mir", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": 