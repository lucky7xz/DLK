{"textgrid.poem.65282": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Der ewige Jude", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich wandre sonder Rast und Ruh',", "tokens": ["Ich", "wand\u00b7re", "son\u00b7der", "Rast", "und", "Ruh'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mein Weg f\u00fchrt keinem Ziele zu;", "tokens": ["Mein", "Weg", "f\u00fchrt", "kei\u00b7nem", "Zie\u00b7le", "zu", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Fremd bin ich in jedwedem Land,", "tokens": ["Fremd", "bin", "ich", "in", "jed\u00b7we\u00b7dem", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und \u00fcberall doch wohlbekannt.", "tokens": ["Und", "\u00fc\u00b7be\u00b7rall", "doch", "wohl\u00b7be\u00b7kannt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Tief in dem Herzen klingt ein Wort,", "tokens": ["Tief", "in", "dem", "Her\u00b7zen", "klingt", "ein", "Wort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Das treibt mich fort von Ort zu Ort;", "tokens": ["Das", "treibt", "mich", "fort", "von", "Ort", "zu", "Ort", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PTKVZ", "APPR", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich spr\u00e4ch's nicht aus, nicht laut, nicht leis',", "tokens": ["Ich", "spr\u00e4ch's", "nicht", "aus", ",", "nicht", "laut", ",", "nicht", "leis'", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "PTKVZ", "$,", "PTKNEG", "ADJD", "$,", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sollt' ew'ge Ruh' auch sein der Preis.", "tokens": ["Sollt'", "ew'\u00b7ge", "Ruh'", "auch", "sein", "der", "Preis", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADJA", "NN", "ADV", "PPOSAT", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Es w\u00e4rmt mich nicht der Sonne Licht,", "tokens": ["Es", "w\u00e4rmt", "mich", "nicht", "der", "Son\u00b7ne", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PTKNEG", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des Abends Thau, er k\u00fchlt mich nicht;", "tokens": ["Des", "A\u00b7bends", "Thau", ",", "er", "k\u00fchlt", "mich", "nicht", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "PPER", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein lauer Nebel h\u00fcllt mich ein", "tokens": ["Ein", "lau\u00b7er", "Ne\u00b7bel", "h\u00fcllt", "mich", "ein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "ART"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In ewig gleichen D\u00e4mmerschein.", "tokens": ["In", "e\u00b7wig", "glei\u00b7chen", "D\u00e4m\u00b7mer\u00b7schein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Kein Mensch sich je zu mir gesellt,", "tokens": ["Kein", "Mensch", "sich", "je", "zu", "mir", "ge\u00b7sellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PRF", "ADV", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es lacht kein Blick mir in der Welt:", "tokens": ["Es", "lacht", "kein", "Blick", "mir", "in", "der", "Welt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Kein Vogel singt auf meinem Pfad,", "tokens": ["Kein", "Vo\u00b7gel", "singt", "auf", "mei\u00b7nem", "Pfad", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ob meinem Haupte rauscht kein Blatt.", "tokens": ["Ob", "mei\u00b7nem", "Haup\u00b7te", "rauscht", "kein", "Blatt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "So zieh' ich Tag und Nacht einher,", "tokens": ["So", "zieh'", "ich", "Tag", "und", "Nacht", "ein\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "KON", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das Herz so voll, die Welt so leer;", "tokens": ["Das", "Herz", "so", "voll", ",", "die", "Welt", "so", "leer", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "$,", "ART", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich habe Alles schon gesehn,", "tokens": ["Ich", "ha\u00b7be", "Al\u00b7les", "schon", "ge\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIS", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und darf doch nicht zur Ruhe gehn.", "tokens": ["Und", "darf", "doch", "nicht", "zur", "Ru\u00b7he", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "PTKNEG", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Vom Felsen st\u00fcrzt der Wasserfall,", "tokens": ["Vom", "Fel\u00b7sen", "st\u00fcrzt", "der", "Was\u00b7ser\u00b7fall", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Fort sch\u00e4umt der Flu\u00df im tiefen Thal;", "tokens": ["Fort", "sch\u00e4umt", "der", "Flu\u00df", "im", "tie\u00b7fen", "Thal", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "++-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er eilt so froh der ew'gen Ruh',", "tokens": ["Er", "eilt", "so", "froh", "der", "ew'\u00b7gen", "Ruh'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dem stillen Ozeane zu.", "tokens": ["Dem", "stil\u00b7len", "O\u00b7zea\u00b7ne", "zu", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.7": {"line.1": {"text": "Der Adler schwingt sich durch die Luft,", "tokens": ["Der", "Ad\u00b7ler", "schwingt", "sich", "durch", "die", "Luft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Verschwebend in des \u00c4thers Duft;", "tokens": ["Ver\u00b7schwe\u00b7bend", "in", "des", "\u00c4\u00b7thers", "Duft", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Hoch in den Wolken steht sein Haus,", "tokens": ["Hoch", "in", "den", "Wol\u00b7ken", "steht", "sein", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auf Alpenspitzen ruht er aus.", "tokens": ["Auf", "Al\u00b7pen\u00b7spit\u00b7zen", "ruht", "er", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Der Delphin durch die Fluthen schweift,", "tokens": ["Der", "Del\u00b7phin", "durch", "die", "Flut\u00b7hen", "schweift", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn in die Bucht der Schiffer l\u00e4uft;", "tokens": ["Wenn", "in", "die", "Bucht", "der", "Schif\u00b7fer", "l\u00e4uft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und nach dem Sturm im Sonnenschein", "tokens": ["Und", "nach", "dem", "Sturm", "im", "Son\u00b7nen\u00b7schein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Schl\u00e4ft er auf Wellenspiegeln ein.", "tokens": ["Schl\u00e4ft", "er", "auf", "Wel\u00b7len\u00b7spie\u00b7geln", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "PTKVZ", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.9": {"line.1": {"text": "Die Wolken treiben hin und her,", "tokens": ["Die", "Wol\u00b7ken", "trei\u00b7ben", "hin", "und", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie sind so matt, sie sind so schwer;", "tokens": ["Sie", "sind", "so", "matt", ",", "sie", "sind", "so", "schwer", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$,", "PPER", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da st\u00fcrzen rauschend sie herab,", "tokens": ["Da", "st\u00fcr\u00b7zen", "rau\u00b7schend", "sie", "her\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Schoo\u00df der Erde wird ihr Grab.", "tokens": ["Der", "Schoo\u00df", "der", "Er\u00b7de", "wird", "ihr", "Grab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Der m\u00fcde Wandrer dieser Welt,", "tokens": ["Der", "m\u00fc\u00b7de", "Wand\u00b7rer", "die\u00b7ser", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein sicher Ziel ist ihm gestellt.", "tokens": ["Ein", "si\u00b7cher", "Ziel", "ist", "ihm", "ge\u00b7stellt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPER", "VVPP", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Was klagt er ob des Tages Noth?", "tokens": ["Was", "klagt", "er", "ob", "des", "Ta\u00b7ges", "Noth", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "KOUS", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vor Nacht noch holt ihn heim der Tod.", "tokens": ["Vor", "Nacht", "noch", "holt", "ihn", "heim", "der", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "VVFIN", "PPER", "PTKVZ", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "O Mensch, der du den Lauf vollbracht,", "tokens": ["O", "Mensch", ",", "der", "du", "den", "Lauf", "voll\u00b7bracht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PRELS", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "li