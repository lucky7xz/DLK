{"textgrid.poem.63167": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es wird die Zeit kommen,", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es wird die Zeit kommen,", "tokens": ["Es", "wird", "die", "Zeit", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Da jeder jungfr\u00e4uliche Schoss sich dir bietet,", "tokens": ["Da", "je\u00b7der", "jung\u00b7fr\u00e4u\u00b7li\u00b7che", "Schoss", "sich", "dir", "bie\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "ADJA", "NN", "PRF", "PPER", "VVFIN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "Und alle Jungfrauen,", "tokens": ["Und", "al\u00b7le", "Jung\u00b7frau\u00b7en", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Schwarze, weisse, rote,", "tokens": ["Schwar\u00b7ze", ",", "weis\u00b7se", ",", "ro\u00b7te", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,", "ADJA", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Gefleckte,", "tokens": ["Ge\u00b7fleck\u00b7te", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "Schwanger sein werden von dir,", "tokens": ["Schwan\u00b7ger", "sein", "wer\u00b7den", "von", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAINF", "VAFIN", "APPR", "PPER", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.7": {"t