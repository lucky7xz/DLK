{"dta.poem.22692": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "242. Die wunderliche umbwech\u00dflung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1657", "urn": "urn:nbn:de:kobv:b4-200905198292", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Geistreiche Sinn- vnd Schlussrime. Wien, 1657."}, "poem": {"stanza.1": {"line.1": {"text": "Schaut wunder! Gottes Sohn wird jung in lauter", "tokens": ["Schaut", "wun\u00b7der", "!", "Got\u00b7tes", "Sohn", "wird", "jung", "in", "lau\u00b7ter"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "NN", "$.", "NN", "NN", "VAFIN", "ADJD", "APPR", "PIAT"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Freuden/", "tokens": ["Freu\u00b7den", "/"], "token_info": ["word", "punct"], "pos": ["NN", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Und mu\u00df mit lauter Angst von hin\u0303en wieder scheide\u0303:", "tokens": ["Und", "mu\u00df", "mit", "lau\u00b7ter", "Angst", "von", "hi\u00f1en", "wie\u00b7der", "schei\u00b7d\u1ebd", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "APPR", "PIAT", "NN", "APPR", "NE", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Wir tom\u0303en auff die Welt mit Thr\u00e4nen/ und vergehn", "tokens": ["Wir", "tom\u0303en", "auff", "die", "Welt", "mit", "Thr\u00e4\u00b7nen", "/", "und", "ver\u00b7gehn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "APPR", "NN", "$(", "KON", "VVINF"], "meter": "-++-+-+-+-+", "measure": "unknown.measure.hexa"}, "line.5": {"text": "Mit Lachen: wo wir recht in seinem Geiste stehn.", "tokens": ["Mit", "La\u00b7chen", ":", "wo", "wir", "recht", "in", "sei\u00b7nem", "Geis\u0