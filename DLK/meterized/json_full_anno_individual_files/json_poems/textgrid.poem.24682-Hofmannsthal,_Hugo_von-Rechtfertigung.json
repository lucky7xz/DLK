{"textgrid.poem.24682": {"metadata": {"author": {"name": "Hofmannsthal, Hugo von", "birth": "N.A.", "death": "N.A."}, "title": "Rechtfertigung", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So wie der Wandrer, der durch manch Verhau,", "tokens": ["So", "wie", "der", "Wand\u00b7rer", ",", "der", "durch", "manch", "Ver\u00b7hau", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ART", "NN", "$,", "PRELS", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Manch bl\u00fchend Dickicht seinen Weg gefunden:", "tokens": ["Manch", "bl\u00fc\u00b7hend", "Di\u00b7ckicht", "sei\u00b7nen", "Weg", "ge\u00b7fun\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Zerri\u00dfne Ranken haben ihn umwunden,", "tokens": ["Zer\u00b7ri\u00df\u00b7ne", "Ran\u00b7ken", "ha\u00b7ben", "ihn", "um\u00b7wun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Auf Haar und Schl\u00e4fen gl\u00e4nzt der frische Tau,", "tokens": ["Auf", "Haar", "und", "Schl\u00e4\u00b7fen", "gl\u00e4nzt", "der", "fri\u00b7sche", "Tau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Und um ihn webt ein Duft noch viele Stunden", "tokens": ["Und", "um", "ihn", "webt", "ein", "Duft", "noch", "vie\u00b7le", "Stun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUI", "PPER", "VVFIN", "ART", "NN", "ADV", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie Fr\u00fchlingsg\u00e4ren und wie \u00c4therblau \u2013:", "tokens": ["Wie", "Fr\u00fch\u00b7lings\u00b7g\u00e4\u00b7ren", "und", "wie", "\u00c4\u00b7ther\u00b7blau", "\u2013", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "NN", "KON", "PWAV", "ADJD", "$(", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "So tr\u00e4gt der Dichter unbewu\u00dft zur Schau", "tokens": ["So", "tr\u00e4gt", "der", "Dich\u00b7ter", "un\u00b7be\u00b7wu\u00dft", "zur", "Schau"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Was schweigsam oft ein Freundesherz empfunden.", "tokens": ["Was", "schweig\u00b7sam", "oft", "ein", "Freun\u00b7des\u00b7herz", "emp\u00b7fun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Er raubt es nicht, es kommt ihm zugeflogen", "tokens": ["Er", "raubt", "es", "nicht", ",", "es", "kommt", "ihm", "zu\u00b7ge\u00b7flo\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "$,", "PPER", "VVFIN", "PPER", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie Tau aus Bl\u00fctenkelchen sich ergie\u00dft;", "tokens": ["Wie", "Tau", "aus", "Bl\u00fc\u00b7ten\u00b7kel\u00b7chen", "sich", "er\u00b7gie\u00dft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "NN", "P