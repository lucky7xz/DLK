{"textgrid.poem.35817": {"metadata": {"author": {"name": "Hensel, Luise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Welt ist falsch; nur Du bist treu,", "genre": "verse", "period": "N.A.", "pub_year": 1818, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Welt ist falsch; nur Du bist treu,", "tokens": ["Die", "Welt", "ist", "falsch", ";", "nur", "Du", "bist", "treu", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$.", "ADV", "PPER", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das that ich oft gewahren;", "tokens": ["Das", "that", "ich", "oft", "ge\u00b7wah\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So mach' von ihr mich los und frei,", "tokens": ["So", "mach'", "von", "ihr", "mich", "los", "und", "frei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPER", "PRF", "PTKVZ", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df ich zu Dir kann fahren.", "tokens": ["Da\u00df", "ich", "zu", "Dir", "kann", "fah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "La\u00df Alles mich meiden", "tokens": ["La\u00df", "Al\u00b7les", "mich", "mei\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["VVIMP", "PIS", "PRF", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "F\u00fcr Dich und mit Dir,", "tokens": ["F\u00fcr", "Dich", "und", "mit", "Dir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "KON", "APPR", "PPER", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "La\u00df fr\u00f6hlich mich scheiden,", "tokens": ["La\u00df", "fr\u00f6h\u00b7lich", "mich", "schei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADJD", "PPER", "VVFIN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Was hab' ich an ihr?", "tokens": ["Was", "hab'", "ich", "an", "ihr", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "APPR", "PPER", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "Was ich vers\u00e4umt, was ich gefehlt,", "tokens": ["Was", "ich", "ver\u00b7s\u00e4umt", ",", "was", "ich", "ge\u00b7fehlt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "$,", "PWS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dir kann ich's nicht verhehlen;", "tokens": ["Dir", "kann", "ich's", "nicht", "ver\u00b7heh\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIS", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Was mich erfreut und was mich qu\u00e4lt,", "tokens": ["Was", "mich", "er\u00b7freut", "und", "was", "mich", "qu\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "KON", "PWS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Will ich Dir ganz befehlen.", "tokens": ["Will", "ich", "Dir", "ganz", "be\u00b7feh\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und willst Du mir Gaben", "tokens": ["Und", "willst", "Du", "mir", "Ga\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "PPER", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Und W\u00fcnsche verleihn:", "tokens": ["Und", "W\u00fcn\u00b7sche", "ver\u00b7leihn", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "VVINF", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Dich selbst mu\u00df ich haben,", "tokens": ["Dich", "selbst", "mu\u00df", "ich", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VMFIN", "PPER", "VAFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Dein selbst will ich sein.", "tokens": ["Dein", "selbst", "will", "ich", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADV", "VMFIN", "PPER", "VAINF", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "La\u00df mich, da\u00df Berg und Thal erschallt,", "tokens": ["La\u00df", "mich", ",", "da\u00df", "Berg", "und", "Thal", "er\u00b7schallt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "KOUS", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dein Lob der Erde singen,", "tokens": ["Dein", "Lob", "der", "Er\u00b7de", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ein reines Herz, das feurig wallt,", "tokens": ["Ein", "rei\u00b7nes", "Herz", ",", "das", "feu\u00b7rig", "wallt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zu Dir gen Himmel schwingen,", "tokens": ["Zu", "Dir", "gen", "Him\u00b7mel", "schwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Da\u00df tief es empfinde,", "tokens": ["Da\u00df", "tief", "es", "emp\u00b7fin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "PPER", "VVFIN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Wie Du mir so treu,", "tokens": ["Wie", "Du", "mir", "so", "treu", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "ADV", "ADJD", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Und streu' in die Winde", "tokens": ["Und", "streu'", "in", "die", "Win\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "All gl\u00e4nzende Spreu.", "tokens": ["All", "gl\u00e4n\u00b7zen\u00b7de", "Spreu", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.7": {"line.1": {"text": "Auf Deine Kraft hab' ich vertraut,", "tokens": ["Auf", "Dei\u00b7ne", "Kraft", "hab'", "ich", "ver\u00b7traut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So la\u00df im Kampf mich siegen,", "tokens": ["So", "la\u00df", "im", "Kampf", "mich", "sie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "APPRART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "La\u00df mich als Deine sel'ge Braut", "tokens": ["La\u00df", "mich", "als", "Dei\u00b7ne", "sel'\u00b7ge", "Braut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "KOUS", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In Deine Arme fliegen.", "tokens": ["In", "Dei\u00b7ne", "Ar\u00b7me", "flie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "O Gnade! o F\u00fclle!", "tokens": ["O", "Gna\u00b7de", "!", "o", "F\u00fcl\u00b7le", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "FM", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Wie liebt mich mein Freund:", "tokens": ["Wie", "liebt", "mich", "mein", "Freund", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Sein ewiger Wille", "tokens": ["Sein", "e\u00b7wi\u00b7ger", "Wil\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Hat selbst uns vereint.", "tokens": ["Hat", "selbst", "uns", "ver\u00b7eint", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPER", "VVPP", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.9": {"line.1": {"text": "Und wollen meine S\u00fcnden Dich", "tokens": ["Und", "wol\u00b7len", "mei\u00b7ne", "S\u00fcn\u00b7den", "Dich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPOSAT", "NN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von meinem Herzen dr\u00e4ngen,", "tokens": ["Von", "mei\u00b7nem", "Her\u00b7zen", "dr\u00e4n\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So will ich fest und fester mich", "tokens": ["So", "will", "ich", "fest", "und", "fes\u00b7ter", "mich"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "PTKVZ", "KON", "ADJD", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "An Deinen Busen h\u00e4ngen.", "tokens": ["An", "Dei\u00b7nen", "Bu\u00b7sen", "h\u00e4n\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Es mu\u00df ja die Liebe", "tokens": ["Es", "mu\u00df", "ja", "die", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Der Liebe verzeihn,", "tokens": ["Der", "Lie\u00b7be", "ver\u00b7zeihn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Und heilige Triebe", "tokens": ["Und", "hei\u00b7li\u00b7ge", "Trie\u00b7be"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Wirst Du mir verleihn.", "tokens": ["Wirst", "Du", "mir", "ver\u00b7leihn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "VVINF", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.11": {"line.1": {"text": "Wo ist Dein goldnes Hochzeitshaus,", "tokens": ["Wo", "ist", "Dein", "gold\u00b7nes", "Hoch\u00b7zeits\u00b7haus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Drin Du mich willst erwarten?", "tokens": ["Drin", "Du", "mich", "willst", "er\u00b7war\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wo spro\u00dft mein wei\u00dfer Rosenstrau\u00df", "tokens": ["Wo", "spro\u00dft", "mein", "wei\u00b7\u00dfer", "Ro\u00b7sen\u00b7strau\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In Deinem ew'gen Garten?", "tokens": ["In", "Dei\u00b7nem", "ew'\u00b7gen", "Gar\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "O, zeig' ihn von ferne", "tokens": ["O", ",", "zeig'", "ihn", "von", "fer\u00b7ne"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "VVFIN", "PPER", "APPR", "ADJA"], "meter": "++--+-", "measure": "trochaic.tri.relaxed"}, "line.2": {"text": "So leuchtend und sch\u00f6n,", "tokens": ["So", "leuch\u00b7tend", "und", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Da\u00df jauchzend ich lerne", "tokens": ["Da\u00df", "jauch\u00b7zend", "ich", "ler\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "VVPP", "PPER", "VVFIN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Die Erde verschm\