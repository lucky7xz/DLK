 Du mir Gaben", "tokens": ["Und", "willst", "Du", "mir", "Ga\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "PPER", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Und W\u00fcnsche verleihn:", "tokens": ["Und", "W\u00fcn\u00b7sche", "ver\u00b7leihn", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "VVINF", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Dich selbst mu\u00df ich haben,", "tokens": ["Dich", "selbst", "mu\u00df", "ich", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VMFIN", "PPER", "VAFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Dein selbst will ich sein.", "tokens": ["Dein", "selbst", "will", "ich", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADV", "VMFIN", "PPER", "VAINF", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.17": {"line.1": {"text": "La\u00df mich, da\u00df Berg und Thal erschallt,", "tokens": ["La\u00df", "mich", ",", "da\u00df", "Berg", "und", "Thal", "er\u00b7schallt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "KOUS", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dein Lob der Erde singen,", "tokens": ["Dein", "Lob", "der", "Er\u00b7de", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ein reines Herz, das feurig wallt,", "tokens": ["Ein", "rei\u00b7nes", "Herz", ",", "das", "feu\u00b7rig", "wallt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ADJD", "VV