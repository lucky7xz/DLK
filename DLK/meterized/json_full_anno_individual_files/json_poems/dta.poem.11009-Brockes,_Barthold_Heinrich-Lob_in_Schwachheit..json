{"dta.poem.11009": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Lob in Schwachheit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1740", "urn": "urn:nbn:de:kobv:b4-200905198572", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Herr! wunderbar sind deine Werke;", "tokens": ["Herr", "!", "wun\u00b7der\u00b7bar", "sind", "dei\u00b7ne", "Wer\u00b7ke", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ADJD", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ich selbst, indem ich sie bemerke,", "tokens": ["Ich", "selbst", ",", "in\u00b7dem", "ich", "sie", "be\u00b7mer\u00b7ke", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "KOUS", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Da ich dein Werk; bin wunderbar.", "tokens": ["Da", "ich", "dein", "Werk", ";", "bin", "wun\u00b7der\u00b7bar", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "$.", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ob ich nun gleich fast nichts ergr\u00fcnde;", "tokens": ["Ob", "ich", "nun", "gleich", "fast", "nichts", "er\u00b7gr\u00fcn\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "ADV", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Gnug, da\u00df ich einer Gottheit Kraft,", "tokens": ["Gnug", ",", "da\u00df", "ich", "ei\u00b7ner", "Got\u00b7theit", "Kraft", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PPER", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die alles im Erhalten schafft,", "tokens": ["Die", "al\u00b7les", "im", "Er\u00b7hal\u00b7ten", "schafft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und, im Gesch\u00f6pf, den Sch\u00f6pfer finde.", "tokens": ["Und", ",", "im", "Ge\u00b7sch\u00f6pf", ",", "den", "Sch\u00f6p\u00b7fer", "fin\u00b7de", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "APPRART", "NN", "$,", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Der Fund geschicht in Schwachheit zwar,", "tokens": ["Der", "Fund", "ge\u00b7schicht", "in", "Schwach\u00b7heit", "zwar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "APPR", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Ich kann ihn hier nicht anders sehen;", "tokens": ["Ich", "kann", "ihn", "hier", "nicht", "an\u00b7ders", "se\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "PTKNEG", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Doch ist auch die\u00df nicht minder wahr:", "tokens": ["Doch", "ist", "auch", "die\u00df", "nicht", "min\u00b7der", "wahr", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "PDS", "PTKNEG", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Es kann auch Schwachheit Gott erh\u00f6hen.", "tokens": ["Es", "kann", "auch", "Schwach\u00b7heit", "Gott", "er\u00b7h\u00f6\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}