{"textgrid.poem.26622": {"metadata": {"author": {"name": "Saar, Ferdinand von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Was \u00fcber mich auch And're m\u00f6gen sagen,", "genre": "verse", "period": "N.A.", "pub_year": 1869, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was \u00fcber mich auch And're m\u00f6gen sagen,", "tokens": ["Was", "\u00fc\u00b7ber", "mich", "auch", "An\u00b7d'\u00b7re", "m\u00f6\u00b7gen", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "PPER", "ADV", "PIS", "VMFIN", "VVINF", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ob du dir selbst gef\u00e4llst in falschen Schl\u00fcssen:", "tokens": ["Ob", "du", "dir", "selbst", "ge\u00b7f\u00e4llst", "in", "fal\u00b7schen", "Schl\u00fcs\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "ADJD", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Was du verbrachst an mir in fr\u00fch'ren Tagen,", "tokens": ["Was", "du", "ver\u00b7brachst", "an", "mir", "in", "fr\u00fch'\u00b7ren", "Ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "APPR", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wirst du zuletzt dir doch gestehen m\u00fcssen.", "tokens": ["Wirst", "du", "zu\u00b7letzt", "dir", "doch", "ge\u00b7ste\u00b7hen", "m\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "PPER", "ADV", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Du wei\u00dft, nie hat ein irdisches Verlangen", "tokens": ["Du", "wei\u00dft", ",", "nie", "hat", "ein", "ir\u00b7di\u00b7sches", "Ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "ADV", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Getr\u00fcbt mein reines, laut'res Deingedenken;", "tokens": ["Ge\u00b7tr\u00fcbt", "mein", "rei\u00b7nes", ",", "laut'\u00b7res", "Dein\u00b7ge\u00b7den\u00b7ken", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVPP", "PPOSAT", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ich war begn\u00fcgt, an deinem Blick zu hangen", "tokens": ["Ich", "war", "be\u00b7gn\u00fcgt", ",", "an", "dei\u00b7nem", "Blick", "zu", "han\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "VVPP", "$,", "APPR", "PPOSAT", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und tief mich in dein Wesen zu versenken.", "tokens": ["Und", "tief", "mich", "in", "dein", "We\u00b7sen", "zu", "ver\u00b7sen\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PRF", "APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Nie hab' ich \u2013 selbst in Tr\u00e4umen nicht, in leisen \u2013", "tokens": ["Nie", "hab'", "ich", "\u2013", "selbst", "in", "Tr\u00e4u\u00b7men", "nicht", ",", "in", "lei\u00b7sen", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "$(", "ADV", "APPR", "NN", "PTKNEG", "$,", "APPR", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gehofft, jemals von dir geliebt zu werden;", "tokens": ["Ge\u00b7hofft", ",", "je\u00b7mals", "von", "dir", "ge\u00b7liebt", "zu", "wer\u00b7den", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "APPR", "PPER", "VVPP", "PTKZU", "VAINF", "$."], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "In Wort und Liedern aber mocht' ich's preisen,", "tokens": ["In", "Wort", "und", "Lie\u00b7dern", "a\u00b7ber", "mocht'", "ich's", "prei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ADV", "VMFIN", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da\u00df du als Weib mir ", "tokens": ["Da\u00df", "du", "als", "Weib", "mir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "KOUS", "NN", "PPER"], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Nicht da\u00df ich dich geseh'n ganz ohne Fehle \u2013", "tokens": ["Nicht", "da\u00df", "ich", "dich", "ge\u00b7seh'n", "ganz", "oh\u00b7ne", "Feh\u00b7le", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "KOUS", "PPER", "PRF", "ADJD", "ADV", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Bewundert hab' ich nie die allzu Reinen:", "tokens": ["Be\u00b7wun\u00b7dert", "hab'", "ich", "nie", "die", "all\u00b7zu", "Rei\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "ADV", "ART", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch glaubt' ich dich von jenem Schwung der Seele,", "tokens": ["Doch", "glaubt'", "ich", "dich", "von", "je\u00b7nem", "Schwung", "der", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PRF", "APPR", "PDAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dem ewig fremd die Kniffe der Gemeinen.", "tokens": ["Dem", "e\u00b7wig", "fremd", "die", "Knif\u00b7fe", "der", "Ge\u00b7mei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJD", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Von jenem Stolze glaubt' ich dich durchlodert,", "tokens": ["Von", "je\u00b7nem", "Stol\u00b7ze", "glaubt'", "ich", "dich", "durch\u00b7lo\u00b7dert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "VVFIN", "PPER", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von jenem Wahrheitsmuth, der im ", "tokens": ["Von", "je\u00b7nem", "Wahr\u00b7heits\u00b7muth", ",", "der", "im"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PDAT", "NN", "$,", "PRELS", "APPRART"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ob auch mit Schmerz \u2013 wenn es die Stunde fodert,", "tokens": ["Ob", "auch", "mit", "Schmerz", "\u2013", "wenn", "es", "die", "Stun\u00b7de", "fo\u00b7dert", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "APPR", "NN", "$(", "KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Verdammen kann, was wir das Liebste nennen.", "tokens": ["Ver\u00b7dam\u00b7men", "kann", ",", "was", "wir", "das", "Liebs\u00b7te", "nen\u00b7nen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "$,", "PRELS", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Die Stunde kam \u2013 du hast dich nicht erwiesen;", "tokens": ["Die", "Stun\u00b7de", "kam", "\u2013", "du", "hast", "dich", "nicht", "er\u00b7wie\u00b7sen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$(", "PPER", "VAFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Du stand'st zu Jenen, die mich schn\u00f6d' verrathen", "tokens": ["Du", "stan\u00b7d'st", "zu", "Je\u00b7nen", ",", "die", "mich", "schn\u00f6d'", "ver\u00b7ra\u00b7then"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PDAT", "$,", "PRELS", "PPER", "VVFIN", "VVPP"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "Und dann mit Hohn in meine Schmerzen bliesen", "tokens": ["Und", "dann", "mit", "Hohn", "in", "mei\u00b7ne", "Schmer\u00b7zen", "blie\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "NN", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und meine Wunden noch mit F\u00fc\u00dfen traten.", "tokens": ["Und", "mei\u00b7ne", "Wun\u00b7den", "noch", "mit", "F\u00fc\u00b7\u00dfen", "tra\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Du stand'st zu ihnen, weil dich Pflicht und Liebe", "tokens": ["Du", "stan\u00b7d'st", "zu", "ih\u00b7nen", ",", "weil", "dich", "Pflicht", "und", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "$,", "KOUS", "PPER", "NN", "KON", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "An sie gefesselt hielt \u2013 wer m\u00f6cht' es tadeln?", "tokens": ["An", "sie", "ge\u00b7fes\u00b7selt", "hielt", "\u2013", "wer", "m\u00f6cht'", "es", "ta\u00b7deln", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVPP", "VVFIN", "$(", "PWS", "VMFIN", "PPER", "VVINF", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Doch mu\u00dftest du, zu solchem Herzenstriebe,", "tokens": ["Doch", "mu\u00df\u00b7test", "du", ",", "zu", "sol\u00b7chem", "Her\u00b7zen\u00b7strie\u00b7be", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "$,", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dich willig selbst verblenden und entadeln.", "tokens": ["Dich", "wil\u00b7lig", "selbst", "ver\u00b7blen\u00b7den", "und", "en\u00b7ta\u00b7deln", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ADV", "VVINF", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Es ist vorbei. Vernarbt sind meine Wunden,", "tokens": ["Es", "ist", "vor\u00b7bei", ".", "Ver\u00b7narbt", "sind", "mei\u00b7ne", "Wun\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "$.", "VVPP", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "L\u00e4ngst ward mir auch Vergeltung schon geboten \u2013", "tokens": ["L\u00e4ngst", "ward", "mir", "auch", "Ver\u00b7gel\u00b7tung", "schon", "ge\u00b7bo\u00b7ten", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "NN", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch heut' noch wird der Schmerz von mir empfunden,", "tokens": ["Doch", "heut'", "noch", "wird", "der", "Schmerz", "von", "mir", "emp\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "VAFIN", "ART", "NN", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da\u00df ich dich werfen mu\u00dfte zu den Todten.", "tokens": ["Da\u00df", "ich", "dich", "wer\u00b7fen", "mu\u00df\u00b7te", "zu", "den", "Tod\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVINF", "VMFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Was \u00fcber mich auch And're m\u00f6gen sagen,", "tokens": ["Was", "\u00fc\u00b7ber", "mich", "auch", "An\u00b7d'\u00b7re", "m\u00f6\u00b7gen", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "PPER", "ADV", "PIS", "VMFIN", "VVINF", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ob du dir selbst gef\u00e4llst in falschen Schl\u00fcssen:", "tokens": ["Ob", "du", "dir", "selbst", "ge\u00b7f\u00e4llst", "in", "fal\u00b7schen", "Schl\u00fcs\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "ADJD", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Was du verbrachst an mir in fr\u00fch'ren Tagen,", "tokens": ["Was", "du", "ver\u00b7brachst", "an", "mir", "in", "fr\u00fch'\u00b7ren", "Ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word",