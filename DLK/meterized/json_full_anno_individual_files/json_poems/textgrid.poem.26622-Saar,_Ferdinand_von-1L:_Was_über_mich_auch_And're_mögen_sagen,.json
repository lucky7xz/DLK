("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Bewundert hab' ich nie die allzu Reinen:", "tokens": ["Be\u00b7wun\u00b7dert", "hab'", "ich", "nie", "die", "all\u00b7zu", "Rei\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "ADV", "ART", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch glaubt' ich dich von jenem Schwung der Seele,", "tokens": ["Doch", "glaubt'", "ich", "dich", "von", "je\u00b7nem", "Schwung", "der", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PRF", "APPR", "PDAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dem ewig fremd die Kniffe der Gemeinen.", "tokens": ["Dem", "e\u00b7wig", "fremd", "die", "Knif\u00b7fe", "der", "Ge\u00b7mei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJD", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.13": {"line.1": {"text": "Von jenem Stolze glaubt' ich dich durchlodert,", "tokens": ["Von", "je\u00b7nem", "Stol\u00b7ze", "glaubt'", "ich", "dich", "durch\u00b7lo\u00b7dert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "VVFIN", "PPER", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von jenem Wahrheitsmuth, der im ", "tokens": ["Von", "je\u00b7nem", "Wahr\u00b7heits\u00b7muth", ",", "der", "im"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PDAT", "NN", "$,", "PRELS", "APPRART"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ob auch mit Schmerz \u2013 wenn es die Stunde fodert,", "tokens": ["Ob", "auch", "mit