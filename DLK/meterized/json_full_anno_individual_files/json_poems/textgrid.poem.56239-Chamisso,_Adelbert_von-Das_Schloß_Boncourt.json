{"textgrid.poem.56239": {"metadata": {"author": {"name": "Chamisso, Adelbert von", "birth": "N.A.", "death": "N.A."}, "title": "Das Schlo\u00df Boncourt", "genre": "verse", "period": "N.A.", "pub_year": 1809, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich tr\u00e4um als Kind mich zur\u00fccke,", "tokens": ["Ich", "tr\u00e4um", "als", "Kind", "mich", "zu\u00b7r\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NE", "KOUS", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und sch\u00fcttle mein greises Haupt;", "tokens": ["Und", "sch\u00fctt\u00b7le", "mein", "grei\u00b7ses", "Haupt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wie sucht ihr mich heim, ihr Bilder,", "tokens": ["Wie", "sucht", "ihr", "mich", "heim", ",", "ihr", "Bil\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PRF", "PTKVZ", "$,", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Die lang ich vergessen geglaubt?", "tokens": ["Die", "lang", "ich", "ver\u00b7ges\u00b7sen", "ge\u00b7glaubt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PPER", "VVPP", "VVPP", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.2": {"line.1": {"text": "Hoch ragt aus schatt'gen Gehegen", "tokens": ["Hoch", "ragt", "aus", "schatt'\u00b7gen", "Ge\u00b7he\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "APPR", "ADJA", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Ein schimmerndes Schlo\u00df hervor,", "tokens": ["Ein", "schim\u00b7mern\u00b7des", "Schlo\u00df", "her\u00b7vor", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ich kenne die T\u00fcrme, die Zinnen,", "tokens": ["Ich", "ken\u00b7ne", "die", "T\u00fcr\u00b7me", ",", "die", "Zin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Die steinerne Br\u00fccke, das Tor.", "tokens": ["Die", "stei\u00b7ner\u00b7ne", "Br\u00fc\u00b7cke", ",", "das", "Tor", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.3": {"line.1": {"text": "Es schauen vom Wappenschilde", "tokens": ["Es", "schau\u00b7en", "vom", "Wap\u00b7pen\u00b7schil\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Die L\u00f6wen so traulich mich an,", "tokens": ["Die", "L\u00f6\u00b7wen", "so", "trau\u00b7lich", "mich", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "PPER", "PTKVZ", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Ich gr\u00fc\u00dfe die alten Bekannten,", "tokens": ["Ich", "gr\u00fc\u00b7\u00dfe", "die", "al\u00b7ten", "Be\u00b7kann\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Und eile den Burghof hinan.", "tokens": ["Und", "ei\u00b7le", "den", "Burg\u00b7hof", "hi\u00b7nan", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.4": {"line.1": {"text": "Dort liegt die Sphinx am Brunnen,", "tokens": ["Dort", "liegt", "die", "Sphinx", "am", "Brun\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dort gr\u00fcnt der Feigenbaum,", "tokens": ["Dort", "gr\u00fcnt", "der", "Fei\u00b7gen\u00b7baum", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dort, hinter diesen Fenstern,", "tokens": ["Dort", ",", "hin\u00b7ter", "die\u00b7sen", "Fens\u00b7tern", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Vertr\u00e4umt ich den ersten Traum.", "tokens": ["Ver\u00b7tr\u00e4umt", "ich", "den", "ers\u00b7ten", "Traum", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Ich tret in die Burgkapelle", "tokens": ["Ich", "tret", "in", "die", "Burg\u00b7ka\u00b7pel\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und suche des Ahnherrn Grab,", "tokens": ["Und", "su\u00b7che", "des", "Ahn\u00b7herrn", "Grab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Dort ist's, dort h\u00e4ngt vom Pfeiler", "tokens": ["Dort", "ist's", ",", "dort", "h\u00e4ngt", "vom", "Pfei\u00b7ler"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "$,", "ADV", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das alte Gewaffen herab.", "tokens": ["Das", "al\u00b7te", "Ge\u00b7waf\u00b7fen", "her\u00b7ab", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.6": {"line.1": {"text": "Noch lesen umflort die Augen", "tokens": ["Noch", "le\u00b7sen", "um\u00b7flort", "die", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVINF", "VVFIN", "ART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Die Z\u00fcge der Inschrift nicht,", "tokens": ["Die", "Z\u00fc\u00b7ge", "der", "In\u00b7schrift", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PTKNEG", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wie hell durch die bunten Scheiben", "tokens": ["Wie", "hell", "durch", "die", "bun\u00b7ten", "Schei\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "APPR", "ART", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Das Licht dar\u00fcber auch bricht.", "tokens": ["Das", "Licht", "da\u00b7r\u00fc\u00b7ber", "auch", "bricht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PAV", "ADV", "VVFIN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.7": {"line.1": {"text": "So stehst du, o Schlo\u00df meiner V\u00e4ter,", "tokens": ["So", "stehst", "du", ",", "o", "Schlo\u00df", "mei\u00b7ner", "V\u00e4\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "FM", "NN", "PPOSAT", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Mir treu und fest in dem Sinn,", "tokens": ["Mir", "treu", "und", "fest", "in", "dem", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "KON", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Und bist von der Erde verschwunden,", "tokens": ["Und", "bist", "von", "der", "Er\u00b7de", "ver\u00b7schwun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Der Pflug geht \u00fcber dich hin.", "tokens": ["Der", "Pflug", "geht", "\u00fc\u00b7ber", "dich", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.8": {"line.1": {"text": "Sei fruchtbar, o teurer Boden,", "tokens": ["Sei", "frucht\u00b7bar", ",", "o", "teu\u00b7rer", "Bo\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "$,", "FM", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Ich segne dich mild und ger\u00fchrt,", "tokens": ["Ich", "seg\u00b7ne", "dich", "mild", "und", "ge\u00b7r\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "KON", "VVPP", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Und segn' ihn zwiefach, wer immer", "tokens": ["Und", "segn'", "ihn", "zwie\u00b7fach", ",", "wer", "im\u00b7mer"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVIMP", "PPER", "ADJD", "$,", "PWS", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den Pflug nun \u00fcber dich f\u00fchrt.", "tokens": ["Den", "Pflug", "nun", "\u00fc\u00b7ber", "dich", "f\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.9": {"line.1": {"text": "Ich aber will auf mich raffen,", "tokens": ["Ich", "a\u00b7ber", "will", "auf", "mich", "raf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VMFIN", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Mein Saitenspiel in der Hand,", "tokens": ["Mein", "Sai\u00b7ten\u00b7spiel", "in", "der", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Die Weiten der Erde durchschweifen,", "tokens": ["Die", "Wei\u00b7ten", "der", "Er\u00b7de", "durch\u00b7schwei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Und singen von Land zu Land.", "tokens": ["Und", "sin\u00b7gen", "von", "Land", "zu", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "APPR", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.10": {"line.1": {"text": "Ich tr\u00e4um als Kind mich zur\u00fccke,", "tokens": ["Ich", "tr\u00e4um", "als", "Kind", "mich", "zu\u00b7r\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NE", "KOUS", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+--+-", "measure"