{"dta.poem.9784": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auf das absterben Hn. J. P.  \n Sonnet.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die ihr der erden crey\u00df nach maa\u00df und circkel mest/", "tokens": ["Die", "ihr", "der", "er\u00b7den", "crey\u00df", "nach", "maa\u00df", "und", "cir\u00b7ckel", "mest", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "VVFIN", "APPR", "VVFIN", "KON", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und einem ieden theil gewisse gr\u00e4ntzen schreibet/", "tokens": ["Und", "ei\u00b7nem", "ie\u00b7den", "theil", "ge\u00b7wis\u00b7se", "gr\u00e4nt\u00b7zen", "schrei\u00b7bet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PIAT", "NN", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Des himmels grosse welt in eur gehirne treibet/", "tokens": ["Des", "him\u00b7mels", "gros\u00b7se", "welt", "in", "eur", "ge\u00b7hir\u00b7ne", "trei\u00b7bet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und was noch \u00fcber der der H\u00f6chste hangen l\u00e4st/", "tokens": ["Und", "was", "noch", "\u00fc\u00b7ber", "der", "der", "H\u00f6chs\u00b7te", "han\u00b7gen", "l\u00e4st", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADV", "APPR", "PRELS", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Zu gr\u00fcnden niemals nicht erm\u00fcdet seyd gewest;", "tokens": ["Zu", "gr\u00fcn\u00b7den", "nie\u00b7mals", "nicht", "er\u00b7m\u00fc\u00b7det", "seyd", "ge\u00b7west", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "ADV", "PTKNEG", "VVPP", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Glaubt/ da\u00df sehr viel von euch noch ungemessen bleibet:", "tokens": ["Glaubt", "/", "da\u00df", "sehr", "viel", "von", "euch", "noch", "un\u00b7ge\u00b7mes\u00b7sen", "blei\u00b7bet", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$(", "KOUS", "ADV", "ADV", "APPR", "PPER", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und ob ihr maa\u00df und ziel den sternen einverleibet/", "tokens": ["Und", "ob", "ihr", "maa\u00df", "und", "ziel", "den", "ster\u00b7nen", "ein\u00b7ver\u00b7lei\u00b7bet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "KON", "VVFIN", "ART", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So schaut doch/ da\u00df ihr nicht eur selbst darbey vergest:", "tokens": ["So", "schaut", "doch", "/", "da\u00df", "ihr", "nicht", "eur", "selbst", "dar\u00b7bey", "ver\u00b7gest", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "$(", "KOUS", "PPER", "PTKNEG", "PPOSAT", "ADV", "PAV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Mest diesen engen raum/ wo Placentinens glieder", "tokens": ["Mest", "die\u00b7sen", "en\u00b7gen", "raum", "/", "wo", "Pla\u00b7cen\u00b7ti\u00b7nens", "glie\u00b7der"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PDAT", "ADJA", "NN", "$(", "PWAV", "NE", "NN"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Bey vielen andern sich zur ruhe lassen nieder:", "tokens": ["Bey", "vie\u00b7len", "an\u00b7dern", "sich", "zur", "ru\u00b7he", "las\u00b7sen", "nie\u00b7der", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "PIS", "PRF", "APPRART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sagt uns/ wie breit und lang/ wie tieff des todten grufft?", "tokens": ["Sagt", "uns", "/", "wie", "breit", "und", "lang", "/", "wie", "tieff", "des", "tod\u00b7ten", "grufft", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$(", "PWAV", "ADJD", "KON", "ADJD", "$(", "PWAV", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Wie kan den grossen mann di\u00df kleine grab umbschlissen/", "tokens": ["Wie", "kan", "den", "gros\u00b7sen", "mann", "di\u00df", "klei\u00b7ne", "grab", "umbschlis\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "ART", "ADJA", "NN", "PDS", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Der seine wissenschafft die halbe welt lie\u00df wissen?", "tokens": ["Der", "sei\u00b7ne", "wis\u00b7sen\u00b7schafft", "die", "hal\u00b7be", "welt", "lie\u00df", "wis\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "VVFIN", "ART", "ADJA", "NN", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Doch ruht er hie/ nun GOtt ihn aus dem leben rufft.", "tokens": ["Doch", "ruht", "er", "hie", "/", "nun", "Gott", "ihn", "aus", "dem", "le\u00b7ben", "rufft", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "$(", "ADV", "NN", "PPER", "APPR", "ART", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}