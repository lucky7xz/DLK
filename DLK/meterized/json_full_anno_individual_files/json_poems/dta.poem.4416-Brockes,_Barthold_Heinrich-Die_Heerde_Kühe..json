{"dta.poem.4416": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Heerde K\u00fche.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1727", "urn": "urn:nbn:de:kobv:b4-200905198599", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Liebstes Vieh, da ich hier stehe,", "tokens": ["Liebs\u00b7tes", "Vieh", ",", "da", "ich", "hier", "ste\u00b7he", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "KOUS", "PPER", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und, wie man dich melket, sehe;", "tokens": ["Und", ",", "wie", "man", "dich", "mel\u00b7ket", ",", "se\u00b7he", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "$,", "PWAV", "PIS", "PPER", "VVFIN", "$,", "VVFIN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "F\u00e4llt mir bey,", "tokens": ["F\u00e4llt", "mir", "bey", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "Auf was Weis\u2019 es m\u00f6glich sey,", "tokens": ["Auf", "was", "Weis'", "es", "m\u00f6g\u00b7lich", "sey", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "NN", "PPER", "ADJD", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Da\u00df in dir das Gras f\u00fcr mich", "tokens": ["Da\u00df", "in", "dir", "das", "Gras", "f\u00fcr", "mich"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "PPER", "ART", "NN", "APPR", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Auf so wundersame Weise", "tokens": ["Auf", "so", "wun\u00b7der\u00b7sa\u00b7me", "Wei\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADV", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "So zum Trank als auch zur Speise", "tokens": ["So", "zum", "Trank", "als", "auch", "zur", "Spei\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "KOKOM", "ADV", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Zubereitet werd\u2019, und sich,", "tokens": ["Zu\u00b7be\u00b7rei\u00b7tet", "werd'", ",", "und", "sich", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "$,", "KON", "PRF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Als in lebendigen Oefen, gleichsam selber distillire.", "tokens": ["Als", "in", "le\u00b7ben\u00b7di\u00b7gen", "O\u00b7e\u00b7fen", ",", "gleich\u00b7sam", "sel\u00b7ber", "dis\u00b7til\u00b7li\u00b7re", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ADJA", "NN", "$,", "ADJD", "ADV", "VVFIN", "$."], "meter": "+--+-+-+-+-+--+--", "measure": "iambic.septa.invert"}, "line.10": {"text": "Sprich nun, Mensch, ob in der That", "tokens": ["Sprich", "nun", ",", "Mensch", ",", "ob", "in", "der", "That"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["VVIMP", "ADV", "$,", "NN", "$,", "KOUS", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Dem, Der es geordnet hat,", "tokens": ["Dem", ",", "Der", "es", "ge\u00b7ord\u00b7net", "hat", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "ART", "PPER", "VVPP", "VAFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.12": {"text": "Nicht unendlich Lob geb\u00fchre!", "tokens": ["Nicht", "un\u00b7end\u00b7lich", "Lob", "ge\u00b7b\u00fch\u00b7re", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}