{"dta.poem.3855": {"metadata": {"author": {"name": "Spindler, Christian Gotthold", "birth": "N.A.", "death": "N.A."}, "title": "Auf eine erbare Wittwe.  \n  J. M. Kuntzin.  1741.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-20581-9", "language": ["de:0.85", "fr:0.14"], "booktitle": "Spindler, Christian Gotthold: Unschuldige Jugend-Fr\u00fcchte. Leipzig, 1745."}, "poem": {"stanza.1": {"line.1": {"text": "So giebst du mir die Ehren-Crone,", "tokens": ["So", "giebst", "du", "mir", "die", "Eh\u00b7ren\u00b7Cro\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mein Joel, dort vor deinem Throne.", "tokens": ["Mein", "Joel", ",", "dort", "vor", "dei\u00b7nem", "Thro\u00b7ne", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NE", "$,", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "O! wie ist es so angenehm", "tokens": ["O", "!", "wie", "ist", "es", "so", "an\u00b7ge\u00b7nehm"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$.", "PWAV", "VAFIN", "PPER", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Jm himmlischen Jerusalem!", "tokens": ["Jm", "himm\u00b7li\u00b7schen", "Je\u00b7ru\u00b7sa\u00b7lem", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NE", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Geliebte Kinder, la\u00dft das Trauren,", "tokens": ["Ge\u00b7lieb\u00b7te", "Kin\u00b7der", ",", "la\u00dft", "das", "Trau\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVIMP", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Stillt euer z\u00e4rtliches Bedauren,", "tokens": ["Stillt", "eu\u00b7er", "z\u00e4rt\u00b7li\u00b7ches", "Be\u00b7dau\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "G\u00f6nnt mir den sanfften Ruhe-Ort,", "tokens": ["G\u00f6nnt", "mir", "den", "sanff\u00b7ten", "Ru\u00b7he\u00b7Ort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Bedenckt mein letztes Segens-Wort.", "tokens": ["Be\u00b7denckt", "mein", "letz\u00b7tes", "Se\u00b7gens\u00b7Wort", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}