{"dta.poem.5356": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Dem\u00fcthige Selbst-Erk\u00e4nntni\u00df.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Will mich des Hochmuths Brut mit Demuth-Larven", "tokens": ["Will", "mich", "des", "Hoch\u00b7muths", "Brut", "mit", "De\u00b7muth\u00b7La\u00b7rven"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PRF", "ART", "NN", "NN", "APPR", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "schrecken,", "tokens": ["schre\u00b7cken", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Will sie mir meines Geists Vernichtungen entdecken,", "tokens": ["Will", "sie", "mir", "mei\u00b7nes", "Geists", "Ver\u00b7nich\u00b7tun\u00b7gen", "ent\u00b7de\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "PPOSAT", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und, durch den H\u00f6llen-Schlu\u00df, Verzweiflung mir erwecken:", "tokens": ["Und", ",", "durch", "den", "H\u00f6l\u00b7len\u00b7Schlu\u00df", ",", "Ver\u00b7zwei\u00b7flung", "mir", "er\u00b7we\u00b7cken", ":"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "APPR", "ART", "NN", "$,", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "\u201dwas bist, was wei\u00dft du doch? Nichts: daraus folget klar,", "tokens": ["\"", "was", "bist", ",", "was", "wei\u00dft", "du", "doch", "?", "Nichts", ":", "da\u00b7raus", "fol\u00b7get", "klar", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VAFIN", "$,", "PWS", "VVFIN", "PPER", "ADV", "$.", "PIS", "$.", "PAV", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "\u201dda\u00df deine Seele nie was G\u00f6ttlichs ist, noch war;", "tokens": ["\"", "da\u00df", "dei\u00b7ne", "See\u00b7le", "nie", "was", "G\u00f6tt\u00b7lichs", "ist", ",", "noch", "war", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "KOUS", "PPOSAT", "NN", "ADV", "PWS", "NE", "VAFIN", "$,", "ADV", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "So zeiget mir dennoch, selbst der Unwissenheit", "tokens": ["So", "zei\u00b7get", "mir", "den\u00b7noch", ",", "selbst", "der", "Un\u00b7wis\u00b7sen\u00b7heit"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "ADV", "ART", "NN"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Erk\u00e4nntni\u00df und Begriff von meiner F\u00e4higkeit", "tokens": ["Er\u00b7k\u00e4nnt\u00b7ni\u00df", "und", "Be\u00b7griff", "von", "mei\u00b7ner", "F\u00e4\u00b7hig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Nicht nur was grosses an; sie machet uns so gar", "tokens": ["Nicht", "nur", "was", "gros\u00b7ses", "an", ";", "sie", "ma\u00b7chet", "uns", "so", "gar"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADV", "PWS", "ADJA", "PTKVZ", "$.", "PPER", "VVFIN", "PPER", "ADV", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Das allerhellste Licht der Wei\u00dfheit offenbar.", "tokens": ["Das", "al\u00b7ler\u00b7hells\u00b7te", "Licht", "der", "Wei\u00df\u00b7heit", "of\u00b7fen\u00b7bar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Nichts zeigt uns deutlicher die unerme\u00dfne Gr\u00f6sse", "tokens": ["Nichts", "zeigt", "uns", "deut\u00b7li\u00b7cher", "die", "un\u00b7er\u00b7me\u00df\u00b7ne", "Gr\u00f6s\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "PPER", "ADJD", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Des Sch\u00f6pfers, und zugleich der Menschen Schw\u00e4ch\u2019 und", "tokens": ["Des", "Sch\u00f6p\u00b7fers", ",", "und", "zu\u00b7gleich", "der", "Men\u00b7schen", "Schw\u00e4ch'", "und"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "KON", "ADV", "ART", "NN", "NN", "KON"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Bl\u00f6sse;", "tokens": ["Bl\u00f6s\u00b7se", ";"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.14": {"text": "Ja nichts erhebet mehr der grossen GOttheit Preis,", "tokens": ["Ja", "nichts", "er\u00b7he\u00b7bet", "mehr", "der", "gros\u00b7sen", "Got\u00b7theit", "Preis", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PIS", "VVFIN", "ADV", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Als eben, da man wei\u00df, da\u00df man nur wenig wei\u00df.", "tokens": ["Als", "e\u00b7ben", ",", "da", "man", "wei\u00df", ",", "da\u00df", "man", "nur", "we\u00b7nig", "wei\u00df", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "$,", "KOUS", "PIS", "VVFIN", "$,", "KOUS", "PIS", "ADV", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Indem wenn die Vernunft so, wie sie soll, verf\u00e4hrt;", "tokens": ["In\u00b7dem", "wenn", "die", "Ver\u00b7nunft", "so", ",", "wie", "sie", "soll", ",", "ver\u00b7f\u00e4hrt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "KOUS", "ART", "NN", "ADV", "$,", "PWAV", "PPER", "VMFIN", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Uns ein gedoppelt Gut daraus entspringet,", "tokens": ["Uns", "ein", "ge\u00b7dop\u00b7pelt", "Gut", "da\u00b7raus", "ent\u00b7sprin\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJD", "ADJD", "PAV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Da es uns nicht allein zur wahren Demuth bringet:", "tokens": ["Da", "es", "uns", "nicht", "al\u00b7lein", "zur", "wah\u00b7ren", "De\u00b7muth", "brin\u00b7get", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "PTKNEG", "ADV", "APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "GoTT selber wird dadurch am w\u00fcrdigsten verehrt.", "tokens": ["GoTT", "sel\u00b7ber", "wird", "da\u00b7durch", "am", "w\u00fcr\u00b7digs\u00b7ten", "ver\u00b7ehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "VAFIN", "PAV", "APPRART", "VAFIN", "VVPP", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}}}}}