{"textgrid.poem.64786": {"metadata": {"author": {"name": "K\u00e4stner, Abraham Gotthelf", "birth": "N.A.", "death": "N.A."}, "title": "9. Sehnsucht nach Wilhelminen", "genre": "verse", "period": "N.A.", "pub_year": 1744, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wann seh' ich dich, entfernte Sch\u00f6ne, wieder,", "tokens": ["Wann", "seh'", "ich", "dich", ",", "ent\u00b7fern\u00b7te", "Sch\u00f6\u00b7ne", ",", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PRF", "$,", "ADJA", "NN", "$,", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die meine Brust so lange schweigend ehrt?", "tokens": ["Die", "mei\u00b7ne", "Brust", "so", "lan\u00b7ge", "schwei\u00b7gend", "ehrt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "ADV", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wann setz' ich mich auf jenen H\u00fcgel nieder,", "tokens": ["Wann", "setz'", "ich", "mich", "auf", "je\u00b7nen", "H\u00fc\u00b7gel", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PRF", "APPR", "PDAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wo du mich erst, was Lieben sey, gelehrt?", "tokens": ["Wo", "du", "mich", "erst", ",", "was", "Lie\u00b7ben", "sey", ",", "ge\u00b7lehrt", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "ADV", "$,", "PWS", "ADJA", "VAFIN", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wann werd' ich einst die holden Augen k\u00fcssen,", "tokens": ["Wann", "werd'", "ich", "einst", "die", "hol\u00b7den", "Au\u00b7gen", "k\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Aus deren Glut das reine Feuer stammt,", "tokens": ["Aus", "de\u00b7ren", "Glut", "das", "rei\u00b7ne", "Feu\u00b7er", "stammt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Das durch die Zeit, so mich von dir gerissen,", "tokens": ["Das", "durch", "die", "Zeit", ",", "so", "mich", "von", "dir", "ge\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "NN", "$,", "ADV", "PPER", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Doch immer noch in meinem Herzen flammt?", "tokens": ["Doch", "im\u00b7mer", "noch", "in", "mei\u00b7nem", "Her\u00b7zen", "flammt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wo seyd ihr hin, ihr angenehmen Stunden,", "tokens": ["Wo", "seyd", "ihr", "hin", ",", "ihr", "an\u00b7ge\u00b7neh\u00b7men", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "PTKVZ", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da ich entz\u00fcckt an ", "tokens": ["Da", "ich", "ent\u00b7z\u00fcckt", "an"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "PTKVZ"], "meter": "-+---", "measure": "dactylic.init"}, "line.3": {"text": "Ach, w\u00e4ret ihr doch nicht so bald verschwunden!", "tokens": ["Ach", ",", "w\u00e4\u00b7ret", "ihr", "doch", "nicht", "so", "bald", "ver\u00b7schwun\u00b7den", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VAFIN", "PPER", "ADV", "PTKNEG", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ach, saht ihr nicht, wie nah die Trennung ging?", "tokens": ["Ach", ",", "saht", "ihr", "nicht", ",", "wie", "nah", "die", "Tren\u00b7nung", "ging", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VVFIN", "PPER", "PTKNEG", "$,", "PWAV", "ADJD", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ihr floht zu sehr, ihr werthen Augenblicke", "tokens": ["Ihr", "floht", "zu", "sehr", ",", "ihr", "wert\u00b7hen", "Au\u00b7gen\u00b7bli\u00b7cke"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADV", "$,", "PPER", "VVFIN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "F\u00fcr mich und Die, die damals mich erg\u00f6tzt,", "tokens": ["F\u00fcr", "mich", "und", "Die", ",", "die", "da\u00b7mals", "mich", "er\u00b7g\u00f6tzt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "KON", "ART", "$,", "PRELS", "ADV", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und lie\u00dft mir nichts von aller Lust zur\u00fccke,", "tokens": ["Und", "lie\u00dft", "mir", "nichts", "von", "al\u00b7ler", "Lust", "zu\u00b7r\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PIS", "APPR", "PIAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Als nur ihr Bild, mir in das Herz ge\u00e4tzt.", "tokens": ["Als", "nur", "ihr", "Bild", ",", "mir", "in", "das", "Herz", "ge\u00b7\u00e4tzt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPOSAT", "NN", "$,", "PPER", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ich sah' dich, Kind, ich f\u00fchlte, was f\u00fcr Triebe", "tokens": ["Ich", "sah'", "dich", ",", "Kind", ",", "ich", "f\u00fchl\u00b7te", ",", "was", "f\u00fcr", "Trie\u00b7be"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$,", "NN", "$,", "PPER", "VVFIN", "$,", "PRELS", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dein feurig Aug' in meiner Brust erweckt;", "tokens": ["Dein", "feu\u00b7rig", "Aug'", "in", "mei\u00b7ner", "Brust", "er\u00b7weckt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ein z\u00e4rtlich Herz, entz\u00fcckt von erster Liebe", "tokens": ["Ein", "z\u00e4rt\u00b7lich", "Herz", ",", "ent\u00b7z\u00fcckt", "von", "ers\u00b7ter", "Lie\u00b7be"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "NN", "$,", "VVPP", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ward dir in mir, und mir in dir entdeckt.", "tokens": ["Ward", "dir", "in", "mir", ",", "und", "mir", "in", "dir", "ent\u00b7deckt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "PPER", "$,", "KON", "PPER", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wir liebten uns. Des strengen Schicksals Hassen", "tokens": ["Wir", "lieb\u00b7ten", "uns", ".", "Des", "stren\u00b7gen", "Schick\u00b7sals", "Has\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$.", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ri\u00df mir mein Gl\u00fcck mit ", "tokens": ["Ri\u00df", "mir", "mein", "Gl\u00fcck", "mit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "PPOSAT", "NN", "APPR"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Ein Jahr ist's schon, da\u00df ich mein Kind verlassen,", "tokens": ["Ein", "Jahr", "ist's", "schon", ",", "da\u00df", "ich", "mein", "Kind", "ver\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "$,", "KOUS", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Ein Jahr ist's schon, da\u00df ich nicht meine bin.", "tokens": ["Ein", "Jahr", "ist's", "schon", ",", "da\u00df", "ich", "nicht", "mei\u00b7ne", "bin", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "$,", "KOUS", "PPER", "PTKNEG", "PPOSAT", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Was lernt ich dich, Vollkommne! doch nur kennen?", "tokens": ["Was", "lernt", "ich", "dich", ",", "Voll\u00b7komm\u00b7ne", "!", "doch", "nur", "ken\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PRF", "$,", "NN", "$.", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Was hast du mir doch deine Gunst geschenkt?", "tokens": ["Was", "hast", "du", "mir", "doch", "dei\u00b7ne", "Gunst", "ge\u00b7schenkt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "PPER", "ADV", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Verbandst du uns, o Himmel, uns zu trennen?", "tokens": ["Ver\u00b7bandst", "du", "uns", ",", "o", "Him\u00b7mel", ",", "uns", "zu", "tren\u00b7nen", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "$,", "FM", "NN", "$,", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Gabst du sie mir, da\u00df mich ihr Wegseyn kr\u00e4nkt?", "tokens": ["Gabst", "du", "sie", "mir", ",", "da\u00df", "mich", "ihr", "Weg\u00b7seyn", "kr\u00e4nkt", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "PPER", "$,", "KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ach! mu\u00dfte mich dies Kind so sehr entz\u00fccken,", "tokens": ["Ach", "!", "mu\u00df\u00b7te", "mich", "dies", "Kind", "so", "sehr", "ent\u00b7z\u00fc\u00b7cken", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VMFIN", "PRF", "PDS", "NN", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Da\u00df sie mir fehlt, und nichts die Sehnsucht stillt?", "tokens": ["Da\u00df", "sie", "mir", "fehlt", ",", "und", "nichts", "die", "Sehn\u00b7sucht", "stillt", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "$,", "KON", "PIS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Ach! ist's die Lust von wenig Augenblicken,", "tokens": ["Ach", "!", "ist's", "die", "Lust", "von", "we\u00b7nig", "Au\u00b7gen\u00b7bli\u00b7cken", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VAFIN", "ART", "NN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Aus der ein Schmerz von einem Jahre quillt?", "tokens": ["Aus", "der", "ein", "Schmerz", "von", "ei\u00b7nem", "Jah\u00b7re", "quillt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Begl\u00fcckter Schmerz! ich f\u00fchl' ihn nicht vergebens,", "tokens": ["Be\u00b7gl\u00fcck\u00b7ter", "Schmerz", "!", "ich", "f\u00fchl'", "ihn", "nicht", "ver\u00b7ge\u00b7bens", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "PPER", "VVFIN", "PPER", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Er st\u00e4rkt die Lust, wenn wir uns wieder sehn.", "tokens": ["Er", "st\u00e4rkt", "die", "Lust", ",", "wenn", "wir", "uns", "wie\u00b7der", "sehn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "KOUS", "PPER", "PRF", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sie k\u00fc\u00dft mich bald, die H\u00e4lfte meines Lebens:", "to