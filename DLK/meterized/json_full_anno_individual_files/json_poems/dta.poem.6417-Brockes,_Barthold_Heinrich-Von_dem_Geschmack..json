{"dta.poem.6417": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von dem Geschmack.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Das S\u00fcsse h\u00e4lt das Mittel zwischen beyden,", "tokens": ["Das", "S\u00fcs\u00b7se", "h\u00e4lt", "das", "Mit\u00b7tel", "zwi\u00b7schen", "bey\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "APPR", "PIAT", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Theilchen, welche rund sich in der R\u00fcnde regen,", "tokens": ["Die", "Theil\u00b7chen", ",", "wel\u00b7che", "rund", "sich", "in", "der", "R\u00fcn\u00b7de", "re\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD", "PRF", "APPR", "ART", "NN", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Erwecken in der Zung\u2019 ein leicht Bewegen,", "tokens": ["Er\u00b7we\u00b7cken", "in", "der", "Zung'", "ein", "leicht", "Be\u00b7we\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ART", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und machen uns mit K\u00fctzeln sanfte Freuden.", "tokens": ["Und", "ma\u00b7chen", "uns", "mit", "K\u00fct\u00b7zeln", "sanf\u00b7te", "Freu\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}