{"dta.poem.17282": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "132.  Verlust und Gewinn.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Der Tod ist mein Gewinn/ Verlust das lange Leben:", "tokens": ["Der", "Tod", "ist", "mein", "Ge\u00b7winn", "/", "Ver\u00b7lust", "das", "lan\u00b7ge", "Le\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "$(", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und dennoch dank ich GOtt da\u00df er mir di\u00df gegeben.", "tokens": ["Und", "den\u00b7noch", "dank", "ich", "Gott", "da\u00df", "er", "mir", "di\u00df", "ge\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PPER", "NN", "KOUS", "PPER", "PPER", "PDS", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich wachs\u2019 und nehme zu/ so lang ich hier noch bin:", "tokens": ["Ich", "wachs'", "und", "neh\u00b7me", "zu", "/", "so", "lang", "ich", "hier", "noch", "bin", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PTKZU", "$(", "ADV", "ADJD", "PPER", "ADV", "ADV", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Darumb ist auch gar wol das Leben mein Gewin.", "tokens": ["Da\u00b7rumb", "ist", "auch", "gar", "wol", "das", "Le\u00b7ben", "mein", "Ge\u00b7win", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "ADV", "ADV", "ADV", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}