{"dta.poem.13011": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Leanders antwort an den  \n Seladon.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Dein brief, den ich itzund auf meinem tische funden,", "tokens": ["Dein", "brief", ",", "den", "ich", "it\u00b7zund", "auf", "mei\u00b7nem", "ti\u00b7sche", "fun\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VVFIN", "$,", "PRELS", "PPER", "ADV", "APPR", "PPOSAT", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dein brief, mein Seladon! den deine gunst gemacht,", "tokens": ["Dein", "brief", ",", "mein", "Se\u00b7la\u00b7don", "!", "den", "dei\u00b7ne", "gunst", "ge\u00b7macht", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VVFIN", "$,", "PPOSAT", "NN", "$.", "ART", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Hat meinen matten geist mit frischer lust umwunden,", "tokens": ["Hat", "mei\u00b7nen", "mat\u00b7ten", "geist", "mit", "fri\u00b7scher", "lust", "um\u00b7wun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "ADJA", "NN", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und statt der worte mir nur rosen \u00fcberbracht.", "tokens": ["Und", "statt", "der", "wor\u00b7te", "mir", "nur", "ro\u00b7sen", "\u00fc\u00b7berb\u00b7racht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "PPER", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Drum hab\u2019 ich ihn auch gleich an meinen mund gedr\u00fccket,", "tokens": ["Drum", "hab'", "ich", "ihn", "auch", "gleich", "an", "mei\u00b7nen", "mund", "ge\u00b7dr\u00fc\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "PPER", "ADV", "ADV", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Denn was so liebreich ist, das mu\u00df gek\u00fcsset seyn.", "tokens": ["Denn", "was", "so", "lieb\u00b7reich", "ist", ",", "das", "mu\u00df", "ge\u00b7k\u00fcs\u00b7set", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADV", "ADJD", "VAFIN", "$,", "PDS", "VMFIN", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Allein so sehr mich auch dein holder kiel erquicket,", "tokens": ["Al\u00b7lein", "so", "sehr", "mich", "auch", "dein", "hol\u00b7der", "kiel", "er\u00b7quic\u00b7ket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "PPER", "ADV", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So kan dein treuer mund mich doch weit mehr erfreun.", "tokens": ["So", "kan", "dein", "treu\u00b7er", "mund", "mich", "doch", "weit", "mehr", "er\u00b7freun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPOSAT", "ADJA", "NN", "PPER", "ADV", "ADJD", "ADV", "VVINF", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Gewi\u00df, es liegt die zeit mir immer in gedancken,", "tokens": ["Ge\u00b7wi\u00df", ",", "es", "liegt", "die", "zeit", "mir", "im\u00b7mer", "in", "ge\u00b7dan\u00b7cken", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VVFIN", "ART", "NN", "PPER", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Da die vertrauligkeit mit uns spatzieren gieng:", "tokens": ["Da", "die", "ver\u00b7trau\u00b7lig\u00b7keit", "mit", "uns", "spat\u00b7zie\u00b7ren", "gieng", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPR", "PPER", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Da der vergn\u00fcgte sinn in einem gr\u00fcnen schrancken", "tokens": ["Da", "der", "ver\u00b7gn\u00fcg\u00b7te", "sinn", "in", "ei\u00b7nem", "gr\u00fc\u00b7nen", "schran\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "APPR", "ART", "ADJA", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Der sorgen schwere last gantz an den nagel hieng:", "tokens": ["Der", "sor\u00b7gen", "schwe\u00b7re", "last", "gantz", "an", "den", "na\u00b7gel", "hieng", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "VVFIN", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Da wir bey frischer milch nicht kalte worte machten,", "tokens": ["Da", "wir", "bey", "fri\u00b7scher", "milch", "nicht", "kal\u00b7te", "wor\u00b7te", "mach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN", "PTKNEG", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Und unsre liebe sich nach wunsch und lust besprach:", "tokens": ["Und", "uns\u00b7re", "lie\u00b7be", "sich", "nach", "wunsch", "und", "lust", "be\u00b7sprach", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "VVFIN", "PRF", "APPR", "ADJD", "KON", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Da wir bald an ein buch, bald an die Floris dachten;", "tokens": ["Da", "wir", "bald", "an", "ein", "buch", ",", "bald", "an", "die", "Flo\u00b7ris", "dach\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "ART", "NN", "$,", "ADV", "APPR", "ART", "NE", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Nun aber folget mir fast nichts als unmuth nach.", "tokens": ["Nun", "a\u00b7ber", "fol\u00b7get", "mir", "fast", "nichts", "als", "un\u00b7muth", "nach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "ADV", "PIS", "KOKOM", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Denn Seladon ist weg, ich kan ihn nicht mehr schauen.", "tokens": ["Denn", "Se\u00b7la\u00b7don", "ist", "weg", ",", "ich", "kan", "ihn", "nicht", "mehr", "schau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "ADV", "$,", "PPER", "VMFIN", "PPER", "PTKNEG", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Er sitzt im rosen-pusch, und wo Florette singt;", "tokens": ["Er", "sitzt", "im", "ro\u00b7sen\u00b7pusch", ",", "und", "wo", "Flo\u00b7ret\u00b7te", "singt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "ADJD", "$,", "KON", "PWAV", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Ich aber lieg\u2019 und geh\u2019 in abgeschiednen auen,", "tokens": ["Ich", "a\u00b7ber", "lieg'", "und", "geh'", "in", "ab\u00b7ge\u00b7schied\u00b7nen", "au\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "KON", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Da kein dergleichen lied vor meinen ohren klingt.", "tokens": ["Da", "kein", "derg\u00b7lei\u00b7chen", "lied", "vor", "mei\u00b7nen", "oh\u00b7ren", "klingt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "PIS", "VVFIN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Ich kan der freyen lufft nicht so, wie du, geniessen:", "tokens": ["Ich", "kan", "der", "frey\u00b7en", "lufft", "nicht", "so", ",", "wie", "du", ",", "ge\u00b7nies\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "ADJA", "NN", "PTKNEG", "ADV", "$,", "PWAV", "PPER", "$,", "VVPP", "$."], "meter": "-+-+-+-+++-+-", "measure": "unknown.measure.septa"}, "line.22": {"text": "Bey tr\u00fcbem wasser quillt kein tropffen reiner lust.", "tokens": ["Bey", "tr\u00fc\u00b7bem", "was\u00b7ser", "quillt", "kein", "tropf\u00b7fen", "rei\u00b7ner", "lust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PIAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Die verse wollen auch nicht, wie die deinen, fliessen.", "tokens": ["Die", "ver\u00b7se", "wol\u00b7len", "auch", "nicht", ",", "wie", "die", "dei\u00b7nen", ",", "flies\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ADV", "PTKNEG", "$,", "PWAV", "ART", "PPOSAT", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Mehr sag\u2019 ich itzund nicht, es ist dir vor bewust.", "tokens": ["Mehr", "sag'", "ich", "it\u00b7zund", "nicht", ",", "es", "ist", "dir", "vor", "be\u00b7wust", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PTKNEG", "$,", "PPER", "VAFIN", "PPER", "APPR", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Was aber fragest du: Kan auch Leander fehlen?", "tokens": ["Was", "a\u00b7ber", "fra\u00b7gest", "du", ":", "Kan", "auch", "Le\u00b7an\u00b7der", "feh\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "PPER", "$.", "VMFIN", "ADV", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Ach stelle, wo du wilst, die hohen lobspr\u00fcch\u2019 ein.", "tokens": ["Ach", "stel\u00b7le", ",", "wo", "du", "wilst", ",", "die", "ho\u00b7hen", "lob\u00b7spr\u00fcch'", "ein", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "$,", "PWAV", "PPER", "VMFIN", "$,", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "K\u00f6nnt\u2019 ich, so wie du schreibst, die freyen hertzen siehlen,", "tokens": ["K\u00f6nnt'", "ich", ",", "so", "wie", "du", "schreibst", ",", "die", "frey\u00b7en", "hert\u00b7zen", "sieh\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "$,", "ADV", "KOKOM", "PPER", "VVFIN", "$,", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Florettens w\u00fcrde l\u00e4ngst in meinen h\u00e4nden seyn.", "tokens": ["Flo\u00b7ret\u00b7tens", "w\u00fcr\u00b7de", "l\u00e4ngst", "in", "mei\u00b7nen", "h\u00e4n\u00b7den", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ADV", "APPR", "PPOSAT", "ADJA", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Denn lie\u00df\u2019 ich wiesen, g\u00e4rt\u2019 und alle w\u00e4lder stehen;", "tokens": ["Denn", "lie\u00df'", "ich", "wie\u00b7sen", ",", "g\u00e4rt'", "und", "al\u00b7le", "w\u00e4l\u00b7der", "ste\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "VVINF", "$,", "VVFIN", "KON", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Ich sagte: Schatten, fl\u00fc\u00df\u2019 und vogel gute nacht!", "tokens": ["Ich", "sag\u00b7te", ":", "Schat\u00b7ten", ",", "fl\u00fc\u00df'", "und", "vo\u00b7gel", "gu\u00b7te", "nacht", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "NN", "$,", "VVFIN", "KON", "NE", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Und w\u00fcrde gantz vergn\u00fcgt in dieses zimmer gehen,", "tokens": ["Und", "w\u00fcr\u00b7de", "gantz", "ver\u00b7gn\u00fcgt", "in", "die\u00b7ses", "zim\u00b7mer", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "VVPP", "APPR", "PDAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Das mir des himmels hand zum paradiese macht.", "tokens": ["Das", "mir", "des", "him\u00b7mels", "hand", "zum", "pa\u00b7ra\u00b7die\u00b7se", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ART", "NN", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Den gru\u00df, den du mir bringst, der wird Floretten gelten,", "tokens": ["Den", "gru\u00df", ",", "den", "du", "mir", "bringst", ",", "der", "wird", "Flo\u00b7ret\u00b7ten", "gel\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "PPER", "VVFIN", "$,", "PRELS", "VAFIN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Weil sie der nachtigall im fingen gleiche geht.", "tokens": ["Weil", "sie", "der", "nach\u00b7ti\u00b7gall", "im", "fin\u00b7gen", "glei\u00b7che", "geht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPRART", "ADJA", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Denn ihre liebligkeit will meine Muse schelten,", "tokens": ["Denn", "ih\u00b7re", "lieb\u00b7lig\u00b7keit", "will", "mei\u00b7ne", "Mu\u00b7se", "schel\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VMFIN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Indem ihr tieffer thon sich nicht so hoch erh\u00f6ht.", "tokens": ["In\u00b7dem", "ihr", "tief\u00b7fer", "thon", "sich", "nicht", "so", "hoch", "er\u00b7h\u00f6ht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "PRF", "PTKNEG", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Es w\u00fcnscht zwar Seladon Leandern viel vergn\u00fcgen;", "tokens": ["Es", "w\u00fcnscht", "zwar", "Se\u00b7la\u00b7don", "Le\u00b7an\u00b7dern", "viel", "ver\u00b7gn\u00fc\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "NE", "NE", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Allein dein s\u00fc\u00dfer wunsch verbleibet ohne frucht;", "tokens": ["Al\u00b7lein", "dein", "s\u00fc\u00b7\u00dfer", "wunsch", "ver\u00b7blei\u00b7bet", "oh\u00b7ne", "frucht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "ADJA", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Denn meine liebe mu\u00df in lauter \u00e4ngsten liegen,", "tokens": ["Denn", "mei\u00b7ne", "lie\u00b7be", "mu\u00df", "in", "lau\u00b7ter", "\u00e4ngs\u00b7ten", "lie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "VMFIN", "APPR", "PIAT", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Weil deine schwester sie stets zu ermorden sucht.", "tokens": ["Weil", "dei\u00b7ne", "schwes\u00b7ter", 