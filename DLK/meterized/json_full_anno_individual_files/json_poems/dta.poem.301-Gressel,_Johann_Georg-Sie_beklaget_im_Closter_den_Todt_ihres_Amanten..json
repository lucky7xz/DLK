{"dta.poem.301": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Sie beklaget im Closter den Todt ihres  \n  Amanten.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Jhr tr\u00fcben Augen weinet Blut/", "tokens": ["Ihr", "tr\u00fc\u00b7ben", "Au\u00b7gen", "wei\u00b7net", "Blut", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Jhr spahret nur die schlechten Z\u00e4hren/", "tokens": ["Ihr", "spah\u00b7ret", "nur", "die", "schlech\u00b7ten", "Z\u00e4h\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ergiesset euch/ ich kans nicht wehren/", "tokens": ["Er\u00b7gies\u00b7set", "euch", "/", "ich", "kans", "nicht", "weh\u00b7ren", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$(", "PPER", "VMFIN", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Quilt/ wie ein frischer Brunnen thut.", "tokens": ["Quilt", "/", "wie", "ein", "fri\u00b7scher", "Brun\u00b7nen", "thut", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$(", "KOKOM", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der herbe Schmertz/ so meine Brust bekrieget/", "tokens": ["Der", "her\u00b7be", "Schmertz", "/", "so", "mei\u00b7ne", "Brust", "be\u00b7krie\u00b7get", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "ADV", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wird nimmermehr durch Fr\u00f6lichkeit besieget", "tokens": ["Wird", "nim\u00b7mer\u00b7mehr", "durch", "Fr\u00f6\u00b7lich\u00b7keit", "be\u00b7sie\u00b7get"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Mein mattes Hertze furchtsahm bebt/", "tokens": ["Mein", "mat\u00b7tes", "Hert\u00b7ze", "furcht\u00b7sahm", "bebt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "VVFIN", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der C\u00f6rper wird sehr hart gequ\u00e4hlet/", "tokens": ["Der", "C\u00f6r\u00b7per", "wird", "sehr", "hart", "ge\u00b7qu\u00e4h\u00b7let", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wie! da\u00df ich nicht schon l\u00e4ngst entseelet!", "tokens": ["Wie", "!", "da\u00df", "ich", "nicht", "schon", "l\u00e4ngst", "ent\u00b7see\u00b7let", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "KOUS", "PPER", "PTKNEG", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das Schicksahl! so beym Sternen schwebt/", "tokens": ["Das", "Schick\u00b7sahl", "!", "so", "beym", "Ster\u00b7nen", "schwebt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "ADV", "APPRART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Hat mich wol recht zum Marter-Holtz ersehen/", "tokens": ["Hat", "mich", "wol", "recht", "zum", "Mar\u00b7ter\u00b7Holtz", "er\u00b7se\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "APPRART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ich soll davon/ und darf doch nicht vergehen.", "tokens": ["Ich", "soll", "da\u00b7von", "/", "und", "darf", "doch", "nicht", "ver\u00b7ge\u00b7hen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PAV", "$(", "KON", "VMFIN", "ADV", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Die starcke St\u00fctze ist entzwey/", "tokens": ["Die", "star\u00b7cke", "St\u00fct\u00b7ze", "ist", "ent\u00b7zwey", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Worauf ich mich vor dem gelehnet;", "tokens": ["Wo\u00b7rauf", "ich", "mich", "vor", "dem", "ge\u00b7leh\u00b7net", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "PRF", "APPR", "ART", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wornach der Geist sich sonst gesehnet/", "tokens": ["Wor\u00b7nach", "der", "Geist", "sich", "sonst", "ge\u00b7seh\u00b7net", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PRF", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ist nicht mehr da/ und schon vorbey.", "tokens": ["Ist", "nicht", "mehr", "da", "/", "und", "schon", "vor\u00b7bey", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "ADV", "ADV", "$(", "KON", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ein Donner schl\u00e4gt mit Sturm und lichten Blitzen", "tokens": ["Ein", "Don\u00b7ner", "schl\u00e4gt", "mit", "Sturm", "und", "lich\u00b7ten", "Blit\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Starck zu mir ein/ wer wil mich nun besch\u00fctzen?", "tokens": ["Starck", "zu", "mir", "ein", "/", "wer", "wil", "mich", "nun", "be\u00b7sch\u00fct\u00b7zen", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "ART", "$(", "PWS", "VMFIN", "PRF", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Mein helles Sonnen-Licht ist fort/", "tokens": ["Mein", "hel\u00b7les", "Son\u00b7nen\u00b7Licht", "ist", "fort", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Himmel ist durchaus verdunckelt/", "tokens": ["Der", "Him\u00b7mel", "ist", "durc\u00b7haus", "ver\u00b7dun\u00b7ckelt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "So da\u00df kein Freuden Stern mehr funckelt;", "tokens": ["So", "da\u00df", "kein", "Freu\u00b7den", "Stern", "mehr", "fun\u00b7ckelt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PIAT", "NN", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Hier ist der rechte Jammer-Ort/", "tokens": ["Hier", "ist", "der", "rech\u00b7te", "Jam\u00b7mer\u00b7Ort", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wo nichts als Nacht und d\u00fcstre Finsternissen", "tokens": ["Wo", "nichts", "als", "Nacht", "und", "d\u00fcst\u00b7re", "Fins\u00b7ter\u00b7nis\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PIS", "KOKOM", "NN", "KON", "VVFIN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die matte Brust und schwachen Geist umschliessen.", "tokens": ["Die", "mat\u00b7te", "Brust", "und", "schwa\u00b7chen", "Geist", "um\u00b7schlies\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Wo soll ich nun ein helles Licht", "tokens": ["Wo", "soll", "ich", "nun", "ein", "hel\u00b7les", "Licht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VMFIN", "PPER", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In meiner Angst und Schrecken finden?", "tokens": ["In", "mei\u00b7ner", "Angst", "und", "Schre\u00b7cken", "fin\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die Dunckelheit wil nicht verschwinden", "tokens": ["Die", "Dun\u00b7ckel\u00b7heit", "wil", "nicht", "ver\u00b7schwin\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "PTKNEG", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Bis mir der Tod das Hertze bricht.", "tokens": ["Bis", "mir", "der", "Tod", "das", "Hert\u00b7ze", "bricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wohlan mein Geist! bereite dich zum Sterben", "tokens": ["Wo\u00b7hlan", "mein", "Geist", "!", "be\u00b7rei\u00b7te", "dich", "zum", "Ster\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "NN", "$.", "VVFIN", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Du kanst doch nichts als Tod und Grab erwerben.", "tokens": ["Du", "kanst", "doch", "nichts", "als", "Tod", "und", "Grab", "er\u00b7wer\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PIS", "KOKOM", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "So sage ich denn gute Nacht", "tokens": ["So", "sa\u00b7ge", "ich", "denn", "gu\u00b7te", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Welt und ihren falschen Freuden/", "tokens": ["Der", "Welt", "und", "ih\u00b7ren", "fal\u00b7schen", "Freu\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Womit sie will die Sinnen weiden/", "tokens": ["Wo\u00b7mit", "sie", "will", "die", "Sin\u00b7nen", "wei\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VMFIN", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dasselbe wird von mir verlacht.", "tokens": ["Das\u00b7sel\u00b7be", "wird", "von", "mir", "ver\u00b7lacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mein Ohr ist taub die Lieder anzuh\u00f6ren/", "tokens": ["Mein", "Ohr", "ist", "taub", "die", "Lie\u00b7der", "an\u00b7zu\u00b7h\u00f6\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "ART", "NN", "VVIZU", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Damit sie denckt mein Hertze zu beth\u00f6ren.", "tokens": ["Da\u00b7mit", "sie", "denckt", "mein", "Hert\u00b7ze", "zu", "be\u00b7th\u00f6\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "PPOSAT", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}