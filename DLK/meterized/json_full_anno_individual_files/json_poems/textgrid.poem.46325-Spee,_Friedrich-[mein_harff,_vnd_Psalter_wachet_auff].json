{"textgrid.poem.46325": {"metadata": {"author": {"name": "Spee, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[mein harff, vnd Psalter wachet auff]", "genre": "verse", "period": "N.A.", "pub_year": 1613, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein harff, vnd Psalter wachet auff,", "tokens": ["Mein", "harff", ",", "vnd", "Psal\u00b7ter", "wa\u00b7chet", "auff", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "KON", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie braucht man Euch so selten?", "tokens": ["Wie", "braucht", "man", "Euch", "so", "sel\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PIS", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die s\u00fcsse seiten stimmt zu hauff,", "tokens": ["Die", "s\u00fcs\u00b7se", "sei\u00b7ten", "stimmt", "zu", "hauff", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dem Sch\u00f6pffer mu\u00df es gelten.", "tokens": ["Dem", "Sch\u00f6pf\u00b7fer", "mu\u00df", "es", "gel\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Weich tod, ich hab noch lebens frist,", "tokens": ["Weich", "tod", ",", "ich", "hab", "noch", "le\u00b7bens", "frist", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "$,", "PPER", "VAFIN", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Will noch