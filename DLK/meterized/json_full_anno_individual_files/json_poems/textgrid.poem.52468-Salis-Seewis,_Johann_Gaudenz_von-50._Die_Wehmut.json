{"textgrid.poem.52468": {"metadata": {"author": {"name": "Salis-Seewis, Johann Gaudenz von", "birth": "N.A.", "death": "N.A."}, "title": "50. Die Wehmut", "genre": "verse", "period": "N.A.", "pub_year": 1798, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mit leisen Harfent\u00f6nen", "tokens": ["Mit", "lei\u00b7sen", "Har\u00b7fen\u00b7t\u00f6\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sei, Wehmut, mir gegr\u00fc\u00dft!", "tokens": ["Sei", ",", "Weh\u00b7mut", ",", "mir", "ge\u00b7gr\u00fc\u00dft", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "$,", "NN", "$,", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "O Nymphe, die der Thr\u00e4nen", "tokens": ["O", "Nym\u00b7phe", ",", "die", "der", "Thr\u00e4\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NN", "$,", "PRELS", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Geweihten Quell verschlie\u00dft!", "tokens": ["Ge\u00b7weih\u00b7ten", "Quell", "ver\u00b7schlie\u00dft", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Mich weht an deiner Schwelle", "tokens": ["Mich", "weht", "an", "dei\u00b7ner", "Schwel\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ein linder Schauer an,", "tokens": ["Ein", "lin\u00b7der", "Schau\u00b7er", "an", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und deines Zwielichts Helle", "tokens": ["Und", "dei\u00b7nes", "Zwie\u00b7lichts", "Hel\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Glimmt auf des Schicksals Bahn.", "tokens": ["Glimmt", "auf", "des", "Schick\u00b7sals", "Bahn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Du, so die Freude weinen,", "tokens": ["Du", ",", "so", "die", "Freu\u00b7de", "wei\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Schwermut l\u00e4cheln hei\u00dft,", "tokens": ["Die", "Schwer\u00b7mut", "l\u00e4\u00b7cheln", "hei\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Kannst Wonn' und Schmerz vereinen,", "tokens": ["Kannst", "Wonn'", "und", "Schmerz", "ver\u00b7ei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da\u00df Harm in Lust verfleu\u00dft;", "tokens": ["Da\u00df", "Harm", "in", "Lust", "ver\u00b7fleu\u00dft", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Du hellst bew\u00f6lkte L\u00fcfte", "tokens": ["Du", "hellst", "be\u00b7w\u00f6lk\u00b7te", "L\u00fcf\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Mit Abendsonnenschein,", "tokens": ["Mit", "A\u00b7bend\u00b7son\u00b7nen\u00b7schein", ","], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "H\u00e4ngst Lampen in die Gr\u00fcfte", "tokens": ["H\u00e4ngst", "Lam\u00b7pen", "in", "die", "Gr\u00fcf\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und kr\u00f6nst den Leichenstein.", "tokens": ["Und", "kr\u00f6nst", "den", "Lei\u00b7chen\u00b7stein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Du nahst, wenn schon die Klage", "tokens": ["Du", "nahst", ",", "wenn", "schon", "die", "Kla\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "ADV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Den Busen sanfter dehnt,", "tokens": ["Den", "Bu\u00b7sen", "sanf\u00b7ter", "dehnt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Gram an Sarkophage", "tokens": ["Der", "Gram", "an", "Sar\u00b7ko\u00b7pha\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die m\u00fcden Schl\u00e4fe lehnt;", "tokens": ["Die", "m\u00fc\u00b7den", "Schl\u00e4\u00b7fe", "lehnt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wenn die Geduld gelassen", "tokens": ["Wenn", "die", "Ge\u00b7duld", "ge\u00b7las\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Sich an die Hoffnung schmiegt,", "tokens": ["Sich", "an", "die", "Hoff\u00b7nung", "schmiegt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Der Z\u00e4hren Tau im nassen,", "tokens": ["Der", "Z\u00e4h\u00b7ren", "Tau", "im", "nas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPRART", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Schmerzlosen Blick versiegt.", "tokens": ["Schmerz\u00b7lo\u00b7sen", "Blick", "ver\u00b7siegt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVPP", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.4": {"line.1": {"text": "Du, die auf Blumenleichen", "tokens": ["Du", ",", "die", "auf", "Blu\u00b7men\u00b7lei\u00b7chen"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["PPER", "$,", "PRELS", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Des Tiefsinns Wimper senkt,", "tokens": ["Des", "Tief\u00b7sinns", "Wim\u00b7per", "senkt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Bei bl\u00e4tterlosen Str\u00e4uchen", "tokens": ["Bei", "bl\u00e4t\u00b7ter\u00b7lo\u00b7sen", "Str\u00e4u\u00b7chen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Bl\u00fctenzeit gedenkt,", "tokens": ["Der", "Bl\u00fc\u00b7ten\u00b7zeit", "ge\u00b7denkt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "In Florens bunte Kronen", "tokens": ["In", "Flo\u00b7rens", "bun\u00b7te", "Kro\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NE", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ein dunkles Veilchen webt,", "tokens": ["Ein", "dunk\u00b7les", "Veil\u00b7chen", "webt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und still, mit Alcyonen,", "tokens": ["Und", "still", ",", "mit", "Al\u00b7cy\u00b7o\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Um Schiffbruchstr\u00fcmmer schwebt:", "tokens": ["Um", "Schiff\u00b7bruchs\u00b7tr\u00fcm\u00b7mer", "schwebt", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUI", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "O du, die sich so gerne", "tokens": ["O", "du", ",", "die", "sich", "so", "ger\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NE", "$,", "PRELS", "PRF", "ADV", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zur\u00fcck zur Kindheit tr\u00e4umt,", "tokens": ["Zu\u00b7r\u00fcck", "zur", "Kind\u00b7heit", "tr\u00e4umt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Selbst ihr Gew\u00f6lk von ferne", "tokens": ["Selbst", "ihr", "Ge\u00b7w\u00f6lk", "von", "fer\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "NN", "APPR", "ADJA"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Mit Sonnengold bes\u00e4umt;", "tokens": ["Mit", "Son\u00b7nen\u00b7gold", "be\u00b7s\u00e4umt", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Was uns Erinn'rung schildert,", "tokens": ["Was", "uns", "Er\u00b7inn'\u00b7rung", "schil\u00b7dert", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Mit stillem Glanz verbr\u00e4mt,", "tokens": ["Mit", "stil\u00b7lem", "Glanz", "ver\u00b7br\u00e4mt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Der Trennung Qualen mildert", "tokens": ["Der", "Tren\u00b7nung", "Qua\u00b7len", "mil\u00b7dert"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und die Verzweiflung z\u00e4hmt;", "tokens": ["Und", "die", "Ver\u00b7zwei\u00b7flung", "z\u00e4hmt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Der Leidenschaften Horden,", "tokens": ["Der", "Lei\u00b7den\u00b7schaf\u00b7ten", "Hor\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Sorgen Rabenzug", "tokens": ["Der", "Sor\u00b7gen", "Ra\u00b7ben\u00b7zug"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Entfliehn vor den Akkorden,", "tokens": ["Ent\u00b7fliehn", "vor", "den", "Ak\u00b7kor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+---+-", "measure": "dactylic.init"}, "line.4": {"text": "Die deine Harfe schlug;", "tokens": ["Die", "dei\u00b7ne", "Har\u00b7fe", "schlug", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Du zauberst Alpens\u00f6hnen,", "tokens": ["Du", "zau\u00b7berst", "Al\u00b7pen\u00b7s\u00f6h\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Verbannt auf Flanderns Moor,", "tokens": ["Ver\u00b7bannt", "auf", "Flan\u00b7derns", "Moor", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Mit Sennenreigent\u00f6nen", "tokens": ["Mit", "Sen\u00b7nen\u00b7rei\u00b7gen\u00b7t\u00f6\u00b7nen"], "token_info": ["word", "word"], "pos": ["APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Der Heimat Bilder vor.", "tokens": ["Der", "Hei\u00b7mat", "Bil\u00b7der", "vor", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "In deinen Schattenhallen", "tokens": ["In", "dei\u00b7nen", "Schat\u00b7ten\u00b7hal\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Weihst du die S\u00e4nger ein,", "tokens": ["Weihst", "du", "die", "S\u00e4n\u00b7ger", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Lehrst junge Nachtigallen", "tokens": ["Lehrst", "jun\u00b7ge", "Nach\u00b7ti\u00b7gal\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Trauermelodei'n;", "tokens": ["Die", "Trau\u00b7er\u00b7me\u00b7lo\u00b7de\u00b7i'n", ";"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Du neigst, wo Gr\u00e4ber gr\u00fcnen,", "tokens": ["Du", "neigst", ",", "wo", "Gr\u00e4\u00b7ber", "gr\u00fc\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Dein Ohr zu H\u00f6ltys Ton;", "tokens": ["Dein", "Ohr", "zu", "H\u00f6l\u00b7tys", "Ton", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "NE", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Pfl\u00fcckst Moos von Burgruinen", "tokens": ["Pfl\u00fcckst", "Moos", "von", "Bur\u00b7gru\u00b7i\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "NN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Mit meinem Matthisson.", "tokens": ["Mit", "mei\u00b7nem", "Mat\u00b7this\u00b7son", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "R\u00fchr unter Thr\u00e4nenweiden", "tokens": ["R\u00fchr", "un\u00b7ter", "Thr\u00e4\u00b7nen\u00b7wei\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["NN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Noch oft mein Saitenspiel;", "tokens": ["Noch", "oft", "mein", "Sai\u00b7ten\u00b7spiel", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Verschmilz auch Gram und Leiden", "tokens": ["Ver\u00b7schmilz", "auch", "Gram", "und", "Lei\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In s\u00fc\u00dfes Nachgef\u00fchl;", "tokens": ["In", "s\u00fc\u00b7\u00dfes", "Nach\u00b7ge\u00b7f\u00fchl", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Gieb St\u00e4rkung dem Erweichten!", "tokens": ["Gieb", "St\u00e4r\u00b7kung", "dem", "Er\u00b7weich\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Heb aus dem Trauerflor,", "tokens": ["Heb", "aus", "dem", "Trau\u00b7er\u00b7flor", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.7": {"text": "Wenn Gottes Sterne leuchten,", "tokens": ["Wenn", "Got\u00b7tes", "Ster\u00b7ne", "leuch\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Den Andachtsblick empor!", "tokens": ["Den", "An\u00b7dachts\u00b7blick", "em\u00b7por", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Mit leisen Harfent\u00f6nen", "tokens": ["Mit", "lei\u00b7sen", "Har\u00b7fen\u00b7t\u00f6\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sei, Wehmut, mir gegr\u00fc\u00dft!", "tokens": ["Sei", ",", "Weh\u00b7mut", ",", "mir", "ge\u00b7gr\u00fc\u00dft", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "$,", "NN",