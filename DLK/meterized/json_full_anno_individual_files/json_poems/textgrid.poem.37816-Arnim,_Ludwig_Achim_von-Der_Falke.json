{"textgrid.poem.37816": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Der Falke", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "W\u00e4r ich ein wilder Falke,", "tokens": ["W\u00e4r", "ich", "ein", "wil\u00b7der", "Fal\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ich wollt mich schwingen auf,", "tokens": ["Ich", "wollt", "mich", "schwin\u00b7gen", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und wollt mich niederlassen", "tokens": ["Und", "wollt", "mich", "nie\u00b7der\u00b7las\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Vor meines Grafen Haus.", "tokens": ["Vor", "mei\u00b7nes", "Gra\u00b7fen", "Haus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Und wollt mit starken Fl\u00fcgel,", "tokens": ["Und", "wollt", "mit", "star\u00b7ken", "Fl\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da schlagen an Liebchens Th\u00fcr,", "tokens": ["Da", "schla\u00b7gen", "an", "Lieb\u00b7chens", "Th\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NN", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Da\u00df springen sollt der Riegel,", "tokens": ["Da\u00df", "sprin\u00b7gen", "sollt", "der", "Rie\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVINF", "VMFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mein Liebchen tr\u00e4t herf\u00fcr.", "tokens": ["Mein", "Lieb\u00b7chen", "tr\u00e4t", "her\u00b7f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "\u00bbh\u00f6rst du die Schl\u00fcssel klingen,", "tokens": ["\u00bb", "h\u00f6rst", "du", "die", "Schl\u00fcs\u00b7sel", "klin\u00b7gen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Dein Mutter ist nicht weit,", "tokens": ["Dein", "Mut\u00b7ter", "ist", "nicht", "weit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So zieh mit mir von hinnen", "tokens": ["So", "zieh", "mit", "mir", "von", "hin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PPER", "APPR", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wohl \u00fcber die Heide breit.\u00ab", "tokens": ["Wohl", "\u00fc\u00b7ber", "die", "Hei\u00b7de", "breit", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ADJD", "$.", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Und wollt in ihrem Nacken", "tokens": ["Und", "wollt", "in", "ih\u00b7rem", "Na\u00b7cken"], "token_info": ["word", 