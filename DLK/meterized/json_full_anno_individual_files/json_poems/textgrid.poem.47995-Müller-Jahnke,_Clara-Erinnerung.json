{"textgrid.poem.47995": {"metadata": {"author": {"name": "M\u00fcller-Jahnke, Clara", "birth": "N.A.", "death": "N.A."}, "title": "Erinnerung", "genre": "verse", "period": "N.A.", "pub_year": 1882, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Selige Kinderzeit!", "tokens": ["Se\u00b7li\u00b7ge", "Kin\u00b7der\u00b7zeit", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "Aus der Vergangenheit", "tokens": ["Aus", "der", "Ver\u00b7gan\u00b7gen\u00b7heit"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "d\u00e4mmernden Tiefen", "tokens": ["d\u00e4m\u00b7mern\u00b7den", "Tie\u00b7fen"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "tauchst du, ein Stern, herauf, \u2013", "tokens": ["tauchst", "du", ",", "ein", "Stern", ",", "her\u00b7auf", ",", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "$,", "ART", "NN", "$,", "PTKVZ", "$,", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "schl\u00e4gst du die Augen auf,", "tokens": ["schl\u00e4gst", "du", "die", "Au\u00b7gen", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "die \u2013 ach, wie lange Zeit! \u2013", "tokens": ["die", "\u2013", "ach", ",", "wie", "lan\u00b7ge", "Zeit", "!", "\u2013"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ART", "$(", "XY", "$,", "PWAV", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "m\u00fcde schon schliefen!",