{"textgrid.poem.49308": {"metadata": {"author": {"name": "Rist, Johann", "birth": "N.A.", "death": "N.A."}, "title": "Fr\u00fchlingsgedicht", "genre": "verse", "period": "N.A.", "pub_year": 1637, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Daphnis wolte Blumen brechen,", "tokens": ["Daph\u00b7nis", "wol\u00b7te", "Blu\u00b7men", "bre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Als der Merz den Fr\u00fchling bracht;", "tokens": ["Als", "der", "Merz", "den", "Fr\u00fch\u00b7ling", "bracht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ach, sagt er, wer kan aussprechen", "tokens": ["Ach", ",", "sagt", "er", ",", "wer", "kan", "aus\u00b7spre\u00b7chen"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ITJ", "$,", "VVFIN", "PPER", "$,", "PWS", "VMFIN", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Meiner bittern Liebe Macht,", "tokens": ["Mei\u00b7ner", "bit\u00b7tern", "Lie\u00b7be", "Macht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Liebe, die mich hat betrogen,", "tokens": ["Lie\u00b7be", ",", "die", "mich", "hat", "be\u00b7tro\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPER", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Da\u00df ich bin \u00fcmher gezogen", "tokens": ["Da\u00df", "ich", "bin", "\u00fcm\u00b7her", "ge\u00b7zo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VAFIN", "ADJD", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Durch die W\u00e4lder Tag und Nacht!", "tokens": ["Durch", "die", "W\u00e4l\u00b7der", "Tag", "und", "Nacht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Die\u00df sind ja die ersten Fr\u00fcchte", "tokens": ["Die\u00df", "sind", "ja", "die", "ers\u00b7ten", "Fr\u00fcch\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ADV", "ART", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "Von den Blumen dieser Zeit,", "tokens": ["Von", "den", "Blu\u00b7men", "die\u00b7ser", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PDAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Da der V\u00f6gel Klinggedichte", "tokens": ["Da", "der", "V\u00f6\u00b7gel", "Kling\u00b7ge\u00b7dich\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.11": {"text": "Menschen, Vieh und Feld erfreut;", "tokens": ["Men\u00b7schen", ",", "Vieh", "und", "Feld", "er\u00b7freut", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "Die\u00df sind zwar die ersten Gaben,", "tokens": ["Die\u00df", "sind", "zwar", "die", "ers\u00b7ten", "Ga\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "----+-+-", "measure": "unknown.measure.di"}, "line.13": {"text": "Die wir von den Wiesen haben", "tokens": ["Die", "wir", "von", "den", "Wie\u00b7sen", "ha\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "APPR", "ART", "NN", "VAFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Durch des Himmels G\u00fctigkeit.", "tokens": ["Durch", "des", "Him\u00b7mels", "G\u00fc\u00b7tig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.15": {"text": "Aber wenn werd' ich erlangen,", "tokens": ["A\u00b7ber", "wenn", "werd'", "ich", "er\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "VAFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "O mein Bl\u00fcmlein Galathe,", "tokens": ["O", "mein", "Bl\u00fcm\u00b7lein", "Ga\u00b7la\u00b7the", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.17": {"text": "Dich wie andre zu \u00fcmfangen,", "tokens": ["Dich", "wie", "and\u00b7re", "zu", "\u00fcm\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "KOKOM", "PIS", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Die ich jetzt f\u00fcr Augen seh?", "tokens": ["Die", "ich", "jetzt", "f\u00fcr", "Au\u00b7gen", "seh", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.19": {"text": "Ach, wenn werd' ich doch ber\u00fchren", "tokens": ["Ach", ",", "wenn", "werd'", "ich", "doch", "be\u00b7r\u00fch\u00b7ren"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "KOUS", "VAFIN", "PPER", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Dich, die du mich pflegst zu f\u00fchren", "tokens": ["Dich", ",", "die", "du", "mich", "pflegst", "zu", "f\u00fch\u00b7ren"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "$,", "PRELS", "PPER", "PRF", "VVFIN", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "Durch den Regen, Reif und Schnee?", "tokens": ["Durch", "den", "Re\u00b7gen", ",", "Reif", "und", "Schnee", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.22": {"text": "Diese Bl\u00fcmlein darf ich tragen", "tokens": ["Die\u00b7se", "Bl\u00fcm\u00b7lein", "darf", "ich", "tra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDAT", "NN", "VMFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.23": {"text": "Mit mir heim in mein Gezelt;", "tokens": ["Mit", "mir", "heim", "in", "mein", "Ge\u00b7zelt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PTKVZ", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.24": {"text": "Aber dich, mein Lieb, zu fragen,", "tokens": ["A\u00b7ber", "dich", ",", "mein", "Lieb", ",", "zu", "fra\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "PPOSAT", "NN", "$,", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.25": {"text": "Ob dir auch ein Ku\u00df gef\u00e4llt,", "tokens": ["Ob", "dir", "auch", "ein", "Ku\u00df", "ge\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.26": {"text": "Darf ich kaum mich unterstehen,", "tokens": ["Darf", "ich", "kaum", "mich", "un\u00b7ter\u00b7ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.27": {"text": "Weil ich nie ein Bild gesehen,", "tokens": ["Weil", "ich", "nie", "ein", "Bild", "ge\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.28": {"text": "Das dir gleichet in der Welt.", "tokens": ["Das", "dir", "glei\u00b7chet", "in", "der", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.29": {"text": "Dieses Bl\u00fcmlein zu gewinnen,", "tokens": ["Die\u00b7ses", "Bl\u00fcm\u00b7lein", "zu", "ge\u00b7win\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.30": {"text": "Kostet weder M\u00fch' noch List;", "tokens": ["Kos\u00b7tet", "we\u00b7der", "M\u00fch'", "noch", "List", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "ADV", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.31": {"text": "Aber ach! da\u00df du von Sinnen", "tokens": ["A\u00b7ber", "ach", "!", "da\u00df", "du", "von", "Sin\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADV", "$.", "KOUS", "PPER", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.32": {"text": "So ganz hart und steinern bist!", "tokens": ["So", "ganz", "hart", "und", "stei\u00b7nern", "bist", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "KON", "VVINF", "VAFIN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.33": {"text": "Keine wei\u00df ich dir zu gleichen,", "tokens": ["Kei\u00b7ne", "wei\u00df", "ich", "dir", "zu", "glei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.34": {"text": "Weil dich niemand kan erweichen,", "tokens": ["Weil", "dich", "nie\u00b7mand", "kan", "er\u00b7wei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIS", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.35": {"text": "Wenn er noch so redlich ist.", "tokens": ["Wenn", "er", "noch", "so", "red\u00b7lich", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "ADJD", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.36": {"text": "K\u00f6nt' ich deine zarten Glieder", "tokens": ["K\u00f6nt'", "ich", "dei\u00b7ne", "zar\u00b7ten", "Glie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.37": {"text": "Stets verwandeln, wenn ich wolt',", "tokens": ["Stets", "ver\u00b7wan\u00b7deln", ",", "wenn", "ich", "wolt'", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "$,", "KOUS", "PPER", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.38": {"text": "Und dich denn verkehren wieder,", "tokens": ["Und", "dich", "denn", "ver\u00b7keh\u00b7ren", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "VVFIN", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.39": {"text": "Fragt' ich nichts nach Geld und Gold;", "tokens": ["Fragt'", "ich", "nichts", "nach", "Geld", "und", "Gold", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIS", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.40": {"text": "Nun wolt' ich f\u00fcr alle Sachen", "tokens": ["Nun", "wolt'", "ich", "f\u00fcr", "al\u00b7le", "Sa\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "PIAT", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.41": {"text": "Solch ein Bl\u00fcmlein aus dir machen,", "tokens": ["Solch", "ein", "Bl\u00fcm\u00b7lein", "aus", "dir", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ART", "NN", "APPR", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.42": {"text": "Das mich stets erfreuen solt'.", "tokens": ["Das", "mich", "stets", "er\u00b7freu\u00b7en", "solt'", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ADV", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.43": {"text": "O wie wolt' ich dich bewahren", "tokens": ["O", "wie", "wolt'", "ich", "dich", "be\u00b7wah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "PWAV", "VMFIN", "PPER", "PRF", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.44": {"text": "In dem Garten meiner Treu!", "tokens": ["In", "dem", "Gar\u00b7ten", "mei\u00b7ner", "Treu", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.45": {"text": "Ei, denn soltestu erfahren,", "tokens": ["Ei", ",", "denn", "sol\u00b7tes\u00b7tu", "er\u00b7fah\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.46": {"text": "Sch\u00f6nste Blum', was lieben sei!", "tokens": ["Sch\u00f6ns\u00b7te", "Blum'", ",", "was", "lie\u00b7ben", "sei", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PWS", "VVINF", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.47": {"text": "Denn so wolt' ich dich mit Freuden", "tokens": ["Denn", "so", "wolt'", "ich", "dich", "mit", "Freu\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VMFIN", "PPER", "PRF", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.48": {"text": "K\u00fcssen auf mein schweres Leiden", "tokens": ["K\u00fcs\u00b7sen", "auf", "mein", "schwe\u00b7res", "Lei\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.49": {"text": "Tag und Nacht ohn' alle Scheu.", "tokens": ["Tag", "und", "Nacht", "ohn'", "al\u00b7le", "Scheu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "APPR", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.50": {"text": "Brich die Sinnen, Galathee,", "tokens": ["Brich", "die", "Sin\u00b7nen", ",", "Ga\u00b7la\u00b7thee", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "ART", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.51": {"text": "Zwinge doch den harten Mut,", "tokens": ["Zwin\u00b7ge", "doch", "den", "har\u00b7ten", "Mut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.52": {"text": "G\u00f6nne Daphnis, da\u00df er sehe", "tokens": ["G\u00f6n\u00b7ne", "Daph\u00b7nis", ",", "da\u00df", "er", "se\u00b7he"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NE", "$,", "KOUS", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.53": {"text": "Dich, sein allerh\u00f6chstes Gut!", "tokens": ["Dich", ",", "sein", "al\u00b7ler\u00b7h\u00f6chs\u00b7tes", "Gut", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.54": {"text": "Sei den Lilien gleich von Herzen,", "tokens": ["Sei", "den", "Li\u00b7li\u00b7en", "gleich", "von", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADV", "APPR", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.55": {"text": "Die nicht stets mit Stachlen scherzen,", "tokens": ["Die", "nicht", "stets", "mit", "Stach\u00b7len", "scher\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "ADV", "APPR", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.56": {"text": "Wie die falsche Rose thut.", "tokens": ["Wie", "die", "fal\u00b7sche", "Ro\u00b7se", "thut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.57": {"text": "Ach, bedenke doch die Thr\u00e4nen,", "tokens": ["Ach", ",", "be\u00b7den\u00b7ke", "doch", "die", "Thr\u00e4\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.58": {"tex