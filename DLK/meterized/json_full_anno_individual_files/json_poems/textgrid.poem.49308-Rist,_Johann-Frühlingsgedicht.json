t"], "pos": ["VMFIN", "PPER", "ADV", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.27": {"text": "Weil ich nie ein Bild gesehen,", "tokens": ["Weil", "ich", "nie", "ein", "Bild", "ge\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.28": {"text": "Das dir gleichet in der Welt.", "tokens": ["Das", "dir", "glei\u00b7chet", "in", "der", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.29": {"text": "Dieses Bl\u00fcmlein zu gewinnen,", "tokens": ["Die\u00b7ses", "Bl\u00fcm\u00b7lein", "zu", "ge\u00b7win\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.30": {"text": "Kostet weder M\u00fch' noch List;", "tokens": ["Kos\u00b7tet", "we\u00b7der", "M\u00fch'", "noch", "List", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "ADV", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.31": {"text": "Aber ach! da\u00df du von Sinnen", "tokens": ["A\u00b7ber", "ach", "!", "da\u00df", "du", "von", "Sin\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADV", "$.", "KOUS", "PPER", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.32": {"text": "So ganz hart und steinern bist!", "tokens": ["So", "ganz", "hart", "und", "stei\u00b7nern", "bist", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "KON", "VVINF", "VAFIN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.33": {"text": "Keine wei\u00df ich dir 