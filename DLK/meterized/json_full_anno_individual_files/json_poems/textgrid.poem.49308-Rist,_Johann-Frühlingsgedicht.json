d", "word", "word"], "pos": ["NE", "NE", "$,", "KOUS", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.53": {"text": "Dich, sein allerh\u00f6chstes Gut!", "tokens": ["Dich", ",", "sein", "al\u00b7ler\u00b7h\u00f6chs\u00b7tes", "Gut", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.54": {"text": "Sei den Lilien gleich von Herzen,", "tokens": ["Sei", "den", "Li\u00b7li\u00b7en", "gleich", "von", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADV", "APPR", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.55": {"text": "Die nicht stets mit Stachlen scherzen,", "tokens": ["Die", "nicht", "stets", "mit", "Stach\u00b7len", "scher\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "ADV", "APPR", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.56": {"text": "Wie die falsche Rose thut.", "tokens": ["Wie", "die", "fal\u00b7sche", "Ro\u00b7se", "thut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.57": {"text": "Ach, bedenke doch die Thr\u00e4nen,", "tokens": ["Ach", ",", "be\u00b7den\u00b7ke", "doch", "die", "Thr\u00e4\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.58": {"text": "Die dein Sch\u00e4fer manchesmal,", "tokens": ["Die", "dein", "Sch\u00e4\u00b7fer", "man\u00b7ches\u00b7mal", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic