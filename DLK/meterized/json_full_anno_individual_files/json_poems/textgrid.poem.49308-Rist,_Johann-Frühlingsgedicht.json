tra"}, "line.14": {"text": "Durch des Himmels G\u00fctigkeit.", "tokens": ["Durch", "des", "Him\u00b7mels", "G\u00fc\u00b7tig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.15": {"text": "Aber wenn werd' ich erlangen,", "tokens": ["A\u00b7ber", "wenn", "werd'", "ich", "er\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "VAFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "O mein Bl\u00fcmlein Galathe,", "tokens": ["O", "mein", "Bl\u00fcm\u00b7lein", "Ga\u00b7la\u00b7the", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.17": {"text": "Dich wie andre zu \u00fcmfangen,", "tokens": ["Dich", "wie", "and\u00b7re", "zu", "\u00fcm\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "KOKOM", "PIS", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Die ich jetzt f\u00fcr Augen seh?", "tokens": ["Die", "ich", "jetzt", "f\u00fcr", "Au\u00b7gen", "seh", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.19": {"text": "Ach, wenn werd' ich doch ber\u00fchren", "tokens": ["Ach", ",", "wenn", "werd'", "ich", "doch", "be\u00b7r\u00fch\u00b7ren"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "KOUS", "VAFIN", "PPER", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Dich, die du mich pflegst zu f\u00fchren", "tokens": ["Dich", ",", "die", "du", "mich", "pflegst", "zu", "f\u00fch\u00b7ren"], "token_info": ["w