{"dta.poem.11156": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auf einen abgedanckten soldaten.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die beute, die du hast, sind schrammen im gesicht,", "tokens": ["Die", "beu\u00b7te", ",", "die", "du", "hast", ",", "sind", "schram\u00b7men", "im", "ge\u00b7sicht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "PRELS", "PPER", "VAFIN", "$,", "VAFIN", "ADJD", "APPRART", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sind steltzen an dem bein, und kr\u00fccken an den h\u00e4nden.", "tokens": ["Sind", "stelt\u00b7zen", "an", "dem", "bein", ",", "und", "kr\u00fc\u00b7cken", "an", "den", "h\u00e4n\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVINF", "APPR", "ART", "PTKVZ", "$,", "KON", "VVFIN", "APPR", "ART", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Doch wundere dich nicht! So gehts an allen enden:", "tokens": ["Doch", "wun\u00b7de\u00b7re", "dich", "nicht", "!", "So", "gehts", "an", "al\u00b7len", "en\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "$.", "ADV", "VVFIN", "APPR", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wo krieg und kr\u00fcge seynd, da fehlts an scherben nicht.", "tokens": ["Wo", "krieg", "und", "kr\u00fc\u00b7ge", "seynd", ",", "da", "fehlts", "an", "scher\u00b7ben", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "VAFIN", "$,", "ADV", "VVFIN", "APPR", "VVFIN", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}