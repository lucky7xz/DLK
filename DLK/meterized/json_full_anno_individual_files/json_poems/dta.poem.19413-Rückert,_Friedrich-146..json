{"dta.poem.19413": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "146.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nicht Schritte soll man thun, die nur zum Ziele f\u00fchren,", "tokens": ["Nicht", "Schrit\u00b7te", "soll", "man", "thun", ",", "die", "nur", "zum", "Zie\u00b7le", "f\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "VMFIN", "PIS", "VVINF", "$,", "PRELS", "ADV", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der alte Meister sprach's, vielmehr will sichs geb\u00fchren,", "tokens": ["Der", "al\u00b7te", "Meis\u00b7ter", "sprach's", ",", "viel\u00b7mehr", "will", "sichs", "ge\u00b7b\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NE", "$,", "ADV", "VMFIN", "PIS", "VVINF", "$,"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Da\u00df jeder Schritt f\u00fcr sich ein Ziel, und nebenbei", "tokens": ["Da\u00df", "je\u00b7der", "Schritt", "f\u00fcr", "sich", "ein", "Ziel", ",", "und", "ne\u00b7ben\u00b7bei"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PIAT", "NN", "APPR", "PRF", "ART", "NN", "$,", "KON", "APPR"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein Fortschritt auf dem Weg zu h\u00f6herm Ziele sei.", "tokens": ["Ein", "Fort\u00b7schritt", "auf", "dem", "Weg", "zu", "h\u00f6\u00b7herm", "Zie\u00b7le", "sei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "APPR", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Das gilt, wie von der Kunst, vom Leben gleicherweise,", "tokens": ["Das", "gilt", ",", "wie", "von", "der", "Kunst", ",", "vom", "Le\u00b7ben", "glei\u00b7cher\u00b7wei\u00b7se", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "PWAV", "APPR", "ART", "NN", "$,", "APPRART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vorz\u00fcglich und zun\u00e4chst doch gilt es von der Reise.", "tokens": ["Vor\u00b7z\u00fcg\u00b7lich", "und", "zu\u00b7n\u00e4chst", "doch", "gilt", "es", "von", "der", "Rei\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADV", "ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Wenn du als Reiseziel betrachtest jeden Schritt,", "tokens": ["Wenn", "du", "als", "Rei\u00b7se\u00b7ziel", "be\u00b7trach\u00b7test", "je\u00b7den", "Schritt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "KOUS", "NN", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wird dir der Weg ein Spiel, und kommst vom Fleck damit.", "tokens": ["Wird", "dir", "der", "Weg", "ein", "Spiel", ",", "und", "kommst", "vom", "Fleck", "da\u00b7mit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "ART", "NN", "$,", "KON", "VVFIN", "APPRART", "NN", "PAV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}