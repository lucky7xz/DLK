{"dta.poem.9640": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Der andere erzehlet absonderlich die ge-  \n m\u00fcths-gaben Sr. Churfl. Durchl.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wjr haben einen weisen F\u00fcrsten/", "tokens": ["Wjr", "ha\u00b7ben", "ei\u00b7nen", "wei\u00b7sen", "F\u00fcrs\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der fromm/ gerecht und tapffer ist/", "tokens": ["Der", "fromm", "/", "ge\u00b7recht", "und", "tapf\u00b7fer", "ist", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "$(", "ADJD", "KON", "ADJD", "VAFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der nicht (wie viel nach blute d\u00fcrsten/)", "tokens": ["Der", "nicht", "(", "wie", "viel", "nach", "blu\u00b7te", "d\u00fcrs\u00b7ten", "/", ")"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "PTKNEG", "$(", "KOKOM", "ADV", "APPR", "VVFIN", "VMFIN", "$(", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Unterthanen plagt und frist.", "tokens": ["Die", "Un\u00b7ter\u00b7tha\u00b7nen", "plagt", "und", "frist", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Er ist gen\u00e4dig/ liebt die seinen/", "tokens": ["Er", "ist", "ge\u00b7n\u00e4\u00b7dig", "/", "liebt", "die", "sei\u00b7nen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "$(", "VVFIN", "ART", "PPOSAT", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Bleibt aber allen lastern feind;", "tokens": ["Bleibt", "a\u00b7ber", "al\u00b7len", "las\u00b7tern", "feind", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und derer/ die es treulich meinen/", "tokens": ["Und", "de\u00b7rer", "/", "die", "es", "treu\u00b7lich", "mei\u00b7nen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "$(", "PRELS", "PPER", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ist er zugleich ein guter freund.", "tokens": ["Ist", "er", "zu\u00b7gleich", "ein", "gu\u00b7ter", "freund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}