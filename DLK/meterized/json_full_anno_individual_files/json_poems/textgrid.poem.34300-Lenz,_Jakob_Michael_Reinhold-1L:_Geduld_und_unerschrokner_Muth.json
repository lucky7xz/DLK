d Nacht der Schmerz", "tokens": ["So", "m\u00fcs\u00b7se", "Tag", "und", "Nacht", "der", "Schmerz"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "NN", "KON", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dir fremde seyn und Lust und Scherz", "tokens": ["Dir", "frem\u00b7de", "seyn", "und", "Lust", "und", "Scherz"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADJA", "VAINF", "KON", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dein sch\u00f6nes Herz bewohnen.", "tokens": ["Dein", "sch\u00f6\u00b7nes", "Herz", "be\u00b7woh\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Alsdann mein Kind ist alles gut,", "tokens": ["Als\u00b7dann", "mein", "Kind", "ist", "al\u00b7les", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VAFIN", "PIS", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Alsdann so mag mein junges Blut", "tokens": ["Als\u00b7dann", "so", "mag", "mein", "jun\u00b7ges", "Blut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VMFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "F\u00fcr dich die Erde f\u00e4rben.", "tokens": ["F\u00fcr", "dich", "die", "Er\u00b7de", "f\u00e4r\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Es ist mir sonst nichts f\u00fcrchterlich", "tokens": ["Es", "ist", "mir", "sonst", "nichts", "f\u00fcrch\u00b7ter\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "PIS", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Als dich betr\u00fcbt zu sehen, dich!", "tokens": ["Als", "dich", "be\u00b7tr\u00fcbt", "zu", "se\u00b7hen", ",", "dich", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "PTKZU", "VVINF", "$,", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Viel sanfter thuts zu sterben.", "tokens": ["Viel", "sanf\u00b7ter", "thuts", "zu", "ster\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Drum fleh ich, heitre dein Gesicht,", "tokens": ["Drum", "fleh", "ich", ",", "heit\u00b7re", "dein", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "$,", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich scheue H\u00f6ll und Himmel nicht,", "tokens": ["Ich", "scheu\u00b7e", "H\u00f6ll", "und", "Him\u00b7mel", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bleibt mir dein Auge offen.", "tokens": ["Bleibt", "mir", "dein", "Au\u00b7ge", "of\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wenn du vergn\u00fcgt und gl\u00fccklich bist,", "tokens": ["Wenn", "du", "ver\u00b7gn\u00fcgt", "und", "gl\u00fcck\u00b7lich", "bist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "KON", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und st\u00fcnd ich auf dem Richtger\u00fcst,", "tokens": ["Und", "st\u00fcnd", "ich", "auf", "dem", "Richt\u00b7ge\u00b7r\u00fcst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dann ist mein Ziel getroffen.", "tokens": ["Dann", "ist", "mein", "Ziel", "ge\u00b7trof\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Und w\u00e4r ich in der Sklaverey,", "tokens": ["Und", "w\u00e4r", "ich", "in", "der", "Skla\u00b7ve\u00b7rey", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und h\u00e4tte nur den Trost dabey,", "tokens": ["Und", "h\u00e4t\u00b7te", "nur", "den", "Trost", "da\u00b7bey", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ART", "NN", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "F\u00fcr dich f\u00fcr dich zu leiden;", "tokens": ["F\u00fcr", "dich", "f\u00fcr", "dich", "zu", "lei\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPR", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und w\u00e4r ich jenseits \u00fcberm Meer", "tokens": ["Und", "w\u00e4r", "ich", "jen\u00b7seits", "\u00fc\u00b7berm", "Meer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+", "measu