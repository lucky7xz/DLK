{"textgrid.poem.34300": {"metadata": {"author": {"name": "Lenz, Jakob Michael Reinhold", "birth": "N.A.", "death": "N.A."}, "title": "1L: Geduld und unerschrokner Muth", "genre": "verse", "period": "N.A.", "pub_year": 1771, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Geduld und unerschrokner Muth", "tokens": ["Ge\u00b7duld", "und", "un\u00b7er\u00b7schrok\u00b7ner", "Muth"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Beseelen mein getreues Blut", "tokens": ["Be\u00b7see\u00b7len", "mein", "ge\u00b7treu\u00b7es", "Blut"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und f\u00fcrcht mich nicht zu sterben.", "tokens": ["Und", "f\u00fcrcht", "mich", "nicht", "zu", "ster\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Himmel kostet Leiden hier,", "tokens": ["Der", "Him\u00b7mel", "kos\u00b7tet", "Lei\u00b7den", "hier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich leide froh, kann ich von dir", "tokens": ["Ich", "lei\u00b7de", "froh", ",", "kann", "ich", "von", "dir"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "$,", "VMFIN", "PPER", "APPR", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mir einen Blick erwerben.", "tokens": ["Mir", "ei\u00b7nen", "Blick", "er\u00b7wer\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Nur du verdienst begl\u00fcckt zu seyn,", "tokens": ["Nur", "du", "ver\u00b7dienst", "be\u00b7gl\u00fcckt", "zu", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ADV", "VVPP", "PTKZU", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Drum will ich gerne Gram und Pein", "tokens": ["Drum", "will", "ich", "ger\u00b7ne", "Gram", "und", "Pein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VMFIN", "PPER", "ADV", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In meine Brust verschliessen.", "tokens": ["In", "mei\u00b7ne", "Brust", "ver\u00b7schlies\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Den Tr\u00e4nen will ich wiederstehn,", "tokens": ["Den", "Tr\u00e4\u00b7nen", "will", "ich", "wie\u00b7ders\u00b7tehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Du Engel sollst sie nimmer sehn", "tokens": ["Du", "En\u00b7gel", "sollst", "sie", "nim\u00b7mer", "sehn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "NE", "VMFIN", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Auf meine Wangen fliessen.", "tokens": ["Auf", "mei\u00b7ne", "Wan\u00b7gen", "flies\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ach traue deutscher Redlichkeit,", "tokens": ["Ach", "trau\u00b7e", "deut\u00b7scher", "Red\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die sich zu deinem Dienste weyht,", "tokens": ["Die", "sich", "zu", "dei\u00b7nem", "Diens\u00b7te", "weyht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und willtu sie belohnen,", "tokens": ["Und", "will\u00b7tu", "sie", "be\u00b7loh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So m\u00fcsse Tag und Nacht der Schmerz", "tokens": ["So", "m\u00fcs\u00b7se", "Tag", "und", "Nacht", "der", "Schmerz"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "NN", "KON", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dir fremde seyn und Lust und Scherz", "tokens": ["Dir", "frem\u00b7de", "seyn", "und", "Lust", "und", "Scherz"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADJA", "VAINF", "KON", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dein sch\u00f6nes Herz bewohnen.", "tokens": ["Dein", "sch\u00f6\u00b7nes", "Herz", "be\u00b7woh\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Alsdann mein Kind ist alles gut,", "tokens": ["Als\u00b7dann", "mein", "Kind", "ist", "al\u00b7les", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VAFIN", "PIS", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Alsdann so mag mein junges Blut", "tokens": ["Als\u00b7dann", "so", "mag", "mein", "jun\u00b7ges", "Blut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VMFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "F\u00fcr dich die Erde f\u00e4rben.", "tokens": ["F\u00fcr", "dich", "die", "Er\u00b7de", "f\u00e4r\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Es ist mir sonst nichts f\u00fcrchterlich", "tokens": ["Es", "ist", "mir", "sonst", "nichts", "f\u00fcrch\u00b7ter\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "PIS", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Als dich betr\u00fcbt zu sehen, dich!", "tokens": ["Als", "dich", "be\u00b7tr\u00fcbt", "zu", "se\u00b7hen", ",", "dich", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "PTKZU", "VVINF", "$,", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Viel sanfter thuts zu sterben.", "tokens": ["Viel", "sanf\u00b7ter", "thuts", "zu", "ster\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Drum fleh ich, heitre dein Gesicht,", "tokens": ["Drum", "fleh", "ich", ",", "heit\u00b7re", "dein", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "$,", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich scheue H\u00f6ll und Himmel nicht,", "tokens": ["Ich", "scheu\u00b7e", "H\u00f6ll", "und", "Him\u00b7mel", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bleibt mir dein Auge offen.", "tokens": ["Bleibt", "mir", "dein", "Au\u00b7ge", "of\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wenn du vergn\u00fcgt und gl\u00fccklich bist,", "tokens": ["Wenn", "du", "ver\u00b7gn\u00fcgt", "und", "gl\u00fcck\u00b7lich", "bist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "KON", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und st\u00fcnd ich auf dem Richtger\u00fcst,", "tokens": ["Und", "st\u00fcnd", "ich", "auf", "dem", "Richt\u00b7ge\u00b7r\u00fcst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dann ist mein Ziel getroffen.", "tokens": ["Dann", "ist", "mein", "Ziel", "ge\u00b7trof\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Und w\u00e4r ich in der Sklaverey,", "tokens": ["Und", "w\u00e4r", "ich", "in", "der", "Skla\u00b7ve\u00b7rey", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und h\u00e4tte nur den Trost dabey,", "tokens": ["Und", "h\u00e4t\u00b7te", "nur", "den", "Trost", "da\u00b7bey", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ART", "NN", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "F\u00fcr dich f\u00fcr dich zu leiden;", "tokens": ["F\u00fcr", "dich", "f\u00fcr", "dich", "zu", "lei\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPR", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und w\u00e4r ich jenseits \u00fcberm Meer", "tokens": ["Und", "w\u00e4r", "ich", "jen\u00b7seits", "\u00fc\u00b7berm", "Meer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und w\u00fcst da\u00df Clephchen gl\u00fccklich w\u00e4r,", "tokens": ["Und", "w\u00fcst", "da\u00df", "Cle\u00b7ph\u00b7chen", "gl\u00fcck\u00b7lich", "w\u00e4r", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KOUS", "NE", "ADJD", "VAFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Doch w\u00e4r ich zu beneiden.", "tokens": ["Doch", "w\u00e4r", "ich", "zu", "be\u00b7nei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Nur sie nur sie mu\u00df gl\u00fcklich seyn,", "tokens": ["Nur", "sie", "nur", "sie", "mu\u00df", "gl\u00fck\u00b7lich", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ADV", "PPER", "VMFIN", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nur sie nur sie verdients allein;", "tokens": ["Nur", "sie", "nur", "sie", "ver\u00b7dients", "al\u00b7lein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ADV", "PPER", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und gieng die Welt zu Grunde,", "tokens": ["Und", "gieng", "die", "Welt", "zu", "Grun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ich selber mit \u2013 o wie so sch\u00f6n", "tokens": ["Ich", "sel\u00b7ber", "mit", "\u2013", "o", "wie", "so", "sch\u00f6n"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "APPR", "$(", "FM", "KOKOM", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "W\u00fcrd ich alsdann zu Grunde gehn.", "tokens": ["W\u00fcrd", "ich", "als\u00b7dann", "zu", "Grun\u00b7de", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Schlag bald du sch\u00f6ne Stunde!", "tokens": ["Schlag", "bald", "du", "sch\u00f6\u00b7ne", "Stun\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Geduld und unerschrokner Muth", "tokens": ["Ge\u00b7duld", "und", "un\u00b7er\u00b7schrok\u00b7ner", "Muth"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Beseelen mein getreues Blut", "tokens": ["Be\u00b7see\u00b7len", "mein", "ge\u00b7treu\u00b7es", "Blut"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und f\u00fcrcht mich nicht zu sterben.", "tokens": ["Und", "f\u00fcrcht", "mich", "nicht", "zu", "ster\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Himmel kostet Leiden hier,", "tokens": ["Der", "Him\u00b7mel", "kos\u00b7tet", "Lei\u00b7den", "hier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich leide froh, kann ich von dir", "tokens": ["Ich", "lei\u00b7de", "froh", ",", "kann", "ich", "von", "dir"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "$,", "VMFIN", "PPER", "APPR", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mir einen Blick erwerben.", "tokens": ["Mir", "ei\u00b7nen", "Blick", "er\u00b7wer\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Nur du verdienst begl\u00fcckt zu seyn,", "tokens": ["Nur", "du", "ver\u00b7dienst", "be\u00b7gl\u00fcckt", "zu", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ADV", "VVPP", "PTKZU", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Drum will ich gerne Gram und Pein", "tokens": ["Drum", "will", "ich", "ger\u00b7ne", "Gram", "und", "Pein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VMFIN", "PPER", "ADV", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In meine Brust verschliessen.", "tokens": ["In", "mei\u00b7ne", "Brust", "ver\u