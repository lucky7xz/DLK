{"dta.poem.10525": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "LxXXII.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Kaum sahen wir uns, und an Augen und Stimme", "tokens": ["Kaum", "sa\u00b7hen", "wir", "uns", ",", "und", "an", "Au\u00b7gen", "und", "Stim\u00b7me"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "$,", "KON", "APPR", "NN", "KON", "NN"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Merkt' ich, da\u00df du mir gewogen bist;", "tokens": ["Merkt'", "ich", ",", "da\u00df", "du", "mir", "ge\u00b7wo\u00b7gen", "bist", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "PPER", "PPER", "VVPP", "VAFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Und stand nicht dabei die Mutter, die schlimme,", "tokens": ["Und", "stand", "nicht", "da\u00b7bei", "die", "Mut\u00b7ter", ",", "die", "schlim\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "PAV", "ART", "NN", "$,", "PRELS", "VVFIN", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ich glaube, wir h\u00e4tten uns gleich gek\u00fc\u00dft.", "tokens": ["Ich", "glau\u00b7be", ",", "wir", "h\u00e4t\u00b7ten", "uns", "gleich", "ge\u00b7k\u00fc\u00dft", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.2": {"line.1": {"text": "Und morgen verlasse ich wieder das St\u00e4dtchen,", "tokens": ["Und", "mor\u00b7gen", "ver\u00b7las\u00b7se", "ich", "wie\u00b7der", "das", "St\u00e4dt\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Und eile fort im alten Lauf;", "tokens": ["Und", "ei\u00b7le", "fort", "im", "al\u00b7ten", "Lauf", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKVZ", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dann lauert am Fenster mein blondes M\u00e4dchen,", "tokens": ["Dann", "lau\u00b7ert", "am", "Fens\u00b7ter", "mein", "blon\u00b7des", "M\u00e4d\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Und freundliche Gr\u00fc\u00dfe werf' ich hinauf.", "tokens": ["Und", "freund\u00b7li\u00b7che", "Gr\u00fc\u00b7\u00dfe", "wer\u00b7f'", "ich", "hin\u00b7auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VAFIN", "PPER", "PTKVZ", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}}}}}