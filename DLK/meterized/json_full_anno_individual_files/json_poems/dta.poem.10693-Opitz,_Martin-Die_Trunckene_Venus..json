{"dta.poem.10693": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "Die Trunckene Venus.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1624", "urn": "urn:nbn:de:kobv:b4-200905197859", "language": ["de:0.99"], "booktitle": "Opitz, Martin: Teutsche P\u00f6emata und: Aristarchvs Wieder die verachtung Teutscher Sprach. Stra\u00dfburg, 1624."}, "poem": {"stanza.1": {"line.1": {"text": "Die sch\u00f6ne Venus gieng mit jhrem kleinen Sohne/", "tokens": ["Die", "sch\u00f6\u00b7ne", "Ve\u00b7nus", "gieng", "mit", "jhrem", "klei\u00b7nen", "Soh\u00b7ne", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Vnd dreyen ", "tokens": ["Vnd", "drey\u00b7en"], "token_info": ["word", "word"], "pos": ["KON", "CARD"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Als sie den Jupiter gleich nicht zu Hausefandt/", "tokens": ["Als", "sie", "den", "Ju\u00b7pi\u00b7ter", "gleich", "nicht", "zu", "Hau\u00b7se\u00b7fandt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "PTKNEG", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Weil er verreiset war ins schwartze Mohrenlandt/", "tokens": ["Weil", "er", "ver\u00b7rei\u00b7set", "war", "ins", "schwart\u00b7ze", "Moh\u00b7ren\u00b7landt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "VAFIN", "APPRART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Das k\u00f6stlich Himmelbrot lag auff der G\u00fclden Schaale/", "tokens": ["Das", "k\u00f6st\u00b7lich", "Him\u00b7mel\u00b7brot", "lag", "auff", "der", "G\u00fcl\u00b7den", "Schaa\u00b7le", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "De\u00df Nectars Liblichkeit roch auff dem gantzen Saale/", "tokens": ["De\u00df", "Nec\u00b7tars", "Lib\u00b7lich\u00b7keit", "roch", "auff", "dem", "gant\u00b7zen", "Saa\u00b7le", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "So da\u00df der s\u00fcsse Tranck jhr in die Nasen kam/", "tokens": ["So", "da\u00df", "der", "s\u00fcs\u00b7se", "Tranck", "jhr", "in", "die", "Na\u00b7sen", "kam", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "ART", "ADJA", "NN", "PPER", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Hierumb sie dann davon nit wenig zu jhr nam.", "tokens": ["Hie\u00b7rumb", "sie", "dann", "da\u00b7von", "nit", "we\u00b7nig", "zu", "jhr", "nam", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "ADV", "PAV", "PTKNEG", "ADV", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Dem Amor ist der Wein auch zimlich eingeflossen/", "tokens": ["Dem", "A\u00b7mor", "ist", "der", "Wein", "auch", "zim\u00b7lich", "ein\u00b7ge\u00b7flos\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VAFIN", "ART", "NN", "ADV", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "So da\u00df er gantz vnd gar Gem\u00fcth vnd Sinn begossen.", "tokens": ["So", "da\u00df", "er", "gantz", "vnd", "gar", "Ge\u00b7m\u00fcth", "vnd", "Sinn", "be\u00b7gos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "ADV", "KON", "ADV", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Nun taumeln sie bere\u00fcscht im Himmel hin vnd her/", "tokens": ["Nun", "tau\u00b7meln", "sie", "be\u00b7re\u00b7\u00fcscht", "im", "Him\u00b7mel", "hin", "vnd", "her", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVFIN", "APPRART", "NN", "PTKVZ", "KON", "PTKVZ", "$("], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "Vnd kriechen durch die W\u00e4ld/ bald f\u00fcr sich/ bald die quer/", "tokens": ["Vnd", "krie\u00b7chen", "durch", "die", "W\u00e4ld", "/", "bald", "f\u00fcr", "sich", "/", "bald", "die", "quer", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$(", "ADV", "APPR", "PRF", "$(", "ADV", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Cupido ist bedacht die M\u00e4nner zu verletzen/", "tokens": ["Cu\u00b7pi\u00b7do", "ist", "be\u00b7dacht", "die", "M\u00e4n\u00b7ner", "zu", "ver\u00b7let\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ADJD", "ART", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Vnd sie das Weibes Volck in Vngemach zu setzen/", "tokens": ["Vnd", "sie", "das", "Wei\u00b7bes", "Volck", "in", "Vn\u00b7ge\u00b7mach", "zu", "set\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ART", "NN", "NN", "APPR", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Geht wegk/ secht euch wol f\u00fcr: je mehr sie truncken sein/", "tokens": ["Geht", "wegk", "/", "secht", "euch", "wol", "f\u00fcr", ":", "je", "mehr", "sie", "trun\u00b7cken", "sein", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "$(", "VVFIN", "PPER", "ADV", "APPR", "$.", "ADV", "ADV", "PPER", "ADJD", "VAINF", "$("], "meter": "-+-++-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.16": {"text": "Je mehr verm\u00f6gen sie in List vnd Liebes pein.", "tokens": ["Je", "mehr", "ver\u00b7m\u00f6\u00b7gen", "sie", "in", "List", "vnd", "Lie\u00b7bes", "pein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "APPR", "NE", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}