{"dta.poem.20062": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Im Herbst .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1851", "urn": "urn:nbn:de:kobv:b4-200905191321", "language": ["de:0.99"], "booktitle": "Fontane, Theodor: Gedichte. Berlin, 1851."}, "poem": {"stanza.1": {"line.1": {"text": "Es f\u00e4llt das Laub wie Regentropfen", "tokens": ["Es", "f\u00e4llt", "das", "Laub", "wie", "Re\u00b7gen\u00b7trop\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "KOKOM", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So zahllos auf die Stoppelflur;", "tokens": ["So", "zahl\u00b7los", "auf", "die", "Stop\u00b7pel\u00b7flur", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Matt pulst der Bach wie letztes Klopfen", "tokens": ["Matt", "pulst", "der", "Bach", "wie", "letz\u00b7tes", "Klop\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN", "KOKOM", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Im Todeskampfe der Natur.", "tokens": ["Im", "To\u00b7des\u00b7kamp\u00b7fe", "der", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Still wird\u2019s! und als den tiefen Frieden", "tokens": ["Still", "wird's", "!", "und", "als", "den", "tie\u00b7fen", "Frie\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "$.", "KON", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein leises Wehen jetzt durchzog,", "tokens": ["Ein", "lei\u00b7ses", "We\u00b7hen", "jetzt", "durch\u00b7zog", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da mocht\u2019 es sein, da\u00df abgeschieden", "tokens": ["Da", "mocht'", "es", "sein", ",", "da\u00df", "ab\u00b7ge\u00b7schie\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "VAINF", "$,", "KOUS", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Erdenseele aufw\u00e4rts flog.", "tokens": ["Die", "Er\u00b7den\u00b7see\u00b7le", "auf\u00b7w\u00e4rts", "flog", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}