{"dta.poem.10863": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Mancherley Vergn\u00fcgen an Blumen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1740", "urn": "urn:nbn:de:kobv:b4-200905198572", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die g\u00fctige Natur, damit der Kreis der Erden", "tokens": ["Die", "g\u00fc\u00b7ti\u00b7ge", "Na\u00b7tur", ",", "da\u00b7mit", "der", "Kreis", "der", "Er\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "KOUS", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dem Menschen m\u00f6cht ein lieblich Wohnhaus werden,", "tokens": ["Dem", "Men\u00b7schen", "m\u00f6cht", "ein", "lieb\u00b7lich", "Wohn\u00b7haus", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ART", "ADJD", "NN", "VAINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Beschlo\u00df, um ihn recht herrlich auszuzieren,", "tokens": ["Be\u00b7schlo\u00df", ",", "um", "ihn", "recht", "herr\u00b7lich", "aus\u00b7zu\u00b7zie\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUI", "PPER", "ADV", "ADJD", "VVIZU", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zu seiner Augen Lust, die Blumen zu formiren.", "tokens": ["Zu", "sei\u00b7ner", "Au\u00b7gen", "Lust", ",", "die", "Blu\u00b7men", "zu", "for\u00b7mi\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ein\u2019 ungez\u00e4hlte Zahl Figuren ward erdacht,", "tokens": ["Ein'", "un\u00b7ge\u00b7z\u00e4hl\u00b7te", "Zahl", "Fi\u00b7gu\u00b7ren", "ward", "er\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und all\u2019 in anderer gef\u00e4rbten Zierlichkeit,", "tokens": ["Und", "all'", "in", "an\u00b7de\u00b7rer", "ge\u00b7f\u00e4rb\u00b7ten", "Zier\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "APPR", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "In unterschiedlicher Vollkommenheit,", "tokens": ["In", "un\u00b7ter\u00b7schied\u00b7li\u00b7cher", "Voll\u00b7kom\u00b7men\u00b7heit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Gebildet und hervorgebracht.", "tokens": ["Ge\u00b7bil\u00b7det", "und", "her\u00b7vor\u00b7ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "KON", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Sie war noch nicht vergn\u00fcgt; sie wollte mehr noch schenken;", "tokens": ["Sie", "war", "noch", "nicht", "ver\u00b7gn\u00fcgt", ";", "sie", "woll\u00b7te", "mehr", "noch", "schen\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PTKNEG", "VVPP", "$.", "PPER", "VMFIN", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und, um noch einen Sinn der Menschen zu vergn\u00fcgen,", "tokens": ["Und", ",", "um", "noch", "ei\u00b7nen", "Sinn", "der", "Men\u00b7schen", "zu", "ver\u00b7gn\u00fc\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUI", "ADV", "ART", "NN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Noch eine Wundergab in ihre Blumen senken,", "tokens": ["Noch", "ei\u00b7ne", "Wun\u00b7der\u00b7gab", "in", "ih\u00b7re", "Blu\u00b7men", "sen\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und den balsamischen Geruch, zur Sch\u00f6nheit, f\u00fcgen.", "tokens": ["Und", "den", "bal\u00b7sa\u00b7mi\u00b7schen", "Ge\u00b7ruch", ",", "zur", "Sch\u00f6n\u00b7heit", ",", "f\u00fc\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$,", "APPRART", "NN", "$,", "VVINF", "$."], "meter": "--+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.13": {"text": "Noch mehr, auch zum Gef\u00fchl, ward von ihr auserlesen", "tokens": ["Noch", "mehr", ",", "auch", "zum", "Ge\u00b7f\u00fchl", ",", "ward", "von", "ihr", "au\u00b7ser\u00b7le\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "$,", "ADV", "APPRART", "NN", "$,", "VAFIN", "APPR", "PPER", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Ein Stoff, der uns vergn\u00fcgt. Ein holdes k\u00fchles Wefen", "tokens": ["Ein", "Stoff", ",", "der", "uns", "ver\u00b7gn\u00fcgt", ".", "Ein", "hol\u00b7des", "k\u00fch\u00b7les", "We\u00b7fen"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVPP", "$.", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Ward diesem lieblichen Gesch\u00f6pf noch einverleibt,", "tokens": ["Ward", "die\u00b7sem", "lieb\u00b7li\u00b7chen", "Ge\u00b7sch\u00f6pf", "noch", "ein\u00b7ver\u00b7leibt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDAT", "ADJA", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Das sich an unsre Haut, nicht sonder Anmuth, reibt,", "tokens": ["Das", "sich", "an", "uns\u00b7re", "Haut", ",", "nicht", "son\u00b7der", "An\u00b7muth", ",", "reibt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PDS", "PRF", "APPR", "PPOSAT", "NN", "$,", "PTKNEG", "ADJA", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Und eine k\u00fchle Kraft in Hirn und Lunge treibt.", "tokens": ["Und", "ei\u00b7ne", "k\u00fch\u00b7le", "Kraft", "in", "Hirn", "und", "Lun\u00b7ge", "treibt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Es sey im Sommer noch so schw\u00fchl;", "tokens": ["Es", "sey", "im", "Som\u00b7mer", "noch", "so", "schw\u00fchl", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPRART", "NN", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "So sind der Blumen Bl\u00e4tter k\u00fchl.", "tokens": ["So", "sind", "der", "Blu\u00b7men", "Bl\u00e4t\u00b7ter", "k\u00fchl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Und so vergn\u00fcgt sie uns ", "tokens": ["Und", "so", "ver\u00b7gn\u00fcgt", "sie", "uns"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.21": {"text": "Ja, wenn man weiter geht; so wird man noch entdecken,", "tokens": ["Ja", ",", "wenn", "man", "wei\u00b7ter", "geht", ";", "so", "wird", "man", "noch", "ent\u00b7de\u00b7cken", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "KOUS", "PIS", "ADV", "VVFIN", "$.", "ADV", "VAFIN", "PIS", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Da\u00df wir in vielen gar was wirklich s\u00fcsses schmecken,", "tokens": ["Da\u00df", "wir", "in", "vie\u00b7len", "gar", "was", "wirk\u00b7lich", "s\u00fcs\u00b7ses", "schme\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PIAT", "ADV", "PWS", "ADJD", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Ohn was in ihr annoch f\u00fcr Honig-Sch\u00e4tze stecken.", "tokens": ["Ohn", "was", "in", "ihr", "an\u00b7noch", "f\u00fcr", "Ho\u00b7nig\u00b7Sch\u00e4t\u00b7ze", "ste\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "APPR", "PPER", "ADV", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "So ist denn offenbar, da\u00df durch der Blumen Heer", "tokens": ["So", "ist", "denn", "of\u00b7fen\u00b7bar", ",", "da\u00df", "durch", "der", "Blu\u00b7men", "Heer"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "ADJD", "$,", "KOUS", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Die Seelen, nicht durch einen Sinn allein,", "tokens": ["Die", "See\u00b7len", ",", "nicht", "durch", "ei\u00b7nen", "Sinn", "al\u00b7lein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PTKNEG", "APPR", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.26": {"text": "Annoch durch mehr,", "tokens": ["An\u00b7noch", "durch", "mehr", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADV", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.27": {"text": "Und gar durch ihrer vier vergn\u00fcget seyn.", "tokens": ["Und", "gar", "durch", "ih\u00b7rer", "vier", "ver\u00b7gn\u00fc\u00b7get", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PPOSAT", "CARD", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.28": {"text": "Ist jedes Sinnes Lust denn wenigstens nicht werth,", "tokens": ["Ist", "je\u00b7des", "Sin\u00b7nes", "Lust", "denn", "we\u00b7nigs\u00b7tens", "nicht", "werth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "ADJA", "NN", "KON", "ADV", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Da\u00df man durch ein Gott-Lob davor den Sch\u00f6pfer ehrt?", "tokens": ["Da\u00df", "man", "durch", "ein", "Got\u00b7tLob", "da\u00b7vor", "den", "Sch\u00f6p\u00b7fer", "ehrt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "ART", "NN", "PAV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}