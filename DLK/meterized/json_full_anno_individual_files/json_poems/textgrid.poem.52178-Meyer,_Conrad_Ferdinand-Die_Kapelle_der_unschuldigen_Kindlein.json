{"textgrid.poem.52178": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Die Kapelle der unschuldigen Kindlein", "genre": "verse", "period": "N.A.", "pub_year": 1861, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Aus Henkerf\u00e4usten flogen zum Himmel sie empor,", "tokens": ["Aus", "Hen\u00b7ker\u00b7f\u00e4us\u00b7ten", "flo\u00b7gen", "zum", "Him\u00b7mel", "sie", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "APPRART", "NN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Sie treten zwei und zweie hinein ins sel'ge Tor,", "tokens": ["Sie", "tre\u00b7ten", "zwei", "und", "zwei\u00b7e", "hin\u00b7ein", "ins", "sel'\u00b7ge", "Tor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "CARD", "KON", "VVFIN", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Einand' am H\u00e4ndchen haltend und singend wohlgemut,", "tokens": ["Ein\u00b7and'", "am", "H\u00e4nd\u00b7chen", "hal\u00b7tend", "und", "sin\u00b7gend", "wohl\u00b7ge\u00b7mut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VVPP", "KON", "ADJD", "ADJD", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Sie tragen in den Locken ein leuchtend Mal von Blut.", "tokens": ["Sie", "tra\u00b7gen", "in", "den", "Lo\u00b7cken", "ein", "leuch\u00b7tend", "Mal", "von", "Blut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ART", "ADJD", "NN", "APPR", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "\u00bbwir kommen in den Himmel \u2013 und solches ist uns lieb \u2013", "tokens": ["\u00bb", "wir", "kom\u00b7men", "in", "den", "Him\u00b7mel", "\u2013", "und", "sol\u00b7ches", "ist", "uns", "lieb", "\u2013"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "APPR", "ART", "NN", "$(", "KON", "PIS", "VAFIN", "PPER", "ADJD", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Weil das gelobte Kindlein statt unser unten blieb!", "tokens": ["Weil", "das", "ge\u00b7lob\u00b7te", "Kin\u00b7dlein", "statt", "un\u00b7ser", "un\u00b7ten", "blieb", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "APPR", "PPOSAT", "ADV", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Wir litten f\u00fcr das B\u00fcblein den herben Todesku\u00df,", "tokens": ["Wir", "lit\u00b7ten", "f\u00fcr", "das", "B\u00fcb\u00b7lein", "den", "her\u00b7ben", "To\u00b7des\u00b7ku\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Den es am bittern Kreuze statt unser leiden mu\u00df!\u00ab", "tokens": ["Den", "es", "am", "bit\u00b7tern", "Kreu\u00b7ze", "statt", "un\u00b7ser", "lei\u00b7den", "mu\u00df", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "PPER", "APPRART", "ADJA", "NN", "APPR", "PPOSAT", "VVINF", "VMFIN", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "Die Engel alle kommen heran in hellem Flug,", "tokens": ["Die", "En\u00b7gel", "al\u00b7le", "kom\u00b7men", "he\u00b7ran", "in", "hel\u00b7lem", "Flug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIS", "VVFIN", "PTKVZ", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Sie bringen sch\u00f6nes Spielzeug und Blumenlust genug.", "tokens": ["Sie", "brin\u00b7gen", "sch\u00f6\u00b7nes", "Spiel\u00b7zeug", "und", "Blu\u00b7men\u00b7lust", "ge\u00b7nug", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "KON", "NN", "ADV", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Jetzt f\u00fchren sie den Reigen mit Fiedel und Schalmei...", "tokens": ["Jetzt", "f\u00fch\u00b7ren", "sie", "den", "Rei\u00b7gen", "mit", "Fie\u00b7del", "und", "Schal\u00b7mei", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "APPR", "NN", "KON", "NN", "$("], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Es klagt aus ferner Tiefe der M\u00fctter Wehgeschrei.", "tokens": ["Es", "klagt", "aus", "fer\u00b7ner", "Tie\u00b7fe", "der", "M\u00fct\u00b7ter", "Weh\u00b7ge\u00b7schrei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.4": {"line.1": {"text": "Aus Henkerf\u00e4usten flogen zum Himmel sie empor,", "tokens": ["Aus", "Hen\u00b7ker\u00b7f\u00e4us\u00b7ten", "flo\u00b7gen", "zum", "Him\u00b7mel", "sie", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "APPRART", "NN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-