{"textgrid.poem.64667": {"metadata": {"author": {"name": "K\u00e4stner, Abraham Gotthelf", "birth": "N.A.", "death": "N.A."}, "title": "294. Ueber die Beschreibung der Feierlichkeiten zu Oels bey der Ankunft des Durchl. Herzogs Friedrich August, und der geleisteten Huldigung", "genre": "verse", "period": "N.A.", "pub_year": 1759, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schreckt von der H\u00f6fe Lust des Ahnenlosen Blick", "tokens": ["Schreckt", "von", "der", "H\u00f6\u00b7fe", "Lust", "des", "Ah\u00b7nen\u00b7lo\u00b7sen", "Blick"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "NN", "ART", "NN", "NN"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Sonst Hellebard' und Bajonet zur\u00fcck,", "tokens": ["Sonst", "Hel\u00b7le\u00b7bard'", "und", "Ba\u00b7jo\u00b7net", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "KON", "NN", "PTKVZ", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "So ladet hier der F\u00fcrst, so froh als ", "tokens": ["So", "la\u00b7det", "hier", "der", "F\u00fcrst", ",", "so", "froh", "als"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "$,", "ADV", "ADJD", "KOKOM"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Unterthanen selber ein.", "tokens": ["Die", "Un\u00b7ter\u00b7tha\u00b7nen", "sel\u00b7ber", "ein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Doch wie es ihn verehrt, wie huldreich ", "tokens": ["Doch", "wie", "es", "ihn", "ver\u00b7ehrt", ",", "wie", "huld\u00b7reich"], "token_info": ["word", "word", "word", "w