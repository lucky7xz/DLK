{"textgrid.poem.48859": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "84. An Sidonien", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du fragest mich um Rat, mein Trost Sidonie,", "tokens": ["Du", "fra\u00b7gest", "mich", "um", "Rat", ",", "mein", "Trost", "Si\u00b7do\u00b7nie", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "NN", "$,", "PPOSAT", "NN", "NE", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "wie du dich laben solst in deinen gro\u00dfen Peinen,", "tokens": ["wie", "du", "dich", "la\u00b7ben", "solst", "in", "dei\u00b7nen", "gro\u00b7\u00dfen", "Pei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "VVINF", "VMFIN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "die unerleidlich dir und unertr\u00e4glich scheinen,", "tokens": ["die", "un\u00b7er\u00b7leid\u00b7lich", "dir", "und", "un\u00b7er\u00b7tr\u00e4g\u00b7lich", "schei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PPER", "KON", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "du fragest mich um Rat und klagst mir di\u00df dein Weh'?", "tokens": ["du", "fra\u00b7gest", "mich", "um", "Rat", "und", "klagst", "mir", "di\u00df", "dein", "Weh'", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "NN", "KON", "VVFIN", "PPER", "PDS", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ach! aber wei\u00dft du nicht, in was Not ich auch steh',", "tokens": ["Ach", "!", "a\u00b7ber", "wei\u00dft", "du", "nicht", ",", "in", "was", "Not", "ich", "auch", "steh'", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "ADV", "VVFIN", "PPER", "PTKNEG", "$,", "APPR", "PRELS", "NN", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "in was Qual ich auch bin? Ach, lasse nach zu weinen,", "tokens": ["in", "was", "Qual", "ich", "auch", "bin", "?", "Ach", ",", "las\u00b7se", "nach", "zu", "wei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "NN", "PPER", "ADV", "VAFIN", "$.", "ITJ", "$,", "VVFIN", "APPR", "PTKZU", "VVINF", "$,"], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "von deinem w\u00e4chst mein Leid, wie deines von dem meinen.", "tokens": ["von", "dei\u00b7nem", "w\u00e4chst", "mein", "Leid", ",", "wie", "dei\u00b7nes", "von", "dem", "mei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "VVFIN", "PPOSAT", "NN", "$,", "PWAV", "PPOSAT", "APPR", "ART", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ach, lasse, bitt' ich, nach, eh denn ich ganz vergeh'.", "tokens": ["Ach", ",", "las\u00b7se", ",", "bitt'", "ich", ",", "nach", ",", "eh", "denn", "ich", "ganz", "ver\u00b7geh'", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VVFIN", "$,", "VVFIN", "PPER", "$,", "APPR", "$,", "KOUS", "KON", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Und suchst du Rat bei dem, der selbst sucht Rat bei dir?", "tokens": ["Und", "suchst", "du", "Rat", "bei", "dem", ",", "der", "selbst", "sucht", "Rat", "bei", "dir", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "NN", "APPR", "ART", "$,", "PRELS", "ADV", "VVFIN", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ist so, wie F\u00f6bus meint, der Meister der Arzneien,", "tokens": ["Ist", "so", ",", "wie", "F\u00f6\u00b7bus", "meint", ",", "der", "Meis\u00b7ter", "der", "Arz\u00b7nei\u00b7en", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "$,", "PWAV", "NE", "VVFIN", "$,", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "da\u00df Feuer Hitze l\u00f6scht, so tu ein Ding mit mir:", "tokens": ["da\u00df", "Feu\u00b7er", "Hit\u00b7ze", "l\u00f6scht", ",", "so", "tu", "ein", "Ding", "mit", "mir", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "NN", "VVFIN", "$,", "ADV", "VVFIN", "ART", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Tr\u00f6st dich an meiner Angst, ich will mich deiner freuen,", "tokens": ["Tr\u00f6st", "dich", "an", "mei\u00b7ner", "Angst", ",", "ich", "will", "mich", "dei\u00b7ner", "freu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$,", "PPER", "VMFIN", "PPER", "PPOSAT", "VVINF", "$,"], "meter": "-+