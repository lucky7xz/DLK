{"textgrid.poem.48483": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "3. Aus dem Alziat \u00fcber die Farben", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die schwarze Farbe steht zu schwarzen Traurigkeiten;", "tokens": ["Die", "schwar\u00b7ze", "Far\u00b7be", "steht", "zu", "schwar\u00b7zen", "Trau\u00b7rig\u00b7kei\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "dieselbe brauchen wir, wenn wir den Sarg begleiten.", "tokens": ["die\u00b7sel\u00b7be", "brau\u00b7chen", "wir", ",", "wenn", "wir", "den", "Sarg", "be\u00b7glei\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "VVFIN", "PPER", "$,", "KOUS", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wei\u00df zeigt die Sinnen an, die ohne Falschheit sein;", "tokens": ["Wei\u00df", "zeigt", "die", "Sin\u00b7nen", "an", ",", "die", "oh\u00b7ne", "Falschheit", "sein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "PTKVZ", "$,", "PRELS", "APPR", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "drum sein die wei\u00dfen R\u00f6ck' euch Priestern so gemein.", "tokens": ["drum", "sein", "die", "wei\u00b7\u00dfen", "R\u00f6ck'", "euch", "Pries\u00b7tern", "so", "ge\u00b7mein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPOSAT", "ART", "ADJA", "NN", "PPER", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Gr\u00fcn lehrt uns, da\u00df man hofft; sonst pfleget man zu sagen,", "tokens": ["Gr\u00fcn", "lehrt", "uns", ",", "da\u00df", "man", "hofft", ";", "sonst", "pfle\u00b7get", "man", "zu", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$,", "KOUS", "PIS", "VVFIN", "$.", "ADV", "VVFIN", "PIS", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "die Sache gr\u00fcne noch, so oft es umgeschlagen.", "tokens": ["die", "Sa\u00b7che", "gr\u00fc\u00b7ne", "noch", ",", "so", "oft", "es", "um\u00b7ge\u00b7schla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "$,", "ADV", "ADV", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Gelb ist Begierde voll; sie ist der B\u00fchler'n gut", "tokens": ["Gelb", "ist", "Be\u00b7gier\u00b7de", "voll", ";", "sie", "ist", "der", "B\u00fch\u00b7ler'n", "gut"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "NN", "ADJD", "$.", "PPER", "VAFIN", "ART", "NN", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "und denen Hoffnung stets, was sie begehren, tut.", "tokens": ["und", "de\u00b7nen", "Hoff\u00b7nung", "stets", ",", "was", "sie", "be\u00b7geh\u00b7ren", ",", "tut", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PDS", "NN", "ADV", "$,", "PRELS", "PPER", "VVINF", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Rot ist Soldaten hold und zeuget frisch Gebl\u00fcte,", "tokens": ["Rot", "ist", "Sol\u00b7da\u00b7ten", "hold", "und", "zeu\u00b7get", "frisch", "Ge\u00b7bl\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "NN", "ADJD", "KON", "VVFIN", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "wie an den Knaben auch ein z\u00fcchtiges Gem\u00fcte.", "tokens": ["wie", "an", "den", "Kna\u00b7ben", "auch", "ein", "z\u00fcch\u00b7ti\u00b7ges", "Ge\u00b7m\u00fc\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Blau ist der Schiffer Art und die, der Andacht voll,", "tokens": ["Blau", "ist", "der", "Schif\u00b7fer", "Art", "und", "die", ",", "der", "An\u00b7dacht", "voll", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "NN", "KON", "ART", "$,", "ART", "NN", "ADJD", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.12": {"text": "gen Himmel stetigs sehn, da\u00df sie Gott h\u00f6ren soll.", "tokens": ["gen", "Him\u00b7mel", "ste\u00b7tigs", "sehn", ",", "da\u00df", "sie", "Gott", "h\u00f6\u00b7ren", "soll", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "VVINF", "$,", "KOUS", "PPER", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Das Goldgelb' ist vor Schlecht' und Feuerrot imgleichen;", "tokens": ["Das", "Gold\u00b7gelb'", "ist", "vor", "Schlecht'", "und", "Feu\u00b7er\u00b7rot", "im\u00b7glei\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "die Kapuziner sicht man so hereinher schleichen.", "tokens": ["die", "Ka\u00b7pu\u00b7zi\u00b7ner", "sicht", "man", "so", "her\u00b7ein\u00b7her", "schlei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIS", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Wer Liebeseifer voll und tief in Angst mu\u00df gehn,", "tokens": ["Wer", "Lie\u00b7be\u00b7sei\u00b7fer", "voll", "und", "tief", "in", "Angst", "mu\u00df", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "ADJD", "KON", "ADJD", "APPR", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "dem soll das Dunkelrot am allerbesten stehn.", "tokens": ["dem", "soll", "das", "Dun\u00b7kel\u00b7rot", "am", "al\u00b7ler\u00b7bes\u00b7ten", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ART", "NN", "APPRART", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Violbraun zieret den, der in Vergn\u00fcgen lebet", "tokens": ["Viol\u00b7braun", "zie\u00b7ret", "den", ",", "der", "in", "Ver\u00b7gn\u00fc\u00b7gen", "le\u00b7bet"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "$,", "PRELS", "APPR", "NN", "VVFIN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.18": {"text": "und, was das Gl\u00fccke giebt, mit nichten widerstrebet.", "tokens": ["und", ",", "was", "das", "Gl\u00fc\u00b7cke", "giebt", ",", "mit", "nich\u00b7ten", "wi\u00b7der\u00b7stre\u00b7bet", ".