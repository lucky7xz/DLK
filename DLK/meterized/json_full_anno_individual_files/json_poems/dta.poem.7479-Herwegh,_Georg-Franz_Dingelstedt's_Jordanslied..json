{"dta.poem.7479": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Franz Dingelstedt's Jordanslied.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192430", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Nachtigall hat f\u00fcr den Aar gesungen,", "tokens": ["Die", "Nach\u00b7ti\u00b7gall", "hat", "f\u00fcr", "den", "Aar", "ge\u00b7sun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der, fortgeflogen aus dem Alpenlande,", "tokens": ["Der", ",", "fort\u00b7ge\u00b7flo\u00b7gen", "aus", "dem", "Al\u00b7pen\u00b7lan\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Verschmachtend lag in unsrem deutschen Sande,", "tokens": ["Ver\u00b7schmach\u00b7tend", "lag", "in", "uns\u00b7rem", "deut\u00b7schen", "San\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Weil er sich hatt' zu hoch hinangeschwungen.", "tokens": ["Weil", "er", "sich", "hatt'", "zu", "hoch", "hin\u00b7an\u00b7ge\u00b7schwun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VAFIN", "PTKA", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wem w\u00e4re nicht ihr Lied ans Herz gedrungen,", "tokens": ["Wem", "w\u00e4\u00b7re", "nicht", "ihr", "Lied", "ans", "Herz", "ge\u00b7drun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PTKNEG", "PPOSAT", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ihr grollend, r\u00fchrend Lied von unsrer Schande?", "tokens": ["Ihr", "grol\u00b7lend", ",", "r\u00fch\u00b7rend", "Lied", "von", "uns\u00b7rer", "Schan\u00b7de", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "$,", "VVPP", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch sprecht, wann sind bei uns des Freien Bande", "tokens": ["Doch", "sprecht", ",", "wann", "sind", "bei", "uns", "des", "Frei\u00b7en", "Ban\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "PWAV", "VAFIN", "APPR", "PPER", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Von eines S\u00e4ngers Liede je gesprungen?", "tokens": ["Von", "ei\u00b7nes", "S\u00e4n\u00b7gers", "Lie\u00b7de", "je", "ge\u00b7sprun\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Du sankest, schier ein Knecht, am Throne nieder,", "tokens": ["Du", "san\u00b7kest", ",", "schier", "ein", "Knecht", ",", "am", "Thro\u00b7ne", "nie\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADV", "ART", "NN", "$,", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Damit der Freie b\u00e4lder auferst\u00e4nde;", "tokens": ["Da\u00b7mit", "der", "Frei\u00b7e", "b\u00e4l\u00b7der", "auf\u00b7er\u00b7st\u00e4n\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Geh' hin, mein Freund, und frag' nach Jahren wieder!", "tokens": ["Geh'", "hin", ",", "mein", "Freund", ",", "und", "frag'", "nach", "Jah\u00b7ren", "wie\u00b7der", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$,", "PPOSAT", "NN", "$,", "KON", "VVFIN", "APPR", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Statt seiner Alpen ", "tokens": ["Statt", "sei\u00b7ner", "Al\u00b7pen"], "token_info": ["word", "word", "word"], "pos": ["NN", "PPOSAT", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Die Macht, sie l\u00e4chelt \u00fcber Deine Lieder,", "tokens": ["Die", "Macht", ",", "sie", "l\u00e4\u00b7chelt", "\u00fc\u00b7ber", "Dei\u00b7ne", "Lie\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und w\u00e4scht noch, ein Pilatus, sich die H\u00e4nde.", "tokens": ["Und", "w\u00e4scht", "noch", ",", "ein", "Pi\u00b7la\u00b7tus", ",", "sich", "die", "H\u00e4n\u00b7de", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "$,", "ART", "NN", "$,", "PRF", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}