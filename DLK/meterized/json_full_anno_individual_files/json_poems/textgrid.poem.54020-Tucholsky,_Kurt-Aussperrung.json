{"textgrid.poem.54020": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Aussperrung", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hier stehn wir in dem Garten", "tokens": ["Hier", "stehn", "wir", "in", "dem", "Gar\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "und warten, warten, warten,", "tokens": ["und", "war\u00b7ten", ",", "war\u00b7ten", ",", "war\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "VVINF", "$,", "VVFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Vater kommt nicht.", "tokens": ["Va\u00b7ter", "kommt", "nicht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PTKNEG", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Bei Krauses wird gleich geschlossen;", "tokens": ["Bei", "Krau\u00b7ses", "wird", "gleich", "ge\u00b7schlos\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "er ist bei den Genossen . . .", "tokens": ["er", "ist", "bei", "den", "Ge\u00b7nos\u00b7sen", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ART", "NN", "$.", "$.", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "Vater ist ausgesperrt.", "tokens": ["Va\u00b7ter", "ist", "aus\u00b7ge\u00b7sperrt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "VVPP", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.2": {"line.1": {"text": "Durchs Rheinland zieht es brausend \u2013", "tokens": ["Durchs", "Rhein\u00b7land", "zieht", "es", "brau\u00b7send", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "PPER", "ADJD", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "sie haben Zweihunderttausend", "tokens": ["sie", "ha\u00b7ben", "Zwei\u00b7hun\u00b7dert\u00b7tau\u00b7send"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VAFIN", "CARD"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "aus den Fabriken gezerrt.", "tokens": ["aus", "den", "Fab\u00b7ri\u00b7ken", "ge\u00b7zerrt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wir stehen hier und darben;", "tokens": ["Wir", "ste\u00b7hen", "hier", "und", "dar\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "KON", "PAV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "es bl\u00fchn die IG-Farben \u2013", "tokens": ["es", "bl\u00fchn", "die", "Ig\u00b7Fa\u00b7rben", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Vater ist ausgesperrt.", "tokens": ["Va\u00b7ter", "ist", "aus\u00b7ge\u00b7sperrt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "VVPP", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.3": {"line.1": {"text": "Wir sind dazu da, um sp\u00e4ter", "tokens": ["Wir", "sind", "da\u00b7zu", "da", ",", "um", "sp\u00e4\u00b7ter"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VAFIN", "PAV", "PTKVZ", "$,", "KOUI", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic