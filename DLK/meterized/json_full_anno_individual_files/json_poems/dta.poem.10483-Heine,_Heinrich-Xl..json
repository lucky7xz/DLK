{"dta.poem.10483": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Xl.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Wie der Mond sich leuchtend dr\u00e4nget", "tokens": ["Wie", "der", "Mond", "sich", "leuch\u00b7tend", "dr\u00e4n\u00b7get"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "PRF", "VVPP", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Durch den dunkeln Wolkenflor,", "tokens": ["Durch", "den", "dun\u00b7keln", "Wol\u00b7ken\u00b7flor", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Also taucht aus dunkeln Zeiten", "tokens": ["Al\u00b7so", "taucht", "aus", "dun\u00b7keln", "Zei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mir ein lichtes Bild hervor.", "tokens": ["Mir", "ein", "lich\u00b7tes", "Bild", "her\u00b7vor", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}, "stanza.2": {"line.1": {"text": "Sa\u00dfen all auf dem Verdecke,", "tokens": ["Sa\u00b7\u00dfen", "all", "auf", "dem", "Ver\u00b7de\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Fuhren stolz hinab den Rhein,", "tokens": ["Fuh\u00b7ren", "stolz", "hin\u00b7ab", "den", "Rhein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ADV", "ART", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und die sommergr\u00fcnen Ufer", "tokens": ["Und", "die", "som\u00b7mer\u00b7gr\u00fc\u00b7nen", "U\u00b7fer"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gl\u00fchn im Abendsonnenschein.", "tokens": ["Gl\u00fchn", "im", "A\u00b7bend\u00b7son\u00b7nen\u00b7schein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Sinnend sa\u00df ich zu den Fu\u00dfen", "tokens": ["Sin\u00b7nend", "sa\u00df", "ich", "zu", "den", "Fu\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Einer Dame, sch\u00f6n und hold;", "tokens": ["Ei\u00b7ner", "Da\u00b7me", ",", "sch\u00f6n", "und", "hold", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "In ihr liebes, bleiches Antlitz", "tokens": ["In", "ihr", "lie\u00b7bes", ",", "blei\u00b7ches", "Ant\u00b7litz"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "$,", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Spielt' das rothe Sonnengold.", "tokens": ["Spielt'", "das", "ro\u00b7the", "Son\u00b7nen\u00b7gold", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Lauten klangen, Buben sangen,", "tokens": ["Lau\u00b7ten", "klan\u00b7gen", ",", "Bu\u00b7ben", "san\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wunderbare Fr\u00f6hlichkeit!", "tokens": ["Wun\u00b7der\u00b7ba\u00b7re", "Fr\u00f6h\u00b7lich\u00b7keit", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und der Himmel wurde blauer,", "tokens": ["Und", "der", "Him\u00b7mel", "wur\u00b7de", "blau\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und die Seele wurde weit.", "tokens": ["Und", "die", "See\u00b7le", "wur\u00b7de", "weit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "M\u00e4hrchenhaft vor\u00fcberzogen", "tokens": ["M\u00e4hr\u00b7chen\u00b7haft", "vor\u00b7\u00fc\u00b7berz\u00b7o\u00b7gen"], "token_info": ["word", "word"], "pos": ["NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Berg und Burgen, Wald und Au';", "tokens": ["Berg", "und", "Bur\u00b7gen", ",", "Wald", "und", "Au'", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und das Alles sah ich gl\u00e4nzen", "tokens": ["Und", "das", "Al\u00b7les", "sah", "ich", "gl\u00e4n\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "PIS", "VVFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In dem Aug' der sch\u00f6nen Frau.", "tokens": ["In", "dem", "Aug'", "der", "sch\u00f6\u00b7nen", "Frau", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}