{"dta.poem.19166": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "13.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich hab' ein wonniges Gefild im Traum gesehn,", "tokens": ["Ich", "hab'", "ein", "won\u00b7ni\u00b7ges", "Ge\u00b7fild", "im", "Traum", "ge\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So heller Lichter, die mir noch im Herzen stehn.", "tokens": ["So", "hel\u00b7ler", "Lich\u00b7ter", ",", "die", "mir", "noch", "im", "Her\u00b7zen", "stehn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$,", "PRELS", "PPER", "ADV", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ich wei\u00df nicht ob ein Land, wo ich daheim einst war,", "tokens": ["Ich", "wei\u00df", "nicht", "ob", "ein", "Land", ",", "wo", "ich", "da\u00b7heim", "einst", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "KOUS", "ART", "NN", "$,", "PWAV", "PPER", "ADV", "ADV", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Daheim einst werde seyn, doch heimisch wunderbar.", "tokens": ["Da\u00b7heim", "einst", "wer\u00b7de", "seyn", ",", "doch", "hei\u00b7misch", "wun\u00b7der\u00b7bar", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "VAFIN", "VAINF", "$,", "ADV", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "So heimisch war es mir, so heimlich und geheim,", "tokens": ["So", "hei\u00b7misch", "war", "es", "mir", ",", "so", "heim\u00b7lich", "und", "ge\u00b7heim", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "PPER", "PPER", "$,", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vertraulich zeigte mir sein Sehnen jeder Keim.", "tokens": ["Ver\u00b7trau\u00b7lich", "zeig\u00b7te", "mir", "sein", "Seh\u00b7nen", "je\u00b7der", "Keim", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PPOSAT", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Ich sah das gr\u00fcne Laub, das nie wird Windesraub,", "tokens": ["Ich", "sah", "das", "gr\u00fc\u00b7ne", "Laub", ",", "das", "nie", "wird", "Win\u00b7des\u00b7raub", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,", "PRELS", "ADV", "VAFIN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Luft von keinem als erf\u00fcllt von Bl\u00fctenstaub.", "tokens": ["Die", "Luft", "von", "kei\u00b7nem", "als", "er\u00b7f\u00fcllt", "von", "Bl\u00fc\u00b7ten\u00b7staub", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PIAT", "KOKOM", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Ich sah des Waldes Kranz im Abendsonnenglanz,", "tokens": ["Ich", "sah", "des", "Wal\u00b7des", "Kranz", "im", "A\u00b7bend\u00b7son\u00b7nen\u00b7glanz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der doch nicht untergieng, und hell war immer ganz.", "tokens": ["Der", "doch", "nicht", "un\u00b7ter\u00b7gieng", ",", "und", "hell", "war", "im\u00b7mer", "ganz", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PTKNEG", "VVFIN", "$,", "KON", "ADJD", "VAFIN", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Da ich so helle seh in Tr\u00e4umen, soll ich klagen,", "tokens": ["Da", "ich", "so", "hel\u00b7le", "seh", "in", "Tr\u00e4u\u00b7men", ",", "soll", "ich", "kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJA", "VVFIN", "APPR", "NN", "$,", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df mehr und mehr den Dienst die Augen mir versagen?", "tokens": ["Da\u00df", "mehr", "und", "mehr", "den", "Dienst", "die", "Au\u00b7gen", "mir", "ver\u00b7sa\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "KON", "ADV", "ART", "NN", "ART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Ja wol, es sieht ein Mensch mit Augen nicht allein;", "tokens": ["Ja", "wol", ",", "es", "sieht", "ein", "Mensch", "mit", "Au\u00b7gen", "nicht", "al\u00b7lein", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADV", "$,", "PPER", "VVFIN", "ART", "NN", "APPR", "NN", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was sehenswerth ist, sieht dein innres Licht allein.", "tokens": ["Was", "se\u00b7hens\u00b7werth", "ist", ",", "sieht", "dein", "inn\u00b7res", "Licht", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VAFIN", "$,", "VVFIN", "PPOSAT", "ADJA", "NN", "ADV", "$."], "meter": "---+-+-+-+-+", "measure": "unknown.measure.penta"}}}}}