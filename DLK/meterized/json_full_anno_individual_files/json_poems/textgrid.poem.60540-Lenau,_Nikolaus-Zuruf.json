{"textgrid.poem.60540": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Zuruf", "genre": "verse", "period": "N.A.", "pub_year": 1839, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Keuschen, Sittigstrengen, Tugendfrommen", "tokens": ["Die", "Keu\u00b7schen", ",", "Sit\u00b7tigs\u00b7tren\u00b7gen", ",", "Tu\u00b7gend\u00b7from\u00b7men"], "token_info": ["word", "word", "punct", "word", "punct", "word"], "pos": ["ART", "NN", "$,", "NN", "$,", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sind lahm und lau, wenns gilt den Strau\u00df zu fechten,", "tokens": ["Sind", "lahm", "und", "lau", ",", "wenns", "gilt", "den", "Strau\u00df", "zu", "fech\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKVZ", "KON", "ADJD", "$,", "KOUS", "VVFIN", "ART", "NE", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wenn ihr Panier ins Blutgedr\u00e4ng gekommen,", "tokens": ["Wenn", "ihr", "Pa\u00b7nier", "ins", "Blut\u00b7ge\u00b7dr\u00e4ng", "ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Doch Helden sind die sogenannten Schlechten.", "tokens": ["Doch", "Hel\u00b7den", "sind", "die", "so\u00b7ge\u00b7nann\u00b7ten", "Schlech\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}}, "stanza.2": {"line.1": {"text": "Der Fromme mit dem feisten Gottvertrauen", "tokens": ["Der", "From\u00b7me", "mit", "dem", "feis\u00b7ten", "Gott\u00b7ver\u00b7trau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Verw\u00e4chst und seine Klinge mit der Scheide:", "tokens": ["Ver\u00b7w\u00e4chst", "und", "sei\u00b7ne", "Klin\u00b7ge", "mit", "der", "Schei\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "PPOSAT", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "\u00bbder starke Gott wird selber durch sich hauen,", "tokens": ["\u00bb", "der", "star\u00b7ke", "Gott", "wird", "sel\u00b7ber", "durch", "sich", "hau\u00b7en", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "ADJA", "NN", "VAFIN", "ADV", "APPR", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Er will es, da\u00df sein Knecht hienieden leide.", "tokens": ["Er", "will", "es", ",", "da\u00df", "sein", "Knecht", "hien\u00b7ie\u00b7den", "lei\u00b7de", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "$,", "KOUS", "PPOSAT", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "La\u00dft nur die Taumler ins Verderben rennen;", "tokens": ["La\u00dft", "nur", "die", "Taum\u00b7ler", "ins", "Ver\u00b7der\u00b7ben", "ren\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ihr seht sie heut frohlocken, morgen modern;", "tokens": ["Ihr", "seht", "sie", "heut", "froh\u00b7lo\u00b7cken", ",", "mor\u00b7gen", "mo\u00b7dern", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "VVINF", "$,", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie Branntweintrunkne schm\u00e4hlich selbstverbrennen,", "tokens": ["Wie", "Brannt\u00b7wein\u00b7trunk\u00b7ne", "schm\u00e4h\u00b7lich", "selbst\u00b7ver\u00b7bren\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mu\u00df jede Schuld in ihrem Rausch verlodern.\u00ab", "tokens": ["Mu\u00df", "je\u00b7de", "Schuld", "in", "ih\u00b7rem", "Rausch", "ver\u00b7lo\u00b7dern", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VMFIN", "PIAT", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Doch solchem Ruf geb\u00fchrt zur Antwort solches:", "tokens": ["Doch", "sol\u00b7chem", "Ruf", "ge\u00b7b\u00fchrt", "zur", "Ant\u00b7wort", "sol\u00b7ches", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "APPRART", "NN", "PIAT", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "O feige Gottesknechtschaft! Kettenhunde!", "tokens": ["O", "fei\u00b7ge", "Got\u00b7tes\u00b7knecht\u00b7schaft", "!", "Ket\u00b7ten\u00b7hun\u00b7de", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$.", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein stumpfes Amen statt des scharfen Dolches?", "tokens": ["Ein", "stump\u00b7fes", "A\u00b7men", "statt", "des", "schar\u00b7fen", "Dolc\u00b7hes", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sp\u00fcrt euer kalter Brand nicht mehr die Wunde?", "tokens": ["Sp\u00fcrt", "eu\u00b7er", "kal\u00b7ter", "Brand", "nicht", "mehr", "die", "Wun\u00b7de", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "PTKNEG", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Der R\u00f6mler wird am Sakrament nicht irre,", "tokens": ["Der", "R\u00f6m\u00b7ler", "wird", "am", "Sak\u00b7ra\u00b7ment", "nicht", "ir\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPRART", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wenn s\u00fcndhart lebt der Priester der Gemeine,", "tokens": ["Wenn", "s\u00fcnd\u00b7hart", "lebt", "der", "Pries\u00b7ter", "der", "Ge\u00b7mei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Weil Gnade nicht gerinnt im Schmutzgeschirre,", "tokens": ["Weil", "Gna\u00b7de", "nicht", "ge\u00b7rinnt", "im", "Schmutz\u00b7ge\u00b7schir\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PTKNEG", "VVPP", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Hostie schmutzt ja nicht, die ewig reine.", "tokens": ["Die", "Hos\u00b7tie", "schmutzt", "ja", "nicht", ",", "die", "e\u00b7wig", "rei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "PTKNEG", "$,", "PRELS", "ADJD", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "O lernt vom R\u00f6mler Weisheit, fromme Zager!", "tokens": ["O", "lernt", "vom", "R\u00f6m\u00b7ler", "Weis\u00b7heit", ",", "from\u00b7me", "Za\u00b7ger", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPRART", "NN", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ist mancher Streiter auch nicht rein des Schmutzes,", "tokens": ["Ist", "man\u00b7cher", "Strei\u00b7ter", "auch", "nicht", "rein", "des", "Schmut\u00b7zes", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "ADV", "PTKNEG", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ist rein doch das Panier im Freiheitslager,", "tokens": ["Ist", "rein", "doch", "das", "Pa\u00b7nier", "im", "Frei\u00b7heits\u00b7la\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "ADV", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und wahr das Herz des ungeschlachten Trutzes.", "tokens": ["Und", "wahr", "das", "Herz", "des", "un\u00b7ge\u00b7schlach\u00b7ten", "Trut\u00b7zes", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Im Strauchgewirr von Glauben, Recht und Sitte", "tokens": ["Im", "Strauch\u00b7ge\u00b7wirr", "von", "Glau\u00b7ben", ",", "Recht", "und", "Sit\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "APPR", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein Ungeheuer liegt in Schlangenringen,", "tokens": ["Ein", "Un\u00b7ge\u00b7heu\u00b7er", "liegt", "in", "Schlan\u00b7gen\u00b7rin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Trat mancher drauf mit unversehnem Tritte", "tokens": ["Trat", "man\u00b7cher", "drauf", "mit", "un\u00b7ver\u00b7seh\u00b7nem", "Trit\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "PAV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und schrie entsetzt; kann das melodisch klingen?", "tokens": ["Und", "schrie", "ent\u00b7setzt", ";", "kann", "das", "me\u00b7lo\u00b7disch", "klin\u00b7gen", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "VVPP", "$.", "VMFIN", "ART", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Ein kaltes, plumpes, bl\u00f6des Ungeheuer,", "tokens": ["Ein", "kal\u00b7tes", ",", "plum\u00b7pes", ",", "bl\u00f6\u00b7des", "Un\u00b7ge\u00b7heu\u00b7er", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das Herzen fri\u00dft und saugt Gehirne trocken,", "tokens": ["Das", "Her\u00b7zen", "fri\u00dft", "und", "saugt", "Ge\u00b7hir\u00b7ne", "tro\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das ewig w\u00e4lzt, ein tr\u00e4ger Wiederk\u00e4uer,", "tokens": ["Das", "e\u00b7wig", "w\u00e4lzt", ",", "ein", "tr\u00e4\u00b7ger", "Wie\u00b7der\u00b7k\u00e4u\u00b7er", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "VVFIN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Des Elends mittelalterliche Brocken.", "tokens": ["Des", "E\u00b7lends", "mit\u00b7tel\u00b7al\u00b7ter\u00b7li\u00b7che", "Bro\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Harpunen in die Schuppen starrer Satzung!", "tokens": ["Har\u00b7pu\u00b7nen", "in", "die", "Schup\u00b7pen", "star\u00b7rer", "Sat\u00b7zung", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und Dolche nach, die Menschheit zu erl\u00f6sen!", "tokens": ["Und", "Dol\u00b7che", "nach", ",", "die", "Menschheit", "zu", "er\u00b7l\u00f6\u00b7sen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PTKVZ", "$,", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Kein blutend Herz dem Untier mehr zur Atzung,", "tokens": ["Kein", "blu\u00b7tend", "Herz", "dem", "Un\u00b7tier", "mehr", "zur", "At\u00b7zung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJD", "NN", "ART", "NN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Messias' Zorn! o komm, erschlag den B\u00f6sen!", "tokens": ["Mes\u00b7si\u00b7as'", "Zorn", "!", "o", "komm", ",", "er\u00b7schlag", "den", "B\u00f6\u00b7sen", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$.", "FM", "VVFIN", "$,", "VVFIN", "ART", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}, "stanza.10": {"line.1": {"text": "Dein Tod am Kreuz, o Christus, ist verloren,", "tokens": ["Dein", "Tod", "am", "Kreuz", ",", "o", "Chris\u00b7tus", ",", "ist", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPRART", "NN", "$,", "FM", "FM", "$,", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wenn du nicht wieder kommst f\u00fcr unsre N\u00f6ten,", "tokens": ["Wenn", "du", "nicht", "wie\u00b7der", "kommst", "f\u00fcr", "uns\u00b7re", "N\u00f6\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ADV", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Prophet, hat uns das V\u00f6lkerleid geschworen,", "tokens": ["Pro\u00b7phet", ",", "hat", "uns", "das", "V\u00f6l\u00b7ker\u00b7leid", "ge\u00b7schwo\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VAFIN", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Messias, da\u00df du diesmal kommst, zu t\u00f6ten.", "tokens": ["Mes\u00b7si\u00b7as", ",", "da\u00df", "du", "dies\u00b7mal", "kommst", ",", "zu", "t\u00f6\u00b7ten", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "KOUS", "PPER", "ADV", "VVFIN", "$,", "PTKZU", "VVINF", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}, "stanza.11": {"line.1": {"text": "Sie fingen auf das Blut von deinen H\u00fcften,", "tokens": ["Sie", "fin\u00b7gen", "auf", "das", "Blut", "von", "dei\u00b7nen", "H\u00fcf\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Welt zu tr\u00e4nken mit gef\u00e4lschter Schale,", "tokens": ["Die", "Welt", "zu", "tr\u00e4n\u00b7ken", "mit", "ge\u00b7f\u00e4lschter", "Scha\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Die Welt damit zur Feigheit zu vergiften,", "tokens": ["Die", "Welt", "da\u00b7mit", "zur", "Feig\u00b7heit", "zu", "ver\u00b7gif\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PAV", "APPRART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sie krankt vom Opium in deinem Grale.", "tokens": ["Sie", "krankt", "vom", "O\u00b7pium", "in", "dei\u00b7nem", "Gra\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.12": {"line.1": {"text": "Darum ans Kreuz dir jetzt die Knaben r\u00fccken,", "tokens": ["Da\u00b7rum", "ans", "Kreuz", "dir", "jetzt", "die", "Kna\u00b7ben", "r\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "APPRART", "NN", "PPER", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sie klettern drauf, um deine Dornenkrone", "tokens": ["Sie", "klet\u00b7tern", "drauf", ",", "um", "dei\u00b7ne", "Dor\u00b7nen\u00b7kro\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos