{"dta.poem.6212": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Betrachtung \u00fcber die Ordnung  \n und Dauer der Welt.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es ist der Wunder-Bau so herrlich eingericht;", "tokens": ["Es", "ist", "der", "Wun\u00b7der\u00b7Bau", "so", "herr\u00b7lich", "ein\u00b7ge\u00b7richt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Er reget sich durch ein so ordentlich Gewicht,", "tokens": ["Er", "re\u00b7get", "sich", "durch", "ein", "so", "or\u00b7dent\u00b7lich", "Ge\u00b7wicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "ADV", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und daurhafft R\u00e4der-Werck. Er w\u00fcrckt zu gleicher Zeit", "tokens": ["Und", "daur\u00b7hafft", "R\u00e4\u00b7der\u00b7\u00b7Werck", ".", "Er", "w\u00fcrckt", "zu", "glei\u00b7cher", "Zeit"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "NN", "$.", "PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "In allen Theilen so, in solcher Einigkeit;", "tokens": ["In", "al\u00b7len", "Thei\u00b7len", "so", ",", "in", "sol\u00b7cher", "Ei\u00b7nig\u00b7keit", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ADV", "$,", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Da\u00df, wenn ein neu Gesetz, den Welt-Creis mit Gewalt", "tokens": ["Da\u00df", ",", "wenn", "ein", "neu", "Ge\u00b7setz", ",", "den", "Welt\u00b7Creis", "mit", "Ge\u00b7walt"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "$,", "KOUS", "ART", "ADJD", "NN", "$,", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Zw\u00e4ng\u2019einen andern Lauff zu nehmen;", "tokens": ["Zw\u00e4ng'\u00b7ei\u00b7nen", "an\u00b7dern", "Lauff", "zu", "neh\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}