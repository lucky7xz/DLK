{"dta.poem.1007": {"metadata": {"author": {"name": "George, Stefan", "birth": "N.A.", "death": "N.A."}, "title": "T RAURIGE   T \u00c4NZE", "genre": "Lyrik", "period": "N.A.", "pub_year": "1897", "urn": "urn:nbn:de:kobv:b4-200905191530", "language": ["de:0.99"], "booktitle": "George, Stefan: Das Jahr der Seele. Berlin, 1897."}, "poem": {"stanza.1": {"line.1": {"text": "Die st&#252;rme stieben &#252;ber brache fl&#228;chen               ", "tokens": ["Die", "st", "&#252;", "rme", "stie\u00b7ben", "&#252;", "ber", "bra\u00b7che", "fl", "&#228;", "chen"], "token_info": ["word", "word", "XML_entity", "word", "word", "XML_entity", "word", "word", "word", "XML_entity", "word"], "pos": ["ART", "FM", "FM", "FM", "FM", "FM", "FM", "FM", "FM", "FM", "FM"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und machen heller ahnung voll die runde               ", "tokens": ["Und", "ma\u00b7chen", "hel\u00b7ler", "ah\u00b7nung", "voll", "die", "run\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJA", "NN", "ADJD", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da wollen sich erstickte fluren r&#228;chen               ", "tokens": ["Da", "wol\u00b7len", "sich", "er\u00b7stick\u00b7te", "flu\u00b7ren", "r", "&#228;", "chen"], "token_info": ["word", "word", "word", "word", "word", "word", "XML_entity", "word"], "pos": ["ADV", "VMFIN", "PRF", "ADJA", "NN", "NE", "$(", "NE"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da zittert seufzen aus dem bergesschlunde             ", "tokens": ["Da", "zit\u00b7tert", "seuf\u00b7zen", "aus", "dem", "ber\u00b7ges\u00b7schlun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NN", "APPR", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Es scheint als ob die schrecklich fernen grollen               ", "tokens": ["Es", "scheint", "als", "ob", "die", "schreck\u00b7lich", "fer\u00b7nen", "grol\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KOKOM", "KOUS", "ART", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Doch eine stimme mahnt aus friedensf\u00f6hren:               ", "tokens": ["Doch", "ei\u00b7ne", "stim\u00b7me", "mahnt", "aus", "frie\u00b7dens\u00b7f\u00f6h\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Hast du mir ehdem nicht versprechen sollen               ", "tokens": ["Hast", "du", "mir", "eh\u00b7dem", "nicht", "ver\u00b7spre\u00b7chen", "sol\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "PPER", "ADV", "PTKNEG", "VVINF", "VMFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der gr&#228;ber ruh mit klage nie zu st\u00f6ren             ", "tokens": ["Der", "gr", "&#228;", "ber", "ruh", "mit", "kla\u00b7ge", "nie", "zu", "st\u00f6\u00b7ren"], "token_info": ["word", "word", "XML_entity", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$(", "APPR", "NN", "APPR", "VVFIN", "ADV", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ich zog vorbei am winterlichen pfahle               ", "tokens": ["Ich", "zog", "vor\u00b7bei", "am", "win\u00b7ter\u00b7li\u00b7chen", "pfah\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vor dem wir nie in leerem weinen knieten               ", "tokens": ["Vor", "dem", "wir", "nie", "in", "lee\u00b7rem", "wei\u00b7nen", "knie\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "PPER", "ADV", "APPR", "ADJA", "VVINF", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ich bat dich nur der bald ihn sieht dem strahle               ", "tokens": ["Ich", "bat", "dich", "nur", "der", "bald", "ihn", "sieht", "dem", "strah\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ART", "ADV", "PPER", "VVFIN", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Des frohen lenzes meinen gruss zu bieten.             ", "tokens": ["Des", "fro\u00b7hen", "len\u00b7zes", "mei\u00b7nen", "gruss", "zu", "bie\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}