{"dta.poem.5399": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "J rdisches  \n  V ergn\u00fcgen  \n in  \n  GOTT .  \n  F \u00fcnfter  T heil.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Hiedurch scheint \u00fcberall im Dunckeln,", "tokens": ["Hie\u00b7durch", "scheint", "\u00fc\u00b7be\u00b7rall", "im", "Dun\u00b7ckeln", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Jm Wald\u2019 und Feld\u2019, an manchem Ort,", "tokens": ["Jm", "Wald'", "und", "Feld'", ",", "an", "man\u00b7chem", "Ort", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "NN", "$,", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Auf manchem Baum, bald hier, bald dort,", "tokens": ["Auf", "man\u00b7chem", "Baum", ",", "bald", "hier", ",", "bald", "dort", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "ADV", "ADV", "$,", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein bunter Sonnen-Strahl zu funckeln.", "tokens": ["Ein", "bun\u00b7ter", "Son\u00b7nen\u00b7Strahl", "zu", "fun\u00b7ckeln", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Es scheinet gleichsam eingesencket", "tokens": ["Es", "schei\u00b7net", "gleich\u00b7sam", "ein\u00b7ge\u00b7sen\u00b7cket"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der Sonnen Glut in ihre Zier,", "tokens": ["Der", "Son\u00b7nen", "Glut", "in", "ih\u00b7re", "Zier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Als h\u00e4tten sie, wie L\u00f6sch-Papier,", "tokens": ["Als", "h\u00e4t\u00b7ten", "sie", ",", "wie", "L\u00f6schPa\u00b7pier", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPER", "$,", "PWAV", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Den Strahl der Sonnen eingetr\u00e4ncket.", "tokens": ["Den", "Strahl", "der", "Son\u00b7nen", "ein\u00b7ge\u00b7tr\u00e4n\u00b7cket", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Manch gelb-und roth-gef\u00e4rbt Geb\u00fcsche", "tokens": ["Manch", "gelb\u00b7\u00b7und", "ro\u00b7th\u00b7ge\u00b7f\u00e4rbt", "Ge\u00b7b\u00fc\u00b7sche"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "NN", "ADJD", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Macht gleichfals, mit gef\u00e4rbtem Licht,", "tokens": ["Macht", "gleich\u00b7fals", ",", "mit", "ge\u00b7f\u00e4rb\u00b7tem", "Licht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Auf mancher Stelle, dem Gesicht", "tokens": ["Auf", "man\u00b7cher", "Stel\u00b7le", ",", "dem", "Ge\u00b7sicht"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PIAT", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Ein bunt und liebliches Gemische.", "tokens": ["Ein", "bunt", "und", "lieb\u00b7li\u00b7ches", "Ge\u00b7mi\u00b7sche", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "An denen vormahls dichten Hecken,", "tokens": ["An", "de\u00b7nen", "vor\u00b7mahls", "dich\u00b7ten", "He\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Die jetzt zwar ziemlich Bl\u00e4tter-lo\u00df,", "tokens": ["Die", "jetzt", "zwar", "ziem\u00b7lich", "Bl\u00e4t\u00b7ter\u00b7lo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "ADV", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Doch noch nicht g\u00e4ntzlich nackt und blo\u00df,", "tokens": ["Doch", "noch", "nicht", "g\u00e4ntz\u00b7lich", "nackt", "und", "blo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PTKNEG", "ADJD", "ADJD", "KON", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Ist neue Sch\u00f6nheit zu entdecken.", "tokens": ["Ist", "neu\u00b7e", "Sch\u00f6n\u00b7heit", "zu", "ent\u00b7de\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Der Bl\u00e4tter gelb und r\u00f6htlich Prangen,", "tokens": ["Der", "Bl\u00e4t\u00b7ter", "gelb", "und", "r\u00f6ht\u00b7lich", "Pran\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "KON", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Das fast wie Gold und wie Rubin,", "tokens": ["Das", "fast", "wie", "Gold", "und", "wie", "Ru\u00b7bin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "KOKOM", "NN", "KON", "PWAV", "NE", "$,"], "meter": "-+-+-++-", "measure": "unknown.measure.tetra"}, "line.19": {"text": "Doch \u00fcberall vermischt mit Gr\u00fcn,", "tokens": ["Doch", "\u00fc\u00b7be\u00b7rall", "ver\u00b7mischt", "mit", "Gr\u00fcn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Sieht man an braunen Aesten hangen.", "tokens": ["Sieht", "man", "an", "brau\u00b7nen", "A\u00b7es\u00b7ten", "han\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.21": {"text": "Man kann auch jetzo mit Vergn\u00fcgen", "tokens": ["Man", "kann", "auch", "jet\u00b7zo", "mit", "Ver\u00b7gn\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VMFIN", "ADV", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Durch bunte Bl\u00e4tter, die so sch\u00f6n,", "tokens": ["Durch", "bun\u00b7te", "Bl\u00e4t\u00b7ter", ",", "die", "so", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "PRELS", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Die kleinen bunten V\u00f6gel sehn,", "tokens": ["Die", "klei\u00b7nen", "bun\u00b7ten", "V\u00f6\u00b7gel", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Weil sie fast unbedecket fliegen.", "tokens": ["Weil", "sie", "fast", "un\u00b7be\u00b7de\u00b7cket", "flie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Der Blick wird \u00fcberall erfreuet;", "tokens": ["Der", "Blick", "wird", "\u00fc\u00b7be\u00b7rall", "er\u00b7freu\u00b7et", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Es ziert so gar das bunte Laub", "tokens": ["Es", "ziert", "so", "gar", "das", "bun\u00b7te", "Laub"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Das dunckle Land, den feuchten Staub,", "tokens": ["Das", "dunck\u00b7le", "Land", ",", "den", "feuch\u00b7ten", "Staub", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Als w\u00e4ren Blumen drauf gestreuet.", "tokens": ["Als", "w\u00e4\u00b7ren", "Blu\u00b7men", "drauf", "ge\u00b7streu\u00b7et", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "NN", "PAV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.29": {"text": "Noch mehrer Sch\u00f6nheit wird erblicket;", "tokens": ["Noch", "meh\u00b7rer", "Sch\u00f6n\u00b7heit", "wird", "er\u00b7bli\u00b7cket", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "VAFIN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.30": {"text": "Denn wie im Herbst der Erden-Rund;", "tokens": ["Denn", "wie", "im", "Herbst", "der", "Er\u00b7den\u00b7Rund", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "So ist die Luft, nicht minder bunt,", "tokens": ["So", "ist", "die", "Luft", ",", "nicht", "min\u00b7der", "bunt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "$,", "PTKNEG", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "Mit Glantz und Farben ausgeschm\u00fccket.", "tokens": ["Mit", "Glantz", "und", "Far\u00b7ben", "aus\u00b7ge\u00b7schm\u00fc\u00b7cket", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}