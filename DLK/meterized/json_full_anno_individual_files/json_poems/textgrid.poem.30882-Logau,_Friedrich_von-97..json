{"textgrid.poem.30882": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "97.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer ihm selbst kan frey befehlen,", "tokens": ["Wer", "ihm", "selbst", "kan", "frey", "be\u00b7feh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "VMFIN", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wer ihm selbst gehorchen kan,", "tokens": ["Wer", "ihm", "selbst", "ge\u00b7hor\u00b7chen", "kan", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mag sich unter diese zehlen,", "tokens": ["Mag", "sich", "un\u00b7ter", "die\u00b7se", "zeh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "APPR", "PDS", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die der Himmel lachet an.", "tokens": ["Die", "der", "Him\u00b7mel", "la\u00b7chet", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wer sein selbst kan f\u00fcglich seyn,", "tokens": ["Wer", "sein", "selbst", "kan", "f\u00fcg\u00b7lich", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "ADV", "VMFIN", "ADJD", "VAINF", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Geh kein andre Pflichten ein.", "tokens": ["Geh", "kein", "and\u00b7re", "Pflich\u00b7ten", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PIAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Der, der andren denckt zu leben,", "tokens": ["Der", ",", "der", "an\u00b7dren", "denckt", "zu", "le\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "PRELS", "PIS", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Dem bleibt von ihm selbst nicht viel,", "tokens": ["Dem", "bleibt", "von", "ihm", "selbst", "nicht", "viel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "PPER", "ADV", "PTKNEG", "ADV", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "Mu\u00df ihm selbsten Urlaub geben,", "tokens": ["Mu\u00df", "ihm", "selbs\u00b7ten", "Ur\u00b7laub", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Darff nicht wollen, was er wil:", "tokens": ["Darff", "nicht", "wol\u00b7len", ",", "was", "er", "wil", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PTKNEG", "VMFIN", "$,", "PWS", "PPER", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Wer sein selbst kan f\u00fcglich seyn,", "tokens": ["Wer", "sein", "selbst", "kan", "f\u00fcg\u00b7lich", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "ADV", "VMFIN", "ADJD", "VAINF", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.12": {"text": "Geh kein andre Pflichten ein.", "tokens": ["Geh", "kein", "and\u00b7re", "Pflich\u00b7ten", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PIAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "Grossen Herren sich verbinden,", "tokens": ["Gros\u00b7sen", "Her\u00b7ren", "sich", "ver\u00b7bin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Heist f\u00fcr seine M\u00fch und Treu", "tokens": ["Heist", "f\u00fcr", "sei\u00b7ne", "M\u00fch", "und", "Treu"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "PPOSAT", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.15": {"text": "Ungunst erndten, Unruh finden", "tokens": ["Un\u00b7gunst", "ernd\u00b7ten", ",", "Un\u00b7ruh", "fin\u00b7den"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADV", "VVINF", "$,", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Und verdienen nichts als Reu:", "tokens": ["Und", "ver\u00b7die\u00b7nen", "nichts", "als", "Reu", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "KOKOM", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.17": {"text": "Wer sein selbst kan f\u00fcglich seyn,", "tokens": ["Wer", "sein", "selbst", "kan", "f\u00fcg\u00b7lich", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "ADV", "VMFIN", "ADJD", "VAINF", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.18": {"text": "Geh kein andre Pflichten ein.", "tokens": ["Geh", "kein", "and\u00b7re", "Pflich\u00b7ten", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PIAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.19": {"text": "Hohen Ohren recht zu singen,", "tokens": ["Ho\u00b7hen", "Oh\u00b7ren", "recht", "zu", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Mu\u00df der Thon gar linde gehn;", "tokens": ["Mu\u00df", "der", "Thon", "gar", "lin\u00b7de", "gehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "ADV", "ADJA", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.21": {"text": "Kein Gesang wil lieblich klingen,", "tokens": ["Kein", "Ge\u00b7sang", "wil", "lieb\u00b7lich", "klin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "Wo der Warheit Noten stehn:", "tokens": ["Wo", "der", "War\u00b7heit", "No\u00b7ten", "stehn", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.23": {"text": "Wer sein selbst kan f\u00fcglich seyn,", "tokens": ["Wer", "sein", "selbst", "kan", "f\u00fcg\u00b7lich", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "ADV", "VMFIN", "ADJD", "VAINF", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.24": {"text": "Geh kein andre Pflichten ein.", "tokens": ["Geh", "kein", "and\u00b7re", "Pflich\u00b7ten", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PIAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.25": {"text": "Hohen Augen wil behagen", "tokens": ["Ho\u00b7hen", "Au\u00b7gen", "wil", "be\u00b7ha\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "VMFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.26": {"text": "Nichts, was nicht von Farben ist;", "tokens": ["Nichts", ",", "was", "nicht", "von", "Far\u00b7ben", "ist", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "PRELS", "PTKNEG", "APPR", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.27": {"text": "Der wird weg viel Flecken tragen,", "tokens": ["Der", "wird", "weg", "viel", "Fle\u00b7cken", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "PIAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.28": {"text": "Der das reine Wei\u00df erkiest:", "tokens": ["Der", "das", "rei\u00b7ne", "Wei\u00df", "er\u00b7kiest", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.29": {"text": "Wer sein selbst kan f\u00fcglich seyn,", "tokens": ["Wer", "sein", "selbst", "kan", "f\u00fcg\u00b7lich", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "ADV", "VMFIN", "ADJD", "VAINF", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.30": {"text": "Geh kein andre Pflichten ein.", "tokens": ["Geh", "kein", "and\u00b7re", "Pflich\u00b7ten", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PIAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.31": {"text": "Reiche Worte, breite Tittel", "tokens": ["Rei\u00b7che", "Wor\u00b7te", ",", "brei\u00b7te", "Tit\u00b7tel"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NE", "NN", "$,", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.32": {"text": "Sind de\u00df Hofes s\u00fcsser Brey", "tokens": ["Sind", "de\u00df", "Ho\u00b7fes", "s\u00fcs\u00b7ser", "Brey"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.33": {"text": "Und die Wiege, die man sch\u00fcttel,", "tokens": ["Und", "die", "Wie\u00b7ge", ",", "die", "man", "sch\u00fct\u00b7tel", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "PIS", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.34": {"text": "Bi\u00df das Kind entschlafen sey:", "tokens": ["Bi\u00df", "das", "Kind", "ent\u00b7schla\u00b7fen", "sey", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.35": {"text": "Wer sein selbst kan f\u00fcglich seyn,", "tokens": ["Wer", "sein", "selbst", "kan", "f\u00fcg\u00b7lich", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "ADV", "VMFIN", "ADJD", "VAINF", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.36": {"text": "Geh kein andre Pflichten ein.", "tokens": ["Geh", "kein", "and\u00b7re", "Pflich\u00b7ten", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PIAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.37": {"text": "Wer sich nicht wil stillen lassen,", "tokens": ["Wer", "sich", "nicht", "wil", "stil\u00b7len", "las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "PTKNEG", "VMFIN", "VVINF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.38": {"text": "Der ist mehr kein liebes Kind;", "tokens": ["Der", "ist", "mehr", "kein", "lie\u00b7bes", "Kind", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "PIAT", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.39": {"text": "Der mu\u00df mehr, wer Gunst wil fassen,", "tokens": ["Der", "mu\u00df", "mehr", ",", "wer", "Gunst", "wil", "fas\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ADV", "$,", "PWS", "NN", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.40": {"text": "Kindisch seyn als Kinder sind:", "tokens": ["Kin\u00b7disch", "seyn", "als", "Kin\u00b7der", "sind", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAINF", "KOKOM", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.41": {"text": "Wer sein selbst kan f\u00fcglich seyn,", "tokens": ["Wer", "sein", "selbst", "kan", "f\u00fcg\u00b7lich", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "ADV", "VMFIN", "ADJD", "VAINF", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.42": {"text": "Geh kein andre Pflichten ein.", "tokens": ["Geh", "kein", "and\u00b7re", "Pflich\u00b7ten", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PIAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.43": {"text": "Ob er viel hat au\u00dfgerichtet,", "tokens": ["Ob", "er", "viel", "hat", "au\u00df\u00b7ge\u00b7rich\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.44": {"text": "Hat er doch nur di\u00df verricht:", "tokens": ["Hat", "er", "doch", "nur", "di\u00df", "ver\u00b7richt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "PDS", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.45": {"text": "Da\u00df, ie mehr