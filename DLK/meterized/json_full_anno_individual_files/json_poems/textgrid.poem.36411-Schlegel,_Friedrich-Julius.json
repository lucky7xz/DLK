{"textgrid.poem.36411": {"metadata": {"author": {"name": "Schlegel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Julius", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Lust, im Geist des Freundes sich vertiefen,", "tokens": ["O", "Lust", ",", "im", "Geist", "des", "Freun\u00b7des", "sich", "ver\u00b7tie\u00b7fen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "APPRART", "NN", "ART", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gleich ihm zu werden, ihn wie sich zu sehen,", "tokens": ["Gleich", "ihm", "zu", "wer\u00b7den", ",", "ihn", "wie", "sich", "zu", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "PTKZU", "VAINF", "$,", "PPER", "PWAV", "PRF", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Was er je sein kann, wissen wie geschehen,", "tokens": ["Was", "er", "je", "sein", "kann", ",", "wis\u00b7sen", "wie", "ge\u00b7sche\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "VAINF", "VMFIN", "$,", "VVFIN", "KOKOM", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Welten klar geschaut, die in ihm schliefen!", "tokens": ["Die", "Wel\u00b7ten", "klar", "ge\u00b7schaut", ",", "die", "in", "ihm", "schlie\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVPP", "$,", "PRELS", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "So viel Geheimes wir ans Licht auch riefen,", "tokens": ["So", "viel", "Ge\u00b7hei\u00b7mes", "wir", "ans", "Licht", "auch", "rie\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "PPER", "APPRART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und achten nichts den Schmerz der geist'gen Wehen,", "tokens": ["Und", "ach\u00b7ten", "nichts", "den", "Schmerz", "der", "geist'\u00b7gen", "We\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sein Erstes kann wohl nie der Geist verstehen,", "tokens": ["Sein", "Ers\u00b7tes", "kann", "wohl", "nie", "der", "Geist", "ver\u00b7ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "ADV", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Schaut da verstummt in unerforschte Tiefen.", "tokens": ["Schaut", "da", "ver\u00b7stummt", "in", "un\u00b7er\u00b7forschte", "Tie\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}}, "stanza.3": {"line.1": {"text": "So schl\u00e4gt die zarte Liebe selbst sich Wunden,", "tokens": ["So", "schl\u00e4gt", "die", "zar\u00b7te", "Lie\u00b7be", "selbst", "sich", "Wun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "ADV", "PRF", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Freund auch scheint von fern uns nur zu nahen,", "tokens": ["Der", "Freund", "auch", "scheint", "von", "fern", "uns", "nur", "zu", "na\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "APPR", "ADJD", "PPER", "ADV", "APPR", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das Sch\u00f6nste dennoch arm dem vollen Triebe:", "tokens": ["Das", "Sch\u00f6ns\u00b7te", "den\u00b7noch", "arm", "dem", "vol\u00b7len", "Trie\u00b7be", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Doch gleicht dem ersten Blick, den wir uns sahen,", "tokens": ["Doch", "gleicht", "dem", "ers\u00b7ten", "Blick", ",", "den", "wir", "uns", "sa\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$,", "PRELS", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der andre, als der h\u00f6chste Freund gefunden,", "tokens": ["Der", "and\u00b7re", ",", "als", "der", "h\u00f6chs\u00b7te", "Freund", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "$,", "KOUS", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das Licht des Einz'gen neu verkl\u00e4rt die Liebe.", "tokens": ["Das", "Licht", "des", "Einz'\u00b7gen", "neu", "ver\u00b7kl\u00e4rt", "die", "Lie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "ADJD", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "O Lust, im Geist des Freundes 