{"dta.poem.13952": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Im Herbst .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "So brecht hervor nur, alte Lieder.", "tokens": ["So", "brecht", "her\u00b7vor", "nur", ",", "al\u00b7te", "Lie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PTKVZ", "ADV", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und brecht das Herz mir ab!", "tokens": ["Und", "brecht", "das", "Herz", "mir", "ab", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Noch einmal gr\u00fc\u00df' ich aus der Ferne wieder", "tokens": ["Noch", "ein\u00b7mal", "gr\u00fc\u00df'", "ich", "aus", "der", "Fer\u00b7ne", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Was ich nur Liebes hab',", "tokens": ["Was", "ich", "nur", "Lie\u00b7bes", "hab'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "NN", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Mich aber zieht es nieder", "tokens": ["Mich", "a\u00b7ber", "zieht", "es", "nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "PPER", "PTKVZ"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Vor Wehmuth wie in's Grab.", "tokens": ["Vor", "Weh\u00b7muth", "wie", "in's", "Grab", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KOKOM", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}