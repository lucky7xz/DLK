n", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VAFIN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Der du dieses Band erkiest,", "tokens": ["Der", "du", "die\u00b7ses", "Band", "er\u00b7kiest", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PDAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "So der Himmel selbst gesponnen,", "tokens": ["So", "der", "Him\u00b7mel", "selbst", "ge\u00b7spon\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und fast mehr als Freyheit ist.", "tokens": ["Und", "fast", "mehr", "als", "Frey\u00b7heit", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "KOUS", "NN", "VAFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.18": {"line.1": {"text": "Sch\u00f6nste Braut, sey nicht best\u00fcrtzet,", "tokens": ["Sch\u00f6ns\u00b7te", "Braut", ",", "sey", "nicht", "be\u00b7st\u00fcrt\u00b7zet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VAFIN", "PTKNEG", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Freude hindert nicht die Zucht,", "tokens": ["Freu\u00b7de", "hin\u00b7dert", "nicht", "die", "Zucht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PTKNEG", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wer hat deine Macht verk\u00fcrtzet,", "tokens": ["Wer", "hat", "dei\u00b7ne", "Macht", "ver\u00b7k\u00fcrt\u00b7zet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPOSAT", "N