{"textgrid.poem.34591": {"metadata": {"author": {"name": "Hoffmannswaldau, Christian Hoffmann von", "birth": "N.A.", "death": "N.A."}, "title": "14. Hochzeit-Gedichte", "genre": "verse", "period": "N.A.", "pub_year": 1647, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sol der Degen an den Nagel?", "tokens": ["Sol", "der", "De\u00b7gen", "an", "den", "Na\u00b7gel", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "APPR", "ART", "NE", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wird der Helm nun abgelegt?", "tokens": ["Wird", "der", "Helm", "nun", "ab\u00b7ge\u00b7legt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ruht der blaue Feuer-Hagel,", "tokens": ["Ruht", "der", "blau\u00b7e", "Feu\u00b7er\u00b7Ha\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "So den Schlacht-Gott selbst bewegt?", "tokens": ["So", "den", "Schlacht\u00b7Gott", "selbst", "be\u00b7wegt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Soll die Lust den Feind zud\u00e4mpfen,", "tokens": ["Soll", "die", "Lust", "den", "Feind", "zu\u00b7d\u00e4mp\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Zubezwingen Stahl und Stein,", "tokens": ["Zu\u00b7be\u00b7zwin\u00b7gen", "Stahl", "und", "Stein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Soll die Brunst zu Sturm und K\u00e4mpffen,", "tokens": ["Soll", "die", "Brunst", "zu", "Sturm", "und", "K\u00e4mpf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Todt und gantz erloschen seyn?", "tokens": ["Todt", "und", "gantz", "er\u00b7lo\u00b7schen", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADV", "VVPP", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Also kan ein s\u00fcsses Blitzen,", "tokens": ["Al\u00b7so", "kan", "ein", "s\u00fcs\u00b7ses", "Blit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und ein Wunder-reicher Brand,", "tokens": ["Und", "ein", "Wun\u00b7der\u00b7rei\u00b7cher", "Brand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dich nach neuer Art erhitzen,", "tokens": ["Dich", "nach", "neu\u00b7er", "Art", "er\u00b7hit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und ver\u00e4ndern Hertz und Hand,", "tokens": ["Und", "ver\u00b7\u00e4n\u00b7dern", "Hertz", "und", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Da\u00df du f\u00fchlst ein neues Brennen,", "tokens": ["Da\u00df", "du", "f\u00fchlst", "ein", "neu\u00b7es", "Bren\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Da\u00df du folgest frembder Fahn,", "tokens": ["Da\u00df", "du", "fol\u00b7gest", "fremb\u00b7der", "Fahn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Da\u00df dich Mars nicht mehr will kennen,", "tokens": ["Da\u00df", "dich", "Mars", "nicht", "mehr", "will", "ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "PTKNEG", "ADV", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Da\u00df du suchest neue Bahn.", "tokens": ["Da\u00df", "du", "su\u00b7chest", "neu\u00b7e", "Bahn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Di\u00df sind Kr\u00e4ften dieser Gaben,", "tokens": ["Di\u00df", "sind", "Kr\u00e4f\u00b7ten", "die\u00b7ser", "Ga\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "NN", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Di\u00df sind Funcken dieser Gluth,", "tokens": ["Di\u00df", "sind", "Fun\u00b7cken", "die\u00b7ser", "Gluth", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "NN", "PDAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "So der Himmel hat gegraben", "tokens": ["So", "der", "Him\u00b7mel", "hat", "ge\u00b7gra\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "VAFIN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In der Liebsten Geist und Blut.", "tokens": ["In", "der", "Liebs\u00b7ten", "Geist", "und", "Blut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "KON", "NN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.5": {"text": "Dieses ist, was dich den Degen,", "tokens": ["Die\u00b7ses", "ist", ",", "was", "dich", "den", "De\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "$,", "PRELS", "PRF", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "So dir noch kein Feind gethan,", "tokens": ["So", "dir", "noch", "kein", "Feind", "ge\u00b7than", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ADV", "PIAT", "NN", "VVPP", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.7": {"text": "Freundlich heisset niederlegen,", "tokens": ["Freund\u00b7lich", "heis\u00b7set", "nie\u00b7der\u00b7le\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und dich \u00fcbermeistern kan.", "tokens": ["Und", "dich", "\u00fc\u00b7ber\u00b7meis\u00b7tern", "kan", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Und wie solten nicht die Blicke,", "tokens": ["Und", "wie", "sol\u00b7ten", "nicht", "die", "Bli\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "VMFIN", "PTKNEG", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die ein keusches Auge f\u00fchrt,", "tokens": ["Die", "ein", "keu\u00b7sches", "Au\u00b7ge", "f\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Derer Glut durch keine T\u00fccke", "tokens": ["De\u00b7rer", "Glut", "durch", "kei\u00b7ne", "T\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Falscher Zeiten wird ber\u00fchrt,", "tokens": ["Fal\u00b7scher", "Zei\u00b7ten", "wird", "be\u00b7r\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Stahl und Eisen selbst entbrennen.", "tokens": ["Stahl", "und", "Ei\u00b7sen", "selbst", "ent\u00b7bren\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Mars und seine Helden-Hand,", "tokens": ["Mars", "und", "sei\u00b7ne", "Hel\u00b7den\u00b7Hand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Sollt er diese Venus kennen,", "tokens": ["Sollt", "er", "die\u00b7se", "Ve\u00b7nus", "ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PDAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "F\u00fchlte mehr als Liebes-Brand.", "tokens": ["F\u00fchl\u00b7te", "mehr", "als", "Lie\u00b7bes\u00b7Brand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "KOUS", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Sind nicht Adel, Witz und Tugend", "tokens": ["Sind", "nicht", "A\u00b7del", ",", "Witz", "und", "Tu\u00b7gend"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "PTKNEG", "NN", "$,", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Vor Geschwister hier geacht?", "tokens": ["Vor", "Ge\u00b7schwis\u00b7ter", "hier", "ge\u00b7acht", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Zeigt die fr\u00fchlings-gleiche Jugend", "tokens": ["Zeigt", "die", "fr\u00fch\u00b7lings\u00b7glei\u00b7che", "Ju\u00b7gend"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nicht die bundte Wunder-Pracht?", "tokens": ["Nicht", "die", "bund\u00b7te", "Wun\u00b7der\u00b7Pracht", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Sind nicht die ber\u00fchmten Sch\u00e4tze,", "tokens": ["Sind", "nicht", "die", "be\u00b7r\u00fchm\u00b7ten", "Sch\u00e4t\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "So die Morgenr\u00f6the tr\u00e4gt,", "tokens": ["So", "die", "Mor\u00b7gen\u00b7r\u00f6\u00b7the", "tr\u00e4gt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Nach der Sch\u00f6nheit Kunst-Gesetze", "tokens": ["Nach", "der", "Sch\u00f6n\u00b7heit", "Kunst\u00b7Ge\u00b7set\u00b7ze"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Auf den keuschen Mund gepr\u00e4gt?", "tokens": ["Auf", "den", "keu\u00b7schen", "Mund", "ge\u00b7pr\u00e4gt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Sind die klaren Asteriten,", "tokens": ["Sind", "die", "kla\u00b7ren", "As\u00b7te\u00b7ri\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und das ungemeine Licht,", "tokens": ["Und", "das", "un\u00b7ge\u00b7mei\u00b7ne", "Licht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "So die Freyheit dir bestritten,", "tokens": ["So", "die", "Frey\u00b7heit", "dir", "be\u00b7strit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und in deine Seele bricht,", "tokens": ["Und", "in", "dei\u00b7ne", "See\u00b7le", "bricht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Nicht der Sonnen selbst zu gleichen,", "tokens": ["Nicht", "der", "Son\u00b7nen", "selbst", "zu", "glei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "So im Himmel Wache h\u00e4lt,", "tokens": ["So", "im", "Him\u00b7mel", "Wa\u00b7che", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und begierig Ihm zuweichen,", "tokens": ["Und", "be\u00b7gie\u00b7rig", "Ihm", "zu\u00b7wei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Zeitlich in die Welle f\u00e4llt?", "tokens": ["Zeit\u00b7lich", "in", "die", "Wel\u00b7le", "f\u00e4llt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Nun du wirst mit solchen Gaben", "tokens": ["Nun", "du", "wirst", "mit", "sol\u00b7chen", "Ga\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "VAFIN", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Von der grossen Hand umkr\u00e4ntzt,", "tokens": ["Von", "der", "gros\u00b7sen", "Hand", "um\u00b7kr\u00e4ntzt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Die mehr Pracht und Sch\u00f6nheit haben,", "tokens": ["Die", "mehr", "Pracht", "und", "Sch\u00f6n\u00b7heit", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "KON", "NN", "VAFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Als in ", "tokens": ["Als", "in"], "token_info": ["word", "word"], "pos": ["KOUS", "APPR"], "meter": "++", "measure": "spondeus"}, "line.5": {"text": "Warlich du hast viel gewonnen,", "tokens": ["War\u00b7lich", "du", "hast", "viel", "ge\u00b7won\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VAFIN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Der du dieses Band erkiest,", "tokens": ["Der", "du", "die\u00b7ses", "Band", "er\u00b7kiest", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PDAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "So der Himmel selbst gesponnen,", "tokens": ["So", "der", "Him\u00b7mel", "selbst", "ge\u00b7spon\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und fast mehr als Freyheit ist.", "tokens": ["Und", "fast", "mehr", "als", "Frey\u00b7heit", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "KOUS", "NN", "VAFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.8": {"line.1": {"text": "Sch\u00f6nste Braut, sey nicht best\u00fcrtzet,", "tokens": ["Sch\u00f6ns\u00b7te", "Braut", ",", "sey", "nicht", "be\u00b7st\u00fcrt\u00b7zet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VAFIN", "PTKNEG", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Freude hindert nicht die Zucht,", "tokens": ["Freu\u00b7de", "hin\u00b7dert", "nicht", "die", "Zucht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PTKNEG", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wer hat deine Macht verk\u00fcrtzet,", "tokens": ["Wer", "hat", "dei\u00b7ne", "Macht", "ver\u00b7k\u00fcrt\u00b7zet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und gest\u00f6rt des Sieges Frucht?", "tokens": ["Und", "ge\u00b7st\u00f6rt", "des", "Sie\u00b7ges", "Frucht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Herrsche frey auf deinem Throne,", "tokens": ["Herr\u00b7sche", "frey", "auf", "dei\u00b7nem", "Thro\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Dessen Freyheit du belegt,", "tokens": ["Des\u00b7sen", "Frey\u00b7heit", "du", "be\u00b7legt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Setzet keines Reiches Crone,", "tokens": ["Set\u00b7zet", "kei\u00b7nes", "Rei\u00b7ches", "Cro\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "F\u00fcr die Fessel, die es tr\u00e4gt.", "tokens": ["F\u00fcr", "die", "Fes\u00b7sel", ",", "die", "es", "tr\u00e4gt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Edles Paar, genie\u00dft der Fr\u00fcchte,", "tokens": ["Ed\u00b7les", "Paar", ",", "ge\u00b7nie\u00dft", "der", "Fr\u00fcch\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "So der Himmel euch geschenckt.", "tokens": ["So", "der", "Him\u00b7mel", "euch", "ge\u00b7schenckt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schaut doch, wie mit einem Lichte", "tokens": ["Schaut", "doch", ",", "wie", "mit", "ei\u00b7nem", "Lich\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "$,", "PWAV", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hesperus sich zu euch lenckt.", "tokens": ["Hes\u00b7pe\u00b7rus", "sich", "zu", "euch", "lenckt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PRF", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Tausend hoch gestellte Kertzen,", "tokens": ["Tau\u00b7send", "hoch", "ge\u00b7stell\u00b7te", "Kert\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Leuchten euch zur Abend Ruh,", "tokens": ["Leuch\u00b7ten", "euch", "zur", "A\u00b7bend", "Ruh", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPRART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und Cupido schleusst mit Schertzen", "tokens": ["Und", "Cu\u00b7pi\u00b7do", "schleusst", "mit", "Schert\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "NE", "VVFIN", "APPR", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Die ber\u00fchmte Kammer zu.", "tokens": ["Die", "be\u00b7r\u00fchm\u00b7te", "Kam\u00b7mer", "zu", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Bleibet lange bey Gel\u00fccke,", "tokens": ["Blei\u00b7bet", "lan\u00b7ge", "bey", "Ge\u00b7l\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Doch nicht allzulang allein,", "tokens": ["Doch", "nicht", "all\u00b7zu\u00b7lang", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ADJD", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Lasset zarter Augen Blicke,", "tokens": ["Las\u00b7set", "zar\u00b7ter", "Au\u00b7gen", "Bli\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Eurer Liebe Zeuge seyn,", "tokens": ["Eu\u00b7rer", "Lie\u00b7be", "Zeu\u00b7ge", "seyn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Zeugen eurer gr\u00fcnen Jugend,", "tokens": ["Zeu\u00b7gen", "eu\u00b7rer", "gr\u00fc\u00b7nen", "Ju\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Zeugen eurer jungen Zeit,", "tokens": ["Zeu\u00b7gen", "eu\u00b7rer", "jun\u00b7gen", "Zeit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Zeugen eurer V\u00e4ter Tugend,", "tokens": ["Zeu\u00b7gen", "eu\u00b7rer", "V\u00e4\u00b7ter", "Tu\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und der Mutter Freundligkeit.", "tokens": ["Und", "der", "Mut\u00b7ter", "Freund\u00b7lig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Sol der Degen an den Nagel?", "tokens": ["Sol", "der", "De\u00b7gen", "an", "den", "Na\u00b7gel", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "APPR", "ART", "NE", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wird der Helm nun abgelegt?", "tokens": ["Wird", "der", "Helm", "nun", "ab\u00b7ge\u00b7legt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ruht der blaue Feuer-Hagel,", "tokens": ["Ruht", "der", "blau\u00b7e", "Feu\u00b7er\u00b7Ha\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "So den Schlacht-Gott selbst bewegt?", "tokens": ["So", "den", "Schlacht\u00b7Gott", "selbst", "be\u00b7wegt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Soll die Lust den Feind zud\u00e4mpfen,", "tokens": ["Soll", "die", "Lust", "den", "Feind", "zu\u00b7d\u00e4mp\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Zubezwingen Stahl und Stein,", "tokens": ["Zu\u00b7be\u00b7zwin\u00b7gen", "Stahl", "und", "Stein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Soll die Brunst zu Sturm und K\u00e4mpffen,", "tokens": ["Soll", "die", "Brunst", "zu", "Sturm", "und", "K\u00e4mpf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Todt und gantz erloschen seyn?", "tokens": ["Todt", "und", "gantz", "er\u00b7lo\u00b7schen", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADV", "VVPP", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Also kan ein s\u00fcsses Blitzen,", "tokens": ["Al\u00b7so", "kan", "ein", "s\u00fcs\u00b7ses", "Blit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und ein Wunder-reicher Brand,", "tokens": ["Und", "ein", "Wun\u00b7der\u00b7rei\u00b7cher", "Brand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dich nach neuer Art erhitzen,", "tokens": ["Dich", "nach", "neu\u00b7er", "Art", "er\u00b7hit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und ver\u00e4ndern Hertz und Hand,", "tokens": ["Und", "ver\u00b7\u00e4n\u00b7dern", "Hertz", "und", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Da\u00df du f\u00fchlst ein neues Brennen,", "tokens": ["Da\u00df", "du", "f\u00fchlst", "ein", "neu\u00b7es", "Bren\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Da\u00df du folgest frembder Fahn,", "tokens": ["Da\u00df", "du", "fol\u00b7gest", "fremb\u00b7der", "Fahn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Da\u00df dich Mars nicht mehr will kennen,", "tokens": ["Da\u00df", "dich", "Mars", "nicht", "mehr", "will", "ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "PTKNEG", "ADV", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Da\u00df du suchest neue Bahn.", "tokens": ["Da\u00df", "du", "su\u00b7chest", "neu\u00b7e", "Bahn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Di\u00df sind Kr\u00e4ften dieser Gaben,", "tokens": ["Di\u00df", "sind", "Kr\u00e4f\u00b7ten", "die\u00b7ser", "Ga\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "NN", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Di\u00df sind Funcken dieser Gluth,", "tokens": ["Di\u00df", "sind", "Fun\u00b7cken", "die\u00b7ser", "Gluth", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "NN", "PDAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "So der Himmel hat gegraben", "tokens": ["So", "der", "Him\u00b7mel", "hat", "ge\u00b7gra\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "VAFIN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In der Liebsten Geist und Blut.", "tokens": ["In", "der", "Liebs\u00b7ten", "Geist", "und", "Blut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "KON", "NN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.5": {"text": "Dieses ist, was dich den Degen,", "tokens": ["Die\u00b7ses", "ist", ",", "was", "dich", "den", "De\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "$,", "PRELS", "PRF", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "So dir noch kein Feind gethan,", "tokens": ["So", "dir", "noch", "kein", "Feind", "ge\u00b7than", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ADV", "PIAT", "NN", "VVPP", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.7": {"text": "Freundlich heisset niederlegen,", "tokens": ["Freund\u00b7lich", "heis\u00b7set", "nie\u00b7der\u00b7le\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und dich \u00fcbermeistern kan.", "tokens": ["Und", "dich", "\u00fc\u00b7ber\u00b7meis\u00b7tern", "kan", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "Und wie solten nicht die Blicke,", "tokens": ["Und", "wie", "sol\u00b7ten", "nicht", "die", "Bli\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "VMFIN", "PTKNEG", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die 