sure": "trochaic.tetra"}}, "stanza.15": {"line.1": {"text": "Sind nicht Adel, Witz und Tugend", "tokens": ["Sind", "nicht", "A\u00b7del", ",", "Witz", "und", "Tu\u00b7gend"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "PTKNEG", "NN", "$,", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Vor Geschwister hier geacht?", "tokens": ["Vor", "Ge\u00b7schwis\u00b7ter", "hier", "ge\u00b7acht", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Zeigt die fr\u00fchlings-gleiche Jugend", "tokens": ["Zeigt", "die", "fr\u00fch\u00b7lings\u00b7glei\u00b7che", "Ju\u00b7gend"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nicht die bundte Wunder-Pracht?", "tokens": ["Nicht", "die", "bund\u00b7te", "Wun\u00b7der\u00b7Pracht", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Sind nicht die ber\u00fchmten Sch\u00e4tze,", "tokens": ["Sind", "nicht", "die", "be\u00b7r\u00fchm\u00b7ten", "Sch\u00e4t\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "So die Morgenr\u00f6the tr\u00e4gt,", "tokens": ["So", "die", "Mor\u00b7gen\u00b7r\u00f6\u00b7the", "tr\u00e4gt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Nach der Sch\u00f6nheit Kunst-Gesetze", "tokens": ["Nach", "der", "Sch\u00f6n\u00b7heit", "Kunst\u00b7Ge\u00b7set\u00b7ze"], "token_info": ["word", "word