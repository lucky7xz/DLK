{"dta.poem.5988": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von dem Bau der Welt.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Aus solchem Schaum, der grob und dicht,", "tokens": ["Aus", "sol\u00b7chem", "Schaum", ",", "der", "grob", "und", "dicht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "PRELS", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ist der Planeten Stoff vermuhtlich zugericht.", "tokens": ["Ist", "der", "Pla\u00b7ne\u00b7ten", "Stoff", "ver\u00b7muht\u00b7lich", "zu\u00b7ge\u00b7richt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "NN", "ADJD", "VVPP", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Da man nicht ferner leugnen kan,", "tokens": ["Da", "man", "nicht", "fer\u00b7ner", "leug\u00b7nen", "kan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PTKNEG", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df unsrer Erden-Ball nicht in der Reih", "tokens": ["Da\u00df", "uns\u00b7rer", "Er\u00b7den\u00b7Ball", "nicht", "in", "der", "Reih"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "PTKNEG", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Auch ein Planete sey.", "tokens": ["Auch", "ein", "Pla\u00b7ne\u00b7te", "sey", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VAFIN", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.6": {"text": "Jhr Wesen zeigt es uns ja selber deutlich an.", "tokens": ["Ihr", "We\u00b7sen", "zeigt", "es", "uns", "ja", "sel\u00b7ber", "deut\u00b7lich", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "PPER", "ADV", "ADV", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ein C\u00f6rper ist ein jeglicher Planet,", "tokens": ["Ein", "C\u00f6r\u00b7per", "ist", "ein", "jeg\u00b7li\u00b7cher", "Pla\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "PIAT", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Der aus verschiednen Stoff und C\u00f6rperchen besteht;", "tokens": ["Der", "aus", "ver\u00b7schied\u00b7nen", "Stoff", "und", "C\u00f6r\u00b7per\u00b7chen", "be\u00b7steht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJA", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Der in dem Mittel-Punct im Fl\u00fcssigen vereint,", "tokens": ["Der", "in", "dem", "Mit\u00b7tel\u00b7Punct", "im", "Fl\u00fcs\u00b7si\u00b7gen", "ver\u00b7eint", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "So wol, als unsre Erd\u2019, ein sestes Gautz,", "tokens": ["So", "wol", ",", "als", "uns\u00b7re", "Erd'", ",", "ein", "ses\u00b7tes", "Gautz", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "KOUS", "PPOSAT", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Und diese Dick\u2019, und diese Dichte,", "tokens": ["Und", "die\u00b7se", "Dick'", ",", "und", "die\u00b7se", "Dich\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "$,", "KON", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Ist Ursach, da\u00df der Strahl vom Lichte", "tokens": ["Ist", "Ur\u00b7sach", ",", "da\u00df", "der", "Strahl", "vom", "Lich\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "NN", "$,", "KOUS", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Zur\u00fccke prallt und helle scheint.", "tokens": ["Zu\u00b7r\u00fc\u00b7cke", "prallt", "und", "hel\u00b7le", "scheint", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "KON", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}