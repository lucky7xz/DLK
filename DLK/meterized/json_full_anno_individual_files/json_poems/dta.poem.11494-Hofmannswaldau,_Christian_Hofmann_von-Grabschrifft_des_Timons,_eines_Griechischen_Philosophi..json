{"dta.poem.11494": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Grabschrifft des Timons,  \n eines Griechischen  Philosophi.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Verwundert euch nur nicht, da\u00df Timons leichen-stein", "tokens": ["Ver\u00b7wun\u00b7dert", "euch", "nur", "nicht", ",", "da\u00df", "Ti\u00b7mons", "lei\u00b7chen\u00b7stein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "PTKNEG", "$,", "KOUS", "NE", "NE"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So ferne von Athen in dieser w\u00fcste lieget:", "tokens": ["So", "fer\u00b7ne", "von", "A\u00b7then", "in", "die\u00b7ser", "w\u00fcs\u00b7te", "lie\u00b7get", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "NE", "APPR", "PDAT", "ADJA", "VVFIN", "$."], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Weil ihn die einsamkeit mehr als die stadt vergn\u00fcget,", "tokens": ["Weil", "ihn", "die", "ein\u00b7sam\u00b7keit", "mehr", "als", "die", "stadt", "ver\u00b7gn\u00fc\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PIAT", "KOKOM", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So muste sie sein haus und auch sein kirch-hof seyn.", "tokens": ["So", "mus\u00b7te", "sie", "sein", "haus", "und", "auch", "sein", "kirch\u00b7hof", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPOSAT", "NN", "KON", "ADV", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}