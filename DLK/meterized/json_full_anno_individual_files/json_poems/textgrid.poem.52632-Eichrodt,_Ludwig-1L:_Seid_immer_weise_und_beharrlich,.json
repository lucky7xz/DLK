{"textgrid.poem.52632": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Seid immer weise und beharrlich,", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Seid immer weise und beharrlich,", "tokens": ["Seid", "im\u00b7mer", "wei\u00b7se", "und", "be\u00b7harr\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und von dem Weine lasset nie!", "tokens": ["Und", "von", "dem", "Wei\u00b7ne", "las\u00b7set", "nie", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das nenn ich deutsche Treue, wahrlich,", "tokens": ["Das", "nenn", "ich", "deut\u00b7sche", "Treu\u00b7e", ",", "wahr\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADJA", "NN", "$,", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das nenn ich mir Philosophie!", "tokens": ["Das", "nenn", "ich", "mir", "Phi\u00b7lo\u00b7so\u00b7phie", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PPER", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Am lieben Becher festzuhalten,", "tokens": ["Am", "lie\u00b7ben", "Be\u00b7cher", "fest\u00b7zu\u00b7hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Macht Leib und Seele mir zur Pflicht;", "tokens": ["Macht", "Leib", "und", "See\u00b7le", "mir", "zur", "Pflicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "KON", "NN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie auch mein Schicksal m\u00f6ge walten,", "tokens": ["Wie", "auch", "mein", "Schick\u00b7sal", "m\u00f6\u00b7ge", "wal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPOSAT", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den guten Menschen beugt es nicht.", "tokens": ["Den", "gu\u00b7ten", "Men\u00b7schen", "beugt", "es", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Die Demuth schickt sich f\u00fcr den Christen;", "tokens": ["Die", "De\u00b7muth", "schickt", "sich", "f\u00fcr", "den", "Chris\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Doch richtet mich ein Freund empor,", "tokens": ["Doch", "rich\u00b7tet", "mich", "ein", "Freund", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und hilft mirs Leben weiterfristen,", "tokens": ["Und", "hilft", "mirs", "Le\u00b7ben", "wei\u00b7ter\u00b7fris\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NE", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das ist der g\u00f6ttliche Humor.", "tokens": ["Das", "ist", "der", "g\u00f6tt\u00b7li\u00b7che", "Hu\u00b7mor", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Auf allen meinen Lebensreisen", "tokens": ["Auf", "al\u00b7len", "mei\u00b7nen", "Le\u00b7bens\u00b7rei\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PIAT", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "F\u00fchr ich den Freund mit mir herum \u2013", "tokens": ["F\u00fchr", "ich", "den", "Freund", "mit", "mir", "he\u00b7rum", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "APPR", "PPER", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Den braven Kerl, den Stern der Weisen,", "tokens": ["Den", "bra\u00b7ven", "Kerl", ",", "den", "Stern", "der", "Wei\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das Taschenevangelium.", "tokens": ["Das", "Ta\u00b7sche\u00b7ne\u00b7van\u00b7ge\u00b7li\u00b7um", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.5": {"line.1": {"text": "Seid immer weise und beharrlich,", "tokens": ["Seid", "im\u00b7mer", "wei\u00b7se", "und", "be\u00b7harr\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und von dem Weine lasset nie!", "tokens": ["Und", "von", "dem", "Wei\u00b7ne", "las\u00b7set", "nie", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos":