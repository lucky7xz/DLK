{"dta.poem.766": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "Ii.  Mittag.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Avff Freunde! last vn\u00df zu der Taffel eylen/ ", "tokens": ["Avff", "Freun\u00b7de", "!", "last", "vn\u00df", "zu", "der", "Taf\u00b7fel", "ey\u00b7len", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "VVFIN", "PPER", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In dem die Sonn ins Himmels mittel h\u00e4lt", "tokens": ["In", "dem", "die", "Sonn", "ins", "Him\u00b7mels", "mit\u00b7tel", "h\u00e4lt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ART", "NN", "APPRART", "NN", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Vnd der von Hitz vnd arbeit matten ", "tokens": ["Vnd", "der", "von", "Hitz", "vnd", "ar\u00b7beit", "mat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "APPR", "NN", "KON", "NN", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sucht jhren weg/ vnd vnsern Tag zu theilen.", "tokens": ["Sucht", "jhren", "weg", "/", "vnd", "vn\u00b7sern", "Tag", "zu", "thei\u00b7len", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "PTKVZ", "$(", "KON", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Der Blumen Zier wird von den flammen pfeylen", "tokens": ["Der", "Blu\u00b7men", "Zier", "wird", "von", "den", "flam\u00b7men", "pfey\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VAFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "zu hart versehrt/ das au\u00dfged\u00f6rtte Feldt", "tokens": ["zu", "hart", "ver\u00b7sehrt", "/", "das", "au\u00df\u00b7ge\u00b7d\u00f6rt\u00b7te", "Feldt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKA", "ADJD", "VVPP", "$(", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "W\u00fcndscht nach dem Taw\u2019 der schnitter nach dem zelt", "tokens": ["W\u00fcnd\u00b7scht", "nach", "dem", "Ta\u00b7w'", "der", "schnit\u00b7ter", "nach", "dem", "zelt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "ART", "ADJA", "APPR", "ART", "NN"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Kein Vogel klagt von seinen Liebes seilen.", "tokens": ["Kein", "Vo\u00b7gel", "klagt", "von", "sei\u00b7nen", "Lie\u00b7bes", "sei\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Das Licht regiert/ der schwartze Schatten fleucht", "tokens": ["Das", "Licht", "re\u00b7giert", "/", "der", "schwart\u00b7ze", "Schat\u00b7ten", "fleucht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$(", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "In eine h\u00f6l/ in welche sich verkreucht", "tokens": ["In", "ei\u00b7ne", "h\u00f6l", "/", "in", "wel\u00b7che", "sich", "ver\u00b7kreucht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$(", "APPR", "PRELS", "PRF", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Den Schand vnd furcht sich zu verbergen zwinget.", "tokens": ["Den", "Schand", "vnd", "furcht", "sich", "zu", "ver\u00b7ber\u00b7gen", "zwin\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "PRF", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Man kan dem glantz des tages ja entgehn!", "tokens": ["Man", "kan", "dem", "glantz", "des", "ta\u00b7ges", "ja", "ent\u00b7gehn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "ART", "NN", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Doch nicht dem licht/ da\u00df/ wo wir jmmer stehn/ ", "tokens": ["Doch", "nicht", "dem", "licht", "/", "da\u00df", "/", "wo", "wir", "jm\u00b7mer", "stehn", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ART", "NN", "$(", "KOUS", "$(", "PWAV", "PPER", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Vns siht vnd richt/ vnd hell\u2019 vnd grufft durch dringet.", "tokens": ["Vns", "siht", "vnd", "richt", "/", "vnd", "hell'", "vnd", "grufft", "durch", "drin\u00b7get", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "$(", "KON", "NN", "KON", "VVFIN", "APPR", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}