{"dta.poem.10148": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Seufzer.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20086-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "O Sch\u00f6pfer! da, vor anderm allen,", "tokens": ["O", "Sch\u00f6p\u00b7fer", "!", "da", ",", "vor", "an\u00b7derm", "al\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "ADV", "$,", "APPR", "PIS", "PIS", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Was auf der Welt, die sch\u00f6ne Welt,", "tokens": ["Was", "auf", "der", "Welt", ",", "die", "sch\u00f6\u00b7ne", "Welt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Als Dein Gesch\u00f6pfe, mir gef\u00e4llt;", "tokens": ["Als", "Dein", "Ge\u00b7sch\u00f6p\u00b7fe", ",", "mir", "ge\u00b7f\u00e4llt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "$,", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So la\u00df, nebst meinem schwachen lallen,", "tokens": ["So", "la\u00df", ",", "nebst", "mei\u00b7nem", "schwa\u00b7chen", "lal\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKVZ", "$,", "APPR", "PPOSAT", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die\u00df mein Gefallen Dir gefallen!", "tokens": ["Die\u00df", "mein", "Ge\u00b7fal\u00b7len", "Dir", "ge\u00b7fal\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}