{"dta.poem.16061": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "115.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wo schlie\u00dfet sich der Raum, und stehet still die Zeit?", "tokens": ["Wo", "schlie\u00b7\u00dfet", "sich", "der", "Raum", ",", "und", "ste\u00b7het", "still", "die", "Zeit", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PRF", "ART", "NN", "$,", "KON", "VVFIN", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wo endet hier und dort sich die Unendlichkeit?", "tokens": ["Wo", "en\u00b7det", "hier", "und", "dort", "sich", "die", "Un\u00b7end\u00b7lich\u00b7keit", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ADV", "KON", "ADV", "PRF", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Dort endet sie in Gott, hier endet sie in dir;", "tokens": ["Dort", "en\u00b7det", "sie", "in", "Gott", ",", "hier", "en\u00b7det", "sie", "in", "dir", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "$,", "ADV", "VVFIN", "PPER", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Schein Unendlichkeit steht zwischen dort und hier.", "tokens": ["Der", "Schein", "Un\u00b7end\u00b7lich\u00b7keit", "steht", "zwi\u00b7schen", "dort", "und", "hier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "APPR", "ADV", "KON", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Den Schein, der zwischen dir und Gott steht, r\u00e4ume fort,", "tokens": ["Den", "Schein", ",", "der", "zwi\u00b7schen", "dir", "und", "Gott", "steht", ",", "r\u00e4u\u00b7me", "fort", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "PPER", "KON", "NN", "VVFIN", "$,", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und einf\u00e4llt Raum und Zeit, dein hier ist ewig dort.", "tokens": ["Und", "ein\u00b7f\u00e4llt", "Raum", "und", "Zeit", ",", "dein", "hier", "ist", "e\u00b7wig", "dort", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "KON", "NN", "$,", "PPOSAT", "ADV", "VAFIN", "ADJD", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}