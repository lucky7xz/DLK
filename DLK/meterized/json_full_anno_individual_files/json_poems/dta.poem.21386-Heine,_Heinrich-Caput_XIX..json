{"dta.poem.21386": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Caput XIX.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-30602-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich kam nach Hannover um Mittagzeit,", "tokens": ["Ich", "kam", "nach", "Han\u00b7no\u00b7ver", "um", "Mit\u00b7tag\u00b7zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NE", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und lie\u00df mir die Stiefel putzen.", "tokens": ["Und", "lie\u00df", "mir", "die", "Stie\u00b7fel", "put\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ich ging sogleich die Stadt zu besehn,", "tokens": ["Ich", "ging", "sog\u00b7leich", "die", "Stadt", "zu", "be\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Ich reise gern mit Nutzen.", "tokens": ["Ich", "rei\u00b7se", "gern", "mit", "Nut\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Mein Gott! da sieht es sauber aus!", "tokens": ["Mein", "Gott", "!", "da", "sieht", "es", "sau\u00b7ber", "aus", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "ADV", "VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Koth liegt nicht auf den Gassen.", "tokens": ["Der", "Koth", "liegt", "nicht", "auf", "den", "Gas\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "APPR", "ART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Viel\u2019 Prachtgeb\u00e4ude sah ich dort,", "tokens": ["Viel'", "Pracht\u00b7ge\u00b7b\u00e4u\u00b7de", "sah", "ich", "dort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sehr imponirende Massen.", "tokens": ["Sehr", "im\u00b7po\u00b7ni\u00b7ren\u00b7de", "Mas\u00b7sen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Besonders gefiel mir ein gro\u00dfer Platz,", "tokens": ["Be\u00b7son\u00b7ders", "ge\u00b7fiel", "mir", "ein", "gro\u00b7\u00dfer", "Platz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Umgeben von stattlichen H\u00e4usern;", "tokens": ["Um\u00b7ge\u00b7ben", "von", "statt\u00b7li\u00b7chen", "H\u00e4u\u00b7sern", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Dort wohnt der K\u00f6nig, dort steht sein Palast,", "tokens": ["Dort", "wohnt", "der", "K\u00f6\u00b7nig", ",", "dort", "steht", "sein", "Pa\u00b7last", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Er ist von sch\u00f6nem Aeu\u00dfern.", "tokens": ["Er", "ist", "von", "sch\u00f6\u00b7nem", "A\u00b7e\u00b7u\u00b7\u00dfern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "(n\u00e4mlich der Palast.) Vor dem Portal", "tokens": ["(", "n\u00e4m\u00b7lich", "der", "Pa\u00b7last", ".", ")", "Vor", "dem", "Por\u00b7tal"], "token_info": ["punct", "word", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["$(", "ADV", "ART", "NN", "$.", "$(", "APPR", "ART", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Zu jeder Seite ein Schildhaus.", "tokens": ["Zu", "je\u00b7der", "Sei\u00b7te", "ein", "Schild\u00b7haus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Rothr\u00f6cke mit Flinten halten dort Wacht,", "tokens": ["Ro\u00b7thr\u00f6\u00b7cke", "mit", "Flin\u00b7ten", "hal\u00b7ten", "dort", "Wacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "VVFIN", "ADV", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Sie sehen drohend und wild aus.", "tokens": ["Sie", "se\u00b7hen", "dro\u00b7hend", "und", "wild", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "VVPP", "KON", "ADJD", "PTKVZ", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}}}}