{"dta.poem.5420": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Heilsahme Schw\u00e4che.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Will man von Hochmuth aufgeblasen, von Stoltz", "tokens": ["Will", "man", "von", "Hoch\u00b7muth", "auf\u00b7ge\u00b7bla\u00b7sen", ",", "von", "Stoltz"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VMFIN", "PIS", "APPR", "NN", "VVPP", "$,", "APPR", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "geschwollen, sich erheben;", "tokens": ["ge\u00b7schwol\u00b7len", ",", "sich", "er\u00b7he\u00b7ben", ";"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VVPP", "$,", "PRF", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So dencke man doch, wo und wie uns unser Leben wird", "tokens": ["So", "den\u00b7cke", "man", "doch", ",", "wo", "und", "wie", "uns", "un\u00b7ser", "Le\u00b7ben", "wird"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "ADV", "$,", "PWAV", "KON", "PWAV", "PPER", "PPOSAT", "NN", "VAFIN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "gegeben.", "tokens": ["ge\u00b7ge\u00b7ben", "."], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "Es wird die Menschheit, sonder Zweiffel, sich weniger er-", "tokens": ["Es", "wird", "die", "Menschheit", ",", "son\u00b7der", "Zweif\u00b7fel", ",", "sich", "we\u00b7ni\u00b7ger", "er"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "KON", "NN", "$,", "PRF", "ADV", "TRUNC"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "h\u00f6h'n, als sch\u00e4men,", "tokens": ["h\u00f6h'n", ",", "als", "sch\u00e4\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Erwegt man Art und Ort, wie wir und wo wir unsern", "tokens": ["Er\u00b7wegt", "man", "Art", "und", "Ort", ",", "wie", "wir", "und", "wo", "wir", "un\u00b7sern"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "NN", "KON", "NN", "$,", "PWAV", "PPER", "KON", "PWAV", "PPER", "PPOSAT"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Anfang nehmen;", "tokens": ["An\u00b7fang", "neh\u00b7men", ";"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.9": {"text": "Betrachten wir hiebey des C\u00f6rpers hinf\u00e4llige Beschaffenheit,", "tokens": ["Be\u00b7trach\u00b7ten", "wir", "hie\u00b7bey", "des", "C\u00f6r\u00b7pers", "hin\u00b7f\u00e4l\u00b7li\u00b7ge", "Be\u00b7schaf\u00b7fen\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.10": {"text": "Der Kranckheit Last, des Lebens K\u00fcrtze und fl\u00fcchtige Ver-", "tokens": ["Der", "Kran\u00b7ck\u00b7heit", "Last", ",", "des", "Le\u00b7bens", "K\u00fcrt\u00b7ze", "und", "fl\u00fcch\u00b7ti\u00b7ge", "Ver"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NE", "$,", "ART", "NN", "NN", "KON", "ADJA", "TRUNC"], "meter": "-+--+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.11": {"text": "g\u00e4nglichkeit", "tokens": ["g\u00e4ng\u00b7lich\u00b7keit"], "token_info": ["word"], "pos": ["NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.12": {"text": "Erwegt man mit gesetztem Sinn, ohn Vorurtheil, zugleich", "tokens": ["Er\u00b7wegt", "man", "mit", "ge\u00b7setz\u00b7tem", "Sinn", ",", "ohn", "Vor\u00b7urt\u00b7heil", ",", "zu\u00b7gleich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["VVFIN", "PIS", "APPR", "ADJA", "NN", "$,", "KOUI", "NN", "$,", "ADV"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.13": {"text": "dabey,", "tokens": ["da\u00b7bey", ","], "token_info": ["word", "punct"], "pos": ["PAV", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.14": {"text": "Wie selber unser Geist so schwach, so eitel, und so niedrig", "tokens": ["Wie", "sel\u00b7ber", "un\u00b7ser", "Geist", "so", "schwach", ",", "so", "ei\u00b7tel", ",", "und", "so", "nied\u00b7rig"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "ADV", "PPOSAT", "NN", "ADV", "ADJD", "$,", "ADV", "ADJD", "$,", "KON", "ADV", "ADJD"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.15": {"text": "sey;", "tokens": ["sey", ";"], "token_info": ["word", "punct"], "pos": ["VAFIN", "$."], "meter": "+", "measure": "single.up"}, "line.16": {"text": "Wie oft ihn Leidenschaft bemeistert; wie wir so wenig", "tokens": ["Wie", "oft", "ihn", "Lei\u00b7den\u00b7schaft", "be\u00b7meis\u00b7tert", ";", "wie", "wir", "so", "we\u00b7nig"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "PPER", "NN", "VVPP", "$.", "PWAV", "PPER", "ADV", "PIS"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.17": {"text": "gr\u00fcndlich wissen;", "tokens": ["gr\u00fcnd\u00b7lich", "wis\u00b7sen", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.18": {"text": "Wie oft er sich so weit verirret: wird man denn nicht ge-", "tokens": ["Wie", "oft", "er", "sich", "so", "weit", "ver\u00b7ir\u00b7ret", ":", "wird", "man", "denn", "nicht", "ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "PPER", "PRF", "ADV", "ADJD", "VVFIN", "$.", "VAFIN", "PIS", "ADV", "PTKNEG", "TRUNC"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.19": {"text": "stehen m\u00fcssen,", "tokens": ["ste\u00b7hen", "m\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "punct"], "pos": ["VVINF", "VMINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.20": {"text": "Da\u00df wir uns hier auf dieser Welt, mit allen unsern Vor-", "tokens": ["Da\u00df", "wir", "uns", "hier", "auf", "die\u00b7ser", "Welt", ",", "mit", "al\u00b7len", "un\u00b7sern", "Vor"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "ADV", "APPR", "PDAT", "NN", "$,", "APPR", "PIAT", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.21": {"text": "zugs Gaben,", "tokens": ["zugs", "Ga\u00b7ben", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.22": {"text": "Mit allem eingebildten Witz, nicht sehr zu br\u00fcsten Ursach", "tokens": ["Mit", "al\u00b7lem", "ein\u00b7ge\u00b7bild\u00b7ten", "Witz", ",", "nicht", "sehr", "zu", "br\u00fcs\u00b7ten", "Ur\u00b7sach"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIS", "ADJA", "NN", "$,", "PTKNEG", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+--+", "measure": "iambic.septa.chol"}, "line.23": {"text": "haben.", "tokens": ["ha\u00b7ben", "."], "token_info": ["word", "punct"], "pos": ["VAFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.24": {"text": "Sey aber darum nicht betr\u00fcbet: es flie\u00dft aus der Erkenntni\u00df", "tokens": ["Sey", "a\u00b7ber", "da\u00b7rum", "nicht", "be\u00b7tr\u00fc\u00b7bet", ":", "es", "flie\u00dft", "aus", "der", "Er\u00b7kennt\u00b7ni\u00df"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "PAV", "PTKNEG", "VVFIN", "$.", "PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+--+---+-", "measure": "iambic.hexa.relaxed"}, "line.25": {"text": "mehr,", "tokens": ["mehr", ","], "token_info": ["word", "punct"], "pos": ["ADV", "$,"], "meter": "-", "measure": "single.down"}, "line.26": {"text": "Als was man anfangs glauben solte. Es flie\u00dft daraus, zu", "tokens": ["Als", "was", "man", "an\u00b7fangs", "glau\u00b7ben", "sol\u00b7te", ".", "Es", "flie\u00dft", "da\u00b7raus", ",", "zu"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["KOUS", "PWS", "PIS", "ADV", "VVINF", "VMFIN", "$.", "PPER", "VVFIN", "PAV", "$,", "APPR"], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.27": {"text": "Gottes Ehr,", "tokens": ["Got\u00b7tes", "Ehr", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.28": {"text": "Der N\u00e4chsten-Liebe Quell, die Demuth, im Leben; und", "tokens": ["Der", "N\u00e4chs\u00b7ten\u00b7Lie\u00b7be", "Quell", ",", "die", "De\u00b7muth", ",", "im", "Le\u00b7ben", ";", "und"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "$,", "APPRART", "NN", "$.", "KON"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.29": {"text": "wann wir erblassen", "tokens": ["wann", "wir", "er\u00b7blas\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "PPER", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.30": {"text": "Der Glaub\u2019, in welchem wir gedultig auf seine Lieb\u2019 uns", "tokens": ["Der", "Glaub'", ",", "in", "wel\u00b7chem", "wir", "ge\u00b7dul\u00b7tig", "auf", "sei\u00b7ne", "Lieb'", "uns"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "APPR", "PRELS", "PPER", "ADJD", "APPR", "PPOSAT", "NN", "PPER"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.31": {"text": "blos verlassen.", "tokens": ["blos", "ver\u00b7las\u00b7sen", "."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}}}}}