{"textgrid.poem.38825": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es steht die holde Jungfrau im Betrachten,", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es steht die holde Jungfrau im Betrachten,", "tokens": ["Es", "steht", "die", "hol\u00b7de", "Jung\u00b7frau", "im", "Be\u00b7trach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie sich Ger\u00e4usch und wilde Freude mehret,", "tokens": ["Wie", "sich", "Ge\u00b7r\u00e4usch", "und", "wil\u00b7de", "Freu\u00b7de", "meh\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "NN", "KON", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ihr Herz, Gem\u00fcth ist still in sich gekehret,", "tokens": ["Ihr", "Herz", ",", "Ge\u00b7m\u00fcth", "ist", "still", "in", "sich", "ge\u00b7keh\u00b7ret", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "NN", "VAFIN", "ADJD", "APPR", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sie kann auf Freunde, Br\u00e4utigam nicht achten.", "tokens": ["Sie", "kann", "auf", "Freun\u00b7de", ",", "Br\u00e4u\u00b7ti\u00b7gam", "nicht", "ach\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "NN", "$,", "NE", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Und wie die G\u00e4ste drinnen tobend lachten,", "tokens": ["Und", "wie