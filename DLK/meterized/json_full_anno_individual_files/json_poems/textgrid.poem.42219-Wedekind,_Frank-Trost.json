{"textgrid.poem.42219": {"metadata": {"author": {"name": "Wedekind, Frank", "birth": "N.A.", "death": "N.A."}, "title": "Trost", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Tod kommt bald und sicher,", "tokens": ["Der", "Tod", "kommt", "bald", "und", "si\u00b7cher", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "KON", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "H\u00e4lt stets sich in der N\u00e4h.", "tokens": ["H\u00e4lt", "stets", "sich", "in", "der", "N\u00e4h", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PRF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er ist ein f\u00fcrchterlicher", "tokens": ["Er", "ist", "ein", "f\u00fcrch\u00b7ter\u00b7li\u00b7cher"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Tr\u00f6ster im Erdenweh.", "tokens": ["Tr\u00f6s\u00b7ter", "im", "Er\u00b7den\u00b7weh", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.2": {"line.1": {"text": "Ich hasse ihn nicht aus Liebe,", "tokens": ["Ich", "has\u00b7se", "ihn", "nicht", "aus", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "APPR", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Ich liebe ihn hei\u00df aus Ha\u00df.", "tokens": ["Ich", "lie\u00b7be", "ihn", "hei\u00df", "aus", "Ha\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "APPR", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wenn man unsterblich bliebe,", "tokens": ["Wenn", "man", "uns\u00b7terb\u00b7lich", "blie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wie grauenvoll w\u00e4re das!", "tokens": ["Wie", "grau\u00b7en\u00b7voll", "w\u00e4\u00b7re", "das", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PDS", "$."], "meter": "-+--+--", "measure": "iambic.di.relaxed"}}, "stanza.3": {"line.1": {"text": "Des Fressens und Weitergebens", "tokens": ["Des", "Fres\u00b7sens", "und", "Wei\u00b7ter\u00b7ge\u00b7bens"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Urewige Wiederkehr", "tokens": ["U\u00b7re\u00b7wi\u00b7ge", "Wie\u00b7der\u00b7kehr"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Als h\u00f6chsten Ertrag des Lebens", "tokens": ["Als", "h\u00f6chs\u00b7ten", "Er\u00b7trag", "des", "Le\u00b7bens"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJA", "NN", "ART", "NN"], "meter": "-+----+-", "measure": "dactylic.init"}, "line.4": {"text": "Ertrag ich nicht l\u00e4nger mehr.", "tokens": ["Er\u00b7trag", "ich", "nicht", "l\u00e4n\u00b7ger", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKNEG", "ADJD", "ADV", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Der Tod kommt bald und sicher,", "tokens": ["Der", "Tod", "kommt", "bald", "und", "si\u00b7cher", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "KON", "ADJD", "$,"], "m