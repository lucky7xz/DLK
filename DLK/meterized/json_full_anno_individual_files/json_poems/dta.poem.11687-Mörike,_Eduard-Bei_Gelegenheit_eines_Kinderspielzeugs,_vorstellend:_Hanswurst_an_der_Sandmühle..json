{"dta.poem.11687": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Bei Gelegenheit eines Kinderspielzeugs,  \n vorstellend:  \n  Hanswurst an der Sandm\u00fchle.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905193969", "language": ["de:0.99"], "booktitle": "M\u00f6rike, Eduard: Gedichte. Stuttgart, 1838."}, "poem": {"stanza.1": {"line.1": {"text": "Gleich bei\u00dfen und kratzen! Gott verdamm's!", "tokens": ["Gleich", "bei\u00b7\u00dfen", "und", "krat\u00b7zen", "!", "Gott", "ver\u00b7dam\u00b7m's", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVINF", "KON", "VVINF", "$.", "NN", "NE", "$."], "meter": "-+--+-+---", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Hab' doch tausend Farben an Hosen und Wamms,", "tokens": ["Hab'", "doch", "tau\u00b7send", "Far\u00b7ben", "an", "Ho\u00b7sen", "und", "Wamms", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "CARD", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Zum Zeichen, da\u00df ich's mit keiner halte!", "tokens": ["Zum", "Zei\u00b7chen", ",", "da\u00df", "ich's", "mit", "kei\u00b7ner", "hal\u00b7te", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "KOUS", "PIS", "APPR", "PIS", "VVFIN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wenn ich meinen Purzelbaum machen kann,", "tokens": ["Wenn", "ich", "mei\u00b7nen", "Pur\u00b7zel\u00b7baum", "ma\u00b7chen", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVINF", "VMFIN", "$,"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Was ficht die Politik mich an?", "tokens": ["Was", "ficht", "die", "Po\u00b7li\u00b7tik", "mich", "an", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}