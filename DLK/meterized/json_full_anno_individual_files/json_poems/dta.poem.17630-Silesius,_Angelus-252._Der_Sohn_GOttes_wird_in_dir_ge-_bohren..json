{"dta.poem.17630": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "252.  Der Sohn GOttes wird in dir ge-  \n bohren.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Mensch schikstu dich darzu/ so zeugt Gott seinen Sohn/", "tokens": ["Mensch", "schiks\u00b7tu", "dich", "dar\u00b7zu", "/", "so", "zeugt", "Gott", "sei\u00b7nen", "Sohn", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PRF", "PAV", "$(", "ADV", "VVFIN", "NN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "All\u2019 Augenblik in dir/ gleich wie in seinem Thron.", "tokens": ["All'", "Au\u00b7gen\u00b7blik", "in", "dir", "/", "gleich", "wie", "in", "sei\u00b7nem", "Thron", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "PPER", "$(", "ADV", "KOKOM", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}