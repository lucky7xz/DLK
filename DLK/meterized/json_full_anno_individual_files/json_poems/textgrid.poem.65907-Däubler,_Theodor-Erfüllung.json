{"textgrid.poem.65907": {"metadata": {"author": {"name": "D\u00e4ubler, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Erf\u00fcllung", "genre": "verse", "period": "N.A.", "pub_year": 1905, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Pers\u00e9phone erscheint in Silberschleiern,", "tokens": ["Per\u00b7s\u00e9\u00b7pho\u00b7ne", "er\u00b7scheint", "in", "Sil\u00b7ber\u00b7schlei\u00b7ern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Um sanfte Mutter schlingt sie zart den Arm,", "tokens": ["Um", "sanf\u00b7te", "Mut\u00b7ter", "schlingt", "sie", "zart", "den", "Arm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADJA", "NN", "VVFIN", "PPER", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Entd\u00fcstert das geweihte Herz von Harm", "tokens": ["Ent\u00b7d\u00fcs\u00b7tert", "das", "ge\u00b7weih\u00b7te", "Herz", "von", "Harm"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und sorgt sich, eigne Wiederkunft zu feiern.", "tokens": ["Und", "sorgt", "sich", ",", "eig\u00b7ne", "Wie\u00b7der\u00b7kunft", "zu", "fei\u00b7ern", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "$,", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Die Birnb\u00e4ume begl\u00fccken uns mit Freiern,", "tokens": ["Die", "Birn\u00b7b\u00e4u\u00b7me", "be\u00b7gl\u00fc\u00b7cken", "uns", "mit", "Frei\u00b7ern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "-++--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Das Bauernhaus bejauchzt ein Schwalbenschwarm:", "tokens": ["Das", "Bau\u00b7ern\u00b7haus", "be\u00b7jauchzt", "ein", "Schwal\u00b7ben\u00b7schwarm", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Das Fr\u00fchjahr wurde wahr \u2013 sein Herhauch warm;", "tokens": ["Das", "Fr\u00fch\u00b7jahr", "wur\u00b7de", "wahr", "\u2013", "sein", "Her\u00b7hauch", "warm", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$(", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Geliebte Meisen picken sich aus Eiern.", "tokens": ["Ge\u00b7lieb\u00b7te", "Mei\u00b7sen", "pi\u00b7cken", "sich", "aus", "Ei\u00b7ern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "PRF", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Gem\u00fct, bald la\u00df bei \u00d6lb\u00e4umen vom Bangen,", "tokens": ["Ge\u00b7m\u00fct", ",", "bald", "la\u00df", "bei", "\u00d6l\u00b7b\u00e4u\u00b7men", "vom", "Ban\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "VVIMP", "APPR", "NN", "APPRART", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Zu ihren Silberzweigen blick um Rast!", "tokens": ["Zu", "ih\u00b7ren", "Sil\u00b7ber\u00b7zwei\u00b7gen", "blick", "um", "Rast", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "me