{"dta.poem.2111": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Schatten.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Um das, was an sich selbst schon sch\u00f6n,", "tokens": ["Um", "das", ",", "was", "an", "sich", "selbst", "schon", "sch\u00f6n", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PDS", "$,", "PRELS", "APPR", "PRF", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Doch desto mehr noch zu erh\u00f6hn,", "tokens": ["Doch", "des\u00b7to", "mehr", "noch", "zu", "er\u00b7h\u00f6hn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So wachsen itzt auf B\u00e4um- und Matten", "tokens": ["So", "wach\u00b7sen", "itzt", "auf", "B\u00e4um", "und", "Mat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "APPR", "TRUNC", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mit jungen Bl\u00e4ttern, junge Schatten,", "tokens": ["Mit", "jun\u00b7gen", "Bl\u00e4t\u00b7tern", ",", "jun\u00b7ge", "Schat\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die, um die helle Zierlichkeit", "tokens": ["Die", ",", "um", "die", "hel\u00b7le", "Zier\u00b7lich\u00b7keit"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ART", "$,", "KOUI", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Des sch\u00f6nen Urbilds zu verbessern,", "tokens": ["Des", "sch\u00f6\u00b7nen", "Ur\u00b7bilds", "zu", "ver\u00b7bes\u00b7sern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Durch ihre nahe Dunkelheit,", "tokens": ["Durch", "ih\u00b7re", "na\u00b7he", "Dun\u00b7kel\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Sich jeden Augenblick vergr\u00f6\u00dfern,", "tokens": ["Sich", "je\u00b7den", "Au\u00b7gen\u00b7blick", "ver\u00b7gr\u00f6\u00b7\u00dfern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Um nicht nur unsre Lust zu mehren", "tokens": ["Um", "nicht", "nur", "uns\u00b7re", "Lust", "zu", "meh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "PTKNEG", "ADV", "PPOSAT", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "In der, durch ihre holde Nacht,", "tokens": ["In", "der", ",", "durch", "ih\u00b7re", "hol\u00b7de", "Nacht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$,", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Annoch erhobnen Farben Pracht;", "tokens": ["An\u00b7noch", "er\u00b7hob\u00b7nen", "Far\u00b7ben", "Pracht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Nein, auch noch K\u00fchlung zu gewehren;", "tokens": ["Nein", ",", "auch", "noch", "K\u00fch\u00b7lung", "zu", "ge\u00b7weh\u00b7ren", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADV", "ADV", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Und vor dem gar zu schw\u00fclen Blitzen", "tokens": ["Und", "vor", "dem", "gar", "zu", "schw\u00fc\u00b7len", "Blit\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Des strengen Sonnenstrals zu sch\u00fctzen.", "tokens": ["Des", "stren\u00b7gen", "Son\u00b7nen\u00b7strals", "zu", "sch\u00fct\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Wodurch man in der That erf\u00e4hrt,", "tokens": ["Wo\u00b7durch", "man", "in", "der", "That", "er\u00b7f\u00e4hrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Da\u00df, wenn die Hitze sich vermehrt,", "tokens": ["Da\u00df", ",", "wenn", "die", "Hit\u00b7ze", "sich", "ver\u00b7mehrt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "KOUS", "ART", "NN", "PRF", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Sich auch die Mittel, die dagegen,", "tokens": ["Sich", "auch", "die", "Mit\u00b7tel", ",", "die", "da\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PRF", "ADV", "ART", "NN", "$,", "PRELS", "PAV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "F\u00fcr uns zugleich zu mehren pflegen.", "tokens": ["F\u00fcr", "uns", "zu\u00b7gleich", "zu", "meh\u00b7ren", "pfle\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "PTKZU", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Wird dann hiedurch nicht klar versp\u00fcrt,", "tokens": ["Wird", "dann", "hie\u00b7durch", "nicht", "klar", "ver\u00b7sp\u00fcrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PAV", "PTKNEG", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Da so viel Guts f\u00fcr uns erlesen,", "tokens": ["Da", "so", "viel", "Guts", "f\u00fcr", "uns", "er\u00b7le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PIAT", "NN", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Es werde, durch ein weises Wesen,", "tokens": ["Es", "wer\u00b7de", ",", "durch", "ein", "wei\u00b7ses", "We\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Die Welt regieret und geziert;", "tokens": ["Die", "Welt", "re\u00b7gie\u00b7ret", "und", "ge\u00b7ziert", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Und da\u00df f\u00fcr so viel edle Gaben,", "tokens": ["Und", "da\u00df", "f\u00fcr", "so", "viel", "ed\u00b7le", "Ga\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "APPR", "ADV", "PIAT", "ADJA", "NN", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.24": {"text": "Woran wir, blo\u00df durch ihn, uns laben,", "tokens": ["Wo\u00b7ran", "wir", ",", "blo\u00df", "durch", "ihn", ",", "uns", "la\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PPER", "$,", "ADV", "APPR", "PPER", "$,", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Jhm Ehre, Preis und Dank geb\u00fchrt?", "tokens": ["Jhm", "Eh\u00b7re", ",", "Preis", "und", "Dank", "ge\u00b7b\u00fchrt", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}