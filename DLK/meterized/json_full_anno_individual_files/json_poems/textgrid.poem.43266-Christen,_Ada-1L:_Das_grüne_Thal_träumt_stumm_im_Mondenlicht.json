{"textgrid.poem.43266": {"metadata": {"author": {"name": "Christen, Ada", "birth": "N.A.", "death": "N.A."}, "title": "1L: Das gr\u00fcne Thal tr\u00e4umt stumm im Mondenlicht", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das gr\u00fcne Thal tr\u00e4umt stumm im Mondenlicht", "tokens": ["Das", "gr\u00fc\u00b7ne", "Thal", "tr\u00e4umt", "stumm", "im", "Mon\u00b7den\u00b7licht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und feierlich die B\u00e4ume niederschauen;", "tokens": ["Und", "fei\u00b7er\u00b7lich", "die", "B\u00e4u\u00b7me", "nie\u00b7der\u00b7schau\u00b7en", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Nachtwind selbst regt seine Fl\u00fcgel nicht,", "tokens": ["Der", "Nacht\u00b7wind", "selbst", "regt", "sei\u00b7ne", "Fl\u00fc\u00b7gel", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "PPOSAT", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Lautlos im Wiesengrund die Nebel brauen.", "tokens": ["Laut\u00b7los", "im", "Wie\u00b7sen\u00b7grund", "die", "Ne\u00b7bel", "brau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "ART", "NN", "VVINF", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}, "stanza.2": {"line.1": {"text": "So schlafumfangen liegt jedwedes Haus,", "tokens": ["So", "schlaf\u00b7um\u00b7fan\u00b7gen", "liegt", "jed\u00b7we\u00b7des", "Haus", ","], "token_info": ["word", "word", "word", "