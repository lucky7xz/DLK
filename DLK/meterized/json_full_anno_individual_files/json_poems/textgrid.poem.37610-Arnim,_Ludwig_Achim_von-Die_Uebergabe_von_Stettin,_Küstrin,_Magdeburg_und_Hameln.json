{"textgrid.poem.37610": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Die Uebergabe von Stettin, K\u00fcstrin, Magdeburg und Hameln", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Trompeter, komm' uns nicht zu nah!", "tokens": ["Trom\u00b7pe\u00b7ter", ",", "komm'", "uns", "nicht", "zu", "nah", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "PTKNEG", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die W\u00e4lle m\u00f6chten fallen,", "tokens": ["Die", "W\u00e4l\u00b7le", "m\u00f6ch\u00b7ten", "fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Was auch bei Jericho geschah,", "tokens": ["Was", "auch", "bei", "Je\u00b7ri\u00b7cho", "ge\u00b7schah", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "APPR", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Erst glaubt man's nicht, dann ist es da,", "tokens": ["Erst", "glaubt", "man's", "nicht", ",", "dann", "ist", "es", "da", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PTKNEG", "$,", "ADV", "VAFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wenn die Trompeten schallen.", "tokens": ["Wenn", "die", "Trom\u00b7pe\u00b7ten", "schal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVINF", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.2": {"line.1": {"text": "Du Gouverneur der Stadt Stettin", "tokens": ["Du", "Gou\u00b7ver\u00b7neur", "der", "Stadt", "Stet\u00b7tin"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "NN", "ART", "NN", "NE"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Machst gar sehr schwache Mienen,", "tokens": ["Machst", "gar", "sehr", "schwa\u00b7che", "Mie\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "ADJA", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Du bist sonst grob, sei einmal k\u00fchn,", "tokens": ["Du", "bist", "sonst", "grob", ",", "sei", "ein\u00b7mal", "k\u00fchn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$,", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn sie an unsr W\u00e4lle ziehn,", "tokens": ["Wenn", "sie", "an", "unsr", "W\u00e4l\u00b7le", "ziehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Mit Kugeln sie bedienen.", "tokens": ["Mit", "Ku\u00b7geln", "sie", "be\u00b7die\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Er f\u00fcllet mit gewalt'ger Eil'", "tokens": ["Er", "f\u00fcl\u00b7let", "mit", "ge\u00b7walt'\u00b7ger", "Eil'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die eingest\u00fcrzten W\u00e4lle,", "tokens": ["Die", "ein\u00b7ge\u00b7st\u00fcrz\u00b7ten", "W\u00e4l\u00b7le", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Geschieht das nur zur Kurzeweil,", "tokens": ["Ge\u00b7schieht", "das", "nur", "zur", "Kur\u00b7ze\u00b7weil", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDS", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da tausend Mann mit Axt und Beil", "tokens": ["Da", "tau\u00b7send", "Mann", "mit", "Axt", "und", "Beil"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "CARD", "NN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die sch\u00f6nen B\u00e4ume f\u00e4llen.", "tokens": ["Die", "sch\u00f6\u00b7nen", "B\u00e4u\u00b7me", "f\u00e4l\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Sie fordern auf, und meinen nicht,", "tokens": ["Sie", "for\u00b7dern", "auf", ",", "und", "mei\u00b7nen", "nicht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "KON", "VVFIN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df du dich w\u00fcrdest geben;", "tokens": ["Da\u00df", "du", "dich", "w\u00fcr\u00b7dest", "ge\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Doch du vertr\u00e4gst kein solch Gericht,", "tokens": ["Doch", "du", "ver\u00b7tr\u00e4gst", "kein", "solch", "Ge\u00b7richt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PIAT", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Tag sich auch schon wieder bricht,", "tokens": ["Der", "Tag", "sich", "auch", "schon", "wie\u00b7der", "bricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "ADV", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Du mu\u00dft dich \u00fcbergeben.", "tokens": ["Du", "mu\u00dft", "dich", "\u00fc\u00b7ber\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Wie das erh\u00f6rt ein altes Weib,", "tokens": ["Wie", "das", "er\u00b7h\u00f6rt", "ein", "al\u00b7tes", "Weib", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PDS", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie ging mit dem Pantoffel", "tokens": ["Sie", "ging", "mit", "dem", "Pan\u00b7tof\u00b7fel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Dem Kommandanten sehr zu Leib,", "tokens": ["Dem", "Kom\u00b7man\u00b7dan\u00b7ten", "sehr", "zu", "Leib", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Es war ihr gar kein Zeitvertreib,", "tokens": ["Es", "war", "ihr", "gar", "kein", "Zeit\u00b7ver\u00b7treib", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Er schrie, der arme Teufel.", "tokens": ["Er", "schrie", ",", "der", "ar\u00b7me", "Teu\u00b7fel", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Die Garnison hat kurz und klein", "tokens": ["Die", "Gar\u00b7ni\u00b7son", "hat", "kurz", "und", "klein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "All ihr Gewehr' zerbrochen,", "tokens": ["All", "ihr", "Ge\u00b7wehr'", "zer\u00b7bro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Zerrissen mu\u00df der Rock auch sein,", "tokens": ["Zer\u00b7ris\u00b7sen", "mu\u00df", "der", "Rock", "auch", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "ART", "NN", "ADV", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wir alle schw\u00f6ren Stein und Bein.", "tokens": ["Wir", "al\u00b7le", "schw\u00f6\u00b7ren", "Stein", "und", "Bein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da\u00df sie sich gern gerochen.", "tokens": ["Da\u00df", "sie", "sich", "gern", "ge\u00b7ro\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Ihr Gouverneurs, denkt diesem nach,", "tokens": ["Ihr", "Gou\u00b7ver\u00b7neurs", ",", "denkt", "die\u00b7sem", "nach", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "PDAT", "APPR", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nichts m\u00fc\u00dft ihr halb beginnen,", "tokens": ["Nichts", "m\u00fc\u00dft", "ihr", "halb", "be\u00b7gin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und brennt das Schnupftuch auch zu Asch'", "tokens": ["Und", "brennt", "das", "Schnupf\u00b7tuch", "auch", "zu", "Asch'"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "ADV", "APPR", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In eurer weiten alten Tasch',", "tokens": ["In", "eu\u00b7rer", "wei\u00b7ten", "al\u00b7ten", "Ta\u00b7sch'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Ihr m\u00fc\u00dft euch nicht besinnen.", "tokens": ["Ihr", "m\u00fc\u00dft", "euch", "nicht", "be\u00b7sin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "F\u00fcr Zaubereien h\u00fctet euch,", "tokens": ["F\u00fcr", "Zau\u00b7be\u00b7rei\u00b7en", "h\u00fc\u00b7tet", "euch", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "K\u00fcstrin ist so gefallen,", "tokens": ["K\u00fcst\u00b7rin", "ist", "so", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "VVPP", "$,"], "meter": "---+-+-", "measure": "unknown.measure.di"}, "line.3": {"text": "Der Kommandante kam sogleich", "tokens": ["Der", "Kom\u00b7man\u00b7dan\u00b7te", "kam", "sog\u00b7leich"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit den Franzosen in Vergleich,", "tokens": ["Mit", "den", "Fran\u00b7zo\u00b7sen", "in", "Ver\u00b7gleich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wie er h\u00f6rt Geld erschallen.", "tokens": ["Wie", "er", "h\u00f6rt", "Geld", "er\u00b7schal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "NN", "VVINF", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.9": {"line.1": {"text": "In Magdeburg, da baten drum", "tokens": ["In", "Mag\u00b7de\u00b7burg", ",", "da", "ba\u00b7ten", "drum"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NE", "$,", "ADV", "VVFIN", "PAV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Elftausend der Jungfrauen,", "tokens": ["Elf\u00b7tau\u00b7send", "der", "Jung\u00b7frau\u00b7en", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sind so viel drin, so sei es drum!", "tokens": ["Sind", "so", "viel", "drin", ",", "so", "sei", "es", "drum", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "ADV", "$,", "ADV", "VAFIN", "PPER", "PAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Kommandante war so dumm,", "tokens": ["Der", "Kom\u00b7man\u00b7dan\u00b7te", "war", "so", "dumm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Kaum will der Feind ihm trauen.", "tokens": ["Kaum", "will", "der", "Feind", "ihm", "trau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "In Hameln pfiff den Kommandant", "tokens": ["In", "Ha\u00b7meln", "pfiff", "den", "Kom\u00b7man\u00b7dant"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der alte Rattenf\u00e4nger,", "tokens": ["Der", "al\u00b7te", "Rat\u00b7ten\u00b7f\u00e4n\u00b7ger", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Es kr\u00e4ht der Hahn zu seiner Schand',", "tokens": ["Es", "kr\u00e4ht", "der", "Hahn", "zu", "sei\u00b7ner", "Schand'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Er hat verrathen Leut' und Land", "tokens": ["Er", "hat", "ver\u00b7ra\u00b7then", "Leut'", "und", "Land"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "VVPP", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und hielt sich auch nicht l\u00e4nger.", "tokens": ["Und", "hielt", "sich", "auch", "nicht", "l\u00e4n\u00b7ger", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADV", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Trompeter, komm' uns nicht zu nah!", "tokens": ["Trom\u00b7pe\u00b7ter", ",", "komm'", "uns", "nicht", "zu", "nah", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "w