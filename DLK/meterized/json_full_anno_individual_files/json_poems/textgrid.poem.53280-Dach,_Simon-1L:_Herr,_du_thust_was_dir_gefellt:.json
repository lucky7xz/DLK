{"textgrid.poem.53280": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "1L: Herr, du thust was dir gefellt:", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Herr, du thust was dir gefellt:", "tokens": ["Herr", ",", "du", "thust", "was", "dir", "ge\u00b7fellt", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VVFIN", "PWS", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Dein sind wir, vnd alles in der Welt.", "tokens": ["Dein", "sind", "wir", ",", "vnd", "al\u00b7les", "in", "der", "Welt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VAFIN", "PPER", "$,", "KON", "PIS", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Du l\u00e4ssest vns gebohren werden:", "tokens": ["Du", "l\u00e4s\u00b7sest", "vns", "ge\u00b7boh\u00b7ren", "wer\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Vnsre Tage stehn bey dir,", "tokens": ["Vns\u00b7re", "Ta\u00b7ge", "stehn", "bey", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Vnsern Geist raffst du von hier,", "tokens": ["Vn\u00b7sern", "Geist", "raffst", "du", "von", "hier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PPER", "APPR", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Vnd machest vns zu Staub vnd Erden.", "tokens": ["Vnd", "ma\u00b7chest", "vns", "zu", "Staub", "vnd", "Er\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Hieran hastu deine Lust,", "tokens": ["Hie\u00b7ran", "has\u00b7tu", "dei\u00b7ne", "Lust", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Weisst auch wol, warumb du solches thust,", "tokens": ["Weisst", "auch", "wol", ",", "wa\u00b7rumb", "du", "sol\u00b7ches", "thust", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "$,", "PWAV", "PPER", "PIS", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Kein Mensch thar dich zu rede stellen:", "tokens": ["Kein", "Mensch", "thar", "dich", "zu", "re\u00b7de", "stel\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "PTKZU", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Keinen siehstu darumb an,", "tokens": ["Kei\u00b7nen", "sieh\u00b7stu", "da\u00b7rumb", "an", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PAV", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Keiner meistert dich vnd kan", "tokens": ["Kei\u00b7ner", "meis\u00b7tert", "dich", "vnd", "kan"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "PPER", "KON", "VMFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Von dir ein b\u00f6ses Vrtheil fellen.", "tokens": ["Von", "dir", "ein", "b\u00f6\u00b7ses", "Vrtheil", "fel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}}, "stanza.3": {"line.1": {"text": "Ach wie vnbegreifflich ist,", "tokens": ["Ach", "wie", "vn\u00b7be\u00b7greif\u00b7flich", "ist", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ADJD", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Herr, dein thun? wird aller Menschen List", "tokens": ["Herr", ",", "dein", "thun", "?", "wird", "al\u00b7ler", "Men\u00b7schen", "List"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "PPOSAT", "VVINF", "$.", "VAFIN", "PIAT", "NN", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Auch deiner wege Spur erfahren?", "tokens": ["Auch", "dei\u00b7ner", "we\u00b7ge", "Spur", "er\u00b7fah\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Niemand geht mit dir zu Raht,", "tokens": ["Nie\u00b7mand", "geht", "mit", "dir", "zu", "Raht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "PPER", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Was dein Sinn beschlossen hat", "tokens": ["Was", "dein", "Sinn", "be\u00b7schlos\u00b7sen", "hat"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "PPOSAT", "NN", "VVPP", "VAFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Darffst du, Gott, keinem offenbahren.", "tokens": ["Darffst", "du", ",", "Gott", ",", "kei\u00b7nem", "of\u00b7fen\u00b7bah\u00b7ren", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "$,", "NN", "$,", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Du verstossest H\u00f6llen-ab,", "tokens": ["Du", "ver\u00b7stos\u00b7sest", "H\u00f6l\u00b7len\u00b7ab", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "\u00d6ffnest auch das finstre Todten-Grab:", "tokens": ["\u00d6ff\u00b7nest", "auch", "das", "finst\u00b7re", "Tod\u00b7ten\u00b7Grab", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Du z\u00fcrnest vnd erbarmst dich wieder,", "tokens": ["Du", "z\u00fcr\u00b7nest", "vnd", "er\u00b7barmst", "dich", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "H\u00fcllst dich wunderseltzam ein", "tokens": ["H\u00fcllst", "dich", "wun\u00b7der\u00b7selt\u00b7zam", "ein"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "VVFIN", "ART"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "In Gest\u00fcrm, in Sonnen-Schein,", "tokens": ["In", "Ge\u00b7st\u00fcrm", ",", "in", "Son\u00b7nen\u00b7Schein", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Erweckst dort Klag- hie Frewden-Lieder.", "tokens": ["Er\u00b7weckst", "dort", "Klag", "hie", "Fre\u00b7wden\u00b7Lie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "TRUNC", "ADV", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Die\u00df ist vns ein tieffes Meer,", "tokens": ["Die\u00df", "ist", "vns", "ein", "tief\u00b7fes", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+---+-+", "measure": "dactylic.init"}, "line.2": {"text": "F\u00e4llt vns au\u00dfzugr\u00fcnden gar zu schwer.", "tokens": ["F\u00e4llt", "vns", "au\u00df\u00b7zu\u00b7gr\u00fcn\u00b7den", "gar", "zu", "schwer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVFIN", "ADV", "PTKA", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Wir wollen deinen Sinn vmbschr\u00e4ncken,", "tokens": ["Wir", "wol\u00b7len", "dei\u00b7nen", "Sinn", "vmb\u00b7schr\u00e4n\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Meinen, also soll' es gehn,", "tokens": ["Mei\u00b7nen", ",", "al\u00b7so", "soll'", "es", "gehn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADV", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wie wir Thoren es verstehn,", "tokens": ["Wie", "wir", "Tho\u00b7ren", "es", "ver\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Du m\u00fcssest thun, wie wir gedencken.", "tokens": ["Du", "m\u00fcs\u00b7sest", "thun", ",", "wie", "wir", "ge\u00b7den\u00b7cken", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "$,", "PWAV", "PPER", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Herr, nimm diesen Irrthum hin!", "tokens": ["Herr", ",", "nimm", "die\u00b7sen", "Irr\u00b7thum", "hin", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVIMP", "PDAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Gieb, da\u00df dir sich vnser Will vnd Sinn", "tokens": ["Gieb", ",", "da\u00df", "dir", "sich", "vn\u00b7ser", "Will", "vnd", "Sinn"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "$,", "KOUS", "PPER", "PRF", "APPR", "NE", "KON", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Stets in Gedult m\u00f6g' vntergeben,", "tokens": ["Stets", "in", "Ge\u00b7dult", "m\u00f6g'", "vn\u00b7ter\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "La\u00df vns deinen Raht allein,", "tokens": ["La\u00df", "vns", "dei\u00b7nen", "Raht", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPOSAT", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Weiser Gott, gef\u00e4llig seyn,", "tokens": ["Wei\u00b7ser", "Gott", ",", "ge\u00b7f\u00e4l\u00b7lig", "seyn", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJD", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Wir m\u00f6gen sterben oder leben.", "tokens": ["Wir", "m\u00f6\u00b7gen", "ster\u00b7ben", "o\u00b7der", "le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "KON", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Herr, du thust was dir gefellt:", "tokens": ["Herr", ",", "du", "th