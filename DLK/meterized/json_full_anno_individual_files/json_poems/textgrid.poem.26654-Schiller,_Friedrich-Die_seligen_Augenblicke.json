{"textgrid.poem.26654": {"metadata": {"author": {"name": "Schiller, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Die seligen Augenblicke", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Laura, \u00fcber diese Welt zu fl\u00fcchten,", "tokens": ["Lau\u00b7ra", ",", "\u00fc\u00b7ber", "die\u00b7se", "Welt", "zu", "fl\u00fcch\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "APPR", "PDAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "W\u00e4hn ich \u2013 mich in Himmelmaienglanz zu lichten,", "tokens": ["W\u00e4hn", "ich", "\u2013", "mich", "in", "Him\u00b7mel\u00b7mai\u00b7en\u00b7glanz", "zu", "lich\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$(", "PPER", "APPR", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "Wenn dein Blick in meine Blicke flimmt,", "tokens": ["Wenn", "dein", "Blick", "in", "mei\u00b7ne", "Bli\u00b7cke", "flimmt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "\u00c4therl\u00fcfte tr\u00e4um ich einzusaugen,", "tokens": ["\u00c4\u00b7ther\u00b7l\u00fcf\u00b7te", "tr\u00e4um", "ich", "ein\u00b7zu\u00b7sau\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Wenn mein Bild in deiner sanften Augen", "tokens": ["Wenn", "mein", "Bild", "in", "dei\u00b7ner", "sanf\u00b7ten", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Himmelblauem Spiegel schwimmt; \u2013", "tokens": ["Him\u00b7mel\u00b7blau\u00b7em", "Spie\u00b7gel", "schwimmt", ";", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Leierklang aus Paradieses Fernen,", "tokens": ["Lei\u00b7er\u00b7klang", "aus", "Pa\u00b7ra\u00b7die\u00b7ses", "Fer\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "ADJA", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Harfenschwung aus angenehmern Sternen", "tokens": ["Har\u00b7fen\u00b7schwung", "aus", "an\u00b7ge\u00b7neh\u00b7mern", "Ster\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Ras ich, in mein trunken Ohr zu ziehn,", "tokens": ["Ras", "ich", ",", "in", "mein", "trun\u00b7ken", "Ohr", "zu", "ziehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "$,", "APPR", "PPOSAT", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Meine Muse f\u00fchlt die Sch\u00e4ferstunde,", "tokens": ["Mei\u00b7ne", "Mu\u00b7se", "f\u00fchlt", "die", "Sch\u00e4\u00b7fer\u00b7stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Wenn von deinem wollusthei\u00dfem Munde", "tokens": ["Wenn", "von", "dei\u00b7nem", "wol\u00b7lust\u00b7hei\u00b7\u00dfem", "Mun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Silbert\u00f6ne ungern fliehn; \u2013", "tokens": ["Sil\u00b7ber\u00b7t\u00f6\u00b7ne", "un\u00b7gern", "fliehn", ";", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["NN", "ADV", "VVINF", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Amoretten seh ich Fl\u00fcgel schwingen,", "tokens": ["A\u00b7mo\u00b7ret\u00b7ten", "seh", "ich", "Fl\u00fc\u00b7gel", "schwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Hinter dir die trunknen Fichten springen", "tokens": ["Hin\u00b7ter", "dir", "die", "trunk\u00b7nen", "Fich\u00b7ten", "sprin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "ART", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Wie von Orpheus' Saitenruf belebt,", "tokens": ["Wie", "von", "Or\u00b7pheus'", "Sai\u00b7ten\u00b7ruf", "be\u00b7lebt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "NE", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Rascher rollen um mich her die Pole,", "tokens": ["Ra\u00b7scher", "rol\u00b7len", "um", "mich", "her", "die", "Po\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "KOUI", "PRF", "APZR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Wenn im Wirbeltanze deine Sohle", "tokens": ["Wenn", "im", "Wir\u00b7bel\u00b7tan\u00b7ze", "dei\u00b7ne", "Soh\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPRART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Fl\u00fcchtig wie die Welle schwebt; \u2013", "tokens": ["Fl\u00fcch\u00b7tig", "wie", "die", "Wel\u00b7le", "schwebt", ";", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADJD", "KOKOM", "ART", "NN", "VVFIN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Deine Blicke \u2013 wenn sie Liebe l\u00e4cheln,", "tokens": ["Dei\u00b7ne", "Bli\u00b7cke", "\u2013", "wenn", "sie", "Lie\u00b7be", "l\u00e4\u00b7cheln", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "KOUS", "PPER", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "K\u00f6nnten Leben durch den Marmor f\u00e4cheln,", "tokens": ["K\u00f6nn\u00b7ten", "Le\u00b7ben", "durch", "den", "Mar\u00b7mor", "f\u00e4\u00b7cheln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Felsenadern Pulse leihn,", "tokens": ["Fel\u00b7se\u00b7na\u00b7dern", "Pul\u00b7se", "leihn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Tr\u00e4ume werden um mich her zu Wesen,", "tokens": ["Tr\u00e4u\u00b7me", "wer\u00b7den", "um", "mich", "her", "zu", "We\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "APPR", "PRF", "APZR", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Kann ich nur in deinen Augen lesen:", "tokens": ["Kann", "ich", "nur", "in", "dei\u00b7nen", "Au\u00b7gen", "le\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Laura, Laura mein! \u2013", "tokens": ["Lau\u00b7ra", ",", "Lau\u00b7ra", "mein", "!", "\u2013"], "token_info": ["word", "punct", "word", "word", "punct", "punct"], "pos": ["NE", "$,", "NE", "PPOSAT", "$.", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Wenn dann, wie gehoben aus den Achsen", "tokens": ["Wenn", "dann", ",", "wie", "ge\u00b7ho\u00b7ben", "aus", "den", "Ach\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "$,", "PWAV", "VVPP", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Zwei Gestirn, in K\u00f6rper K\u00f6rper wachsen,", "tokens": ["Zwei", "Ge\u00b7stirn", ",", "in", "K\u00f6r\u00b7per", "K\u00f6r\u00b7per", "wach\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "$,", "APPR", "NN", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Mund an Mund gewurzelt brennt,", "tokens": ["Mund", "an", "Mund", "ge\u00b7wur\u00b7zelt", "brennt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "VVPP", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wollustfunken aus den Augen regnen,", "tokens": ["Wol\u00b7lust\u00b7fun\u00b7ken", "aus", "den", "Au\u00b7gen", "reg\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Seelen wie entbunden sich begegnen", "tokens": ["See\u00b7len", "wie", "ent\u00b7bun\u00b7den", "sich", "be\u00b7geg\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "KOKOM", "VVFIN", "PRF", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "In des Atems Flammenwind, \u2013 \u2013 \u2013", "tokens": ["In", "des", "A\u00b7tems", "Flam\u00b7men\u00b7wind", ",", "\u2013", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,", "$(", "$(", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Qualentz\u00fccken \u2013 \u2013 Paradiesesschmerzen! \u2013 \u2013", "tokens": ["Qua\u00b7lent\u00b7z\u00fc\u00b7cken", "\u2013", "\u2013", "Pa\u00b7ra\u00b7die\u00b7ses\u00b7schmer\u00b7zen", "!", "\u2013", "\u2013"], "token_info": ["word", "punct", "punct", "word", "punct", "punct", "punct"], "pos": ["NN", "$(", "$(", "NN", "$.", "$(", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wilder flutet zum beklommnen Herzen,", "tokens": ["Wil\u00b7der", "flu\u00b7tet", "zum", "be\u00b7klomm\u00b7nen", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Wie Gewappnete zur Schlacht, das Blut,", "tokens": ["Wie", "Ge\u00b7wapp\u00b7ne\u00b7te", "zur", "Schlacht", ",", "das", "Blut", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPRART", "NN", "$,", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Die Natur, der Endlichkeit vergessen,", "tokens": ["Die", "Na\u00b7tur", ",", "der", "End\u00b7lich\u00b7keit", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Wagts, mit h\u00f6hern Wesen sich zu messen,", "tokens": ["Wagts", ",", "mit", "h\u00f6\u00b7hern", "We\u00b7sen", "sich", "zu", "mes\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "APPR", "ADJA", "NN", "PRF", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Schwindelt ob der acherontschen Flut.", "tokens": ["Schwin\u00b7delt", "ob", "der", "ac\u00b7he\u00b7ront\u00b7schen", "Flut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOUS", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "Eine Pause drohet hier den Sinnen,", "tokens": ["Ei\u00b7ne", "Pau\u00b7se", "dro\u00b7het", "hier", "den", "Sin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Schwarzes Dunkel jagt den Tag von hinnen,", "tokens": ["Schwar\u00b7zes", "Dun\u00b7kel", "jagt", "den", "Tag", "von", "hin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "ART", "NN", "APPR", "ADV", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Nacht verschlingt den Quell des Lichts \u2013", "tokens": ["Nacht", "ver\u00b7schlingt", "den", "Quell", "des", "Lichts", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "ART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Leises.. Murmeln... dumpfer.. hin.. verloren..", "tokens": ["Lei\u00b7ses", "..", "Mur\u00b7meln", "...", "dum\u00b7pfer", "..", "hin", "..", "ver\u00b7lo\u00b7ren", ".."], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$.", "NN", "$(", "ADJD", "$.", "PTKVZ", "$.", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Stirbt... allm\u00e4hlich.. in den trunknen... Ohren...", "tokens": ["Stirbt", "...", "all\u00b7m\u00e4h\u00b7lich", "..", "in", "den", "trunk\u00b7nen", "...", "Oh\u00b7ren", "..."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "$(", "ADJD", "$.", "APPR", "ART", "ADJA", "$(", "NN", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Und die Welt ist.... Nichts....", "tokens": ["Und", "die", "Welt", "ist", "....", "Nichts", "...."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "$.", "PIS", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.8": {"line.1": {"text": "Ach, vielleicht verpra\u00dfte tausend Monde,", "tokens": ["Ach", ",", "viel\u00b7leicht", "ver\u00b7pra\u00df\u00b7te", "tau\u00b7send", "Mon\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADV", "VVFIN", "CARD", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Laura, die Elysiumssekunde,", "tokens": ["Lau\u00b7ra", ",", "die", "E\u00b7ly\u00b7si\u00b7ums\u00b7se\u00b7kun\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "All begraben in dem schmalen Raum;", "tokens": ["All", "be\u00b7gra\u00b7ben", "in", "dem", "schma\u00b7len", "Raum", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Weggewirbelt von der Todeswonne,", "tokens": ["Weg\u00b7ge\u00b7wir\u00b7belt", "von", "der", "To\u00b7des\u00b7won\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Landen wir an einer andern Sonne,", "tokens": ["Lan\u00b7den", "wir", "an", "ei\u00b7ner", "an\u00b7dern", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Laura! und es war ein Traum.", "tokens": ["Lau\u00b7ra", "!", "und", "es", "war", "ein", "Traum", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "KON", "PPER", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "O da\u00df doch der Fl\u00fcgel Chronos' harrte,", "tokens": ["O", "da\u00df", "doch", "der", "Fl\u00fc\u00b7gel", "Chro\u00b7nos'", "harr\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOUS", "ADV", "ART", "NN", "NE", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Hingebannt ob dieser Gruppe starrte", "tokens": ["Hin\u00b7ge\u00b7bannt", "ob", "die\u00b7ser", "Grup\u00b7pe", "starr\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "KOUS", "PDAT", "NN", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Wie ein Marmorbild \u2013 \u2013 die Zeit!", "tokens": ["Wie", "ein", "Mar\u00b7mor\u00b7bild", "\u2013", "\u2013", "die", "Zeit", "!"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$(", "$(", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Aber ach! ins Meer des Todes jagen", "tokens": ["A\u00b7ber", "ach", "!", "ins", "Meer", "des", "To\u00b7des", "ja\u00b7gen"], "t