{"textgrid.poem.36852": {"metadata": {"author": {"name": "Waiblinger, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Elftes Lied", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Aber warum von Palermo du schweigst? Norm\u00e4nnischer Baukunst,", "tokens": ["A\u00b7ber", "wa\u00b7rum", "von", "Pa\u00b7ler\u00b7mo", "du", "schweigst", "?", "Nor\u00b7m\u00e4n\u00b7ni\u00b7scher", "Bau\u00b7kunst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PWAV", "APPR", "NE", "PPER", "VVFIN", "$.", "ADJA", "NN", "$,"], "meter": "+--+------+-+-+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "Gothischer Kirchen ist dort, maur'scher Pal\u00e4ste so viel.", "tokens": ["Go\u00b7thi\u00b7scher", "Kir\u00b7chen", "ist", "dort", ",", "maur'\u00b7scher", "Pa\u00b7l\u00e4s\u00b7te", "so", "viel", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "ADV", "$,", "ADJA", "NN", "ADV", "ADV", "$."], "meter": "+--+-+-+--+--+", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Denke des Domes nur in Monreale, des alten,", "tokens": ["Den\u00b7ke", "des", "Do\u00b7mes", "nur", "in", "Mon\u00b7re\u00b7a\u00b7le", ",", "des", "al\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADV", "APPR", "NE", "$,", "ART", "ADJA", "$,"], "meter": "+--+-+-+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Frommer Mosaik, des Styls, der nur gerecht ist vor Gott.", "tokens": ["From\u00b7mer", "Mo\u00b7saik", ",", "des", "Styls", ",", "der", "nur", "ge\u00b7recht", "ist", "vor", "Gott", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ART", "NN", "$,", "PRELS", "ADV", "ADJD", "VAFIN", "APPR", "NN", "$."], "meter": "+----+-+-+--+", "measure": "iambic.penta.chol"}, "line.5": {"text": "Wie, von Palermo zu h\u00f6ren, ihr w\u00fcnscht es, christlichen Freunde?", "tokens": ["Wie", ",", "von", "Pa\u00b7ler\u00b7mo", "zu", "h\u00f6\u00b7ren", ",", "ihr", "w\u00fcnscht", "es", ",", "christ\u00b7li\u00b7chen", "Freun\u00b7de", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "$,", "APPR", "NE", "PTKZU", "VVINF", "$,", "PPER", "VVFIN", "PPER", "$,", "ADJA", "NN", "$."], "meter": "--+-+-+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Nun doch, wie immer, bin ich euch zu erz\u00e4hlen bereit.", "tokens": ["Nun", "doch", ",", "wie", "im\u00b7mer", ",", "bin", "ich", "euch", "zu", "er\u00b7z\u00e4h\u00b7len", "be\u00b7reit", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "PWAV", "ADV", "$,", "VAFIN", "PPER", "PPER", "PTKZU", "VVINF", "ADJD", "$."], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "Morgens weih' ich ein St\u00fcndchen mir selbst und meinen Gedanken.", "tokens": ["Mor\u00b7gens", "weih'", "ich", "ein", "St\u00fcnd\u00b7chen", "mir", "selbst", "und", "mei\u00b7nen", "Ge\u00b7dan\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "PPER", "ADV", "KON", "PPOSAT", "NN", "$."], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.8": {"text": "Drauf in den Wagen \u2013 er ist reinlich und h\u00fcbsch und bequem \u2013", "tokens": ["Drauf", "in", "den", "Wa\u00b7gen", "\u2013", "er", "ist", "rein\u00b7lich", "und", "h\u00fcbsch", "und", "be\u00b7quem", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "APPR", "ART", "NN", "$(", "PPER", "VAFIN", "ADJD", "KON", "ADJD", "KON", "ADJD", "$("], "meter": "-+-+---+--+--+", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Oder durchs laute Gew\u00fchl des \u00fcberf\u00fcllten Toledo", "tokens": ["O\u00b7der", "durchs", "lau\u00b7te", "Ge\u00b7w\u00fchl", "des", "\u00fc\u00b7berf\u00b7\u00fcll\u00b7ten", "To\u00b7le\u00b7do"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "---+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "Dr\u00e4ng' ich mich auch und mir d\u00fcnkt hier in Neapel zu sein.", "tokens": ["Dr\u00e4ng'", "ich", "mich", "auch", "und", "mir", "d\u00fcnkt", "hier", "in", "Nea\u00b7pel", "zu", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "KON", "PPER", "VVFIN", "ADV", "APPR", "NE", "PTKZU", "VAINF", "$."], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.11": {"text": "Vieles besch\u00e4ftigt mich, mich erfreut das Get\u00fcmmel, der Reichthum,", "tokens": ["Vie\u00b7les", "be\u00b7sch\u00e4f\u00b7tigt", "mich", ",", "mich", "er\u00b7freut", "das", "Ge\u00b7t\u00fcm\u00b7mel", ",", "der", "Reicht\u00b7hum", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "$,", "PPER", "VVFIN", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.12": {"text": "Mich der th\u00e4tige Trieb, mich die allt\u00e4gliche Welt.", "tokens": ["Mich", "der", "th\u00e4\u00b7ti\u00b7ge", "Trieb", ",", "mich", "die", "all\u00b7t\u00e4g\u00b7li\u00b7che", "Welt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "$,", "PRF", "ART", "ADJA", "NN", "$."], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.13": {"text": "Weih' ich aber dem Sch\u00f6nen den Blick, gleich erfa\u00dft mich ein Bettler", "tokens": ["Weih'", "ich", "a\u00b7ber", "dem", "Sch\u00f6\u00b7nen", "den", "Blick", ",", "gleich", "er\u00b7fa\u00dft", "mich", "ein", "Bett\u00b7ler"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "ART", "NN", "$,", "ADV", "VVFIN", "PPER", "ART", "NN"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.14": {"text": "Winselnd und wei\u00dfen Barts, nackt wie das Weib ihn gebar.", "tokens": ["Win\u00b7selnd", "und", "wei\u00b7\u00dfen", "Barts", ",", "nackt", "wie", "das", "Weib", "ihn", "ge\u00b7bar", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJA", "NN", "$,", "ADJD", "KOKOM", "ART", "NN", "PPER", "ADJD", "$."], "meter": "+--+-+-+-+--+", "measure": "iambic.hexa.invert"}, "line.15": {"text": "Gern besuch ich die Freunde, die wohlgesinnten, und Nektar,", "tokens": ["Gern", "be\u00b7such", "ich", "die", "Freun\u00b7de", ",", "die", "wohl\u00b7ge\u00b7sinn\u00b7ten", ",", "und", "Nek\u00b7tar", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,", "ART", "ADJA", "$,", "KON", "NN", "$,"], "meter": "+-+--+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.16": {"text": "Altsikulischer, giebt Leben und Scherz dem Gespr\u00e4ch.", "tokens": ["Alt\u00b7si\u00b7ku\u00b7li\u00b7scher", ",", "giebt", "Le\u00b7ben", "und", "Scherz", "dem", "Ge\u00b7spr\u00e4ch", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "NN", "KON", "NN", "ART", "NN", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.17": {"text": "Meist doch streif' ich am Strande des Meers und betrachte die Barken", "tokens": ["Meist", "doch", "streif'", "ich", "am", "Stran\u00b7de", "des", "Meers", "und", "be\u00b7trach\u00b7te", "die", "Bar\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "VVFIN", "PPER", "APPRART", "NN", "ART", "NN", "KON", "VVFIN", "ART", "NN"], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.18": {"text": "Und die Schiffer, wie sie hier zu Rosaliens Berg,", "tokens": ["Und", "die", "Schif\u00b7fer", ",", "wie", "sie", "hier", "zu", "Ro\u00b7sa\u00b7li\u00b7ens", "Berg", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PWAV", "PPER", "ADV", "APPR", "NE", "NN", "$,"], "meter": "--+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.19": {"text": "Oder zum Kap hinschweben von Zafaran, mich belustigt", "tokens": ["O\u00b7der", "zum", "Kap", "hin\u00b7schwe\u00b7ben", "von", "Za\u00b7fa\u00b7ran", ",", "mich", "be\u00b7lus\u00b7tigt"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "APPRART", "NN", "VVPP", "APPR", "NE", "$,", "PPER", "VVFIN"], "meter": "+--+-+--+--+-+-", "measure": "iambic.hexa.invert"}, "line.20": {"text": "Jetzt die st\u00e4dtische Pracht, G\u00e4rten und Park und Palast,", "tokens": ["Jetzt", "die", "st\u00e4d\u00b7ti\u00b7sche", "Pracht", ",", "G\u00e4r\u00b7ten", "und", "Park", "und", "Pa\u00b7last", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$,", "NN", "KON", "NN", "KON", "NN", "$,"], "meter": "--+--++--+--+", "measure": "anapaest.di.plus"}, "line.21": {"text": "Jetzt das lieblichste Bild \u00e4olischer Inseln. Es f\u00fchrt mich", "tokens": ["Jetzt", "das", "lieb\u00b7lichs\u00b7te", "Bild", "\u00e4\u00b7o\u00b7li\u00b7scher", "In\u00b7seln", ".", "Es", "f\u00fchrt", "mich"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN", "ADJA", "NN", "$.", "PPER", "VVFIN", "PPER"], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}, "line.22": {"text": "Stunden und Tage der Weg so durch Palermos Natur.", "tokens": ["Stun\u00b7den", "und", "Ta\u00b7ge", "der", "Weg", "so", "durch", "Pa\u00b7ler\u00b7mos", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ART", "NN", "ADV", "APPR", "NE", "NN", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.23": {"text": "Alle Berg', ich erklettre sie k\u00fchn; doch bist du vor allen,", "tokens": ["Al\u00b7le", "Ber\u00b7g'", ",", "ich", "er\u00b7klett\u00b7re", "sie", "k\u00fchn", ";", "doch", "bist", "du", "vor", "al\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PPER", "VVFIN", "PPER", "ADJD", "$.", "ADV", "VAFIN", "PPER", "APPR", "PIAT", "$,"], "meter": "--+-+-+--+-++-+-", "measure": "iambic.septa.relaxed"}, "line.24": {"text": "Fels'ger Cypressenpark, Bocca di Falco, mir lieb.", "tokens": ["Fels'\u00b7ger", "Cyp\u00b7res\u00b7sen\u00b7park", ",", "Boc\u00b7ca", "di", "Fal\u00b7co", ",", "mir", "lieb", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "FM", "FM", "FM", "$,", "PPER", "ADJD", "$."], "meter": "+-+---+--+--+", "measure": "trochaic.penta.relaxed"}, "line.25": {"text": "Auch die G\u00e4rten durchwandl' ich und sehe Brasiliens Pflanzen", "tokens": ["Auch", "die", "G\u00e4r\u00b7ten", "durch\u00b7wandl'", "ich", "und", "se\u00b7he", "Bra\u00b7si\u00b7li\u00b7ens", "Pflan\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "VVFIN", "PPER", "KON", "VVFIN", "NE", "NN"], "meter": "--+--+--+-+-+-+-", "measure": "anapaest.tri.plus"}, "line.26": {"text": "Frei, in gl\u00fccklicher Luft, wie in der Heimath erbl\u00fchn.", "tokens": ["Frei", ",", "in", "gl\u00fcck\u00b7li\u00b7cher", "Luft", ",", "wie", "in", "der", "Hei\u00b7math", "er\u00b7bl\u00fchn", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "ADJA", "NN", "$,", "PWAV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.27": {"text": "Werd' ich m\u00fcde, so lockt die Citron', es lockt mich der Maulbeer", "tokens": ["Werd'", "ich", "m\u00fc\u00b7de", ",", "so", "lockt", "die", "Ci\u00b7tron'", ",", "es", "lockt", "mich", "der", "Maul\u00b7beer"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADJD", "$,", "ADV", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "PPER", "ART", "NN"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.28": {"text": "In den Schatten und reicht Schutz vor der Sonne Gewalt.", "tokens": ["In", "den", "Schat\u00b7ten", "und", "reicht", "Schutz", "vor", "der", "Son\u00b7ne", "Ge\u00b7walt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KON", "VVFIN", "NN", "APPR", "ART", "NN", "NN", "$."], "meter": "--+--++--+--+", "measure": "anapaest.di.plus"}, "line.29": {"text": "Aber den Durst, bald stillt ihn Indiens stachliche Feige,", "tokens": ["A\u00b7ber", "den", "Durst", ",", "bald", "stillt", "ihn", "In\u00b7di\u00b7ens", "stach\u00b7li\u00b7che", "Fei\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "ADV", "VVFIN", "PPER", "ADV", "ADJA", "NN", "$,"], "meter": "+--+-+-+--+--+-", "measure": "hexameter"}, "line.30": {"text": "Bald der Brunnen und bald stillt ihn der s\u00fc\u00dfe Sorbet.", "tokens": ["Bald", "der", "Brun\u00b7nen", "und", "bald", "stillt", "ihn", "der", "s\u00fc\u00b7\u00dfe", "Sor\u00b7bet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "KON", "ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}, "line.31": {"text": "Denn am Abend kehr' ich zur Stadt, und muntre Gesellschaft,", "tokens": ["Denn", "am", "A\u00b7bend", "kehr'", "ich", "zur", "Stadt", ",", "und", "mun\u00b7tre", "Ge\u00b7sell\u00b7schaft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VVFIN", "PPER", "APPRART", "NN", "$,", "KON", "ADJA", "NN", "$,"], "meter": "--+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.32": {"text": "Wie dem Vogel die Luft, ist sie mir n\u00f6thig, o Freund.", "tokens": ["Wie", "dem", "Vo\u00b7gel", "die", "Luft", ",", "ist", "sie", "mir", "n\u00f6\u00b7thig", ",", "o", "Freund", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "NN", "$,", "VAFIN", "PPER", "PPER", "ADJD", "$,", "FM", "NN", "$."], "meter": "--+--+--+-+-+", "measure": "anapaest.tri.plus"}, "line.33": {"text": "Christlicher Freund, dich hab' ich gemeint; doch zu guter Gesellschaft,", "tokens": ["Christ\u00b7li\u00b7cher", "Freund", ",", "dich", "hab'", "ich", "ge\u00b7meint", ";", "doch", "zu", "gu\u00b7ter", "Ge\u00b7sell\u00b7schaft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PPER", "VAFIN", "PPER", "VVPP", "$.", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "+--+-+--+--+--+-", "measure": "hexameter"}, "line.34": {"text": "Merk es, z\u00e4hl' ich bei Nacht immer ein Liebchen dazu.", "tokens": ["Merk", "es", ",", "z\u00e4hl'", "ich", "bei", "Nacht", "im\u00b7mer", "ein", "Lieb\u00b7chen", "da\u00b7zu", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "VVFIN", "PPER", "APPR", "NN", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Aber warum von Palermo du schweigst? Norm\u00e4nnischer Baukunst,", "tokens": ["A\u00b7ber", "wa\u00b7rum", "von", "Pa\u00b7ler\u00b7mo", "du", "schweigst", "?", "Nor\u00b7m\u00e4n\u00b7ni\u00b7scher", "Bau\u00b7kunst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PWAV", "APPR", "NE", "PPER", "VVFIN", "$.", "ADJA", "NN", "$,"], "meter": "+--+------+-+-+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "Gothischer Kirchen ist dort, maur'scher Pal\u00e4ste so viel.", "tokens": ["Go\u00b7thi\u00b7scher", "Kir\u00b7chen", "ist", "dort", ",", "maur'\u00b7scher", "Pa\u00b7l\u00e4s\u00b7te", "so", "viel", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "ADV", "$,", "ADJA", "NN", "ADV", "ADV", "$."], "meter": "+--+-+-+--+--+", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Denke des Domes nur in Monreale, des alten,", "tokens": ["Den\u00b7ke", "des", "Do\u00b7mes", "nur", "in", "Mon\u00b7re\u00b7a\u00b7le", ",", "des", "al\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "