{"textgrid.poem.41789": {"metadata": {"author": {"name": "Otto, Louise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Das ist die Murg, zum Flu\u00df geworden,", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das ist die Murg, zum Flu\u00df geworden,", "tokens": ["Das", "ist", "die", "Murg", ",", "zum", "Flu\u00df", "ge\u00b7wor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "APPRART", "NN", "VAPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die erst als Waldbach dumpf gebraust, \u2013", "tokens": ["Die", "erst", "als", "Wald\u00b7bach", "dumpf", "ge\u00b7braust", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADV", "KOUS", "NN", "ADJD", "VVPP", "$,", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "O drohe nur des Feindes Horden,", "tokens": ["O", "dro\u00b7he", "nur", "des", "Fein\u00b7des", "Hor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "ART", "NN", "NE", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der frech an Deinen Ufern haust!", "tokens": ["Der", "frech", "an", "Dei\u00b7nen", "U\u00b7fern", "haust", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Es kam der Reichsfeind \u00fcber Rhein,", "tokens": ["Es", "kam", "der", "Reichs\u00b7feind", "\u00fc\u00b7ber", "Rhein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Badenser k\u00e4mpfen mit Franzosen,", "tokens": ["Ba\u00b7den\u00b7ser", "k\u00e4mp\u00b7fen", "mit", "Fran\u00b7zo\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und wieder hei\u00dft: ein Deutscher sein,", "tokens": ["Und", "wie\u00b7der", "hei\u00dft", ":", "ein", "Deut\u00b7scher", "sein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "$.", "ART", "NN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Sich z\u00e4hlen zu den Hilfelosen!", "tokens": ["Sich", "z\u00e4h\u00b7len", "zu", "den", "Hil\u00b7fe\u00b7lo\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Die Festung Rohrburg ruft in N\u00f6ten", "tokens": ["Die", "Fes\u00b7tung", "Rohr\u00b7burg", "ruft", "in", "N\u00f6\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NE", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Reichstruppen auf zu Schirm und Wehr", "tokens": ["Reich\u00b7strup\u00b7pen", "auf", "zu", "Schirm", "und", "Wehr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Feinde drohen, brennen, t\u00f6ten \u2013", "tokens": ["Die", "Fein\u00b7de", "dro\u00b7hen", ",", "bren\u00b7nen", ",", "t\u00f6\u00b7ten", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "VVINF", "$,", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zur Rettung naht kein deutsches Heer!", "tokens": ["Zur", "Ret\u00b7tung", "naht", "kein", "deut\u00b7sches", "Heer", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Schon ist verloren Wall um Wall,", "tokens": ["Schon", "ist", "ver\u00b7lo\u00b7ren", "Wall", "um", "Wall", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "VVPP", "NE", "APPR", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Schon st\u00fcrmt der Feind an allen Thoren,", "tokens": ["Schon", "st\u00fcrmt", "der", "Feind", "an", "al\u00b7len", "Tho\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Singt trotz'ger Siegeslieder Schall \u2013", "tokens": ["Singt", "trotz'\u00b7ger", "Sie\u00b7ges\u00b7lie\u00b7der", "Schall", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "In Rohrburg Jammerruf: \u00bbVerloren!\u00ab", "tokens": ["In", "Rohr\u00b7burg", "Jam\u00b7mer\u00b7ruf", ":", "\u00bb", "Ver\u00b7lo\u00b7ren", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct", "word", "punct", "punct"], "pos": ["APPR", "NE", "NN", "$.", "$(", "NN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ein B\u00fcrger naht dem Kommandanten,", "tokens": ["Ein", "B\u00fcr\u00b7ger", "naht", "dem", "Kom\u00b7man\u00b7dan\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein deutscher B\u00fcrger schlicht und recht;", "tokens": ["Ein", "deut\u00b7scher", "B\u00fcr\u00b7ger", "schlicht", "und", "recht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der spricht: \u00bbNoch sind wir nicht zu Schanden,", "tokens": ["Der", "spricht", ":", "\u00bb", "Noch", "sind", "wir", "nicht", "zu", "Schan\u00b7den", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$.", "$(", "ADV", "VAFIN", "PPER", "PTKNEG", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ich werde kein Franzosenknecht!", "tokens": ["Ich", "wer\u00b7de", "kein", "Fran\u00b7zo\u00b7sen\u00b7knecht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Kein Markgraf will, kein Herzog helfen,", "tokens": ["Kein", "Mark\u00b7graf", "will", ",", "kein", "Her\u00b7zog", "hel\u00b7fen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "$,", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Kein Reichssoldat, kein General \u2013", "tokens": ["Kein", "Reichs\u00b7sol\u00b7dat", ",", "kein", "Ge\u00b7ne\u00b7ral", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Sie dienen Waiblingern und Welfen \u2013", "tokens": ["Sie", "die\u00b7nen", "Waib\u00b7lin\u00b7gern", "und", "Wel\u00b7fen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.8": {"text": "Dem deutschen Volke bleibt die Qual!\u00ab", "tokens": ["Dem", "deut\u00b7schen", "Vol\u00b7ke", "bleibt", "die", "Qual", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "\u00bbmir helfen zwanzig K\u00f6niginnen", "tokens": ["\u00bb", "mir", "hel\u00b7fen", "zwan\u00b7zig", "K\u00f6\u00b7ni\u00b7gin\u00b7nen"], "token_info": ["punct", "word", "word", "word", "word"], "pos": ["$(", "PPER", "VVINF", "CARD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit ihres Volkes gro\u00dfer Schar;", "tokens": ["Mit", "ih\u00b7res", "Vol\u00b7kes", "gro\u00b7\u00dfer", "Schar", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die sollen uns den Sieg gewinnen", "tokens": ["Die", "sol\u00b7len", "uns", "den", "Sieg", "ge\u00b7win\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VMFIN", "PPER", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und von uns wenden die Gefahr.", "tokens": ["Und", "von", "uns", "wen\u00b7den", "die", "Ge\u00b7fahr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Hoch oben auf dem Festungsbau", "tokens": ["Hoch", "o\u00b7ben", "auf", "dem", "Fes\u00b7tungs\u00b7bau"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sind schon die Truppen und Kasernen \u2013", "tokens": ["Sind", "schon", "die", "Trup\u00b7pen", "und", "Ka\u00b7ser\u00b7nen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "KON", "NN", "$("], "meter": "-+-+---+-", "measure": "unknown.measure.tri"}, "line.7": {"text": "Kommt, haltet Eure Heeresschau,", "tokens": ["Kommt", ",", "hal\u00b7tet", "Eu\u00b7re", "Hee\u00b7res\u00b7schau", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wollt meine Mannschaft kennen lernen!\u00ab", "tokens": ["Wollt", "mei\u00b7ne", "Mann\u00b7schaft", "ken\u00b7nen", "ler\u00b7nen", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "VVINF", "VVINF", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Und oben auf den h\u00f6chsten Schanzen", "tokens": ["Und", "o\u00b7ben", "auf", "den", "h\u00f6chs\u00b7ten", "Schan\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Des B\u00fcrgers Bienenk\u00f6rbe stehen.", "tokens": ["Des", "B\u00fcr\u00b7gers", "Bie\u00b7nen\u00b7k\u00f6r\u00b7be", "ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Hurrah! \u2013 Wie zum lust'gen Tanzen", "tokens": ["Ein", "Hur\u00b7rah", "!", "\u2013", "Wie", "zum", "lust'\u00b7gen", "Tan\u00b7zen"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$.", "$(", "PWAV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zum Sturme die Franzosen gehen \u2013", "tokens": ["Zum", "Stur\u00b7me", "die", "Fran\u00b7zo\u00b7sen", "ge\u00b7hen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und schaut! hinab in ihre Reihn", "tokens": ["Und", "schaut", "!", "hin\u00b7ab", "in", "ih\u00b7re", "Reihn"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$.", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ein Wurf in weitgeschwungnem Bogen \u2013", "tokens": ["Ein", "Wurf", "in", "weit\u00b7ge\u00b7schwung\u00b7nem", "Bo\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Welch Summen, Brausen, Sch\u00fctteln, Schrein \u2013", "tokens": ["Welch", "Sum\u00b7men", ",", "Brau\u00b7sen", ",", "Sch\u00fct\u00b7teln", ",", "Schrein", "\u2013"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PIAT", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ein Korb dem seine Schar entflogen!", "tokens": ["Ein", "Korb", "dem", "sei\u00b7ne", "Schar", "ent\u00b7flo\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Und zwanzig so! \u2013 und Millionen", "tokens": ["Und", "zwan\u00b7zig", "so", "!", "\u2013", "und", "Mil\u00b7lion\u00b7en"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word"], "pos": ["KON", "CARD", "ADV", "$.", "$(", "KON", "NN"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.2": {"text": "Von Bienen in der Feinde Heer;", "tokens": ["Von", "Bie\u00b7nen", "in", "der", "Fein\u00b7de", "Heer", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da gilt kein Rufen und kein Schonen,", "tokens": ["Da", "gilt", "kein", "Ru\u00b7fen", "und", "kein", "Scho\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "KON", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da hilft nicht Sturm und Gegenwehr!", "tokens": ["Da", "hilft", "nicht", "Sturm", "und", "Ge\u00b7gen\u00b7wehr", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PTKNEG", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wie scharf und spitz der Bienenpfeil", "tokens": ["Wie", "scharf", "und", "spitz", "der", "Bie\u00b7nen\u00b7pfeil"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "KON", "VVIMP", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Versandt mit zornig gift'gem Brummen, \u2013", "tokens": ["Ver\u00b7sandt", "mit", "zor\u00b7nig", "gift'\u00b7gem", "Brum\u00b7men", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVPP", "APPR", "ADJD", "ADJA", "NN", "$,", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und in der Flucht sucht jeder Heil", "tokens": ["Und", "in", "der", "Flucht", "sucht", "je\u00b7der", "Heil"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Vor diesem Stechen, diesem Summen!", "tokens": ["Vor", "die\u00b7sem", "Ste\u00b7chen", ",", "die\u00b7sem", "Sum\u00b7men", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,", "PDAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Kein Mann h\u00e4lt Stand! die K\u00e4mpfer weichen", "tokens": ["Kein", "Mann", "h\u00e4lt", "Stand", "!", "die", "K\u00e4mp\u00b7fer", "wei\u00b7chen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PIAT", "NN", "VVFIN", "NN", "$.", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vor solcher Freischar k\u00fchnem Flug.", "tokens": ["Vor", "sol\u00b7cher", "Frei\u00b7schar", "k\u00fch\u00b7nem", "Flug", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ihr Summen ist ihr Siegeszeichen,", "tokens": ["Ihr", "Sum\u00b7men", "ist", "ihr", "Sie\u00b7ges\u00b7zei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ihr Stachel mehr als Schwerter schlug.", "tokens": ["Ihr", "Sta\u00b7chel", "mehr", "als", "Schwer\u00b7ter", "schlug", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "KOUS", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Feinde rings in toller Flucht \u2013", "tokens": ["Die", "Fein\u00b7de", "rings", "in", "tol\u00b7ler", "Flucht", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sie werfen v