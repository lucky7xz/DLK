{"dta.poem.16134": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "188.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Rechne nicht auf die Welt und ihren Freudenzoll;", "tokens": ["Rech\u00b7ne", "nicht", "auf", "die", "Welt", "und", "ih\u00b7ren", "Freu\u00b7den\u00b7zoll", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "APPR", "ART", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Sie gibt es tropfenweis und nimmt den Becher voll.", "tokens": ["Sie", "gibt", "es", "trop\u00b7fen\u00b7weis", "und", "nimmt", "den", "Be\u00b7cher", "voll", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "KON", "VVFIN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "In Groschen streckt sie vor, und will zum Zins den Thaler,", "tokens": ["In", "Gro\u00b7schen", "streckt", "sie", "vor", ",", "und", "will", "zum", "Zins", "den", "Tha\u00b7ler", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "PTKVZ", "$,", "KON", "VMFIN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Kein St\u00fcndchen Stundung auch gibt sie dem s\u00e4um'gen Zahler.", "tokens": ["Kein", "St\u00fcnd\u00b7chen", "Stun\u00b7dung", "auch", "gibt", "sie", "dem", "s\u00e4um'\u00b7gen", "Zah\u00b7ler", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "NN",