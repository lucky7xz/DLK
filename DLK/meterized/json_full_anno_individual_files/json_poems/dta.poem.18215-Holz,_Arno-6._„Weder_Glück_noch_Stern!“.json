{"dta.poem.18215": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "6.  \n  \u201eWeder Gl\u00fcck noch Stern!\u201c", "genre": "Lyrik", "period": "N.A.", "pub_year": "1886", "urn": "urn:nbn:de:kobv:b4-200905192665", "language": ["de:0.99"], "booktitle": "Holz, Arno: Das Buch der Zeit. Lieder eines Modernen. Z\u00fcrich, 1886."}, "poem": {"stanza.1": {"line.1": {"text": "Ein Tr\u00e4umer! milderte die Nachbarschaft", "tokens": ["Ein", "Tr\u00e4u\u00b7mer", "!", "mil\u00b7der\u00b7te", "die", "Nach\u00b7bar\u00b7schaft"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$.", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und nur sein Herzfreund sprach: Er war ein Dichter!", "tokens": ["Und", "nur", "sein", "Herz\u00b7freund", "sprach", ":", "Er", "war", "ein", "Dich\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPOSAT", "NN", "VVFIN", "$.", "PPER", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Vor seinem Krankenlager aber sa\u00df", "tokens": ["Vor", "sei\u00b7nem", "Kran\u00b7ken\u00b7la\u00b7ger", "a\u00b7ber", "sa\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die bleiche Schwester der Barmherzigkeit", "tokens": ["Die", "blei\u00b7che", "Schwes\u00b7ter", "der", "Barm\u00b7her\u00b7zig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und blickte sinnend auf ein Blatt Papier,", "tokens": ["Und", "blick\u00b7te", "sin\u00b7nend", "auf", "ein", "Blatt", "Pa\u00b7pier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Das gestern erst der flinke Telegraph,", "tokens": ["Das", "ge\u00b7stern", "erst", "der", "flin\u00b7ke", "Te\u00b7le\u00b7gra\u00b7ph", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "--+--+-+--+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Mit seinen krausen Zeichen \u00fcberdeckt,", "tokens": ["Mit", "sei\u00b7nen", "krau\u00b7sen", "Zei\u00b7chen", "\u00fc\u00b7berd\u00b7eckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und nur mit M\u00fche konnte sie entziffern:", "tokens": ["Und", "nur", "mit", "M\u00fc\u00b7he", "konn\u00b7te", "sie", "ent\u00b7zif\u00b7fern", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "NN", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "\u201eihr erstes St\u00fcck! Ein Sensationserfolg!", "tokens": ["\u201e", "ihr", "ers\u00b7tes", "St\u00fcck", "!", "Ein", "Sen\u00b7sa\u00b7ti\u00b7on\u00b7ser\u00b7folg", "!"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "ADJA", "NN", "$.", "ART", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Ber\u00fchmt mit einem Schlag! Wir gratuliren!\u201c", "tokens": ["Be\u00b7r\u00fchmt", "mit", "ei\u00b7nem", "Schlag", "!", "Wir", "gra\u00b7tu\u00b7li\u00b7ren", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$.", "PPER", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Er aber, dem dies kleine Blatt Papier", "tokens": ["Er", "a\u00b7ber", ",", "dem", "dies", "klei\u00b7ne", "Blatt", "Pa\u00b7pier"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "$,", "PRELS", "PDS", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Die hei\u00dfersehnte Botschaft k\u00fcnden sollte:", "tokens": ["Die", "hei\u00b7\u00dfer\u00b7sehn\u00b7te", "Bot\u00b7schaft", "k\u00fcn\u00b7den", "soll\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Gl\u00fcck auf, nun hast du nicht umsonst gelebt \u2014", "tokens": ["Gl\u00fcck", "auf", ",", "nun", "hast", "du", "nicht", "um\u00b7sonst", "ge\u00b7lebt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "ADV", "VAFIN", "PPER", "PTKNEG", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Er schlief und sah es nicht, denn er war todt.", "tokens": ["Er", "schlief", "und", "sah", "es", "nicht", ",", "denn", "er", "war", "todt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PPER", "PTKNEG", "$,", "KON", "PPER", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Der dunkle Winterabend warf sein Licht", "tokens": ["Der", "dunk\u00b7le", "Win\u00b7ter\u00b7a\u00b7bend", "warf", "sein", "Licht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Kalt durch die zugefrornen Fensterscheiben", "tokens": ["Kalt", "durch", "die", "zu\u00b7ge\u00b7fror\u00b7nen", "Fens\u00b7ter\u00b7schei\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und spielte zitternd um ein Frauenbild,", "tokens": ["Und", "spiel\u00b7te", "zit\u00b7ternd", "um", "ein", "Frau\u00b7en\u00b7bild", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Das auf die bleiche Stirn des todten Dulders", "tokens": ["Das", "auf", "die", "blei\u00b7che", "Stirn", "des", "tod\u00b7ten", "Dul\u00b7ders"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "APPR", "ART", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Uns\u00e4glich sch\u00f6n und mitleidsvoll herabsah.", "tokens": ["Un\u00b7s\u00e4g\u00b7lich", "sch\u00f6n", "und", "mit\u00b7le\u00b7ids\u00b7voll", "her\u00b7ab\u00b7sah", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "KON", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Darunter aber wand ein welker Kranz", "tokens": ["Da\u00b7run\u00b7ter", "a\u00b7ber", "wand", "ein", "wel\u00b7ker", "Kranz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Sich gr\u00fcn um ein vergilbtes Atlasband;", "tokens": ["Sich", "gr\u00fcn", "um", "ein", "ver\u00b7gilb\u00b7tes", "At\u00b7las\u00b7band", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Drauf stand, voreinst von Freundeshand geschrieben,", "tokens": ["Drauf", "stand", ",", "vor\u00b7e\u00b7inst", "von", "Freun\u00b7des\u00b7hand", "ge\u00b7schrie\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "$,", "ADV", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Das Spr\u00fcchlein: Lorbeerbaum und Bettelstab!", "tokens": ["Das", "Spr\u00fcch\u00b7lein", ":", "Lor\u00b7beer\u00b7baum", "und", "Bet\u00b7tel\u00b7stab", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}