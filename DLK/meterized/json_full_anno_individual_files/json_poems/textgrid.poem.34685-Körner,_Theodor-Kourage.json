{"textgrid.poem.34685": {"metadata": {"author": {"name": "K\u00f6rner, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Kourage", "genre": "verse", "period": "N.A.", "pub_year": 1802, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kourage! Kourage!", "tokens": ["Kou\u00b7ra\u00b7ge", "!", "Kou\u00b7ra\u00b7ge", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$.", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "Du machst mich ganz schachmatt.", "tokens": ["Du", "machst", "mich", "ganz", "schach\u00b7matt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wer war denn nur der Teufelskerl,", "tokens": ["Wer", "war", "denn", "nur", "der", "Teu\u00b7fels\u00b7kerl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der dich erfunden hat?", "tokens": ["Der", "dich", "er\u00b7fun\u00b7den", "hat", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "\u00bbmama, ich soll Soldate werd'n,", "tokens": ["\u00bb", "ma\u00b7ma", ",", "ich", "soll", "Sol\u00b7da\u00b7te", "werd'n", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ITJ", "$,", "PPER", "VMFIN", "NN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mama, da wird nichts draus,", "tokens": ["Ma\u00b7ma", ",", "da", "wird", "nichts", "draus", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "VAFIN", "PIS", "PAV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie schie\u00dfen einem im Dunkeln", "tokens": ["Sie", "schie\u00b7\u00dfen", "ei\u00b7nem", "im", "Dun\u00b7keln"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "APPRART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Wohl gar ein Auge aus.", "tokens": ["Wohl", "gar", "ein", "Au\u00b7ge", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "\u00bbich soll 'ne Flinte tragen,", "tokens": ["\u00bb", "ich", "soll", "'ne", "Flin\u00b7te", "tra\u00b7gen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Solch feiner Leute Kind!", "tokens": ["Solch", "fei\u00b7ner", "Leu\u00b7te", "Kind", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mama, sie wissen's noch gar nicht,", "tokens": ["Ma\u00b7ma", ",", "sie", "wis\u00b7sen's", "noch", "gar", "nicht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VVFIN", "ADV", "ADV", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Was d'Franzosen f\u00fcr Flegel sind.\u00ab \u2013", "tokens": ["Was", "d'\u00b7Fran\u00b7zo\u00b7sen", "f\u00fcr", "Fle\u00b7gel", "sind", ".", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PWS", "PDS", "APPR", "NN", "VAFIN", "$.", "$(", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "\u00bbmein Thomas! bleib' zu Hause!", "tokens": ["\u00bb", "mein", "Tho\u00b7mas", "!", "bleib'", "zu", "Hau\u00b7se", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NE", "$.", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Pfui! Blut! ein schlechter Durst.", "tokens": ["Pfui", "!", "Blut", "!", "ein", "schlech\u00b7ter", "Durst", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$.", "NN", "$.", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich lass' dir ein \u00c4pfelchen braten", "tokens": ["Ich", "lass'", "dir", "ein", "\u00c4p\u00b7fel\u00b7chen", "bra\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und kauf' dir 'ne Braunschweiger Wurst.\u00ab \u2013", "tokens": ["Und", "kauf'", "dir", "'ne", "Braun\u00b7schwei\u00b7ger", "Wurst", ".", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "ADJA", "NN", "$.", "$(", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "\u00bbfein's Liebchen, ich wollt' Soldat werd'n,", "tokens": ["\u00bb", "fein's", "Lieb\u00b7chen", ",", "ich", "wollt'", "Sol\u00b7dat", "werd'n", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJA", "NN", "$,", "PPER", "VMFIN", "NN", "VAINF", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Die Mutter leidet's nicht! \u2013", "tokens": ["Die", "Mut\u00b7ter", "lei\u00b7det's", "nicht", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da mu\u00df ich's lassen bleiben,", "tokens": ["Da", "mu\u00df", "ich's", "las\u00b7sen", "blei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIS", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "'s w\u00e4r' wider die Kindespflicht.\u00ab \u2013", "tokens": ["'", "s", "w\u00e4r", "'", "wi\u00b7der", "die", "Kin\u00b7des\u00b7pflicht", ".", "\u00ab", "\u2013"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct", "punct", "punct"], "pos": ["$(", "PPER", "VAFIN", "$(", "APPR", "ART", "NN", "$.", "$(", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "\u00bbdie Kindespflicht mu\u00df weichen,", "tokens": ["\u00bb", "die", "Kin\u00b7des\u00b7pflicht", "mu\u00df", "wei\u00b7chen", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zum Kampfe, trauter Thoms!", "tokens": ["Zum", "Kamp\u00b7fe", ",", "trau\u00b7ter", "Thoms", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sieg' oder stirb f\u00fcrs Vaterland,", "tokens": ["Sieg'", "o\u00b7der", "stirb", "f\u00fcrs", "Va\u00b7ter\u00b7land", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie Heldens\u00f6hne Roms!\u00ab \u2013", "tokens": ["Wie", "Hel\u00b7den\u00b7s\u00f6h\u00b7ne", "Roms", "!", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct", "punct"], "pos": ["PWAV", "NN", "NE", "$.", "$(", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "\u00bbich hab ja mein gewisses Brot", "tokens": ["\u00bb", "ich", "hab", "ja", "mein", "ge\u00b7wis\u00b7ses", "Brot"], "token_info": ["punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "PPER", "VAFIN", "ADV", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Beim Schwefel und bei Kaffee,", "tokens": ["Beim", "Schwe\u00b7fel", "und", "bei", "Kaf\u00b7fee", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und morgen sollt' die Hochzeit sein,", "tokens": ["Und", "mor\u00b7gen", "sollt'", "die", "Hoch\u00b7zeit", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "ART", "NN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und ich soll sterben? \u2013 O weh'!", "tokens": ["Und", "ich", "soll", "ster\u00b7ben", "?", "\u2013", "O", "weh'", "!"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "VVINF", "$.", "$(", "NE", "PTKVZ", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.8": {"line.1": {"text": "\u00bbwenn die Franzosen nun keinen Spa\u00df verstehn?", "tokens": ["\u00bb", "wenn", "die", "Fran\u00b7zo\u00b7sen", "nun", "kei\u00b7nen", "Spa\u00df", "ver\u00b7stehn", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "ART", "NN", "ADV", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Die Kerls sind indiskret! \u2013", "tokens": ["Die", "Kerls", "sind", "in\u00b7dis\u00b7kret", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ach, la\u00df sie sich schlagen \u2013 wir k\u00fcssen,", "tokens": ["Ach", ",", "la\u00df", "sie", "sich", "schla\u00b7gen", "\u2013", "wir", "k\u00fcs\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$,", "VVIMP", "PPER", "PRF", "VVINF", "$(", "PPER", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Gib acht, da\u00df es ohne mich geht.\u00ab \u2013", "tokens": ["Gib", "acht", ",", "da\u00df", "es", "oh\u00b7ne", "mich", "geht", ".", "\u00ab", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["VVIMP", "CARD", "$,", "KOUS", "PPER", "APPR", "PPER", "VVFIN", "$.", "$(", "$("], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.9": {"line.1": {"text": "\u00bbei, willst du mein Treuliebster sein,", "tokens": ["\u00bb", "ei", ",", "willst", "du", "mein", "Treu\u00b7liebs\u00b7ter", "sein", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ITJ", "$,", "VMFIN", "PPER", "PPOSAT", "NN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So zeige dich als Mann,", "tokens": ["So", "zei\u00b7ge", "dich", "als", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "KOUS", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Denn der versteht das Lieben nicht,", "tokens": ["Denn", "der", "ver\u00b7steht", "das", "Lie\u00b7ben", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "VVFIN", "ART", "ADJA", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der nicht auch zuschlagen kann.\u00ab \u2013", "tokens": ["Der", "nicht", "auch", "zu\u00b7schla\u00b7gen", "kann", ".", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ART", "PTKNEG", "ADV", "VVINF", "VMFIN", "$.", "$(", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "\u00bbnun! soll's denn sein und mu\u00df es sein \u2013", "tokens": ["\u00bb", "nun", "!", "soll's", "denn", "sein", "und", "mu\u00df", "es", "sein", "\u2013"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "$.", "VMFIN", "ADV", "VAINF", "KON", "VMFIN", "PPER", "VAINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "O Welt, wie bist du verkehrt! \u2013", "tokens": ["O", "Welt", ",", "wie", "bist", "du", "ver\u00b7kehrt", "!", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "NN", "$,", "PWAV", "VAFIN", "PPER", "VVPP", "$.", "$("], "meter": "-+-++-+", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Nun, Mutter, so kauf' mir ein B\u00fcchsenrohr,", "tokens": ["Nun", ",", "Mut\u00b7ter", ",", "so", "kauf'", "mir", "ein", "B\u00fcch\u00b7sen\u00b7rohr", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "NN", "$,", "ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Ach Gott, und kauf' mir ein Schwert!", "tokens": ["Ach", "Gott", ",", "und", "kauf'", "mir", "ein", "Schwert", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "$,", "KON", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.11": {"line.1": {"text": "\u00bbund kauf' mir ein Pfund Kourage \u2013", "tokens": ["\u00bb", "und", "kauf'", "mir", "ein", "Pfund", "Kou\u00b7ra\u00b7ge", "\u2013"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "VVFIN", "PPER", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das Geld wend' ich gerne dran \u2013", "tokens": ["Das", "Geld", "wend'", "ich", "ger\u00b7ne", "dran", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "PAV", "$("], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Und kauf' mir ein gro\u00dfes Rosinenfa\u00df,", "tokens": ["Und", "kauf'", "mir", "ein", "gro\u00b7\u00dfes", "Ro\u00b7si\u00b7nen\u00b7fa\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "In das ich mich kriechen kann.", "tokens": ["In", "das", "ich", "mich", "krie\u00b7chen", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "PRF", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "\u00bbund packe mich in Baumwolle ein,", "tokens": ["\u00bb", "und", "pa\u00b7cke", "mich", "in", "Baum\u00b7wol\u00b7le", "ein", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "VVFIN", "PRF", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Wenn ich marschieren mu\u00df,", "tokens": ["Wenn", "ich", "mar\u00b7schie\u00b7ren", "mu\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dann nehm' ich's mit dem St\u00e4rksten auf \u2013", "tokens": ["Dann", "nehm'", "ich's", "mit", "dem", "St\u00e4rks\u00b7ten", "auf", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "APPR", "ART", "NN", "APPR", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Pautz!! \u2013 Gott, da f\u00e4llt ein Schu\u00df!\u00ab \u2013", "tokens": ["Pautz", "!!", "\u2013", "Gott", ",", "da", "f\u00e4llt", "ein", "Schu\u00df", "!", "\u00ab", "\u2013"], "token_info": ["word", "punct", "punct", "word", "punct", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["NN", "$.", "$(", "NN", "$,", "ADV", "VVFIN", "ART", "NN", "$.", "$(", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "\u00bbsei au\u00dfer Angst, mein S\u00f6hnchen,", "tokens": ["\u00bb", "sei", "au\u00b7\u00dfer", "Angst", ",", "mein", "S\u00f6hn\u00b7chen", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "VAFIN", "APPR", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "'s war nur die Kellerth\u00fcr! \u2013", "tokens": ["'s", "war", "nur", "die", "Kel\u00b7lert\u00b7h\u00fcr", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Herr Jesus, du wirst ja ganz leichenbla\u00df!", "tokens": ["Herr", "Je\u00b7sus", ",", "du", "wirst", "ja", "ganz", "lei\u00b7chen\u00b7bla\u00df", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "PPER", "VAFIN", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Geschwind, und nimm ein Klystier!\u00ab", "tokens": ["Ge\u00b7schwind", ",", "und", "nimm", "ein", "Klys\u00b7tier", "!", "\u00ab"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "$,", "KON", "VVIMP", "ART", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Und als er wieder zu sich kam,", "tokens": ["Und", "als", "er", "wie\u00b7der", "zu", "sich", "kam", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "APPR", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zu alter Heldenkraft,", "tokens": ["Zu", "al\u00b7ter", "Hel\u00b7den\u00b7kraft", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da nahm er Abschied von der Braut \u2013", "tokens": ["Da", "nahm", "er", "Ab\u00b7schied", "von", "der", "Braut", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und fra\u00df Lokriziensaft!", "tokens": ["Und", "fra\u00df", "Lo\u00b7kri\u00b7zi\u00b7en\u00b7saft", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.15": {"line.1": {"text": "Kourage! Kourage!", "tokens": ["Kou\u00b7ra\u00b7ge", "!", "Kou\u00b7ra\u00b7ge", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$.", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "Du machst mich ganz schachmatt.", "tokens": ["Du", "machst", "mich", "ganz", "schach\u00b7matt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wer war denn nur der Teufelskerl,", "tokens": ["Wer", "war", "denn", "nur", "der", "Teu\u00b7fels\u00b7kerl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der dich erfunden hat?", "tokens": ["Der", "dich", "er\u00b7fun\u00b7den", "hat", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.16": {"line.1": {"text": "\u00bbmama, ich soll Soldate werd'n,", "tokens": ["\u00bb", "ma\u00b7ma", ",", "ich", "soll", "Sol\u00b7da\u00b7te", "werd'n", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ITJ", "$,", "PPER", "VMFIN", "NN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mama, da wird nichts draus,", "tokens": ["Ma\u00b7ma", ",", "da", "wird", "nichts", "draus", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "VAFIN", "PIS", "PAV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie schie\u00dfen einem im Dunkeln", "tokens": ["Sie", "schie\u00b7\u00dfen", "ei\u00b7nem", "im", "Dun\u00b7keln"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "APPRART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Wohl gar ein Auge aus.", "tokens": ["Wohl", "gar", "ein", "Au\u00b7ge", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.17": {"line.1": {"text": "\u00bbich soll 'ne Flinte tragen,", "tokens": ["\u00bb", "ich", "soll", "'ne", "Flin\u00b7te", "tra\u00b7gen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Solch feiner Leute Kind!", "tokens": ["Solch", "fei\u00b7ner", "Leu\u00b7te", "Kind", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mama, sie wissen's noch gar nicht,", "tokens": ["Ma\u00b7ma", ",", "sie", "wis\u00b7sen's", "noch", "gar", "nicht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VVFIN", "ADV", "ADV", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Was d'Franzosen f\u00fcr Flegel sind.\u00ab \u2013", "tokens": ["Was", "d'\u00b7Fran\u00b7zo\u00b7sen", "f\u00fcr", "Fle\u00b7gel", "sind", ".", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PWS", "PDS", "APPR", "NN", "VAFIN", "$.", "$(", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.18": {"line.1": {"text": "\u00bbmein Thomas! bleib' zu Hause!", "tokens": ["\u00bb", "mein", "Tho\u00b7mas", "!", "bleib'", "zu", "Hau\u00b7se", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NE", "$.", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Pfui! Blut! ein schlechter Durst.", "tokens": ["Pfui", "!", "Blut", "!", "ein", "schlech\u00b7ter", "Durst", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$.", "NN", "$.", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich lass' dir ein \u00c4pfelchen braten", "tokens": ["Ich", "lass'", "dir", "ein", "\u00c4p\u00b7fel\u00b7chen", "bra\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und kauf' dir 'ne Braunschweiger Wurst.\u00ab \u2013", "tokens": ["Und", "kauf'", "dir", "'ne", "Braun\u00b7schwei\u00b7ger", "Wurst", ".", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "ADJA", "NN", "$.", "$(", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.19": {"line.1": {"text": "\u00bbfein's Liebchen, ich wollt' Soldat werd'n,", "tokens": ["\u00bb", "fein's", "Lieb\u00b7chen", ",", "ich", "wollt'", "Sol\u00b7dat", "werd'n", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJA", "NN", "$,", "PPER", "VMFIN", "NN", "VAINF", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Die Mutter leidet's nicht! \u2013", "tokens": ["Die", "Mut\u00b7ter", "lei\u00b7det's", "nicht", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da mu\u00df ich's lassen bleiben,", "tokens": ["Da", "mu\u00df", "ich's", "las\u00b7sen", "blei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIS", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "'s w\u00e4r' wider die Kindespflicht.\u00ab \u2013", "tokens": ["'", "s", "w\u00e4r", "'", "wi\u00b7der", "die", "Kin\u00b7des\u00b7pflicht", ".", "\u00ab", "\u2013"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct", "punct", "punct"], "pos": ["$(", "PPER", "VAFIN", "$(", "APPR", "ART", "NN", "$.", "$(", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.20": {"line.1": {"text": "\u00bbdie Kindespflicht mu\u00df weichen,", "tokens": ["\u00bb", "die", "Kin\u00b7des\u00b7pflicht", "mu\u00df", "wei\u00b7chen", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zum Kampfe, trauter Thoms!", "tokens": ["Zum", "Kamp\u00b7fe", ",", "trau\u00b7ter", "Thoms", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sieg' oder stirb f\u00fcrs Vaterland,", "tokens": ["Sieg'", "o\u00b7der", "stirb", "f\u00fcrs", "Va\u00b7ter\u00b7land", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie Heldens\u00f6hne Roms!\u00ab \u2013", "tokens": ["Wie", "Hel\u00b7den\u00b7s\u00f6h\u00b7ne", "Roms", "!", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct", "punct"], "pos": ["PWAV", "NN", "NE", "$.", "$(", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.21": {"line.1": {"text": "\u00bbich hab ja mein gewisses Brot", "tokens": ["\u00bb", "ich", "hab", "ja", "mein", "ge\u00b7wis\u00b7ses", "Brot"], "token_info": ["punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "PPER", "VAFIN", "ADV", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Beim Schwefel und bei Kaffee,", "tokens": ["Beim", "Schwe\u00b7fel", "und", "bei", "Kaf\u00b7fee", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und morgen sollt' die Hochzeit sein,", "tokens": ["Und", "mor\u00b7gen", "sollt'", "die", "Hoch\u00b7zeit", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "ART", "NN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und ich soll sterben? \u2013 O weh'!", "tokens": ["Und", "ich", "soll", "ster\u00b7ben", "?", "\u2013", "O", "weh'", "!"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "VVINF", "$.", "$(", "NE", "PTKVZ", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.22": {"line.1": {"text": "\u00bbwenn die Franzosen nun keinen Spa\u00df verstehn?", "tokens": ["\u00bb", "wenn", "die", "Fran\u00b7zo\u00b7sen", "nun", "kei\u00b7nen", "Spa\u00df", "ver\u00b7stehn", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "ART", "NN", "ADV", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Die Kerls sind indiskret! \u2013", "tokens": ["Die", "Kerls", "sind", "in\u00b7dis\u00b7kret", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ach, la\u00df sie sich schlagen \u2013 wir k\u00fcssen,", "tokens": ["Ach", ",", "la\u00df", "sie", "sich", "schla\u00b7gen", "\u2013", "wir", "k\u00fcs\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$,", "VVIMP", "PPER", "PRF", "VVINF", "$(", "PPER", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Gib acht, da\u00df es ohne mich geht.\u00ab \u2013", "tokens": ["Gib", "acht", ",", "da\u00df", "es", "oh\u00b7ne", "mich", "geht", ".", "\u00ab", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["VVIMP", "CARD", "$,", "KOUS", "PPER", "APPR", "PPER", "VVFIN", "$.", "$(", "$("], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.23": {"line.1": {"text": "\u00bbei, willst du mein Treuliebster sein,", "tokens": ["\u00bb", "ei", ",", "willst", "du", "mein", "Treu\u00b7liebs\u00b7ter", "sein", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ITJ", "$,", "VMFIN", "PPER", "PPOSAT", "NN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So zeige dich als Mann,", "tokens": ["So", "zei\u00b7ge", "dich", "als", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "KOUS", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Denn der versteht das Lieben nicht,", "tokens": ["Denn", "der", "ver\u00b7steht", "das", "Lie\u00b7ben", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "VVFIN", "ART", "ADJA", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der nicht auch zuschlagen kann.\u00ab \u2013", "tokens": ["Der", "nicht", "auch", "zu\u00b7schla\u00b7gen", "kann", ".", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ART", "PTKNEG", "ADV", "VVINF", "VMFIN", "$.", "$(", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.24": {"line.1": {"text": "\u00bbnun! soll's denn sein und mu\u00df es sein \u2013", "tokens": ["\u00bb", "nun", "!", "soll's", "denn", "sein", "und", "mu\u00df", "es", "sein", "\u2013"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "$.", "VMFIN", "ADV", "VAINF", "KON", "VMFIN", "PPER", "VAINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "O Welt, wie bist du verkehrt! \u2013", "tokens": ["O", "Welt", ",", "wie", "bist", "du", "ver\u00b7kehrt", "!", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "NN", "$,", "PWAV", "VAFIN", "PPER", "VVPP", "$.", "$("], "meter": "-+-++-+", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Nun, Mutter, so kauf' mir ein B\u00fcchsenrohr,", "tokens": ["Nun", ",", "Mut\u00b7ter", ",", "so", "kauf'", "mir", "ein", "B\u00fcch\u00b7sen\u00b7rohr", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "NN", "$,", "ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Ach Gott, und kauf' mir ein Schwert!", "tokens": ["Ach", "Gott", ",", "und", "kauf'", "mir", "ein", "Schwert", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "$,", "KON", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.25": {"line.1": {"text": "\u00bbund kauf' mir ein Pfund Kourage \u2013", "tokens": ["\u00bb", "und", "kauf'", "mir", "ein", "Pfund", "Kou\u00b7ra\u00b7ge", "\u2013"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "VVFIN", "PPER", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das Geld wend' ich gerne dran \u2013", "tokens": ["Das", "Geld", "wend'", "ich", "ger\u00b7ne", "dran", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "PAV", "$("], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Und kauf' mir ein gro\u00dfes Rosinenfa\u00df,", "tokens": ["Und", "kauf'", "mir", "ein", "gro\u00b7\u00dfes", "Ro\u00b7si\u00b7nen\u00b7fa\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "In das ich mich kriechen kann.", "tokens": ["In", "das", "ich", "mich", "krie\u00b7chen", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "PRF", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.26": {"line.1": {"text": "\u00bbund packe mich in Baumwolle ein,", "tokens": ["\u00bb", "und", "pa\u00b7cke", "mich", "in", "Baum\u00b7wol\u00b7le", "ein", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "VVFIN", "PRF", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Wenn ich marschieren mu\u00df,", "tokens": ["Wenn", "ich", "mar\u00b7schie\u00b7ren", "mu\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dann nehm' ich's mit dem St\u00e4rksten auf \u2013", "tokens": ["Dann", "nehm'", "ich's", "mit", "dem", "St\u00e4rks\u00b7ten", "auf", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "APPR", "ART", "NN", "APPR", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Pautz!! \u2013 Gott, da f\u00e4llt ein Schu\u00df!\u00ab \u2013", "tokens": ["Pautz", "!!", "\u2013", "Gott", ",", "da", "f\u00e4llt", "ein", "Schu\u00df", "!", "\u00ab", "\u2013"], "token_info": ["word", "punct", "punct", "word", "punct", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["NN", "$.", "$(", "NN", "$,", "ADV", "VVFIN", "ART", "NN", "$.", "$(", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.27": {"line.1": {"text": "\u00bbsei au\u00dfer Angst, mein S\u00f6hnchen,", "tokens": ["\u00bb", "sei", "au\u00b7\u00dfer", "Angst", ",", "mein", "S\u00f6hn\u00b7chen", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "VAFIN", "APPR", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "'s war nur die Kellerth\u00fcr! \u2013", "tokens": ["'s", "war", "nur", "die", "Kel\u00b7lert\u00b7h\u00fcr", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Herr Jesus, du wirst ja ganz leichenbla\u00df!", "tokens": ["Herr", "Je\u00b7sus", ",", "du", "wirst", "ja", "ganz", "lei\u00b7chen\u00b7bla\u00df", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "PPER", "VAFIN", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Geschwind, und nimm ein Klystier!\u00ab", "tokens": ["Ge\u00b7schwind", ",", "und", "nimm", "ein", "Klys\u00b7tier", "!", "\u00ab"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "$,", "KON", "VVIMP", "ART", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.28": {"line.1": {"text": "Und als er wieder zu sich kam,", "tokens": ["Und", "als", "er", "wie\u00b7der", "zu", "sich", "kam", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "APPR", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zu alter Heldenkraft,", "tokens": ["Zu", "al\u00b7ter", "Hel\u00b7den\u00b7kraft", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da nahm er Abschied von der Braut \u2013", "tokens": ["Da", "nahm", "er", "Ab\u00b7schied", "von", "der", "Braut", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"