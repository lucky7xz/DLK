 ["ADV", "ADJD", "APPR", "PIS", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.46": {"text": "Und wenns die Scheelsucht bitterlich verdreu\u00dft,", "tokens": ["Und", "wenns", "die", "Scheel\u00b7sucht", "bit\u00b7ter\u00b7lich", "ver\u00b7dreu\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.47": {"text": "So darf, so will, so mu\u00df ichs dennoch sagen:", "tokens": ["So", "darf", ",", "so", "will", ",", "so", "mu\u00df", "ichs", "den\u00b7noch", "sa\u00b7gen", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "$,", "ADV", "VMFIN", "$,", "ADV", "VMFIN", "PIS", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.48": {"text": "In dieses Weibes Herz verl\u00e4ugnete sich nie", "tokens": ["In", "die\u00b7ses", "Wei\u00b7bes", "Herz", "ver\u00b7l\u00e4ug\u00b7ne\u00b7te", "sich", "nie"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "NN", "VVFIN", "PRF", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.49": {"text": "Die th\u00e4tigste Philosophie! \u2014", "tokens": ["Die", "th\u00e4\u00b7tigs\u00b7te", "Phi\u00b7lo\u00b7so\u00b7phie", "!"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}