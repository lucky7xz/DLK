{"dta.poem.20686": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "An  \n  eine adliche Schuldnerin,  \n f\u00fcr welche sich die Dichterin verb\u00fcrgt hatte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1792", "urn": "urn:nbn:de:kobv:b4-200905193016", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Weg ist nun eine Woche schon,                 ", "tokens": ["Weg", "ist", "nun", "ei\u00b7ne", "Wo\u00b7che", "schon", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zwei Wochen werden auch verschwinden \u2014", "tokens": ["Zwei", "Wo\u00b7chen", "wer\u00b7den", "auch", "ver\u00b7schwin\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VAFIN", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Nach Deiner Kummerbitte Ton", "tokens": ["Nach", "Dei\u00b7ner", "Kum\u00b7mer\u00b7bit\u00b7te", "Ton"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Verschwand die erste Woche schon,", "tokens": ["Ver\u00b7schwand", "die", "ers\u00b7te", "Wo\u00b7che", "schon", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Woher soll sich Bezahlung finden?", "tokens": ["Wo\u00b7her", "soll", "sich", "Be\u00b7zah\u00b7lung", "fin\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PRF", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Du machest ein Geheimni\u00df mir", "tokens": ["Du", "ma\u00b7chest", "ein", "Ge\u00b7heim\u00b7ni\u00df", "mir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Aus der vorgeblich gro\u00dfen Sache,", "tokens": ["Aus", "der", "vor\u00b7geb\u00b7lich", "gro\u00b7\u00dfen", "Sa\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ein Abgesandter kam von Dir,", "tokens": ["Ein", "Ab\u00b7ge\u00b7sand\u00b7ter", "kam", "von", "Dir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Der sagte das Geheimni\u00df laut,", "tokens": ["Der", "sag\u00b7te", "das", "Ge\u00b7heim\u00b7ni\u00df", "laut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Weil ich ihn frug, worauf die Hoffnung sey gebaut?", "tokens": ["Weil", "ich", "ihn", "frug", ",", "wo\u00b7rauf", "die", "Hoff\u00b7nung", "sey", "ge\u00b7baut", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "$,", "PWAV", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Da nannt\u2019 er ein Arcanum mir, und prahlte", "tokens": ["Da", "nannt'", "er", "ein", "Ar\u00b7ca\u00b7num", "mir", ",", "und", "prahl\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "PPER", "$,", "KON", "VVFIN"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "Er sey selbst bei der Sache intressirt,", "tokens": ["Er", "sey", "selbst", "bei", "der", "Sa\u00b7che", "in\u00b7tres\u00b7sirt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Er, dessen alt Gewand kein Mensch mit Dank bezahlte,", "tokens": ["Er", ",", "des\u00b7sen", "alt", "Ge\u00b7wand", "kein", "Mensch", "mit", "Dank", "be\u00b7zahl\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELAT", "ADJD", "NN", "PIAT", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Der nicht den Stab des Mangels f\u00fchrt \u2014", "tokens": ["Der", "nicht", "den", "Stab", "des", "Man\u00b7gels", "f\u00fchrt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "ART", "NN", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Wahrhaftig, wenn Dein magrer Bote", "tokens": ["Wahr\u00b7haf\u00b7tig", ",", "wenn", "Dein", "mag\u00b7rer", "Bo\u00b7te"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "$,", "KOUS", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Ein Mann von den drei M\u00e4nnern ist,", "tokens": ["Ein", "Mann", "von", "den", "drei", "M\u00e4n\u00b7nern", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "CARD", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Mit welchen Du gesch\u00e4ftig bist,", "tokens": ["Mit", "wel\u00b7chen", "Du", "ge\u00b7sch\u00e4f\u00b7tig", "bist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "PPER", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "An dem Arcanum, das von \u00e4chtem Korn und Schroote", "tokens": ["An", "dem", "Ar\u00b7ca\u00b7num", ",", "das", "von", "\u00e4ch\u00b7tem", "Korn", "und", "Schroo\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "APPR", "ADJA", "NN", "KON", "NN"], "meter": "--+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.19": {"text": "Dir blanke Thaler bringen soll:", "tokens": ["Dir", "blan\u00b7ke", "Tha\u00b7ler", "brin\u00b7gen", "soll", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Dann steht es mi\u00dflich um die Leute,", "tokens": ["Dann", "steht", "es", "mi\u00df\u00b7lich", "um", "die", "Leu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Die sich verlie\u00dfen hoffnungsvoll,", "tokens": ["Die", "sich", "ver\u00b7lie\u00b7\u00dfen", "hoff\u00b7nungs\u00b7voll", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Da\u00df Deine Miene Gold bedeute,", "tokens": ["Da\u00df", "Dei\u00b7ne", "Mie\u00b7ne", "Gold", "be\u00b7deu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Wenn Du Dir solch ein Ansehn gabst,", "tokens": ["Wenn", "Du", "Dir", "solch", "ein", "An\u00b7sehn", "gabst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "PIAT", "ART", "NN", "VVFIN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.24": {"text": "Als ob Du gro\u00dfe Renten sicher", "tokens": ["Als", "ob", "Du", "gro\u00b7\u00dfe", "Ren\u00b7ten", "si\u00b7cher"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "PPER", "ADJA", "NN", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Aus gro\u00dfen hohen H\u00e4nden habst \u2014", "tokens": ["Aus", "gro\u00b7\u00dfen", "ho\u00b7hen", "H\u00e4n\u00b7den", "habst"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADJA", "NN", "VAFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Sprich, was studierst Du doch f\u00fcr B\u00fccher:", "tokens": ["Sprich", ",", "was", "stu\u00b7dierst", "Du", "doch", "f\u00fcr", "B\u00fc\u00b7cher", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "PWS", "VVFIN", "PPER", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.27": {"text": "Von deutscher Wolle willst Du nun", "tokens": ["Von", "deut\u00b7scher", "Wol\u00b7le", "willst", "Du", "nun"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VMFIN", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Die feinste Spansche Wolle machen?", "tokens": ["Die", "feins\u00b7te", "Span\u00b7sche", "Wol\u00b7le", "ma\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NE", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.29": {"text": "Du willst ein Wunderwerkchen thun?", "tokens": ["Du", "willst", "ein", "Wun\u00b7der\u00b7werk\u00b7chen", "thun", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "Ich lache bei sehr wenig Sachen,", "tokens": ["Ich", "la\u00b7che", "bei", "sehr", "we\u00b7nig", "Sa\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.31": {"text": "Doch bei ", "tokens": ["Doch", "bei"], "token_info": ["word", "word"], "pos": ["KON", "APPR"], "meter": "++", "measure": "spondeus"}}, "stanza.2": {"line.1": {"text": "Zwingt Dich die Drangsaal nicht zu schon gewohnter", "tokens": ["Zwingt", "Dich", "die", "Drang\u00b7saal", "nicht", "zu", "schon", "ge\u00b7wohn\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKNEG", "PTKA", "ADV", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "List;", "tokens": ["List", ";"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+", "measure": "single.up"}, "line.3": {"text": "So wird Dirs dennoch gehn wie manchem Alchimist,", "tokens": ["So", "wird", "Dirs", "den\u00b7noch", "gehn", "wie", "man\u00b7chem", "Al\u00b7chi\u00b7mist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NN", "ADV", "VVINF", "KOKOM", "PIAT", "NN", "$,"], "meter": "-+---+-+-+-+", "measure": "dactylic.init"}, "line.4": {"text": "Der Tag und Nacht darauf verwendet,", "tokens": ["Der", "Tag", "und", "Nacht", "da\u00b7rauf", "ver\u00b7wen\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "PAV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und wenn er nun das lezte Werk vollendet,", "tokens": ["Und", "wenn", "er", "nun", "das", "lez\u00b7te", "Werk", "voll\u00b7en\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wenn nun das Wunder soll geschehn,", "tokens": ["Wenn", "nun", "das", "Wun\u00b7der", "soll", "ge\u00b7schehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Veredeltes Metall im Tiegel", "tokens": ["Ver\u00b7e\u00b7del\u00b7tes", "Me\u00b7tall", "im", "Tie\u00b7gel"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Mit rothem Goldesglanz zu sehn,", "tokens": ["Mit", "ro\u00b7them", "Gol\u00b7des\u00b7glanz", "zu", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Ach, dann bekommt der K\u00f6nig Fl\u00fcgel,", "tokens": ["Ach", ",", "dann", "be\u00b7kommt", "der", "K\u00f6\u00b7nig", "Fl\u00fc\u00b7gel", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADV", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Flieht aus dem Laber\u2019torium", "tokens": ["Flieht", "aus", "dem", "La\u00b7ber'\u00b7to\u00b7ri\u00b7um"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "In alle L\u00fcfte ringsherum", "tokens": ["In", "al\u00b7le", "L\u00fcf\u00b7te", "rings\u00b7he\u00b7rum"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Und nichts bleibt als ein Aschenh\u00fcgel.", "tokens": ["Und", "nichts", "bleibt", "als", "ein", "A\u00b7schen\u00b7h\u00fc\u00b7gel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Der Laborant wird kummerstumm \u2014", "tokens": ["Der", "La\u00b7bo\u00b7rant", "wird", "kum\u00b7mer\u00b7stumm"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Da\u00df Gott erbarm! so kann Dir\u2019s auch ergehen,", "tokens": ["Da\u00df", "Gott", "er\u00b7barm", "!", "so", "kann", "Dir's", "auch", "er\u00b7ge\u00b7hen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADJD", "$.", "ADV", "VMFIN", "PIS", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Dann giebst Du Deinem Schicksal Schuld,", "tokens": ["Dann", "giebst", "Du", "Dei\u00b7nem", "Schick\u00b7sal", "Schuld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Und alle Gl\u00e4ubiger verlieren die Geduld,", "tokens": ["Und", "al\u00b7le", "Gl\u00e4u\u00b7bi\u00b7ger", "ver\u00b7lie\u00b7ren", "die", "Ge\u00b7duld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Und ich soll f\u00fcr die Summa stehen,", "tokens": ["Und", "ich", "soll", "f\u00fcr", "die", "Sum\u00b7ma", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Die Du bei St*zen aufgeborgt.", "tokens": ["Die", "Du", "bei", "St", "*", "zen", "auf\u00b7ge\u00b7borgt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NN", "XY", "VVINF", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Nie darf ich\u2019s wagen unbesorgt", "tokens": ["Nie", "darf", "ich's", "wa\u00b7gen", "un\u00b7be\u00b7sorgt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PIS", "VVINF", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Vor innerlicher Schaam dies Haus noch zu betreten,", "tokens": ["Vor", "in\u00b7ner\u00b7li\u00b7cher", "Schaam", "dies", "Haus", "noch", "zu", "be\u00b7tre\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PDS", "NN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Wo Du Dir Silber ausgebeten,", "tokens": ["Wo", "Du", "Dir", "Sil\u00b7ber", "aus\u00b7ge\u00b7be\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Weil Dich mein redlich Herz empfahl.", "tokens": ["Weil", "Dich", "mein", "red\u00b7lich", "Herz", "emp\u00b7fahl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "ADJD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Gew\u00f6hne Dich zur Wahrheit doch einmahl", "tokens": ["Ge\u00b7w\u00f6h\u00b7ne", "Dich", "zur", "Wahr\u00b7heit", "doch", "ein\u00b7mahl"], "tok