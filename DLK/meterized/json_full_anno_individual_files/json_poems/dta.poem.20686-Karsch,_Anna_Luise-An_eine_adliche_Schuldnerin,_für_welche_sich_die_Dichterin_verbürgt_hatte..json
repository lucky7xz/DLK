}, "line.4": {"text": "Wird Deinem Wappen Schande machen;", "tokens": ["Wird", "Dei\u00b7nem", "Wap\u00b7pen", "Schan\u00b7de", "ma\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und flistern mu\u00df ich Dir ins Ohr:", "tokens": ["Und", "flis\u00b7tern", "mu\u00df", "ich", "Dir", "ins", "Ohr", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VMFIN", "PPER", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Redlichkeit geht allen Wappen vor,", "tokens": ["Die", "Red\u00b7lich\u00b7keit", "geht", "al\u00b7len", "Wap\u00b7pen", "vor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und allem Glanz von tausend Jahren.", "tokens": ["Und", "al\u00b7lem", "Glanz", "von", "tau\u00b7send", "Jah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "NN", "APPR", "CARD", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ich lie\u00dfe mich oft gern bei meine Freunde fahren,", "tokens": ["Ich", "lie\u00b7\u00dfe", "mich", "oft", "gern", "bei", "mei\u00b7ne", "Freun\u00b7de", "fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Denn sauer wird mir jeder Gang,", "tokens": ["Denn", "sau\u00b7er", "wird", "mir", "je\u00b7der", "Gang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "PPER", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Mein Eingeweid ist schwach, ich f\u00fchls bei jedem Tritte,", "tokens": ["Mein", "Ein\u00b7ge\u00b7weid", "ist", "schwach", ",", "ich", "f\u00fchls", "bei", "je\u00b7dem", "Trit\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$,", "PPER", "ADV", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Mu\u00df ruhen unterwegs oft Viertelstunden lang;", "tokens": ["Mu\u00df", "ru\u00b7hen", "un\u00b7ter\u00b7wegs", "oft", "Vier\u00b7tel\u00b7stun\u00b7den", "lang", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVINF", "ADV", "ADV", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und dennoch macht ich mirs zur Sitte", "tokens": ["Und", "den\u00b7noch", "macht", "ich", "mirs", "zur", "Sit\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "NE", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Kein Geld zu borgen, um dem Lohngedungnen Mann", "tokens": ["Kein", "Geld", "zu", "bor\u00b7gen", ",", "um", "dem", "Lohn\u00b7ge\u00b7dung\u00b7nen", "Mann"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "PTKZU", "VVINF", "$,", "KOUI", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Zu zahlen, der durch Vorgespann", "tokens": ["Zu", "zah\u00b7len", ",", "der", "durch", "Vor\u00b7ge\u00b7spann"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PTKZU", "VVINF", "$,", "PRELS", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Mich ganz bequem zu Freunden br\u00e4chte.", "tokens": ["Mich", "ganz", "be\u00b7quem", "zu", "Freun\u00b7den", "br\u00e4ch\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJD", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Noch weniger borgt ich mir Geld,", "tokens": ["Noch", "we\u00b7ni\u00b7ger", "borgt", "ich", "mir", "Geld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "PPER", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Damit man mich bemerken m\u00f6chte,", "tokens": ["Da\u00b7mit", "man", "mich", "be\u00b7mer\u00b7ken", "m\u00f6ch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PRF", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Wenn vorgefahren wird, und wenn der Kutscher", "tokens": ["Wenn", "vor\u00b7ge\u00b7fah\u00b7ren", "wird", ",", "und", "wenn", "der", "Kut\u00b7scher"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "VVINF", "VAFIN", "$,", "KON", "KOUS", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "h\u00e4lt \u2014", "tokens": ["h\u00e4lt"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "+", "measure": "single.up"}, "line.20": {"text": "Ich kenne schon, seit f\u00fcnf und zwanzig Jahren,", "tokens": ["Ich", "ken\u00b7ne", "schon", ",", "seit", "f\u00fcnf", "und", "zwan\u00b7zig", "Jah\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "APPR", "CARD", "KON", "CARD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Ein wirklich edles Weib ", "