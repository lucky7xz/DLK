{"textgrid.poem.38683": {"metadata": {"author": {"name": "Matthisson, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Phantasie", "genre": "verse", "period": "N.A.", "pub_year": 1792, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn der Morgen sich r\u00f6thet, wenn des Abends", "tokens": ["Wenn", "der", "Mor\u00b7gen", "sich", "r\u00f6\u00b7thet", ",", "wenn", "des", "A\u00b7bends"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "PRF", "VVFIN", "$,", "KOUS", "ART", "NN"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Goldgew\u00f6lke die Fichtenh\u00f6hn beleuchten,", "tokens": ["Gold\u00b7ge\u00b7w\u00f6l\u00b7ke", "die", "Fich\u00b7ten\u00b7h\u00f6hn", "be\u00b7leuch\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Wenn die heiligen Sterne schimmern, denk' ich", "tokens": ["Wenn", "die", "hei\u00b7li\u00b7gen", "Ster\u00b7ne", "schim\u00b7mern", ",", "denk'", "ich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVFIN", "$,", "VVFIN", "PPER"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Dein, o Geliebte!", "tokens": ["Dein", ",", "o", "Ge\u00b7lieb\u00b7te", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "$,", "FM", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Dann vernehm' ich, mit Geistesohr, die Laute", "tokens": ["Dann", "ver\u00b7nehm'", "ich", ",", "mit", "Geis\u00b7te\u00b7sohr", ",", "die", "Lau\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "APPR", "NN", "$,", "ART", "NN"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Deiner Grazienlippen; sanfter hallt nicht", "tokens": ["Dei\u00b7ner", "Gra\u00b7zi\u00b7en\u00b7lip\u00b7pen", ";", "sanf\u00b7ter", "hallt", "nicht"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$.", "ADJD", "VVFIN", "PTKNEG"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Aeols Harfenget\u00f6n' in Philomelens", "tokens": ["A\u00b7e\u00b7ols", "Ha\u00b7rfen\u00b7ge\u00b7t\u00f6n'", "in", "Phi\u00b7lo\u00b7me\u00b7lens"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "NE", "APPR", "NE"], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.4": {"text": "Wehmuthsakkorde.", "tokens": ["Weh\u00b7mut\u00b7hsak\u00b7kor\u00b7de", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "Dann erscheinen mir, im Erinnrungstraume,", "tokens": ["Dann", "er\u00b7schei\u00b7nen", "mir", ",", "im", "E\u00b7rinn\u00b7rungs\u00b7trau\u00b7me", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "APPRART", "NN", "$,"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Hirtenfluren, umragt von Alpengipfeln,", "tokens": ["Hir\u00b7ten\u00b7flu\u00b7ren", ",", "um\u00b7ragt", "von", "Al\u00b7pen\u00b7gip\u00b7feln", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Wo, nach Blumen zu sp\u00e4hn, du in des Aufgangs", "tokens": ["Wo", ",", "nach", "Blu\u00b7men", "zu", "sp\u00e4hn", ",", "du", "in", "des", "Auf\u00b7gangs"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "$,", "APPR", "NN", "PTKZU", "VVINF", "$,", "PPER", "APPR", "ART", "NN"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Glorie wandelst.", "tokens": ["Glo\u00b7rie", "wan\u00b7delst", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.4": {"line.1": {"text": "Dann beseligt mich, bei der Eiswelt Wundern,", "tokens": ["Dann", "be\u00b7se\u00b7ligt", "mich", ",", "bei", "der", "Eis\u00b7welt", "Wun\u00b7dern", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "APPR", "ART", "NN", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Dein Entz\u00fccken, und deiner Fr\u00fchlingswange", "tokens": ["Dein", "Ent\u00b7z\u00fc\u00b7cken", ",", "und", "dei\u00b7ner", "Fr\u00fch\u00b7lings\u00b7wan\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "KON", "PPOSAT", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Dunkler flammende R\u00f6the, bei des Montblancs", "tokens": ["Dunk\u00b7ler", "flam\u00b7men\u00b7de", "R\u00f6\u00b7the", ",", "bei", "des", "Mont\u00b7blancs"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "ADJA", "NN", "$,", "APPR", "ART", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Abendverkl\u00e4rung.", "tokens": ["A\u00b7bend\u00b7ver\u00b7kl\u00e4\u00b7rung", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.5": {"line.1": {"text": "Dann durchgleiten wir, in umsch\u00e4umter Barke,", "tokens": ["Dann", "durch\u00b7glei\u00b7ten", "wir", ",", "in", "um\u00b7sch\u00e4um\u00b7ter", "Bar\u00b7ke", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Des lemanischen Halbmonds gr\u00fcne Fluthen;", "tokens": ["Des", "le\u00b7ma\u00b7ni\u00b7schen", "Halb\u00b7monds", "gr\u00fc\u00b7ne", "Flut\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Froher spiegelt sich Ph\u00f6bus nirgends, froher", "tokens": ["Fro\u00b7her", "spie\u00b7gelt", "sich", "Ph\u00f6\u00b7bus", "nir\u00b7gends", ",", "fro\u00b7her"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["NN", "VVFIN", "PRF", "NE", "ADV", "$,", "ADJA"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Nirgends Diana.", "tokens": ["Nir\u00b7gends", "Di\u00b7a\u00b7na", "."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "NE", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "Dort bei Meillerie weihtest du dem J\u00fcngling,", "tokens": ["Dort", "bei", "Meil\u00b7le\u00b7rie", "weih\u00b7test", "du", "dem", "J\u00fcng\u00b7ling", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Den ein feindlicher Stern aus Amors Himmel", "tokens": ["Den", "ein", "feind\u00b7li\u00b7cher", "Stern", "aus", "A\u00b7mors", "Him\u00b7mel"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ART", "ADJA", "NN", "APPR", "NE", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "In den Tartarus \u00f6der Felsen bannte,", "tokens": ["In", "den", "Tar\u00b7ta\u00b7rus", "\u00f6\u00b7der", "Fel\u00b7sen", "bann\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Thr\u00e4nen des Mitleids.", "tokens": ["Thr\u00e4\u00b7nen", "des", "Mit\u00b7leids", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.7": {"line.1": {"text": "Hier, bei Juliens Dorf, am Burggem\u00e4uer,", "tokens": ["Hier", ",", "bei", "Ju\u00b7li\u00b7ens", "Dorf", ",", "am", "Burg\u00b7ge\u00b7m\u00e4u\u00b7er", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "$,", "APPR", "NE", "NE", "$,", "APPRART", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Windest gl\u00e4nzenden Epheu du zum Kranze,", "tokens": ["Win\u00b7dest", "gl\u00e4n\u00b7zen\u00b7den", "E\u00b7pheu", "du", "zum", "Kran\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "PPER", "APPRART", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Den, mit sinnendem Ernst, wir fromm der Unschuld", "tokens": ["Den", ",", "mit", "sin\u00b7nen\u00b7dem", "Ernst", ",", "wir", "fromm", "der", "Un\u00b7schuld"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "APPR", "ADJA", "NN", "$,", "PPER", "ADJD", "ART", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Genius opfern.", "tokens": ["Ge\u00b7nius", "op\u00b7fern", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.8": {"line.1": {"text": "Schon birgt hinter dem Jura sich die Sonne,", "tokens": ["Schon", "birgt", "hin\u00b7ter", "dem", "Ju\u00b7ra", "sich", "die", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "PRF", "ART", "NN", "$,"], "meter": "-++--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und mit bl\u00e4ulichem Scheine flimmt der Gl\u00fchwurm;", "tokens": ["Und", "mit", "bl\u00e4u\u00b7li\u00b7chem", "Schei\u00b7ne", "flimmt", "der", "Gl\u00fch\u00b7wurm", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Horch! des wirthlichen D\u00f6rfchens dumpfe Glocken", "tokens": ["Horch", "!", "des", "wirth\u00b7li\u00b7chen", "D\u00f6rf\u00b7chens", "dump\u00b7fe", "Glo\u00b7cken"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$.", "ART", "ADJA", "NN", "ADJA", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Mahnen zur Heimkehr.", "tokens": ["Mah\u00b7nen", "zur", "Heim\u00b7kehr", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.9": {"line.1": {"text": "Sonnen sinken und steigen; Lenze werden", "tokens": ["Son\u00b7nen", "sin\u00b7ken", "und", "stei\u00b7gen", ";", "Len\u00b7ze", "wer\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "VVINF", "KON", "VVINF", "$.", "NN", "VAINF"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Bl\u00fchn und sterben: Doch keine Morgenr\u00f6the,", "tokens": ["Bl\u00fchn", "und", "ster\u00b7ben", ":", "Doch", "kei\u00b7ne", "Mor\u00b7gen\u00b7r\u00f6\u00b7the", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVINF", "$.", "KON", "PIAT", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Doch kein kehrender Fr\u00fchling, ach! vereint auf", "tokens": ["Doch", "kein", "keh\u00b7ren\u00b7der", "Fr\u00fch\u00b7ling", ",", "ach", "!", "ver\u00b7eint", "auf"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["KON", "PIAT", "ADJA", "NN", "$,", "ITJ", "$.", "VVPP", "APPR"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Erden uns wieder.", "tokens": ["Er\u00b7den", "uns", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.10": {"line.1": {"text": "Unsre Pfade sind fern und weit geschieden!", "tokens": ["Uns\u00b7re", "Pfa\u00b7de", "sind", "fern", "und", "weit", "ge\u00b7schie\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "KON", "ADJD", "VVPP", "$."], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Bl\u00fcthen wehn auf den einen, d\u00fcrre Bl\u00e4tter", "tokens": ["Bl\u00fc\u00b7then", "wehn", "auf", "den", "ei\u00b7nen", ",", "d\u00fcr\u00b7re", "Bl\u00e4t\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "VVFIN", "APPR", "ART", "ART", "$,", "ADJA", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Auf den andern herab: Doch beid', o Wonne!", "tokens": ["Auf", "den", "an\u00b7dern", "her\u00b7ab", ":", "Doch", "beid'", ",", "o", "Won\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ADV", "$.", "KON", "PTKVZ", "$,", "FM", "NN", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Leiten zum Grabe.", "tokens": ["Lei\u00b7ten", "zum", "Gra\u00b7be", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.11": {"line.1": {"text": "Wenn das meine sich l\u00e4ngst, nur von Aurorens", "tokens": ["Wenn", "das", "mei\u00b7ne", "sich", "l\u00e4ngst", ",", "nur", "von", "Au\u00b7ro\u00b7rens"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PDS", "VVFIN", "PRF", "ADV", "$,", "ADV", "APPR", "NE"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Thr\u00e4nen einsam bethaut, mit Halmen deckte,", "tokens": ["Thr\u00e4\u00b7nen", "ein\u00b7sam", "be\u00b7thaut", ",", "mit", "Hal\u00b7men", "deck\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "VVPP", "$,", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Werden Mirten, o Freundin, dir der Locken", "tokens": ["Wer\u00b7den", "Mir\u00b7ten", ",", "o", "Freun\u00b7din", ",", "dir", "der", "Lo\u00b7cken"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "NN", "$,", "FM", "NN", "$,", "PPER", "ART", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "F\u00fclle noch kr\u00e4nzen!", "tokens": ["F\u00fcl\u00b7le", "noch", "kr\u00e4n\u00b7zen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVINF", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.12": {"line.1": {"text": "Dich, Vertraute der h\u00f6hern Welt, beschw\u00f6r' ich", "tokens": ["Dich", ",", "Ver\u00b7trau\u00b7te", "der", "h\u00f6\u00b7hern", "Welt", ",", "be\u00b7schw\u00f6r'", "ich"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "$,", "NN", "ART", "ADJA", "NN", "$,", "VVFIN", "PPER"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Beim unsterblichen Einklang edler Seelen:", "tokens": ["Beim", "uns\u00b7terb\u00b7li\u00b7chen", "Ein\u00b7klang", "ed\u00b7ler", "See\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "La\u00df im reinsten der Herzen dann des Freundes", "tokens": ["La\u00df", "im", "reins\u00b7ten", "der", "Her\u00b7zen", "dann", "des", "Freun\u00b7des"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "APPRART", "VVFIN", "ART", "NN", "ADV", "ART", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Bild nicht erbleichen!", "tokens": ["Bild", "nicht", "er\u00b7blei\u00b7chen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "VVINF", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.13": {"line.1": {"text": "Wenn der Morgen sich r\u00f6thet, wenn des Abends", "tokens": ["Wenn", "der", "Mor\u00b7gen", "sich", "r\u00f6\u00b7thet", ",", "wenn", "des", "A\u00b7bends"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "PRF", "VVFIN", "$,", "KOUS", "ART", "NN"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Goldgew\u00f6lke die Fichtenh\u00f6hn beleuchten,", "tokens": ["Gold\u00b7ge\u00b7w\u00f6l\u00b7ke", "die", "Fich\u00b7ten\u00b7h\u00f6hn", "be\u00b7leuch\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Wenn die heiligen Sterne schimmern, denk' ich", "tokens": ["Wenn", "die", "hei\u00b7li\u00b7gen", "Ster\u00b7ne", "schim\u00b7mern", ",", "denk'", "ich"],