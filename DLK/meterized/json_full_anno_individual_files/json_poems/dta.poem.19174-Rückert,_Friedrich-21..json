{"dta.poem.19174": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "21.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich gieng den Strom hinauf und forschte nach der Quelle,", "tokens": ["Ich", "gieng", "den", "Strom", "hin\u00b7auf", "und", "forschte", "nach", "der", "Quel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKVZ", "KON", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Aus deren Scho\u00dfe sich erg\u00f6sse jede Welle.", "tokens": ["Aus", "de\u00b7ren", "Scho\u00b7\u00dfe", "sich", "er\u00b7g\u00f6s\u00b7se", "je\u00b7de", "Wel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "PRF", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Je weiter aber ich hinaufkam, ward mir kund,", "tokens": ["Je", "wei\u00b7ter", "a\u00b7ber", "ich", "hin\u00b7auf\u00b7kam", ",", "ward", "mir", "kund", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "PPER", "VVFIN", "$,", "VAFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Statt einer Quelle sei's ein ganzer Quellengrund.", "tokens": ["Statt", "ei\u00b7ner", "Quel\u00b7le", "sei's", "ein", "gan\u00b7zer", "Quel\u00b7len\u00b7grund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "So, welcher Sache nach du forschen magst und graben,", "tokens": ["So", ",", "wel\u00b7cher", "Sa\u00b7che", "nach", "du", "for\u00b7schen", "magst", "und", "gra\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAT", "NN", "APPR", "PPER", "VVINF", "VMFIN", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Statt einen Grund wirst du gefunden viele haben.", "tokens": ["Statt", "ei\u00b7nen", "Grund", "wirst", "du", "ge\u00b7fun\u00b7den", "vie\u00b7le", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VAFIN", "PPER", "VVPP", "PIS", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}