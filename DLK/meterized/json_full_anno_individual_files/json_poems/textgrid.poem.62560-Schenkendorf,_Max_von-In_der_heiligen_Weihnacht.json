{"textgrid.poem.62560": {"metadata": {"author": {"name": "Schenkendorf, Max von", "birth": "N.A.", "death": "N.A."}, "title": "In der heiligen Weihnacht", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kommt ihr Hirten, kommt ihr Armen,", "tokens": ["Kommt", "ihr", "Hir\u00b7ten", ",", "kommt", "ihr", "Ar\u00b7men", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Seht das ewige Erbarmen,", "tokens": ["Seht", "das", "e\u00b7wi\u00b7ge", "Er\u00b7bar\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Das sich uns hat zugesellt,", "tokens": ["Das", "sich", "uns", "hat", "zu\u00b7ge\u00b7sellt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PRF", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Nicht den K\u00f6nigen der Erden,", "tokens": ["Nicht", "den", "K\u00f6\u00b7ni\u00b7gen", "der", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Hirten will er \u00e4hnlich werden,", "tokens": ["Hir\u00b7ten", "will", "er", "\u00e4hn\u00b7lich", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "ADJD", "VAINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Er, der Herr der ganzen Welt.", "tokens": ["Er", ",", "der", "Herr", "der", "gan\u00b7zen", "Welt", "."], "token_info": ["word", "punct", "