{"dta.poem.293": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Als  Doris  ihm gek\u00fcsset.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Mein Gem\u00fcth ist voller Freuden/", "tokens": ["Mein", "Ge\u00b7m\u00fcth", "ist", "vol\u00b7ler", "Freu\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und mein Geist am Himmel hafft/", "tokens": ["Und", "mein", "Geist", "am", "Him\u00b7mel", "hafft", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "APPRART", "NN", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Meine ", "tokens": ["Mei\u00b7ne"], "token_info": ["word"], "pos": ["PPOSAT"], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "Wo die Lust Vergn\u00fcgen schafft/", "tokens": ["Wo", "die", "Lust", "Ver\u00b7gn\u00fc\u00b7gen", "schafft", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "NN", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Jhren Mund darf ich bek\u00fcssen/", "tokens": ["Ih\u00b7ren", "Mund", "darf", "ich", "be\u00b7k\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PPER", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und auch geniessen", "tokens": ["Und", "auch", "ge\u00b7nies\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADV", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Von den Lippen ", "tokens": ["Von", "den", "Lip\u00b7pen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.2": {"line.1": {"text": "Milder Himmel mehr mein Gl\u00fccke", "tokens": ["Mil\u00b7der", "Him\u00b7mel", "mehr", "mein", "Gl\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "NN", "ADV", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "H\u00e4uffe meine Liebes-Lust!", "tokens": ["H\u00e4uf\u00b7fe", "mei\u00b7ne", "Lie\u00b7bes\u00b7Lust", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schenck mir Gnaden-reiche Blicke/", "tokens": ["Schenck", "mir", "Gna\u00b7den\u00b7rei\u00b7che", "Bli\u00b7cke", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mache mir den Ort bewust/", "tokens": ["Ma\u00b7che", "mir", "den", "Ort", "be\u00b7wust", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wo ", "tokens": ["Wo"], "token_info": ["word"], "pos": ["PWAV"], "meter": "+", "measure": "single.up"}, "line.6": {"text": "Jm Schnee stets gr\u00fcnen", "tokens": ["Jm", "Schnee", "stets", "gr\u00fc\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADV", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Auf den H\u00fcgeln ihrer Brust.", "tokens": ["Auf", "den", "H\u00fc\u00b7geln", "ih\u00b7rer", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Jhrer Lippen Wollust-Keller/", "tokens": ["Ih\u00b7rer", "Lip\u00b7pen", "Wol\u00b7lust\u00b7Kel\u00b7ler", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NE", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo berauschet Geist und Seel/", "tokens": ["Wo", "be\u00b7rau\u00b7schet", "Geist", "und", "Seel", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "NN", "KON", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schencket heissen ", "tokens": ["Schen\u00b7cket", "heis\u00b7sen"], "token_info": ["word", "word"], "pos": ["VVFIN", "VVINF"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Aus des Munds Corallen-H\u00f6hl/", "tokens": ["Aus", "des", "Munds", "Coral\u00b7len\u00b7H\u00f6hl", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NE", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und der Br\u00fcste reiffe Reben", "tokens": ["Und", "der", "Br\u00fcs\u00b7te", "reif\u00b7fe", "Re\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.6": {"text": "Sehr h\u00e4uffig geben", "tokens": ["Sehr", "h\u00e4uf\u00b7fig", "ge\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADJD", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Den so s\u00fcssen Liebes-Oehl.", "tokens": ["Den", "so", "s\u00fcs\u00b7sen", "Lie\u00b7bes\u00b7Oehl", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Doris deine zarten Lippen", "tokens": ["Do\u00b7ris", "dei\u00b7ne", "zar\u00b7ten", "Lip\u00b7pen"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zeigen meinem Schiff den Bay/", "tokens": ["Zei\u00b7gen", "mei\u00b7nem", "Schiff", "den", "Bay", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ART", "NE", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo es von den scharffen Klippen/", "tokens": ["Wo", "es", "von", "den", "scharf\u00b7fen", "Klip\u00b7pen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und von allen St\u00fcrmen frey;", "tokens": ["Und", "von", "al\u00b7len", "St\u00fcr\u00b7men", "frey", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PIAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "In dem sch\u00f6nsten Haven lieget", "tokens": ["In", "dem", "sch\u00f6ns\u00b7ten", "Ha\u00b7ven", "lie\u00b7get"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und sich vergn\u00fcget", "tokens": ["Und", "sich", "ver\u00b7gn\u00fc\u00b7get"], "token_info": ["word", "word", "word"], "pos": ["KON", "PRF", "VVFIN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "An ", "tokens": ["An"], "token_info": ["word"], "pos": ["APPR"], "meter": "+", "measure": "single.up"}}, "stanza.5": {"line.1": {"text": "Sp\u00fchrt man sonst wo harte Wetter/", "tokens": ["Sp\u00fchrt", "man", "sonst", "wo", "har\u00b7te", "Wet\u00b7ter", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "PWAV", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Oder da\u00df es Winter ist/", "tokens": ["O\u00b7der", "da\u00df", "es", "Win\u00b7ter", "ist", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "NN", "VAFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "So sind da doch Rosen-Bl\u00e4tter", "tokens": ["So", "sind", "da", "doch", "Ro\u00b7sen\u00b7Bl\u00e4t\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "ADV", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Die ein steter Fr\u00fchling k\u00fc\u00dft;", "tokens": ["Die", "ein", "ste\u00b7ter", "Fr\u00fch\u00b7ling", "k\u00fc\u00dft", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Da ist niemahls Blitz und Regen/", "tokens": ["Da", "ist", "nie\u00b7mahls", "Blitz", "und", "Re\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "NN", "KON", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Vor Donner-Schl\u00e4gen", "tokens": ["Vor", "Don\u00b7ner\u00b7Schl\u00e4\u00b7gen"], "token_info": ["word", "word"], "pos": ["APPR", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Man ein holdes Gl\u00fcck genie\u00dft.", "tokens": ["Man", "ein", "hol\u00b7des", "Gl\u00fcck", "ge\u00b7nie\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Diesen Vorschmack von dem Leben", "tokens": ["Die\u00b7sen", "Vor\u00b7schmack", "von", "dem", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDAT", "NN", "APPR", "ART", "NN"], "meter": "+-+---+-", "measure": "unknown.measure.tri"}, "line.2": {"text": "Und den Anfang solcher Freud/", "tokens": ["Und", "den", "An\u00b7fang", "sol\u00b7cher", "Freud", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PIAT", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mu\u00df der Lippen Kost uns geben", "tokens": ["Mu\u00df", "der", "Lip\u00b7pen", "Kost", "uns", "ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "ART", "NN", "NN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nebst der Br\u00fcste S\u00fcssigkeit/", "tokens": ["Nebst", "der", "Br\u00fcs\u00b7te", "S\u00fcs\u00b7sig\u00b7keit", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Diese f\u00fchren zu dem Lande", "tokens": ["Die\u00b7se", "f\u00fch\u00b7ren", "zu", "dem", "Lan\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und Wollust Strande/", "tokens": ["Und", "Wol\u00b7lust", "Stran\u00b7de", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "NN", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Wo ", "tokens": ["Wo"], "token_info": ["word"], "pos": ["PWAV"], "meter": "+", "measure": "single.up"}}}}}