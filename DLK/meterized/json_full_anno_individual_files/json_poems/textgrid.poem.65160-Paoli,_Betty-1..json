{"textgrid.poem.65160": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Geliebtes Kind! zum Trost, da\u00df ferne", "tokens": ["Ge\u00b7lieb\u00b7tes", "Kind", "!", "zum", "Trost", ",", "da\u00df", "fer\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$.", "APPRART", "NN", "$,", "KOUS", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Von dir die Welle mich verschlug,", "tokens": ["Von", "dir", "die", "Wel\u00b7le", "mich", "ver\u00b7schlug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie ruf' ich mir so oft, so gerne", "tokens": ["Wie", "ruf'", "ich", "mir", "so", "oft", ",", "so", "ger\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "PPER", "ADV", "ADV", "$,", "ADV", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zur\u00fcck dein Antlitz, Zug f\u00fcr Zug!", "tokens": ["Zu\u00b7r\u00fcck", "dein", "Ant\u00b7litz", ",", "Zug", "f\u00fcr", "Zug", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "$,", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Als w\u00e4rst du leibhaft mir erschienen", "tokens": ["Als", "w\u00e4rst", "du", "leib\u00b7haft", "mir", "er\u00b7schie\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "VAFIN", "PPER", "ADJD", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Stehst du vor mir, Geb\u00e4rd' und Mienen", "tokens": ["Stehst", "du", "vor", "mir", ",", "Ge\u00b7b\u00e4rd'", "und", "Mie\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "PPER", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "So hold, so sittig und so klug!", "tokens": ["So", "hold", ",", "so", "sit\u00b7tig", "und", "so", "klug", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADV", "ADJD", "KON", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ja! tief hab' ich es eingesogen,", "tokens": ["Ja", "!", "tief", "hab'", "ich", "es", "ein\u00b7ge\u00b7so\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "ADJD", "VAFIN", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dein Bild in meiner Seele Grund!", "tokens": ["Dein", "Bild", "in", "mei\u00b7ner", "See\u00b7le", "Grund", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich seh' der Stirne reinen Bogen,", "tokens": ["Ich", "seh'", "der", "Stir\u00b7ne", "rei\u00b7nen", "Bo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das zarte Kinn, den weichen Mund,", "tokens": ["Das", "zar\u00b7te", "Kinn", ",", "den", "wei\u00b7chen", "Mund", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Augen klare Lichtkristalle,", "tokens": ["Der", "Au\u00b7gen", "kla\u00b7re", "Licht\u00b7kris\u00b7tal\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Das blonde Haar, im lock'gen Falle", "tokens": ["Das", "blon\u00b7de", "Haar", ",", "im", "lock'\u00b7gen", "Fal\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Umspielend des Gesichtchens Rund!", "tokens": ["Um\u00b7spie\u00b7lend", "des", "Ge\u00b7sicht\u00b7chens", "Rund", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Sie m\u00f6gen neckend mir erwidern,", "tokens": ["Sie", "m\u00f6\u00b7gen", "ne\u00b7ckend", "mir", "er\u00b7wi\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADJD", "PPER", "VVINF", "$,"], "meter": "-+-----+-", "measure": "dactylic.init"}, "line.2": {"text": "Ein Sch\u00f6nheitwunder seist du nicht!", "tokens": ["Ein", "Sch\u00f6n\u00b7heit\u00b7wun\u00b7der", "seist", "du", "nicht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wer kann, wer mag den Reiz zergliedern,", "tokens": ["Wer", "kann", ",", "wer", "mag", "den", "Reiz", "zer\u00b7glie\u00b7dern", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "$,", "PWS", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der ihn mit s\u00fc\u00dfem Bann umflicht?", "tokens": ["Der", "ihn", "mit", "s\u00fc\u00b7\u00dfem", "Bann", "um\u00b7flicht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wer krittelnd erst durchsp\u00e4h'n die Z\u00fcge,", "tokens": ["Wer", "krit\u00b7telnd", "erst", "durch\u00b7sp\u00e4h'n", "die", "Z\u00fc\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Aus denen ohne Falsch und L\u00fcge", "tokens": ["Aus", "de\u00b7nen", "oh\u00b7ne", "Falsch", "und", "L\u00fc\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "So rein der Strahl des Himmels bricht?", "tokens": ["So", "rein", "der", "Strahl", "des", "Him\u00b7mels", "bricht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ich wei\u00df nur eines: wenn verlocken", "tokens": ["Ich", "wei\u00df", "nur", "ei\u00b7nes", ":", "wenn", "ver\u00b7lo\u00b7cken"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "PIS", "$.", "KOUS", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mich will ein tr\u00fcgerisches Licht,", "tokens": ["Mich", "will", "ein", "tr\u00fc\u00b7ge\u00b7ri\u00b7sches", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Leidenschaft mit Sturmesglocken", "tokens": ["Die", "Lei\u00b7den\u00b7schaft", "mit", "Stur\u00b7mes\u00b7glo\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zu dem beth\u00f6rten Geiste spricht:", "tokens": ["Zu", "dem", "be\u00b7th\u00f6r\u00b7ten", "Geis\u00b7te", "spricht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dann k\u00e4mpft den wilden Aufruhr nieder,", "tokens": ["Dann", "k\u00e4mpft", "den", "wil\u00b7den", "Auf\u00b7ruhr", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Des Friedens Klarheit schenkt mir wieder", "tokens": ["Des", "Frie\u00b7dens", "Klar\u00b7heit", "schenkt", "mir", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Ein Blick in dieses Angesicht!", "tokens": ["Ein", "Blick", "in", "die\u00b7ses", "An\u00b7ge\u00b7sicht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Und wenn ich zu erliegen meine", "tokens": ["Und", "wenn", "ich", "zu", "er\u00b7lie\u00b7gen", "mei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "PTKZU", "VVINF", "PPOSAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Des Tagwerks dumpfem Einerlei,", "tokens": ["Des", "Tag\u00b7werks", "dum\u00b7pfem", "Ei\u00b7ner\u00b7lei", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein einz'ger Blick auf dich, du Meine!", "tokens": ["Ein", "einz'\u00b7ger", "Blick", "auf", "dich", ",", "du", "Mei\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPER", "$,", "PPER", "PPOSAT", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und ich bin wieder stark und frei.", "tokens": ["Und", "ich", "bin", "wie\u00b7der", "stark", "und", "frei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "O du mein Licht auf dunklem Pfade!", "tokens": ["O", "du", "mein", "Licht", "auf", "dunk\u00b7lem", "Pfa\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "PPOSAT", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Du Zeugnis mir von Gottes Gnade!", "tokens": ["Du", "Zeug\u00b7nis", "mir", "von", "Got\u00b7tes", "Gna\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "PPER", "APPR", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Du mir im Herbst erbl\u00fchter Mai!", "tokens": ["Du", "mir", "im", "Herbst", "er\u00b7bl\u00fch\u00b7ter", "Mai", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPER", "APPRART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Geliebtes Kind! zum Trost, da\u00df ferne", "tokens": ["Ge\u00b7lieb\u00b7tes", "Kind", "!", "zum", "Trost", ",", "da\u00df", "fer\u00b7ne