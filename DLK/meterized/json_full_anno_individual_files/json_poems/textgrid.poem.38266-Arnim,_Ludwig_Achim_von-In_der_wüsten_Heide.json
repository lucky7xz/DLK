{"textgrid.poem.38266": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "In der w\u00fcsten Heide", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Allhier in dieser w\u00fcsten Haid", "tokens": ["All\u00b7hier", "in", "die\u00b7ser", "w\u00fcs\u00b7ten", "Haid"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wohnt keine Seele weit und breit,", "tokens": ["Wohnt", "kei\u00b7ne", "See\u00b7le", "weit", "und", "breit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die wilden Thier allein,", "tokens": ["Die", "wil\u00b7den", "Thier", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Die seh ich selbst Mitleiden tragen,", "tokens": ["Die", "seh", "ich", "selbst", "Mit\u00b7lei\u00b7den", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die V\u00f6gel traurig seyn,", "tokens": ["Die", "V\u00f6\u00b7gel", "trau\u00b7rig", "seyn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VAINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Und mich mit schwacher Stimm beklagen;", "tokens": ["Und", "mich", "mit", "schwa\u00b7cher", "Stimm", "be\u00b7kla\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos