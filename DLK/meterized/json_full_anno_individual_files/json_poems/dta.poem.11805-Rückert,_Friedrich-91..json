{"dta.poem.11805": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "91.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein Geist, der sch\u00f6pferisch den meinen angehaucht,", "tokens": ["Ein", "Geist", ",", "der", "sch\u00f6p\u00b7fe\u00b7risch", "den", "mei\u00b7nen", "an\u00b7ge\u00b7haucht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD", "ART", "PPOSAT", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In dessen Glanz ich mich mit Sehnsucht eingetaucht;", "tokens": ["In", "des\u00b7sen", "Glanz", "ich", "mich", "mit", "Sehn\u00b7sucht", "ein\u00b7ge\u00b7taucht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "PPER", "PRF", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ich habe doch von ihm nichts als die Form genommen,", "tokens": ["Ich", "ha\u00b7be", "doch", "von", "ihm", "nichts", "als", "die", "Form", "ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "PPER", "PIS", "KOKOM", "ART", "NN", "VVPP", "$,"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und aller Stoff ist mir von andern hergekommen.", "tokens": ["Und", "al\u00b7ler", "Stoff", "ist", "mir", "von", "an\u00b7dern", "her\u00b7ge\u00b7kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VAFIN", "PPER", "APPR", "PIS", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Die Welt ist lauter Stoff; du nimmst von denen eben", "tokens": ["Die", "Welt", "ist", "lau\u00b7ter", "Stoff", ";", "du", "nimmst", "von", "de\u00b7nen", "e\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PIAT", "NN", "$.", "PPER", "VVFIN", "APPR", "PRELS", "ADV"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den Stoff, nimmst sie als Stoff, die sonst nichts k\u00f6nnen geben.", "tokens": ["Den", "Stoff", ",", "nimmst", "sie", "als", "Stoff", ",", "die", "sonst", "nichts", "k\u00f6n\u00b7nen", "ge\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "PPER", "KOUS", "NN", "$,", "PRELS", "ADV", "PIS", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Und nur dem Geiste selbst, der dir das H\u00f6chste gab,", "tokens": ["Und", "nur", "dem", "Geis\u00b7te", "selbst", ",", "der", "dir", "das", "H\u00f6chs\u00b7te", "gab", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "ADV", "$,", "PRELS", "PPER", "ART", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das geistige Gepr\u00e4g, nimmst du nichts Ird'sches ab.", "tokens": ["Das", "geis\u00b7ti\u00b7ge", "Ge\u00b7pr\u00e4g", ",", "nimmst", "du", "nichts", "Ird'\u00b7sches", "ab", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "VVFIN", "PPER", "PIS", "PIS", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "So hat die Sonnenblum' ihr Himmelsbild in Augen,", "tokens": ["So", "hat", "die", "Son\u00b7nen\u00b7blum'", "ihr", "Him\u00b7mels\u00b7bild", "in", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "PPOSAT", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und l\u00e4\u00dft die Wurzel rings im Boden Nahrung saugen.", "tokens": ["Und", "l\u00e4\u00dft", "die", "Wur\u00b7zel", "rings", "im", "Bo\u00b7den", "Nah\u00b7rung", "sau\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADV", "APPRART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}