": ["ADV", "VVFIN", "PPER", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und klopfest an Pall\u00e4sten an;", "tokens": ["Und", "klop\u00b7fest", "an", "Pal\u00b7l\u00e4s\u00b7ten", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dort sch\u00fctzest du der Helden Nachen,", "tokens": ["Dort", "sch\u00fct\u00b7zest", "du", "der", "Hel\u00b7den", "Na\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die stolzer Dr\u00e4nger Schrecken sind,", "tokens": ["Die", "stol\u00b7zer", "Dr\u00e4n\u00b7ger", "Schre\u00b7cken", "sind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und wiegst die Freiheit im Erwachen", "tokens": ["Und", "wiegst", "die", "Frei\u00b7heit", "im", "Er\u00b7wa\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Auf deinem Pf\u00fchl, das zarte Kind.", "tokens": ["Auf", "dei\u00b7nem", "Pf\u00fchl", ",", "das", "zar\u00b7te", "Kind", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.13": {"line.1": {"text": "Und wo mit unbarmherz'gen Fluten", "tokens": ["Und", "wo", "mit", "un\u00b7barm\u00b7her\u00b7z'\u00b7gen", "Flu\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "APPR", "ADJA", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Dein Strom des Armen Flur erreicht,", "tokens": ["Dein", "Strom", "des", "Ar\u00b7men", "Flur", "er\u00b7reicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da \u00f6ffnet Gott die Hand der Guten,", "tokens": ["Da", "\u00f6ff\u00b7net", "Gott", "die", "Hand", "der", "Gu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da wird des Nachbarn Herz erweicht;", "tokens": ["Da", "wird", "des", "Nach\u00b7barn", "Herz", "er\u00b7weicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da sprie\u00dft in tausend goldnen Aehren", "tokens": ["Da", "sprie\u00dft", "in", "tau\u00b7send", "gold\u00b7nen", "A\u00b7eh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "CARD", "ADJA", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Die reiche Saat des Mitleids auf,", "tokens": ["Die", "rei\u00b7che", "Saat", "des", "Mit\u00b7leids", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und, wo er meinte zu verheeren,", "tokens": ["Und", ",", "wo", "er", "mein\u00b7te", "zu", "ver\u00b7hee\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PWAV", "PPER", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Entkeimet Segen deinem Lauf.", "tokens": ["Ent\u00b7kei\u00b7met", "Se\u00b7gen", "dei\u00b7nem", "Lauf", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.14": {"line.1": {"text": "So diene denn dem Herrn der Erde", "tokens": ["So", "die\u00b7ne", "denn", "dem", "Herrn", "der", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Im neuen Jahr, du dunkle Kraft!", "tokens": ["Im", "neu\u00b7en", "Jahr", ",", "du", "dunk\u00b7le", "Kraft", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wir glauben, da\u00df er schirmen werde,", "tokens": ["Wir", "glau\u00b7ben", ",", "da\u00df", "er", "schir\u00b7men", "wer\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "VVINF", "VAFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Was Leib und Seele Heil verschafft.", "tokens": ["Was", "Leib", "und", "See\u00b7le", "Heil", "ver\u00b7schafft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "KON", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und ob den Himmel Nacht umzogen,", "tokens": ["Und", "ob", "den", "Him\u00b7mel", "Nacht", "um\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und ob ein Sturm die Welt durchweht:", "tokens": ["Und", "ob", "ein", "Sturm", "die", "Welt", "durch\u00b7weht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wir sehen seinen Friedensbogen,", "tokens": ["Wir", "se\u00b7hen", "sei\u00b7nen", "Frie\u00b7dens\u00b7bo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Der \u00fcber allen Wassern steht.", "tokens": ["Der", "\u00fc\u00b7ber", "al\u00b7len", "Was\u00b7sern", "steht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}