{"textgrid.poem.39808": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "3. An das Wasser", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wir stehen an des Jahres Schwelle,", "tokens": ["Wir", "ste\u00b7hen", "an", "des", "Jah\u00b7res", "Schwel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Thor der Zeit ist aufgethan;", "tokens": ["Ein", "Thor", "der", "Zeit", "ist", "auf\u00b7ge\u00b7than", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch hinter uns wogt deine Welle,", "tokens": ["Doch", "hin\u00b7ter", "uns", "wogt", "dei\u00b7ne", "Wel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-++--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Du tobend Element, uns an.", "tokens": ["Du", "to\u00b7bend", "E\u00b7le\u00b7ment", ",", "uns", "an", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADJD", "NN", "$,", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wir blicken r\u00fcckw\u00e4rts noch mit Schrecken;", "tokens": ["Wir", "bli\u00b7cken", "r\u00fcck\u00b7w\u00e4rts", "noch", "mit", "Schre\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Folgst du uns durch die Pforte nach?", "tokens": ["Folgst", "du", "uns", "durch", "die", "Pfor\u00b7te", "nach", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Willst wieder unsre Fluren decken", "tokens": ["Willst", "wie\u00b7der", "uns\u00b7re", "Flu\u00b7ren", "de\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und wallen \u00fcber unser Dach?", "tokens": ["Und", "wal\u00b7len", "\u00fc\u00b7ber", "un\u00b7ser", "Dach", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Was in der Zukunft sei verborgen,", "tokens": ["Was", "in", "der", "Zu\u00b7kunft", "sei", "ver\u00b7bor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wir br\u00fcteten darob ein Jahr,", "tokens": ["Wir", "br\u00fc\u00b7te\u00b7ten", "da\u00b7rob", "ein", "Jahr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PAV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wir stritten uns um unsre Sorgen \u2013", "tokens": ["Wir", "strit\u00b7ten", "uns", "um", "uns\u00b7re", "Sor\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da braust uns nahe die Gefahr;", "tokens": ["Da", "braust", "uns", "na\u00b7he", "die", "Ge\u00b7fahr", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die V\u00f6lker h\u00f6ren auf zu hadern,", "tokens": ["Die", "V\u00f6l\u00b7ker", "h\u00f6\u00b7ren", "auf", "zu", "ha\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Sie schweigen staunend, graun-erf\u00fcllt,", "tokens": ["Sie", "schwei\u00b7gen", "stau\u00b7nend", ",", "graun\u00b7er\u00b7f\u00fcllt", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Inde\u00df dein Strom aus allen Adern", "tokens": ["In\u00b7de\u00df", "dein", "Strom", "aus", "al\u00b7len", "A\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPOSAT", "NN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Der alten Erde zornig quillt.", "tokens": ["Der", "al\u00b7ten", "Er\u00b7de", "zor\u00b7nig", "quillt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Willst du den Boden wieder fressen,", "tokens": ["Willst", "du", "den", "Bo\u00b7den", "wie\u00b7der", "fres\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der einst entstiegen deinem Schoo\u00df?", "tokens": ["Der", "einst", "ent\u00b7stie\u00b7gen", "dei\u00b7nem", "Schoo\u00df", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Z\u00fcrnst du dem Menschen, der vermessen", "tokens": ["Z\u00fcrnst", "du", "dem", "Men\u00b7schen", ",", "der", "ver\u00b7mes\u00b7sen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dich furchet auf dem stolzen Flo\u00df?", "tokens": ["Dich", "fur\u00b7chet", "auf", "dem", "stol\u00b7zen", "Flo\u00df", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der spielend, mit beseelten D\u00e4mpfen,", "tokens": ["Der", "spie\u00b7lend", ",", "mit", "be\u00b7seel\u00b7ten", "D\u00e4mp\u00b7fen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Durch deine wilden Wogen schl\u00fcpft,", "tokens": ["Durch", "dei\u00b7ne", "wil\u00b7den", "Wo\u00b7gen", "schl\u00fcpft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und trotz der Winde grimmen K\u00e4mpfen", "tokens": ["Und", "trotz", "der", "Win\u00b7de", "grim\u00b7men", "K\u00e4mp\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Das leichte Boot an's Ufer kn\u00fcpft?", "tokens": ["Das", "leich\u00b7te", "Boot", "an's", "U\u00b7fer", "kn\u00fcpft", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Du wirst ihn doch nicht unterwerfen:", "tokens": ["Du", "wirst", "ihn", "doch", "nicht", "un\u00b7ter\u00b7wer\u00b7fen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Aus deiner Tiefe str\u00f6mst du nur,", "tokens": ["Aus", "dei\u00b7ner", "Tie\u00b7fe", "str\u00f6mst", "du", "nur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ihm dessen Willen einzusch\u00e4rfen,", "tokens": ["Ihm", "des\u00b7sen", "Wil\u00b7len", "ein\u00b7zu\u00b7sch\u00e4r\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PDS", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Herr ist \u00fcber die Natur;", "tokens": ["Der", "Herr", "ist", "\u00fc\u00b7ber", "die", "Na\u00b7tur", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Erkennet er des Sch\u00f6pfers St\u00e4rke,", "tokens": ["Er\u00b7ken\u00b7net", "er", "des", "Sch\u00f6p\u00b7fers", "St\u00e4r\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und \u00fcbt in Demut seinen Geist,", "tokens": ["Und", "\u00fcbt", "in", "De\u00b7mut", "sei\u00b7nen", "Geist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "So sch\u00fctzt ihn der bei'm guten Werke,", "tokens": ["So", "sch\u00fctzt", "ihn", "der", "bei'm", "gu\u00b7ten", "Wer\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Der deine Flut sich legen hei\u00dft.", "tokens": ["Der", "dei\u00b7ne", "Flut", "sich", "le\u00b7gen", "hei\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "PRF", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Er brauchet dich zu seiner Ehre,", "tokens": ["Er", "brau\u00b7chet", "dich", "zu", "sei\u00b7ner", "Eh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sein Wort bezeichnet dir die Bahn;", "tokens": ["Sein", "Wort", "be\u00b7zeich\u00b7net", "dir", "die", "Bahn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Hier schwellest du den Flu\u00df zum Meere,", "tokens": ["Hier", "schwel\u00b7lest", "du", "den", "Flu\u00df", "zum", "Mee\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und klopfest an Pall\u00e4sten an;", "tokens": ["Und", "klop\u00b7fest", "an", "Pal\u00b7l\u00e4s\u00b7ten", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dort sch\u00fctzest du der Helden Nachen,", "tokens": ["Dort", "sch\u00fct\u00b7zest", "du", "der", "Hel\u00b7den", "Na\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die stolzer Dr\u00e4nger Schrecken sind,", "tokens": ["Die", "stol\u00b7zer", "Dr\u00e4n\u00b7ger", "Schre\u00b7cken", "sind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und wiegst die Freiheit im Erwachen", "tokens": ["Und", "wiegst", "die", "Frei\u00b7heit", "im", "Er\u00b7wa\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Auf deinem Pf\u00fchl, das zarte Kind.", "tokens": ["Auf", "dei\u00b7nem", "Pf\u00fchl", ",", "das", "zar\u00b7te", "Kind", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Und wo mit unbarmherz'gen Fluten", "tokens": ["Und", "wo", "mit", "un\u00b7barm\u00b7her\u00b7z'\u00b7gen", "Flu\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "APPR", "ADJA", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Dein Strom des Armen Flur erreicht,", "tokens": ["Dein", "Strom", "des", "Ar\u00b7men", "Flur", "er\u00b7reicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da \u00f6ffnet Gott die Hand der Guten,", "tokens": ["Da", "\u00f6ff\u00b7net", "Gott", "die", "Hand", "der", "Gu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da wird des Nachbarn Herz erweicht;", "tokens": ["Da", "wird", "des", "Nach\u00b7barn", "Herz", "er\u00b7weicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da sprie\u00dft in tausend goldnen Aehren", "tokens": ["Da", "sprie\u00dft", "in", "tau\u00b7send", "gold\u00b7nen", "A\u00b7eh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "CARD", "ADJA", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Die reiche Saat des Mitleids auf,", "tokens": ["Die", "rei\u00b7che", "Saat", "des", "Mit\u00b7leids", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und, wo er meinte zu verheeren,", "tokens": ["Und", ",", "wo", "er", "mein\u00b7te", "zu", "ver\u00b7hee\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PWAV", "PPER", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Entkeimet Segen deinem Lauf.", "tokens": ["Ent\u00b7kei\u00b7met", "Se\u00b7gen", "dei\u00b7nem", "Lauf", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "So diene denn dem Herrn der Erde", "tokens": ["So", "die\u00b7ne", "denn", "dem", "Herrn", "der", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Im neuen Jahr, du dunkle Kraft!", "tokens": ["Im", "neu\u00b7en", "Jahr", ",", "du", "dunk\u00b7le", "Kraft", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wir glauben, da\u00df er schirmen werde,", "tokens": ["Wir", "glau\u00b7ben", ",", "da\u00df", "er", "schir\u00b7men", "wer\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "VVINF", "VAFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Was Leib und Seele Heil verschafft.", "tokens": ["Was", "Leib", "und", "See\u00b7le", "Heil", "ver\u00b7schafft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "KON", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und ob den Himmel Nacht umzogen,", "tokens": ["Und", "ob", "den", "Him\u00b7mel", "Nacht", "um\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und ob ein Sturm die Welt durchweht:", "tokens": ["Und", "ob", "ein", "Sturm", "die", "Welt", "durch\u00b7weht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "lin