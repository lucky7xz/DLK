{"dta.poem.7301": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Katharine Sch\u00fccking.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-20090519994", "language": ["de:0.85", "af:0.14"], "booktitle": "Droste-H\u00fclshoff, Annette von: Gedichte. Stuttgart u. a., 1844."}, "poem": {"stanza.1": {"line.1": {"text": "Du hast es nie geahndet, nie gewu\u00dft,", "tokens": ["Du", "hast", "es", "nie", "ge\u00b7ahn\u00b7det", ",", "nie", "ge\u00b7wu\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "VVPP", "$,", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wie gro\u00df mein Lieben ist zu dir gewesen,", "tokens": ["Wie", "gro\u00df", "mein", "Lie\u00b7ben", "ist", "zu", "dir", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPOSAT", "ADJA", "VAFIN", "APPR", "PPER", "VAPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nie hat dein klares Aug' in meiner Brust", "tokens": ["Nie", "hat", "dein", "kla\u00b7res", "Aug'", "in", "mei\u00b7ner", "Brust"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPOSAT", "ADJA", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die scheu verh\u00fcllte Runenschrift gelesen,", "tokens": ["Die", "scheu", "ver\u00b7h\u00fcll\u00b7te", "Ru\u00b7nen\u00b7schrift", "ge\u00b7le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Wenn du mir freundlich reichtest deine Hand,", "tokens": ["Wenn", "du", "mir", "freund\u00b7lich", "reich\u00b7test", "dei\u00b7ne", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADJD", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und wir zusammen durch die Gr\u00fcne wallten,", "tokens": ["Und", "wir", "zu\u00b7sam\u00b7men", "durch", "die", "Gr\u00fc\u00b7ne", "wall\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Nicht wu\u00dftest du, da\u00df wie ein G\u00f6tterpfand", "tokens": ["Nicht", "wu\u00df\u00b7test", "du", ",", "da\u00df", "wie", "ein", "G\u00f6t\u00b7ter\u00b7pfand"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKNEG", "VVFIN", "PPER", "$,", "KOUS", "KOKOM", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Ich, wie ein k\u00f6stlich Kleinod sie gehalten.", "tokens": ["Ich", ",", "wie", "ein", "k\u00f6st\u00b7lich", "Klei\u00b7nod", "sie", "ge\u00b7hal\u00b7ten", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PWAV", "ART", "ADJD", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Du sahst mich nicht als ich, ein heftig Kind,", "tokens": ["Du", "sahst", "mich", "nicht", "als", "ich", ",", "ein", "hef\u00b7tig", "Kind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "KOUS", "PPER", "$,", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Vom ersten Ku\u00df der jungen Muse trunken,", "tokens": ["Vom", "ers\u00b7ten", "Ku\u00df", "der", "jun\u00b7gen", "Mu\u00b7se", "trun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Im Garten kniete, wo die Quelle rinnt,", "tokens": ["Im", "Gar\u00b7ten", "knie\u00b7te", ",", "wo", "die", "Quel\u00b7le", "rinnt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$,", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und weinend in die Gr\u00e4ser bin gesunken;", "tokens": ["Und", "wei\u00b7nend", "in", "die", "Gr\u00e4\u00b7ser", "bin", "ge\u00b7sun\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Als zitternd ich gedreht der Th\u00fcre Schlo\u00df,", "tokens": ["Als", "zit\u00b7ternd", "ich", "ge\u00b7dreht", "der", "Th\u00fc\u00b7re", "Schlo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVPP", "PPER", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Da ich zum ersten Mal dich sollte schauen,", "tokens": ["Da", "ich", "zum", "ers\u00b7ten", "Mal", "dich", "soll\u00b7te", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "ADJA", "NN", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Westphalens Dichterin, und wie da flo\u00df", "tokens": ["West\u00b7pha\u00b7lens", "Dich\u00b7te\u00b7rin", ",", "und", "wie", "da", "flo\u00df"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NN", "$,", "KON", "PWAV", "ADV", "VVFIN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.8": {"text": "Durch mein bewegtes Herz ein selig Grauen.", "tokens": ["Durch", "mein", "be\u00b7weg\u00b7tes", "Herz", "ein", "se\u00b7lig", "Grau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Sehr jung war ich und sehr an Liebe reich,", "tokens": ["Sehr", "jung", "war", "ich", "und", "sehr", "an", "Lie\u00b7be", "reich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "PPER", "KON", "ADV", "APPR", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Begeisterung der Hauch von dem ich lebte;", "tokens": ["Be\u00b7geis\u00b7te\u00b7rung", "der", "Hauch", "von", "dem", "ich", "leb\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ach! Manches ist zerst\u00e4ubt, der Asche gleich,", "tokens": ["Ach", "!", "Man\u00b7ches", "ist", "zer\u00b7st\u00e4ubt", ",", "der", "A\u00b7sche", "gleich", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PIS", "VAFIN", "VVPP", "$,", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Was einst als Flamme durch die Adern bebte!", "tokens": ["Was", "einst", "als", "Flam\u00b7me", "durch", "die", "A\u00b7dern", "beb\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "KOUS", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Mein Blick war klar und mein Erkennen stark,", "tokens": ["Mein", "Blick", "war", "klar", "und", "mein", "Er\u00b7ken\u00b7nen", "stark", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "KON", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Von seinem Throne mu\u00dfte Manches steigen,", "tokens": ["Von", "sei\u00b7nem", "Thro\u00b7ne", "mu\u00df\u00b7te", "Man\u00b7ches", "stei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VMFIN", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und was ich einst genannt des Lebens Mark,", "tokens": ["Und", "was", "ich", "einst", "ge\u00b7nannt", "des", "Le\u00b7bens", "Mark", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "ADV", "VVPP", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Das f\u00fchlt' ich jetzt mit frischem Stolz mein eigen.", "tokens": ["Das", "f\u00fchlt'", "ich", "jetzt", "mit", "fri\u00b7schem", "Stolz", "mein", "ei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "So scheut' ich es, als fromme Sch\u00fclerin,", "tokens": ["So", "scheut'", "ich", "es", ",", "als", "from\u00b7me", "Sch\u00fc\u00b7le\u00b7rin", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "$,", "KOUS", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Dir wieder in das dunkle Aug' zu sehen,", "tokens": ["Dir", "wie\u00b7der", "in", "das", "dunk\u00b7le", "Aug'", "zu", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ich wollte nicht vor meiner Meisterin", "tokens": ["Ich", "woll\u00b7te", "nicht", "vor", "mei\u00b7ner", "Meis\u00b7te\u00b7rin"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PTKNEG", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Hochm\u00fcthig, mit bedecktem Haupte, stehen.", "tokens": ["Hoch\u00b7m\u00fct\u00b7hig", ",", "mit", "be\u00b7deck\u00b7tem", "Haup\u00b7te", ",", "ste\u00b7hen", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADJD", "$,", "APPR", "ADJA", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Auch war ich krank, mein Sinnen sehr verwirrt,", "tokens": ["Auch", "war", "ich", "krank", ",", "mein", "Sin\u00b7nen", "sehr", "ver\u00b7wirrt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "$,", "PPOSAT", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und keinen Namen mocht' ich sehnend nennen;", "tokens": ["Und", "kei\u00b7nen", "Na\u00b7men", "mocht'", "ich", "seh\u00b7nend", "nen\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VMFIN", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Doch hat dies deine Liebe nicht geirrt,", "tokens": ["Doch", "hat", "dies", "dei\u00b7ne", "Lie\u00b7be", "nicht", "ge\u00b7irrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PDS", "PPOSAT", "NN", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Du drangst zu mir nach langer Jahre Trennen.", "tokens": ["Du", "drangst", "zu", "mir", "nach", "lan\u00b7ger", "Jah\u00b7re", "Tren\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "PPER", "APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Und als du vor mich tratest, fest und klar,", "tokens": ["Und", "als", "du", "vor", "mich", "tra\u00b7test", ",", "fest", "und", "klar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "APPR", "PPER", "VVFIN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und blicktest tief mir in der Seele Gr\u00fcnde,", "tokens": ["Und", "blick\u00b7test", "tief", "mir", "in", "der", "See\u00b7le", "Gr\u00fcn\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "PPER", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da ward ich meiner Schw\u00e4che wohl gewahr,", "tokens": ["Da", "ward", "ich", "mei\u00b7ner", "Schw\u00e4\u00b7che", "wohl", "ge\u00b7wahr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Was ich gedacht, das schien mir schwere S\u00fcnde.", "tokens": ["Was", "ich", "ge\u00b7dacht", ",", "das", "schien", "mir", "schwe\u00b7re", "S\u00fcn\u00b7de", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "$,", "PDS", "VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Dein Bild, du Starke in der L\u00e4utrung Brand,", "tokens": ["Dein", "Bild", ",", "du", "Star\u00b7ke", "in", "der", "L\u00e4ut\u00b7rung", "Brand", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPER", "NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Stieg wie ein Ph\u00f6nix aus der Asche wieder,", "tokens": ["Stieg", "wie", "ein", "Ph\u00f6\u00b7nix", "aus", "der", "A\u00b7sche", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ART", "NN", "APPR", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und tief im Herzen hab' ich es erkannt,", "tokens": ["Und", "tief", "im", "Her\u00b7zen", "hab'", "ich", "es", "er\u00b7kannt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPRART", "NN", "VAFIN", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Wie zehnfach gr\u00f6\u00dfer du als deine Lieder.", "tokens": ["Wie", "zehn\u00b7fach", "gr\u00f6\u00b7\u00dfer", "du", "als", "dei\u00b7ne", "Lie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ADJD", "PPER", "KOKOM", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Du sahst, Bescheid'ne, nicht, da\u00df damals hier", "tokens": ["Du", "sahst", ",", "Be\u00b7schei\u00b7d'\u00b7ne", ",", "nicht", ",", "da\u00df", "da\u00b7mals", "hier"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "NN", "$,", "PTKNEG", "$,", "KOUS", "ADV", "ADV"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Aus deinem Blick Genesung ich getrunken,", "tokens": ["Aus", "dei\u00b7nem", "Blick", "Ge\u00b7ne\u00b7sung", "ich", "ge\u00b7trun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df deines Mundes Laute damals mir", "tokens": ["Da\u00df", "dei\u00b7nes", "Mun\u00b7des", "Lau\u00b7te", "da\u00b7mals", "mir"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "NN", "ADV", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wie Naphtha in die Seele sind gesunken.", "tokens": ["Wie", "Naph\u00b7tha", "in", "die", "See\u00b7le", "sind", "ge\u00b7sun\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "APPR", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ein jedes Wort, durchsichtig wie Krystall", "tokens": ["Ein", "je\u00b7des", "Wort", ",", "durch\u00b7sich\u00b7tig", "wie", "Krys\u00b7tall"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "PIAT", "NN", "$,", "ADJD", "KOKOM", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und kr\u00e4ftig gleich dem edelsten der Weine,", "tokens": ["Und", "kr\u00e4f\u00b7tig", "gleich", "dem", "e\u00b7dels\u00b7ten", "der", "Wei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "ART", "ADJA", "ART", "NN", "$,"], "meter": "-+-+-+---+-", "measure": "unknown.measure