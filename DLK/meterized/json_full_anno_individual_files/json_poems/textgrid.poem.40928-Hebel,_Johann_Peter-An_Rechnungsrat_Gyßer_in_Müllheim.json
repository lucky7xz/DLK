{"textgrid.poem.40928": {"metadata": {"author": {"name": "Hebel, Johann Peter", "birth": "N.A.", "death": "N.A."}, "title": "An Rechnungsrat Gy\u00dfer in M\u00fcllheim", "genre": "verse", "period": "N.A.", "pub_year": 1793, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dunderschie\u00df! Wer rennt mer in mi G\u00e4u?", "tokens": ["Dun\u00b7der\u00b7schie\u00df", "!", "Wer", "rennt", "mer", "in", "mi", "G\u00e4u", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$.", "PWS", "VVFIN", "ADV", "APPR", "NE", "NE", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Isch's der Gy\u00dfer? \u2013 's isch bi miner Treu", "tokens": ["Isch's", "der", "Gy\u00b7\u00dfer", "?", "\u2013", "'s", "isch", "bi", "mi\u00b7ner", "Treu"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "ART", "NN", "$.", "$(", "PPER", "ADJD", "ADJD", "ADJA", "NN"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Euer Gl\u00fcck, a\u00df Ihr's sind, Meister Gy\u00dfer!", "tokens": ["Eu\u00b7er", "Gl\u00fcck", ",", "a\u00df", "Ih\u00b7r's", "sind", ",", "Meis\u00b7ter", "Gy\u00b7\u00dfer", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "PIS", "VAFIN", "$,", "PPOSAT", "NN", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Rime her! \u2013 Potz F\u00fcrio, und Miser-", "tokens": ["Ri\u00b7me", "her", "!", "\u2013", "Potz", "F\u00fc\u00b7rio", ",", "und", "Mi\u00b7ser"], "token_info": ["word", "word", "punct", "punct", "word", "word", "punct", "word", "word"], "pos": ["NN", "PTKVZ", "$.", "$(", "NE", "NE", "$,", "KON", "TRUNC"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "ere, Domine! 's h\u00e4tt schier verseit,", "tokens": ["e\u00b7re", ",", "Do\u00b7mi\u00b7ne", "!", "'s", "h\u00e4tt", "schier", "ver\u00b7seit", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$.", "PPER", "VAFIN", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.6": {"text": "h\u00e4tt mi nit d'Verzwiflung use treit.", "tokens": ["h\u00e4tt", "mi", "nit", "d'\u00b7Ver\u00b7zwif\u00b7lung", "u\u00b7se", "treit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.7": {"text": "Jez, was Euer Versli abetrifft,", "tokens": ["Jez", ",", "was", "Eu\u00b7er", "Ver\u00b7sli", "a\u00b7betr\u00b7ifft", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.8": {"text": "uf mi Seecht, i bi voll Chib und Gift,", "tokens": ["uf", "mi", "Seecht", ",", "i", "bi", "voll", "Chib", "und", "Gift", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "$,", "FM", "FM", "ADJD", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.9": {"text": "a\u00df me Ratte mit mer ch\u00f6nnt verge.", "tokens": ["a\u00df", "me", "Rat\u00b7te", "mit", "mer", "ch\u00f6nnt", "ver\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.10": {"text": "Drum, i ha gmeint, 's ch\u00f6nn 's sust niemes meh,", "tokens": ["Drum", ",", "i", "ha", "gmeint", ",", "'s", "ch\u00f6nn", "'s", "sust", "nie\u00b7mes", "meh", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "NE", "NE", "VVFIN", "$,", "PPER", "NE", "PPER", "VVFIN", "PIS", "VVFIN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.11": {"text": "weder ich, mit miner lange Pfife,", "tokens": ["we\u00b7der", "ich", ",", "mit", "mi\u00b7ner", "lan\u00b7ge", "Pfi\u00b7fe", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "APPR", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.12": {"text": "und Ihr w\u00fcsset's au so sch\u00f6n z'begrife.", "tokens": ["und", "Ihr", "w\u00fcs\u00b7set's", "au", "so", "sch\u00f6n", "z'\u00b7be\u00b7gri\u00b7fe", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "NE", "ADV", "ADJD", "VVFIN", "$."], "meter": "--+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.13": {"text": "Lueget, 's Hamberch sott enander schelte,", "tokens": ["Lue\u00b7get", ",", "'s", "Ham\u00b7berch", "sott", "en\u00b7an\u00b7der", "schel\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "NN", "ADV", "ADV", "VVFIN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.14": {"text": "doch, wil Ihr's sind, willi 's Recht lo gelte.", "tokens": ["doch", ",", "wil", "Ih\u00b7r's", "sind", ",", "wil\u00b7li", "'s", "Recht", "lo", "gel\u00b7te", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "VMFIN", "PIS", "VAFIN", "$,", "PWAV", "PPER", "NN", "NE", "VVFIN", "$."], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.15": {"text": "Euer Versli isch so nett und gschlacht,", "tokens": ["Eu\u00b7er", "Ver\u00b7sli", "isch", "so", "nett", "und", "gschlacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "ADV", "ADJD", "KON", "VVFIN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.16": {"text": "a\u00df i schier mein, i heig's selber gmacht.", "tokens": ["a\u00df", "i", "schier", "mein", ",", "i", "heig's", "sel\u00b7ber", "gmacht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "ADJD", "PPOSAT", "$,", "NE", "NE", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.17": {"text": "Frili, wer's bidenkt, es isch ke Wunder,", "tokens": ["Fri\u00b7li", ",", "wer's", "bi\u00b7denkt", ",", "es", "isch", "ke", "Wun\u00b7der", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWS", "VVFIN", "$,", "PPER", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.18": {"text": "a\u00df er's ch\u00f6nnet, schla' mi au der Dunder.", "tokens": ["a\u00df", "er's", "ch\u00f6n\u00b7net", ",", "schla'", "mi", "au", "der", "Dun\u00b7der", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "VVFIN", "$,", "VVFIN", "NE", "NE", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.19": {"text": "Ihr trinket urig Poesie", "tokens": ["Ihr", "trin\u00b7ket", "u\u00b7rig", "Poe\u00b7sie"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.20": {"text": "in lange Z\u00fcge z'M\u00fcllen an der Post.", "tokens": ["in", "lan\u00b7ge", "Z\u00fc\u00b7ge", "z'\u00b7M\u00fcl\u00b7len", "an", "der", "Post", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "APPR", "ART", "NN", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.21": {"text": "Tausig Sappermost,", "tokens": ["Tau\u00b7sig", "Sap\u00b7per\u00b7most", ","], "token_info": ["word", "word", "punct"], "pos": ["CARD", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.22": {"text": "isch sel nit e chospire Wi!", "tokens": ["isch", "sel", "nit", "e", "chos\u00b7pi\u00b7re", "Wi", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.23": {"text": "Aber ch\u00f6mmet, sind er's echt im Stand,", "tokens": ["A\u00b7ber", "ch\u00f6m\u00b7met", ",", "sind", "er's", "echt", "im", "Stand", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "$,", "VAFIN", "PIS", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.24": {"text": "doher au ne Rung ins Welschchornland,", "tokens": ["do\u00b7her", "au", "ne", "Rung", "ins", "Wel\u00b7schchorn\u00b7land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "NE", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.25": {"text": "sufet Prosa usem nasse Z\u00fcber", "tokens": ["su\u00b7fet", "Pro\u00b7sa", "u\u00b7sem", "nas\u00b7se", "Z\u00fc\u00b7ber"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.26": {"text": "in der Chuchi ('s tribt mer d'Augen \u00fcber);", "tokens": ["in", "der", "Chu\u00b7chi", "(", "'s", "tribt", "mer", "d'\u00b7Au\u00b7gen", "\u00fc\u00b7ber", ")", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "NE", "$(", "PPER", "VVFIN", "ADV", "PAV", "APPR", "$(", "$."], "meter": "--+--+--+-+-", "measure": "anapaest.tri.plus"}, "line.27": {"text": "sel bi Gollig luegt en anderst a.", "tokens": ["sel", "bi", "Gol\u00b7lig", "luegt", "en", "an\u00b7derst", "a."], "token_info": ["word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.28": {"text": "Zwor i wil's bikenne, jo i ha", "tokens": ["Zwor", "i", "wil's", "bi\u00b7ken\u00b7ne", ",", "jo", "i", "ha"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "$,", "NE", "NE", "NE"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.29": {"text": "au no Oberl\u00e4nder Poesie", "tokens": ["au", "no", "O\u00b7ber\u00b7l\u00e4n\u00b7der", "Poe\u00b7sie"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "NE", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.30": {"text": "imme F\u00e4\u00dfli, und henk d'Zunge dri,", "tokens": ["im\u00b7me", "F\u00e4\u00df\u00b7li", ",", "und", "henk", "d'\u00b7Zun\u00b7ge", "dri", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "$,", "KON", "VVFIN", "PDS", "PTKVZ", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.31": {"text": "wenn's nit go will. Aber 's isch ke Art,", "tokens": ["wenn's", "nit", "go", "will", ".", "A\u00b7ber", "'s", "isch", "ke", "Art", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "NE", "VMFIN", "$.", "KON", "PPER", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.32": {"text": "nei es isch n\u00fct, uf der sandige Hart.", "tokens": ["nei", "es", "isch", "n\u00fct", ",", "uf", "der", "san\u00b7di\u00b7ge", "Hart", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "$,", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.33": {"text": "He der w\u00fcsset's wohl, i hannich jo", "tokens": ["He", "der", "w\u00fcs\u00b7set's", "wohl", ",", "i", "han\u00b7nich", "jo"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "ART", "VVFIN", "ADV", "$,", "NE", "NE", "NE"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.34": {"text": "lang und mengmol gseh bim F\u00fce\u00dfli stoh.", "tokens": ["lang", "und", "meng\u00b7mol", "gseh", "bim", "F\u00fce\u00df\u00b7li", "stoh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "FM", "FM", "FM", "FM", "FM", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.35": {"text": "(churz het Euch no niene niemes gseh,", "tokens": ["(", "churz", "het", "Euch", "no", "ni\u00b7e\u00b7ne", "nie\u00b7mes", "gseh", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.36": {"text": "wer's bihauptet, seit ke Wohret meh.)", "tokens": ["wer's", "bi\u00b7haup\u00b7tet", ",", "seit", "ke", "Woh\u00b7ret", "meh", ".", ")"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "VVPP", "$,", "FM.la", "FM.la", "FM.la", "FM.la", "$.", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.37": {"text": "Selmols, traui, het's au Batze gchost,", "tokens": ["Sel\u00b7mols", ",", "trau\u00b7i", ",", "het's", "au", "Bat\u00b7ze", "gchost", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "$,", "NE", "NE", "NE", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.38": {"text": "bis der f\u00fc\u00fcrig Geist in Eure Odere", "tokens": ["bis", "der", "f\u00fc\u00fc\u00b7rig", "Geist", "in", "Eu\u00b7re", "O\u00b7de\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJD", "NN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.39": {"text": "und in Eurem Chopf het welle lodere,", "tokens": ["und", "in", "Eu\u00b7rem", "Chopf", "het", "wel\u00b7le", "lo\u00b7de\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "NN", "VAFIN", "PIS", "VVFIN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.40": {"text": "und 's isch doch nit gsi, wie an der Post.", "tokens": ["und", "'s", "isch", "doch", "nit", "gsi", ",", "wie", "an", "der", "Post", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADJD", "ADV", "PTKNEG", "NE", "$,", "PWAV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.41": {"text": "Neie wohl! Se hettich au der Schmid", "tokens": ["Nei\u00b7e", "wohl", "!", "Se", "het\u00b7tich", "au", "der", "Schmid"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "$.", "NE", "NE", "NE", "ART", "NN"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.42": {"text": "z'H\u00fcglen \u00fcberlistet mit mim Lied!", "tokens": ["z'\u00b7H\u00fcg\u00b7len", "\u00fc\u00b7berl\u00b7is\u00b7tet", "mit", "mim", "Lied", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVINF", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.43": {"text": "So ne gscheite Ma, wie Ihr sust sind,", "tokens": ["So", "ne", "gschei\u00b7te", "Ma", ",", "wie", "Ihr", "sust", "sind", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "ADJA", "NN", "$,", "PWAV", "PPER", "VVFIN", "VAFIN", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.44": {"text": "chauft e Chatz im Sack, und seig sie blind!", "tokens": ["chauft", "e", "Chatz", "im", "Sack", ",", "und", "seig", "sie", "blind", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "NE", "APPRART", "NN", "$,", "KON", "VVFIN", "PPER", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.45": {"text": "Geb der Himmel, a\u00df sie sch\u00f6ner Art", "tokens": ["Geb", "der", "Him\u00b7mel", ",", "a\u00df", "sie", "sch\u00f6\u00b7ner", "Art"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "$,", "VVFIN", "PPER", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.46": {"text": "und mit chloren Augen use fahrt,", "tokens": ["und", "mit", "chlo\u00b7ren", "Au\u00b7gen", "u\u00b7se", "fahrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.47": {"text": "wenni 's S\u00e4ckli l\u00f6s und lock und sag:", "tokens": ["wen\u00b7ni", "'s", "S\u00e4ck\u00b7li", "l\u00f6s", "und", "lock", "und", "sag", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "NE", "KON", "NE", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.48": {"text": "\u00bbb\u00fc\u00fcsli chumm, und lo\u00df di seh am Tag!\u00ab", "tokens": ["\u00bb", "b\u00fc\u00fcs\u00b7li", "chumm", ",", "und", "lo\u00df", "di", "seh", "am", "Tag", "!", "\u00ab"], "token_inf