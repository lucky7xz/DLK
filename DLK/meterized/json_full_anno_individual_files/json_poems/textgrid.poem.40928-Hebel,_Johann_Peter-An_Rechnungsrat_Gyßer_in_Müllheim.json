.10": {"text": "Drum, i ha gmeint, 's ch\u00f6nn 's sust niemes meh,", "tokens": ["Drum", ",", "i", "ha", "gmeint", ",", "'s", "ch\u00f6nn", "'s", "sust", "nie\u00b7mes", "meh", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "NE", "NE", "VVFIN", "$,", "PPER", "NE", "PPER", "VVFIN", "PIS", "VVFIN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.11": {"text": "weder ich, mit miner lange Pfife,", "tokens": ["we\u00b7der", "ich", ",", "mit", "mi\u00b7ner", "lan\u00b7ge", "Pfi\u00b7fe", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "APPR", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.12": {"text": "und Ihr w\u00fcsset's au so sch\u00f6n z'begrife.", "tokens": ["und", "Ihr", "w\u00fcs\u00b7set's", "au", "so", "sch\u00f6n", "z'\u00b7be\u00b7gri\u00b7fe", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "NE", "ADV", "ADJD", "VVFIN", "$."], "meter": "--+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.13": {"text": "Lueget, 's Hamberch sott enander schelte,", "tokens": ["Lue\u00b7get", ",", "'s", "Ham\u00b7berch", "sott", "en\u00b7an\u00b7der", "schel\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "NN", "ADV", "ADV", "VVFIN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.14": {"text": "doch, wil Ihr's sind, willi 's Recht lo gelte.", "tokens": ["doch", ",", "wil", "Ih\u00b7r's", "sind", ",", "wil\u00b7li", "'s", "Recht", "lo", "gel\u00b7te", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "VMFIN", "PIS", "VAFIN", "$,", "PWAV", "PPER", "NN", "NE", "VVFIN", "$."], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.15": {"text": "Euer Versli isch so nett und gschlacht,", "tokens": ["Eu\u00b7er", "Ver\u00b7sli", "isch", "so", "nett", "und", "gschlacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "ADV", "ADJD", "KON", "VVFIN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.16": {"text": "a\u00df i schier mein, i heig's selber gmacht.", "tokens": ["a\u00df", "i", "schier", "mein", ",", "i", "heig's", "sel\u00b7ber", "gmacht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "ADJD", "PPOSAT", "$,", "NE", "NE", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.17": {"text": "Frili, wer's bidenkt, es isch ke Wunder,", "tokens": ["Fri\u00b7li", ",", "wer's", "bi\u00b7denkt", ",", "es", "isch", "ke", "Wun\u00b7der", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWS", "VVFIN", "$,", "PPER", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.18": {"text": "a\u00df er's ch\u00f6nnet, schla' mi au der Dunder.", "tokens": ["a\u00df", "er's", "ch\u00f6n\u00b7net", ",", "schla'", "mi", "au", "der", "Dun\u00b7der", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "VVFIN", "$,", "VVFIN", "NE", "NE", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.19": {"text": "Ihr trinket urig Poesie", "tokens": ["Ihr", "trin\u00b7ket", "u\u00b7rig", "Poe\u00b7sie"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.20": {"text": "in lange Z\u00fcge z'M\u00fcllen an der Post.", "tokens": ["in", "lan\u00b7ge", "Z\u00fc\u00b7ge", "z'\u00b7