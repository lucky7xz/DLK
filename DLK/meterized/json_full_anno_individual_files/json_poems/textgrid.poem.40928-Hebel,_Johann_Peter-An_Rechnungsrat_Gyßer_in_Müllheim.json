)", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "NE", "$(", "PPER", "VVFIN", "ADV", "PAV", "APPR", "$(", "$."], "meter": "--+--+--+-+-", "measure": "anapaest.tri.plus"}, "line.27": {"text": "sel bi Gollig luegt en anderst a.", "tokens": ["sel", "bi", "Gol\u00b7lig", "luegt", "en", "an\u00b7derst", "a."], "token_info": ["word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.28": {"text": "Zwor i wil's bikenne, jo i ha", "tokens": ["Zwor", "i", "wil's", "bi\u00b7ken\u00b7ne", ",", "jo", "i", "ha"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "$,", "NE", "NE", "NE"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.29": {"text": "au no Oberl\u00e4nder Poesie", "tokens": ["au", "no", "O\u00b7ber\u00b7l\u00e4n\u00b7der", "Poe\u00b7sie"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "NE", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.30": {"text": "imme F\u00e4\u00dfli, und henk d'Zunge dri,", "tokens": ["im\u00b7me", "F\u00e4\u00df\u00b7li", ",", "und", "henk", "d'\u00b7Zun\u00b7ge", "dri", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "$,", "KON", "VVFIN", "PDS", "PTKVZ", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.31": {"text": "wenn's nit go will. Aber 's isch ke Art,", "tokens": ["wenn's", "nit", "go", "will", ".", "A\u00b7ber", "'s", "isch", "ke", "Art", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "NE", "VMFIN", "$.", "KON", "PPER", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed