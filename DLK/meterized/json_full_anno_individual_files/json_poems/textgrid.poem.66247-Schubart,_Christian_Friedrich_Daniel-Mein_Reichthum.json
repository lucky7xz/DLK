{"textgrid.poem.66247": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "Mein Reichthum", "genre": "verse", "period": "N.A.", "pub_year": 1774, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Elysium ist v\u00f6llig mein,", "tokens": ["E\u00b7ly\u00b7si\u00b7um", "ist", "v\u00f6l\u00b7lig", "mein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ADJD", "PPOSAT", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Cytherens halber Lorbeerhain;", "tokens": ["Cy\u00b7the\u00b7rens", "hal\u00b7ber", "Lor\u00b7beer\u00b7hain", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Am Styx hab' ich Cypressenw\u00e4lder,", "tokens": ["Am", "Styx", "hab'", "ich", "Cyp\u00b7res\u00b7sen\u00b7w\u00e4l\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PPER", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Und in Arkadien Hain, Silberbach und Felder;", "tokens": ["Und", "in", "Ar\u00b7ka\u00b7di\u00b7en", "Hain", ",", "Sil\u00b7ber\u00b7bach", "und", "Fel\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NE", "NE", "$,", "NN", "KON", "NN", "$."], "meter": "--+----+-+-+-", "measure": "anapaest.init"}, "line.5": {"text": "Die Berge Pindus und Parna\u00df,", "tokens": ["Die", "Ber\u00b7ge", "Pin\u00b7dus", "und", "Par\u00b7na\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "KON", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Den Helikon nicht zu vergessen,", "tokens": ["Den", "He\u00b7li\u00b7kon", "nicht", "zu", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Sind mein per fas und per nefas;", "tokens": ["Sind", "mein", "per", "fas", "und", "per", "ne\u00b7fas", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "ADJA", "NN", "KON", "APPR", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "O das Empyrium hab' ich schon l\u00e4ngst besessen.", "tokens": ["O", "das", "Em\u00b7py\u00b7ri\u00b7um", "hab'", "ich", "schon", "l\u00e4ngst", "be\u00b7ses\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "VAFIN", "PPER", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Mit pr\u00e4chtigen Regalien", "tokens": ["Mit", "pr\u00e4ch\u00b7ti\u00b7gen", "Re\u00b7ga\u00b7li\u00b7en"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Geh\u00f6rt mir ganz Thessalien,", "tokens": ["Ge\u00b7h\u00f6rt", "mir", "ganz", "Thes\u00b7sa\u00b7li\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "NE", "$,"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.11": {"text": "Und im Olymp sind zwanzig Hufen meine", "tokens": ["Und", "im", "O\u00b7lymp", "sind", "zwan\u00b7zig", "Hu\u00b7fen", "mei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "VAFIN", "CARD", "NN", "PPOSAT"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Mit Haus und Hof und Scheune.", "tokens": ["Mit", "Haus", "und", "Hof", "und", "Scheu\u00b7ne", "."], "token_info"