{"dta.poem.11902": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "63.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Von einer Seele tr\u00e4umt' ich, einer fernen lieben,", "tokens": ["Von", "ei\u00b7ner", "See\u00b7le", "tr\u00e4umt'", "ich", ",", "ei\u00b7ner", "fer\u00b7nen", "lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "$,", "ART", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "An die ich lange nicht gedacht und nicht geschrieben.", "tokens": ["An", "die", "ich", "lan\u00b7ge", "nicht", "ge\u00b7dacht", "und", "nicht", "ge\u00b7schrie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ADV", "PTKNEG", "VVPP", "KON", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "In der Erinnerung war mir das Angesicht", "tokens": ["In", "der", "E\u00b7rin\u00b7ne\u00b7rung", "war", "mir", "das", "An\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VAFIN", "PPER", "ART", "NN"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Erblichen, und nun zeigt' ein Traum es mir ganz licht.", "tokens": ["Er\u00b7bli\u00b7chen", ",", "und", "nun", "zeigt'", "ein", "Traum", "es", "mir", "ganz", "licht", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "ADV", "VVFIN", "ART", "NN", "PPER", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ich sprach im Traum: Wer sagt mir, was der Traum bedeute,", "tokens": ["Ich", "sprach", "im", "Traum", ":", "Wer", "sagt", "mir", ",", "was", "der", "Traum", "be\u00b7deu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$.", "PWS", "VVFIN", "PPER", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df ich dich schleierlos erblick' im Glanz der Br\u00e4ute?", "tokens": ["Da\u00df", "ich", "dich", "schlei\u00b7er\u00b7los", "er\u00b7blick'", "im", "Glanz", "der", "Br\u00e4u\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADJD", "VVFIN", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Des andern Tages kam die Botschaft mir, es sei", "tokens": ["Des", "an\u00b7dern", "Ta\u00b7ges", "kam", "die", "Bot\u00b7schaft", "mir", ",", "es", "sei"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN", "PPER", "$,", "PPER", "VAFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die liebe Seele hingegangen k\u00f6rperfrei.", "tokens": ["Die", "lie\u00b7be", "See\u00b7le", "hin\u00b7ge\u00b7gan\u00b7gen", "k\u00f6r\u00b7per\u00b7frei", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Das hat der Traum gemeint, da\u00df sie nicht ist gestorben,", "tokens": ["Das", "hat", "der", "Traum", "ge\u00b7meint", ",", "da\u00df", "sie", "nicht", "ist", "ge\u00b7stor\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "VVPP", "$,", "KOUS", "PPER", "PTKNEG", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df sie den rechten Glanz des Daseyns nun erworben.", "tokens": ["Da\u00df", "sie", "den", "rech\u00b7ten", "Glanz", "des", "Da\u00b7seyns", "nun", "er\u00b7wor\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}}}}}