{"dta.poem.9775": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auf die hochzeit Hn. C. B. mit jungfer  \n M. E. W.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Djr w\u00fcnsch' ich/ werther freund/ mehr freudenreiche\nstunden;", "tokens": ["Djr", "w\u00fcn\u00b7sch'", "ich", "/", "wert\u00b7her", "freund", "/", "mehr", "freu\u00b7den\u00b7rei\u00b7che", "stun\u00b7den", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "$(", "ADJA", "NN", "$(", "PIAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.2": {"text": "Als anmuths-rosen auf derselben wangen steh\u2019n/", "tokens": ["Als", "an\u00b7muths\u00b7ro\u00b7sen", "auf", "der\u00b7sel\u00b7ben", "wan\u00b7gen", "steh'n", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "PDAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die durch des himmels trieb sich deiner hand verbunden/", "tokens": ["Die", "durch", "des", "him\u00b7mels", "trieb", "sich", "dei\u00b7ner", "hand", "ver\u00b7bun\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "PRF", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und will/ o sch\u00f6ne nacht! mit dir zu bette geh\u2019n;", "tokens": ["Und", "will", "/", "o", "sch\u00f6\u00b7ne", "nacht", "!", "mit", "dir", "zu", "bet\u00b7te", "geh'n", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "$(", "FM", "ADJA", "NN", "$.", "APPR", "PPER", "PTKZU", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Dir w\u00fcnsch ich einen may voll lieblicher je\u00dfminen/", "tokens": ["Dir", "w\u00fcnsch", "ich", "ei\u00b7nen", "may", "voll", "lieb\u00b7li\u00b7cher", "je\u00df\u00b7mi\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NE", "ADJD", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die keine zeit verdirbt/ und immerw\u00e4hrend gr\u00fcnen.", "tokens": ["Die", "kei\u00b7ne", "zeit", "ver\u00b7dirbt", "/", "und", "im\u00b7mer\u00b7w\u00e4h\u00b7rend", "gr\u00fc\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "$(", "KON", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Mir aber w\u00fcnsch ich glut und flammenreiche sinnen/", "tokens": ["Mir", "a\u00b7ber", "w\u00fcnsch", "ich", "glut", "und", "flam\u00b7men\u00b7rei\u00b7che", "sin\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PPER", "NN", "KON", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein blat mit amber-rauch und bisam-\u00f6l benetzt;", "tokens": ["Ein", "blat", "mit", "am\u00b7ber\u00b7rauch", "und", "bi\u00b7sam\u00b7\u00f6l", "be\u00b7netzt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "KON", "NE", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und einen kiel/ daraus Budorgis reden rinnen/", "tokens": ["Und", "ei\u00b7nen", "kiel", "/", "da\u00b7raus", "Bu\u00b7dor\u00b7gis", "re\u00b7den", "rin\u00b7nen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$(", "PAV", "NN", "VVINF", "VVINF", "$("], "meter": "-+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Damit der fremde reim einheimschen gleich gesch\u00e4tzt/", "tokens": ["Da\u00b7mit", "der", "frem\u00b7de", "reim", "ein\u00b7heim\u00b7schen", "gleich", "ge\u00b7sch\u00e4tzt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "ADJA", "NN", "VVFIN", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Von dir/ geehrter freund/ mit fr\u00f6lichen geberden/", "tokens": ["Von", "dir", "/", "ge\u00b7ehr\u00b7ter", "freund", "/", "mit", "fr\u00f6\u00b7li\u00b7chen", "ge\u00b7ber\u00b7den", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$(", "ADJA", "NN", "$(", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Bey deinem freuden-fest kan durchgelesen werden.", "tokens": ["Bey", "dei\u00b7nem", "freu\u00b7den\u00b7fest", "kan", "durch\u00b7ge\u00b7le\u00b7sen", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VMFIN", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Es stund mir dazumahl ein feld der freuden offen/", "tokens": ["Es", "stund", "mir", "da\u00b7zu\u00b7mahl", "ein", "feld", "der", "freu\u00b7den", "of\u00b7fen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ART", "NN", "ART", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich wei\u00df nicht/ wie ich dich dasselbe mahl empfieng;", "tokens": ["Ich", "wei\u00df", "nicht", "/", "wie", "ich", "dich", "das\u00b7sel\u00b7be", "mahl", "emp\u00b7fi\u00b7eng", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$(", "PWAV", "PPER", "PRF", "PDAT", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.3": {"text": "Als ich dich reisenden zum ersten angetroffen/", "tokens": ["Als", "ich", "dich", "rei\u00b7sen\u00b7den", "zum", "ers\u00b7ten", "an\u00b7ge\u00b7trof\u00b7fen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADJD", "APPRART", "ADJA", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da deiner k\u00fcnste schiff gef\u00fcll\u2019t zu hause gieng/", "tokens": ["Da", "dei\u00b7ner", "k\u00fcns\u00b7te", "schiff", "ge\u00b7f\u00fcll't", "zu", "hau\u00b7se", "gieng", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "VVPP", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und du aus Preussen schon mit dem zur\u00fccke kommen/", "tokens": ["Und", "du", "aus", "Preus\u00b7sen", "schon", "mit", "dem", "zu\u00b7r\u00fc\u00b7cke", "kom\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "NN", "ADV", "APPR", "ART", "VVFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Was ich mir in der Marck zu holen vorgenommen.", "tokens": ["Was", "ich", "mir", "in", "der", "Marck", "zu", "ho\u00b7len", "vor\u00b7ge\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PRF", "APPR", "ART", "NN", "PTKZU", "VVINF", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Der inhalt dessen war: (so viel ich noch gedencke)", "tokens": ["Der", "in\u00b7halt", "des\u00b7sen", "war", ":(", "so", "viel", "ich", "noch", "ge\u00b7den\u00b7cke", ")"], "token_info": ["word", "word", "word", "word", "emoticon", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PDS", "VAFIN", "$(", "ADV", "ADV", "PPER", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Willkommen liebster freund/ der musen sch\u00f6nste zier;", "tokens": ["Will\u00b7kom\u00b7men", "liebs\u00b7ter", "freund", "/", "der", "mu\u00b7sen", "sch\u00f6ns\u00b7te", "zier", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$(", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Du kommst vom Helicon/ wohin ich mich erst lencke/", "tokens": ["Du", "kommst", "vom", "He\u00b7li\u00b7con", "/", "wo\u00b7hin", "ich", "mich", "erst", "len\u00b7cke", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$(", "PWAV", "PPER", "PRF", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der H\u00f6chste leite dich/ und sey zugleich mit mir:", "tokens": ["Der", "H\u00f6chs\u00b7te", "lei\u00b7te", "dich", "/", "und", "sey", "zu\u00b7gleich", "mit", "mir", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$(", "KON", "VAFIN", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Er lasse dich zu hau\u00df in steten freuden schweben/", "tokens": ["Er", "las\u00b7se", "dich", "zu", "hau\u00df", "in", "ste\u00b7ten", "freu\u00b7den", "schwe\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "NN", "APPR", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und mir von deinem thun viel gute nachricht geben.", "tokens": ["Und", "mir", "von", "dei\u00b7nem", "thun", "viel", "gu\u00b7te", "nach\u00b7richt", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "PPOSAT", "VVFIN", "PIAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}