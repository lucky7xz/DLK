{"textgrid.poem.50605": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "An einen Splitterrichter", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Magst du, was meine Fehler sind,", "tokens": ["Magst", "du", ",", "was", "mei\u00b7ne", "Feh\u00b7ler", "sind", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "$,", "PRELS", "PPOSAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von meinem Feinde gerne h\u00f6ren,", "tokens": ["Von", "mei\u00b7nem", "Fein\u00b7de", "ger\u00b7ne", "h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "So la\u00df auch meinen Freund dich lehren,", "tokens": ["So", "la\u00df", "auch", "mei\u00b7nen", "Freund", "dich", "leh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "ADV", "PPOSAT", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Was man bei mir f\u00fcr Tugend find't;", "tokens": ["Was", "man", "bei", "mir", "f\u00fcr", "Tu\u00b7gend", "find't", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "APPR", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Spricht man dort \u00fcbel ohne Maa\u00dfe,", "tokens": ["Spricht", "man", "dort", "\u00fc\u00b7bel", "oh\u00b7ne", "Maa\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und legt man hier zu viel mir zu,", "tokens": ["Und", "legt", "man", "hier", "zu", "viel", "mir", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "ADV", "PTKA", "PIS", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "So w\u00e4hle du die Mittelstra\u00dfe", "tokens": ["So", "w\u00e4h\u00b7le", "du", "die", "Mit\u00b7tel\u00b7stra\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und denk', ich bin ein Mensch, wie du.", "tokens": ["Und", "denk'", ",", "ich", "bin", "ein", "Mensch", ",", "wie", "du", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PPER", "VAFIN", "ART", "NN", "$,", "PWAV", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Magst du, was meine Fehler sind,", "tokens": ["Magst", "du", ",", "was", "mei\u00b7ne", "Feh\u00b7ler", "sind", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "$,", "PRELS", "PPOSAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von meinem Feinde gerne h\u00f6ren,", "tokens": ["Von", "mei\u00b7nem", "Fein\u00b7de", "ger\u00b7ne", "h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "So la\u00df auch meinen Freund dich lehren,", "tokens": ["So", "la\u00df", "auch", "mei\u00b7nen", "Freund", "dich", "leh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "ADV", "