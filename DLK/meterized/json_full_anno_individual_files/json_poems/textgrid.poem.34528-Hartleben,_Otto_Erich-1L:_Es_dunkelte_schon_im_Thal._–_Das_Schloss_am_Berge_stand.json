{"textgrid.poem.34528": {"metadata": {"author": {"name": "Hartleben, Otto Erich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es dunkelte schon im Thal. \u2013 Das Schloss am Berge stand", "genre": "verse", "period": "N.A.", "pub_year": 1893, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es dunkelte schon im Thal. \u2013 Das Schloss am Berge stand", "tokens": ["Es", "dun\u00b7kel\u00b7te", "schon", "im", "Thal", ".", "\u2013", "Das", "Schloss", "am", "Ber\u00b7ge", "stand"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "NN", "$.", "$(", "ART", "NN", "APPRART", "NN", "VVFIN"], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "gespenstisch gross im gelben Abendsonnenschein.", "tokens": ["ge\u00b7spens\u00b7tisch", "gross", "im", "gel\u00b7ben", "A\u00b7bend\u00b7son\u00b7nen\u00b7schein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Doch gegen\u00fcber auf dem Friedhof, der sich rings", "tokens": ["Doch", "ge\u00b7gen\u00b7\u00fc\u00b7ber", "auf", "dem", "Fried\u00b7hof", ",", "der", "sich", "rings"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "ART", "NN", "$,", "PRELS", "PRF", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "mit weisser Mauer um die alte Kirche schloss,", "tokens": ["mit", "weis\u00b7ser", "Mau\u00b7er", "um", "die", "al\u00b7te", "Kir\u00b7che", "schloss", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measu