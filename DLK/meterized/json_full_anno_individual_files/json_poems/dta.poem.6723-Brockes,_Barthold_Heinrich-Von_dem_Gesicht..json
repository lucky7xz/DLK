{"dta.poem.6723": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von dem Gesicht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Man kennt Gelehrte zwar, die anders dencken,", "tokens": ["Man", "kennt", "Ge\u00b7lehr\u00b7te", "zwar", ",", "die", "an\u00b7ders", "den\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "NN", "ADV", "$,", "PRELS", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die sich voll Eigensinn zum P\u00f6bels Jrrthum lencken.", "tokens": ["Die", "sich", "voll", "Ei\u00b7gen\u00b7sinn", "zum", "P\u00f6\u00b7bels", "Jrr\u00b7thum", "len\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "ADJD", "NN", "APPRART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Jhr Vorurtheil formirt die falsche Schl\u00fcsse,", "tokens": ["Ihr", "Vor\u00b7urt\u00b7heil", "for\u00b7mirt", "die", "fal\u00b7sche", "Schl\u00fcs\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da\u00df, was ein C\u00f6rper w\u00fcrckt, er auch besitzen m\u00fcsse.", "tokens": ["Da\u00df", ",", "was", "ein", "C\u00f6r\u00b7per", "w\u00fcrckt", ",", "er", "auch", "be\u00b7sit\u00b7zen", "m\u00fcs\u00b7se", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "PRELS", "ART", "NN", "VVFIN", "$,", "PPER", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ist wol die Art und Weise, wie wir sehn,", "tokens": ["Ist", "wol", "die", "Art", "und", "Wei\u00b7se", ",", "wie", "wir", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "KON", "NN", "$,", "PWAV", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Wie sie sie uns erkl\u00e4ren, zu verstehn?", "tokens": ["Wie", "sie", "sie", "uns", "er\u00b7kl\u00e4\u00b7ren", ",", "zu", "ver\u00b7stehn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "PRF", "VVINF", "$,", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}