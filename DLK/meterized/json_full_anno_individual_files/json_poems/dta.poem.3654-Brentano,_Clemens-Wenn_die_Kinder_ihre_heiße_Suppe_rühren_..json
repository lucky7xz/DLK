{"dta.poem.3654": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Wenn die Kinder ihre hei\u00dfe Suppe r\u00fchren .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519172", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Lirum larum L\u00f6ffelstiel,               ", "tokens": ["Li\u00b7rum", "la\u00b7rum", "L\u00f6f\u00b7fel\u00b7stiel", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Alte Weiber essen viel,", "tokens": ["Al\u00b7te", "Wei\u00b7ber", "es\u00b7sen", "viel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Junge m\u00fcssen fasten,", "tokens": ["Jun\u00b7ge", "m\u00fcs\u00b7sen", "fas\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Brod liegt im Kasten,", "tokens": ["Brod", "liegt", "im", "Kas\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Messer liegt daneben,", "tokens": ["Mes\u00b7ser", "liegt", "da\u00b7ne\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PAV", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Ey was ein lustig Leben!", "tokens": ["Ey", "was", "ein", "lus\u00b7tig", "Le\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PWS", "ART", "ADJD", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}}}}