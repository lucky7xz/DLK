{"textgrid.poem.34463": {"metadata": {"author": {"name": "Hartleben, Otto Erich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Gegangen bin ich \u00fcber braune Haide.", "genre": "verse", "period": "N.A.", "pub_year": 1886, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gegangen bin ich \u00fcber braune Haide.", "tokens": ["Ge\u00b7gan\u00b7gen", "bin", "ich", "\u00fc\u00b7ber", "brau\u00b7ne", "Hai\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "APPR", "ADJA", "NE", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Es knisterten die trocknen Haidekr\u00e4uter,", "tokens": ["Es", "knis\u00b7ter\u00b7ten", "die", "trock\u00b7nen", "Hai\u00b7de\u00b7kr\u00e4u\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "die ewig gleichen, unter meinen Schritten.", "tokens": ["die", "e\u00b7wig", "glei\u00b7chen", ",", "un\u00b7ter", "mei\u00b7nen", "Schrit\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVINF", "$,", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und ich verstand nicht, was sie knisterten.", "tokens": ["Und", "ich", "ver\u00b7stand", "nicht", ",", "was", "sie", "knis\u00b7ter\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PTKNEG", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Doch Todesschwermuth w\u00e4lzte sich auf mich,", "tokens": ["Doch", "To\u00b7des\u00b7schwer\u00b7muth", "w\u00e4lz\u00b7te", "sich", "auf", "mich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PRF", "APPR", "PPER", "$,"], "meter": "-+-+-+-++-", "measure": "unknown.measure.penta"}, "line.6": {"text": "indes die Augen m\u00fcd zu Boden starrten. \u2013", "tokens": ["in\u00b7des", "die", "Au\u00b7gen", "m\u00fcd", "zu", "Bo\u00b7den", "starr\u00b7ten", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ART", "NN", "ADJD", "APPR", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Gewandert bin ich durch die Palmenw\u00e4lder.", "tokens": ["Ge\u00b7wan\u00b7dert", "bin", "ich", "durch", "die", "Pal\u00b7men\u00b7w\u00e4l\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Es rauschten laut die hohen Wipfelh\u00e4upter,", "tokens": ["Es", "rauschten", "laut", "die", "ho\u00b7hen", "Wip\u00b7fel\u00b7h\u00e4up\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "die ewig gleichen, \u00fcber mir in L\u00fcften ...", "tokens": ["die", "e\u00b7wig", "glei\u00b7chen", ",", "\u00fc\u00b7ber", "mir", "in", "L\u00fcf\u00b7ten", "..."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVINF", "$,", "APPR", "PPER", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und da verstand ich, was die Palmen rauschten:", "tokens": ["Und", "da", "ver\u00b7stand", "ich", ",", "was", "die", "Pal\u00b7men", "rauschten", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "po