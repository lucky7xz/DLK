{"textgrid.poem.60000": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Der schlimmste Feind", "genre": "verse", "period": "N.A.", "pub_year": 1871, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dies Volk, das seine B\u00e4ume wieder", "tokens": ["Dies", "Volk", ",", "das", "sei\u00b7ne", "B\u00e4u\u00b7me", "wie\u00b7der"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "NN", "$,", "PRELS", "PPOSAT", "NN", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Bis in den Himmel wachsen sieht", "tokens": ["Bis", "in", "den", "Him\u00b7mel", "wach\u00b7sen", "sieht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "APPR", "ART", "NN", "VVINF", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und auf der Erde platt und bieder", "tokens": ["Und", "auf", "der", "Er\u00b7de", "platt", "und", "bie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "PTKVZ", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Am Knechtschaftskarren weiter zieht;", "tokens": ["Am", "Knecht\u00b7schafts\u00b7kar\u00b7ren", "wei\u00b7ter", "zieht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Dies Volk, das auf die Weisheit dessen", "tokens": ["Dies", "Volk", ",", "das", "auf", "die", "Weis\u00b7heit", "des\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PDS", "NN", "$,", "PRELS", "APPR", "ART", "NN", "PDS"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vertraut, der Ro\u00df und Reiter h\u00e4lt,", "tokens": ["Ver\u00b7traut", ",", "der", "Ro\u00df", "und", "Rei\u00b7ter", "h\u00e4lt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "ART", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und mit Ergebenheitsadressen", "tokens": ["Und", "mit", "Er\u00b7ge\u00b7ben\u00b7hei\u00b7tsad\u00b7res\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["KON", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Frisch, fromm und fr\u00f6hlich r\u00fcckt ins Feld;", "tokens": ["Frisch", ",", "fromm", "und", "fr\u00f6h\u00b7lich", "r\u00fcckt", "ins", "Feld", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJD", "KON", "ADJD", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Dies Volk, das einst aus C\u00e4sars Sch\u00fcssel", "tokens": ["Dies", "Volk", ",", "das", "einst", "aus", "C\u00e4\u00b7sars", "Sch\u00fcs\u00b7sel"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PDS", "NN", "$,", "PRELS", "ADV", "APPR", "NE", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und Becher sich so gern erfrischt", "tokens": ["Und", "Be\u00b7cher", "sich", "so", "gern", "er\u00b7frischt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "PRF", "ADV", "ADV", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und sich, wie Mommsen, seinen R\u00fcssel", "tokens": ["Und", "sich", ",", "wie", "Momm\u00b7sen", ",", "sei\u00b7nen", "R\u00fcs\u00b7sel"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KON", "PRF", "$,", "PWAV", "NN", "$,", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "An C\u00e4sars Tischtuch abgewischt;", "tokens": ["An", "C\u00e4\u00b7sars", "Tischtuch", "ab\u00b7ge\u00b7wischt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Dies Volk, das gegen Blut und Eisen", "tokens": ["Dies", "Volk", ",", "das", "ge\u00b7gen", "Blut", "und", "Ei\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PDS", "NN", "$,", "PRELS", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Jungfr\u00e4ulich sch\u00fcchtern sich geziert,", "tokens": ["Jung\u00b7fr\u00e4u\u00b7lich", "sch\u00fcch\u00b7tern", "sich", "ge\u00b7ziert", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Um schlie\u00dflich den Erfolg zu preisen,", "tokens": ["Um", "schlie\u00df\u00b7lich", "den", "Er\u00b7folg", "zu", "prei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADJD", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Womit man Stra\u00dfburg bombardiert.", "tokens": ["Wo\u00b7mit", "man", "Stra\u00df\u00b7burg", "bom\u00b7bar\u00b7diert", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "NE", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Dies Volk, das im gemeinen Kitzel", "tokens": ["Dies", "Volk", ",", "das", "im", "ge\u00b7mei\u00b7nen", "Kit\u00b7zel"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "NN", "$,", "PRELS", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Macht das neue Heil erblickt", "tokens": ["Der", "Macht", "das", "neu\u00b7e", "Heil", "er\u00b7blickt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und als \u00bbErzieher\u00ab seine Spitzel", "tokens": ["Und", "als", "\u00bb", "Er\u00b7zie\u00b7her", "\u00ab", "sei\u00b7ne", "Spit\u00b7zel"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["KON", "KOUS", "$(", "NN", "$(", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den unterjochten \u00bbBr\u00fcdern\u00ab schickt.", "tokens": ["Den", "un\u00b7ter\u00b7joch\u00b7ten", "\u00bb", "Br\u00fc\u00b7dern", "\u00ab", "schickt", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "$(", "NN", "$(", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Die Alten, Lieben, Wohlbekannten", "tokens": ["Die", "Al\u00b7ten", ",", "Lie\u00b7ben", ",", "Wohl\u00b7be\u00b7kann\u00b7ten"], "token_info": ["word", "word", "punct", "word", "punct", "word"], "pos": ["ART", "NN", "$,", "ADJA", "$,", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Von Anno Sechsundsechzig her,", "tokens": ["Von", "An\u00b7no", "Sech\u00b7sund\u00b7sech\u00b7zig", "her", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "CARD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Schafott- und Bundesbeil-Votanten,", "tokens": ["Scha\u00b7fot\u00b7t", "und", "Bun\u00b7des\u00b7beil\u00b7Vo\u00b7tan\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["TRUNC", "KON", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "Sie werden mit verschmitzten H\u00e4nden", "tokens": ["Sie", "wer\u00b7den", "mit", "ver\u00b7schmitz\u00b7ten", "H\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Entrei\u00dfen euch des Sieges Frucht;", "tokens": ["Ent\u00b7rei\u00b7\u00dfen", "euch", "des", "Sie\u00b7ges", "Frucht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie werden euren Lorbeer sch\u00e4nden,", "tokens": ["Sie", "wer\u00b7den", "eu\u00b7ren", "Lor\u00b7beer", "sch\u00e4n\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df euch die ganze Welt verflucht!", "tokens": ["Da\u00df", "euch", "die", "gan\u00b7ze", "Welt", "ver\u00b7flucht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Frankreichs gekr\u00f6nter Possenrei\u00dfer", "tokens": ["Fran\u00b7kreichs", "ge\u00b7kr\u00f6n\u00b7ter", "Pos\u00b7sen\u00b7rei\u00b7\u00dfer"], "token_info": ["word", "word", "word"], "pos": ["NE", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wird nach Paris zur\u00fcckgebracht;", "tokens": ["Wird", "nach", "Pa\u00b7ris", "zu\u00b7r\u00fcck\u00b7ge\u00b7bracht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "NE", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Euch holt man einen Heldenkaiser", "tokens": ["Euch", "holt", "man", "ei\u00b7nen", "Hel\u00b7den\u00b7kai\u00b7ser"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIS", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Aus mittelalterlicher Nacht.", "tokens": ["Aus", "mit\u00b7tel\u00b7al\u00b7ter\u00b7li\u00b7cher", "Nacht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Das Blut von W\u00f6rth, das Blut von Spichern,", "tokens": ["Das", "Blut", "von", "W\u00f6rth", ",", "das", "Blut", "von", "Spi\u00b7chern", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "$,", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Von Mars-la-Tour und Gravelotte,", "tokens": ["Von", "Mar\u00b7sla\u00b7Tour", "und", "Gra\u00b7ve\u00b7lot\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Einheit und Freiheit sollt es sichern \u2013", "tokens": ["Ein\u00b7heit", "und", "Frei\u00b7heit", "sollt", "es", "si\u00b7chern", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VMFIN", "PPER", "VVINF", "$("], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Einheit und Freiheit? Gro\u00dfer Gott!", "tokens": ["Ein\u00b7heit", "und", "Frei\u00b7heit", "?", "Gro\u00b7\u00dfer", "Gott", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$.", "ADJA", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.10": {"line.1": {"text": "Geeinigt wird Alt-Deutschland stehn;", "tokens": ["Ge\u00b7ei\u00b7nigt", "wird", "Al\u00b7tDeut\u00b7schland", "stehn", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dem Rausche folgt ein Katzenjammer,", "tokens": ["Dem", "Rau\u00b7sche", "folgt", "ein", "Kat\u00b7zen\u00b7jam\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df euch die Augen \u00fcbergehn.", "tokens": ["Da\u00df", "euch", "die", "Au\u00b7gen", "\u00fc\u00b7ber\u00b7gehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Mit patriotischem Erg\u00f6tzen", "tokens": ["Mit", "pat\u00b7ri\u00b7o\u00b7ti\u00b7schem", "Er\u00b7g\u00f6t\u00b7zen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Habt ihr Viktoria geknallt;", "tokens": ["Habt", "ihr", "Vik\u00b7to\u00b7ria", "ge\u00b7knallt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Der Rest ist Schweigen oder L\u00f6tzen,", "tokens": ["Der", "Rest", "ist", "Schwei\u00b7gen", "o\u00b7der", "L\u00f6t\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Kriegsidiotentum, Gewalt.", "tokens": ["Krie\u00b7gsi\u00b7dio\u00b7ten\u00b7tum", ",", "Ge\u00b7walt", "."], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$,", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Es wird die Fuchtel mit der Knute", "tokens": ["Es", "wird", "die", "Fuch\u00b7tel", "mit", "der", "Knu\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Heilige Allianz erneun;", "tokens": ["Die", "Hei\u00b7li\u00b7ge", "Al\u00b7li\u00b7anz", "er\u00b7neun", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Europa kann am \u00dcbermute", "tokens": ["Eu\u00b7ro\u00b7pa", "kann", "am", "\u00dc\u00b7ber\u00b7mu\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VMFIN", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Siegreicher Junker sich erfreun.", "tokens": ["Sieg\u00b7rei\u00b7cher", "Jun\u00b7ker", "sich", "er\u00b7freun", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "PRF", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.13": {"line.1": {"text": "Gleich Kindern la\u00dft ihr euch betr\u00fcgen,", "tokens": ["Gleich", "Kin\u00b7dern", "la\u00dft", "ihr", "euch", "be\u00b7tr\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVFIN", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Bis ihr zu sp\u00e4t erkennt, o weh! \u2013", "tokens": ["Bis", "ihr", "zu", "sp\u00e4t", "er\u00b7kennt", ",", "o", "weh", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["APPR", "PPOSAT", "PTKA", "ADJD", "VVFIN", "$,", "FM", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Wacht am Rhein wird nicht gen\u00fcgen,", "tokens": ["Die", "Wacht", "am", "Rhein", "wird", "nicht", "ge\u00b7n\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NE