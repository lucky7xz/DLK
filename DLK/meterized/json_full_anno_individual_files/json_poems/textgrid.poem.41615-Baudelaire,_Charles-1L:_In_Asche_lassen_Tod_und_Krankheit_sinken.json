{"textgrid.poem.41615": {"metadata": {"author": {"name": "Baudelaire, Charles", "birth": "N.A.", "death": "N.A."}, "title": "1L: In Asche lassen Tod und Krankheit sinken", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In Asche lassen Tod und Krankheit sinken", "tokens": ["In", "A\u00b7sche", "las\u00b7sen", "Tod", "und", "Krank\u00b7heit", "sin\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVINF", "NN", "KON", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die stolze Glut, die einst uns licht umfing.", "tokens": ["Die", "stol\u00b7ze", "Glut", ",", "die", "einst", "uns", "licht", "um\u00b7fing", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ADV", "PPER", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Von dieser gro\u00dfen Augen s\u00fc\u00dfem Blinken,", "tokens": ["Von", "die\u00b7ser", "gro\u00b7\u00dfen", "Au\u00b7gen", "s\u00fc\u00b7\u00dfem", "Blin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Von diesem Mund, daran mein Herz verging,", "tokens": ["Von", "die\u00b7sem", "Mund", ",", "da\u00b7ran", "mein", "Herz", "ver\u00b7ging", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,", "PAV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Von diesen K\u00fcssen, hold wie Balsamschauer,", "tokens": ["Von", "die\u00b7sen", "K\u00fcs\u00b7sen", ",", "hold", "wie", "Bal\u00b7sam\u