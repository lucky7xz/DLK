{"dta.poem.8625": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "An die Ersehnte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1832", "urn": "urn:nbn:de:kobv:b4-200905193572", "language": ["de:0.85", "fr:0.14"], "booktitle": "Lenau, Nikolaus: Gedichte. Stuttgart, 1832."}, "poem": {"stanza.1": {"line.1": {"text": "Umsonst! du bist auf immer mir verloren!", "tokens": ["Um\u00b7sonst", "!", "du", "bist", "auf", "im\u00b7mer", "mir", "ver\u00b7lo\u00b7ren", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "PPER", "VAFIN", "APPR", "ADV", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Laut rufend in den dunkeln Wald des Lebens,", "tokens": ["Laut", "ru\u00b7fend", "in", "den", "dun\u00b7keln", "Wald", "des", "Le\u00b7bens", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "APPR", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Hat ohne Rast die Sehnsucht dich beschworen;", "tokens": ["Hat", "oh\u00b7ne", "Rast", "die", "Sehn\u00b7sucht", "dich", "be\u00b7schwo\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "NN", "ART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ihr Ruf durchklang die Einsamkeit vergebens.", "tokens": ["Ihr", "Ruf", "durch\u00b7klang", "die", "Ein\u00b7sam\u00b7keit", "ver\u00b7ge\u00b7bens", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Tief ist mein Herz erkrankt an einer Ahnung,", "tokens": ["Tief", "ist", "mein", "Herz", "er\u00b7krankt", "an", "ei\u00b7ner", "Ah\u00b7nung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPOSAT", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von der ich nimmer wohl genesen werde,", "tokens": ["Von", "der", "ich", "nim\u00b7mer", "wohl", "ge\u00b7ne\u00b7sen", "wer\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ADV", "ADV", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Es fl\u00fcstert mir mein Herz die tr\u00fcbe Mahnung;", "tokens": ["Es", "fl\u00fcs\u00b7tert", "mir", "mein", "Herz", "die", "tr\u00fc\u00b7be", "Mah\u00b7nung", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PPOSAT", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "\u201enoch ist sie nicht geboren dieser Erde!\u201c", "tokens": ["\u201e", "noch", "ist", "sie", "nicht", "ge\u00b7bo\u00b7ren", "die\u00b7ser", "Er\u00b7de", "!", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "VAFIN", "PPER", "PTKNEG", "VVPP", "PDAT", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "\u201edie Stunden, die mit frohen Wanders\u00e4ngen", "tokens": ["\u201e", "die", "Stun\u00b7den", ",", "die", "mit", "fro\u00b7hen", "Wan\u00b7der\u00b7s\u00e4n\u00b7gen"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "ART", "NN", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "\u201edas M\u00e4dchen einst durchs Erdenthal geleiten,", "tokens": ["\u201e", "das", "M\u00e4d\u00b7chen", "einst", "durchs", "Er\u00b7dent\u00b7hal", "ge\u00b7lei\u00b7ten", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "ADV", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "\u201esie schlummern in der Zukunft Schatteng\u00e4ngen", "tokens": ["\u201e", "sie", "schlum\u00b7mern", "in", "der", "Zu\u00b7kunft", "Schat\u00b7ten\u00b7g\u00e4n\u00b7gen"], "token_info": ["punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "PPER", "VVFIN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "\u201ebei ihrer B\u00fcrde noch von Seligkeiten,", "tokens": ["\u201e", "bei", "ih\u00b7rer", "B\u00fcr\u00b7de", "noch", "von", "Se\u00b7lig\u00b7kei\u00b7ten", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "PPOSAT", "NN", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "\u201evon Seligkeiten, die mit leichten H\u00e4nden", "tokens": ["\u201e", "von", "Se\u00b7lig\u00b7kei\u00b7ten", ",", "die", "mit", "leich\u00b7ten", "H\u00e4n\u00b7den"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "APPR", "NN", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "\u201edie wachen einst entgegenstreuen Allen,", "tokens": ["\u201e", "die", "wa\u00b7chen", "einst", "ent\u00b7ge\u00b7gen\u00b7streu\u00b7en", "Al\u00b7len", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "ADJA", "ADV", "VVFIN", "NE", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "\u201ean welche sie die sch\u00f6ne Gunst verschwenden,", "tokens": ["\u201e", "an", "wel\u00b7che", "sie", "die", "sch\u00f6\u00b7ne", "Gunst", "ver\u00b7schwen\u00b7den", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "PRELS", "PPER", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "\u201emit ihrer K\u00f6nigin vorbeizuwallen.", "tokens": ["\u201e", "mit", "ih\u00b7rer", "K\u00f6\u00b7ni\u00b7gin", "vor\u00b7bei\u00b7zu\u00b7wal\u00b7len", "."], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "\u201edie eine aber von den Schl\u00e4ferinnen", "tokens": ["\u201e", "die", "ei\u00b7ne", "a\u00b7ber", "von", "den", "Schl\u00e4\u00b7fe\u00b7rin\u00b7nen"], "token_info": ["punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "ART", "ART", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "\u201ewird locken sie zur K\u00fchle von Cypressen,", "tokens": ["\u201e", "wird", "lo\u00b7cken", "sie", "zur", "K\u00fch\u00b7le", "von", "Cyp\u00b7res\u00b7sen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "VVFIN", "PPER", "APPRART", "NN", "APPR", "NE", "$,"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "\u201eund f\u00fchren sie, versenkt in stilles Sinnen,", "tokens": ["\u201e", "und", "f\u00fch\u00b7ren", "sie", ",", "ver\u00b7senkt", "in", "stil\u00b7les", "Sin\u00b7nen", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "VVFIN", "PPER", "$,", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "\u201ean deinen H\u00fcgel, moosig und vergessen;", "tokens": ["\u201e", "an", "dei\u00b7nen", "H\u00fc\u00b7gel", ",", "moo\u00b7sig", "und", "ver\u00b7ges\u00b7sen", ";"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "PPOSAT", "NN", "$,", "ADJD", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "\u201edann irrt dein Geist um deine Asche bange,", "tokens": ["\u201e", "dann", "irrt", "dein", "Geist", "um", "dei\u00b7ne", "A\u00b7sche", "ban\u00b7ge", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "\u201edann zittern Geist und Staub sich zu vereinen;", "tokens": ["\u201e", "dann", "zit\u00b7tern", "Geist", "und", "Staub", "sich", "zu", "ver\u00b7ei\u00b7nen", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "NN", "KON", "NN", "PRF", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "\u201edas M\u00e4dchen aber wird am Grabeshange,", "tokens": ["\u201e", "das", "M\u00e4d\u00b7chen", "a\u00b7ber", "wird", "am", "Gra\u00b7bes\u00b7han\u00b7ge", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "ADV", "VAFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "\u201egeheim ergriffen, stille stehn \u2014 und weinen.\u201c", "tokens": ["\u201e", "ge\u00b7heim", "er\u00b7grif\u00b7fen", ",", "stil\u00b7le", "stehn", "und", "wei\u00b7nen", ".", "\u201c"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["$(", "ADJD", "VVPP", "$,", "ADJA", "VVINF", "$(", "KON", "VVINF", "$.", "$("], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}}}}}