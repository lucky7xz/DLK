{"dta.poem.3298": {"metadata": {"author": {"name": "Uz, Johann Peter", "birth": "N.A.", "death": "N.A."}, "title": "Neujahrs-Wunsch  \n des  \n  Nachtw\u00e4chters zu Ternate.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1755", "urn": "urn:nbn:de:kobv:b4-20614-6", "language": ["de:0.99"], "booktitle": "Uz, Johann Peter: Lyrische und andere Gedichte. 2. Aufl. Ansbach, 1755."}, "poem": {"stanza.1": {"line.1": {"text": "Weckt eure Gatten k\u00fcssend auf,", "tokens": ["Weckt", "eu\u00b7re", "Gat\u00b7ten", "k\u00fcs\u00b7send", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Jhr Sch\u00f6nen von Ternate!", "tokens": ["Ihr", "Sch\u00f6\u00b7nen", "von", "Ter\u00b7na\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "NE", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "H\u00f6rt, bey des Jahres neuem Lauf,", "tokens": ["H\u00f6rt", ",", "bey", "des", "Jah\u00b7res", "neu\u00b7em", "Lauf", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie mir ein Wunsch gerathe!", "tokens": ["Wie", "mir", "ein", "Wunsch", "ge\u00b7ra\u00b7the", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ein M\u00e4dchen, das sich Muse nennt,", "tokens": ["Ein", "M\u00e4d\u00b7chen", ",", "das", "sich", "Mu\u00b7se", "nennt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PRF", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Durchstreicht mit mir die Strassen;", "tokens": ["Durch\u00b7streicht", "mit", "mir", "die", "Stras\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und was mein Herz euch gutes g\u00f6nnt,", "tokens": ["Und", "was", "mein", "Herz", "euch", "gu\u00b7tes", "g\u00f6nnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPOSAT", "NN", "PPER", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Will sie in Reime fassen.", "tokens": ["Will", "sie", "in", "Rei\u00b7me", "fas\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Wohlan! die Freude werde neu,", "tokens": ["Wo\u00b7hlan", "!", "die", "Freu\u00b7de", "wer\u00b7de", "neu", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie sich das Jahr verneuet!", "tokens": ["Wie", "sich", "das", "Jahr", "ver\u00b7neu\u00b7et", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Es fliehe finstre Heucheley,", "tokens": ["Es", "flie\u00b7he", "finst\u00b7re", "Heu\u00b7che\u00b7ley", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die sich im Winkel freuet!", "tokens": ["Die", "sich", "im", "Win\u00b7kel", "freu\u00b7et", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Richt Eigennutz, nur Z\u00e4rtlichkeit", "tokens": ["Richt", "Ei\u00b7gen\u00b7nutz", ",", "nur", "Z\u00e4rt\u00b7lich\u00b7keit"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NN", "NN", "$,", "ADV", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sey Stifter unsrer Ehen:", "tokens": ["Sey", "Stif\u00b7ter", "uns\u00b7rer", "E\u00b7hen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So wird man Hymens g\u00fcldne Zeit", "tokens": ["So", "wird", "man", "Hy\u00b7mens", "g\u00fcld\u00b7ne", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PIS", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auch Jahre dauern sehen.", "tokens": ["Auch", "Jah\u00b7re", "dau\u00b7ern", "se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVINF", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Die s\u00fcsse Falschheit unsrer Zeit", "tokens": ["Die", "s\u00fcs\u00b7se", "Falschheit", "uns\u00b7rer", "Zeit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PPOSAT", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Entweiche von der Erde,", "tokens": ["Ent\u00b7wei\u00b7che", "von", "der", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df alte wahre Redlichkeit", "tokens": ["Da\u00df", "al\u00b7te", "wah\u00b7re", "Red\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Noch einmal Mode werde.", "tokens": ["Noch", "ein\u00b7mal", "Mo\u00b7de", "wer\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "NN", "VAFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Es drohe Miswachs und Verlust", "tokens": ["Es", "dro\u00b7he", "Mis\u00b7wachs", "und", "Ver\u00b7lust"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Gelehrten Schmierereyen:", "tokens": ["Ge\u00b7lehr\u00b7ten", "Schmie\u00b7re\u00b7re\u00b7yen", ":"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Nur m\u00fcsse junger M\u00e4dchen Brust", "tokens": ["Nur", "m\u00fcs\u00b7se", "jun\u00b7ger", "M\u00e4d\u00b7chen", "Brust"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und guter Wein gedeihen!", "tokens": ["Und", "gu\u00b7ter", "Wein", "ge\u00b7dei\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Gib, Himmel! deinen alten Wein", "tokens": ["Gib", ",", "Him\u00b7mel", "!", "dei\u00b7nen", "al\u00b7ten", "Wein"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["VVIMP", "$,", "NN", "$.", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den fr\u00f6hligen Poeten,", "tokens": ["Den", "fr\u00f6h\u00b7li\u00b7gen", "Po\u00b7et\u00b7en", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die in der Musen Lorbeerhayn", "tokens": ["Die", "in", "der", "Mu\u00b7sen", "Lor\u00b7beer\u00b7hayn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Oft, leider! durstig treten.", "tokens": ["Oft", ",", "lei\u00b7der", "!", "durs\u00b7tig", "tre\u00b7ten", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "$.", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Nur Wasser, alter Weisen Trank,", "tokens": ["Nur", "Was\u00b7ser", ",", "al\u00b7ter", "Wei\u00b7sen", "Trank", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "$,", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gib unsern jungen Weisen;", "tokens": ["Gib", "un\u00b7sern", "jun\u00b7gen", "Wei\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und jage den Monaden-Zank", "tokens": ["Und", "ja\u00b7ge", "den", "Mo\u00b7na\u00b7den\u00b7Zank"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN"], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.4": {"text": "Von freudenvollen Schm\u00e4usen.", "tokens": ["Von", "freu\u00b7den\u00b7vol\u00b7len", "Schm\u00e4u\u00b7sen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Der Geiz mag sein erwuchert Gut", "tokens": ["Der", "Geiz", "mag", "sein", "er\u00b7wuc\u00b7hert", "Gut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "PPOSAT", "VVPP", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nur h\u00fcten, nicht genie\u00dfen!", "tokens": ["Nur", "h\u00fc\u00b7ten", ",", "nicht", "ge\u00b7nie\u00b7\u00dfen", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVINF", "$,", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Doch la\u00df ein B\u00e4chlein g\u00fcldner Fluth", "tokens": ["Doch", "la\u00df", "ein", "B\u00e4ch\u00b7lein", "g\u00fcld\u00b7ner", "Fluth"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVIMP", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auch auf den Weisen fliessen!", "tokens": ["Auch", "auf", "den", "Wei\u00b7sen", "flies\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Denn unsre Weibchen kosten viel,", "tokens": ["Denn", "uns\u00b7re", "Weib\u00b7chen", "kos\u00b7ten", "viel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn sie uns lieben sollen:", "tokens": ["Wenn", "sie", "uns", "lie\u00b7ben", "sol\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wieviel erfordert Putz und Spiel", "tokens": ["Wie\u00b7viel", "er\u00b7for\u00b7dert", "Putz", "und", "Spiel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und wann wir schmausen wollen!", "tokens": ["Und", "wann", "wir", "schmau\u00b7sen", "wol\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Heil allen, denen Heil gebricht;", "tokens": ["Heil", "al\u00b7len", ",", "de\u00b7nen", "Heil", "ge\u00b7bricht", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "$,", "PRELS", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Heil sey dem ganzen Staate!", "tokens": ["Heil", "sey", "dem", "gan\u00b7zen", "Staa\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die\u00df w\u00fcnsch ich aus bezahlter Pflicht,", "tokens": ["Die\u00df", "w\u00fcnsch", "ich", "aus", "be\u00b7zahl\u00b7ter", "Pflicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nachtw\u00e4chter von Ternate.", "tokens": ["Nacht\u00b7w\u00e4ch\u00b7ter", "von", "Ter\u00b7na\u00b7te", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NE", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}