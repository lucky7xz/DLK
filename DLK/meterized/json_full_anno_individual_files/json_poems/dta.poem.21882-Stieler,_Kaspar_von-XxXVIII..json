{"dta.poem.21882": {"metadata": {"author": {"name": "Stieler, Kaspar von", "birth": "N.A.", "death": "N.A."}, "title": "XxXVIII.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1660", "urn": "urn:nbn:de:kobv:b4-20586-2", "language": ["de:0.99"], "booktitle": "Filidor der Dorfferer [i. e. Stieler, Kaspar von]: Die Geharnschte Venus. Hamburg, 1660."}, "poem": {"stanza.1": {"line.1": {"text": "Mein Lieben fing sich an im Schnee/", "tokens": ["Mein", "Lie\u00b7ben", "fing", "sich", "an", "im", "Schnee", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "VVFIN", "PRF", "APPR", "APPRART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "weil ich zuviel den Schnee beschauet.", "tokens": ["weil", "ich", "zu\u00b7viel", "den", "Schnee", "be\u00b7schau\u00b7et", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Auch ists der Schnee/ vor dehm mir", "tokens": ["Auch", "ists", "der", "Schnee", "/", "vor", "dehm", "mir"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "$(", "APPR", "PRELS", "PPER"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "der Schnee/ den ich stets vor mir seh.", "tokens": ["der", "Schnee", "/", "den", "ich", "stets", "vor", "mir", "seh", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PRELS", "PPER", "ADV", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Schnee komm/ Schnee weich/ so wil ich", "tokens": ["Schnee", "komm", "/", "Schnee", "weich", "/", "so", "wil", "ich"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVFIN", "$(", "NN", "ADJD", "$(", "ADV", "VMFIN", "PPER"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.6": {"text": "da\u00df dich der Himmel hat getragen.", "tokens": ["da\u00df", "dich", "der", "Him\u00b7mel", "hat", "ge\u00b7tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}