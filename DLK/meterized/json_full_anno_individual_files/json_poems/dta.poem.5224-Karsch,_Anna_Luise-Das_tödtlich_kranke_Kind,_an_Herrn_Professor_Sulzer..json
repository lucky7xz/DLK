{"dta.poem.5224": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "Das t\u00f6dtlich kranke Kind,  \n an Herrn Professor Sulzer.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1764", "urn": "urn:nbn:de:kobv:b4-200905197778", "language": ["de:0.99"], "booktitle": "Karsch, Anna Luise: Auserlesene Gedichte. Berlin, 1764."}, "poem": {"stanza.1": {"line.1": {"text": "Ihr traurig Bild mu\u00df immer vor dir schweben", "tokens": ["Ihr", "trau\u00b7rig", "Bild", "mu\u00df", "im\u00b7mer", "vor", "dir", "schwe\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADJD", "NN", "VMFIN", "ADV", "APPR", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Bey Freunden, bey Gesch\u00e4ften deiner Pflicht!", "tokens": ["Bey", "Freun\u00b7den", ",", "bey", "Ge\u00b7sch\u00e4f\u00b7ten", "dei\u00b7ner", "Pflicht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "APPR", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Schon zeichnet sich in ihrem Angesicht", "tokens": ["Schon", "zeich\u00b7net", "sich", "in", "ih\u00b7rem", "An\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der Tod, kaum athmet noch in ihr das matte Leben!", "tokens": ["Der", "Tod", ",", "kaum", "ath\u00b7met", "noch", "in", "ihr", "das", "mat\u00b7te", "Le\u00b7ben", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "VVFIN", "ADV", "APPR", "PPOSAT", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Des Arztes Kunst, und treuster bester Flei\u00df", "tokens": ["Des", "Arz\u00b7tes", "Kunst", ",", "und", "treus\u00b7ter", "bes\u00b7ter", "Flei\u00df"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "KON", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Ist hier umsonst; denn, kein Vernunftschlu\u00df wei\u00df", "tokens": ["Ist", "hier", "um\u00b7sonst", ";", "denn", ",", "kein", "Ver\u00b7nunft\u00b7schlu\u00df", "wei\u00df"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ADV", "$.", "ADV", "$,", "PIAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Den h\u00f6hern Rathschlu\u00df je zu hintertreiben,", "tokens": ["Den", "h\u00f6\u00b7hern", "Rath\u00b7schlu\u00df", "je", "zu", "hin\u00b7ter\u00b7trei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Der Engeln nicht erlaubt, auf dieser Welt zu bleiben.", "tokens": ["Der", "En\u00b7geln", "nicht", "er\u00b7laubt", ",", "auf", "die\u00b7ser", "Welt", "zu", "blei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "VVPP", "$,", "APPR", "PDAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}