{"textgrid.poem.40654": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Im Hofraum fl\u00fcstert noch der alte Bronnen", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im Hofraum fl\u00fcstert noch der alte Bronnen", "tokens": ["Im", "Hof\u00b7raum", "fl\u00fcs\u00b7tert", "noch", "der", "al\u00b7te", "Bron\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie einst, als diese Mauern Klosterhallen,", "tokens": ["Wie", "einst", ",", "als", "die\u00b7se", "Mau\u00b7ern", "Klos\u00b7ter\u00b7hal\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "$,", "KOUS", "PDAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Er sah im Zwangshabit einst M\u00f6nche wallen", "tokens": ["Er", "sah", "im", "Zwangs\u00b7ha\u00b7bit", "einst", "M\u00f6n\u00b7che", "wal\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ADV", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und sang sie ein in der Verz\u00fcckung Wonnen.", "tokens": ["Und", "sang", "sie", "ein", "in", "der", "Ver\u00b7z\u00fc\u00b7ckung", "Won\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Doch andern Kultus hat der Herr ersonnen,", "tokens": ["Doch", "an\u00b7dern", "Kul\u00b7tus", "hat", "der", "Herr", "er\u00b7son\u00b7nen", ","], "