{"dta.poem.19880": {"metadata": {"author": {"name": "B\u00fcrger, Gottfried August", "birth": "N.A.", "death": "N.A."}, "title": "Abendfantasie eines Liebenden.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1778", "urn": "urn:nbn:de:kobv:b4-20090519672", "language": ["de:0.99"], "booktitle": "B\u00fcrger, Gottfried August: Gedichte. G\u00f6ttingen, 1778."}, "poem": {"stanza.1": {"line.1": {"text": "In weiche Ruh hinabgesunken,               ", "tokens": ["In", "wei\u00b7che", "Ruh", "hin\u00b7ab\u00b7ge\u00b7sun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Unaufgest\u00f6rt von Harm und Not,", "tokens": ["Un\u00b7auf\u00b7ge\u00b7st\u00f6rt", "von", "Harm", "und", "Not", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Vom s\u00fcssen Labebecher trunken,", "tokens": ["Vom", "s\u00fcs\u00b7sen", "La\u00b7be\u00b7be\u00b7cher", "trun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den ihr der Gott des Schlummers bot,", "tokens": ["Den", "ihr", "der", "Gott", "des", "Schlum\u00b7mers", "bot", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Woleingelult vom Abendliede", "tokens": ["Wo\u00b7lein\u00b7ge\u00b7lult", "vom", "A\u00b7ben\u00b7dlie\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["NE", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der wachen Freundin Nachtigal,", "tokens": ["Der", "wa\u00b7chen", "Freun\u00b7din", "Nach\u00b7ti\u00b7gal", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Schl\u00e4ft meine Herzens-Adonide", "tokens": ["Schl\u00e4ft", "mei\u00b7ne", "Her\u00b7zens\u00b7A\u00b7do\u00b7ni\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Nun ihr beh\u00e4glich Schl\u00e4fchen al.", "tokens": ["Nun", "ihr", "be\u00b7h\u00e4g\u00b7lich", "Schl\u00e4f\u00b7chen", "al."], "token_info": ["word", "word", "word", "word", "abbreviation"], "pos": ["ADV", "PPER", "ADJD", "NN", "NE"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wolauf, mein liebender Gedanke,", "tokens": ["Wo\u00b7lauf", ",", "mein", "lie\u00b7ben\u00b7der", "Ge\u00b7dan\u00b7ke", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wolauf, zu ihrem Lager hin!", "tokens": ["Wo\u00b7lauf", ",", "zu", "ih\u00b7rem", "La\u00b7ger", "hin", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und webe, gleich der Epheuranke,", "tokens": ["Und", "we\u00b7be", ",", "gleich", "der", "E\u00b7pheu\u00b7ran\u00b7ke", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dich um die traute Schl\u00e4ferin!", "tokens": ["Dich", "um", "die", "trau\u00b7te", "Schl\u00e4\u00b7fe\u00b7rin", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Geneus der \u00fcbers\u00fcssen F\u00fclle", "tokens": ["Ge\u00b7neus", "der", "\u00fc\u00b7ber\u00b7s\u00fcs\u00b7sen", "F\u00fcl\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Von aller Erdenseligkeit,", "tokens": ["Von", "al\u00b7ler", "Er\u00b7den\u00b7se\u00b7lig\u00b7keit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wovon zu kosten noch ihr Wille,", "tokens": ["Wo\u00b7von", "zu", "kos\u00b7ten", "noch", "ihr", "Wil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PTKZU", "VVINF", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und ewig ach! vielleicht verbeut! \u2014", "tokens": ["Und", "e\u00b7wig", "ach", "!", "viel\u00b7leicht", "ver\u00b7beut", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["KON", "ADJD", "ITJ", "$.", "ADV", "VVFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ahi! Was h\u00f6r\u2019 ich f\u00fcr Ges\u00e4usel?", "tokens": ["A\u00b7hi", "!", "Was", "h\u00f6r'", "ich", "f\u00fcr", "Ge\u00b7s\u00e4u\u00b7sel", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PWS", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das ist ihr Schlummerodemzug.", "tokens": ["Das", "ist", "ihr", "Schlum\u00b7me\u00b7ro\u00b7dem\u00b7zug", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So leise wallt, durch das Gekr\u00e4usel", "tokens": ["So", "lei\u00b7se", "wallt", ",", "durch", "das", "Ge\u00b7kr\u00e4u\u00b7sel"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADJD", "VVFIN", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Des jungen Laubes, Zefyrs Flug.", "tokens": ["Des", "jun\u00b7gen", "Lau\u00b7bes", ",", "Ze\u00b7fyrs", "Flug", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ahi! Da h\u00f6r\u2019 ich das Gest\u00f6ne,", "tokens": ["A\u00b7hi", "!", "Da", "h\u00f6r'", "ich", "das", "Ge\u00b7st\u00f6\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Das Wollust aus den Busen st\u00f6st,", "tokens": ["Das", "Wol\u00b7lust", "aus", "den", "Bu\u00b7sen", "st\u00f6st", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wie Bienensang und Schilfget\u00f6ne,", "tokens": ["Wie", "Bie\u00b7nen\u00b7sang", "und", "Schilf\u00b7ge\u00b7t\u00f6\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Wann Abendwind dazwischen bl\u00e4st.", "tokens": ["Wann", "Ab\u00b7end\u00b7wind", "da\u00b7zwi\u00b7schen", "bl\u00e4st", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "PAV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "O, wie so sch\u00f6n dahin gegossen,", "tokens": ["O", ",", "wie", "so", "sch\u00f6n", "da\u00b7hin", "ge\u00b7gos\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "ADV", "ADJD", "PAV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Umleuchtet sie des Mondes Licht!", "tokens": ["Um\u00b7leuch\u00b7tet", "sie", "des", "Mon\u00b7des", "Licht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Blumen der Gesundheit sprossen", "tokens": ["Die", "Blu\u00b7men", "der", "Ge\u00b7sund\u00b7heit", "spros\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Auf ihrem wonnigen Gesicht.", "tokens": ["Auf", "ih\u00b7rem", "won\u00b7ni\u00b7gen", "Ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Arme liegen ausgeschlagen,", "tokens": ["Die", "Ar\u00b7me", "lie\u00b7gen", "aus\u00b7ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Als wolten sie, mit Innigkeit,", "tokens": ["Als", "wol\u00b7ten", "sie", ",", "mit", "In\u00b7nig\u00b7keit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "VMFIN", "PPER", "$,", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Um den den Liebesknoten schlagen,", "tokens": ["Um", "den", "den", "Lie\u00b7bes\u00b7kno\u00b7ten", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Dem sie im Traume ganz sich weiht. \u2014", "tokens": ["Dem", "sie", "im", "Trau\u00b7me", "ganz", "sich", "weiht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "PPER", "APPRART", "NN", "ADV", "PRF", "VVFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Nun kehre wieder! Nun entwanke", "tokens": ["Nun", "keh\u00b7re", "wie\u00b7der", "!", "Nun", "ent\u00b7wan\u00b7ke"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "$.", "ADV", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dem Wonnebett\u2019! Du hast genug!", "tokens": ["Dem", "Won\u00b7ne\u00b7bett'", "!", "Du", "hast", "ge\u00b7nug", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "PPER", "VAFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sonst wirst du trunken, mein Gedanke,", "tokens": ["Sonst", "wirst", "du", "trun\u00b7ken", ",", "mein", "Ge\u00b7dan\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sonst l\u00e4hmt der Taumel deinen Flug.", "tokens": ["Sonst", "l\u00e4hmt", "der", "Tau\u00b7mel", "dei\u00b7nen", "Flug", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Du loderst auf in Durstesflammen! \u2014", "tokens": ["Du", "lo\u00b7derst", "auf", "in", "Durs\u00b7tes\u00b7flam\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "APPR", "APPR", "NN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ha! wirf ins Meer der Wonne dich!", "tokens": ["Ha", "!", "wirf", "ins", "Meer", "der", "Won\u00b7ne", "dich", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VVFIN", "APPRART", "NN", "ART", "NN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Schlagt, Wellen, \u00fcber mir zusammen!", "tokens": ["Schlagt", ",", "Wel\u00b7len", ",", "\u00fc\u00b7ber", "mir", "zu\u00b7sam\u00b7men", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$,", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ich brenne! brenne! k\u00fchlet mich!", "tokens": ["Ich", "bren\u00b7ne", "!", "bren\u00b7ne", "!", "k\u00fch\u00b7let", "mich", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "VVFIN", "$.", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}