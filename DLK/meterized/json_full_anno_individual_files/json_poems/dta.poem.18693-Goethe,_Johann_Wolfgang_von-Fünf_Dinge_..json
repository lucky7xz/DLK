{"dta.poem.18693": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang von", "birth": "N.A.", "death": "N.A."}, "title": "F\u00fcnf Dinge .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1819", "urn": "urn:nbn:de:kobv:b4-200905191692", "language": ["de:0.99"], "booktitle": "Goethe, Johann Wolfgang von: West-\u00f6stlicher Divan. Stuttgart, 1819."}, "poem": {"stanza.1": {"line.1": {"text": "F\u00fcnf Dinge bringen f\u00fcnfe nicht hervor,", "tokens": ["F\u00fcnf", "Din\u00b7ge", "brin\u00b7gen", "f\u00fcn\u00b7fe", "nicht", "her\u00b7vor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVINF", "VVFIN", "PTKNEG", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Du, dieser Lehre \u00f6ffne du dein Ohr:", "tokens": ["Du", ",", "die\u00b7ser", "Leh\u00b7re", "\u00f6ff\u00b7ne", "du", "dein", "Ohr", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PDAT", "NN", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der stolzen Brust wird Freundschaft nicht ent-", "tokens": ["Der", "stol\u00b7zen", "Brust", "wird", "Freund\u00b7schaft", "nicht", "ent"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN", "NN", "PTKNEG", "TRUNC"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "sprossen.", "tokens": ["spros\u00b7sen", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Unh\u00f6flich sind der Niedrigkeit Genossen;", "tokens": ["Un\u00b7h\u00f6f\u00b7lich", "sind", "der", "Nied\u00b7rig\u00b7keit", "Ge\u00b7nos\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ein B\u00f6sewicht gelangt zu keiner Gr\u00f6sse;", "tokens": ["Ein", "B\u00f6\u00b7se\u00b7wicht", "ge\u00b7langt", "zu", "kei\u00b7ner", "Gr\u00f6s\u00b7se", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Der Neidische erbarmt sich nicht der Bl\u00f6sse;", "tokens": ["Der", "Nei\u00b7di\u00b7sche", "er\u00b7barmt", "sich", "nicht", "der", "Bl\u00f6s\u00b7se", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "PTKNEG", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Der L\u00fcgner hofft vergeblich Treu\u2019 und Glauben;", "tokens": ["Der", "L\u00fcg\u00b7ner", "hofft", "ver\u00b7geb\u00b7lich", "Treu'", "und", "Glau\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Das halte fest und niemand lass dir\u2019s rauben.", "tokens": ["Das", "hal\u00b7te", "fest", "und", "nie\u00b7mand", "lass", "dir's", "rau\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PTKVZ", "KON", "PIS", "VVFIN", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}