{"dta.poem.12986": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "* *", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die pyramiden stehn offt mehr als tausend jahre:", "tokens": ["Die", "py\u00b7ra\u00b7mi\u00b7den", "stehn", "offt", "mehr", "als", "tau\u00b7send", "jah\u00b7re", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "PIAT", "KOKOM", "CARD", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der arme mensch lebt nicht den zehnden theil davon,", "tokens": ["Der", "ar\u00b7me", "mensch", "lebt", "nicht", "den", "zehn\u00b7den", "theil", "da\u00b7von", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PTKNEG", "ART", "ADJA", "NN", "PAV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Indessen ist er doch, wie sie, ein erden-sohn.", "tokens": ["In\u00b7des\u00b7sen", "ist", "er", "doch", ",", "wie", "sie", ",", "ein", "er\u00b7den\u00b7sohn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "$,", "PWAV", "PPER", "$,", "ART", "NN", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Ein nichts legt seinen leib in kurtzem auf die bahre;", "tokens": ["Ein", "nichts", "legt", "sei\u00b7nen", "leib", "in", "kurt\u00b7zem", "auf", "die", "bah\u00b7re", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "PPOSAT", "NN", "APPR", "ADJA", "APPR", "ART", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und jene rei\u00dft kein streit der elementen ein.", "tokens": ["Und", "je\u00b7ne", "rei\u00dft", "kein", "streit", "der", "e\u00b7le\u00b7men\u00b7ten", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "PIAT", "NN", "ART", "ADJA", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Hat GOttes ebenbild so wenig krafft und st\u00e4rcke?", "tokens": ["Hat", "Got\u00b7tes", "e\u00b7ben\u00b7bild", "so", "we\u00b7nig", "krafft", "und", "st\u00e4r\u00b7cke", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "ADV", "ADV", "PIAT", "NN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Kommt, weisen! und erstaunt, dieweil des H\u00f6chsten wercke", "tokens": ["Kommt", ",", "wei\u00b7sen", "!", "und", "er\u00b7staunt", ",", "die\u00b7weil", "des", "H\u00f6chs\u00b7ten", "wer\u00b7cke"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "VVFIN", "$.", "KON", "ADJD", "$,", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Weit, weit gebrechlicher, als menschen wercke seyn.", "tokens": ["Weit", ",", "weit", "ge\u00b7brech\u00b7li\u00b7cher", ",", "als", "men\u00b7schen", "wer\u00b7cke", "seyn", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "ADJA", "$,", "KOUS", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}