, ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Gott, unsern Hort zu ehren,", "tokens": ["Gott", ",", "un\u00b7sern", "Hort", "zu", "eh\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Da\u00df er die Liebe sey,", "tokens": ["Da\u00df", "er", "die", "Lie\u00b7be", "sey", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Da\u00df er die Welt sampt allen Sachen", "tokens": ["Da\u00df", "er", "die", "Welt", "sampt", "al\u00b7len", "Sa\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Erschaffen hab nur uns zu gut,", "tokens": ["Er\u00b7schaf\u00b7fen", "hab", "nur", "uns", "zu", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ADV", "PPER", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Todt, S\u00fcnde, H\u00f6ll und ihren Drachen", "tokens": ["Todt", ",", "S\u00fcn\u00b7de", ",", "H\u00f6ll", "und", "ih\u00b7ren", "Dra\u00b7chen"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "NN", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Erw\u00fcrgt durch seines Sohnes Blut,", "tokens": ["Er\u00b7w\u00fcrgt", "durch", "sei\u00b7nes", "Soh\u00b7nes", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der f\u00fcr uns ist gestorben", "tokens": ["Der", "f\u00fcr", "uns", "ist", "ge\u00b7stor\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "PPER", "VAFIN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "In h\u00f6chster Schmach und Hohn,", "tokens": ["In", "h\u00f6chs\u00b7ter", "Schmach", "und", "Hohn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Dadurch er uns erworben", "tokens": ["Da\u00b7durch", "er", "uns", "er\u00b7wor\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "PPER", "PRF", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die ewig' Ehrenkron,", "tokens": ["Die", "e\u00b7wig'", "Eh\u00b7ren\u00b7kron", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Er giebt sich gr\u00fcndlich zu erkennen,", "tokens": ["Er", "giebt", "sich", "gr\u00fcnd\u00b7lich", "zu", "er\u00b7ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da sieht man uns in seiner, ihn", "tokens": ["Da", "sieht", "man", "uns", "in", "sei\u00b7ner", ",", "ihn"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "VVFIN", "PIS", "PRF", "APPR", "PPOSAT", "$,", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In unsrer Liebe hertzlich brennen", "tokens": ["In", "uns\u00b7rer", "Lie\u00b7be", "hertz\u00b7lich", "bren\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und tausend Frewden auff uns ziehn.", "tokens": ["Und", "tau\u00b7send", "Frew\u00b7den", "auff", "uns", "ziehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "CARD", "NN", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da geht es nur in Sprunge,", "tokens": ["Da", "geht", "es", "nur", "in", "Sprun\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Da kennt man keine Pein,", "tokens": ["Da", "kennt", "man", "kei\u00b7ne", "Pein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Da sieht man aller Zunge", "tokens": ["Da", "sieht", "man", "al\u00b7ler", "Zun\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Nur Ruhm und Lachen seyn.", "tokens": ["Nur", "Ruhm", "und", "La\u00b7chen", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "NN", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Was s\u00e4um ich mich? Komm, mein Verlangen,", "tokens": ["Was", "s\u00e4um", "ich", "mich", "?", "Komm", ",", "mein", "Ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PRF", "$.", "VVFIN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Herr Jesu, komm und spann mich au\u00df,", "tokens": ["Herr", "Je\u00b7su", ",", "komm", "und", "spann", "mich", "au\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "VVFIN", "KON", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich habe Lust dich zu umbfangen,", "tokens": ["Ich", "ha\u00b7be", "Lust", "dich", "zu", "umb\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nimm mich in deines Vaters Hau\u00df!", "tokens": ["Nimm", "mich", "in", "dei\u00b7nes", "Va\u00b7ters", "Hau\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Du kommst? Sey willkomm! Erde", "tokens": ["Du", "kommst", "?", "Sey", "will\u00b7komm", "!", "Er\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "$.", "VAIMP", "ADJD", "$.", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Sammt deinem falschen Pracht,", "tokens": ["Sammt", "dei\u00b7nem", "fal\u00b7schen", "Pracht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Nun ich so selig werde", "tokens": ["Nun", "ich", "so", "se\u00b7lig", "wer\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "ADV", "ADJD", "VAFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Entbunden, gute Nacht!", "tokens": ["Ent\u00b7bun\u00b7den", ",", "gu\u00b7te", "Nacht", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "