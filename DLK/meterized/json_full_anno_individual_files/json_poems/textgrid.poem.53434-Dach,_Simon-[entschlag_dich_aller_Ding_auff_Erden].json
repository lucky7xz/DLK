, "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PAV", "$,", "PRELS", "ADV", "ADV", "VAFIN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Sucht sich die S\u00fcnd an dir zu reiben,", "tokens": ["Sucht", "sich", "die", "S\u00fcnd", "an", "dir", "zu", "rei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "APPR", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Bek\u00fcmmert dich der H\u00f6llen List,", "tokens": ["Be\u00b7k\u00fcm\u00b7mert", "dich", "der", "H\u00f6l\u00b7len", "List", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Schau Christus offne Seiten,", "tokens": ["Schau", "Chris\u00b7tus", "off\u00b7ne", "Sei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "ADJA", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.6": {"text": "Fleuch gl\u00e4ubig da hinein,", "tokens": ["Fleuch", "gl\u00e4u\u00b7big", "da", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "So wird dich nichts bestreiten", "tokens": ["So", "wird", "dich", "nichts", "be\u00b7strei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PIS", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und du wirft sicher seyn.", "tokens": ["Und", "du", "wirft", "si\u00b7cher", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADJD", "VAINF", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.