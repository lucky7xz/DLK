{"textgrid.poem.53434": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "[entschlag dich aller Ding auff Erden]", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Entschlag dich aller Ding auff Erden,", "tokens": ["Ent\u00b7schlag", "dich", "al\u00b7ler", "Ding", "auff", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PIAT", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "O meine Seele, denn du bald", "tokens": ["O", "mei\u00b7ne", "See\u00b7le", ",", "denn", "du", "bald"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "PPOSAT", "NN", "$,", "KON", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Von mir solst abgefordert werden!", "tokens": ["Von", "mir", "solst", "ab\u00b7ge\u00b7for\u00b7dert", "wer\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VMFIN", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Gott ist der Wittwen Auffenthalt,", "tokens": ["Gott", "ist", "der", "Witt\u00b7wen", "Auf\u00b7fent\u00b7halt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Waisen Schirm und Schatten,", "tokens": ["Der", "Wai\u00b7sen", "Schirm", "und", "Schat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und wird aus lieber Treu", "tokens": ["Und", "wird", "aus", "lie\u00b7ber", "Treu"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Den Frevel nicht erstatten,", "tokens": ["Den", "Fre\u00b7vel", "nicht", "er\u00b7stat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Das ihnen sch\u00e4dlich sey.", "tokens": ["Das", "ih\u00b7nen", "sch\u00e4d\u00b7lich", "sey", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ADJD", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Was hinten ist, la\u00df bleiben,", "tokens": ["Was", "hin\u00b7ten", "ist", ",", "la\u00df", "blei\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "ADV", "VAFIN", "$,", "VVIMP", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Tracht darnach, was da vornen ist,", "tokens": ["Tracht", "dar\u00b7nach", ",", "was", "da", "vor\u00b7nen", "ist", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PAV", "$,", "PRELS", "ADV", "ADV", "VAFIN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Sucht sich die S\u00fcnd an dir zu reiben,", "tokens": ["Sucht", "sich", "die", "S\u00fcnd", "an", "dir", "zu", "rei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "APPR", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Bek\u00fcmmert dich der H\u00f6llen List,", "tokens": ["Be\u00b7k\u00fcm\u00b7mert", "dich", "der", "H\u00f6l\u00b7len", "List", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Schau Christus offne Seiten,", "tokens": ["Schau", "Chris\u00b7tus", "off\u00b7ne", "Sei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "ADJA", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.6": {"text": "Fleuch gl\u00e4ubig da hinein,", "tokens": ["Fleuch", "gl\u00e4u\u00b7big", "da", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "So wird dich nichts bestreiten", "tokens": ["So", "wird", "dich", "nichts", "be\u00b7strei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PIS", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und du wirft sicher seyn.", "tokens": ["Und", "du", "wirft", "si\u00b7cher", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADJD", "VAINF", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "La\u00df stets in deinem Hertzen schweben", "tokens": ["La\u00df", "stets", "in", "dei\u00b7nem", "Hert\u00b7zen", "schwe\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "ADV", "APPR", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Den Himmels-Bau, der Frewden Saal,", "tokens": ["Den", "Him\u00b7mels\u00b7Bau", ",", "der", "Frew\u00b7den", "Saal", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dahin du bald dich wirst begeben", "tokens": ["Da\u00b7hin", "du", "bald", "dich", "wirst", "be\u00b7ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "PPER", "ADV", "PPER", "VAFIN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Aus diesem Angst- und Thr\u00e4nen Thal,", "tokens": ["Aus", "die\u00b7sem", "Angst", "und", "Thr\u00e4\u00b7nen", "Thal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "TRUNC", "KON", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da wo die Frommen halten", "tokens": ["Da", "wo", "die", "From\u00b7men", "hal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PWAV", "ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ihr Hall-Jahr allezeit,", "tokens": ["Ihr", "Hall\u00b7Jahr", "al\u00b7le\u00b7zeit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wo Lust und Leben walten", "tokens": ["Wo", "Lust", "und", "Le\u00b7ben", "wal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "KON", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und tausend Herrligkeit.", "tokens": ["Und", "tau\u00b7send", "Herr\u00b7lig\u00b7keit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "CARD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Da wirstu erstlich h\u00f6ren klingen", "tokens": ["Da", "wirs\u00b7tu", "erst\u00b7lich", "h\u00f6\u00b7ren", "klin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADJD", "VVINF", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Musica, das Himmels-Kind,", "tokens": ["Die", "Mu\u00b7si\u00b7ca", ",", "das", "Him\u00b7mels\u00b7Kind", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "F\u00fcr der die Lieder, so wir singen", "tokens": ["F\u00fcr", "der", "die", "Lie\u00b7der", ",", "so", "wir", "sin\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "ART", "NN", "$,", "ADV", "PPER", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Auff Erden, Tr\u00e4um und Schatten sind;", "tokens": ["Auff", "Er\u00b7den", ",", "Tr\u00e4um", "und", "Schat\u00b7ten", "sind", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da l\u00e4\u00dft sich David h\u00f6ren", "tokens": ["Da", "l\u00e4\u00dft", "sich", "Da\u00b7vid", "h\u00f6\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "NE", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und seine Canterey,", "tokens": ["Und", "sei\u00b7ne", "Can\u00b7te\u00b7rey", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Gott, unsern Hort zu ehren,", "tokens": ["Gott", ",", "un\u00b7sern", "Hort", "zu", "eh\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Da\u00df er die Liebe sey,", "tokens": ["Da\u00df", "er", "die", "Lie\u00b7be", "sey", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Da\u00df er die Welt sampt allen Sachen", "tokens": ["Da\u00df", "er", "die", "Welt", "sampt", "al\u00b7len", "Sa\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Erschaffen hab nur uns zu gut,", "tokens": ["Er\u00b7schaf\u00b7fen", "hab", "nur", "uns", "zu", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ADV", "PPER", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Todt, S\u00fcnde, H\u00f6ll und ihren Drachen", "tokens": ["Todt", ",", "S\u00fcn\u00b7de", ",", "H\u00f6ll", "und", "ih\u00b7ren", "Dra\u00b7chen"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "NN", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Erw\u00fcrgt durch seines Sohnes Blut,", "tokens": ["Er\u00b7w\u00fcrgt", "durch", "sei\u00b7nes", "Soh\u00b7nes", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der f\u00fcr uns ist gestorben", "tokens": ["Der", "f\u00fcr", "uns", "ist", "ge\u00b7stor\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "PPER", "VAFIN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "In h\u00f6chster Schmach und Hohn,", "tokens": ["In", "h\u00f6chs\u00b7ter", "Schmach", "und", "Hohn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Dadurch er uns erworben", "tokens": ["Da\u00b7durch", "er", "uns", "er\u00b7wor\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "PPER", "PRF", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die ewig' Ehrenkron,", "tokens": ["Die", "e\u00b7wig'", "Eh\u00b7ren\u00b7kron", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Er giebt sich gr\u00fcndlich zu erkennen,", "tokens": ["Er", "giebt", "sich", "gr\u00fcnd\u00b7lich", "zu", "er\u00b7ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da sieht man uns in seiner, ihn", "tokens": ["Da", "sieht", "man", "uns", "in", "sei\u00b7ner", ",", "ihn"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "VVFIN", "PIS", "PRF", "APPR", "PPOSAT", "$,", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In unsrer Liebe hertzlich brennen", "tokens": ["In", "uns\u00b7rer", "Lie\u00b7be", "hertz\u00b7lich", "bren\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und tausend Frewden auff uns ziehn.", "tokens": ["Und", "tau\u00b7send", "Frew\u00b7den", "auff", "uns", "ziehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "CARD", "NN", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da geht es nur in Sprunge,", "tokens": ["Da", "geht", "es", "nur", "in", "Sprun\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Da kennt man keine Pein,", "tokens": ["Da", "kennt", "man", "kei\u00b7ne", "Pein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Da sieht man aller Zunge", "tokens": ["Da", "sieht", "man", "al\u00b7ler", "Zun\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Nur Ruhm und Lachen seyn.", "tokens": ["Nur", "Ruhm", "und", "La\u00b7chen", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "NN", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Was s\u00e4um ich mich? Komm, mein Verlangen,", "tokens": ["Was", "s\u00e4um", "ich", "mich", "?", "Komm", ",", "mein", "Ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PRF", "$.", "VVFIN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Herr Jesu, komm und spann mich au\u00df,", "tokens": ["Herr", "Je\u00b7su", ",", "komm", "und", "spann", "mich", "au\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "VVFIN", "KON", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich habe Lust dich zu umbfangen,", "tokens": ["Ich", "ha\u00b7be", "Lust", "dich", "zu", "umb\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nimm mich in deines Vaters Hau\u00df!", "tokens": ["Nimm", "mich", "in", "dei\u00b7nes", "Va\u00b7ters", "Hau\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Du kommst? Sey willkomm! Erde", "tokens": ["Du", "kommst", "?", "Sey", "will\u00b7komm", "!", "Er\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "$.", "VAIMP", "ADJD", "$.", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Sammt deinem falschen Pracht,", "tokens": ["Sammt", "dei\u00b7nem", "fal\u00b7schen", "Pracht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Nun ich so selig werde", "tokens": ["Nun", "ich", "so", "se\u00b7lig", "wer\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "ADV", "ADJD", "VAFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Entbunden, gute Nacht!", "tokens": ["Ent\u00b7bun\u00b7den", ",", "gu\u00b7te", "Nacht", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Entschlag dich aller Ding auff Erden,", "tokens": ["Ent\u00b7schlag", "dich", "al\u00b7ler", "Ding", "auff", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PIAT", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "O meine Seele, denn du bald", "tokens": ["O", "mei\u00b7ne", "See\u00b7le", ",", "denn", "du", "bald"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "PPOSAT", "NN", "$,", "KON", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Von mir solst abgefordert werden!", "tokens": ["Von", "mir", "solst", "ab\u00b7ge\u00b7for\u00b7dert", "wer\u00b7den", "!"], "token_info": ["word", "word", "