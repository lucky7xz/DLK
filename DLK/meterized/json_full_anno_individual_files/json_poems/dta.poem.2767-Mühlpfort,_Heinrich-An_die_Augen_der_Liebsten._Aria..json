{"dta.poem.2767": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "An die Augen der Liebsten.  \n  Aria.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Jhr sch\u00f6nen Augen ihr/", "tokens": ["Ihr", "sch\u00f6\u00b7nen", "Au\u00b7gen", "ihr", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PPOSAT", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Ich f\u00fchle Gluth/", "tokens": ["Ich", "f\u00fch\u00b7le", "Gluth", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Und eure Wunder Zier", "tokens": ["Und", "eu\u00b7re", "Wun\u00b7der", "Zier"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Erhitzt mein Blut.", "tokens": ["Er\u00b7hitzt", "mein", "Blut", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Die angenehme Freundlichkeit/", "tokens": ["Die", "an\u00b7ge\u00b7neh\u00b7me", "Freund\u00b7lich\u00b7keit", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So s\u00fcsse Blicke strect/", "tokens": ["So", "s\u00fcs\u00b7se", "Bli\u00b7cke", "strect", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Macht mich erfreut.", "tokens": ["Macht", "mich", "er\u00b7freut", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVFIN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "Jhr Fackeln meiner Seel", "tokens": ["Ihr", "Fa\u00b7ckeln", "mei\u00b7ner", "Seel"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Ich bin entbrant/", "tokens": ["Ich", "bin", "ent\u00b7brant", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "\naus eurer schwartzen H\u00f6hl", "tokens": ["aus", "eu\u00b7rer", "schwart\u00b7zen", "H\u00f6hl"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Und Diamant/", "tokens": ["Und", "Di\u00b7a\u00b7mant", "/"], "token_info": ["word", "word", "punct"], "pos": ["KON", "NN", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.12": {"text": "Komt mir der s\u00fcsse Gegenschein", "tokens": ["Komt", "mir", "der", "s\u00fcs\u00b7se", "Ge\u00b7gen\u00b7schein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Da\u00df ich verliebt mu\u00df seyn/", "tokens": ["Da\u00df", "ich", "ver\u00b7liebt", "mu\u00df", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "VMFIN", "VAINF", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.14": {"text": "In meiner Pein.", "tokens": ["In", "mei\u00b7ner", "Pein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.15": {"text": "Jhr Flammen meiner Lust/", "tokens": ["Ihr", "Flam\u00b7men", "mei\u00b7ner", "Lust", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPOSAT", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.16": {"text": "Wie brennt ihr so?", "tokens": ["Wie", "brennt", "ihr", "so", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.17": {"text": "Wie macht ihr meine Brust", "tokens": ["Wie", "macht", "ihr", "mei\u00b7ne", "Brust"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.18": {"text": "So hertzlich froh!", "tokens": ["So", "hertz\u00b7lich", "froh", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.19": {"text": " leitsterne in das Paradei\u00df/", "tokens": ["leits\u00b7ter\u00b7ne", "in", "das", "Pa\u00b7ra\u00b7dei\u00df", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$("], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.20": {"text": "Eur so geliebtes Wei\u00df/", "tokens": ["Eur", "so", "ge\u00b7lieb\u00b7tes", "Wei\u00df", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADV", "ADJA", "NN", "$("], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.21": {"text": "Das macht mir hei\u00df.", "tokens": ["Das", "macht", "mir", "hei\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.22": {"text": "Strahlt Kertzen in der Nacht/", "tokens": ["Strahlt", "Kert\u00b7zen", "in", "der", "Nacht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.23": {"text": "Wei\u00dft mir die Bahn.", "tokens": ["Wei\u00dft", "mir", "die", "Bahn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.24": {"text": "Ich bin ja eurer Pracht", "tokens": ["Ich", "bin", "ja", "eu\u00b7rer", "Pracht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.25": {"text": "Gantz unterthan.", "tokens": ["Gantz", "un\u00b7ter\u00b7than", "."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.26": {"text": "Die Sonne mu\u00df sich nicht ent-", "tokens": ["Die", "Son\u00b7ne", "mu\u00df", "sich", "nicht", "ent"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "PRF", "PTKNEG", "TRUNC"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.27": {"text": "\nsonst wird mein Leben fliehn/", "tokens": ["sonst", "wird", "mein", "Le\u00b7ben", "fliehn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.28": {"text": "Und gantz verbl\u00fchn.", "tokens": ["Und", "gantz", "ver\u00b7bl\u00fchn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVINF", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.29": {"text": "Ach k\u00fc\u00dft ich diesen Strahl", "tokens": ["Ach", "k\u00fc\u00dft", "ich", "die\u00b7sen", "Strahl"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ITJ", "VVFIN", "PPER", "PDAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.30": {"text": "In heisser Brunst/", "tokens": ["In", "heis\u00b7ser", "Brunst", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.31": {"text": "Der mir theils schencket Quahl", "tokens": ["Der", "mir", "theils", "schen\u00b7cket", "Quahl"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ADV", "VVFIN", "NN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.32": {"text": "Theils s\u00fcsse Gunst;", "tokens": ["Theils", "s\u00fcs\u00b7se", "Gunst", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.33": {"text": "Ich schw\u00f6re da\u00df ich sterbe so/", "tokens": ["Ich", "schw\u00f6\u00b7re", "da\u00df", "ich", "ster\u00b7be", "so", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOUS", "PPER", "VVFIN", "ADV", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.34": {"text": "Und bin in eurer Loh/", "tokens": ["Und", "bin", "in", "eu\u00b7rer", "Loh", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.35": {"text": "Von Hertzen froh.", "tokens": ["Von", "Hert\u00b7zen", "froh", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}