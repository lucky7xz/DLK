: {"text": "Aber dein m\u00e4chtiges Lied der R\u00f6merinn Herze zu r\u00fchren,", "tokens": ["A\u00b7ber", "dein", "m\u00e4ch\u00b7ti\u00b7ges", "Lied", "der", "R\u00f6\u00b7me\u00b7rinn", "Her\u00b7ze", "zu", "r\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "ART", "NN", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Singt sie, die Deutsche, nur f\u00fcr das Geh\u00f6r.", "tokens": ["Singt", "sie", ",", "die", "Deut\u00b7sche", ",", "nur", "f\u00fcr", "das", "Ge\u00b7h\u00f6r", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ART", "NN", "$,", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+++-+", "measure": "zehnsilber"}, "line.3": {"text": "F\u00fchlende Sch\u00f6nen verstehn dich nicht mehr!", "tokens": ["F\u00fch\u00b7len\u00b7de", "Sch\u00f6\u00b7nen", "ver\u00b7stehn", "dich", "nicht", "mehr", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "PTKNEG", "ADV", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.16": {"line.1": {"text": "Freunde des Alterthums, h\u00f6rt! Die Ewigkeit werd' ich nicht suchen,", "tokens": ["Freun\u00b7de", "des", "Al\u00b7ter\u00b7thums", ",", "h\u00f6rt", "!", "Die", "E\u00b7wig\u00b7keit", "werd'", "ich", "nicht", "su\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "VVFIN", "$.", "ART", "NN", "VAFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "+--+--+-+-+-+-+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "Da mich die Welt, der ich lebe, vergi\u00dft;", "tokens": ["Da", "mich", "die", "Welt", ",", "der", "ich", "le\u00b7be", ",", "ver\u00b7gi\