{"dta.poem.16398": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "59.  Wie Gott wil sol man wollen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "W\u00e4r\u2019 ich ein Seraphin/ so wolt ich lieber seyn/", "tokens": ["W\u00e4r'", "ich", "ein", "Se\u00b7ra\u00b7phin", "/", "so", "wolt", "ich", "lie\u00b7ber", "seyn", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "$(", "ADV", "VMFIN", "PPER", "ADV", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dem H\u00f6chsten zugefalln/ das schn\u00f6dste W\u00fcrmelein.", "tokens": ["Dem", "H\u00f6chs\u00b7ten", "zu\u00b7ge\u00b7falln", "/", "das", "schn\u00f6ds\u00b7te", "W\u00fcr\u00b7mel\u00b7ein", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$(", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}