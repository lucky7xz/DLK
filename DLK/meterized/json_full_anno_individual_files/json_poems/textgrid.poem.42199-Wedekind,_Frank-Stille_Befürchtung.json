{"textgrid.poem.42199": {"metadata": {"author": {"name": "Wedekind, Frank", "birth": "N.A.", "death": "N.A."}, "title": "Stille Bef\u00fcrchtung", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Seit ich dir mein ganzes Herz entladen,", "tokens": ["Seit", "ich", "dir", "mein", "gan\u00b7zes", "Herz", "ent\u00b7la\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "PPOSAT", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Peinigt mich geheimnisvolles Weh:", "tokens": ["Pei\u00b7nigt", "mich", "ge\u00b7heim\u00b7nis\u00b7vol\u00b7les", "Weh", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Morgens dr\u00e4ngt's mich seltsam, mich zu baden;", "tokens": ["Mor\u00b7gens", "dr\u00e4ngt's", "mich", "selt\u00b7sam", ",", "mich", "zu", "ba\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "$,", "PPER", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Abends treibt's mich m\u00e4chtig ins Caf\u00e9;", "tokens": ["A\u00b7bends", "treibt's", "mich", "m\u00e4ch\u00b7tig", "ins", "Ca\u00b7f\u00e9", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPRART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Nachts umgaukeln mich verr\u00fcckte Tr\u00e4ume", "tokens": ["Nachts", "um\u00b7gau\u00b7keln", "mich", "ver\u00b7r\u00fcck\u00b7te", "Tr\u00e4u\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "VVFIN", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Da\u00df die Seele bang um Hilfe schreit;", "tokens": ["Da\u00df", "die", "See\u00b7le", "bang", "um", "Hil\u00b7fe", "schreit", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADJD", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Eng bedr\u00fccken mich des Himmels R\u00e4ume,", "tokens": ["Eng", "be\u00b7dr\u00fc\u00b7cken", "mich", "des", "Him\u00b7mels", "R\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PRF", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Die Gew\u00e4nder werden mir zu weit;", "tokens": ["Die", "Ge\u00b7w\u00e4n\u00b7der", "wer\u00b7den", "mir", "zu", "weit", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "PTKA", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Vor den Augen schwirrt ein schwarzer Falter \u2013", "tokens": ["Vor", "den", "Au\u00b7gen", "schwirrt", "ein", "schwar\u00b7zer", "Fal\u00b7ter", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Sprich, o sprich, wie soll ich das verstehn!", "tokens": ["Sprich", ",", "o", "sprich", ",", "wie", "soll", "ich", "das", "ver\u00b7stehn", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "FM", "ADJD", "$,", "PWAV", "VMFIN", "PPER", "PDS", "VVINF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "'s ist ein heimlich zartes Knospenalter;", "t