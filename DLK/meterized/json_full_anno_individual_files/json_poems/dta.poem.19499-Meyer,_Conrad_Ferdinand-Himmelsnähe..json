{"dta.poem.19499": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Himmelsn\u00e4he.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-200905193933", "language": ["de:0.99"], "booktitle": "Meyer, Conrad Ferdinand: Gedichte. Leipzig, 1882."}, "poem": {"stanza.1": {"line.1": {"text": "In meiner Firne feierlichem Kreis", "tokens": ["In", "mei\u00b7ner", "Fir\u00b7ne", "fei\u00b7er\u00b7li\u00b7chem", "Kreis"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Lagr' ich an schmalem Felsengrate hier,", "tokens": ["La\u00b7gr'", "ich", "an", "schma\u00b7lem", "Fel\u00b7sen\u00b7gra\u00b7te", "hier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "ADJA", "NN", "ADV", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "Aus einem gr\u00fcn erstarrten Meer von Eis", "tokens": ["Aus", "ei\u00b7nem", "gr\u00fcn", "er\u00b7starr\u00b7ten", "Meer", "von", "Eis"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJD", "ADJA", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Erhebt die Silberzacke sich vor mir.", "tokens": ["Er\u00b7hebt", "die", "Sil\u00b7ber\u00b7za\u00b7cke", "sich", "vor", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PRF", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Der Schnee, der am Gekl\u00fcfte hing zerstreut,", "tokens": ["Der", "Schnee", ",", "der", "am", "Ge\u00b7kl\u00fcf\u00b7te", "hing", "zer\u00b7streut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPRART", "NN", "VVFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "In hundert Rinnen rieselt er davon", "tokens": ["In", "hun\u00b7dert", "Rin\u00b7nen", "rie\u00b7selt", "er", "da\u00b7von"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "CARD", "NN", "VVFIN", "PPER", "PAV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und aus der schwarzen Feuchte schimmert heut", "tokens": ["Und", "aus", "der", "schwar\u00b7zen", "Feuch\u00b7te", "schim\u00b7mert", "heut"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "VVFIN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der Soldanelle zarte Glocke schon.", "tokens": ["Der", "Sol\u00b7da\u00b7nel\u00b7le", "zar\u00b7te", "Glo\u00b7cke", "schon", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Bald nahe tost, bald fern, der Wasserfall,", "tokens": ["Bald", "na\u00b7he", "tost", ",", "bald", "fern", ",", "der", "Was\u00b7ser\u00b7fall", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "$,", "ADV", "ADJD", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Er st\u00e4ubt und st\u00fcrzt, nun rechts, nun links verweht,", "tokens": ["Er", "st\u00e4ubt", "und", "st\u00fcrzt", ",", "nun", "rechts", ",", "nun", "links", "ver\u00b7weht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "$,", "ADV", "ADV", "$,", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ein tiefes Schweigen und ein steter Schall,", "tokens": ["Ein", "tie\u00b7fes", "Schwei\u00b7gen", "und", "ein", "ste\u00b7ter", "Schall", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ein Wind, ein Strom, ein Athem, ein Gebet!", "tokens": ["Ein", "Wind", ",", "ein", "Strom", ",", "ein", "A\u00b7them", ",", "ein", "Ge\u00b7bet", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Nur neben mir des Murmelthieres Pfiff,", "tokens": ["Nur", "ne\u00b7ben", "mir", "des", "Mur\u00b7melt\u00b7hie\u00b7res", "Pfiff", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Nur \u00fcber mir des Geiers heis'rer Schrei,", "tokens": ["Nur", "\u00fc\u00b7ber", "mir", "des", "Gei\u00b7ers", "heis'\u00b7rer", "Schrei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ich bin allein auf meinem Felsenriff", "tokens": ["Ich", "bin", "al\u00b7lein", "auf", "mei\u00b7nem", "Fel\u00b7sen\u00b7riff"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und ich empfinde da\u00df Gott bei mir sei.", "tokens": ["Und", "ich", "emp\u00b7fin\u00b7de", "da\u00df", "Gott", "bei", "mir", "sei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "KOUS", "NN", "APPR", "PPER", "VAFIN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}}}}