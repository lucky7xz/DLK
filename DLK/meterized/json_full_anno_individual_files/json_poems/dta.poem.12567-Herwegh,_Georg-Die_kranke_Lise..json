{"dta.poem.12567": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Die kranke Lise.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1843", "urn": "urn:nbn:de:kobv:b4-200905192440", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Weihnacht! die kranke Lise schreitet", "tokens": ["Weih\u00b7nacht", "!", "die", "kran\u00b7ke", "Li\u00b7se", "schrei\u00b7tet"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$.", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Durch's Fauburg hin in banger Flucht,", "tokens": ["Durch's", "Fau\u00b7burg", "hin", "in", "ban\u00b7ger", "Flucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie hat zu Haus kein Bett bereitet", "tokens": ["Sie", "hat", "zu", "Haus", "kein", "Bett", "be\u00b7rei\u00b7tet"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "NN", "PIAT", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "F\u00fcr ihres Leibes erste Frucht.", "tokens": ["F\u00fcr", "ih\u00b7res", "Lei\u00b7bes", "ers\u00b7te", "Frucht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wohl manches prunkt im F\u00fcrstensaale,", "tokens": ["Wohl", "man\u00b7ches", "prunkt", "im", "F\u00fcrs\u00b7ten\u00b7saa\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Den stolzer Kerzen Glanz erhellt \u2014", "tokens": ["Den", "stol\u00b7zer", "Ker\u00b7zen", "Glanz", "er\u00b7hellt"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Marsch, Lise, weiter, zum Spitale!", "tokens": ["Marsch", ",", "Li\u00b7se", ",", "wei\u00b7ter", ",", "zum", "Spi\u00b7ta\u00b7le", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "NE", "$,", "ADV", "$,", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Dort kommt das Volk zur Welt.", "tokens": ["Dort", "kommt", "das", "Volk", "zur", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "\u201emein armer Weber mag nur zetteln,", "tokens": ["\u201e", "mein", "ar\u00b7mer", "We\u00b7ber", "mag", "nur", "zet\u00b7teln", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "ADJA", "NN", "VMFIN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sein Flei\u00df und Schwei\u00df \u2014 was helfen sie?", "tokens": ["Sein", "Flei\u00df", "und", "Schwei\u00df", "was", "hel\u00b7fen", "sie", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "$(", "PWS", "VVFIN", "PPER", "$."], "meter": "-+-++--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Das Volk mu\u00df Sarg und Wiege betteln:", "tokens": ["Das", "Volk", "mu\u00df", "Sarg", "und", "Wie\u00b7ge", "bet\u00b7teln", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Kind, dem sie unter meinem Herzen", "tokens": ["Kind", ",", "dem", "sie", "un\u00b7ter", "mei\u00b7nem", "Her\u00b7zen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PRELS", "PPER", "APPR", "PPOSAT", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Die Lust am Leben schon verg\u00e4llt,", "tokens": ["Die", "Lust", "am", "Le\u00b7ben", "schon", "ver\u00b7g\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Geduld, bis wir im Haus der Schmerzen!", "tokens": ["Ge\u00b7duld", ",", "bis", "wir", "im", "Haus", "der", "Schmer\u00b7zen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Dort kommt das Volk zur Welt.", "tokens": ["Dort", "kommt", "das", "Volk", "zur", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "\u201esie feiern heut dem Gott der Armen,", "tokens": ["\u201e", "sie", "fei\u00b7ern", "heut", "dem", "Gott", "der", "Ar\u00b7men", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "ADV", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die reichen Herr'n, ein Freudenfest:", "tokens": ["Die", "rei\u00b7chen", "Herr'n", ",", "ein", "Freu\u00b7den\u00b7fest", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch glaubt nicht, da\u00df sich das Erbarmen", "tokens": ["Doch", "glaubt", "nicht", ",", "da\u00df", "sich", "das", "Er\u00b7bar\u00b7men"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PTKNEG", "$,", "KOUS", "PRF", "ART", "NN"], "meter": "-+-++--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "An ihrem Tische sehen l\u00e4\u00dft,", "tokens": ["An", "ih\u00b7rem", "Ti\u00b7sche", "se\u00b7hen", "l\u00e4\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da\u00df je in ihre Festpokale", "tokens": ["Da\u00df", "je", "in", "ih\u00b7re", "Fest\u00b7po\u00b7ka\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der Schimmer einer Thr\u00e4ne f\u00e4llt \u2014", "tokens": ["Der", "Schim\u00b7mer", "ei\u00b7ner", "Thr\u00e4\u00b7ne", "f\u00e4llt"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Marsch, Lise, weiter, zum Spitale!", "tokens": ["Marsch", ",", "Li\u00b7se", ",", "wei\u00b7ter", ",", "zum", "Spi\u00b7ta\u00b7le", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "NE", "$,", "ADV", "$,", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Dort kommt das Volk zur Welt.", "tokens": ["Dort", "kommt", "das", "Volk", "zur", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "\u201edu machst mir wahrlich viel Beschwerden,", "tokens": ["\u201e", "du", "machst", "mir", "wahr\u00b7lich", "viel", "Be\u00b7schwer\u00b7den", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PPER", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Liebe Kind, ich dacht' es nie;", "tokens": ["Der", "Lie\u00b7be", "Kind", ",", "ich", "dacht'", "es", "nie", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "PPER", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das wird ein wilder Junge werden:", "tokens": ["Das", "wird", "ein", "wil\u00b7der", "Jun\u00b7ge", "wer\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "F\u00fcr eurer Prinzen zarte Nerven", "tokens": ["F\u00fcr", "eu\u00b7rer", "Prin\u00b7zen", "zar\u00b7te", "Ner\u00b7ven"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ist Daun' auf Daune hoch geschwellt:", "tokens": ["Ist", "Daun'", "auf", "Dau\u00b7ne", "hoch", "ge\u00b7schwellt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "APPR", "NE", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So kommt das Volk zur Welt.", "tokens": ["So", "kommt", "das", "Volk", "zur", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "\u201ekl\u00e4ng' noch die Trommel unserm Ohre", "tokens": ["\u201e", "kl\u00e4ng'", "noch", "die", "Trom\u00b7mel", "un\u00b7serm", "Oh\u00b7re"], "token_info": ["punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "VVFIN", "ADV", "ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und w\u00e4r' noch ", "tokens": ["Und", "w\u00e4r'", "noch"], "token_info": ["word", "word", "word"], "pos": ["KON", "VAFIN", "ADV"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Der Lappen einer Trikolore,", "tokens": ["Der", "Lap\u00b7pen", "ei\u00b7ner", "Tri\u00b7ko\u00b7lo\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Er sollte deine Windel sein;", "tokens": ["Er", "soll\u00b7te", "dei\u00b7ne", "Win\u00b7del", "sein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Du w\u00e4rst getauft, eh' seine Schaale", "tokens": ["Du", "w\u00e4rst", "ge\u00b7tauft", ",", "eh'", "sei\u00b7ne", "Schaa\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "VVPP", "$,", "KOUS", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ein Pfaffe dir zu H\u00e4ubten h\u00e4lt \u2014", "tokens": ["Ein", "Pfaf\u00b7fe", "dir", "zu", "H\u00e4ub\u00b7ten", "h\u00e4lt"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Marsch, Lise, weiter, zum Spitale!", "tokens": ["Marsch", ",", "Li\u00b7se", ",", "wei\u00b7ter", ",", "zum", "Spi\u00b7ta\u00b7le", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "NE", "$,", "ADV", "$,", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Dort kommt das Volk zur Welt.", "tokens": ["Dort", "kommt", "das", "Volk", "zur", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "\u201ewer wird so ungest\u00fcm sich melden?", "tokens": ["\u201e", "wer", "wird", "so", "un\u00b7ge\u00b7st\u00fcm", "sich", "mel\u00b7den", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VAFIN", "ADV", "ADJD", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mein kleines Herz, was suchst du hie?", "tokens": ["Mein", "klei\u00b7nes", "Herz", ",", "was", "suchst", "du", "hie", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PWS", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nur noch zum Grabe jener Helden!", "tokens": ["Nur", "noch", "zum", "Gra\u00b7be", "je\u00b7ner", "Hel\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPRART", "NN", "PDAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dort seh' ich in des Fr\u00fchroth's Helle", "tokens": ["Dort", "seh'", "ich", "in", "des", "Fr\u00fchro\u00b7th's", "Hel\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die Julis\u00e4ule aufgestellt \u2014\u201c", "tokens": ["Die", "Ju\u00b7li\u00b7s\u00e4u\u00b7le", "auf\u00b7ge\u00b7stellt", "\u201c"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVPP", "$(", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und nieder sank sie auf der Schwelle; \u2014", "tokens": ["Und", "nie\u00b7der", "sank", "sie", "auf", "der", "Schwel\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PTKVZ", "VVFIN", "PPER", "APPR", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}