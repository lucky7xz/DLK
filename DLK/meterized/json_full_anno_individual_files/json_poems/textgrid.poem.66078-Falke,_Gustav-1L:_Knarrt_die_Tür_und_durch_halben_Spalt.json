{"textgrid.poem.66078": {"metadata": {"author": {"name": "Falke, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "1L: Knarrt die T\u00fcr und durch halben Spalt", "genre": "verse", "period": "N.A.", "pub_year": 1884, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Knarrt die T\u00fcr und durch halben Spalt", "tokens": ["Knarrt", "die", "T\u00fcr", "und", "durch", "hal\u00b7ben", "Spalt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "KON", "APPR", "ADJA", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Schiebt sich die zierlichste Gestalt,", "tokens": ["Schiebt", "sich", "die", "zier\u00b7lichs\u00b7te", "Ge\u00b7stalt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Tr\u00e4gt eine Tulpe in der Hand:", "tokens": ["Tr\u00e4gt", "ei\u00b7ne", "Tul\u00b7pe", "in", "der", "Hand", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sieh mal, Papa, was Ursel fand.", "tokens": ["Sieh", "mal", ",", "Pa\u00b7pa", ",", "was", "Ur\u00b7sel", "fand", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "$,", "NN", "$,", "PWS", "NN", "VVFIN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.2": {"line.1": {"text": "Wirklich? Was du nicht sagst. Ei seht!", "tokens": ["Wirk\u00b7lich", "?", "Was", "du", "nicht", "sagst", ".", "Ei", "seht", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "$.", "PWS", "PPER", "PTKNEG", "VVFIN", "$.", "NN", "VVFIN", "$."], "meter": "+----++-", "measure": "dactylic.init"}, "line.2": {"text": "Die sch\u00f6nste Tulpe vom ganzen Beet.", "tokens": ["Die", "sch\u00f6ns\u00b7te", "Tul\u00b7pe", "vom", "gan\u00b7zen", "Beet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Gefunden hast du den Feuerhelm?", "tokens": ["Ge\u00b7fun\u00b7den", "hast", "du", "den", "Feu\u00b7er\u00b7helm", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ART", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ich f\u00fcrchte, du flunkerst, kleiner Schelm.", "tokens": ["Ich", "f\u00fcrch\u00b7te", ",", "du", "flun\u00b7kerst", ",", "klei\u00b7ner", "Schelm", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "$,", "ADJA", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Sie kichert und guckt in den Kelch hinein,", "tokens": ["Sie", "ki\u00b7chert", "und", "guckt", "in", "den", "Kelch", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Freut sich, und ich soll b\u00f6se sein?", "tokens": ["Freut", "sich", ",", "und", "ich", "soll", "b\u00f6\u00b7se", "sein", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "$,", "KON", "PPER", "VMFIN", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gelbt sich am Griffel das N\u00e4schen und macht", "tokens": ["Gelbt", "sich", "am", "Grif\u00b7fel", "das", "N\