{"dta.poem.2147": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Geistige Abg\u00f6tterey und ihre  \n Sch\u00e4dlichkeit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Von einer Gottheit, die unendlich, ein k\u00f6rperliches", "tokens": ["Von", "ei\u00b7ner", "Got\u00b7theit", ",", "die", "un\u00b7end\u00b7lich", ",", "ein", "k\u00f6r\u00b7per\u00b7li\u00b7ches"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "ADJD", "$,", "ART", "ADJA"], "meter": "-+--+--+-+++-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Bild formiren,", "tokens": ["Bild", "for\u00b7mi\u00b7ren", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Ist untersagt, auch l\u00e4cherlich, indem es der Unendlichkeit", "tokens": ["Ist", "un\u00b7ter\u00b7sagt", ",", "auch", "l\u00e4\u00b7cher\u00b7lich", ",", "in\u00b7dem", "es", "der", "Un\u00b7end\u00b7lich\u00b7keit"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "VVPP", "$,", "ADV", "ADJD", "$,", "KOUS", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.4": {"text": "Nachtheilig ist und unanst\u00e4ndig. Doch g\u00f6ttlicher Voll-", "tokens": ["Nacht\u00b7hei\u00b7lig", "ist", "und", "un\u00b7an\u00b7st\u00e4n\u00b7dig", ".", "Doch", "g\u00f6tt\u00b7li\u00b7cher", "Voll"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "KON", "ADJD", "$.", "KON", "ADJA", "TRUNC"], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "kommenheit", "tokens": ["kom\u00b7men\u00b7heit"], "token_info": ["word"], "pos": ["NN"], "meter": "+--", "measure": "dactylic.init"}, "line.6": {"text": "Ist es nicht minder unanst\u00e4ndig, wenn Menschen sich so", "tokens": ["Ist", "es", "nicht", "min\u00b7der", "un\u00b7an\u00b7st\u00e4n\u00b7dig", ",", "wenn", "Men\u00b7schen", "sich", "so"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "PTKNEG", "ADV", "ADJD", "$,", "KOUS", "NN", "PRF", "ADV"], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "weit verlieren,", "tokens": ["weit", "ver\u00b7lie\u00b7ren", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.8": {"text": "Und im Gehirn ein geistig Bild von einem alten Mann", "tokens": ["Und", "im", "Ge\u00b7hirn", "ein", "geis\u00b7tig", "Bild", "von", "ei\u00b7nem", "al\u00b7ten", "Mann"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "ART", "ADJD", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.9": {"text": "erh\u00f6hn,", "tokens": ["er\u00b7h\u00f6hn", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.10": {"text": "An welchem sie mit ihrem Geist bekleben, und nicht wei-", "tokens": ["An", "wel\u00b7chem", "sie", "mit", "ih\u00b7rem", "Geist", "be\u00b7kle\u00b7ben", ",", "und", "nicht", "wei"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PRELS", "PPER", "APPR", "PPOSAT", "NN", "VVINF", "$,", "KON", "PTKNEG", "TRUNC"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.11": {"text": "ter gehn.", "tokens": ["ter", "gehn", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVINF", "$."], "meter": "-+", "measure": "iambic.single"}, "line.12": {"text": "Noch mehr, man stellt sich solchen Gott, als wenn er nur", "tokens": ["Noch", "mehr", ",", "man", "stellt", "sich", "sol\u00b7chen", "Gott", ",", "als", "wenn", "er", "nur"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "$,", "PIS", "VVFIN", "PRF", "PIAT", "NN", "$,", "KOKOM", "KOUS", "PPER", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "aufs B\u00f6se siehet,", "tokens": ["aufs", "B\u00f6\u00b7se", "sie\u00b7het", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.14": {"text": "Als w\u00e4re er um unsre Laster scharf zu bestrafen nur be-", "tokens": ["Als", "w\u00e4\u00b7re", "er", "um", "uns\u00b7re", "Las\u00b7ter", "scharf", "zu", "be\u00b7stra\u00b7fen", "nur", "be"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "VAFIN", "PPER", "APPR", "PPOSAT", "NN", "ADJD", "PTKZU", "VVINF", "ADV", "TRUNC"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.15": {"text": "m\u00fchet,", "tokens": ["m\u00fc\u00b7het", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.16": {"text": "Als einen scharfen Richter vor. Wie! streiten nicht so", "tokens": ["Als", "ei\u00b7nen", "schar\u00b7fen", "Rich\u00b7ter", "vor", ".", "Wie", "!", "strei\u00b7ten", "nicht", "so"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "PTKVZ", "$.", "PWAV", "$.", "VVFIN", "PTKNEG", "ADV"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.17": {"text": "strenge Triebe", "tokens": ["stren\u00b7ge", "Trie\u00b7be"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.18": {"text": "Mit der Unendlichkeit der Weisheit, mit der Unendlich-", "tokens": ["Mit", "der", "Un\u00b7end\u00b7lich\u00b7keit", "der", "Weis\u00b7heit", ",", "mit", "der", "Un\u00b7end\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,", "APPR", "ART", "TRUNC"], "meter": "-+-+-+-+-+-+--", "measure": "unknown.measure.hexa"}, "line.19": {"text": "keit der Liebe", "tokens": ["keit", "der", "Lie\u00b7be"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ART", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.20": {"text": "Und mit der ganzen Gottheit selbst? Allein: ist Gott", "tokens": ["Und", "mit", "der", "gan\u00b7zen", "Got\u00b7theit", "selbst", "?", "Al\u00b7lein", ":", "ist", "Gott"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "ADV", "$.", "ADV", "$.", "VAFIN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "denn nicht gerecht?", "tokens": ["denn", "nicht", "ge\u00b7recht", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.22": {"text": "O ja: gerecht ist seine Liebe. Sie straft zur Be\u00dfrung", "tokens": ["O", "ja", ":", "ge\u00b7recht", "ist", "sei\u00b7ne", "Lie\u00b7be", ".", "Sie", "straft", "zur", "Be\u00df\u00b7rung"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "ITJ", "$.", "ADJD", "VAFIN", "PPOSAT", "NN", "$.", "PPER", "ADJD", "APPRART", "NN"], "meter": "+--+-+-+--+-+-", "measure": "iambic.hexa.invert"}, "line.23": {"text": "allezeit.", "tokens": ["al\u00b7le\u00b7zeit", "."], "token_info": ["word", "punct"], "pos": ["ADV", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.24": {"text": "Es prediget uns die Natur und die Vernunft ohn Un-", "tokens": ["Es", "pre\u00b7di\u00b7get", "uns", "die", "Na\u00b7tur", "und", "die", "Ver\u00b7nunft", "ohn", "Un"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "KON", "ART", "NN", "APPR", "TRUNC"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.25": {"text": "terla\u00df,", "tokens": ["ter\u00b7la\u00df", ","], "token_info": ["word", "punct"], "pos": ["ADJD", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.26": {"text": "Es k\u00f6nne die Gerechtigkeit", "tokens": ["Es", "k\u00f6n\u00b7ne", "die", "Ge\u00b7rech\u00b7tig\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Kein Gegensatz der Liebe seyn. Der Liebe Gegensatz ist", "tokens": ["Kein", "Ge\u00b7gen\u00b7satz", "der", "Lie\u00b7be", "seyn", ".", "Der", "Lie\u00b7be", "Ge\u00b7gen\u00b7satz", "ist"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "ART", "NN", "VAINF", "$.", "ART", "NN", "NN", "VAFIN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.28": {"text": "Ha\u00df.", "tokens": ["Ha\u00df", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}}}}}