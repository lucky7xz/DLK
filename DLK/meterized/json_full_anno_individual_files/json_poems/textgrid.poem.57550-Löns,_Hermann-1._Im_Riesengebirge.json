{"textgrid.poem.57550": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "1. Im Riesengebirge", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Verschneiter Berge Silberkuppen \u2013", "tokens": ["Ver\u00b7schnei\u00b7ter", "Ber\u00b7ge", "Sil\u00b7ber\u00b7kup\u00b7pen", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der gelbe Mond treibt dr\u00fcber her,", "tokens": ["Der", "gel\u00b7be", "Mond", "treibt", "dr\u00fc\u00b7ber", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PAV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und hohe Tannen, stumm und d\u00fcster,", "tokens": ["Und", "ho\u00b7he", "Tan\u00b7nen", ",", "stumm", "und", "d\u00fcs\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zerriss'ner Felsen wirres Meer;", "tokens": ["Zer\u00b7riss'\u00b7ner", "Fel\u00b7sen", "wir\u00b7res", "Meer", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Des Bobers fr\u00fchjahrstr\u00fcbe Wellen", "tokens": ["Des", "Bo\u00b7bers", "fr\u00fch\u00b7jahr\u00b7str\u00fc\u00b7be", "Wel\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Durchglei\u00dft des Mondes Silberschein,", "tokens": ["Durch\u00b7glei\u00dft", "des", "Mon\u00b7des", "Sil\u00b7ber\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Die Wasseramsel pfeift und trillert \u2013", "tokens": ["Die", "Was\u00b7ser\u00b7am\u00b7sel", "pfeift", "und", "tril\u00b7lert", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Heut ist es schwer, allein zu sein ...", "tokens": ["Heut", "ist", "es", "schwer", ",", "al\u00b7lein", "zu", "sein", "..."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "$,", "ADV", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Vom Grunde taucht ein Antlitz auf", "tokens": ["Vom", "Grun\u00b7de", "taucht", "ein", "Ant\u00b7litz", "auf"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit Augen treu und sonnenklar,", "tokens": ["Mit", "Au\u00b7gen", "treu", "und", "son\u00b7nen\u00b7klar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit wei\u00dfer, faltenloser Stirn", "tokens": ["Mit", "wei\u00b7\u00dfer", ",", "fal\u00b7ten\u00b7lo\u00b7ser", "Stirn"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und kurzem, goldigrotem Haar,", "tokens": ["Und", "kur\u00b7zem", ",", "gol\u00b7di\u00b7gro\u00b7tem", "Haar", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Welle rauscht ein leises Wort,", "tokens": ["Die", "Wel\u00b7le", "rauscht", "ein", "lei\u00b7ses", "Wort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ein Lachen klingt, so schmerzensrein,", "tokens": ["Ein", "La\u00b7chen", "klingt", ",", "so", "schmer\u00b7zen\u00b7srein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wie ich es oft von ihr geh\u00f6rt \u2013", "tokens": ["Wie", "ich", "es", "oft", "von", "ihr", "ge\u00b7h\u00f6rt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "ADV", "APPR", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "O w\u00e4rst du mein!", "tokens": ["O", "w\u00e4rst", "du", "mein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "PPOSAT", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Krampf' dich zusammen, stolzes Herz,", "tokens": ["Krampf'", "dich", "zu\u00b7sam\u00b7men", ",", "stol\u00b7zes", "Herz", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKVZ", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und zittre nicht, du hei\u00dfer Leib,", "tokens": ["Und", "zitt\u00b7re", "nicht", ",", "du", "hei\u00b7\u00dfer", "Leib", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dies Weib wird nie dein eigen sein,", "tokens": ["Dies", "Weib", "wird", "nie", "dein", "ei\u00b7gen", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "VAFIN", "ADV", "PPOSAT", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Denn sie ist eines andern Weib!", "tokens": ["Denn", "sie", "ist", "ei\u00b7nes", "an\u00b7dern", "Weib", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Es war bei heller Lampen Schein,", "tokens": ["Es", "war", "bei", "hel\u00b7ler", "Lam\u00b7pen", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da sah ich sie zum erstenmal,", "tokens": ["Da", "sah", "ich", "sie", "zum", "ers\u00b7ten\u00b7mal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "APPRART", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bei Gl\u00e4serklang, Gesang und Wein,", "tokens": ["Bei", "Gl\u00e4\u00b7ser\u00b7klang", ",", "Ge\u00b7sang", "und", "Wein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bei Unterhaltung, matt und schal;", "tokens": ["Bei", "Un\u00b7ter\u00b7hal\u00b7tung", ",", "matt", "und", "schal", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sie sa\u00df mir grade gegen\u00fcber,", "tokens": ["Sie", "sa\u00df", "mir", "gra\u00b7de", "ge\u00b7gen\u00b7\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ihr frohes Kinderantlitz war", "tokens": ["Ihr", "fro\u00b7hes", "Kin\u00b7de\u00b7rant\u00b7litz", "war"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Bei all dem neidischgelben Klatsche", "tokens": ["Bei", "all", "dem", "nei\u00b7dischgel\u00b7ben", "Klat\u00b7sche"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "ART", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "So unber\u00fchrt und fromm und klar,", "tokens": ["So", "un\u00b7be\u00b7r\u00fchrt", "und", "fromm", "und", "klar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Sie plauderte so kindlichfromm", "tokens": ["Sie", "plau\u00b7der\u00b7te", "so", "kind\u00b7lich\u00b7fromm"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Vom Sch\u00f6nen, das die Erde gibt...", "tokens": ["Vom", "Sch\u00f6\u00b7nen", ",", "das", "die", "Er\u00b7de", "gibt", "..."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Sie war ein Weib, kaum achtzehn Jahre,", "tokens": ["Sie", "war", "ein", "Weib", ",", "kaum", "acht\u00b7zehn", "Jah\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "ADV", "CARD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sie liebte und sie ward geliebt ...", "tokens": ["Sie", "lieb\u00b7te", "und", "sie", "ward", "ge\u00b7liebt", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie reizend scho\u00df die rote Glut", "tokens": ["Wie", "rei\u00b7zend", "scho\u00df", "die", "ro\u00b7te", "Glut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ihr \u00fcber Nacken und Gesicht \u2013", "tokens": ["Ihr", "\u00fc\u00b7ber", "Na\u00b7cken", "und", "Ge\u00b7sicht", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich wu\u00dft' es, nie w\u00fcrd' sie mein eigen,", "tokens": ["Ich", "wu\u00dft'", "es", ",", "nie", "w\u00fcrd'", "sie", "mein", "ei\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "ADV", "VAFIN", "PPER", "PPOSAT", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ich wu\u00dft's und schlo\u00df die Augen ", "tokens": ["Ich", "wu\u00dft's", "und", "schlo\u00df", "die", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Verschneiter Berge Silberkuppen \u2013", "tokens": ["Ver\u00b7schnei\u00b7ter", "Ber\u00b7ge", "Sil\u00b7ber\u00b7kup\u00b7pen", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der gelbe Mond treibt dr\u00fcber her,", "tokens": ["Der", "gel\u00b7be", "Mond", "treibt", "dr\u00fc\u00b7ber", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PAV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und hohe Tannen, stumm und d\u00fcster,", "tokens": ["Und", "ho\u00b7he", "Tan\u00b7nen", ",", "stumm", "und", "d\u00fcs\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zerriss'ner Felsen wirres Meer;", "tokens": ["Zer\u00b7riss'\u00b7ner", "Fel\u00b7sen", "wir\u00b7res", "Meer", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Des Bobers fr\u00fchjahrstr\u00fcbe Wellen", "tokens": ["Des", "Bo\u00b7bers", "fr\u00fch\u00b7jahr\u00b7str\u00fc\u00b7be", "Wel\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Durchglei\u00dft des Mondes Silberschein,", "tokens": ["Durch\u00b7glei\u00dft", "des", "Mon\u00b7des", "Sil\u00b7ber\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Die Wasseramsel pfeift und trillert \u2013", "tokens": ["Die", "Was\u00b7ser\u00b7am\u00b7sel", "pfeift", "und", "tril\u00b7lert", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Heut ist es schwer, allein zu sein ...", "tokens": ["Heut", "ist", "es", "schwer", ",", "al\u00b7lein", "zu", "sein", "..."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "$,", "ADV", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Vom Grunde taucht ein Antlitz auf", "tokens": ["Vom", "Grun\u00b7de", "taucht", "ein", "Ant\u00b7litz", "auf"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit Augen treu und sonnenklar,", "tokens": ["Mit", "Au\u00b7gen", "treu", "und", "son\u00b7nen\u00b7klar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit wei\u00dfer, faltenloser Stirn", "tokens": ["Mit", "wei\u00b7\u00dfer", ",", "fal\u00b7ten\u00b7lo\u00b7ser", "Stirn"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und kurzem, goldigrotem Haar,", "tokens": ["Und", "kur\u00b7zem", ",", "gol\u00b7di\u00b7gro\u00b7tem", "Haar", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Welle rauscht ein leises Wort,", "tokens": ["Die", "Wel\u00b7le", "rauscht", "ein", "lei\u00b7ses", "Wort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ein Lachen klingt, so sch