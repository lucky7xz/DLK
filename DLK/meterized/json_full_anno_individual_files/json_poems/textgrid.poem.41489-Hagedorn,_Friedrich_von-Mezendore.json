{"textgrid.poem.41489": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Mezendore", "genre": "verse", "period": "N.A.", "pub_year": 1731, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Herr Nicolaus Klimm erfand", "tokens": ["Herr", "Ni\u00b7co\u00b7laus", "Klimm", "er\u00b7fand"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "NE", "NE", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Mehr L\u00e4nder als ich Reime,", "tokens": ["Mehr", "L\u00e4n\u00b7der", "als", "ich", "Rei\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KOUS", "PPER", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So gar ein unterirdisch Land", "tokens": ["So", "gar", "ein", "un\u00b7ter\u00b7ir\u00b7disch", "Land"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vern\u00fcnft'ger Thier' und B\u00e4ume.", "tokens": ["Ver\u00b7n\u00fcnft'\u00b7ger", "Thier'", "und", "B\u00e4u\u00b7me", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die Ober- und die Unterwelt", "tokens": ["Die", "O\u00b7ber", "und", "die", "Un\u00b7ter\u00b7welt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "TRUNC", "KON", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Bewunderten den gro\u00dfen Held.", "tokens": ["Be\u00b7wun\u00b7der\u00b7ten", "den", "gro\u00b7\u00dfen", "Held", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.7": {"text": "Er pranget im Register", "tokens": ["Er", "pran\u00b7get", "im", "Re\u00b7gis\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Der Kaiser und der K\u00fcster.", "tokens": ["Der", "Kai\u00b7ser", "und", "der", "K\u00fcs\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Des Landes Name klinget fein,", "tokens": ["Des", "Lan\u00b7des", "Na\u00b7me", "klin\u00b7get", "fein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und schmeichelt recht dem Ohre.", "tokens": ["Und", "schmei\u00b7chelt", "recht", "dem", "Oh\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Es hei\u00dfet, (was kann sch\u00f6ner sein?)", "tokens": ["Es", "hei\u00b7\u00dfet", ",", "(", "was", "kann", "sch\u00f6\u00b7ner", "sein", "?", ")"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "$,", "$(", "PWS", "VMFIN", "ADJD", "VAINF", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Es hei\u00dfet Mezendore.", "tokens": ["Es", "hei\u00b7\u00dfet", "Me\u00b7zen\u00b7do\u00b7re", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Hier hat das thierische Geschlecht", "tokens": ["Hier", "hat", "das", "thie\u00b7ri\u00b7sche", "Ge\u00b7schlecht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und jeder Baum das B\u00fcrgerrecht,", "tokens": ["Und", "je\u00b7der", "Baum", "das", "B\u00fcr\u00b7ger\u00b7recht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wenn er, wie sich's geh\u00f6ret,", "tokens": ["Wenn", "er", ",", "wie", "sich's", "ge\u00b7h\u00f6\u00b7ret", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "PWAV", "PIS", "VVFIN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.8": {"text": "Die Obrigkeit verehret.", "tokens": ["Die", "Ob\u00b7rig\u00b7keit", "ver\u00b7eh\u00b7ret", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Der L\u00f6we bleibet allemal", "tokens": ["Der", "L\u00f6\u00b7we", "blei\u00b7bet", "al\u00b7le\u00b7mal"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NE", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Monarch des ganzen Staates.", "tokens": ["Mon\u00b7arch", "des", "gan\u00b7zen", "Staa\u00b7tes", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Elephanten trifft die Wahl", "tokens": ["Die", "E\u00b7le\u00b7phan\u00b7ten", "trifft", "die", "Wahl"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zu Gliedern seines Rathes.", "tokens": ["Zu", "Glie\u00b7dern", "sei\u00b7nes", "Ra\u00b7thes", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ein luftiger Cham\u00e4leon", "tokens": ["Ein", "luf\u00b7ti\u00b7ger", "Cha\u00b7m\u00e4\u00b7le\u00b7on"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Tr\u00e4gt stets das Canzleramt davon,", "tokens": ["Tr\u00e4gt", "stets", "das", "Canz\u00b7ler\u00b7amt", "da\u00b7von", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und was er angefangen,", "tokens": ["Und", "was", "er", "an\u00b7ge\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Vollf\u00fchren F\u00fcchs' und Schlangen.", "tokens": ["Voll\u00b7f\u00fch\u00b7ren", "F\u00fcchs'", "und", "Schlan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Die Ritterschaft bestehet hier", "tokens": ["Die", "Rit\u00b7ter\u00b7schaft", "be\u00b7ste\u00b7het", "hier"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Aus Strau\u00dfen und aus Pfauen.", "tokens": ["Aus", "Strau\u00b7\u00dfen", "und", "aus", "Pfau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das Oechslein und das andre Thier", "tokens": ["Das", "O\u00b7echs\u00b7lein", "und", "das", "and\u00b7re", "Thier"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "ADJA", "NN"], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.4": {"text": "L\u00e4\u00dft sich als B\u00fcrger schauen.", "tokens": ["L\u00e4\u00dft", "sich", "als", "B\u00fcr\u00b7ger", "schau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "KOUS", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Das Schaf, der Hamster und das Schwein", "tokens": ["Das", "Schaf", ",", "der", "Hams\u00b7ter", "und", "das", "Schwein"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sind Bauern, oder k\u00f6nnten's sein.", "tokens": ["Sind", "Bau\u00b7ern", ",", "o\u00b7der", "k\u00f6nn\u00b7ten's", "sein", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "$,", "KON", "VMFIN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die sich dem Lehramt weihen,", "tokens": ["Die", "sich", "dem", "Le\u00b7hramt", "wei\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Sind trockne Papageien.", "tokens": ["Sind", "trock\u00b7ne", "Pa\u00b7pa\u00b7gei\u00b7en", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Das Kriegesheer trotzt auf die Treu'", "tokens": ["Das", "Krie\u00b7ges\u00b7heer", "trotzt", "auf", "die", "Treu'"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ge\u00fcbter Tigerschaaren,", "tokens": ["Ge\u00b7\u00fcb\u00b7ter", "Ti\u00b7ger\u00b7schaa\u00b7ren", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das leichte Hirschvolk dient dabei", "tokens": ["Das", "leich\u00b7te", "Hirschvolk", "dient", "da\u00b7bei"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PAV"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Statt streifender Husaren.", "tokens": ["Statt", "strei\u00b7fen\u00b7der", "Hu\u00b7sa\u00b7ren", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die Flotten f\u00fchrt das Wasserpferd,", "tokens": ["Die", "Flot\u00b7ten", "f\u00fchrt", "das", "Was\u00b7ser\u00b7pferd", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der Raubfisch mit dem scharfen Schwert,", "tokens": ["Der", "Raub\u00b7fisch", "mit", "dem", "schar\u00b7fen", "Schwert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Den S\u00e4uger", "tokens": ["Den", "S\u00e4u\u00b7ger"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.8": {"text": "Hilft ihrer Seemacht streiten.", "tokens": ["Hilft", "ih\u00b7rer", "See\u00b7macht", "strei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Die Kammer n\u00e4hrt aus weiser Huld", "tokens": ["Die", "Kam\u00b7mer", "n\u00e4hrt", "aus", "wei\u00b7ser", "Huld"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zehn hochbetraute B\u00e4ren,", "tokens": ["Zehn", "hoch\u00b7be\u00b7trau\u00b7te", "B\u00e4\u00b7ren", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["CARD", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Den Anlauf jeder alten Schuld", "tokens": ["Den", "An\u00b7lauf", "je\u00b7der", "al\u00b7ten", "Schuld"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gebietrisch abzuwehren.", "tokens": ["Ge\u00b7bie\u00b7trisch", "ab\u00b7zu\u00b7weh\u00b7ren", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVIZU", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der Habicht nimmt die Steuern ein:", "tokens": ["Der", "Ha\u00b7bicht", "nimmt", "die", "Steu\u00b7ern", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Den Dohlen mu\u00df der Reiche leihn:", "tokens": ["Den", "Doh\u00b7len", "mu\u00df", "der", "Rei\u00b7che", "leihn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ART", "NE", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Zu P\u00e4chtern setzt man Raben", "tokens": ["Zu", "P\u00e4ch\u00b7tern", "setzt", "man", "Ra\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "PIS", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Von ungemeinen Gaben.", "tokens": ["Von", "un\u00b7ge\u00b7mei\u00b7nen", "Ga\u00b7ben", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Das Richteramt wird hier bestellt", "tokens": ["Das", "Rich\u00b7ter\u00b7amt", "wird", "hier", "be\u00b7stellt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADV", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Durch Menschen-gleiche B\u00e4ume.", "tokens": ["Durch", "Men\u00b7schen\u00b7glei\u00b7che", "B\u00e4u\u00b7me", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Birke straft die junge Welt,", "tokens": ["Die", "Bir\u00b7ke", "straft", "die", "jun\u00b7ge", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Lorbeer schlechte Reime:", "tokens": ["Der", "Lor\u00b7beer", "schlech\u00b7te", "Rei\u00b7me", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und weil hier Frost und N\u00fcchternheit", "tokens": ["Und", "weil", "hier", "Frost", "und", "N\u00fcch\u00b7tern\u00b7heit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ADV", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Nur gar zu oft den Dichtern dr\u00e4ut;", "tokens": ["Nur", "gar", "zu", "oft", "den", "Dich\u00b7tern", "dr\u00e4ut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PTKA", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "So hei\u00dfen sie die Reben", "tokens": ["So", "hei\u00b7\u00dfen", "sie", "die", "Re\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Sich und den Vers beleben.", "tokens": ["Sich", "und", "den", "Vers", "be\u00b7le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "KON", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Die G\u00e4nse schnattern vor Gericht", "tokens": ["Die", "G\u00e4n\u00b7se", "schnat\u00b7tern", "vor", "Ge\u00b7richt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Laut schallende Recesse,", "tokens": ["Laut", "schal\u00b7len\u00b7de", "Re\u00b7ces\u00b7se", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "++-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Damit der Kauz, als Schreiber, nicht", "tokens": ["Da\u00b7mit", "der", "Kauz", ",", "als", "Schrei\u00b7ber", ",", "nicht"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PAV", "ART", "NN", "$,", "KOUS", "NN", "$,", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den kleinsten Satz vergesse.", "tokens": ["Den", "kleins\u00b7ten", "Satz", "ver\u00b7ges\u00b7se", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Allein, vor niederm Ding und Recht", "tokens": ["Al\u00b7lein", ",", "vor", "nie\u00b7derm", "Ding", "und", "Recht"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "APPR", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Erscheinen Elster, Staar und Specht;", "tokens": ["Er\u00b7schei\u00b7nen", "Els\u00b7ter", ",",