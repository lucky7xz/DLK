{"textgrid.poem.57922": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Die Mordsmode", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Einen kleinen Vogel hatte fr\u00fcher", "tokens": ["Ei\u00b7nen", "klei\u00b7nen", "Vo\u00b7gel", "hat\u00b7te", "fr\u00fc\u00b7her"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NE", "VAFIN", "ADJD"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Jede Dame, n\u00e4mlich auf dem Hut,", "tokens": ["Je\u00b7de", "Da\u00b7me", ",", "n\u00e4m\u00b7lich", "auf", "dem", "Hut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Unter zwei bis dreien heutzutage", "tokens": ["Un\u00b7ter", "zwei", "bis", "drei\u00b7en", "heut\u00b7zu\u00b7ta\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "CARD", "APPR", "CARD", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Es die Modedame nicht mehr tut.", "tokens": ["Es", "die", "Mo\u00b7de\u00b7da\u00b7me", "nicht", "mehr", "tut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "PTKNEG", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "In der ganzen Welt beginnt ein Morden,", "tokens": ["In", "der", "gan\u00b7zen", "Welt", "be\u00b7ginnt", "ein", "Mor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "\u00dcberall da knallt das Schie\u00dfgewehr,", "tokens": ["\u00dc\u00b7be\u00b7rall", "da", "knallt", "das", "Schie\u00df\u00b7ge\u00b7wehr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Rar geworden sind die Papageien,", "tokens": ["Rar", "ge\u00b7wor\u00b7den", "sind", "die", "Pa\u00b7pa\u00b7gei\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAPP", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Kolibris, die gibt's schon gar nicht mehr.", "tokens": ["Ko\u00b7lib\u00b7ris", ",", "die", "gibt's", "schon", "gar", "nicht", "mehr", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "VVFIN", "ADV", "ADV", "PTKNEG", "ADV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Einen bessern Piepmatz sich zu leisten,", "tokens": ["Ei\u00b7nen", "bes\u00b7sern", "Pie\u00b7pmatz", "sich", "zu", "leis\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PRF", "PTKZU", "VVINF", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Ach, der Mittelstand, der kann es nicht,", "tokens": ["Ach", ",", "der", "Mit\u00b7tel\u00b7stand", ",", "der", "kann", "es", "nicht", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ART", "NN", "$,", "PRELS", "VMFIN", "PPER", "PTKNEG", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Aber einen Vogel mu\u00df man haben,", "tokens": ["A\u00b7ber", "ei\u00b7nen", "Vo\u00b7gel", "mu\u00df", "man", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NE", "VMFIN", "PIS", "VAFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und so nimmt man eben, was man kriegt.", "tokens": ["Und", "so", "nimmt", "man", "e\u00b7ben", ",", "was", "man", "kriegt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PIS", "ADV", "$,", "PRELS", "PIS", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "\u00bbnein, die Preise sind nicht zu bezahlen,\u00ab", "tokens": ["\u00bb", "nein", ",", "die", "Prei\u00b7se", "sind", "nicht", "zu", "be\u00b7zah\u00b7len", ",", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PTKANT", "$,", "ART", "NN", "VAFIN", "PTKNEG", "PTKZU", "VVINF", "$,", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Sagt die Hausfrau, \u00bbwas ist da zu tun?", "tokens": ["Sagt", "die", "Haus\u00b7frau", ",", "\u00bb", "was", "ist", "da", "zu", "tun", "?"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "$(", "PWS", "VAFIN", "ADV", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "F\u00fcr die M\u00e4dchen nehm' ich tote Spatzen", "tokens": ["F\u00fcr", "die", "M\u00e4d\u00b7chen", "nehm'", "ich", "to\u00b7te", "Spat\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und f\u00fcr mich das olle Legehuhn!\u00ab", "tokens": ["Und", "f\u00fcr", "mich", "das", "ol\u00b7le", "Le\u00b7ge\u00b7huhn", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "APPR", "PPER", "ART", "ADJA", "NN", "