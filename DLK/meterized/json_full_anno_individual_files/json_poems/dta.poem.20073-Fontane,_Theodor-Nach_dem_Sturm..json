{"dta.poem.20073": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Nach dem Sturm.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1851", "urn": "urn:nbn:de:kobv:b4-200905191321", "language": ["de:0.99"], "booktitle": "Fontane, Theodor: Gedichte. Berlin, 1851."}, "poem": {"stanza.1": {"line.1": {"text": "O frage nicht warum noch itzt,", "tokens": ["O", "fra\u00b7ge", "nicht", "wa\u00b7rum", "noch", "itzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PTKNEG", "PWAV", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wo mir des Gl\u00fcckes Sonne leuchtet,", "tokens": ["Wo", "mir", "des", "Gl\u00fc\u00b7ckes", "Son\u00b7ne", "leuch\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der Gram auf meiner Stirne sitzt,", "tokens": ["Der", "Gram", "auf", "mei\u00b7ner", "Stir\u00b7ne", "sitzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und oftmals mir das Auge feuchtet.", "tokens": ["Und", "oft\u00b7mals", "mir", "das", "Au\u00b7ge", "feuch\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Sahst Du das Meer? hoch th\u00fcrmen dort", "tokens": ["Sahst", "Du", "das", "Meer", "?", "hoch", "th\u00fcr\u00b7men", "dort"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "$.", "ADJD", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auch nach dem Sturm sich noch die Wogen;", "tokens": ["Auch", "nach", "dem", "Sturm", "sich", "noch", "die", "Wo\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "PRF", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die B\u00e4ume schau: sie tropfen fort,", "tokens": ["Die", "B\u00e4u\u00b7me", "schau", ":", "sie", "trop\u00b7fen", "fort", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$.", "PPER", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn l\u00e4ngst der Regen weggezogen.", "tokens": ["Wenn", "l\u00e4ngst", "der", "Re\u00b7gen", "weg\u00b7ge\u00b7zo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}