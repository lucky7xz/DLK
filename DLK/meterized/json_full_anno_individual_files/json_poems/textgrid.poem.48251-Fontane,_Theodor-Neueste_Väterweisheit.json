{"textgrid.poem.48251": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Neueste V\u00e4terweisheit", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zieh nun also in die Welt,", "tokens": ["Zieh", "nun", "al\u00b7so", "in", "die", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Tue beharrlich, was dir gef\u00e4llt,", "tokens": ["Tue", "be\u00b7harr\u00b7lich", ",", "was", "dir", "ge\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "$,", "PWS", "PPER", "VVPP", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Werde keiner Gef\u00fchle Beute,", "tokens": ["Wer\u00b7de", "kei\u00b7ner", "Ge\u00b7f\u00fch\u00b7le", "Beu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Meide sorglich arme Leute,", "tokens": ["Mei\u00b7de", "sorg\u00b7lich", "ar\u00b7me", "Leu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Werde kein gelehrter Klauber,", "tokens": ["Wer\u00b7de", "kein", "ge\u00b7lehr\u00b7ter", "Klau\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wissenschaft ist fauler Zauber,", "tokens": ["Wis\u00b7sen\u00b7schaft", "ist", "fau\u00b7ler", "Zau\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "punct"], 