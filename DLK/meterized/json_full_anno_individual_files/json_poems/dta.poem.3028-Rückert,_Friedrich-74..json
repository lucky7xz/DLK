{"dta.poem.3028": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "74.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Voll G\u00f6tter ist die Welt, die alle sind zusammen", "tokens": ["Voll", "G\u00f6t\u00b7ter", "ist", "die", "Welt", ",", "die", "al\u00b7le", "sind", "zu\u00b7sam\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "NN", "VAFIN", "ART", "NN", "$,", "PRELS", "PIS", "VAFIN", "ADV"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein G\u00f6ttliches, daraus, darein zur\u00fcck sie schwammen.", "tokens": ["Ein", "G\u00f6tt\u00b7li\u00b7ches", ",", "da\u00b7raus", ",", "da\u00b7rein", "zu\u00b7r\u00fcck", "sie", "schwam\u00b7men", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PAV", "$,", "PAV", "PTKVZ", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Und wem die Sinne sind von ihrer Gunst erschlossen,", "tokens": ["Und", "wem", "die", "Sin\u00b7ne", "sind", "von", "ih\u00b7rer", "Gunst", "er\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "VAFIN", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ist \u00fcberall umweht von ihnen und umflossen.", "tokens": ["Ist", "\u00fc\u00b7be\u00b7rall", "um\u00b7weht", "von", "ih\u00b7nen", "und", "um\u00b7flos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "VVFIN", "APPR", "PPER", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wer achtet ihren Wink, und auf ihr Zeichen merkt,", "tokens": ["Wer", "ach\u00b7tet", "ih\u00b7ren", "Wink", ",", "und", "auf", "ihr", "Zei\u00b7chen", "merkt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPOSAT", "NN", "$,", "KON", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "F\u00fchlt sich auf jeder Bahn gef\u00f6rdert und gest\u00e4rkt.", "tokens": ["F\u00fchlt", "sich", "auf", "je\u00b7der", "Bahn", "ge\u00b7f\u00f6r\u00b7dert", "und", "ge\u00b7st\u00e4rkt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PIAT", "NN", "VVPP", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Und wer entgegen ihrem Willen seinen stemmt,", "tokens": ["Und", "wer", "ent\u00b7ge\u00b7gen", "ih\u00b7rem", "Wil\u00b7len", "sei\u00b7nen", "stemmt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "APPR", "PPOSAT", "NN", "PPOSAT", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "F\u00fchlt sich in jedem Werk gehindert und gehemmt.", "tokens": ["F\u00fchlt", "sich", "in", "je\u00b7dem", "Werk", "ge\u00b7hin\u00b7dert", "und", "ge\u00b7hemmt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PIAT", "NN", "VVPP", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}