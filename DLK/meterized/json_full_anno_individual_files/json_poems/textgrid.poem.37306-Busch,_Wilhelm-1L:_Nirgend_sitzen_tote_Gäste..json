{"textgrid.poem.37306": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nirgend sitzen tote G\u00e4ste.", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nirgend sitzen tote G\u00e4ste.", "tokens": ["Nir\u00b7gend", "sit\u00b7zen", "to\u00b7te", "G\u00e4s\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Allerorten lebt die Kraft.", "tokens": ["Al\u00b7le\u00b7ror\u00b7ten", "lebt", "die", "Kraft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ist nicht selbst der Fels, der feste,", "tokens": ["Ist", "nicht", "selbst", "der", "Fels", ",", "der", "fes\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "ADV", "ART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Eine Kraftgenossenschaft?", "tokens": ["Ei\u00b7ne", "Kraft\u00b7ge\u00b7nos\u00b7sen\u00b7schaft", "?"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Durch und durch aus Eigenheiten,", "tokens": ["Durch", "und", "durch", "aus", "Ei\u00b7gen\u00b7hei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KON", "APPR", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "So und so zu sein bestrebt,", "tokens": ["So", "und", "so", "zu", "sein", "be\u00b7strebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KON", "ADV", "PTKZU", "VAINF", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Die sich lieben, die sich streiten,", "tokens": ["Die", "sich", "lie\u00b7ben", ",", "die", "sich", "strei\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "VVINF", "$,", "PRELS", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wird die bunte Welt gewebt.", "tokens": ["Wird", "die", "bun\u00b7te", "Welt", "ge\u00b7webt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Hier gelingt es, da mi\u00dfgl\u00fcckt es.", "tokens": ["Hier", "ge\u00b7lingt", "es", ",", "da", "mi\u00df\u00b7gl\u00fcckt", "es", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ADV", "VVFIN", "PPER", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "W\u00fcnsche finden keine Rast.", "tokens": ["W\u00fcn\u00b7sche", "fin\u00b7den", "kei\u00b7ne", "Rast", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Unterdr\u00fccker, Unterdr\u00fccktes,", "tokens": ["Un\u00b7ter\u00b7dr\u00fc\u00b7cker", ",", "Un\u00b7ter\u00b7dr\u00fcck\u00b7tes", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Jedes Ding hat seine Last.", "tokens": ["Je\u00b7des", "Ding", "hat", "sei\u00b7ne", "Last", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line