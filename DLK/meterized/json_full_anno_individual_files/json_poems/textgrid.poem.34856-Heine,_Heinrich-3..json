ord", "punct"], "pos": ["NN", "$,", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Wandelst du durch die Gem\u00e4cher.", "tokens": ["Wan\u00b7delst", "du", "durch", "die", "Ge\u00b7m\u00e4\u00b7cher", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Belastet mit Diamanten", "tokens": ["Be\u00b7las\u00b7tet", "mit", "Di\u00b7a\u00b7man\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "APPR", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und Perlen und Br\u00fcsseler Kanten,", "tokens": ["Und", "Per\u00b7len", "und", "Br\u00fcs\u00b7se\u00b7ler", "Kan\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Dein wei\u00dfer Busen schwellet", "tokens": ["Dein", "wei\u00b7\u00dfer", "Bu\u00b7sen", "schwel\u00b7let"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und freudig \u00fcberquellet.", "tokens": ["Und", "freu\u00b7dig", "\u00fc\u00b7berq\u00b7uel\u00b7let", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Das ist ein L\u00e4cheln und Nicken", "tokens": ["Das", "ist", "ein", "L\u00e4\u00b7cheln", "und", "Ni\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "NN", "KON", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Und Knicksen und tiefes B\u00fccken!", "tokens": ["Und", "Knick\u00b7sen", "und", "tie\u00b7fes", "B\u00fc\u00b7cken", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "Die Herzogin von Pavia,", "tokens": ["Die", "Her\u00b7zo\u00b7gin", "von", "Pa\u00b7via", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.10": {"text": "Die nennt dich: \u00bbCara mia.\u00ab", "tokens": ["Die", "nennt", "dich", ":", "\u00bb", "Ca\u00b7ra", "mi\u00b7a.", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct", "word", "abbreviation", "punct"], "pos": ["PDS", "VVFIN", "PPER", "$.", "$(", "FM.la", "FM.la", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.11": {"text": "Die Junker und die Schranzen,", "tokens": ["Die", "Jun\u00b7ker", "und", "die", "Schran\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Die wollen mit dir tanzen;", "tokens": ["Die", "wol\u00b7len", "mit", "dir", "tan\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Und der Krone witziger Erbe", "tokens": ["Und", "der", "Kro\u00b7ne", "wit\u00b7zi\u00b7ger", "Er\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJA", "NN"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.14": {"text": "Ruft laut im Saal: \u00bbS\u00fcperbe", "tokens": ["Ruft", "laut", "im", "Saal", ":", "\u00bb", "S\u00fc\u00b7per\u00b7be"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word"], "pos": ["VVFIN", "ADJD", "APPRART", "NN", "$.", "$(", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.15": {"text": "Schwingt sie 