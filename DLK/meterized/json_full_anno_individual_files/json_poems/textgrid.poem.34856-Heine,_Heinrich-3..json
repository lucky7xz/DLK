{"textgrid.poem.34856": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Gr\u00e4fin Gudel von Gudelfeld,", "tokens": ["O", "Gr\u00e4\u00b7fin", "Gu\u00b7del", "von", "Gu\u00b7del\u00b7feld", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "NE", "APPR", "NE", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Dir huldigt die Menschheit, denn du hast Geld!", "tokens": ["Dir", "hul\u00b7digt", "die", "Menschheit", ",", "denn", "du", "hast", "Geld", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "KON", "PPER", "VAFIN", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Du wirst mit vieren kutschieren,", "tokens": ["Du", "wirst", "mit", "vie\u00b7ren", "kut\u00b7schie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Man wird dich bei Hof pr\u00e4sentieren.", "tokens": ["Man", "wird", "dich", "bei", "Hof", "pr\u00e4\u00b7sen\u00b7tie\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PRF", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Es tr\u00e4gt dich die goldne Karosse", "tokens": ["Es", "tr\u00e4gt", "dich", "die", "gold\u00b7ne", "Ka\u00b7ros\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Zum kerzenschi