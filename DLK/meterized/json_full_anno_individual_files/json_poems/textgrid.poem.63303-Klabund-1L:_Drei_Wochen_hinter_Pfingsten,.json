{"textgrid.poem.63303": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "1L: Drei Wochen hinter Pfingsten,", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Drei Wochen hinter Pfingsten,", "tokens": ["Drei", "Wo\u00b7chen", "hin\u00b7ter", "Pfings\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da traf ich einen Mann,", "tokens": ["Da", "traf", "ich", "ei\u00b7nen", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der nahm mich ohne den geringsten", "tokens": ["Der", "nahm", "mich", "oh\u00b7ne", "den", "ge\u00b7rings\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PRF", "APPR", "ART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Einwand als Wirtschafterin an.", "tokens": ["Ein\u00b7wand", "als", "Wirt\u00b7schaf\u00b7te\u00b7rin", "an", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "KOKOM", "NN", "PTKVZ", "$."], "meter": "-+-+---+", "measure": "unknown.measure.tri"}}, "stanza.2": {"line.1": {"text": "Ich hab' ihm die Suppe versalzen", "tokens": ["Ich", "hab'", "ihm", "die", "Sup\u00b7pe", "ver\u00b7sal\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "ART", "NN", "VVINF"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Und auch die Sommerzeit,", "tokens": ["Und", "auch", "die", "Som\u00b7mer\u00b7zeit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+", "m