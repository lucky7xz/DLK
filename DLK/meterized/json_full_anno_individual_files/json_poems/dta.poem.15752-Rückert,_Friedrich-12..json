{"dta.poem.15752": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "12.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es ist nur Eitelkeit, wenn du dir vorgenommen,", "tokens": ["Es", "ist", "nur", "Ei\u00b7tel\u00b7keit", ",", "wenn", "du", "dir", "vor\u00b7ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "NN", "$,", "KOUS", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mein Freund, da\u00df, was du schreibst, sei alles ganz vollkommen.", "tokens": ["Mein", "Freund", ",", "da\u00df", ",", "was", "du", "schreibst", ",", "sei", "al\u00b7les", "ganz", "voll\u00b7kom\u00b7men", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "KOUS", "$,", "PWS", "PPER", "VVFIN", "$,", "VAFIN", "PIS", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die leichte T\u00e4ndelei ist nicht der M\u00fche werth,", "tokens": ["Die", "leich\u00b7te", "T\u00e4n\u00b7de\u00b7lei", "ist", "nicht", "der", "M\u00fc\u00b7he", "werth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PTKNEG", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und minder noch die Welt, die solchen Schmuck begehrt.", "tokens": ["Und", "min\u00b7der", "noch", "die", "Welt", ",", "die", "sol\u00b7chen", "Schmuck", "be\u00b7gehrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ART", "NN", "$,", "PRELS", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Sag' ihr, der Welt, eh sie Vollkommenheit verlange", "tokens": ["Sag'", "ihr", ",", "der", "Welt", ",", "eh", "sie", "Voll\u00b7kom\u00b7men\u00b7heit", "ver\u00b7lan\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "ART", "NN", "$,", "KOUS", "PPER", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von uns, da\u00df sie erst selbst Vollkommenheit erlange!", "tokens": ["Von", "uns", ",", "da\u00df", "sie", "erst", "selbst", "Voll\u00b7kom\u00b7men\u00b7heit", "er\u00b7lan\u00b7ge", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "KOUS", "PPER", "ADV", "ADV", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}