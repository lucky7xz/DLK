{"dta.poem.18777": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang von", "birth": "N.A.", "death": "N.A."}, "title": "Gingo biloba .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1819", "urn": "urn:nbn:de:kobv:b4-200905191692", "language": ["de:0.99"], "booktitle": "Goethe, Johann Wolfgang von: West-\u00f6stlicher Divan. Stuttgart, 1819."}, "poem": {"stanza.1": {"line.1": {"text": "Dieses Baum\u2019s Blatt, der von Osten", "tokens": ["Die\u00b7ses", "Baum's", "Blatt", ",", "der", "von", "Os\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDAT", "NN", "NN", "$,", "PRELS", "APPR", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Meinem Garten anvertraut,", "tokens": ["Mei\u00b7nem", "Gar\u00b7ten", "an\u00b7ver\u00b7traut", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Giebt geheimen Sinn zu kosten,", "tokens": ["Giebt", "ge\u00b7hei\u00b7men", "Sinn", "zu", "kos\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie\u2019s den Wissenden erbaut.", "tokens": ["Wie's", "den", "Wis\u00b7sen\u00b7den", "er\u00b7baut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ist es Ein lebendig Wesen?", "tokens": ["Ist", "es", "Ein", "le\u00b7ben\u00b7dig", "We\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJD", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Das sich in sich selbst getrennt,", "tokens": ["Das", "sich", "in", "sich", "selbst", "ge\u00b7trennt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PRF", "APPR", "PRF", "ADV", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sind es zwey? die sich erlesen,", "tokens": ["Sind", "es", "zwey", "?", "die", "sich", "er\u00b7le\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "CARD", "$.", "PRELS", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dass man sie als eines kennt.", "tokens": ["Dass", "man", "sie", "als", "ei\u00b7nes", "kennt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "KOUS", "PIS", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Solche Frage zu erwiedern", "tokens": ["Sol\u00b7che", "Fra\u00b7ge", "zu", "er\u00b7wie\u00b7dern"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "NN", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Fand ich wohl den rechten Sinn;", "tokens": ["Fand", "ich", "wohl", "den", "rech\u00b7ten", "Sinn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "F\u00fchlst du nicht an meinen Liedern", "tokens": ["F\u00fchlst", "du", "nicht", "an", "mei\u00b7nen", "Lie\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKNEG", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dass ich Eins und doppelt bin?", "tokens": ["Dass", "ich", "Eins", "und", "dop\u00b7pelt", "bin", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "KON", "ADJD", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}