{"dta.poem.12487": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Die Ruthe   .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1843", "urn": "urn:nbn:de:kobv:b4-200905192440", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Kaum geht im deutschen Land ein Riegel,", "tokens": ["Kaum", "geht", "im", "deut\u00b7schen", "Land", "ein", "Rie\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Schlo\u00df und eine Kette los:", "tokens": ["Ein", "Schlo\u00df", "und", "ei\u00b7ne", "Ket\u00b7te", "los", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So steckt man hinter unsres Rheines Spiegel", "tokens": ["So", "steckt", "man", "hin\u00b7ter", "uns\u00b7res", "Rhei\u00b7nes", "Spie\u00b7gel"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Geschwind als Ruthe den Franzos!", "tokens": ["Ge\u00b7schwind", "als", "Ru\u00b7the", "den", "Fran\u00b7zos", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOUS", "NN", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Und du, mein Volk, du glaubst den M\u00e4ren", "tokens": ["Und", "du", ",", "mein", "Volk", ",", "du", "glaubst", "den", "M\u00e4\u00b7ren"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPER", "$,", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und dein Verstand ergreift die Flucht,", "tokens": ["Und", "dein", "Ver\u00b7stand", "er\u00b7greift", "die", "Flucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du rupfst den ", "tokens": ["Du", "rupfst", "den"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Den man dir ", "tokens": ["Den", "man", "dir"], "token_info": ["word", "word", "word"], "pos": ["ART", "PIS", "PPER"], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.3": {"line.1": {"text": "Du rupfst den Hahn, inde\u00df der ", "tokens": ["Du", "rupfst", "den", "Hahn", ",", "in\u00b7de\u00df", "der"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ADV", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dir tief in deine Leber fri\u00dft:", "tokens": ["Dir", "tief", "in", "dei\u00b7ne", "Le\u00b7ber", "fri\u00dft", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du tr\u00e4umst von Einheit, und du glaubst dich freier,", "tokens": ["Du", "tr\u00e4umst", "von", "Ein\u00b7heit", ",", "und", "du", "glaubst", "dich", "frei\u00b7er", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$,", "KON", "PPER", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wenn dein Gef\u00e4ngni\u00df gr\u00f6\u00dfer ist.", "tokens": ["Wenn", "dein", "Ge\u00b7f\u00e4ng\u00b7ni\u00df", "gr\u00f6\u00b7\u00dfer", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Du n\u00e4hst dir an die wei\u00dfe M\u00fctze", "tokens": ["Du", "n\u00e4hst", "dir", "an", "die", "wei\u00b7\u00dfe", "M\u00fct\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Schellen der Philosophie", "tokens": ["Die", "Schel\u00b7len", "der", "Phi\u00b7lo\u00b7so\u00b7phie"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und folgst dem Irrlicht, klingend, in die Pf\u00fctze", "tokens": ["Und", "folgst", "dem", "Irr\u00b7licht", ",", "klin\u00b7gend", ",", "in", "die", "Pf\u00fct\u00b7ze"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "$,", "VVPP", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Obskuranten-Kompagnie!", "tokens": ["Der", "Ob\u00b7sku\u00b7ran\u00b7ten\u00b7Kom\u00b7pa\u00b7gnie", "!"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "O \u201eEckstein Nationen!\u201c", "tokens": ["O", "\u201e", "E\u00b7ckstein", "Na\u00b7ti\u00b7o\u00b7nen", "!", "\u201c"], "token_info": ["word", "punct", "word", "word", "punct", "punct"], "pos": ["NE", "$(", "PPOSAT", "NN", "$.", "$("], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Drum .... .... ..... .... dich an \u2014", "tokens": ["Drum", "....", "....", ".....", "....", "dich", "an"], "token_info": ["word", "punct", "punct", "punct", "punct", "word", "word", "punct"], "pos": ["PAV", "$.", "$(", "$(", "$.", "PPER", "PTKVZ", "$("], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "O ", "tokens": ["O"], "token_info": ["word"], "pos": ["NE"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Das 30 ", "tokens": ["Das", "30"], "token_info": ["word", "number"], "pos": ["ART", "CARD"], "meter": "-", "measure": "single.down"}}}}}