{"dta.poem.2873": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "74.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie \u00fcbel ihr vergleicht! des Einen Wirklichkeit,", "tokens": ["Wie", "\u00fc\u00b7bel", "ihr", "ver\u00b7gleicht", "!", "des", "Ei\u00b7nen", "Wirk\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "VVFIN", "$.", "ART", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Des Andern Ideal, die Kluft ist freilich weit.", "tokens": ["Des", "An\u00b7dern", "I\u00b7deal", ",", "die", "Kluft", "ist", "frei\u00b7lich", "weit", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Den Wuchs nicht wie er ist, doch sollt' und k\u00f6nnte seyn,", "tokens": ["Den", "Wuchs", "nicht", "wie", "er", "ist", ",", "doch", "sollt'", "und", "k\u00f6nn\u00b7te", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "PWAV", "PPER", "VAFIN", "$,", "ADV", "VMFIN", "KON", "VMFIN", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bringt ihr in Anschlag hier, Ausw\u00fcchse dort allein.", "tokens": ["Bringt", "ihr", "in", "An\u00b7schlag", "hier", ",", "Aus\u00b7w\u00fcch\u00b7se", "dort", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "ADV", "$,", "NN", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Die Todtenaschen dort, und hier die Lebensflammen;", "tokens": ["Die", "Tod\u00b7ten\u00b7asc\u00b7hen", "dort", ",", "und", "hier", "die", "Le\u00b7bens\u00b7flam\u00b7men", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "KON", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da k\u00f6nnt ihr freilich leicht hier preisen, dort verdammen.", "tokens": ["Da", "k\u00f6nnt", "ihr", "frei\u00b7lich", "leicht", "hier", "prei\u00b7sen", ",", "dort", "ver\u00b7dam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADJD", "ADV", "VVFIN", "$,", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "La\u00dft sehn, ob nicht die Glut sich auch in Asche legt,", "tokens": ["La\u00dft", "sehn", ",", "ob", "nicht", "die", "Glut", "sich", "auch", "in", "A\u00b7sche", "legt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "VVINF", "$,", "KOUS", "PTKNEG", "ART", "NN", "PRF", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und ob die Asche nicht noch einen Funken hegt!", "tokens": ["Und", "ob", "die", "A\u00b7sche", "nicht", "noch", "ei\u00b7nen", "Fun\u00b7ken", "hegt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "PTKNEG", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}