{"dta.poem.4441": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der unvern\u00fcnftige Zweifel.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1727", "urn": "urn:nbn:de:kobv:b4-200905198599", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "GoTT, der Himmel und die H\u00f6lle sind vielleicht nur", "tokens": ["GoTT", ",", "der", "Him\u00b7mel", "und", "die", "H\u00f6l\u00b7le", "sind", "viel\u00b7leicht", "nur"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "ART", "NN", "KON", "ART", "NN", "VAFIN", "ADV", "ADV"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "eit'le Grillen.", "tokens": ["eit'\u00b7le", "Gril\u00b7len", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "--+-", "measure": "anapaest.init"}, "line.3": {"text": "Ist es m\u00f6glich, da\u00df ein Mensch bey dem Zweifel ruhig bleibt?", "tokens": ["Ist", "es", "m\u00f6g\u00b7lich", ",", "da\u00df", "ein", "Mensch", "bey", "dem", "Zwei\u00b7fel", "ru\u00b7hig", "bleibt", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "$,", "KOUS", "ART", "NN", "APPR", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Untersuch\u2019s! Sonst wird zu sp\u00e4t, wenn sie deinen Wahn", "tokens": ["Un\u00b7ter\u00b7such's", "!", "Sonst", "wird", "zu", "sp\u00e4t", ",", "wenn", "sie", "dei\u00b7nen", "Wahn"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$.", "ADV", "VAFIN", "PTKA", "ADJD", "$,", "KOUS", "PPER", "PPOSAT", "NN"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.5": {"text": "vertreibt,", "tokens": ["ver\u00b7treibt", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.6": {"text": "Die nur gar zu grause Wahrheit dich mit Angst und Qual", "tokens": ["Die", "nur", "gar", "zu", "grau\u00b7se", "Wahr\u00b7heit", "dich", "mit", "Angst", "und", "Qual"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ADV", "APPR", "ADJA", "NN", "PRF", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.7": {"text": "erf\u00fcllen.", "tokens": ["er\u00b7f\u00fcl\u00b7len", "."], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "-+-", "measure": "amphibrach.single"}}}}}