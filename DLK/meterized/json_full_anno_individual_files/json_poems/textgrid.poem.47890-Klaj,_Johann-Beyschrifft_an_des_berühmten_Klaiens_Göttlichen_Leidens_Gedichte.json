{"textgrid.poem.47890": {"metadata": {"author": {"name": "Klaj, Johann", "birth": "N.A.", "death": "N.A."}, "title": "Beyschrifft an des ber\u00fchmten Klaiens G\u00f6ttlichen Leidens Gedichte", "genre": "verse", "period": "N.A.", "pub_year": 1636, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hastu nicht/ mein werther Freund/", "tokens": ["Has\u00b7tu", "nicht", "/", "mein", "wert\u00b7her", "Freund", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "$(", "PPOSAT", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Dein und meinen Seelenretter/", "tokens": ["Dein", "und", "mei\u00b7nen", "See\u00b7len\u00b7ret\u00b7ter", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "KON", "PPOSAT", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Als des Davids letzten Vetter/", "tokens": ["Als", "des", "Da\u00b7vids", "letz\u00b7ten", "Vet\u00b7ter", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NE", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "(eh ich dich zu kennen meint)", "tokens": ["(", "eh", "ich", "dich", "zu", "ken\u00b7nen", "meint", ")"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PPER", "PRF", "PTKZU", "VVINF", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Angesungen in der Wiegen", "tokens": ["An\u00b7ge\u00b7sun\u00b7gen", "in", "der", "Wie\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Vnd gelobet sein Verm\u00fcgen?", "tokens": ["Vnd", "ge\u00b7lo\u00b7bet", "sein", "Ver\u00b7m\u00fc\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Du warst mit ihm in dem Stall/", "tokens": ["Du", "warst", "mit", "ihm", "in", "dem", "Stall", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "APPR", "ART", "NN", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Da der Ochs und M\u00fchlendiener", "tokens": ["Da", "der", "Ochs", "und", "M\u00fch\u00b7len\u00b7die\u00b7ner"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Standen bey dem Weltvers\u00fchner;", "tokens": ["Stan\u00b7den", "bey", "dem", "Welt\u00b7ver\u00b7s\u00fch\u00b7ner", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da der ekkte Feuerpall", "tokens": ["Da", "der", "ekk\u00b7te", "Feu\u00b7er\u00b7pall"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.5": {"text": "Vnter Gottesburg erschiene/", "tokens": ["Vn\u00b7ter", "Got\u00b7tes\u00b7burg", "er\u00b7schie\u00b7ne", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADJA", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Heyden auch zu machen k\u00fchne.", "tokens": ["Hey\u00b7den", "auch", "zu", "ma\u00b7chen", "k\u00fch\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Da der Morgenl\u00e4nder Cron", "tokens": ["Da", "der", "Mor\u00b7gen\u00b7l\u00e4n\u00b7der", "Cron"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Vnd die Wei\u00dfheit ihrer St\u00e4dte", "tokens": ["Vnd", "die", "Wei\u00df\u00b7heit", "ih\u00b7rer", "St\u00e4d\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ihre grosse Reise th\u00e4te", "tokens": ["Ih\u00b7re", "gros\u00b7se", "Rei\u00b7se", "th\u00e4\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bis zu Gottes-Jungfer-Sohn;", "tokens": ["Bis", "zu", "Got\u00b7tes\u00b7Jung\u00b7fer\u00b7Sohn", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Da die Heydenschafft mit Gaben", "tokens": ["Da", "die", "Hey\u00b7den\u00b7schafft", "mit", "Ga\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Auch Erl\u00f6sung wolten haben.", "tokens": ["Auch", "Er\u00b7l\u00f6\u00b7sung", "wol\u00b7ten", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VMFIN", "VAINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Dir ist auch nicht unbekand", "tokens": ["Dir", "ist", "auch", "nicht", "un\u00b7be\u00b7kand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "PTKNEG", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Vnsers Kindes sein Elende/", "tokens": ["Vn\u00b7sers", "Kin\u00b7des", "sein", "E\u00b7len\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Als es auf der Flucht behende", "tokens": ["Als", "es", "auf", "der", "Flucht", "be\u00b7hen\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Zog hin in der Heyden Land/", "tokens": ["Zog", "hin", "in", "der", "Hey\u00b7den", "Land", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ART", "ADJA", "NN", "$("], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.5": {"text": "Ascalons des wilden Heyden", "tokens": ["A\u00b7sca\u00b7lons", "des", "wil\u00b7den", "Hey\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sein Erw\u00fcrgen zu vermeiden.", "tokens": ["Sein", "Er\u00b7w\u00fcr\u00b7gen", "zu", "ver\u00b7mei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Hastu nicht den Teuffelsman/", "tokens": ["Has\u00b7tu", "nicht", "den", "Teuf\u00b7fels\u00b7man", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "ART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Den ungleichen Welttyrannen", "tokens": ["Den", "un\u00b7glei\u00b7chen", "Welt\u00b7ty\u00b7ran\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Vnd das herbe Weiberzannen", "tokens": ["Vnd", "das", "her\u00b7be", "Wei\u00b7ber\u00b7zan\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Greulich mit gef\u00fchret an;", "tokens": ["Greu\u00b7lich", "mit", "ge\u00b7f\u00fch\u00b7ret", "an", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "VVPP", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Seinen Hertzensschmertz und Qwelen", "tokens": ["Sei\u00b7nen", "Hert\u00b7zens\u00b7schmertz", "und", "Qwe\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Weistu alles zu erzehlen.", "tokens": ["Weis\u00b7tu", "al\u00b7les", "zu", "er\u00b7zeh\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Du l\u00e4st ferner doch nicht seyn", "tokens": ["Du", "l\u00e4st", "fer\u00b7ner", "doch", "nicht", "seyn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "PTKNEG", "VAINF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Hin zu Gottes eignen Mauren", "tokens": ["Hin", "zu", "Got\u00b7tes", "eig\u00b7nen", "Mau\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Eine Reise mit zu dauren", "tokens": ["Ei\u00b7ne", "Rei\u00b7se", "mit", "zu", "dau\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In dem zw\u00f6lfften Jahresschein;", "tokens": ["In", "dem", "zw\u00f6lff\u00b7ten", "Jah\u00b7res\u00b7schein", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Du wirst von dem jungen Munde", "tokens": ["Du", "wirst", "von", "dem", "jun\u00b7gen", "Mun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Reden eine gute Stunde.", "tokens": ["Re\u00b7den", "ei\u00b7ne", "gu\u00b7te", "Stun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Salemsburg und Gotteshau\u00df/", "tokens": ["Sa\u00b7lems\u00b7burg", "und", "Got\u00b7tes\u00b7hau\u00df", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ihre Lehrer und die Lehre", "tokens": ["Ih\u00b7re", "Leh\u00b7rer", "und", "die", "Leh\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "KON", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Vnd der Hohenpriester Ehre", "tokens": ["Vnd", "der", "Ho\u00b7hen\u00b7pries\u00b7ter", "Eh\u00b7re"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Werden da mit brechen rau\u00df.", "tokens": ["Wer\u00b7den", "da", "mit", "bre\u00b7chen", "rau\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "ADJA", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Vnsers Mitlers Wanderschafften", "tokens": ["Vn\u00b7sers", "Mit\u00b7lers", "Wan\u00b7der\u00b7schaff\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Dir auch im Ged\u00e4chtni\u00df hafften.", "tokens": ["Dir", "auch", "im", "Ge\u00b7d\u00e4cht\u00b7ni\u00df", "haff\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Itzo fertigt deine Kiel", "tokens": ["It\u00b7zo", "fer\u00b7tigt", "dei\u00b7ne", "Kiel"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Vnsers Bruders rote Wunden;", "tokens": ["Vn\u00b7sers", "Bru\u00b7ders", "ro\u00b7te", "Wun\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Fessel/ wormit er gebunden/", "tokens": ["Fes\u00b7sel", "/", "wor\u00b7mit", "er", "ge\u00b7bun\u00b7den", "/"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$(", "PWAV", "PPER", "VVPP", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Vnd des Leidens allzuviel:", "tokens": ["Vnd", "des", "Lei\u00b7dens", "all\u00b7zu\u00b7viel", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PIAT", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Lauter blauvermengte Striemen", "tokens": ["Lau\u00b7ter", "blau\u00b7ver\u00b7meng\u00b7te", "Strie\u00b7men"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Von den Henkergeisselriemen.", "tokens": ["Von", "den", "Hen\u00b7ker\u00b7geis\u00b7sel\u00b7rie\u00b7men", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stan