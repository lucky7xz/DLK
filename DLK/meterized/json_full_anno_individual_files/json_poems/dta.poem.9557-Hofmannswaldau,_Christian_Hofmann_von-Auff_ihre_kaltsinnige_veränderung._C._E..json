{"dta.poem.9557": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auff ihre kaltsinnige ver\u00e4nderung.  \n C. E.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Melinde/ geht dein feur so zeitig schon zu end?", "tokens": ["Me\u00b7lin\u00b7de", "/", "geht", "dein", "feur", "so", "zei\u00b7tig", "schon", "zu", "end", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "VVFIN", "PPOSAT", "NN", "ADV", "ADJD", "ADV", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie geht di\u00df immer zu? kein funcke soll sich r\u00fchren/", "tokens": ["Wie", "geht", "di\u00df", "im\u00b7mer", "zu", "?", "kein", "fun\u00b7cke", "soll", "sich", "r\u00fch\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PDS", "ADV", "PTKVZ", "$.", "PIAT", "NN", "VMFIN", "PRF", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Du wilt nur schnee und ei\u00df in deinen adern f\u00fchren/", "tokens": ["Du", "wilt", "nur", "schnee", "und", "ei\u00df", "in", "dei\u00b7nen", "a\u00b7dern", "f\u00fch\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "VVFIN", "KON", "ADJD", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ists m\u00f6glich/ hat das blat so pl\u00f6tzlich sich gewendt?", "tokens": ["Ists", "m\u00f6g\u00b7lich", "/", "hat", "das", "blat", "so", "pl\u00f6tz\u00b7lich", "sich", "ge\u00b7wendt", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$(", "VAFIN", "ART", "NN", "ADV", "ADJD", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Doch was verwundr\u2019 ich mich? wenn man die flam\u0303en tren\u0303t/", "tokens": ["Doch", "was", "ver\u00b7wundr'", "ich", "mich", "?", "wenn", "man", "die", "flam\u0303en", "tre\u00f1t", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VVFIN", "PPER", "PRF", "$.", "KOUS", "PIS", "ART", "VVINF", "VVFIN", "$("], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "So mu\u00df nothwendig sich derselben krafft verliehren/", "tokens": ["So", "mu\u00df", "noth\u00b7wen\u00b7dig", "sich", "der\u00b7sel\u00b7ben", "krafft", "ver\u00b7lieh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ADJD", "PRF", "PDAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und kan man dieses nicht an deinem hertzen sp\u00fcren/", "tokens": ["Und", "kan", "man", "die\u00b7ses", "nicht", "an", "dei\u00b7nem", "hert\u00b7zen", "sp\u00fc\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PIS", "PDS", "PTKNEG", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Das in zertheilter gunst vor mich und jeden brennt?", "tokens": ["Das", "in", "zer\u00b7theil\u00b7ter", "gunst", "vor", "mich", "und", "je\u00b7den", "brennt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ADJA", "NN", "APPR", "PPER", "KON", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Noch gleichwohl soll ich dich als eine g\u00f6ttin ehren/", "tokens": ["Noch", "gleich\u00b7wohl", "soll", "ich", "dich", "als", "ei\u00b7ne", "g\u00f6t\u00b7tin", "eh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VMFIN", "PPER", "PRF", "KOUS", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und meine liebes-glut fast t\u00e4glich neu vermehren/", "tokens": ["Und", "mei\u00b7ne", "lie\u00b7bes\u00b7glut", "fast", "t\u00e4g\u00b7lich", "neu", "ver\u00b7meh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADV", "ADJD", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Verge\u00df ich einen blick/ so ists nicht recht gethan.", "tokens": ["Ver\u00b7ge\u00df", "ich", "ei\u00b7nen", "blick", "/", "so", "ists", "nicht", "recht", "ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$(", "ADV", "VAFIN", "PTKNEG", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Wie aber reimt sichs wohl: mein gantzes hertz begehren/", "tokens": ["Wie", "a\u00b7ber", "reimt", "sichs", "wohl", ":", "mein", "gant\u00b7zes", "hertz", "be\u00b7geh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VVFIN", "PIS", "ADV", "$.", "PPOSAT", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Und von dem deinen kaum den kleinsten theil gew\u00e4hren?", "tokens": ["Und", "von", "dem", "dei\u00b7nen", "kaum", "den", "kleins\u00b7ten", "theil", "ge\u00b7w\u00e4h\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "PPOSAT", "ADV", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "O nein! Melinde/ nein! so geht der tausch nicht an!", "tokens": ["O", "nein", "!", "Me\u00b7lin\u00b7de", "/", "nein", "!", "so", "geht", "der", "tausch", "nicht", "an", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKANT", "$.", "NE", "$(", "PTKANT", "$.", "ADV", "VVFIN", "ART", "ADJD", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}