{"dta.poem.13177": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Neuntes Buch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Da dieses theure Paar zugleich herein gegangen,", "tokens": ["Da", "die\u00b7ses", "theu\u00b7re", "Paar", "zu\u00b7gleich", "her\u00b7ein", "ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "ADJA", "NN", "ADV", "PTKVZ", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hat man im Saal f\u00fcr Lust zu murmeln angefangen;", "tokens": ["Hat", "man", "im", "Saal", "f\u00fcr", "Lust", "zu", "mur\u00b7meln", "an\u00b7ge\u00b7fan\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "APPRART", "NN", "APPR", "NN", "PTKZU", "VVINF", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Noch mehr, als gleich darauf die kleine Reihe kam,", "tokens": ["Noch", "mehr", ",", "als", "gleich", "da\u00b7rauf", "die", "klei\u00b7ne", "Rei\u00b7he", "kam", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "KOUS", "ADV", "PAV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "450Die der Anwesenheit Erblickung an sich nahm.", "tokens": ["der", "An\u00b7we\u00b7sen\u00b7heit", "Er\u00b7bli\u00b7ckung", "an", "sich", "nahm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPR", "PRF", "VVFIN", "$."], "meter": "-++-+-+-+-+", "measure": "unknown.measure.hexa"}, "line.5": {"text": "Ein ungef\u00e4hrer Trieb, ein schnelles Herz-Erg\u00f6zen", "tokens": ["Ein", "un\u00b7ge\u00b7f\u00e4h\u00b7rer", "Trieb", ",", "ein", "schnel\u00b7les", "Her\u00b7zEr\u00b7g\u00f6zen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Schien aller Sinnen Macht in neuen Trost zu sezen.", "tokens": ["Schien", "al\u00b7ler", "Sin\u00b7nen", "Macht", "in", "neu\u00b7en", "Trost", "zu", "se\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "NN", "APPR", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "O zarter Bluhmen-Krei\u00df! O Anmuths-reicher Flor!", "tokens": ["O", "zar\u00b7ter", "Bluh\u00b7men\u00b7Krei\u00df", "!", "O", "An\u00b7muths\u00b7rei\u00b7cher", "Flor", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$.", "NE", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Drey kleine ", "tokens": ["Drey", "klei\u00b7ne"], "token_info": ["word", "word"], "pos": ["CARD", "ADJA"], "meter": "-+-", "measure": "amphibrach.single"}, "line.9": {"text": "455Sie f\u00fchrten Wechsels-wei\u00df einander bey den H\u00e4nden,", "tokens": ["f\u00fchr\u00b7ten", "Wech\u00b7sels\u00b7wei\u00df", "ein\u00b7an\u00b7der", "bey", "den", "H\u00e4n\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.10": {"text": "Ohn ihrer Augen-Winck, als reizend, zu verwenden.", "tokens": ["Ohn", "ih\u00b7rer", "Au\u00b7gen\u00b7Winck", ",", "als", "rei\u00b7zend", ",", "zu", "ver\u00b7wen\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "KOUS", "VVPP", "$,", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Wann Flei\u00df, Natur und Kunst ein Garten-Bethe ziert,", "tokens": ["Wann", "Flei\u00df", ",", "Na\u00b7tur", "und", "Kunst", "ein", "Gar\u00b7ten\u00b7Be\u00b7the", "ziert", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "$,", "NN", "KON", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Da\u00df es ein holder Stoff von tausend Bluhmen wird;", "tokens": ["Da\u00df", "es", "ein", "hol\u00b7der", "Stoff", "von", "tau\u00b7send", "Bluh\u00b7men", "wird", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "APPR", "CARD", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "So spielt und pranget es nicht mit so reinen L\u00fcsten,", "tokens": ["So", "spielt", "und", "pran\u00b7get", "es", "nicht", "mit", "so", "rei\u00b7nen", "L\u00fcs\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "PPER", "PTKNEG", "APPR", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "460Als die, mit welchen sie den Reiz des Blicks vers\u00fc\u00dften.", "tokens": ["die", ",", "mit", "wel\u00b7chen", "sie", "den", "Reiz", "des", "Blicks", "ver\u00b7s\u00fc\u00df\u00b7ten", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "APPR", "PWAT", "PPER", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.15": {"text": "Jhr Antliz bildete der Eltern Herzen ab,", "tokens": ["Ihr", "Ant\u00b7liz", "bil\u00b7de\u00b7te", "der", "El\u00b7tern", "Her\u00b7zen", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Dann es den unsrigen zu wiederhohlen gab:", "tokens": ["Dann", "es", "den", "uns\u00b7ri\u00b7gen", "zu", "wie\u00b7der\u00b7hoh\u00b7len", "gab", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ART", "PPOSS", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Wie liebreich beyde sich dem Tugend-Kreise zeigen;", "tokens": ["Wie", "lieb\u00b7reich", "bey\u00b7de", "sich", "dem", "Tu\u00b7gen\u00b7dKrei\u00b7se", "zei\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PIS", "PRF", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Die\u00df war den Kindern auch so, wie den Eltern, eigen.", "tokens": ["Die\u00df", "war", "den", "Kin\u00b7dern", "auch", "so", ",", "wie", "den", "El\u00b7tern", ",", "ei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "ADV", "ADV", "$,", "PWAV", "ART", "NN", "$,", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}