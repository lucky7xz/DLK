{"dta.poem.20867": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Der  \n Sterbende  \n  Socrates.", "genre": "Lyrik; Prosa; Drama", "period": "N.A.", "pub_year": "1679", "urn": "urn:nbn:de:kobv:b4-20289-1", "language": ["de:0.99"], "booktitle": "Hofmann von Hofmannswaldau, Christian: Deutsche Ubersetzungen und Gedichte. Breslau, 1679."}, "poem": {"stanza.1": {"line.1": {"text": "Die Seele so den Flug recht nach dem Himmel lencket/", "tokens": ["Die", "See\u00b7le", "so", "den", "Flug", "recht", "nach", "dem", "Him\u00b7mel", "len\u00b7cket", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ART", "NN", "ADJD", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wird destoweniger durch Schwerigkeit gekr\u00e4ncket/", "tokens": ["Wird", "de\u00b7sto\u00b7we\u00b7ni\u00b7ger", "durch", "Schwe\u00b7rig\u00b7keit", "ge\u00b7kr\u00e4n\u00b7cket", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "APPR", "NN", "VVPP", "$("], "meter": "+-+-+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Je mehr sie von der Last des Leibes abgelegt/", "tokens": ["Je", "mehr", "sie", "von", "der", "Last", "des", "Lei\u00b7bes", "ab\u00b7ge\u00b7legt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "APPR", "ART", "NN", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sie treibet fl\u00fcchtig hin zu ihren alten Quellen/", "tokens": ["Sie", "trei\u00b7bet", "fl\u00fcch\u00b7tig", "hin", "zu", "ih\u00b7ren", "al\u00b7ten", "Quel\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ADV", "APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Jhr wolgef\u00fchrter Lauff wird itzt durch nichts bewegt/", "tokens": ["Ihr", "wol\u00b7ge\u00b7f\u00fchr\u00b7ter", "Lauff", "wird", "itzt", "durch", "nichts", "be\u00b7wegt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "ADV", "APPR", "PIS", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und ihren F\u00fcrsatz wei\u00df kein Zufall umzuf\u00e4llen.", "tokens": ["Und", "ih\u00b7ren", "F\u00fcr\u00b7satz", "wei\u00df", "kein", "Zu\u00b7fall", "um\u00b7zu\u00b7f\u00e4l\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PIAT", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Sie liebet einen Zweck/ der unser Auge blend/", "tokens": ["Sie", "lie\u00b7bet", "ei\u00b7nen", "Zweck", "/", "der", "un\u00b7ser", "Au\u00b7ge", "blend", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$(", "ART", "PPOSAT", "NN", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Jhr gantzes Tichten ist auf Wissenschafft gewend/", "tokens": ["Ihr", "gant\u00b7zes", "Tich\u00b7ten", "ist", "auf", "Wis\u00b7sen\u00b7schafft", "ge\u00b7wend", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "APPR", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Jm Blute wohnet sie/ und l\u00e4st sich Blut nicht leiten/", "tokens": ["Jm", "Blu\u00b7te", "woh\u00b7net", "sie", "/", "und", "l\u00e4st", "sich", "Blut", "nicht", "lei\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "$(", "KON", "VVFIN", "PRF", "NN", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sie bleibet fest und steiff auf ihrer glatten Bahn/", "tokens": ["Sie", "blei\u00b7bet", "fest", "und", "steiff", "auf", "ih\u00b7rer", "glat\u00b7ten", "Bahn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "KON", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Sie fleucht das faule Fleisch besteckt auf allen Seiten/", "tokens": ["Sie", "fleucht", "das", "fau\u00b7le", "Fleisch", "be\u00b7steckt", "auf", "al\u00b7len", "Sei\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "VVFIN", "APPR", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und zeiget wie kein Sinn sie recht verleiten kan.", "tokens": ["Und", "zei\u00b7get", "wie", "kein", "Sinn", "sie", "recht", "ver\u00b7lei\u00b7ten", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KOKOM", "PIAT", "NN", "PPER", "ADJD", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Und weil man sie also gantz eingezogen sp\u00fcret/", "tokens": ["Und", "weil", "man", "sie", "al\u00b7so", "gantz", "ein\u00b7ge\u00b7zo\u00b7gen", "sp\u00fc\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PIS", "PPER", "ADV", "ADV", "VVPP", "VVFIN", "$("], "meter": "-+--+-++-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Von der Verdrie\u00dfligkeit des Leibes unber\u00fchret/", "tokens": ["Von", "der", "Ver\u00b7drie\u00df\u00b7lig\u00b7keit", "des", "Lei\u00b7bes", "un\u00b7be\u00b7r\u00fch\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "ADJD", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So nimmt sie nichts mit ihr/ wann sie von dannen zeucht/", "tokens": ["So", "nimmt", "sie", "nichts", "mit", "ihr", "/", "wann", "sie", "von", "dan\u00b7nen", "zeucht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIS", "APPR", "PPOSAT", "$(", "PWAV", "PPER", "APPR", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Begierde/ Liebesbrunst mit Torheit/ Angst und Schrecken/", "tokens": ["Be\u00b7gier\u00b7de", "/", "Lie\u00b7bes\u00b7brunst", "mit", "Tor\u00b7heit", "/", "Angst", "und", "Schre\u00b7cken", "/"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$(", "NN", "APPR", "NN", "$(", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ja alles was sonst mehr nach dem Gebl\u00fcte reucht/", "tokens": ["Ja", "al\u00b7les", "was", "sonst", "mehr", "nach", "dem", "Ge\u00b7bl\u00fc\u00b7te", "reucht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PIS", "PWS", "ADV", "ADV", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+--+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Das l\u00e4st den faulen Leib sich einiglich bedecken.", "tokens": ["Das", "l\u00e4st", "den", "fau\u00b7len", "Leib", "sich", "ei\u00b7nig\u00b7lich", "be\u00b7de\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "ADJA", "NN", "PRF", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Wann sie nun klar und rein den Himmelsport erreicht/", "tokens": ["Wann", "sie", "nun", "klar", "und", "rein", "den", "Him\u00b7mel\u00b7sport", "er\u00b7reicht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ADJD", "KON", "ADJD", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In dem der bleiche Todt nicht seine Netze streicht/", "tokens": ["In", "dem", "der", "blei\u00b7che", "Todt", "nicht", "sei\u00b7ne", "Net\u00b7ze", "streicht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "ADJA", "NN", "PTKNEG", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So schauet man um sie viel Ehren-Cronen scheinen/", "tokens": ["So", "schau\u00b7et", "man", "um", "sie", "viel", "Eh\u00b7ren\u00b7Cro\u00b7nen", "schei\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "APPR", "PPER", "PIAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die G\u00f6tterreiche Schaar vertreibet ihr die Zeit/", "tokens": ["Die", "G\u00f6t\u00b7ter\u00b7rei\u00b7che", "Schaar", "ver\u00b7trei\u00b7bet", "ihr", "die", "Zeit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und was kein sterblicher mit Warheit kan verneinen/", "tokens": ["Und", "was", "kein", "sterb\u00b7li\u00b7cher", "mit", "War\u00b7heit", "kan", "ver\u00b7nei\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PIAT", "ADJA", "APPR", "NN", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Jhr Wolstand dringet tieff hin in die Ewigkeit.", "tokens": ["Ihr", "Wol\u00b7stand", "drin\u00b7get", "tieff", "hin", "in", "die", "E\u00b7wig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJD", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Die aber so den Sinn ihr hat zur Lust erlesen/", "tokens": ["Die", "a\u00b7ber", "so", "den", "Sinn", "ihr", "hat", "zur", "Lust", "er\u00b7le\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "ART", "NN", "PPER", "VAFIN", "APPRART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Seele der die Zucht ein Hencker ist gewesen/", "tokens": ["Die", "See\u00b7le", "der", "die", "Zucht", "ein", "Hen\u00b7cker", "ist", "ge\u00b7we\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ART", "NN", "ART", "NN", "VAFIN", "VAPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und der die Wissenschafft nur Schrecken hat erweckt/", "tokens": ["Und", "der", "die", "Wis\u00b7sen\u00b7schafft", "nur", "Schre\u00b7cken", "hat", "er\u00b7weckt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ART", "NN", "ADV", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die nur mit Fleisch und Blut ein B\u00fcndni\u00df hat gemachet/", "tokens": ["Die", "nur", "mit", "Fleisch", "und", "Blut", "ein", "B\u00fcn\u00b7dni\u00df", "hat", "ge\u00b7ma\u00b7chet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "NN", "KON", "NN", "ART", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und welcher Nichtigkeit sich auch so weit erstreckt/", "tokens": ["Und", "wel\u00b7cher", "Nich\u00b7tig\u00b7keit", "sich", "auch", "so", "weit", "er\u00b7streckt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAT", "NN", "PRF", "ADV", "ADV", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Da\u00df sie in grosser Lust der hohen Freundschafft lachet;", "tokens": ["Da\u00df", "sie", "in", "gros\u00b7ser", "Lust", "der", "ho\u00b7hen", "Freund\u00b7schafft", "la\u00b7chet", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Die in der Sinnen Scho\u00df nun viehisch worden ist/", "tokens": ["Die", "in", "der", "Sin\u00b7nen", "Scho\u00df", "nun", "vie\u00b7hisch", "wor\u00b7den", "ist", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "NN", "ADV", "ADJD", "VAPP", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die hat gar selten hier was l\u00f6bliches erkiest/", "tokens": ["Die", "hat", "gar", "sel\u00b7ten", "hier", "was", "l\u00f6b\u00b7li\u00b7ches", "er\u00b7kiest", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ADJD", "ADV", "PWS", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sie lieget gantz erstarrt durch schn\u00f6des Fleisch erstecket/", "tokens": ["Sie", "lie\u00b7get", "gantz", "er\u00b7starrt", "durch", "schn\u00f6\u00b7des", "Fleisch", "er\u00b7ste\u00b7cket", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "VVPP", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die Wollust machet sie durch ihr Getr\u00e4ncke voll/", "tokens": ["Die", "Wol\u00b7lust", "ma\u00b7chet", "sie", "durch", "ihr", "Ge\u00b7tr\u00e4n\u00b7cke", "voll", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und dieses was bey ihr den Thr\u00e4nen-Bach erwecket/", "tokens": ["Und", "die\u00b7ses", "was", "bey", "ihr", "den", "Thr\u00e4\u00b7nen\u00b7Bach", "er\u00b7we\u00b7cket", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "PRELS", "APPR", "PPER", "ART", "NN", "VVFIN", "$("], "meter": "-+-++--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Ist da\u00df sie nun die Burg des Leibes meiden soll.", "tokens": ["Ist", "da\u00df", "sie", "nun", "die", "Burg", "des", "Lei\u00b7bes", "mei\u00b7den", "soll", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "KOUS", "PPER", "ADV", "ART", "NN", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Dieweil ihr Wesen nun im Fleische gantz ersoffen/", "tokens": ["Die\u00b7weil", "ihr", "We\u00b7sen", "nun", "im", "Flei\u00b7sche", "gantz", "er\u00b7sof\u00b7fen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "APPRART", "NN", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So hat sie auch gewi\u00df viel Freyheit nicht zu hoffen/", "tokens": ["So", "hat", "sie", "auch", "ge\u00b7wi\u00df", "viel", "Frey\u00b7heit", "nicht", "zu", "hof\u00b7fen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADV", "PIAT", "NN", "PTKNEG", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und wird sie endlich gleich dem Leibe gantz entr\u00fcckt/", "tokens": ["Und", "wird", "sie", "end\u00b7lich", "gleich", "dem", "Lei\u00b7be", "gantz", "ent\u00b7r\u00fcckt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "ADV", "ART", "NN", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So weicht ihr aller Glantz/ dieweil ihr Licht verlohren/", "tokens": ["So", "weicht", "ihr", "al\u00b7ler", "Glantz", "/", "die\u00b7weil", "ihr", "Licht", "ver\u00b7loh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "NN", "$(", "KOUS", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ja weil die schwere Last sie auch noch immer dr\u00fc