{"dta.poem.19194": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "41.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Geselligkeit erh\u00f6ht den Menschen nicht allein,", "tokens": ["Ge\u00b7sel\u00b7lig\u00b7keit", "er\u00b7h\u00f6ht", "den", "Men\u00b7schen", "nicht", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das Thier veredelt auch und steigert der Verein.", "tokens": ["Das", "Thier", "ver\u00b7e\u00b7delt", "auch", "und", "stei\u00b7gert", "der", "Ver\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Der Biber baut voll Kunst, der in Gesellschaft lebt,", "tokens": ["Der", "Bi\u00b7ber", "baut", "voll", "Kunst", ",", "der", "in", "Ge\u00b7sell\u00b7schaft", "lebt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "NN", "$,", "PRELS", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Inde\u00df der einsame nur schlechte H\u00f6len gr\u00e4bt.", "tokens": ["In\u00b7de\u00df", "der", "ein\u00b7sa\u00b7me", "nur", "schlech\u00b7te", "H\u00f6\u00b7len", "gr\u00e4bt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "ADV", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Des Bienenstaates Flei\u00df th\u00fcrmt goldner Schl\u00f6sser Duft,", "tokens": ["Des", "Bie\u00b7nen\u00b7staa\u00b7tes", "Flei\u00df", "th\u00fcrmt", "gold\u00b7ner", "Schl\u00f6s\u00b7ser", "Duft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nicht Wachs noch Honig f\u00fcllt der Mauerbiene Kluft.", "tokens": ["Nicht", "Wachs", "noch", "Ho\u00b7nig", "f\u00fcllt", "der", "Mau\u00b7er\u00b7bie\u00b7ne", "Kluft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "ADV", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}