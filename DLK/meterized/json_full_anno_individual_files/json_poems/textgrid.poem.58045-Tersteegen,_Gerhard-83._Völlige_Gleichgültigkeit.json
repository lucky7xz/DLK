{"textgrid.poem.58045": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "83. V\u00f6llige Gleichg\u00fcltigkeit", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gleichwie ein leichtes Blatt, also gleichg\u00fcltig schwebet", "tokens": ["Gleich\u00b7wie", "ein", "leich\u00b7tes", "Blatt", ",", "al\u00b7so", "gleich\u00b7g\u00fcl\u00b7tig", "schwe\u00b7bet"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "$,", "ADV", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In Gottes Luft mein willenloser Sinn;", "tokens": ["In", "Got\u00b7tes", "Luft", "mein", "wil\u00b7len\u00b7lo\u00b7ser", "Sinn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Kein Wollen sonst in mir als Gottes Wollen lebet,", "tokens": ["Kein", "Wol\u00b7len", "sonst", "in", "mir", "als", "Got\u00b7tes", "Wol\u00b7len", "le\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "APPR", "PPER", "KOUS", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sein mind'ster Wink bl\u00e4st meinen Willen hin,", "tokens": ["Sein", "min\u00b7d'\u00b7ster", "Wink", "bl\u00e4st", "mei\u00b7nen", "Wil\u00b7len", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Zu lassen und zu tun, zu leiden oder nicht,", "tokens": ["Zu", "las\u00b7sen", "und", "zu", "tun", ",", "zu", "lei\u00b7den", "o\u00b7der", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "KON", "PTKZU", "VVINF", "$,", "PTKZU", "VVINF", "KON", "PTKNEG", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Es ist mir alles eins, Herr, wenn dein Will' geschicht.", "tokens": ["Es", "ist", "mir", "al\u00b7les", "eins", ",", "Herr", ",", "wenn", "dein", "Will'", "ge\u00b7schicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PIS", "PIS", "$,", "NN", "$,", "KOUS", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Gleichwie ein leichtes Blatt, also gleichg\u00fcltig schwebet", "tokens": ["Gleich\u00b7wie", "ein", "leich\u00b7tes", "Blatt", ",", "al\u00b7so", "gleich\u00b7g\u00fcl\u00b7tig", "schwe\u00b7bet"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "$,", "ADV", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In Gottes Luft mein willenloser Sinn;", "tokens": ["In", "Got\u00b7tes", "Luft", "mein", "wil\u00b7len\u00b7lo\u00b7ser", "Sinn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Kein Wollen sonst in mir als Gottes Wollen lebet,", "tokens": ["Kein", "Wol\u00b7len", "sonst", "in", "mir", "als", "Got\u00b7tes", "Wol\u00b7len", "le\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "APPR", "PPER", "KOUS", "NN", "NN", "VVFIN", "$