{"dta.poem.19148": {"metadata": {"author": {"name": "Zinzendorf, Nicolaus Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "CxXVI.  Auf Johann Dobers, des T\u00f6pfers,  \n 30ten Geburts-Tag.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20688-6", "language": ["de:0.99"], "booktitle": "Zinzendorf, Nicolaus Ludwig von: Teutscher Gedichte Erster Theil. Herrnhuth, 1735."}, "poem": {"stanza.1": {"line.1": {"text": "Ich wills wagen, von der sch\u00f6nen Pracht", "tokens": ["Ich", "wills", "wa\u00b7gen", ",", "von", "der", "sch\u00f6\u00b7nen", "Pracht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "VVINF", "$,", "APPR", "ART", "ADJA", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Was zu sagen, die aus JEsu lacht,", "tokens": ["Was", "zu", "sa\u00b7gen", ",", "die", "aus", "Je\u00b7su", "lacht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PTKZU", "VVINF", "$,", "PRELS", "APPR", "NE", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Aber wag ich mich zu sehr?", "tokens": ["A\u00b7ber", "wag", "ich", "mich", "zu", "sehr", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PRF", "PTKA", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Seine Wunder sind ein Meer,", "tokens": ["Sei\u00b7ne", "Wun\u00b7der", "sind", "ein", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Doch last h\u00f6ren was der Heyland macht.", "tokens": ["Doch", "last", "h\u00f6\u00b7ren", "was", "der", "Hey\u00b7land", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "VVINF", "PWS", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Heiligs Wesen \u00f6fne mir dein Buch,", "tokens": ["Hei\u00b7ligs", "We\u00b7sen", "\u00f6f\u00b7ne", "mir", "dein", "Buch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Ich kans lesen, weg mit Mosis Tuch!", "tokens": ["Ich", "kans", "le\u00b7sen", ",", "weg", "mit", "Mo\u00b7sis", "Tuch", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "$,", "ADV", "APPR", "NE", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Noch unausgesprochnes Wort!", "tokens": ["Noch", "un\u00b7aus\u00b7ge\u00b7sproch\u00b7nes", "Wort", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Stimmen her von deiner Pfort", "tokens": ["Stim\u00b7men", "her", "von", "dei\u00b7ner", "Pfort"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Von der Arche, das ist mein Gesuch.", "tokens": ["Von", "der", "Ar\u00b7che", ",", "das", "ist", "mein", "Ge\u00b7such", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PDS", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Du bist wahrlich eine gute Lieb!", "tokens": ["Du", "bist", "wahr\u00b7lich", "ei\u00b7ne", "gu\u00b7te", "Lieb", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Und beharrlich in dem Liebes-Trieb.", "tokens": ["Und", "be\u00b7harr\u00b7lich", "in", "dem", "Lie\u00b7bes\u00b7Trieb", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Niemand kan so traurig seyn,", "tokens": ["Nie\u00b7mand", "kan", "so", "trau\u00b7rig", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "ADV", "ADJD", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df ihn deiner Augen Schein", "tokens": ["Da\u00df", "ihn", "dei\u00b7ner", "Au\u00b7gen", "Schein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Nicht erfreute, wenn er vor dir blieb.", "tokens": ["Nicht", "er\u00b7freu\u00b7te", ",", "wenn", "er", "vor", "dir", "blieb", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "$,", "KOUS", "PPER", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Jesu Creutze, wo ich ihn erst sah\u2019,", "tokens": ["Je\u00b7su", "Creut\u00b7ze", ",", "wo", "ich", "ihn", "erst", "sah'", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PWAV", "PPER", "PPER", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Komm! und reitze mein Hallelujah!", "tokens": ["Komm", "!", "und", "reit\u00b7ze", "mein", "Hal\u00b7le\u00b7lu\u00b7jah", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Denn wenn ich in Ohnmacht w\u00e4r,", "tokens": ["Denn", "wenn", "ich", "in", "Ohn\u00b7macht", "w\u00e4r", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "APPR", "NN", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und es schallte ohngef\u00e4hr", "tokens": ["Und", "es", "schall\u00b7te", "ohn\u00b7ge\u00b7f\u00e4hr"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Was vom Creutze, w\u00e4r ich wieder da.", "tokens": ["Was", "vom", "Creut\u00b7ze", ",", "w\u00e4r", "ich", "wie\u00b7der", "da", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPRART", "NN", "$,", "VAFIN", "PPER", "ADV", "ADV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Auserwehlte, und vor unser Schuld", "tokens": ["Au\u00b7ser\u00b7wehl\u00b7te", ",", "und", "vor", "un\u00b7ser", "Schuld"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "KON", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Ausgeqv\u00e4lte g\u00f6ttliche Geduld!", "tokens": ["Aus\u00b7ge\u00b7qv\u00e4l\u00b7te", "g\u00f6tt\u00b7li\u00b7che", "Ge\u00b7duld", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "K\u00f6nig nach dem alten Recht!", "tokens": ["K\u00f6\u00b7nig", "nach", "dem", "al\u00b7ten", "Recht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Nach dem neuen aber Knecht,", "tokens": ["Nach", "dem", "neu\u00b7en", "a\u00b7ber", "Knecht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wiederbringer der verlohrnen Huld!", "tokens": ["Wie\u00b7der\u00b7brin\u00b7ger", "der", "ver\u00b7lohr\u00b7nen", "Huld", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "Solten Zeugen deiner Wunder-Pracht", "tokens": ["Sol\u00b7ten", "Zeu\u00b7gen", "dei\u00b7ner", "Wun\u00b7der\u00b7Pracht"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "K\u00f6nnen schweigen von dem Lebens-Safft,", "tokens": ["K\u00f6n\u00b7nen", "schwei\u00b7gen", "von", "dem", "Le\u00b7bens\u00b7\u00b7S\u00b7afft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Der in blutiger Gestallt", "tokens": ["Der", "in", "blu\u00b7ti\u00b7ger", "Ge\u00b7stallt"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Durch die gantze Erde wallt.", "tokens": ["Durch", "die", "gant\u00b7ze", "Er\u00b7de", "wallt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Sind doch Felsen dr\u00fcber aufgeklafft.", "tokens": ["Sind", "doch", "Fel\u00b7sen", "dr\u00fc\u00b7ber", "auf\u00b7ge\u00b7klafft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "NN", "PAV", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "Heil\u2019ger Tempel mit dem Rauch-Altar", "tokens": ["Heil'\u00b7ger", "Tem\u00b7pel", "mit", "dem", "Rauch\u00b7Al\u00b7tar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Die Exempel sind noch allzu rar", "tokens": ["Die", "Ex\u00b7em\u00b7pel", "sind", "noch", "all\u00b7zu", "rar"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADV", "PTKA", "ADJD"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Von den Blitzen, die geschehn,", "tokens": ["Von", "den", "Blit\u00b7zen", ",", "die", "ge\u00b7schehn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Von den Stimmen, die ergehn,", "tokens": ["Von", "den", "Stim\u00b7men", ",", "die", "er\u00b7gehn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Von den Donnern in dem Gnaden-Jahr.", "tokens": ["Von", "den", "Don\u00b7nern", "in", "dem", "Gna\u00b7den\u00b7Jahr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.8": {"line.1": {"text": "Wir, die Armen und so Schm\u00e4hlige!", "tokens": ["Wir", ",", "die", "Ar\u00b7men", "und", "so", "Schm\u00e4h\u00b7li\u00b7ge", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ART", "NN", "KON", "ADV", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Durchs Erbarmen aber Selige,", "tokens": ["Durchs", "Er\u00b7bar\u00b7men", "a\u00b7ber", "Se\u00b7li\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Wohnen so in einer Stadt", "tokens": ["Woh\u00b7nen", "so", "in", "ei\u00b7ner", "Stadt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wo man nur zu nehmen hat;", "tokens": ["Wo", "man", "nur", "zu", "neh\u00b7men", "hat", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ADV", "PTKZU", "VVINF", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Denn der Gaben sind unzehlige.", "tokens": ["Denn", "der", "Ga\u00b7ben", "sind", "un\u00b7zeh\u00b7li\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "ADJA", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.9": {"line.1": {"text": "Unter andern sieh\u2019t man einige", "tokens": ["Un\u00b7ter", "an\u00b7dern", "sieh't", "man", "ei\u00b7ni\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PIS", "VVFIN", "PIS", "PIAT"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Bey uns wandern, die das Deinige", "tokens": ["Bey", "uns", "wan\u00b7dern", ",", "die", "das", "Dei\u00b7ni\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPER", "VVFIN", "$,", "PRELS", "ART", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Warten, wie es sich geb\u00fchrt,", "tokens": ["War\u00b7ten", ",", "wie", "es", "sich", "ge\u00b7b\u00fchrt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "PPER", "PRF", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Die schon manches Hertz ger\u00fchrt", "tokens": ["Die", "schon", "man\u00b7ches", "Hertz", "ge\u00b7r\u00fchrt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "PIAT", "NN", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Guter Sch\u00f6pffer, was du machst, ist gut,", "tokens": ["Gu\u00b7ter", "Sch\u00f6pf\u00b7fer", ",", "was", "du", "machst", ",", "ist", "gut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PWS", "PPER", "VVFIN", "$,", "VAFIN", "ADJD", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Seinen Thon wies ihn beliebt,", "tokens": ["Sei\u00b7nen", "Thon", "wies", "ihn", "be\u00b7liebt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ohne da\u00df er Antwort giebt,)", "tokens": ["Oh\u00b7ne", "da\u00df", "er", "Ant\u00b7wort", "giebt", ",", ")"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "KOUS", "PPER", "NN", "VVFIN", "$,", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Dir mi\u00dfr\u00e4th auch nie nichts in der Gluth.", "tokens": ["Dir", "mi\u00df\u00b7r\u00e4\u00b7th", "auch", "nie", "nichts", "in", "der", "Gluth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "PIS", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.11": {"line.1": {"text": "Gieb uns allen, die so hertzlich gern", "tokens": ["Gieb", "uns", "al\u00b7len", ",", "die", "so", "hertz\u00b7lich", "gern"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "PIS", "$,", "PRELS", "ADV", "ADJD", "ADV"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "M\u00f6chten wallen nach dem Sinn des HErrn,", "tokens": ["M\u00f6ch\u00b7ten", "wal\u00b7len", "nach", "dem", "Sinn", "des", "Herrn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVINF", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Und nicht m\u00f6gen seelig seyn,", "tokens": ["Und", "nicht", "m\u00f6\u00b7gen", "see\u00b7lig", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "VMFIN", "ADJD", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Als durch JEsu Blut allein,", "tokens": ["Als", "durch", "Je\u00b7su", "Blut", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "NE", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Gieb uns diesen hellen Morgenstern!", "tokens": ["Gieb", "uns", "die\u00b7sen", "hel\u00b7len", "Mor\u00b7gens\u00b7tern", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PDAT", "ADJA", "NN", "$."], "meter": "+-+-+-+--", "measure": "unknown.measure.tetra"}}}}}