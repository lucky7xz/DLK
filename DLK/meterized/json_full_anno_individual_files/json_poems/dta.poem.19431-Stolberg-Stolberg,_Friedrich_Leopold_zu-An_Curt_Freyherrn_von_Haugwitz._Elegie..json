{"dta.poem.19431": {"metadata": {"author": {"name": "Stolberg-Stolberg, Friedrich Leopold zu", "birth": "N.A.", "death": "N.A."}, "title": "An Curt Freyherrn von Haugwitz.  \n  Elegie.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1779", "urn": "urn:nbn:de:kobv:b4-20589-5", "language": ["de:0.99"], "booktitle": "Stolberg-Stolberg, Christian zu; Stolberg-Stolberg, Friedrich Leopold zu: Gedichte. Leipzig, 1779."}, "poem": {"stanza.1": {"line.1": {"text": "S\u00fcsser duftet die Flur, und k\u00fchler hauchet der", "tokens": ["S\u00fcs\u00b7ser", "duf\u00b7tet", "die", "Flur", ",", "und", "k\u00fch\u00b7ler", "hau\u00b7chet", "der"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN", "$,", "KON", "ADJD", "VVFIN", "ART"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Abend;", "tokens": ["A\u00b7bend", ";"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Nur ein welkendes Roth weilt am azurenen West.", "tokens": ["Nur", "ein", "wel\u00b7ken\u00b7des", "Roth", "weilt", "am", "a\u00b7zu\u00b7re\u00b7nen", "West", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "--+--++-+-+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Stille thauet herab, und Ruh\u2019, und sanfte Be-", "tokens": ["Stil\u00b7le", "thau\u00b7et", "her\u00b7ab", ",", "und", "Ruh'", ",", "und", "sanf\u00b7te", "Be"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVFIN", "ADV", "$,", "KON", "NN", "$,", "KON", "ADJA", "TRUNC"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.5": {"text": "geistrung", "tokens": ["geis\u00b7trung"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.6": {"text": "Auf den einsamen Pfad, welchen der Waller", "tokens": ["Auf", "den", "ein\u00b7sa\u00b7men", "Pfad", ",", "wel\u00b7chen", "der", "Wal\u00b7ler"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "PWAT", "ART", "NN"], "meter": "---+-+---+-", "measure": "unknown.measure.tri"}, "line.7": {"text": "betrit.", "tokens": ["be\u00b7trit", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.8": {"text": "Hesperus schaut auf ihn mit freundlichen Blicken", "tokens": ["Hes\u00b7pe\u00b7rus", "schaut", "auf", "ihn", "mit", "freund\u00b7li\u00b7chen", "Bli\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "APPR", "PPER", "APPR", "ADJA", "NN"], "meter": "---+---+--+-", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "hernieder,", "tokens": ["her\u00b7nie\u00b7der", ","], "token_info": ["word", "punct"], "pos": ["PTKVZ", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.10": {"text": "Lispelt segnend ihm zu: Geh\u2019 in Frieden dahin!", "tokens": ["Lis\u00b7pelt", "seg\u00b7nend", "ihm", "zu", ":", "Geh'", "in", "Frie\u00b7den", "da\u00b7hin", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVPP", "PPER", "PTKVZ", "$.", "NE", "APPR", "NN", "PAV", "$."], "meter": "+-+-+-+-+--+", "measure": "iambic.hexa.chol"}, "line.11": {"text": "Ich auch wander\u2019 umher, und such auf einsamen", "tokens": ["Ich", "auch", "wan\u00b7der'", "um\u00b7her", ",", "und", "such", "auf", "ein\u00b7sa\u00b7men"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "PTKVZ", "$,", "KON", "ADV", "APPR", "ADJA"], "meter": "--+--+-+--+-", "measure": "anapaest.di.plus"}, "line.12": {"text": "Pfaden", "tokens": ["Pfa\u00b7den"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.13": {"text": "Ruh\u2019 und lindernden Trost f\u00fcr mein sinkendes", "tokens": ["Ruh'", "und", "lin\u00b7dern\u00b7den", "Trost", "f\u00fcr", "mein", "sin\u00b7ken\u00b7des"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "ADJA", "NN", "APPR", "PPOSAT", "NN"], "meter": "+-+--+--+--", "measure": "trochaic.tetra.relaxed"}, "line.14": {"text": "Herz.", "tokens": ["Herz", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}, "line.15": {"text": "Ach vergebens! \u2014 O du der besten J\u00fcnglinge", "tokens": ["Ach", "ver\u00b7ge\u00b7bens", "!", "O", "du", "der", "bes\u00b7ten", "J\u00fcng\u00b7lin\u00b7ge"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "ADV", "$.", "$(", "NE", "NE", "ART", "ADJA", "NN"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.16": {"text": "Bester,", "tokens": ["Bes\u00b7ter", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.17": {"text": "Den ich liebe, so sehr, als ich zu lieben ver-", "tokens": ["Den", "ich", "lie\u00b7be", ",", "so", "sehr", ",", "als", "ich", "zu", "lie\u00b7ben", "ver"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "PPER", "VVFIN", "$,", "ADV", "ADV", "$,", "KOUS", "PPER", "PTKZU", "VVINF", "TRUNC"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.18": {"text": "mag;", "tokens": ["mag", ";"], "token_info": ["word", "punct"], "pos": ["VMFIN", "$."], "meter": "+", "measure": "single.up"}, "line.19": {"text": "Dem die milde Natur der Gaben sch\u00f6nste, die selten", "tokens": ["Dem", "die", "mil\u00b7de", "Na\u00b7tur", "der", "Ga\u00b7ben", "sch\u00f6ns\u00b7te", ",", "die", "sel\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PDS", "ART", "ADJA", "NN", "ART", "NN", "VVFIN", "$,", "PRELS", "ADJD"], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.20": {"text": "Sie verleiht, ein Herz zarter Empfindung,", "tokens": ["Sie", "ver\u00b7leiht", ",", "ein", "Herz", "zar\u00b7ter", "Emp\u00b7fin\u00b7dung", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ART", "NN", "ADJA", "NN", "$,"], "meter": "--+-++--+-", "measure": "iambic.tetra.relaxed"}, "line.21": {"text": "verlieh;", "tokens": ["ver\u00b7lieh", ";"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.22": {"text": "Den sie der Freundschaft schuf, der Lieb\u2019, und", "tokens": ["Den", "sie", "der", "Freund\u00b7schaft", "schuf", ",", "der", "Lieb'", ",", "und"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ART", "PPER", "ART", "NN", "VVFIN", "$,", "ART", "NN", "$,", "KON"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "stilleren Freuden;", "tokens": ["stil\u00b7le\u00b7ren", "Freu\u00b7den", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.24": {"text": "Sanfte Melancholie, deine Feindinnen nicht!", "tokens": ["Sanf\u00b7te", "Me\u00b7lan\u00b7cho\u00b7lie", ",", "dei\u00b7ne", "Fein\u00b7din\u00b7nen", "nicht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PPOSAT", "NN", "PTKNEG", "$."], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}, "line.25": {"text": "Ach du windest dich los aus deines Freundes Um-", "tokens": ["Ach", "du", "win\u00b7dest", "dich", "los", "aus", "dei\u00b7nes", "Freun\u00b7des", "Um"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "PPER", "VVFIN", "PPER", "PTKVZ", "APPR", "PPOSAT", "NN", "TRUNC"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.26": {"text": "armung;", "tokens": ["ar\u00b7mung", ";"], "token_info": ["word", "punct"], "pos": ["ADJD", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.27": {"text": "Scheidest z\u00f6gernd von ihm \u2014 ach! auf ewig", "tokens": ["Schei\u00b7dest", "z\u00f6\u00b7gernd", "von", "ihm", "ach", "!", "auf", "e\u00b7wig"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["VVFIN", "ADJD", "APPR", "PPER", "$(", "ITJ", "$.", "APPR", "ADJD"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.28": {"text": "vielleicht! &#8212; &#8212;", "tokens": ["viel\u00b7leicht", "!", "&#8212;", "&#8212;"], "token_info": ["word", "punct", "XML_entity", "XML_entity"], "pos": ["ADV", "$.", "$(", "$."], "meter": "-+", "measure": "iambic.single"}, "line.29": {"text": "Also sind sie dahin, der Freundschaft heilige Jahre,", "tokens": ["Al\u00b7so", "sind", "sie", "da\u00b7hin", ",", "der", "Freund\u00b7schaft", "hei\u00b7li\u00b7ge", "Jah\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PAV", "$,", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.30": {"text": "Deren jeglicher Tag fester und fester uns band?", "tokens": ["De\u00b7ren", "jeg\u00b7li\u00b7cher", "Tag", "fes\u00b7ter", "und", "fes\u00b7ter", "uns", "band", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIAT", "NN", "ADJD", "KON", "ADJD", "PPER", "VVFIN", "$."], "meter": "+-+---+--+--+", "measure": "trochaic.penta.relaxed"}, "line.31": {"text": "Also sind sie verbl\u00fcht, die Veilchen, welche mir", "tokens": ["Al\u00b7so", "sind", "sie", "ver\u00b7bl\u00fcht", ",", "die", "Veil\u00b7chen", ",", "wel\u00b7che", "mir"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "VVPP", "$,", "ART", "NN", "$,", "PRELS", "PPER"], "meter": "---+-+-+-+-+", "measure": "unknown.measure.penta"}, "line.32": {"text": "oftmal", "tokens": ["oft\u00b7mal"], "token_info": ["word"], "pos": ["ADV"], "meter": "+-", "measure": "trochaic.single"}, "line.33": {"text": "Deine gef\u00e4llige Hand streut\u2019 in den m\u00fchsamen", "tokens": ["Dei\u00b7ne", "ge\u00b7f\u00e4l\u00b7li\u00b7ge", "Hand", "streut'", "in", "den", "m\u00fch\u00b7sa\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "APPR", "ART", "ADJA"], "meter": "+--+--+-+-+--", "measure": "dactylic.di.plus"}, "line.34": {"text": "Weg?", "tokens": ["Weg", "?"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}, "line.35": {"text": "Nein! sie sind nicht verbl\u00fcht! In jeder heiteren", "tokens": ["Nein", "!", "sie", "sind", "nicht", "ver\u00b7bl\u00fcht", "!", "In", "je\u00b7der", "hei\u00b7te\u00b7ren"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKANT", "$.", "PPER", "VAFIN", "PTKNEG", "VVPP", "$.", "APPR", "PIS", "VVINF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Stunde", "tokens": ["Stun\u00b7de"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.37": {"text": "Kehrt mir l\u00e4chelnd zur\u00fcck jede genossene Lust.", "tokens": ["Kehrt", "mir", "l\u00e4\u00b7chelnd", "zu\u00b7r\u00fcck", "je\u00b7de", "ge\u00b7nos\u00b7se\u00b7ne", "Lust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADJD", "PTKVZ", "PIAT", "ADJA", "NN", "$."], "meter": "+-+---+--+--+", "measure": "trochaic.penta.relaxed"}, "line.38": {"text": "O dann sollen mich oft Phantome der Abend\u2019 um-", "tokens": ["O", "dann", "sol\u00b7len", "mich", "oft", "Phan\u00b7to\u00b7me", "der", "A\u00b7bend'", "um"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "ADV", "VMFIN", "PPER", "ADV", "NN", "ART", "NN", "TRUNC"], "meter": "+-+--+-+--+--", "measure": "trochaic.penta.relaxed"}, "line.39": {"text": "schweben,", "tokens": ["schwe\u00b7ben", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.40": {"text": "Die, uns jeglichesmal t\u00e4uschend, zu fl\u00fcchtig ent-", "tokens": ["Die", ",", "uns", "jeg\u00b7li\u00b7ches\u00b7mal", "t\u00e4u\u00b7schend", ",", "zu", "fl\u00fcch\u00b7tig", "ent"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "$,", "PPER", "ADV", "ADJD", "$,", "PTKA", "ADJD", "TRUNC"], "meter": "--+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.41": {"text": "flohn!", "tokens": ["flohn", "!"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-", "measure": "single.down"}, "line.42": {"text": "Jezo wanderten wir, mit Fr\u00fchlingsruhe ge-", "tokens": ["Je\u00b7zo", "wan\u00b7der\u00b7ten", "wir", ",", "mit", "Fr\u00fch\u00b7lings\u00b7ru\u00b7he", "ge"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "APPR", "NN", "TRUNC"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.43": {"text": "segnet,", "tokens": ["seg\u00b7net", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.44": {"text": "Arm geschlungen in Arm, bl\u00fchende Th\u00e4ler", "tokens": ["Arm", "ge\u00b7schlun\u00b7gen", "in", "Arm", ",", "bl\u00fc\u00b7hen\u00b7de", "Th\u00e4\u00b7ler"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "VVPP", "APPR", "NN", "$,", "ADJA", "NN"], "meter": "+-+--++--+-", "measure": "trochaic.penta.relaxed"}, "line.45": {"text": "hinab;", "tokens": ["hin\u00b7ab", ";"], "token_info": ["word", "punct"], "pos": ["PTKVZ", "$."], "meter": "-+", "measure": "iambic.single"}, "line.46": {"text": "Lagerten jezo uns hin am moosigen Ufer des", "tokens": ["La\u00b7ger\u00b7ten", "je\u00b7zo", "uns", "hin", "am", "moo\u00b7si\u00b7gen", "U\u00b7fer", "des"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "PPER", "ADV", "APPRART", "ADJA", "NN", "ART"], "meter": "+--+--+-+--+--", "measure": "dactylic.di.plus"}, "line.47": {"text": "Baches,", "tokens": ["Ba\u00b7ches", ","], "token_info": ["word", "punct"], "pos": ["NE", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.48": {"text": "Und dem s\u00fcssen Geschw\u00e4z horchte vertrau-", "tokens": ["Und", "dem", "s\u00fcs\u00b7sen", "Ge\u00b7schw\u00e4z", "horch\u00b7te", "ver\u00b7trau"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "TRUNC"], "meter": "--+--++--+", "measure": "anapaest.di.plus"}, "line.49": {"text": "lich der Mond.", "tokens": ["lich", "der", "Mond", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "$."], "meter": "--+", "measure": "anapaest.init"}, "line.50": {"text": "O, wie schmolz uns dann das Herz in sanfter", "tokens": ["O", ",", "wie", "schmolz", "uns", "dann", "das", "Herz", "in", "sanf\u00b7ter"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "PWAV", "ADJD", "PPER", "ADV", "ART", "NN", "APPR", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.51": {"text": "Empfindung!", "tokens": ["Emp\u00b7fin\u00b7dung", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.52": {"text": "O, wie schmeckten wir dich, himmlische Freund-", "tokens": ["O", ",", "wie", "schmeck\u00b7ten", "wir", "dich", ",", "himm\u00b7li\u00b7sche", "Freun\u00b7d"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "$,", "PWAV", "VVFIN", "PPER", "PRF", "$,", "ADJA", "TRUNC"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.53": {"text": "schaft, so s\u00fc\u00df!", "tokens": ["schaft", ",", "so", "s\u00fc\u00df", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADV", "ADJD", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.54": {"text": "Einstens pfl\u00fcckt\u2019 ich zwo junge Vergi\u00dfmeinnicht,", "tokens": ["Eins\u00b7tens", "pfl\u00fcckt'", "ich", "zwo", "jun\u00b7ge", "Ver\u00b7gi\u00df\u00b7mein\u00b7nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "CARD", "ADJA", "NN", "$,"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.55": {"text": "und streute,", "tokens": ["und", "streu\u00b7te", ","], "token_info": ["word", "word", "punct"], "pos": ["KON", "VVFIN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.56": {"text": "Wo am kl\u00e4rsten er flo\u00df, sie in den kr\u00e4uselnden", "tokens": ["Wo", "am", "kl\u00e4rs\u00b7ten", "er", "flo\u00df", ",", "sie", "in", "den", "kr\u00e4u\u00b7seln\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "APPRART", "ADJA", "PPER", "VVFIN", "$,", "PPER", "APPR", "ART", "ADJA"], "meter": "--+--+-+-+--", "measure": "anapaest.di.plus"}, "line.57": {"text": "Bach.", "tokens": ["Bach", "."], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+", "measure": "single.up"}, "line.58": {"text": "Eine ri\u00df er hinweg; die andere weilt\u2019 am Ufer!", "tokens": ["Ei\u00b7ne", "ri\u00df", "er", "hin\u00b7weg", ";", "die", "an\u00b7de\u00b7re", "weilt'", "am", "U\u00b7fer", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "PTKVZ", "$.", "ART", "ADJA", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+--+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.59": {"text": "Und du starrtest mich an; Thr\u00e4nen bew\u00f6lkten", "tokens": ["Und", "du", "starr\u00b7test", "mich", "an", ";", "Thr\u00e4\u00b7nen", "be\u00b7w\u00f6lk\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPER", "PTKVZ", "$.", "NN", "VVINF"], "meter": "--+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.60": {"text": "den Blick!", "tokens": ["den", "Blick", "!"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.61": {"text": "Ich verstand dich! Auch mich ergrif der b\u00e4ngste", "tokens": ["Ich", "ver\u00b7stand", "dich", "!", "Auch", "mich", "er\u00b7grif", "der", "b\u00e4ngs\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$.", "ADV", "PPER", "VVFIN", "ART", "ADJA"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.62": {"text": "Gedanke:", "tokens": ["Ge\u00b7dan\u00b7ke", ":"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.63": {"text": "Ach! wenn einst das Geschick uns wie die", "tokens": ["Ach", "!", "wenn", "einst", "das", "Ge\u00b7schick", "uns", "wie", "die"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "KOUS", "ADV", "ART", "NN", "PPER", "KOKOM", "ART"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.64": {"text": "Blumen verstreut!", "tokens": ["Blu\u00b7men", "ver\u00b7streut", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.65": {"text": "So schlich Wehmut oft in unsere Freuden; so", "tokens": ["So", "schlich", "Weh\u00b7mut", "oft", "in", "un\u00b7se\u00b7re", "Freu\u00b7den", ";", "so"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "ADJD", "NN", "ADV", "APPR", "PPOSAT", "NN", "$.", "ADV"], "meter": "--+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.66": {"text": "sprosset", "tokens": ["spros\u00b7set"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "+-", "measure": "trochaic.single"}, "line.67": {"text": "In dem Myrtengeb\u00fcsch\u2019 eine Zypresse mit", "tokens": ["In", "dem", "Myr\u00b7ten\u00b7ge\u00b7b\u00fcsch'", "ei\u00b7ne", "Zyp\u00b7res\u00b7se", "mit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN", "APPR"], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.68": {"text": "auf.", "tokens": ["auf", "."], "token_info": ["word", "punct"], "pos": ["PTKVZ", "$."], "meter": "+", "measure": "single.up"}, "line.69": {"text": "Oftmal standen wir still am schroffen Hange des", "tokens": ["Oft\u00b7mal", "stan\u00b7den", "wir", "still", "am", "schrof\u00b7fen", "Han\u00b7ge", "des"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPRART", "ADJA", "NN", "ART"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.70": {"text": "Felsen,", "tokens": ["Fel\u00b7sen", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.71": {"text": "M\u00fcden Pilgern gleich, \u00fcber die St\u00e4be gelehnt;", "tokens": ["M\u00fc\u00b7den", "Pil\u00b7gern", "gleich", ",", "\u00fc\u00b7ber", "die", "St\u00e4\u00b7be", "ge\u00b7lehnt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "$,", "APPR", "ART", "NN", "VVPP", "$."], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.72": {"text": "Und umh\u00fcllte mich dann der Nebel der schwarzen", "tokens": ["Und", "um\u00b7h\u00fcll\u00b7te", "mich", "dann", "der", "Ne\u00b7bel", "der", "schwar\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ART", "NN", "ART", "ADJA"], "meter": "--+--+-+--+-", "measure": "anapaest.di.plus"}, "line.73": {"text": "Schwermut,", "tokens": ["Schwer\u00b7mut", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.74": {"text": "O so sch\u00fcttet\u2019 ich, Freund, dir in das deine", "tokens": ["O", "so", "sch\u00fct\u00b7tet'", "ich", ",", "Freund", ",", "dir", "in", "das", "dei\u00b7ne"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "ADV", "VVFIN", "PPER", "$,", "NN", "$,", "PPER", "APPR", "ART", "PPOSAT"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.75": {"text": "mein Herz!", "tokens": ["mein", "Herz", "!"], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.76": {"text": "Seufzend h\u00f6rtest du mich, und jede Sorge, die", "tokens": ["Seuf\u00b7zend", "h\u00f6r\u00b7test", "du", "mich", ",", "und", "je\u00b7de", "Sor\u00b7ge", ",", "die"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["ADJD", "VVFIN", "PPER", "PRF", "$,", "KON", "PIAT", "NN", "$,", "PRELS"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.77": {"text": "theilend", "tokens": ["thei\u00b7lend"], "token_info": ["word"], "pos": ["ADJD"], "meter": "+-", "measure": "trochaic.single"}, "line.78": {"text": "Du mir nahmest, erhob meine beklommene", "tokens": ["Du", "mir", "nah\u00b7mest", ",", "er\u00b7hob", "mei\u00