{"textgrid.poem.59217": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1822, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Nachtigall schweigt, sie hat ihr Nest gefunden,", "tokens": ["Die", "Nach\u00b7ti\u00b7gall", "schweigt", ",", "sie", "hat", "ihr", "Nest", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PPER", "VAFIN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Tr\u00e4g ziehn die Quellen, die so k\u00fchle sprangen,", "tokens": ["Tr\u00e4g", "ziehn", "die", "Quel\u00b7len", ",", "die", "so", "k\u00fch\u00b7le", "spran\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,", "PRELS", "ADV", "ADJA", "VVFIN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Von tr\u00fcber Schw\u00fcle liegt die Welt umfangen,", "tokens": ["Von", "tr\u00fc\u00b7ber", "Schw\u00fc\u00b7le", "liegt", "die", "Welt", "um\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "So hat den Lenz der Sommer \u00fcberwunden.", "tokens": ["So", "hat", "den", "Lenz", "der", "Som\u00b7mer", "\u00fc\u00b7berw\u00b7un\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Noch nie hat es die Brust so tief empfunden,", "tokens": ["Noch", "nie", "hat", "es", "die", "Brust", "so", "tief", "emp\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "ART", "NN", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Es ist, als ob viel Stimmen heimlich sangen:", "tokens": ["Es", "ist", ",", "als", "ob", "viel", "Stim\u00b7men", "heim\u00b7lich", "san\u00b7gen", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "KOKOM", "KOUS", "PIAT", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "\u00bbauch dein Lenz, froher S\u00e4nger, ist vergangen,", "tokens": ["\u00bb", "auch", "dein", "Lenz", ",", "fro\u00b7her", "S\u00e4n\u00b7ger", ",", "ist", "ver\u00b7gan\u00b7gen", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "ADV", "PPOSAT", "NN", "$,", "ADJA", "NN", "$,", "VAFIN", "VVPP", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "An Weib und Kind ist nun der Sinn gebunden!\u00ab", "tokens": ["An", "Weib", "und", "Kind", "ist", "nun", "der", "Sinn", "ge\u00b7bun\u00b7den", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VAFIN", "ADV", "ART", "NN", "VVPP", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "O komm, Geliebte, komm zu mir zur\u00fccke!", "tokens": ["O", "komm", ",", "Ge\u00b7lieb\u00b7te", ",", "komm", "zu", "mir", "zu\u00b7r\u00fc\u00b7cke", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "NN", "$,", "VVFIN", "APPR", "PPER", "PTKVZ", "