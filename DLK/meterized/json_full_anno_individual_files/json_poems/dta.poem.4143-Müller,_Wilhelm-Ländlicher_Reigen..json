{"dta.poem.4143": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "L\u00e4ndlicher Reigen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1821", "urn": "urn:nbn:de:kobv:b4-200905194209", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich hab' ein Herz gefunden", "tokens": ["Ich", "hab'", "ein", "Herz", "ge\u00b7fun\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wohl in dem Mond April,", "tokens": ["Wohl", "in", "dem", "Mond", "Ap\u00b7ril", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wo alle Narren wandern:", "tokens": ["Wo", "al\u00b7le", "Nar\u00b7ren", "wan\u00b7dern", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Einen Narren ich nicht will.", "tokens": ["Ei\u00b7nen", "Nar\u00b7ren", "ich", "nicht", "will", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "PTKNEG", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Drum will ich's weiter schicken,", "tokens": ["Drum", "will", "ich's", "wei\u00b7ter", "schi\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PIS", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Bis da\u00df es wird gescheit,", "tokens": ["Bis", "da\u00df", "es", "wird", "ge\u00b7scheit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und kommt es klug zur\u00fccke,", "tokens": ["Und", "kommt", "es", "klug", "zu\u00b7r\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zum Lieben ist's immer noch Zeit.", "tokens": ["Zum", "Lie\u00b7ben", "ist's", "im\u00b7mer", "noch", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "VAFIN", "ADV", "ADV", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}}}}