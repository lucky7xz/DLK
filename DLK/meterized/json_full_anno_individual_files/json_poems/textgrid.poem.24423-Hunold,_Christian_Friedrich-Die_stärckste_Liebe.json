{"textgrid.poem.24423": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Die st\u00e4rckste Liebe", "genre": "verse", "period": "N.A.", "pub_year": 1701, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du sprichst/ Ungl\u00fccklicher/ ich will die Liebe fliehn.", "tokens": ["Du", "sprichst", "/", "Un\u00b7gl\u00fcck\u00b7li\u00b7cher", "/", "ich", "will", "die", "Lie\u00b7be", "fliehn", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "NN", "$(", "PPER", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du hast der Liebe Meer noch lange nicht ergr\u00fcndet.", "tokens": ["Du", "hast", "der", "Lie\u00b7be", "Meer", "noch", "lan\u00b7ge", "nicht", "er\u00b7gr\u00fcn\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "NN", "ADV", "ADV", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wilst du das schn\u00f6de Gifft aus keinem Menschen ziehn/", "tokens": ["Wilst", "du", "das", "schn\u00f6\u00b7de", "Gifft", "aus", "kei\u00b7nem", "Men\u00b7schen", "ziehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "ADJA", "NN", "APPR", "PIAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wie bist du dir so sch\u00f6n/ und in dich selbst entz\u00fcndet?", "tokens": ["Wie", "bist", "du", "dir", "so", "sch\u00f6n", "/", "und", "in", "dich", "selbst", "ent\u00b7z\u00fcn\u00b7det", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "PPER", "ADV", "ADJD", "$(", "KON", "APPR", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Von dieser Liebe trennt den Menschen keine Noth/", "tokens": ["Von", "die\u00b7ser", "Lie\u00b7be", "trennt", "den", "Men\u00b7schen", "kei\u00b7ne", "Noth", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVFIN", "ART", "NN", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Sie dauret bi\u00df ins Grab/ und bringt zuletzt den Todt.", "tokens": ["Sie", "dau\u00b7ret", "bi\u00df", "ins", "Grab", "/", "und", "bringt", "zu\u00b7letzt", "den", "Todt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "NN", "$(", "KON", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Du sprichst/ Ungl\u00fccklicher/ ich will die Liebe fliehn.", "tokens": ["Du", "sprichst", "/", "Un\u00b7gl\u00fcck\u00b7li\u00b7cher", "/", "ich", "will", "die", "Lie\u00b7be", "fliehn", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "NN", "$(", "PPER", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du hast der Liebe Meer noch lange nicht ergr\u00fcndet.", "tokens": ["Du", "hast", "der", "Lie\u00b7be", "Meer", "noch", "lan\u00b7ge", "nicht", "er\u00b7gr\u00fcn\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "NN", "ADV", "ADV", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.ia