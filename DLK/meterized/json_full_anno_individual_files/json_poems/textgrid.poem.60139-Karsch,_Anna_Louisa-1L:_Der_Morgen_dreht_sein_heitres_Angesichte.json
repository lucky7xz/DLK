+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und da\u00df nach Brod nicht meine Sorgen fragen;", "tokens": ["Und", "da\u00df", "nach", "Brod", "nicht", "mei\u00b7ne", "Sor\u00b7gen", "fra\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "APPR", "NN", "PTKNEG", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Dies will mein Gott, dies ist durch ihn.", "tokens": ["Dies", "will", "mein", "Gott", ",", "dies", "ist", "durch", "ihn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPOSAT", "NN", "$,", "PDS", "VAFIN", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Mein ruhig Herz und dieser stille Friede,", "tokens": ["Mein", "ru\u00b7hig", "Herz", "und", "die\u00b7ser", "stil\u00b7le", "Frie\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "KON", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der um mich herrscht, der keinen Tag mich m\u00fcde", "tokens": ["Der", "um", "mich", "herrscht", ",", "der", "kei\u00b7nen", "Tag", "mich", "m\u00fc\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "PPER", "VVFIN", "$,", "PRELS", "PIAT", "NN", "PPER", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Von Arbeit, oder von Verdrusse, sieht;", "tokens": ["Von", "Ar\u00b7beit", ",", "o\u00b7der", "von", "Ver\u00b7drus\u00b7se", ",", "sieht", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "$,", "KON", "APPR", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Das sanfte Feur, das durch die Adern dringet,", "tokens": ["Das", "sanf\u00b7te", "Feur", ",", "das", "durch", "die", "A\u00b7dern", "drin\u00b7get", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und dis Gef\u00fchl, das in mir denkt, und singet,", "tokens": ["Und", "dis", "Ge\u00b7f\u00fchl", ",", "das", "in", "mir", "denkt", ",", "und", "sin\u00b7get", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PDS", "NN", "$,", "PRELS", "APPR", "PPER", "VVFIN", "$,", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Das dank ich dem, der mich durch G\u00fcte zieht.", "tokens": ["Das", "dank", "ich", "dem", ",", "der", "mich", "durch", "G\u00fc\u00b7te", "zieht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ART", "$,", "PRELS", "PRF", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Ich heische nicht aus seinen vollen H\u00e4nden", "tokens": ["Ich", "hei\u00b7sche", "nicht", "aus", "sei\u00b7nen", "vol\u00b7len", "H\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein gr\u00f6sser Gl\u00fcck. Nicht Reichthum soll er senden,", "tokens": ["Ein", "gr\u00f6s\u00b7ser", "Gl\u00fcck", ".", "Nicht", "Reicht\u00b7hum", "soll", "er", "sen\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "PTKNEG", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nicht eiteln Ruhm und was ins Auge f\u00e4llt.", "tokens": ["Nicht", "ei\u00b7teln", "Ruhm", "und", "was", "ins", "Au\u00b7ge", "f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJA", "NN", "KON", "PWS", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mein Mittelstand, der Rock, der reinlich kleidet,", "tokens": ["Mein", "Mit\u00b7tel\u00b7stand", ",", "der", "Rock", ",", "der", "rein\u00b7lich", "klei\u00b7det", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ART", "NN", "$,", "PRELS", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ein gnugsam Brod, genossen unbeneidet,", "tokens": ["Ein", "gnug\u00b7sam", "Brod", ",", "ge\u00b7nos\u00b7sen", "un\u00b7be\u00b7nei\u00b7det", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "$,", "VVPP", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Dies sey mein Theil und bleib es in der Welt.", "tokens": ["Dies", "sey", "mein", "Theil", "und", "bleib", "es", "in", "der", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "NN", "KON", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}