{"textgrid.poem.60139": {"metadata": {"author": {"name": "Karsch, Anna Louisa", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der Morgen dreht sein heitres Angesichte", "genre": "verse", "period": "N.A.", "pub_year": 1761, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Morgen dreht sein heitres Angesichte", "tokens": ["Der", "Mor\u00b7gen", "dreht", "sein", "heit\u00b7res", "An\u00b7ge\u00b7sich\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Uns l\u00e4chelnd zu, und weckt mit sanftem Lichte", "tokens": ["Uns", "l\u00e4\u00b7chelnd", "zu", ",", "und", "weckt", "mit", "sanf\u00b7tem", "Lich\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADJD", "PTKVZ", "$,", "KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Creaturen an den Tag hervor!", "tokens": ["Die", "Crea\u00b7tu\u00b7ren", "an", "den", "Tag", "her\u00b7vor", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Der Sperling schwazt, die muntern H\u00e4hne kr\u00e4hen", "tokens": ["Der", "Sper\u00b7ling", "schwazt", ",", "die", "mun\u00b7tern", "H\u00e4h\u00b7ne", "kr\u00e4\u00b7hen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Den Lobgesang, und aller Augen sehen,", "tokens": ["Den", "Lob\u00b7ge\u00b7sang", ",", "und", "al\u00b7ler", "Au\u00b7gen", "se\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KON", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Zu Gott, der sie ern\u00e4hrt, empor.", "tokens": ["Zu", "Gott", ",", "der", "sie", "er\u00b7n\u00e4hrt", ",", "em\u00b7por", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "PPER", "VVPP", "$,", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Auch ich bin wach, und meinem ersten Blicke", "tokens": ["Auch", "ich", "bin", "wach", ",", "und", "mei\u00b7nem", "ers\u00b7ten", "Bli\u00b7cke"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "VAFIN", "ADJD", "$,", "KON", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Befehl ich, da\u00df er Dank zum Himmel schicke", "tokens": ["Be\u00b7fehl", "ich", ",", "da\u00df", "er", "Dank", "zum", "Him\u00b7mel", "schi\u00b7cke"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "$,", "KOUS", "PPER", "APPR", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "F\u00fcr diese Ruh, f\u00fcr diese sanfte Nacht!", "tokens": ["F\u00fcr", "die\u00b7se", "Ruh", ",", "f\u00fcr", "die\u00b7se", "sanf\u00b7te", "Nacht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,", "APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Es ist ein Gott, der diese Welt regieret,", "tokens": ["Es", "ist", "ein", "Gott", ",", "der", "die\u00b7se", "Welt", "re\u00b7gie\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PRELS", "PDAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Der aus dem Staub mich wunderbar gef\u00fchret,", "tokens": ["Der", "aus", "dem", "Staub", "mich", "wun\u00b7der\u00b7bar", "ge\u00b7f\u00fch\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und der mir Freud und Freunde macht!", "tokens": ["Und", "der", "mir", "Freud", "und", "Freun\u00b7de", "macht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PPER", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Es ist ein Gott! er sah oft meine Z\u00e4hren,", "tokens": ["Es", "ist", "ein", "Gott", "!", "er", "sah", "oft", "mei\u00b7ne", "Z\u00e4h\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$.", "PPER", "VVFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und h\u00f6rte Kinder Brod von mir begehren,", "tokens": ["Und", "h\u00f6r\u00b7te", "Kin\u00b7der", "Brod", "von", "mir", "be\u00b7geh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "NN", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wann lange schon die Mittags-Sonne schien.", "tokens": ["Wann", "lan\u00b7ge", "schon", "die", "Mit\u00b7tags\u00b7Son\u00b7ne", "schien", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Sie sind dahin, die Tage meiner Plagen,", "tokens": ["Sie", "sind", "da\u00b7hin", ",", "die", "Ta\u00b7ge", "mei\u00b7ner", "Pla\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PAV", "$,", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und da\u00df nach Brod nicht meine Sorgen fragen;", "tokens": ["Und", "da\u00df", "nach", "Brod", "nicht", "mei\u00b7ne", "Sor\u00b7gen", "fra\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "APPR", "NN", "PTKNEG", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Dies will mein Gott, dies ist durch ihn.", "tokens": ["Dies", "will", "mein", "Gott", ",", "dies", "ist", "durch", "ihn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPOSAT", "NN", "$,", "PDS", "VAFIN", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Mein ruhig Herz und dieser stille Friede,", "tokens": ["Mein", "ru\u00b7hig", "Herz", "und", "die\u00b7ser", "stil\u00b7le", "Frie\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "KON", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der um mich herrscht, der keinen Tag mich m\u00fcde", "tokens": ["Der", "um", "mich", "herrscht", ",", "der", "kei\u00b7nen", "Tag", "mich", "m\u00fc\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "PPER", "VVFIN", "$,", "PRELS", "PIAT", "NN", "PPER", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Von Arbeit, oder von Verdrusse, sieht;", "tokens": ["Von", "Ar\u00b7beit", ",", "o\u00b7der", "von", "Ver\u00b7drus\u00b7se", ",", "sieht", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "$,", "KON", "APPR", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Das sanfte Feur, das durch die Adern dringet,", "tokens": ["Das", "sanf\u00b7te", "Feur", ",", "das", "durch", "die", "A\u00b7dern", "drin\u00b7get", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und dis Gef\u00fchl, das in mir denkt, und singet,", "tokens": ["Und", "dis", "Ge\u00b7f\u00fchl", ",", "das", "in", "mir", "denkt", ",", "und", "sin\u00b7get", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PDS", "NN", "$,", "PRELS", "APPR", "PPER", "VVFIN", "$,", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Das dank ich dem, der mich durch G\u00fcte zieht.", "tokens": ["Das", "dank", "ich", "dem", ",", "der", "mich", "durch", "G\u00fc\u00b7te", "zieht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ART", "$,", "PRELS", "PRF", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Ich heische nicht aus seinen vollen H\u00e4nden", "tokens": ["Ich", "hei\u00b7sche", "nicht", "aus", "sei\u00b7nen", "vol\u00b7len", "H\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein gr\u00f6sser Gl\u00fcck. Nicht Reichthum soll er senden,", "tokens": ["Ein", "gr\u00f6s\u00b7ser", "Gl\u00fcck", ".", "Nicht", "Reicht\u00b7hum", "soll", "er", "sen\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "PTKNEG", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nicht eiteln Ruhm und was ins Auge f\u00e4llt.", "tokens": ["Nicht", "ei\u00b7teln", "Ruhm", "und", "was", "ins", "Au\u00b7ge", "f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJA", "NN", "KON", "PWS", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mein Mittelstand, der Rock, der reinlich kleidet,", "tokens": ["Mein", "Mit\u00b7tel\u00b7stand", ",", "der", "Rock", ",", "der", "rein\u00b7lich", "klei\u00b7det", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ART", "NN", "$,", "PRELS", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ein gnugsam Brod, genossen unbeneidet,", "tokens": ["Ein", "gnug\u00b7sam", "Brod", ",", "ge\u00b7nos\u00b7sen", "un\u00b7be\u00b7nei\u00b7det", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "$,", "VVPP", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Dies sey mein Theil und bleib es in der Welt.", "tokens": ["Dies", "sey", "mein", "Theil", "und", "bleib", "es", "in", "der", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "NN", "KON", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.