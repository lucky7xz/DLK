{"dta.poem.12003": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "23.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Woher du kamest nicht, und nicht wohin du gehst,", "tokens": ["Wo\u00b7her", "du", "ka\u00b7mest", "nicht", ",", "und", "nicht", "wo\u00b7hin", "du", "gehst", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "PTKNEG", "$,", "KON", "PTKNEG", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Stelle kennst du nur zur Noth, wo nun du stehst.", "tokens": ["Die", "Stel\u00b7le", "kennst", "du", "nur", "zur", "Noth", ",", "wo", "nun", "du", "stehst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$,", "PWAV", "ADV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "So kennst du von der Welt, vom allgemeinen Leben,", "tokens": ["So", "kennst", "du", "von", "der", "Welt", ",", "vom", "all\u00b7ge\u00b7mei\u00b7nen", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Auch End' und Anfang nicht, nur kaum der Mitte Schweben.", "tokens": ["Auch", "End'", "und", "An\u00b7fang", "nicht", ",", "nur", "kaum", "der", "Mit\u00b7te", "Schwe\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "NN", "PTKNEG", "$,", "ADV", "ADV", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Sie geht nach einem Ziel, doch scheint es zu entweichen,", "tokens": ["Sie", "geht", "nach", "ei\u00b7nem", "Ziel", ",", "doch", "scheint", "es", "zu", "ent\u00b7wei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,", "ADV", "VVFIN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du gehst nach einem auch, doch wirst du's nie erreichen.", "tokens": ["Du", "gehst", "nach", "ei\u00b7nem", "auch", ",", "doch", "wirst", "du's", "nie", "er\u00b7rei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADV", "$,", "ADV", "VAFIN", "PIS", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Je h\u00f6her auf du klimmst, je h\u00f6her steigt die Leiter;", "tokens": ["Je", "h\u00f6\u00b7her", "auf", "du", "klimmst", ",", "je", "h\u00f6\u00b7her", "steigt", "die", "Lei\u00b7ter", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "PPER", "VVFIN", "$,", "ADV", "ADJD", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Je weiter spielt die Zeit, dehnt sich der Spielraum weiter.", "tokens": ["Je", "wei\u00b7ter", "spielt", "die", "Zeit", ",", "dehnt", "sich", "der", "Spiel\u00b7raum", "wei\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "ART", "NN", "$,", "VVFIN", "PRF", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "So bleibt dir und der Welt statt alles Zielerringens", "tokens": ["So", "bleibt", "dir", "und", "der", "Welt", "statt", "al\u00b7les", "Zie\u00b7ler\u00b7rin\u00b7gens"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "KON", "ART", "NN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In jedem Nu nur dies Gef\u00fchl des Vorwertsdringens.", "tokens": ["In", "je\u00b7dem", "Nu", "nur", "dies", "Ge\u00b7f\u00fchl", "des", "Vor\u00b7werts\u00b7drin\u00b7gens", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADV", "ADV", "PDS", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Schad' auch um euch, wenn ihr das Ende je gew\u00f6nnet,", "tokens": ["Schad'", "auch", "um", "euch", ",", "wenn", "ihr", "das", "En\u00b7de", "je", "ge\u00b7w\u00f6n\u00b7net", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "PPER", "$,", "KOUS", "PPER", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ihr endlichen, die ihr kein Ende denken k\u00f6nnet!", "tokens": ["Ihr", "end\u00b7li\u00b7chen", ",", "die", "ihr", "kein", "En\u00b7de", "den\u00b7ken", "k\u00f6n\u00b7net", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "$,", "PRELS", "PPER", "PIAT", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}