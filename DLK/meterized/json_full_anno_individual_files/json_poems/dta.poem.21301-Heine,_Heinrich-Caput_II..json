{"dta.poem.21301": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Caput II.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-30602-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein Passagier, der neben mir stand,", "tokens": ["Ein", "Pas\u00b7sa\u00b7gier", ",", "der", "ne\u00b7ben", "mir", "stand", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Bemerkte mir, ich h\u00e4tte", "tokens": ["Be\u00b7merk\u00b7te", "mir", ",", "ich", "h\u00e4t\u00b7te"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "PPER", "VAFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Jetzt vor mir den preu\u00dfischen Zollverein,", "tokens": ["Jetzt", "vor", "mir", "den", "preu\u00b7\u00dfi\u00b7schen", "Zoll\u00b7ver\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Die gro\u00dfe Douanenkette.", "tokens": ["Die", "gro\u00b7\u00dfe", "Dou\u00b7a\u00b7nen\u00b7ket\u00b7te", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+----+-", "measure": "dactylic.init"}}, "stanza.2": {"line.1": {"text": "\u201eder Zollverein\u201c \u2013 bemerkte er \u2013", "tokens": ["\u201e", "der", "Zoll\u00b7ver\u00b7ein", "\u201c", "\u2013", "be\u00b7merk\u00b7te", "er", "\u2013"], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "$(", "$(", "VVFIN", "PPER", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u201ewird unser Volksthum begr\u00fcnden,", "tokens": ["\u201e", "wird", "un\u00b7ser", "Volkst\u00b7hum", "be\u00b7gr\u00fcn\u00b7den", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Er wird das zersplitterte Vaterland", "tokens": ["Er", "wird", "das", "zer\u00b7split\u00b7ter\u00b7te", "Va\u00b7ter\u00b7land"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Zu einem Ganzen verbinden.", "tokens": ["Zu", "ei\u00b7nem", "Gan\u00b7zen", "ver\u00b7bin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "\u201eer giebt die \u00e4u\u00dfere Einheit uns,", "tokens": ["\u201e", "er", "giebt", "die", "\u00e4u\u00b7\u00dfe\u00b7re", "Ein\u00b7heit", "uns", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "ART", "ADJA", "NN", "PPER", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die sogenannt materielle;", "tokens": ["Die", "so\u00b7ge\u00b7nannt", "ma\u00b7te\u00b7ri\u00b7el\u00b7le", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die geistige Einheit giebt uns die Censur,", "tokens": ["Die", "geis\u00b7ti\u00b7ge", "Ein\u00b7heit", "giebt", "uns", "die", "Cen\u00b7sur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Die wahrhaft ideelle \u2013", "tokens": ["Die", "wahr\u00b7haft", "i\u00b7deel\u00b7le", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.4": {"line.1": {"text": "\u201esie giebt die innere Einheit uns,", "tokens": ["\u201e", "sie", "giebt", "die", "in\u00b7ne\u00b7re", "Ein\u00b7heit", "uns", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "ART", "ADJA", "NN", "PPER", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die Einheit im Denken und Sinnen;", "tokens": ["Die", "Ein\u00b7heit", "im", "Den\u00b7ken", "und", "Sin\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "KON", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Ein einiges Deutschland thut uns Noth,", "tokens": ["Ein", "ei\u00b7ni\u00b7ges", "Deutschland", "thut", "uns", "Noth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "NN", "$,"], "meter": "----+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Einig nach Au\u00dfen und Innen.\u201c", "tokens": ["Ei\u00b7nig", "nach", "Au\u00b7\u00dfen", "und", "In\u00b7nen", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADJD", "APPR", "NN", "KON", "NN", "$.", "$("], "meter": "+--+--+-", "measure": "dactylic.tri"}}}}}