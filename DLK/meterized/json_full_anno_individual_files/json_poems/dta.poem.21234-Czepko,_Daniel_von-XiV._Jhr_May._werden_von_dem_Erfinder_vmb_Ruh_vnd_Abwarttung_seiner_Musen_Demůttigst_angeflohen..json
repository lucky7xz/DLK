{"dta.poem.21234": {"metadata": {"author": {"name": "Czepko, Daniel von", "birth": "N.A.", "death": "N.A."}, "title": "XiV.  \n  Jhr May.  \n werden von dem Erfinder vmb Ruh vnd Abwarttung  \n seiner Musen Dem\u016fttigst angeflohen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-20871-8", "language": ["de:0.99"], "booktitle": "Czepko, Daniel von: Triumph Bogen. Breslau, 1641."}, "poem": {"stanza.1": {"line.1": {"text": "Was Gott sonst eintzig gibt/ der Jhnen diese Cron/", "tokens": ["Was", "Gott", "sonst", "eint\u00b7zig", "gibt", "/", "der", "Jh\u00b7nen", "die\u00b7se", "Cron", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "ADV", "ADJD", "VVFIN", "$(", "ART", "PPER", "PDAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als die Sich ", "tokens": ["Als", "die", "Sich"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "ART", "PRF"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Nach diesem fang ich an die Augen auff zuheben/", "tokens": ["Nach", "die\u00b7sem", "fang", "ich", "an", "die", "Au\u00b7gen", "auff", "zu\u00b7he\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "VVFIN", "PPER", "APPR", "ART", "NN", "APPR", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "O hohe Majest\u00e4t/ vnd knie vor Jhren Thron.", "tokens": ["O", "ho\u00b7he", "Ma\u00b7jes\u00b7t\u00e4t", "/", "vnd", "knie", "vor", "Ih\u00b7ren", "Thron", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$(", "KON", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Zwar meine Biete trifft dergleichen ", "tokens": ["Zwar", "mei\u00b7ne", "Bie\u00b7te", "trifft", "derg\u00b7lei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "NN", "VVFIN", "PIS"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Sie Jhm selbst versagt: Ob Sie gleich alles k\u00f6nnen/", "tokens": ["Der", "Sie", "Jhm", "selbst", "ver\u00b7sagt", ":", "Ob", "Sie", "gleich", "al\u00b7les", "k\u00f6n\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPER", "ADV", "VVPP", "$.", "KOUS", "PPER", "ADV", "PIS", "VMFIN", "$("], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "O Kayser/ k\u00f6nnen Sie es Jhnen nicht verg\u00f6nnen:", "tokens": ["O", "Kay\u00b7ser", "/", "k\u00f6n\u00b7nen", "Sie", "es", "Jh\u00b7nen", "nicht", "ver\u00b7g\u00f6n\u00b7nen", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$(", "VMFIN", "PPER", "PPER", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dann meine Musa seufftzt ", "tokens": ["Dann", "mei\u00b7ne", "Mu\u00b7sa", "seufftzt"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Doch wann ich auff di\u00df ", "tokens": ["Doch", "wann", "ich", "auff", "di\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PPER", "APPR", "PDS"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "So hei\u00dft ein gutter Geist mich jmmer n\u00e4her treten/", "tokens": ["So", "hei\u00dft", "ein", "gut\u00b7ter", "Geist", "mich", "jm\u00b7mer", "n\u00e4\u00b7her", "tre\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "PPER", "ADV", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Vnd spricht/ die Biet ist gro\u00df/ die du an jtzt gebeten/", "tokens": ["Vnd", "spricht", "/", "die", "Biet", "ist", "gro\u00df", "/", "die", "du", "an", "jtzt", "ge\u00b7be\u00b7ten", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$(", "ART", "NN", "VAFIN", "ADJD", "$(", "PRELS", "PPER", "APPR", "ADV", "VVPP", "$("], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Nicht ", "tokens": ["Nicht"], "token_info": ["word"], "pos": ["PTKNEG"], "meter": "+", "measure": "single.up"}}}}}