{"textgrid.poem.46218": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "An Amelia Elisabeth", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mit mehr verdienst und wert, dan mit begir und pracht", "tokens": ["Mit", "mehr", "ver\u00b7dienst", "und", "wert", ",", "dan", "mit", "be\u00b7gir", "und", "pracht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "KON", "ADJD", "$,", "ADV", "APPR", "ADJA", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "dem dapfern Hessenland den z\u00fcgel zimlich halten:", "tokens": ["dem", "dap\u00b7fern", "Hes\u00b7sen\u00b7land", "den", "z\u00fc\u00b7gel", "zim\u00b7lich", "hal\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "in kriegs- und fridenszeit, in freindschaft und zwitracht,", "tokens": ["in", "kriegs", "und", "fri\u00b7dens\u00b7zeit", ",", "in", "freind\u00b7schaft", "und", "zwit\u00b7racht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "TRUNC", "KON", "NN", "$,", "APPR", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "mit billichkeit und recht das regiment verwalten:", "tokens": ["mit", "bil\u00b7lich\u00b7keit", "und", "recht", "das", "re\u00b7gi\u00b7ment", "ver\u00b7wal\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "ADJD", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der tugend allzeit hold, mit macht der lastern macht", "tokens": ["Der", "tu\u00b7gend", "all\u00b7zeit", "hold", ",", "mit", "macht", "der", "las\u00b7tern", "macht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "ADJD", "$,", "APPR", "VVFIN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "verst\u00f6ren, da\u00df glaub, lieb und hofnung nicht erkalten:", "tokens": ["ver\u00b7st\u00f6\u00b7ren", ",", "da\u00df", "glaub", ",", "lieb", "und", "hof\u00b7nung", "nicht", "er\u00b7kal\u00b7ten", ":"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "KOUS", "VVFIN", "$,", "ADJD", "KON", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "ohne alle hochfahrt hoch, durch hohe werk volbracht", "tokens": ["oh\u00b7ne", "al\u00b7le", "hoch\u00b7fahrt", "hoch", ",", "durch", "ho\u00b7he", "werk", "vol\u00b7bracht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PIS", "VVFIN", "ADJD", "$,", "APPR", "ADJA", "NN", "VVPP"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.8": {"text": "verhindern, sein lob, ruhm und namen zu veralten:", "tokens": ["ver\u00b7hin\u00b7dern", ",", "sein", "lob", ",", "ruhm", "und", "na\u00b7men", "zu", "ver\u00b7al\u00b7ten", ":"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVINF", "$,", "PPOSAT", "NN", "$,", "NN", "KON", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Der b\u00f6sen straf und forcht, der frommen aufenthalt,", "tokens": ["Der", "b\u00f6\u00b7sen", "straf", "und", "forcht", ",", "der", "from\u00b7men", "auf\u00b7ent\u00b7halt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "KON", "VVFIN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "mit gutem rat und that geschweigen des volks klagen:", "tokens": ["mit", "gu\u00b7tem", "rat", "und", "that", "ge\u00b7schwei\u00b7gen", "des", "volks", "kla\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "VVFIN", "KON", "VVFIN", "VVPP", "ART", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "ist, f\u00fcrstin, eure kunst und eures geists gewalt.", "tokens": ["ist", ",", "f\u00fcrs\u00b7tin", ",", "eu\u00b7re", "kunst", "und", "eu\u00b7res", "geists", "ge\u00b7walt", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "NE", "$,", "PPOSAT", "NN", "KON", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Daher die weite welt mu\u00df billich von euch sagen,", "tokens": ["Da\u00b7her", "die", "wei\u00b7te", "welt", "mu\u00df", "bil\u00b7lich", "von", "euch", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "ADJA", "NN", "VMFIN", "ADJD", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "da\u00df wahre g\u00f6ttin ihr, in menschlicher gestalt,", "tokens": ["da\u00df", "wah\u00b7re", "g\u00f6t\u00b7tin", "ihr", ",", "in", "menschli\u00b7cher", "ge\u00b7stalt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "PPER", "$,", "APPR", "ADJA", "NN", "$,"], "meter"