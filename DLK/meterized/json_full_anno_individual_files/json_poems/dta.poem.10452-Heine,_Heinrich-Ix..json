{"dta.poem.10452": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Ix.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Der Mond ist aufgegangen", "tokens": ["Der", "Mond", "ist", "auf\u00b7ge\u00b7gan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und \u00fcberstrahlt die Well'n;", "tokens": ["Und", "\u00fc\u00b7bers\u00b7trahlt", "die", "Well'n", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich halte mein Liebchen umfangen,", "tokens": ["Ich", "hal\u00b7te", "mein", "Lieb\u00b7chen", "um\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Und unsre Herzen schwell'n.", "tokens": ["Und", "uns\u00b7re", "Her\u00b7zen", "schwell'", "n."], "token_info": ["word", "word", "word", "word", "abbreviation"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "NE"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Im Arm des holden Kindes", "tokens": ["Im", "Arm", "des", "hol\u00b7den", "Kin\u00b7des"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ruh' ich allein am Strand;", "tokens": ["Ruh'", "ich", "al\u00b7lein", "am", "Strand", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Was horchst du bei'm Rauschen des Windes?", "tokens": ["Was", "horchst", "du", "bei'm", "Rau\u00b7schen", "des", "Win\u00b7des", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Was zuckt deine wei\u00dfe Hand?", "tokens": ["Was", "zuckt", "dei\u00b7ne", "wei\u00b7\u00dfe", "Hand", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "\u201edas ist kein Rauschen des Windes,", "tokens": ["\u201e", "das", "ist", "kein", "Rau\u00b7schen", "des", "Win\u00b7des", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PDS", "VAFIN", "PIAT", "NN", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Das ist der Seejungfern Gesang,", "tokens": ["Das", "ist", "der", "See\u00b7jung\u00b7fern", "Ge\u00b7sang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und meine Schwestern sind es,", "tokens": ["Und", "mei\u00b7ne", "Schwes\u00b7tern", "sind", "es", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "PPER", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die einst das Meer verschlang.\u201c", "tokens": ["Die", "einst", "das", "Meer", "ver\u00b7schlang", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADV", "ART", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}