{"dta.poem.19917": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "Die vier wahnsinnigen Br\u00fcder .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905193111", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ausgetrocknet zu Gerippen", "tokens": ["Aus\u00b7ge\u00b7trock\u00b7net", "zu", "Ge\u00b7rip\u00b7pen"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sitzen in des Wahnsinns Haus", "tokens": ["Sit\u00b7zen", "in", "des", "Wahn\u00b7sinns", "Haus"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Vier \u2014 von ihren bleichen Lippen", "tokens": ["Vier", "von", "ih\u00b7ren", "blei\u00b7chen", "Lip\u00b7pen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["CARD", "$(", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Gehet keine Rede aus,", "tokens": ["Ge\u00b7het", "kei\u00b7ne", "Re\u00b7de", "aus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Sitzen einander gegen\u00fcber", "tokens": ["Sit\u00b7zen", "ein\u00b7an\u00b7der", "ge\u00b7gen\u00b7\u00fc\u00b7ber"], "token_info": ["word", "word", "word"], "pos": ["NN", "PRF", "APPR"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Blickend immer holer, tr\u00fcber.", "tokens": ["Bli\u00b7ckend", "im\u00b7mer", "ho\u00b7ler", ",", "tr\u00fc\u00b7ber", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ADJD", "ADV", "ADJD", "$,", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Doch schl\u00e4gt Mitternacht die Stunde", "tokens": ["Doch", "schl\u00e4gt", "Mit\u00b7ter\u00b7nacht", "die", "Stun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "NN", "ART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Str\u00e4ubet sich ihr Haar empor", "tokens": ["Str\u00e4u\u00b7bet", "sich", "ihr", "Haar", "em\u00b7por"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "PPOSAT", "NN", "PTKVZ"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und dann t\u00f6nt aus ihrem Munde", "tokens": ["Und", "dann", "t\u00f6nt", "aus", "ih\u00b7rem", "Mun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Jedesmal in dumpfem Chor:", "tokens": ["Je\u00b7des\u00b7mal", "in", "dum\u00b7pfem", "Chor", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Dies irae dies illa", "tokens": ["Dies", "i\u00b7rae", "dies", "il\u00b7la"], "token_info": ["word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Solvet secla in favilla.", "tokens": ["Sol\u00b7vet", "se\u00b7cla", "in", "fa\u00b7vil\u00b7la", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Waren einst vier schlimme Br\u00fcder,", "tokens": ["Wa\u00b7ren", "einst", "vier", "schlim\u00b7me", "Br\u00fc\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "CARD", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hatten nur gezecht, gel\u00e4rmt,", "tokens": ["Hat\u00b7ten", "nur", "ge\u00b7zecht", ",", "ge\u00b7l\u00e4rmt", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "ADV", "VVPP", "$,", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Beym Gesang verbuhlter Lieder", "tokens": ["Beym", "Ge\u00b7sang", "ver\u00b7buhl\u00b7ter", "Lie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Durch die heil'ge Nacht geschw\u00e4rmt", "tokens": ["Durch", "die", "heil'\u00b7ge", "Nacht", "ge\u00b7schw\u00e4rmt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Keines freundlichen Berathers", "tokens": ["Kei\u00b7nes", "freund\u00b7li\u00b7chen", "Be\u00b7ra\u00b7thers"], "token_info": ["word", "word", "word"], "pos": ["PIAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Warnung half, kein Wort des Vaters.", "tokens": ["War\u00b7nung", "half", ",", "kein", "Wort", "des", "Va\u00b7ters", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "PIAT", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Noch im Sterben sprach der alte", "tokens": ["Noch", "im", "Ster\u00b7ben", "sprach", "der", "al\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "VVFIN", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zu den schlimmen S\u00f6hnen vier:", "tokens": ["Zu", "den", "schlim\u00b7men", "S\u00f6h\u00b7nen", "vier", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "CARD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Warnt euch nicht der Tod, der kalte,", "tokens": ["Warnt", "euch", "nicht", "der", "Tod", ",", "der", "kal\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "ART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Alles f\u00fchret er von hier:", "tokens": ["Al\u00b7les", "f\u00fch\u00b7ret", "er", "von", "hier", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "APPR", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Dies irae dies illa", "tokens": ["Dies", "i\u00b7rae", "dies", "il\u00b7la"], "token_info": ["word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Solvet secla in favilla.", "tokens": ["Sol\u00b7vet", "se\u00b7cla", "in", "fa\u00b7vil\u00b7la", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Und er sprach's und war verschieden,", "tokens": ["Und", "er", "sprach's", "und", "war", "ver\u00b7schie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "NE", "KON", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Jene aber r\u00fchrt es nicht;", "tokens": ["Je\u00b7ne", "a\u00b7ber", "r\u00fchrt", "es", "nicht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch er gieng zum ew'gen Frieden,", "tokens": ["Doch", "er", "gieng", "zum", "ew'\u00b7gen", "Frie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Jene, wie zum Hochgericht,", "tokens": ["Je\u00b7ne", ",", "wie", "zum", "Hoch\u00b7ge\u00b7richt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PWAV", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Treibt es in der Welt Get\u00fcmmel,", "tokens": ["Treibt", "es", "in", "der", "Welt", "Ge\u00b7t\u00fcm\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Nach der H\u00f6lle, fern dem Himmel.", "tokens": ["Nach", "der", "H\u00f6l\u00b7le", ",", "fern", "dem", "Him\u00b7mel", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ADJD", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Und gebuhlet und geschw\u00e4rmet", "tokens": ["Und", "ge\u00b7buh\u00b7let", "und", "ge\u00b7schw\u00e4r\u00b7met"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "KON", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ward es wieder lange Jahr,", "tokens": ["Ward", "es", "wie\u00b7der", "lan\u00b7ge", "Jahr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Andrer Noth sie nie geh\u00e4rmet,", "tokens": ["A\u00b7ndrer", "Noth", "sie", "nie", "ge\u00b7h\u00e4r\u00b7met", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPER", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Keinem greiser ward das Haar.", "tokens": ["Kei\u00b7nem", "grei\u00b7ser", "ward", "das", "Haar", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADJD", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Lust'ge Br\u00fcder! habt nicht Zweifel:", "tokens": ["Lust'\u00b7ge", "Br\u00fc\u00b7der", "!", "habt", "nicht", "Zwei\u00b7fel", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "VAFIN", "PTKNEG", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Eine M\u00e4hr' ist Gott und Teufel.", "tokens": ["Ei\u00b7ne", "M\u00e4hr'", "ist", "Gott", "und", "Teu\u00b7fel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Einst als Mitternacht gekommen", "tokens": ["Einst", "als", "Mit\u00b7ter\u00b7nacht", "ge\u00b7kom\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "KOUS", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kehrten taumelnd sie vom Schmaus,", "tokens": ["Kehr\u00b7ten", "tau\u00b7melnd", "sie", "vom", "Schmaus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Horch! da t\u00f6nt Gesang der Frommen", "tokens": ["Horch", "!", "da", "t\u00f6nt", "Ge\u00b7sang", "der", "From\u00b7men"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$.", "ADV", "VVFIN", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Aus dem nahen Gotteshaus.", "tokens": ["Aus", "dem", "na\u00b7hen", "Got\u00b7tes\u00b7haus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Lasset euer Bell'n, ihr Hunde!", "tokens": ["Las\u00b7set", "eu\u00b7er", "Bell'n", ",", "ihr", "Hun\u00b7de", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Schreyen sie aus Satans Munde.", "tokens": ["Schre\u00b7yen", "sie", "aus", "Sa\u00b7tans", "Mun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "St\u00fcrzen, die verruchten Wichte,", "tokens": ["St\u00fcr\u00b7zen", ",", "die", "ver\u00b7ruch\u00b7ten", "Wich\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Br\u00fcllend durch das heilge Thor,", "tokens": ["Br\u00fcl\u00b7lend", "durch", "das", "heil\u00b7ge", "Thor", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber wie zum Weltgerichte", "tokens": ["A\u00b7ber", "wie", "zum", "Welt\u00b7ge\u00b7rich\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PWAV", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "T\u00f6net hier der ernste Chor:", "tokens": ["T\u00f6\u00b7net", "hier", "der", "erns\u00b7te", "Chor", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Dies irae dies illa", "tokens": ["Dies", "i\u00b7rae", "dies", "il\u00b7la"], "token_info": ["word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Solvet secla in favilla.", "tokens": ["Sol\u00b7vet", "se\u00b7cla", "in", "fa\u00b7vil\u00b7la", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Und ihr Mund \u2014 weit steht er offen,", "tokens": ["Und", "ihr", "Mund", "weit", "steht", "er", "of\u00b7fen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$(", "ADJD", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Doch kein W\u00f6rtlein aus ihm geht,", "tokens": ["Doch", "kein", "W\u00f6rt\u00b7lein", "aus", "ihm", "geht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Gottes Zorn hat sie getroffen,", "tokens": ["Got\u00b7tes", "Zorn", "hat", "sie", "ge\u00b7trof\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Jeder wie ein Steinbild steht,", "tokens": ["Je\u00b7der", "wie", "ein", "Stein\u00b7bild", "steht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "KOKOM", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Grau die Haare, bleich die Wangen.", "tokens": ["Grau", "die", "Haa\u00b7re", ",", "bleich", "die", "Wan\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "ADJD", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wahnsinn hat ihr Haupt befangen.", "tokens": ["Wahn\u00b7sinn", "hat", "ihr", "Haupt", "be\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Ausgetrocknet zu Gerippen", "tokens": ["Aus\u00b7ge\u00b7trock\u00b7net", "zu", "Ge\u00b7rip\u00b7pen"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sitzen in des Wahnsinns Haus", "tokens": ["Sit\u00b7zen", "in", "des", "Wahn\u00b7sinns", "Haus"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nun die vier, \u2014 von ihren Lippen", "tokens": ["Nun", "die", "vier", ",", "von", "ih\u00b7ren", "Lip\u00b7pen"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["ADV", "ART", "CARD", "$,", "$(", "APPR", "PPOSAT", "NN"], "meter": "-++--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Gehet keine Rede aus,", "tokens": ["Ge\u00b7het", "kei\u00b7ne", "Re\u00b7de", "aus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Sitzen einander gegen\u00fcber", "tokens": ["Sit\u00b7zen", "ein\u00b7an\u00b7der", "ge\u00b7gen\u00b7\u00fc\u00b7ber"], "token_info": ["word", "word", "word"], "pos": ["NN", "PRF", "APPR"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Blickend immer holer, tr\u00fcber.", "tokens": ["Bli\u00b7ckend", "im\u00b