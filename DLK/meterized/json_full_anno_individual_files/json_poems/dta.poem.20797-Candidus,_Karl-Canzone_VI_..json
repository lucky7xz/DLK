{"dta.poem.20797": {"metadata": {"author": {"name": "Candidus, Karl", "birth": "N.A.", "death": "N.A."}, "title": "Canzone   VI .", "genre": "Epos", "period": "N.A.", "pub_year": "1854", "urn": "urn:nbn:de:kobv:b4-20090519783", "language": ["de:0.99"], "booktitle": "Candidus, Karl: Der deutsche Christus. F\u00fcnfzehn Canzonen. Leipzig, 1844."}, "poem": {"stanza.1": {"line.1": {"text": "Was sich als Wahrheit tr\u00f6stend senkt in Weh her", "tokens": ["Was", "sich", "als", "Wahr\u00b7heit", "tr\u00f6s\u00b7tend", "senkt", "in", "Weh", "her"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "PRF", "KOUS", "NN", "ADJD", "VVFIN", "APPR", "NN", "APZR"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und was als Irrthum sich um Wahrheit windet,", "tokens": ["Und", "was", "als", "Irr\u00b7thum", "sich", "um", "Wahr\u00b7heit", "win\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "KOUS", "NN", "PRF", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Was fertig scheint und was sich zeigt als Strich nur,", "tokens": ["Was", "fer\u00b7tig", "scheint", "und", "was", "sich", "zeigt", "als", "Strich", "nur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VVFIN", "KON", "PWS", "PRF", "VVFIN", "KOUS", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Was irrend schweift und was zurecht sich findet,", "tokens": ["Was", "ir\u00b7rend", "schweift", "und", "was", "zu\u00b7recht", "sich", "fin\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VVFIN", "KON", "PWS", "VVFIN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Was irgend ist, birgt dich, o Wort, von jeher", "tokens": ["Was", "ir\u00b7gend", "ist", ",", "birgt", "dich", ",", "o", "Wort", ",", "von", "je\u00b7her"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PWS", "ADV", "VAFIN", "$,", "VVFIN", "PPER", "$,", "FM", "NN", "$,", "APPR", "ADV"], "meter": "-+--++-+--+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Und Alles Endliche besteht durch dich nur.", "tokens": ["Und", "Al\u00b7les", "End\u00b7li\u00b7che", "be\u00b7steht", "durch", "dich", "nur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "APPR", "PPER", "ADV", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.7": {"text": "Sieh! was dir scheu entwich nur,", "tokens": ["Sieh", "!", "was", "dir", "scheu", "ent\u00b7wich", "nur", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PWS", "PPER", "ADJD", "ADJD", "ADV", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.8": {"text": "Bleibt Gr\u00f6\u00dfe noch und ist als solche du stets,", "tokens": ["Bleibt", "Gr\u00f6\u00b7\u00dfe", "noch", "und", "ist", "als", "sol\u00b7che", "du", "stets", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "ADV", "KON", "VAFIN", "KOKOM", "PIAT", "PPER", "ADV", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.9": {"text": "Und dich nicht schauen sollt' ich da wo F\u00fclle", "tokens": ["Und", "dich", "nicht", "schau\u00b7en", "sollt'", "ich", "da", "wo", "F\u00fcl\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "PTKNEG", "VVINF", "VMFIN", "PPER", "ADV", "PWAV", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "In bildungsreger H\u00fclle", "tokens": ["In", "bil\u00b7dungs\u00b7re\u00b7ger", "H\u00fcl\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Den Sinn erfreut und nimmt fortw\u00e4hrend zu stets?", "tokens": ["Den", "Sinn", "er\u00b7freut", "und", "nimmt", "fort\u00b7w\u00e4h\u00b7rend", "zu", "stets", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "KON", "VVFIN", "VVPP", "PTKZU", "VVFIN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.12": {"text": "Im Menschen Jesus aber stralt und brennet", "tokens": ["Im", "Men\u00b7schen", "Je\u00b7sus", "a\u00b7ber", "stralt", "und", "bren\u00b7net"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "NE", "ADV", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Dein Licht da\u00df nur, wer blind ist, es verkennet.", "tokens": ["Dein", "Licht", "da\u00df", "nur", ",", "wer", "blind", "ist", ",", "es", "ver\u00b7ken\u00b7net", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KOUS", "ADV", "$,", "PWS", "ADJD", "VAFIN", "$,", "PPER", "VVFIN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Aufging der Menschheit Sonne voller Klarheit", "tokens": ["Auf\u00b7ging", "der", "Menschheit", "Son\u00b7ne", "vol\u00b7ler", "Klar\u00b7heit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "NN", "ADJA", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Im Galil\u00e4er der als dich sich wu\u00dfte,", "tokens": ["Im", "Ga\u00b7li\u00b7l\u00e4\u00b7er", "der", "als", "dich", "sich", "wu\u00df\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "KOUS", "PPER", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Als das Unendlich-Endliche sich kannte,", "tokens": ["Als", "das", "Un\u00b7end\u00b7lich\u00b7End\u00b7li\u00b7che", "sich", "kann\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "--++-+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Und Solches klar und redlich, wie er mu\u00dfte,", "tokens": ["Und", "Sol\u00b7ches", "klar", "und", "red\u00b7lich", ",", "wie", "er", "mu\u00df\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "ADJD", "KON", "ADJD", "$,", "PWAV", "PPER", "VMFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Auch aussprach und bezeugte als die Wahrheit.", "tokens": ["Auch", "aus\u00b7sprach", "und", "be\u00b7zeug\u00b7te", "als", "die", "Wahr\u00b7heit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "VVFIN", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Sein Lieben das die Gottheit Vater nannte", "tokens": ["Sein", "Lie\u00b7ben", "das", "die", "Got\u00b7theit", "Va\u00b7ter", "nann\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "ART", "ART", "NN", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und mild das All umspannte", "tokens": ["Und", "mild", "das", "All", "um\u00b7spann\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "ART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "War Ursach, Werk und Wesen h\u00f6chster Einheit.", "tokens": ["War", "Ur\u00b7sach", ",", "Werk", "und", "We\u00b7sen", "h\u00f6chs\u00b7ter", "Ein\u00b7heit", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "$,", "NN", "KON", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Das war nicht jenes kalte, stolze, arme", "tokens": ["Das", "war", "nicht", "je\u00b7nes", "kal\u00b7te", ",", "stol\u00b7ze", ",", "ar\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["PDS", "VAFIN", "PTKNEG", "PDAT", "ADJA", "$,", "ADJA", "$,", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "\u201ebrahma bin Ich!\u201c nein, warme", "tokens": ["\u201e", "brah\u00b7ma", "bin", "Ich", "!", "\u201c", "nein", ",", "war\u00b7me"], "token_info": ["punct", "word", "word", "word", "punct", "punct", "word", "punct", "word"], "pos": ["$(", "ADJD", "VAFIN", "PPER", "$.", "$(", "PTKANT", "$,", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Gem\u00fctserweiterung zur Allgemeinheit.", "tokens": ["Ge\u00b7m\u00fct\u00b7ser\u00b7wei\u00b7te\u00b7rung", "zur", "All\u00b7ge\u00b7mein\u00b7heit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Fortan hie\u00df Jesus jedes h\u00f6chste Lieben", "tokens": ["For\u00b7tan", "hie\u00df", "Je\u00b7sus", "je\u00b7des", "h\u00f6chs\u00b7te", "Lie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "NE", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Und bist von ihm untrennbar du geblieben.", "tokens": ["Und", "bist", "von", "ihm", "un\u00b7trenn\u00b7bar", "du", "ge\u00b7blie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "PPER", "ADJD", "PPER", "VVPP", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}}, "stanza.3": {"line.1": {"text": "Er war's, der dich zuerst in sich erkannte", "tokens": ["Er", "wa\u00b7r's", ",", "der", "dich", "zu\u00b7erst", "in", "sich", "er\u00b7kann\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "$,", "PRELS", "PPER", "ADV", "APPR", "PRF", "VVFIN"], "meter": "--+-+-+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Und dir Gestalt und Name mochte geben", "tokens": ["Und", "dir", "Ge\u00b7stalt", "und", "Na\u00b7me", "moch\u00b7te", "ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "NN", "KON", "NN", "VMFIN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So wie du ihm, denn du bist er, er du ja.", "tokens": ["So", "wie", "du", "ihm", ",", "denn", "du", "bist", "er", ",", "er", "du", "ja", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "PPER", "PPER", "$,", "KON", "PPER", "VAFIN", "PPER", "$,", "PPER", "PPER", "ADV", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "Des menschlichen Bewu\u00dftseins Licht und Leben,", "tokens": ["Des", "menschli\u00b7chen", "Be\u00b7wu\u00dfts\u00b7eins", "Licht", "und", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "KON", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Und wie bisher ihm unser Weihrauch brannte,", "tokens": ["Und", "wie", "bis\u00b7her", "ihm", "un\u00b7ser", "Weih\u00b7rauch", "brann\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ADV", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "So t\u00f6net ihm der Zukunft Halleluja.", "tokens": ["So", "t\u00f6\u00b7net", "ihm", "der", "Zu\u00b7kunft", "Hal\u00b7le\u00b7lu\u00b7ja", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "NE", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.7": {"text": "Der Welt Unruh und Ruh ja,", "tokens": ["Der", "Welt", "Un\u00b7ruh", "und", "Ruh", "ja", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "NN", "PTKANT", "$,"], "meter": "-+---+-", "measure": "dactylic.init"}, "line.8": {"text": "Sie sind in ihm nur. Schlichtgro\u00df prophezeite", "tokens": ["Sie", "sind", "in", "ihm", "nur", ".", "Schlicht\u00b7gro\u00df", "pro\u00b7phe\u00b7zei\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "ADV", "$.", "FM.la", "FM.la"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Er Solches selbst mit wahrhaft g\u00f6ttlich klarer", "tokens": ["Er", "Sol\u00b7ches", "selbst", "mit", "wahr\u00b7haft", "g\u00f6tt\u00b7lich", "kla\u00b7rer"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "PIS", "ADV", "APPR", "ADV", "ADJD", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Und ewig wunderbarer", "tokens": ["Und", "e\u00b7wig", "wun\u00b7der\u00b7ba\u00b7rer"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJD", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Bestimmtheit dem schwerfassenden Geleite.", "tokens": ["Be\u00b7stimm\u00b7theit", "dem", "schwer\u00b7fas\u00b7sen\u00b7den", "Ge\u00b7lei\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Am Bergquell aber sch\u00f6pften die Genossen", "tokens": ["Am", "Berg\u00b7quell", "a\u00b7ber", "sch\u00f6pf\u00b7ten", "die", "Ge\u00b7nos\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Bis Lebensstr\u00f6me auch von ihnen flossen.", "tokens": ["Bis", "Le\u00b7bens\u00b7str\u00f6\u00b7me", "auch", "von", "ih\u00b7nen", "flos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ein Tropfen dieser Fluten ward gefangen", "tokens": ["Ein", "Trop\u00b7fen", "die\u00b7ser", "Flu\u00b7ten", "ward", "ge\u00b7fan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PDAT", "NN", "VAFIN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In \u00e4rmlich ird'nem Kr\u00fcglein. Welch ein Wunder!", "tokens": ["In", "\u00e4rm\u00b7lich", "ird'\u00b7nem", "Kr\u00fcg\u00b7lein", ".", "Welch", "ein", "Wun\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "$.", "PIAT", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein Brunnen ward's, der wie er flo\u00df und flie\u00dfet,", "tokens": ["Ein", "Brun\u00b7nen", "ward's", ",", "der", "wie", "er", "flo\u00df", "und", "flie\u00b7\u00dfet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$,", "PRELS", "KOKOM", "PPER", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Doch nie versieget noch verarmt bei runder", "tokens": ["Doch", "nie", "ver\u00b7sie\u00b7get", "noch", "ver\u00b7armt", "bei", "run\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "ADV", "VVPP", "APPR", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Umlagerung des Volks, das mit Verlangen", "tokens": ["Um\u00b7la\u00b7ge\u00b7rung", "des", "Volks", ",", "das", "mit", "Ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ART", "NN", "$,", "PRELS", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die Flut sch\u00f6pft, die sich fort und fort ergie\u00dfet.", "tokens": ["Die", "Flut", "sch\u00f6pft", ",", "die", "sich", "fort", "und", "fort", "er\u00b7gie\u00b7\u00dfet", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PRELS", "PRF", "PTKVZ", "KON", "PTKVZ", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Der Born der also schie\u00dfet", "tokens": ["Der", "Born", "der", "al\u00b7so", "schie\u00b7\u00dfet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "ADV", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "In Kraft und Frische, tiefher, klar und klingend,", "tokens": ["In", "Kraft", "und", "Fri\u00b7sche", ",", "tief\u00b7her", ",", "klar", "und", "klin\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,", "ADJA", "$,", "ADJD", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Das ist das h\u00f6chste Schrift gewordne Leben,", "tokens": ["Das", "ist", "das", "h\u00f6chs\u00b7te", "Schrift", "ge\u00b7word\u00b7ne", "Le\u00b7ben", ","], "token_info":