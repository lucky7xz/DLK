ehr", "fest", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PTKNEG", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.18": {"line.1": {"text": "Wo, Tod, sind deine Schrecken,", "tokens": ["Wo", ",", "Tod", ",", "sind", "dei\u00b7ne", "Schre\u00b7cken", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "NN", "$,", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "O H\u00f6lle, wo dein Sieg?", "tokens": ["O", "H\u00f6l\u00b7le", ",", "wo", "dein", "Sieg", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PWAV", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und Satan, wie dich decken", "tokens": ["Und", "Sa\u00b7tan", ",", "wie", "dich", "de\u00b7cken"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "NN", "$,", "PWAV", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In diesem heil'gen Krieg?", "tokens": ["In", "die\u00b7sem", "heil'\u00b7gen", "Krieg", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.19": {"line.1": {"text": "Beschritten ist der Grenze", "tokens": ["Be\u00b7schrit\u00b7ten", "ist", "der", "Gren\u00b7ze"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VAFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Geweihter Zauberkreis,", "tokens": ["Ge\u00b7weih\u00b7ter", "Zau\u00b7ber\u00b7kreis", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nicht mehr um Eichenkr\u00e4nze", "tokens": ["Nicht", "mehr", "um", "Ei\u00b7chen\u00b7kr\u00e4n\u00b7ze"], "token_i