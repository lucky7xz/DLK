{"textgrid.poem.62413": {"metadata": {"author": {"name": "Schenkendorf, Max von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Feuer sind entglommen", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Feuer sind entglommen", "tokens": ["Die", "Feu\u00b7er", "sind", "ent\u00b7glom\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Auf Bergen nah' und fern,", "tokens": ["Auf", "Ber\u00b7gen", "nah'", "und", "fern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKVZ", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ha, Windsbraut, sei willkommen,", "tokens": ["Ha", ",", "Winds\u00b7braut", ",", "sei", "will\u00b7kom\u00b7men", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$,", "NN", "$,", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Willkommen Sturm des Herrn.", "tokens": ["Will\u00b7kom\u00b7men", "Sturm", "des", "Herrn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "O zeuch durch unsre Felder", "tokens": ["O", "zeuch", "durch", "uns\u00b7re", "Fel\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und reinige das Land,", "tokens": ["Und", "rei\u00b7ni\u00b7ge", "das", "Land", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Durch unsre Tannenw\u00e4lder,", "tokens": ["Durch", "uns\u00b7re", "Tan\u00b7nen\u00b7w\u00e4l\u00b7der", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Du Sturm von Gott gesandt.", "tokens": ["Du", "Sturm", "von", "Gott", "ge\u00b7sandt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ihr Th\u00fcrme, hoch erhoben", "tokens": ["Ihr", "Th\u00fcr\u00b7me", ",", "hoch", "er\u00b7ho\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "ADJD", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In freier Himmelsluft,", "tokens": ["In", "frei\u00b7er", "Him\u00b7mel\u00b7sluft", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So zauberisch umwoben", "tokens": ["So", "zau\u00b7be\u00b7risch", "um\u00b7wo\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADJD", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Von blauem Wolkenduft.", "tokens": ["Von", "blau\u00b7em", "Wol\u00b7ken\u00b7duft", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Wie habt ihr oft gerufen", "tokens": ["Wie", "habt", "ihr", "oft", "ge\u00b7ru\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die andachtvolle Schaar,", "tokens": ["Die", "an\u00b7dacht\u00b7vol\u00b7le", "Schaar", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wenn an des Altars Stufen", "tokens": ["Wenn", "an", "des", "Al\u00b7tars", "Stu\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das Heil zu finden war.", "tokens": ["Das", "Heil", "zu", "fin\u00b7den", "war", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Die Wetter oft sich brachen", "tokens": ["Die", "Wet\u00b7ter", "oft", "sich", "bra\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "PRF", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Vor eurem Glockenklang;", "tokens": ["Vor", "eu\u00b7rem", "Glo\u00b7cken\u00b7klang", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nun f\u00fchrt ihr andre Sprachen,", "tokens": ["Nun", "f\u00fchrt", "ihr", "and\u00b7re", "Spra\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Es klingt wie Brautgesang.", "tokens": ["Es", "klingt", "wie", "Braut\u00b7ge\u00b7sang", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Das Land ist aufgestanden \u2013", "tokens": ["Das", "Land", "ist", "auf\u00b7ge\u00b7stan\u00b7den", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein herrlich Osterfest \u2013", "tokens": ["Ein", "herr\u00b7lich", "Os\u00b7ter\u00b7fest", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NE", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ist frei von Sklavenbanden,", "tokens": ["Ist", "frei", "von", "Skla\u00b7ven\u00b7ban\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die hielten nicht mehr fest.", "tokens": ["Die", "hiel\u00b7ten", "nicht", "mehr", "fest", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PTKNEG", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Wo, Tod, sind deine Schrecken,", "tokens": ["Wo", ",", "Tod", ",", "sind", "dei\u00b7ne", "Schre\u00b7cken", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "NN", "$,", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "O H\u00f6lle, wo dein Sieg?", "tokens": ["O", "H\u00f6l\u00b7le", ",", "wo", "dein", "Sieg", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PWAV", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und Satan, wie dich decken", "tokens": ["Und", "Sa\u00b7tan", ",", "wie", "dich", "de\u00b7cken"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "NN", "$,", "PWAV", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In diesem heil'gen Krieg?", "tokens": ["In", "die\u00b7sem", "heil'\u00b7gen", "Krieg", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Beschritten ist der Grenze", "tokens": ["Be\u00b7schrit\u00b7ten", "ist", "der", "Gren\u00b7ze"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VAFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Geweihter Zauberkreis,", "tokens": ["Ge\u00b7weih\u00b7ter", "Zau\u00b7ber\u00b7kreis", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nicht mehr um Eichenkr\u00e4nze", "tokens": ["Nicht", "mehr", "um", "Ei\u00b7chen\u00b7kr\u00e4n\u00b7ze"], "token_info": ["word", "word", "word", "word"], "pos": ["PTKNEG", "ADV", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ficht J\u00fcngling nun und Greis.", "tokens": ["Ficht", "J\u00fcng\u00b7ling", "nun", "und", "Greis", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ADV", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Nun gilt es um das Leben,", "tokens": ["Nun", "gilt", "es", "um", "das", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Es gilt ums h\u00f6chste Gut,", "tokens": ["Es", "gilt", "ums", "h\u00f6chs\u00b7te", "Gut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wir setzen dran, wir geben", "tokens": ["Wir", "set\u00b7zen", "dran", ",", "wir", "ge\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PAV", "$,", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mit Freuden unser Blut.", "tokens": ["Mit", "Freu\u00b7den", "un\u00b7ser", "Blut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Du liebende Gemeine,", "tokens": ["Du", "lie\u00b7ben\u00b7de", "Ge\u00b7mei\u00b7ne", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,"], "meter": "-+---+-", "measure": "dactylic.init"}, "line.2": {"text": "Wie sonst am Tisch des Herrn", "tokens": ["Wie", "sonst", "am", "Tisch", "des", "Herrn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Im gl\u00e4ubigen Vereine,", "tokens": ["Im", "gl\u00e4u\u00b7bi\u00b7gen", "Ver\u00b7ei\u00b7ne", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wie fr\u00f6hlich strahlt dein Stern!", "tokens": ["Wie", "fr\u00f6h\u00b7lich", "strahlt", "dein", "Stern", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Wie lieblich klingt, wie heiter", "tokens": ["Wie", "lieb\u00b7lich", "klingt", ",", "wie", "hei\u00b7ter"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "ADJD", "VVFIN", "$,", "PWAV", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Losung Bibelton:", "tokens": ["Der", "Lo\u00b7sung", "Bi\u00b7bel\u00b7ton", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Die Feuer sind entglommen", "tokens": ["Die", "Feu\u00b7er", "sind", "ent\u00b7glom\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri