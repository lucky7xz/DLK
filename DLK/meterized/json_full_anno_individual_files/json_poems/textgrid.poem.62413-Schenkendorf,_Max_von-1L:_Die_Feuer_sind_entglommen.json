, "ADV", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Im gl\u00e4ubigen Vereine,", "tokens": ["Im", "gl\u00e4u\u00b7bi\u00b7gen", "Ver\u00b7ei\u00b7ne", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wie fr\u00f6hlich strahlt dein Stern!", "tokens": ["Wie", "fr\u00f6h\u00b7lich", "strahlt", "dein", "Stern", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.22": {"line.1": {"text": "Wie lieblich klingt, wie heiter", "tokens": ["Wie", "lieb\u00b7lich", "klingt", ",", "wie", "hei\u00b7ter"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "ADJD", "VVFIN", "$,", "PWAV", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Losung Bibelton:", "tokens": ["Der", "Lo\u00b7sung", "Bi\u00b7bel\u00b7ton", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}