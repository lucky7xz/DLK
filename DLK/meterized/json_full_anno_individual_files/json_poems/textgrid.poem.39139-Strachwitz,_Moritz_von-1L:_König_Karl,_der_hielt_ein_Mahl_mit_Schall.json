{"textgrid.poem.39139": {"metadata": {"author": {"name": "Strachwitz, Moritz von", "birth": "N.A.", "death": "N.A."}, "title": "1L: K\u00f6nig Karl, der hielt ein Mahl mit Schall", "genre": "verse", "period": "N.A.", "pub_year": 1834, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "K\u00f6nig Karl, der hielt ein Mahl mit Schall", "tokens": ["K\u00f6\u00b7nig", "Karl", ",", "der", "hielt", "ein", "Mahl", "mit", "Schall"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "NE", "$,", "PRELS", "VVFIN", "ART", "NN", "APPR", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Im Schlosse zu Paris,", "tokens": ["Im", "Schlos\u00b7se", "zu", "Pa\u00b7ris", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "NE", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Als auf der Jagd von Roncevall", "tokens": ["Als", "auf", "der", "Jagd", "von", "Ron\u00b7ce\u00b7vall"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Roland sein Leben lie\u00df.", "tokens": ["Ro\u00b7land", "sein", "Le\u00b7ben", "lie\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.2": {"line.1": {"text": "K\u00f6nig Karl sprang auf in Angst und Zorn,", "tokens": ["K\u00f6\u00b7nig", "Karl", "sprang", "auf", "in", "Angst", "und", "Zorn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VVFIN", "APPR", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Er horchte lang und tief:", "tokens": ["Er", "horch\u00b7te", "lang", "und", "tief", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbmir ist, als h\u00f6rt' ich Rolands Horn,", "tokens": ["\u00bb", "mir", "ist", ",", "als", "h\u00f6rt'", "ich", "Ro\u00b7lands", "Horn", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "$,", "KOUS", "VVFIN", "PPER", "NE", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das fern um Hilfe rief.", "tokens": ["Das", "fern", "um", "Hil\u00b7fe", "rief", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Mir ist, als h\u00f6rt' ich Olifant,", "tokens": ["Mir", "ist", ",", "als", "h\u00f6rt'", "ich", "O\u00b7li\u00b7fant", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "KOUS", "VVFIN", "PPER", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es hallt aus der spanischen Mark,", "tokens": ["Es", "hallt", "aus", "der", "spa\u00b7ni\u00b7schen", "Mark", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Es hallt her\u00fcber aus Mohrenland", "tokens": ["Es", "hallt", "her\u00b7\u00fc\u00b7ber", "aus", "Moh\u00b7ren\u00b7land"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Gewaltig und zauberstark.", "tokens": ["Ge\u00b7wal\u00b7tig", "und", "zau\u00b7ber\u00b7stark", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Am Ebro k\u00e4mpft mein werter Pair,", "tokens": ["Am", "Eb\u00b7ro", "k\u00e4mpft", "mein", "wer\u00b7ter", "Pair", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NE", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Ritter von Anglant,", "tokens": ["Der", "Rit\u00b7ter", "von", "An\u00b7glant", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Und wenn er dort erschlagen w\u00e4r',", "tokens": ["Und", "wenn", "er", "dort", "er\u00b7schla\u00b7gen", "w\u00e4r'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dann sei mir Gott zur Hand!\u00ab", "tokens": ["Dann", "sei", "mir", "Gott", "zur", "Hand", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "APPRART", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Und tiefe Stille brach herein,", "tokens": ["Und", "tie\u00b7fe", "Stil\u00b7le", "brach", "her\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von wetterschw\u00fcler Art,", "tokens": ["Von", "wet\u00b7ter\u00b7schw\u00fc\u00b7ler", "Art", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es bi\u00df Herr Karl in banger Pein", "tokens": ["Es", "bi\u00df", "Herr", "Karl", "in", "ban\u00b7ger", "Pein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "APPR", "NN", "NE", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den stolzen Silberbart.", "tokens": ["Den", "stol\u00b7zen", "Sil\u00b7ber\u00b7bart", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Da klang es her\u00fcber zum zweitenmal,", "tokens": ["Da", "klang", "es", "her\u00b7\u00fc\u00b7ber", "zum", "zwei\u00b7ten\u00b7mal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPRART", "ADV", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Es klang nicht leis' und lind,", "tokens": ["Es", "klang", "nicht", "leis'", "und", "lind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es schmetterte durch den K\u00f6nigssaal,", "tokens": ["Es", "schmet\u00b7ter\u00b7te", "durch", "den", "K\u00f6\u00b7nigs\u00b7saal", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wie rasender Wirbelwind.", "tokens": ["Wie", "ra\u00b7sen\u00b7der", "Wir\u00b7bel\u00b7wind", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.7": {"line.1": {"text": "Und als zum Dritten das Horn erscholl,", "tokens": ["Und", "als", "zum", "Drit\u00b7ten", "das", "Horn", "er\u00b7scholl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "APPRART", "ADJA", "ART", "NN", "ADJD", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Da borsten Gew\u00f6lb' und Wand,", "tokens": ["Da", "bors\u00b7ten", "Ge\u00b7w\u00f6lb'", "und", "Wand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Da sank der Humpen, Weines voll,", "tokens": ["Da", "sank", "der", "Hum\u00b7pen", ",", "Wei\u00b7nes", "voll", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dem K\u00f6nig aus der Hand.", "tokens": ["Dem", "K\u00f6\u00b7nig", "aus", "der", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Und wie der Ruf durch Hall' und Turm", "tokens": ["Und", "wie", "der", "Ruf", "durch", "Hall'", "und", "Turm"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "NN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zum dritten Mal gegellt,", "tokens": ["Zum", "drit\u00b7ten", "Mal", "ge\u00b7gellt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da hatte des Ritters Atemsturm", "tokens": ["Da", "hat\u00b7te", "des", "Rit\u00b7ters", "A\u00b7tems\u00b7turm"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Das silberne Horn zerschellt.", "