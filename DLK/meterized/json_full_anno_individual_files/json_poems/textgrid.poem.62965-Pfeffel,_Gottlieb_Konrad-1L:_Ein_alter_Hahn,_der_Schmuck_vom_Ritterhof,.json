{"textgrid.poem.62965": {"metadata": {"author": {"name": "Pfeffel, Gottlieb Konrad", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein alter Hahn, der Schmuck vom Ritterhof,", "genre": "verse", "period": "N.A.", "pub_year": 1784, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein alter Hahn, der Schmuck vom Ritterhof,", "tokens": ["Ein", "al\u00b7ter", "Hahn", ",", "der", "Schmuck", "vom", "Rit\u00b7ter\u00b7hof", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Fieng vor Auroren an, den Morgen zu verk\u00fcnden.", "tokens": ["Fi\u00b7eng", "vor", "Au\u00b7ro\u00b7ren", "an", ",", "den", "Mor\u00b7gen", "zu", "ver\u00b7k\u00fcn\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "PTKVZ", "$,", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "\u00bbh\u00f6r auf, rief ein Kapaun, die Ohren mir zu schinden!", "tokens": ["\u00bb", "h\u00f6r", "auf", ",", "rief", "ein", "Ka\u00b7paun", ",", "die", "Oh\u00b7ren", "mir", "zu", "schin\u00b7den", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PTKVZ", "$,", "VVFIN", "ART", "NN", "$,", "ART", "NN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Auch ohne deinen Ruf, Herr Philosoph,", "tokens": ["Auch", "oh\u00b7ne", "dei\u00b7nen", "Ruf", ",", "Herr", "Phi\u00b7lo\u00b7soph", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "$,", "NN", "NE", "$,"]