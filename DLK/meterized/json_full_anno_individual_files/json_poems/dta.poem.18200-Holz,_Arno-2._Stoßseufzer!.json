{"dta.poem.18200": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "2.  \n  Sto\u00dfseufzer!", "genre": "Lyrik", "period": "N.A.", "pub_year": "1886", "urn": "urn:nbn:de:kobv:b4-200905192665", "language": ["de:0.99"], "booktitle": "Holz, Arno: Das Buch der Zeit. Lieder eines Modernen. Z\u00fcrich, 1886."}, "poem": {"stanza.1": {"line.1": {"text": "Ein wahrer Papier-Ocean!", "tokens": ["Ein", "wah\u00b7rer", "Pa\u00b7pier\u00b7O\u00b7cean", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Tagt\u00e4glich drei neue Novellen,", "tokens": ["Tag\u00b7t\u00e4g\u00b7lich", "drei", "neu\u00b7e", "No\u00b7vel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "CARD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Tagt\u00e4glich ein neuer Roman!", "tokens": ["Tag\u00b7t\u00e4g\u00b7lich", "ein", "neu\u00b7er", "Ro\u00b7man", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "In s\u00fc\u00dflicher Selbstpanegyrik", "tokens": ["In", "s\u00fc\u00df\u00b7li\u00b7cher", "Selbst\u00b7pa\u00b7ne\u00b7gy\u00b7rik"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Entw\u00e4ssert in jedes Journal", "tokens": ["Ent\u00b7w\u00e4s\u00b7sert", "in", "je\u00b7des", "Jour\u00b7nal"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "PIAT", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Die unvermeidliche Lyrik", "tokens": ["Die", "un\u00b7ver\u00b7meid\u00b7li\u00b7che", "Ly\u00b7rik"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ihre Thr\u00e4nenk\u00fcbelmoral.", "tokens": ["Ih\u00b7re", "Thr\u00e4\u00b7nen\u00b7k\u00fc\u00b7bel\u00b7mo\u00b7ral", "."], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.3": {"line.1": {"text": "Die Welt ist nimmer die alte,", "tokens": ["Die", "Welt", "ist", "nim\u00b7mer", "die", "al\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ART", "ADJA", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Sie stinkt wie ein Limburger K\u00e4s", "tokens": ["Sie", "stinkt", "wie", "ein", "Lim\u00b7bur\u00b7ger", "K\u00e4s"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KOKOM", "ART", "ADJA", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Und bringt in jeder Spalte", "tokens": ["Und", "bringt", "in", "je\u00b7der", "Spal\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sechs Tohuwabohuessays.", "tokens": ["Sechs", "To\u00b7hu\u00b7wa\u00b7bo\u00b7hu\u00b7es\u00b7says", "."], "token_info": ["word", "word", "punct"], "pos": ["CARD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Der Zeitgeist diktirt seinem Kater", "tokens": ["Der", "Zeit\u00b7geist", "dik\u00b7tirt", "sei\u00b7nem", "Ka\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVPP", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Eine gallige Selbstparodie", "tokens": ["Ei\u00b7ne", "gal\u00b7li\u00b7ge", "Selbst\u00b7pa\u00b7ro\u00b7die"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Und krank liegt das deutsche Theater", "tokens": ["Und", "krank", "liegt", "das", "deut\u00b7sche", "The\u00b7a\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "An chronischer Selbstmordmanie.", "tokens": ["An", "chro\u00b7ni\u00b7scher", "Selbst\u00b7mord\u00b7ma\u00b7nie", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Die Kunst war einst unwiderstehlich,", "tokens": ["Die", "Kunst", "war", "einst", "un\u00b7wi\u00b7der\u00b7steh\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie die Lurlei hoch \u00fcber dem Rhein,", "tokens": ["Wie", "die", "Lur\u00b7lei", "hoch", "\u00fc\u00b7ber", "dem", "Rhein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADJD", "APPR", "ART", "NE", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Doch heute denkt jeder: \u201eO selig,", "tokens": ["Doch", "heu\u00b7te", "denkt", "je\u00b7der", ":", "\u201e", "O", "se\u00b7lig", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PIS", "$.", "$(", "NE", "ADJD", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Ein Wiederk\u00e4uer zu sein!\u201c", "tokens": ["Ein", "Wie\u00b7der\u00b7k\u00e4u\u00b7er", "zu", "sein", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "PTKZU", "VAINF", "$.", "$("], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.6": {"line.1": {"text": "Dort liegen Herrn Hartmanns Schriften,", "tokens": ["Dort", "lie\u00b7gen", "Herrn", "Hart\u00b7manns", "Schrif\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "NE", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Wei\u00df Teufel, der Kerl hat Recht \u2014", "tokens": ["Wei\u00df", "Teu\u00b7fel", ",", "der", "Kerl", "hat", "Recht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "ART", "NN", "VAFIN", "NN", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ich m\u00f6chte die Welt vergiften", "tokens": ["Ich", "m\u00f6ch\u00b7te", "die", "Welt", "ver\u00b7gif\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ART", "NN", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Mit meinem Stiefelknecht!", "tokens": ["Mit", "mei\u00b7nem", "Stie\u00b7fel\u00b7knecht", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}