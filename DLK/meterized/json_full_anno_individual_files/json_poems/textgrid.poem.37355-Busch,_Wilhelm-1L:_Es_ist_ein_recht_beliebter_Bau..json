{"textgrid.poem.37355": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es ist ein recht beliebter Bau.", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es ist ein recht beliebter Bau.", "tokens": ["Es", "ist", "ein", "recht", "be\u00b7lieb\u00b7ter", "Bau", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wer wollte ihn nicht loben?", "tokens": ["Wer", "woll\u00b7te", "ihn", "nicht", "lo\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Drin wohnt ein Mann mit seiner Frau,", "tokens": ["Drin", "wohnt", "ein", "Mann", "mit", "sei\u00b7ner", "Frau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sie unten und er oben.", "tokens": ["Sie", "un\u00b7ten", "und", "er", "o\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "KON", "PPER", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Er, als ein schlaugewiegter Mann,", "tokens": ["Er", ",", "als", "ein", "schlau\u00b7ge\u00b7wieg\u00b7ter", "Mann", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "KOUS", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "H\u00e4lt viel auf weise Lehren,", "tokens": ["H\u00e4lt", "viel", "auf", "wei\u00b7se", "Leh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sie, ungest\u00fcm und drauf und dran,", "tokens": ["Sie", ",", "un\u00b7ge\u00b7st\u00fcm", "und", "drauf", "und", "dran", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ADJD", "KON", "PAV", "KON", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Tut das, was ihr Begehren.", "tokens": ["Tut", "das", ",", "was", "ihr", "Be\u00b7geh\u00b7ren", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PDS", "$,", "PRELS", "PPOSAT", "NN", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}}, "stanza.3": {"line.1": {"text": "Sie l\u00e4\u00dft ihn reden und begeht,", "tokens": ["Sie", "l\u00e4\u00dft", "ihn", "re\u00b7den", "und", "be\u00b7geht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVINF", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Blind, wie sie ist, viel W\u00fcstes,", "tokens": ["Blind", ",", "wie", "sie", "ist", ",", "viel", "W\u00fcs\u00b7tes", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "PPER", "VAFIN", "$,", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und bringt sie das in Schwulit\u00e4t,", "tokens": ["Und", "bringt", "sie", "das", "in", "Schwu\u00b7li\u00b7t\u00e4t", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Na, sagt er k\u00fchl, da siehst es.", "tokens": ["Na", ",", "sagt", "er", "k\u00fchl", ",", "da", "siehst", "es", "."], "token_info"