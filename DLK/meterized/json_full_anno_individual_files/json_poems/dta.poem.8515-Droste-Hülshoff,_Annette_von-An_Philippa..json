{"dta.poem.8515": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "An Philippa.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1860", "urn": "urn:nbn:de:kobv:b4-200905191007", "language": ["de:0.85", "fr:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Im Osten quillt das junge Licht,", "tokens": ["Im", "Os\u00b7ten", "quillt", "das", "jun\u00b7ge", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sein goldner Duft spielt auf den Wellen,", "tokens": ["Sein", "gold\u00b7ner", "Duft", "spielt", "auf", "den", "Wel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und wie ein zartes Traumgesicht", "tokens": ["Und", "wie", "ein", "zar\u00b7tes", "Traum\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Seh ich ein fernes Segel schwellen;", "tokens": ["Seh", "ich", "ein", "fer\u00b7nes", "Se\u00b7gel", "schwel\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.5": {"text": "O k\u00f6nnte ich der M\u00f6ve gleich", "tokens": ["O", "k\u00f6nn\u00b7te", "ich", "der", "M\u00f6\u00b7ve", "gleich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VMFIN", "PPER", "ART", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Umkreisen es im lust\u2019gen Ringen!", "tokens": ["Um\u00b7krei\u00b7sen", "es", "im", "lust'\u00b7gen", "Rin\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "O w\u00e4re mein der L\u00fcfte Reich,", "tokens": ["O", "w\u00e4\u00b7re", "mein", "der", "L\u00fcf\u00b7te", "Reich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPOSAT", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Mein junge, lebensfrische Schwingen.", "tokens": ["Mein", "jun\u00b7ge", ",", "le\u00b7bens\u00b7fri\u00b7sche", "Schwin\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Um dich, Philippa, spielt das Licht,", "tokens": ["Um", "dich", ",", "Phil\u00b7ip\u00b7pa", ",", "spielt", "das", "Licht", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "$,", "NE", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dich hat der Morgenhauch umgeben,", "tokens": ["Dich", "hat", "der", "Mor\u00b7gen\u00b7hauch", "um\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Du bist ein liebes Traumgesicht", "tokens": ["Du", "bist", "ein", "lie\u00b7bes", "Traum\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Am Horizont von meinem Leben;", "tokens": ["Am", "Ho\u00b7ri\u00b7zont", "von", "mei\u00b7nem", "Le\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Seh deine Flagge ich so fern", "tokens": ["Seh", "dei\u00b7ne", "Flag\u00b7ge", "ich", "so", "fern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "PPER", "ADV", "ADJD"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Und tr\u00e4umerisch von Duft umflossen,", "tokens": ["Und", "tr\u00e4u\u00b7me\u00b7risch", "von", "Duft", "um\u00b7flos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Vergessen m\u00f6cht\u2019 ich dann so gern,", "tokens": ["Ver\u00b7ges\u00b7sen", "m\u00f6cht'", "ich", "dann", "so", "gern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "ADV", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Da\u00df sich mein Horizont geschlossen;", "tokens": ["Da\u00df", "sich", "mein", "Ho\u00b7ri\u00b7zont", "ge\u00b7schlos\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Vergessen, da\u00df mein Abend kam,", "tokens": ["Ver\u00b7ges\u00b7sen", ",", "da\u00df", "mein", "A\u00b7bend", "kam", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "KOUS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mein Licht verzittert Funk\u2019 an Funken,", "tokens": ["Mein", "Licht", "ver\u00b7zit\u00b7tert", "Funk'", "an", "Fun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df Zeit mir l\u00e4ngst die Flagge nahm", "tokens": ["Da\u00df", "Zeit", "mir", "l\u00e4ngst", "die", "Flag\u00b7ge", "nahm"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "PPER", "ADV", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und meine Segel l\u00e4ngst gesunken;", "tokens": ["Und", "mei\u00b7ne", "Se\u00b7gel", "l\u00e4ngst", "ge\u00b7sun\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Doch k\u00f6nnen sie nicht jugendlich", "tokens": ["Doch", "k\u00f6n\u00b7nen", "sie", "nicht", "ju\u00b7gend\u00b7lich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "PTKNEG", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und frisch sich neben deinen breiten,", "tokens": ["Und", "frisch", "sich", "ne\u00b7ben", "dei\u00b7nen", "brei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PRF", "APPR", "PPOSAT", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Philippa, lieben kann ich dich", "tokens": ["Phil\u00b7ip\u00b7pa", ",", "lie\u00b7ben", "kann", "ich", "dich"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "VVINF", "VMFIN", "PPER", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und segnend deine Fahrt begleiten.", "tokens": ["Und", "seg\u00b7nend", "dei\u00b7ne", "Fahrt", "be\u00b7glei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}