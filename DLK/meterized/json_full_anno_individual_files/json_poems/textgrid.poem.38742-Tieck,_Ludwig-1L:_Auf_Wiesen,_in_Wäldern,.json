{"textgrid.poem.38742": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Auf Wiesen, in W\u00e4ldern,", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf Wiesen, in W\u00e4ldern,", "tokens": ["Auf", "Wie\u00b7sen", ",", "in", "W\u00e4l\u00b7dern", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "APPR", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "An Str\u00f6men, auf Feldern", "tokens": ["An", "Str\u00f6\u00b7men", ",", "auf", "Fel\u00b7dern"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "$,", "APPR", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Quillt gl\u00fchendes Leben,", "tokens": ["Quillt", "gl\u00fc\u00b7hen\u00b7des", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Die B\u00e4ume sie streben", "tokens": ["Die", "B\u00e4u\u00b7me", "sie", "stre\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PPER", "VVFIN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Zum Himmel hinan.", "tokens": ["Zum", "Him\u00b7mel", "hi\u00b7nan", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Es fliehen mit Eilen", "tokens": ["Es", "flie\u00b7hen", "mit", "Ei\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Die Quellen von steilen", "tokens": ["Die", "Quel\u00b7len", "von", "stei\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Gebirgen und suchen sich ebene Bahn,", "tokens": ["Ge\u00b7bir\u00b7gen", "und", "su\u00b7chen", "sich", "e\u00b7be\u00b7ne", "Bahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "PRF", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.9": {"text": "Durch Dornengestr\u00e4uche,", "tokens": ["Durch", "Dor\u00b7nen\u00b7ge\u00b7str\u00e4u\u00b7che", ","], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "Vor\u00fcber der Eiche,", "tokens": ["Vor\u00b7\u00fc\u00b7ber", "der", "Ei\u00b7che", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.11": {"text": "Dem Wurzelgeflecht;", "tokens": ["Dem", "Wur\u00b7zel\u00b7ge\u00b7flecht", ";"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.12": {"text": "Und rund um die Quelle", "tokens": ["Und", "rund", "um", "die", "Quel\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.13": {"text": "Besieht sich in jeder fortschreitenden Welle", "tokens": ["Be\u00b7sieht", "sich", "in", "je\u00b7der", "fort\u00b7schrei\u00b7ten\u00b7den", "Wel\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "APPR", "PIAT", "ADJA", "NN"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.14": {"text": "Der kindischen Blumen neugierig Geschlecht.", "tokens": ["Der", "kin\u00b7di\u00b7schen", "Blu\u00b7men", "neu\u00b7gie\u00b7rig", "Ge\u00b7schlecht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.15": {"text": "In Steinkl\u00fcften suchen", "tokens": ["In", "Stein\u00b7kl\u00fcf\u00b7ten", "su\u00b7chen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NN", "VVINF"], "meter": "-++-+-", "measure": "unknown.measure.tri"}, "line.16": {"text": "Die hellgr\u00fcnen Buchen", "tokens": ["Die", "hell\u00b7gr\u00fc\u00b7nen", "Bu\u00b7chen"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.17": {"text": "Gen\u00fcgsamen Raum,", "tokens": ["Ge\u00b7n\u00fcg\u00b7sa\u00b7men", "Raum", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.18": {"text": "Sie zittern und nicken", "tokens": ["Sie", "zit\u00b7tern", "und", "ni\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.19": {"text": "Und rauschen und schm\u00fccken", "tokens": ["Und", "rau\u00b7schen", "und", "schm\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "KON", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.20": {"text": "Den felsigen Saum.", "tokens": ["Den", "fel\u00b7si\u00b7gen", "Saum", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.21": {"text": "So findet die Quelle", "tokens": ["So", "fin\u00b7det", "die", "Quel\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.22": {"text": "Der Baum sein Stelle", "tokens": ["Der", "Baum", "sein", "Stel\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.23": {"text": "Und treibet sich's recht:", "tokens": ["Und", "trei\u00b7bet", "sich's", "recht", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "ADJD", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.24": {"text": "So dauert, geneset", "tokens": ["So", "dau\u00b7ert", ",", "ge\u00b7ne\u00b7set"], "token_info": ["word", "word", "punct", "word"], "pos": ["ADV", "VVFIN", "$,", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.25": {"text": "Und stirbt und verweset", "tokens": ["Und", "stirbt", "und", "ver\u00b7we\u00b7set"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "KON", "VVFIN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.26": {"text": "Zufrieden so manches gebohrne Geschlecht. \u2013", "tokens": ["Zu\u00b7frie\u00b7den", "so", "man\u00b7ches", "ge\u00b7bohr\u00b7ne", "Ge\u00b7schlecht", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "ADV", "PIAT", "ADJA", "NN", "$.", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.27": {"text": "Nur der Mensch geht in der Irre,", "tokens": ["Nur", "der", "Mensch", "geht", "in", "der", "Ir\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.28": {"text": "Will heut hier seyn, morgen dort,", "tokens": ["Will", "heut", "hier", "seyn", ",", "mor\u00b7gen", "dort", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ADV", "VAINF", "$,", "ADV", "ADV", "$,"], "meter": "-+----+", "measure": "dactylic.init"}, "line.29": {"text": "Alle Sinne im Gewirre", "tokens": ["Al\u00b7le", "Sin\u00b7ne", "im", "Ge\u00b7wir\u00b7re"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "NN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.30": {"text": "Sucht er stets den fernen Ort.", "tokens": ["Sucht", "er", "stets", "den", "fer\u00b7nen", "Ort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.31": {"text": "Will nicht in der Heimath dauern", "tokens": ["Will", "nicht", "in", "der", "Hei\u00b7math", "dau\u00b7ern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PTKNEG", "APPR", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.32": {"text": "Weithin dehnt er seinen Blick,", "tokens": ["Weit\u00b7hin", "dehnt", "er", "sei\u00b7nen", "Blick", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.33": {"text": "Wandert unter Regenschauern", "tokens": ["Wan\u00b7dert", "un\u00b7ter", "Re\u00b7gen\u00b7schau\u00b7ern"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.34": {"text": "Und sieht dann mit bangem Trauern", "tokens": ["Und", "sieht", "dann", "mit", "ban\u00b7gem", "Trau\u00b7ern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.35": {"text": "Nach dem erst verschm\u00e4hten Gl\u00fcck.", "tokens": ["Nach", "dem", "erst", "ver\u00b7schm\u00e4h\u00b7ten", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADV", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.36": {"text": "Wie in monderhellten Hainen", "tokens": ["Wie", "in", "mon\u00b7der\u00b7hell\u00b7ten", "Hai\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.37": {"text": "Wolken durch den Himmel fliehn,", "tokens": ["Wol\u00b7ken", "durch", "den", "Him\u00b7mel", "fliehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.38": {"text": "Bald die B\u00e4ume gl\u00e4nzend scheinen,", "tokens": ["Bald", "die", "B\u00e4u\u00b7me", "gl\u00e4n\u00b7zend", "schei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.39": {"text": "Schatten wieder abw\u00e4rts ziehn:", "tokens": ["Schat\u00b7ten", "wie\u00b7der", "ab\u00b7w\u00e4rts", "ziehn", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADV", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.40": {"text": "Also auch des Menschen Seele,", "tokens": ["Al\u00b7so", "auch", "des", "Men\u00b7schen", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.41": {"text": "Da\u00df er durch sein ganzes Leben", "tokens": ["Da\u00df", "er", "durch", "sein", "gan\u00b7zes", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.42": {"text": "Rastlos auf und ab sich qu\u00e4le,", "tokens": ["Rast\u00b7los", "auf", "und", "ab", "sich", "qu\u00e4\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "KON", "APPR", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.43": {"text": "Ward die Sehnsucht ihm gegeben. \u2013", "tokens": ["Ward", "die", "Sehn\u00b7sucht", "ihm", "ge\u00b7ge\u00b7ben", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "ART", "NN", "PPER", "VVPP", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.44": {"text": "Doch wohl mir, ich f\u00fchle", "tokens": ["Doch", "wohl", "mir", ",", "ich", "f\u00fch\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ADV", "PPER", "$,", "PPER", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.45": {"text": "Zerreissen dies Band!", "tokens": ["Zer\u00b7reis\u00b7sen", "dies", "Band", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PDS", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.46": {"text": "Ich nahe dem Ziele", "tokens": ["Ich", "na\u00b7he", "dem", "Zie\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "APPR", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.47": {"text": "Das fern und ferner seit lange mir schwand.", "tokens": ["Das", "fern", "und", "fer\u00b7ner", "seit", "lan\u00b7ge", "mir", "schwand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "KON", "ADV", "APPR", "ADV", "PPER", "VVFIN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.48": {"text": "Das b\u00e4ngliche Schwanken,", "tokens": ["Das", "b\u00e4ng\u00b7li\u00b7che", "Schwan\u00b7ken", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.49": {"text": "Das n\u00fcchterne Kranken,", "tokens": ["Das", "n\u00fcch\u00b7ter\u00b7ne", "Kran\u00b7ken", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.50": {"text": "Vor\u00fcber an mir! \u2013", "tokens": ["Vor\u00b7\u00fc\u00b7ber", "an", "mir", "!", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ADV", "APPR", "PPER", "$.", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.51": {"text": "Wie soll ich dir danken?", "tokens": ["Wie", "soll", "ich", "dir", "dan\u00b7ken", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "PPER", "VVINF", "$."], "meter": "+---+-", "measure": "dactylic.init"}, "line.52": {"text": "O Liebste! o sprich, wie vergilt' ich es dir?", "tokens": ["O", "Liebs\u00b7te", "!", "o", "sprich", ",", "wie", "ver\u00b7gilt'", "ich", "es", "dir", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "FM", "ADJD", "$,", "PWAV", "VVFIN", "PPER", "PPER", "PPER", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.2": {"line.1": {"text": "Auf Wiesen, in W\u00e4ldern,", "tokens": ["Auf", "Wie\u00b7sen", ",", "in", "W\u00e4l\u00b7dern", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "APPR", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "An Str\u00f6men, auf Feldern", "tokens": ["An", "Str\u00f6\u00b7men", ",", "auf", "Fel\u00b7dern"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "$,", "APPR", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Quillt gl\u00fchendes Leben,", "tokens": ["Quillt", "gl\u00fc\u00b7hen\u00b7des", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Die B\u00e4ume sie streben", "tokens": ["Die", "B\u00e4u\u00b7me", "sie", "stre\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PPER", "VVFIN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Zum Himmel hinan.", "tokens": ["Zum", "Him\u00b7mel", "hi\u00b7nan", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Es fliehen mit Eilen", "tokens": ["Es", "flie\u00b7hen", "mit", "Ei\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Die Quellen von steilen", "tokens": ["Die", "Quel\u00b7len", "von", "stei\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Gebirgen und suchen sich ebene Bahn,", "tokens": ["Ge\u00b7bir\u00b7gen", "und", "su\u00b7chen", "sich", "e\u00b7be\u00b7ne", "Bahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "PRF", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.9": {"text": "Durch Dornengestr\u00e4uche,", "tokens": ["Durch", "Dor\u00b7nen\u00b7ge\u00b7str\u00e4u\u00b7che", ","], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "Vor\u00fcber der Eiche,", "tokens": ["Vor\u00b7\u00fc\u00b7ber", "der", "Ei\u00b7che", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.11": {"text": "Dem Wurzelgeflecht;", "tokens": ["Dem", "Wur\u00b7zel\u00b7ge\u00b7flecht", ";"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.12": {"text": "Und rund um die Quelle", "tokens": ["Und", "rund", "um", "die", "Quel\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.13": {"text": "Besieht sich in jeder fortschreitenden Welle", "tokens": ["Be\u00b7sieht", "sich", "in", "je\u00b7der", "fort\u00b7schrei\u00b7ten\u00b7den", "Wel\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "APPR", "PIAT", "ADJA", "NN"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.14": {"text": "Der kindischen Blumen neugierig Geschlecht.", "tokens": ["Der", "kin\u00b7di\u00b7schen", "Blu\u00b7men", "neu\u00b7gie\u00b7rig", "Ge\u00b7schlecht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.15": {"text": "In Steinkl\u00fcften suchen", "tokens": ["In", "Stein\u00b7kl\u00fcf\u00b7ten", "su\u00b7chen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NN", "VVINF"], "meter": "-++-+-", "measure": "unknown.measure.tri"}, "line.16": {"text": "Die hellgr\u00fcnen Buchen", "tokens": ["Die", "hell\u00b7gr\u00fc\u00b7nen", "Bu\u00b7chen"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.17": {"text": "Gen\u00fcgsamen Raum,", "tokens": ["Ge\u00b7n\u00fcg\u00b7sa\u00b7men", "Raum", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.18": {"text": "Sie zittern und nicken", "tokens": ["Sie", "zit\u00b7tern", "und", "ni\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.19": {"text": "Und rauschen und schm\u00fccken", "tokens": ["Und", "rau\u00b7schen", "und", "schm\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "KON", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.20": {"text": "Den felsigen Saum.", "tokens": ["Den", "fel\u00b7si\u00b7gen", "Saum", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.21": {"text": "So findet die Quelle", "tokens": ["So", "fin\u00b7det", "die", "Quel\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.22": {"text": "Der Baum sein Stelle", "tokens": ["Der", "Baum", "sein", "Stel\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.23": {"text": "Und treibet sich's recht:", "tokens": ["Und", "trei\u00b7bet", "sich's", "recht", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "ADJD", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.24": {"text": "So dauert, geneset", "tokens": ["So", "dau\u00b7ert", ",", "ge\u00b7ne\u00b7set"], "token_info": ["word", "word", "punct", "word"], "pos": ["ADV", "VVFIN", "$,", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.25": {"text": "Und stirbt und verweset", "tokens": ["Und", "stirbt", "und", "ver\u00b7we\u00b7set"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "KON", "VVFIN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.26": {"text": "Zufrieden so manches gebohrne Geschlecht. \u2013", "tokens": ["Zu\u00b7frie\u00b7den", "so", "man\u00b7ches", "ge\u00b7bohr\u00b7ne", "Ge\u00b7schlecht", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "ADV", "PIAT", "ADJA", "NN", "$.", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.27": {"text": "Nur der Mensch geht in der Irre,", "tokens": ["Nur", "der", "Mensch", "geht", "in", "der", "Ir\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.28": {"text": "Will heut hier seyn, morgen dort,", "tokens": ["Will", "heut", "hier", "seyn", ",", "mor\u00b7gen", "dort", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ADV", "VAINF", "$,", "ADV", "ADV", "$,"], "meter": "-+----+", "measure": "dactylic.init"}, "line.29": {"text": "Alle Sinne im Gewirre", "tokens": ["Al\u00b7le", "Sin\u00b7ne", "im", "Ge\u00b7wir\u00b7re"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "NN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.30": {"text": "Sucht er stets den fernen Ort.", "tokens": ["Sucht", "er", "stets", "den", "fer\u00b7nen", "Ort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.31": {"text": "Will nicht in der Heimath dauern", "tokens": ["Will", "nicht", "in", "der", "Hei\u00b7math", "dau\u00b7ern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PTKNEG", "APPR", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.32": {"text": "Weithin dehnt er seinen Blick,", "tokens": ["Weit\u00b7hin", "dehnt", "er", "sei\u00b7nen", "Blick", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.33": {"text": "Wandert unter Regenschauern", "tokens": ["Wan\u00b7dert", "un\u00b7ter", "Re\u00b7gen\u00b7schau\u00b7ern"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.34": {"text": "Und sieht dann mit bangem Trauern", "tokens": ["Und", "sieht", "dann", "mit", "ban\u00b7gem", "Trau\u00b7ern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.35": {"text": "Nach dem erst verschm\u00e4hten Gl\u00fcck.", "tokens": ["Nach", "dem", "erst", "ver\u00b7schm\u00e4h\u00b7ten", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADV", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.36": {"text": "Wie in monderhellten Hainen", "tokens": ["Wie", "in", "mon\u00b7der\u00b7hell\u00b7ten", "Hai\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.37": {"text": "Wolken durch den Himmel fliehn,", "tokens": ["Wol\u00b7ken", "durch", "den", "Him\u00b7mel", "fliehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.38": {"text": "Bald die B\u00e4ume gl\u00e4nzend scheinen,", "tokens": ["Bald", "die", "B\u00e4u\u00b7me", "gl\u00e4n\u00b7zend", "schei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.39": {"text": "Schatten wieder abw\u00e4rts ziehn:", "tokens": ["Schat\u00b7ten", "wie\u00b7der", "ab\u00b7w\u00e4rts", "ziehn", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADV", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.40": {"text": "Also auch des Menschen Seele,", "tokens": ["Al\u00b7so", "auch", "des", "Men\u00b7schen", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.41": {"text": "Da\u00df er durch sein ganzes Leben", "tokens": ["Da\u00df", "er", "durch", "sein", "gan\u00b7zes", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.42": {"text": "Rastlos auf und ab sich qu\u00e4le,", "tokens": ["Rast\u00b7los", "auf", "und", "ab", "sich", "qu\u00e4\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "KON", "APPR", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.43": {"text": "Ward die Sehnsucht ihm gegeben. \u2013", "tokens": ["Ward", "die", "Sehn\u00b7sucht", "ihm", "ge\u00b7ge\u00b7ben", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "ART", "NN", "PPER", "VVPP", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.44": {"text": "Doch wohl mir, ich f\u00fchle", "tokens": ["Doch", "wohl", "mir", ",", "ich", "f\u00fch\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ADV", "PPER", "$,", "PPER", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.45": {"text": "Zerreissen dies Band!", "tokens": ["Zer\u00b7reis\u00b7sen", "dies", "Band", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PDS", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.46": {"text": "Ich nahe dem Ziele", "tokens": ["Ich", "na\u00b7he", "dem", "Zie\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "APPR", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.47": {"text": "Das fern und ferner seit lange mir schwand.", "tokens": ["Das", "fern", "und", "fer\u00b7ner", "seit", "lan\u00b7ge", "mir", "schwand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "KON", "ADV", "APPR", "ADV", "PPER", "VVFIN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.48": {"text": "Das b\u00e4ngliche Schwanken,", "tokens": ["Das", "b\u00e4ng\u00b7li\u00b7che", "Schwan\u00b7ken", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.49": {"text": "Das n\u00fcchterne Kranken,", "tokens": ["Das", "n\u00fcch\u00b7ter\u00b7ne", "Kran\u00b7ken", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.50": {"text": "Vor\u00fcber an mir! \u2013", "tokens": ["Vor\u00b7\u00fc\u00b7ber", "an", "mir", "!", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ADV", "APPR", "PPER", "$.", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.51": {"text": "Wie soll ich dir danken?", "tokens": ["Wie", "soll", "ich", "dir", "dan\u00b7ken", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "PPER", "VVINF", "$."], "meter": "+---+-", "measure": "dactylic.init"}, "line.52": {"text": "O Liebste! o sprich, wie vergilt' ich es dir?", "tokens": ["O", "Liebs\u00b7te", "!", "o", "sprich", ",", "wie", "ver\u00b7gilt'", "ich", "es", "dir", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "FM", "ADJD", "$,", "PWAV", "VVFIN", "PPER", "PPER", "PPER", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}}}}