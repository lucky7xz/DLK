{"dta.poem.20712": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "Versuch eines Gesanges  \n zur  \n Geburtsfeier Sr. Excellenz des K\u00f6niglich Preu\u00dfischen  \n Kabinets-Ministers  \n  Grafen von Hertzberg .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1792", "urn": "urn:nbn:de:kobv:b4-200905193016", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Horcht immerhin auf Siegesboten-Ton                 ", "tokens": ["Horcht", "im\u00b7mer\u00b7hin", "auf", "Sie\u00b7ges\u00b7bo\u00b7ten\u00b7Ton"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ihr Barden des ber\u00fchmten Praters!", "tokens": ["Ihr", "Bar\u00b7den", "des", "be\u00b7r\u00fchm\u00b7ten", "Pra\u00b7ters", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ihr harret lange, lange schon", "tokens": ["Ihr", "har\u00b7ret", "lan\u00b7ge", ",", "lan\u00b7ge", "schon"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "$,", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auf Thaten eures V\u00f6lkervaters,", "tokens": ["Auf", "Tha\u00b7ten", "eu\u00b7res", "V\u00f6l\u00b7ker\u00b7va\u00b7ters", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und r\u00fcstet euch zu hoher Ode Flug \u2014", "tokens": ["Und", "r\u00fcs\u00b7tet", "euch", "zu", "ho\u00b7her", "O\u00b7de", "Flug"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ADJA", "NN", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Ich singe nicht den Kaiserzug;", "tokens": ["Ich", "sin\u00b7ge", "nicht", "den", "Kai\u00b7ser\u00b7zug", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Was k\u00fcmmert mich die Kriegesflotte,", "tokens": ["Was", "k\u00fcm\u00b7mert", "mich", "die", "Krie\u00b7ges\u00b7flot\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Wohin sie ihren Donner trug \u2014", "tokens": ["Wo\u00b7hin", "sie", "ih\u00b7ren", "Don\u00b7ner", "trug"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Ich singe Dank dem h\u00f6chsten Gotte", "tokens": ["Ich", "sin\u00b7ge", "Dank", "dem", "h\u00f6chs\u00b7ten", "Got\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "F\u00fcr die Geburt des Ersten K\u00f6niglichen Raths", "tokens": ["F\u00fcr", "die", "Ge\u00b7burt", "des", "Ers\u00b7ten", "K\u00f6\u00b7nig\u00b7li\u00b7chen", "Raths"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Zu Kriegs- und Friedenszeiten!", "tokens": ["Zu", "Kriegs", "und", "Frie\u00b7dens\u00b7zei\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "TRUNC", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Er blickt umher und merkt aufs Wohl des Staats,", "tokens": ["Er", "blickt", "um\u00b7her", "und", "merkt", "aufs", "Wohl", "des", "Staats", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "KON", "VVFIN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sein forschend Auge sieht von weiten", "tokens": ["Sein", "for\u00b7schend", "Au\u00b7ge", "sieht", "von", "wei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "VVPP", "NN", "VVFIN", "APPR", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Am Horizont der Monarchien", "tokens": ["Am", "Ho\u00b7ri\u00b7zont", "der", "Mon\u00b7ar\u00b7chi\u00b7en"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN"], "meter": "-+-+-+---", "measure": "unknown.measure.tri"}, "line.4": {"text": "Gewitterw\u00f6lkchen, eh sie sich zusammenziehn.", "tokens": ["Ge\u00b7wit\u00b7ter\u00b7w\u00f6lk\u00b7chen", ",", "eh", "sie", "sich", "zu\u00b7sam\u00b7men\u00b7ziehn", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Er wei\u00df vorher zu \u00fcberdenken,", "tokens": ["Er", "wei\u00df", "vor\u00b7her", "zu", "\u00fc\u00b7ber\u00b7den\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wohin sich ihre Blitze lenken", "tokens": ["Wo\u00b7hin", "sich", "ih\u00b7re", "Blit\u00b7ze", "len\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PRF", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Wei\u00df, ob sie abzuleiten sind,", "tokens": ["Wei\u00df", ",", "ob", "sie", "ab\u00b7zu\u00b7lei\u00b7ten", "sind", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KOUS", "PPER", "VVIZU", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wenn ein Orcan, ein Wirbelwind", "tokens": ["Wenn", "ein", "Or\u00b7can", ",", "ein", "Wir\u00b7bel\u00b7wind"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "ART", "NE", "$,", "ART", "NN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.9": {"text": "Die Wetterwolken n\u00e4her br\u00e4chte \u2014", "tokens": ["Die", "Wet\u00b7ter\u00b7wol\u00b7ken", "n\u00e4\u00b7her", "br\u00e4ch\u00b7te"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Ich aber f\u00fchle nur, wie gern", "tokens": ["Ich", "a\u00b7ber", "f\u00fch\u00b7le", "nur", ",", "wie", "gern"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "ADV", "$,", "PWAV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Mein Lied die Weisheit singen m\u00f6chte,", "tokens": ["Mein", "Lied", "die", "Weis\u00b7heit", "sin\u00b7gen", "m\u00f6ch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Die in ihm gl\u00e4nzet wie ein Stern", "tokens": ["Die", "in", "ihm", "gl\u00e4n\u00b7zet", "wie", "ein", "Stern"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "PPER", "VVFIN", "KOKOM", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Am Himmel dunkler N\u00e4chte.", "tokens": ["Am", "Him\u00b7mel", "dunk\u00b7ler", "N\u00e4ch\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "W\u00e4r ich noch drei\u00dfig Sommer fern", "tokens": ["W\u00e4r", "ich", "noch", "drei\u00b7\u00dfig", "Som\u00b7mer", "fern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "CARD", "NN", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von der Matronenstufe,", "tokens": ["Von", "der", "Mat\u00b7ro\u00b7nen\u00b7stu\u00b7fe", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "H\u00e4tt\u2019 ich mein zw\u00f6lftes Lustrum nicht", "tokens": ["H\u00e4tt'", "ich", "mein", "zw\u00f6lf\u00b7tes", "Lust\u00b7rum", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Schon \u00fcberlebt noch vor dem Rufe,", "tokens": ["Schon", "\u00fc\u00b7ber\u00b7lebt", "noch", "vor", "dem", "Ru\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Dem Friedrich folgte zu der Stufe", "tokens": ["Dem", "Fried\u00b7rich", "folg\u00b7te", "zu", "der", "Stu\u00b7fe"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NE", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Des Throns in Gottes Sonnenlicht:", "tokens": ["Des", "Throns", "in", "Got\u00b7tes", "Son\u00b7nen\u00b7licht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Dann wollt ich einen Hymnus singen", "tokens": ["Dann", "wollt", "ich", "ei\u00b7nen", "Hym\u00b7nus", "sin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Voll Jugend und voll Wonnegluth", "tokens": ["Voll", "Ju\u00b7gend", "und", "voll", "Won\u00b7neg\u00b7luth"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "NN", "KON", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Der Weisheit Herzbergs, und dem Muth,", "tokens": ["Der", "Weis\u00b7heit", "Herz\u00b7bergs", ",", "und", "dem", "Muth", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$,", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Der besser kann durch Hindernisse dringen,", "tokens": ["Der", "bes\u00b7ser", "kann", "durch", "Hin\u00b7der\u00b7nis\u00b7se", "drin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VMFIN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Als Cineas und als M\u00e4cen,", "tokens": ["Als", "Ci\u00b7ne\u00b7as", "und", "als", "M\u00e4\u00b7cen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "KON", "KOUS", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Zwo Staatsminister grauer Zeiten \u2014", "tokens": ["Zwo", "Staats\u00b7mi\u00b7nis\u00b7ter", "grau\u00b7er", "Zei\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ich bin zu schwach, die Stimme zu erh\u00f6hn,", "tokens": ["Ich", "bin", "zu", "schwach", ",", "die", "Stim\u00b7me", "zu", "er\u00b7h\u00f6hn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKA", "ADJD", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Drum bet\u2019 ich nur mit alten Leuten:", "tokens": ["Drum", "bet'", "ich", "nur", "mit", "al\u00b7ten", "Leu\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Gott! la\u00df den Thron gesichert stehn,", "tokens": ["Gott", "!", "la\u00df", "den", "Thron", "ge\u00b7si\u00b7chert", "stehn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VVIMP", "ART", "NN", "VVPP", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den Friedrich Wilhelm zum Besitzen,", "tokens": ["Den", "Fried\u00b7rich", "Wil\u00b7helm", "zum", "Be\u00b7sit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "NE", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Befestiget von Friedrichs Hand,", "tokens": ["Be\u00b7fes\u00b7ti\u00b7get", "von", "Fried\u00b7richs", "Hand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NE", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Weit \u00fcber andre gl\u00e4nzend fand.", "tokens": ["Weit", "\u00fc\u00b7ber", "and\u00b7re", "gl\u00e4n\u00b7zend", "fand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PIS", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Erhalte dieses Thrones St\u00fctzen", "tokens": ["Er\u00b7hal\u00b7te", "die\u00b7ses", "Thro\u00b7nes", "St\u00fct\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "F\u00fcrs Vaterland!", "tokens": ["F\u00fcrs", "Va\u00b7ter\u00b7land", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "La\u00df unersch\u00fcttert, la\u00df noch lange", "tokens": ["La\u00df", "un\u00b7er\u00b7sch\u00fct\u00b7tert", ",", "la\u00df", "noch", "lan\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVIMP", "ADJD", "$,", "VVIMP", "ADV", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Den Pfeiler Herzberg dicht am Fu\u00df des Throns", "tokens": ["Den", "Pfei\u00b7ler", "Herz\u00b7berg", "dicht", "am", "Fu\u00df", "des", "Throns"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NE", "ADJD", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Besungen werden vom Gesange", "tokens": ["Be\u00b7sun\u00b7gen", "wer\u00b7den", "vom", "Ge\u00b7san\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VAFIN", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Des h\u00f6chsten Dichtertons!", "tokens": ["Des", "h\u00f6chs\u00b7ten", "Dich\u00b7ter\u00b7tons", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}