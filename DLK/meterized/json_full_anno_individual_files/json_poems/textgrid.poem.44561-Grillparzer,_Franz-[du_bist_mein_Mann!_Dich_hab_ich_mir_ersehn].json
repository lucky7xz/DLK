{"textgrid.poem.44561": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "[du bist mein Mann! Dich hab ich mir ersehn]", "genre": "verse", "period": "N.A.", "pub_year": 1848, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du bist mein Mann! Dich hab ich mir ersehn", "tokens": ["Du", "bist", "mein", "Mann", "!", "Dich", "hab", "ich", "mir", "er\u00b7sehn"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "$.", "PPER", "VAFIN", "PPER", "PPER", "VVINF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Von allen, die jetzt wirken und jetzt schaffen.", "tokens": ["Von", "al\u00b7len", ",", "die", "jetzt", "wir\u00b7ken", "und", "jetzt", "schaf\u00b7fen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "$,", "PRELS", "ADV", "VVINF", "KON", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nicht, weil du siegst, und was noch sonst geschehn", "tokens": ["Nicht", ",", "weil", "du", "siegst", ",", "und", "was", "noch", "sonst", "ge\u00b7schehn"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "$,", "KOUS", "PPER", "VVFIN", "$,", "KON", "PWS", "ADV", "ADV", "VVINF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Im blutgen Spiel des Streites und der Waffen.", "tokens": ["Im", "blut\u00b7gen", "Spiel", "des", "Strei\u00b7tes", "und", "der", "Waf\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Das haben mehrere gleich dir getan,", "tokens": ["Das", "ha\u00b7ben", "meh\u00b7re\u00b7re", "gleich", "dir", "ge\u00b7tan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PIAT", "ADV", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der eine mehr, die andern etwa minder.", "tokens": ["Der", "ei\u00b7ne", "mehr", ",", "die", "an\u00b7dern", "et\u00b7wa", "min\u00b7der", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADV", "$,", "PRELS", "PIS", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Was gehn den Dichter auch die Waffen an?", "tokens": ["Was", "gehn", "den", "Dich\u00b7ter", "auch", "die", "Waf\u00b7fen", "an", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Er sammelt gern um sich des Friedens Kinder.", "tokens": ["Er", "sam\u00b7melt", "gern", "um", "sich", "des", "Frie\u00b7dens", "Kin\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PRF", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Doch, was in allen Lagen gro\u00df und hehr", "tokens": ["Doch", ",", "was", "in", "al\u00b7len", "La\u00b7gen", "gro\u00df", "und", "hehr"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "$,", "PRELS", "APPR", "PIAT", "NN", "ADJD", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und eigen nur den t\u00fcchtigen Naturen,", "tokens": ["Und", "ei\u00b7gen", "nur", "den", "t\u00fcch\u00b7ti\u00b7gen", "Na\u00b7tu\u00b7ren", ","], "token_info