{"textgrid.poem.26019": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Und jemand geigt im Armenkleid", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dein Haar h\u00e4lt mich schwerer als Ketten gefangen;", "tokens": ["Dein", "Haar", "h\u00e4lt", "mich", "schwe\u00b7rer", "als", "Ket\u00b7ten", "ge\u00b7fan\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ADJD", "KOKOM", "NN", "PTKVZ", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Wenn nur ", "tokens": ["Wenn", "nur"], "token_info": ["word", "word"], "pos": ["KOUS", "ADV"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Klingt meine Kette bis ans Ende der Welt.", "tokens": ["Klingt", "mei\u00b7ne", "Ket\u00b7te", "bis", "ans", "En\u00b7de", "der", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "APPRART", "NN", "ART", "NN", "$."], "meter": "+--+-+-+--+", "measure": "iambic.penta.invert"}, "line.4": {"text": "Alle Rosen sind s\u00fc\u00df wie deine N\u00e4he,", "tokens": ["Al\u00b7le", "Ro\u00b7sen", "sind", "s\u00fc\u00df", "wie", "dei\u00b7ne", "N\u00e4\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "ADJD", "KOKOM", "PPOSAT", "NN", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.5": {"text": "Aber die Rosen werden zu Schmerzen, wenn du mir fern bist:", "tokens": ["A\u00b7ber", "die", "Ro\u00b7sen", "wer\u00b7den", "zu", "Schmer\u00b7zen", ",", "wenn", "du", "mir", "fern", "bist", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word",