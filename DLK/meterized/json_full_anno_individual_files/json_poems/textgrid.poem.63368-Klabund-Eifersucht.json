"ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Von den Wolken auf die Wipfel,", "tokens": ["Von", "den", "Wol\u00b7ken", "auf", "die", "Wip\u00b7fel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Von der Sonne auf die Erde.", "tokens": ["Von", "der", "Son\u00b7ne", "auf", "die", "Er\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Und so sah ich eines Tages \u2013", "tokens": ["Und", "so", "sah", "ich", "ei\u00b7nes", "Ta\u00b7ges", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Vorzustellen: Michael Jaroschin,", "tokens": ["Vor\u00b7zu\u00b7stel\u00b7len", ":", "Mic\u00b7hael", "Ja\u00b7ro\u00b7schin", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$.", "NE", "NE", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Sonnengott von Profession \u2013", "tokens": ["Son\u00b7nen\u00b7gott", "von", "Pro\u00b7fes\u00b7si\u00b7on", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$("], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Sah ich eines Tages nachts", "tokens": ["Sah", "ich", "ei\u00b7nes", "Ta\u00b7ges", "nachts"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPR"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "(jaroschin scheint auch des Nachts),", "tokens": ["(", "ja\u00b7ro\u00b7schin", "scheint", "auch", "des", "Nachts", ")", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "VVFIN", "ADV", "ART", "ADV", "$(", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "Sah ich durch ein unverhangnes", "tokens": ["Sah", "ich", "durch", "ein", "un\u00b7ver\u00b7hang\u00b7nes"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Fenster... die geliebte Frau.", "tokens": ["Fens\u00b7ter", "...", "die", "ge\u00b7lieb\u00b7te", "Frau", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$(", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Sah die liebliche, die liebe,", "tokens": ["Sah", "die", "lieb\u00b7li\u00b7che", ",", "die", "lie\u00b7be", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "$,", "PRELS", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sah die Liebste, die Geliebte \u2013 \u2013 \u2013", "tokens": ["Sah", "die", "Liebs\u00b7te", ",", "die", "Ge\u00b7lieb\u00b7te", "\u2013", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "punct", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "ART", "NN", "$(", "$(", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "In den Armen eines andern \u2013", "tokens": ["In", "den", "Ar\u00b7men", "ei\u00b7nes", "an\u00b7dern", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Eines h\u00f6heren Beamten,", "tokens": ["Ei\u00b7nes", "h\u00f6\u00b7he\u00b7ren", "Be\u00b7am\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Eines niederen Charakters.", "tokens": ["Ei\u00b7nes", "nie\u00b7de\u00b7ren", "Cha\u00b7rak\u00b7ters", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Da erbleichte selbst die Sonne,", "tokens": ["Da", "er\u00b7bleich\u00b7te", "selbst", "die", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Vorzustellen: Michael Jaroschin,", "tokens": ["Vor\u00b7zu\u00b7stel\u00b7len", ":", "Mic\u00b7hael", "Ja\u00b7ro\u00b7schin", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$.", "NE", "NE", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Hob den goldnen Sonnendolch und", "tokens": ["Hob", "den", "gold\u00b7nen", "Son\u00b7nen\u00b7dolch", "und"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "ART", "ADJA", "NN", "KON"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Stie\u00df ihn strahlend durch das Fenster,", "tokens": ["Stie\u00df", "ihn", "strah\u00b7lend", "durch", "das", "Fens\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Stie\u00df dem Mann ihn in den Nacken,", "tokens": ["Stie\u00df", "dem", "Mann", "ihn", "in", "den", "Na\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Fuhr der Dolch da durc