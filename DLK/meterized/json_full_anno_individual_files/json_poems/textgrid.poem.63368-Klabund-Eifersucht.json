"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Weiber vor dem Sonnengotte!", "tokens": ["Wei\u00b7ber", "vor", "dem", "Son\u00b7nen\u00b7got\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ihn betrog die Sonnenfrau,", "tokens": ["Ihn", "be\u00b7trog", "die", "Son\u00b7nen\u00b7frau", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und sie mu\u00dfte darum sterben.", "tokens": ["Und", "sie", "mu\u00df\u00b7te", "da\u00b7rum", "ster\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PAV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Vorzustellen: Michael Jaroschin", "tokens": ["Vor\u00b7zu\u00b7stel\u00b7len", ":", "Mic\u00b7hael", "Ja\u00b7ro\u00b7schin"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$.", "NE", "NE"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "H\u00e4lt die Wacht im Irrenhause", "tokens": ["H\u00e4lt", "die", "Wacht", "im", "Ir\u00b7ren\u00b7hau\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Als ein R\u00e4cher seiner Ehre,", "tokens": ["Als", "ein", "R\u00e4\u00b7cher", "sei\u00b7ner", "Eh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "R\u00e4cher jeder Mannesehre.", "tokens": ["R\u00e4\u00b7cher", "je\u00b7der", "Man\u00b7ne\u00b7seh\u00b7re", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PIAT