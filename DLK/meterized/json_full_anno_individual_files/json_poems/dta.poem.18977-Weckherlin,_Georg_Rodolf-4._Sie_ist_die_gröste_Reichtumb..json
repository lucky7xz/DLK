{"dta.poem.18977": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "4.  \n  Sie ist die gr\u00f6ste Reichtumb.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-200905198111", "language": ["de:0.99"], "booktitle": "Weckherlin, Georg Rodolf: Gaistliche und Weltliche Gedichte. Amsterdam, 1641."}, "poem": {"stanza.1": {"line.1": {"text": "Das pr\u00e4chtigste Kriegs schiff/ dem je das Meer", "tokens": ["Das", "pr\u00e4ch\u00b7tigs\u00b7te", "Kriegs", "schiff", "/", "dem", "je", "das", "Meer"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$(", "ART", "ADV", "ART", "NN"], "meter": "-+---+-+-+", "measure": "dactylic.init"}, "line.2": {"text": "war kund/", "tokens": ["war", "kund", "/"], "token_info": ["word", "word", "punct"], "pos": ["VAFIN", "PTKVZ", "$("], "meter": "--", "measure": "unknown.measure.zero"}, "line.3": {"text": "Hat keinen mast so hoch/ als hoch ist mein begehr\u1ebd;", "tokens": ["Hat", "kei\u00b7nen", "mast", "so", "hoch", "/", "als", "hoch", "ist", "mein", "be\u00b7gehr\u1ebd", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "ADV", "ADJD", "$(", "KOUS", "ADJD", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Kein \u00e4ncker halb so starck vn\u0303 beissend in den gr\u0169d/", "tokens": ["Kein", "\u00e4n\u00b7cker", "halb", "so", "starck", "v\u00f1", "beis\u00b7send", "in", "den", "gr\u0169d", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADJD", "ADV", "ADJD", "ADJD", "VVPP", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Als meine lieb vn\u0303 trew/ die vnauffh\u00f6rlich wehren.", "tokens": ["Als", "mei\u00b7ne", "lieb", "v\u00f1", "trew", "/", "die", "vnauff\u00b7h\u00f6r\u00b7lich", "weh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJD", "ADV", "ADJD", "$(", "ART", "ADJD", "VVINF", "$."], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "So kn\u00fcpfet auch kein sayl noch leyn ein solchen bund/", "tokens": ["So", "kn\u00fcp\u00b7fet", "auch", "kein", "sayl", "noch", "leyn", "ein", "sol\u00b7chen", "bund", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PIAT", "NN", "ADV", "ADV", "ART", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als die zart krause haar/ die meinen gaist be-", "tokens": ["Als", "die", "zart", "krau\u00b7se", "haar", "/", "die", "mei\u00b7nen", "gaist", "be"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJD", "ADJA", "NN", "$(", "ART", "PPOSAT", "NN", "TRUNC"], "meter": "--++-+-+-+-", "measure": "anapaest.init"}, "line.3": {"text": "schweren;", "tokens": ["schwe\u00b7ren", ";"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "Kein wind blie\u00df jemahl auff die seegel stoltz vnd", "tokens": ["Kein", "wind", "blie\u00df", "je\u00b7mahl", "auff", "die", "see\u00b7gel", "stoltz", "vnd"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VVFIN", "ADV", "APPR", "ART", "NN", "ADJD", "KON"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "rund", "tokens": ["rund"], "token_info": ["word"], "pos": ["ADJD"], "meter": "+", "measure": "single.up"}, "line.6": {"text": "Als mich die s\u00fcsse lufft des rohten munds beth\u00f6re\u0304.", "tokens": ["Als", "mich", "die", "s\u00fcs\u00b7se", "lufft", "des", "roh\u00b7ten", "munds", "bet\u00b7h\u00f6r\u0113", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Kein Schiffm\u0101 hat jemahls in einer schwartze\u0304 nacht", "tokens": ["Kein", "Schiff\u00b7m\u0101", "hat", "je\u00b7mahls", "in", "ei\u00b7ner", "schwartz\u0113", "nacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VAFIN", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Ein halb so klares liecht oder gestirn erblicket", "tokens": ["Ein", "halb", "so", "kla\u00b7res", "liecht", "o\u00b7der", "ge\u00b7stirn", "er\u00b7bli\u00b7cket"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "ADV", "ADJA", "NN", "KON", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Als hell seind de augstern/ mein trost vnd Amors", "tokens": ["Als", "hell", "seind", "de", "augs\u00b7tern", "/", "mein", "trost", "vnd", "A\u00b7mors"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "ADJD", "VAFIN", "NE", "VVINF", "$(", "PPOSAT", "NN", "KON", "NE"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "pracht:", "tokens": ["pracht", ":"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+", "measure": "single.up"}}, "stanza.4": {"line.1": {"text": "So hat auch noch kein schiff/ nach langer fahrt be-", "tokens": ["So", "hat", "auch", "noch", "kein", "schiff", "/", "nach", "lan\u00b7ger", "fahrt", "be"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "ADV", "PIAT", "VVFIN", "$(", "APPR", "ADJA", "VVFIN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "glicket/", "tokens": ["gli\u00b7cket", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Ein Kleinoth so viel wehrt zu vns v\u00f5 Ost gebracht/", "tokens": ["Ein", "Klei\u00b7noth", "so", "viel", "wehrt", "zu", "vns", "v\u00f5", "Ost", "ge\u00b7bracht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADV", "VVFIN", "APPR", "PPER", "NE", "NE", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Als dises Kleinoth ist das alle welt erquicket.", "tokens": ["Als", "di\u00b7ses", "Klei\u00b7noth", "ist", "das", "al\u00b7le", "welt", "er\u00b7quic\u00b7ket", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "VAFIN", "ART", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}