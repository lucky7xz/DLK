], "pos": ["KON", "ART", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Und der Geschmack mit der hellen Stirne.", "tokens": ["Und", "der", "Ge\u00b7schmack", "mit", "der", "hel\u00b7len", "Stir\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.22": {"line.1": {"text": "Schmidt, der mir gleich ist, den die Unsterblichen", "tokens": ["Schmidt", ",", "der", "mir", "gleich", "ist", ",", "den", "die", "U\u00b7nsterb\u00b7li\u00b7chen"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "PRELS", "PPER", "ADV", "VAFIN", "$,", "PRELS", "ART", "NN"], "meter": "+---+--+---", "measure": "trochaic.tri.relaxed"}, "line.2": {"text": "Des Hains Ges\u00e4ngen neben mir auferziehn!", "tokens": ["Des", "Hains", "Ge\u00b7s\u00e4n\u00b7gen", "ne\u00b7ben", "mir", "auf\u00b7er\u00b7ziehn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und Rothe, der sich freyer Weisheit", "tokens": ["Und", "Ro\u00b7the", ",", "der", "sich", "frey\u00b7er", "Weis\u00b7heit"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "NN", "$,", "PRELS", "PRF", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und der vertrauteren Freundschaft weihte.", "tokens": ["Und", "der", "ver\u00b7trau\u00b7te\u00b7ren", "Freund\u00b7schaft", "weih\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}}}}