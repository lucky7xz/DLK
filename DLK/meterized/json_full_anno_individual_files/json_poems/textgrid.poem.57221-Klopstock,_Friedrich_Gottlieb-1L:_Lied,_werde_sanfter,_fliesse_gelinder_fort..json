{"textgrid.poem.57221": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "1L: Lied, werde sanfter, fliesse gelinder fort.", "genre": "verse", "period": "N.A.", "pub_year": 1763, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Lied, werde sanfter, fliesse gelinder fort.", "tokens": ["Lied", ",", "wer\u00b7de", "sanf\u00b7ter", ",", "flies\u00b7se", "ge\u00b7lin\u00b7der", "fort", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VAFIN", "ADJD", "$,", "VVFIN", "ADJD", "PTKVZ", "$."], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Wie auf die Rosen hell aus des Morgens Hand", "tokens": ["Wie", "auf", "die", "Ro\u00b7sen", "hell", "aus", "des", "Mor\u00b7gens", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN", "ADJD", "APPR", "ART", "ADV", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Der Thau herabtr\u00e4uft, denn dort k\u00f6mt er", "tokens": ["Der", "Thau", "her\u00b7ab\u00b7tr\u00e4uft", ",", "denn", "dort", "k\u00f6mt", "er"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "KON", "ADV", "VVFIN", "PPER"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Fr\u00f6hlicher heut und entw\u00f6lkt mein Gellert.", "tokens": ["Fr\u00f6h\u00b7li\u00b7cher", "heut", "und", "ent\u00b7w\u00f6lkt", "mein", "Gel\u00b7lert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Dich soll der sch\u00f6nsten Mutter geliebteste", "tokens": ["Dich", "soll", "der", "sch\u00f6ns\u00b7ten", "Mut\u00b7ter", "ge\u00b7lieb\u00b7tes\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ART", "ADJA", "NN", "ADJA"], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Und sch\u00f6nste Tochter lesen, und reizender", "tokens": ["Und", "sch\u00f6ns\u00b7te", "Toch\u00b7ter", "le\u00b7sen", ",", "und", "rei\u00b7zen\u00b7der"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ADJA", "NN", "VVINF", "$,", "KON", "ADJA"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Im Lesen werden, dich in Unschuld,", "tokens": ["Im", "Le\u00b7sen", "wer\u00b7den", ",", "dich", "in", "Un\u00b7schuld", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAINF", "$,", "PRF", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sieht sie dich etwa wo schlummern, k\u00fcssen.", "tokens": ["Sieht", "sie", "dich", "et\u00b7wa", "wo", "schlum\u00b7mern", ",", "k\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "PWAV", "VVFIN", "$,", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Auf meinem Schooss, in meinen Umarmungen", "tokens": ["Auf", "mei\u00b7nem", "Scho\u00b7oss", ",", "in", "mei\u00b7nen", "Um\u00b7ar\u00b7mun\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "APPR", "PPOSAT", "NN"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Soll einst die Freundin, welche mich lieben wird,", "tokens": ["Soll", "einst", "die", "Freun\u00b7din", ",", "wel\u00b7che", "mich", "lie\u00b7ben", "wird", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "NN", "$,", "PRELS", "PPER", "VVINF", "VAFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Dein s\u00fcss Geschw\u00e4tz mir sanft erz\u00e4hlen,", "tokens": ["Dein", "s\u00fcss", "Ge\u00b7schw\u00e4tz", "mir", "sanft", "er\u00b7z\u00e4h\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und es zugleich an der Hand als Mutter", "tokens": ["Und", "es", "zu\u00b7gleich", "an", "der", "Hand", "als", "Mut\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "ADV", "APPR", "ART", "NN", "KOUS", "NN"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.4": {"line.1": {"text": "Die kleine Zilie lehren. Des Herzens Werth", "tokens": ["Die", "klei\u00b7ne", "Zi\u00b7lie", "leh\u00b7ren", ".", "Des", "Her\u00b7zens", "Werth"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVINF", "$.", "ART", "NN", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Zeigt auf dem Schauplatz keiner mit jenem Reiz,", "tokens": ["Zeigt", "auf", "dem", "Schau\u00b7platz", "kei\u00b7ner", "mit", "je\u00b7nem", "Reiz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "PIS", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Den du ihm gabst. Da einst die beyden", "tokens": ["Den", "du", "ihm", "gabst", ".", "Da", "einst", "die", "bey\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "PPER", "PPER", "VVFIN", "$.", "ADV", "ADV", "ART", "PIAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Edleren M\u00e4dchen mit stiller Grossmuth,", "tokens": ["Ed\u00b7le\u00b7ren", "M\u00e4d\u00b7chen", "mit", "stil\u00b7ler", "Gross\u00b7muth", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.5": {"line.1": {"text": "Euch unnachahmbar, welchen nur Sch\u00f6nheit bl\u00fcht,", "tokens": ["Euch", "un\u00b7nac\u00b7hahm\u00b7bar", ",", "wel\u00b7chen", "nur", "Sch\u00f6n\u00b7heit", "bl\u00fcht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "$,", "PWAT", "ADV", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Sich in die Blumen setzten, da weint' ich, Freund,", "tokens": ["Sich", "in", "die", "Blu\u00b7men", "setz\u00b7ten", ",", "da", "weint'", "ich", ",", "Freund", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "$,", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Da flossen ungesehne Thr\u00e4nen", "tokens": ["Da", "flos\u00b7sen", "un\u00b7ge\u00b7seh\u00b7ne", "Thr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Aus dem ger\u00fchrten entz\u00fcckten Auge.", "tokens": ["Aus", "dem", "ge\u00b7r\u00fchr\u00b7ten", "ent\u00b7z\u00fcck\u00b7ten", "Au\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "Da schwebte lange freudiger Ernst um mich.", "tokens": ["Da", "schweb\u00b7te", "lan\u00b7ge", "freu\u00b7di\u00b7ger", "Ernst", "um", "mich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ADJA", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "O Tugend! rief ich, Tugend, wie sch\u00f6n bist du!", "tokens": ["O", "Tu\u00b7gend", "!", "rief", "ich", ",", "Tu\u00b7gend", ",", "wie", "sch\u00f6n", "bist", "du", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "VVFIN", "PPER", "$,", "NN", "$,", "PWAV", "ADJD", "VAFIN", "PPER", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Welch g\u00f6ttlich Meisterst\u00fcck sind Seelen,", "tokens": ["Welch", "g\u00f6tt\u00b7lich", "Meis\u00b7ter\u00b7st\u00fcck", "sind", "See\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VAFIN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die sich hinauf bis zu dir erheben!", "tokens": ["Die", "sich", "hin\u00b7auf", "bis", "zu", "dir", "er\u00b7he\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "ADV", "ADV", "APPR", "PPER", "VVINF", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.7": {"line.1": {"text": "Der du uns auch liebst, Olde, kom n\u00e4her her,", "tokens": ["Der", "du", "uns", "auch", "liebst", ",", "Ol\u00b7de", ",", "kom", "n\u00e4\u00b7her", "her", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PRF", "ADV", "VVFIN", "$,", "NE", "$,", "VVFIN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Du Kenner, der du edel und feuervoll,", "tokens": ["Du", "Ken\u00b7ner", ",", "der", "du", "e\u00b7del", "und", "feu\u00b7er\u00b7voll", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "PRELS", "PPER", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Unbiegsam beyden, beyden furchtbar,", "tokens": ["Un\u00b7bieg\u00b7sam", "bey\u00b7den", ",", "bey\u00b7den", "furcht\u00b7bar", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "PIAT", "$,", "PIAT", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "St\u00fcmper der Tugend und Schriften hassest!", "tokens": ["St\u00fcm\u00b7per", "der", "Tu\u00b7gend", "und", "Schrif\u00b7ten", "has\u00b7sest", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "KON", "NN", "VAFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.8": {"line.1": {"text": "Du, der bald Zweifler, und Philosoph bald war,", "tokens": ["Du", ",", "der", "bald", "Zweif\u00b7ler", ",", "und", "Phi\u00b7lo\u00b7soph", "bald", "war", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "ADV", "NN", "$,", "KON", "NN", "ADV", "VAFIN", "$,"], "meter": "---+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Bald Sp\u00f6tter aller menschlichen Handlungen,", "tokens": ["Bald", "Sp\u00f6t\u00b7ter", "al\u00b7ler", "menschli\u00b7chen", "Hand\u00b7lun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-++-", "measure": "unknown.measure.penta"}, "line.3": {"text": "Bald Miltons, und Homerus Priester,", "tokens": ["Bald", "Mil\u00b7tons", ",", "und", "Ho\u00b7me\u00b7rus", "Pries\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "$,", "KON", "NE", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Bald Misanthrope, bald Freund, bald Dichter,", "tokens": ["Bald", "Mi\u00b7san\u00b7thro\u00b7pe", ",", "bald", "Freund", ",", "bald", "Dich\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "NN", "$,", "ADV", "NN", "$,", "ADV", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.9": {"line.1": {"text": "Viel Zeiten, K\u00fchnert, hast du schon durchgelebt,", "tokens": ["Viel", "Zei\u00b7ten", ",", "K\u00fch\u00b7nert", ",", "hast", "du", "schon", "durch\u00b7ge\u00b7lebt", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "NE", "$,", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Von Eisen Zeiten, silberne, goldene!", "tokens": ["Von", "Ei\u00b7sen", "Zei\u00b7ten", ",", "sil\u00b7ber\u00b7ne", ",", "gol\u00b7de\u00b7ne", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "VVFIN", "$,", "ADJA", "$."], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Kom, Freund, kom wieder zu des Britten", "tokens": ["Kom", ",", "Freund", ",", "kom", "wie\u00b7der", "zu", "des", "Brit\u00b7ten"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "NN", "$,", "VVFIN", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zeit, und zur\u00fcck zu des M\u00e4oniden!", "tokens": ["Zeit", ",", "und", "zu\u00b7r\u00fcck", "zu", "des", "M\u00e4o\u00b7ni\u00b7den", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "ADV", "APPR", "ART", "NN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}}, "stanza.10": {"line.1": {"text": "Noch zween erblick' ich. Den hat vereintes Blut,", "tokens": ["Noch", "zween", "er\u00b7blick'", "ich", ".", "Den", "hat", "ver\u00b7ein\u00b7tes", "Blut", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "VVFIN", "PPER", "$.", "NE", "VAFIN", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Mehr noch die Freundschaft, z\u00e4rtlich mir zugesellt,", "tokens": ["Mehr", "noch", "die", "Freund\u00b7schaft", ",", "z\u00e4rt\u00b7lich", "mir", "zu\u00b7ge\u00b7sellt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "$,", "ADJD", "PPER", "VVPP", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und den des Umgangs s\u00fcsse Reizung,", "tokens": ["Und", "den", "des", "Um\u00b7gangs", "s\u00fcs\u00b7se", "Rei\u00b7zung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Und der Geschmack mit der hellen Stirne.", "tokens": ["Und", "der", "Ge\u00b7schmack", "mit", "der", "hel\u00b7len", "Stir\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.11": {"line.1": {"text": "Schmidt, der mir gleich ist, den die Unsterblichen", "tokens": ["Schmidt", ",", "der", "mir", "gleich", "ist", ",", "den", "die", "U\u00b7nsterb\u00b7li\u00b7chen"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "PRELS", "PPER", "ADV", "VAFIN", "$,", "PRELS", "ART", "NN"], "meter": "+---+--+---", "measure": "trochaic.tri.relaxed"}, "line.2": {"text": "Des Hains Ges\u00e4ngen neben mir auferziehn!", "tokens": ["Des", "Hains", "Ge\u00b7s\u00e4n\u00b7gen", "ne\u00b7ben", "mir", "auf\u00b7er\u00b7ziehn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und Rothe, der sich freyer Weisheit", "tokens": ["Und", "Ro\u00b7the", ",", "der", "sich", "frey\u00b7er", "Weis\u00b7heit"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "NN", "$,", "PRELS", "PRF", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und der vertrauteren Freundschaft weihte.", "tokens": ["Und", "der", "ver\u00b7trau\u00b7te\u00b7ren", "Freund\u00b7schaft", "weih\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.12": {"line.1": {"text": "Lied, werde sanfter, fliesse gelinder fort.", "tokens": ["Lied", ",", "wer\u00b7de", "sanf\u00b7ter", ",", "flies\u00b7se", "ge\u00b7lin\u00b7der", "fort", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VAFIN", "ADJD", "$,", "VVFIN", "ADJD", "PTKVZ", "$."], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Wie auf die Rosen hell aus des Morgens Hand", "tokens": ["Wie", "auf", "die", "Ro\u00b7sen", "hell", "aus", "des", "Mor\u00b7gens", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN", "ADJD", "APPR", "ART", "ADV", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Der Thau herabtr\u00e4uft, denn dort k\u0