{"textgrid.poem.54295": {"metadata": {"author": {"name": "Ziegler, Christiana Mariana von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mop\u00df sitzt recht straff zu Pferd, wenn er bi\u00dfweilen reit,", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mop\u00df sitzt recht straff zu Pferd, wenn er bi\u00dfweilen reit,", "tokens": ["Mop\u00df", "sitzt", "recht", "straff", "zu", "Pferd", ",", "wenn", "er", "bi\u00df\u00b7wei\u00b7len", "reit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADJD", "VVFIN", "APPR", "NN", "$,", "KOUS", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Gassen sind so schmal, die Strassen nicht recht breit,", "tokens": ["Die", "Gas\u00b7sen", "sind", "so", "schmal", ",", "die", "Stras\u00b7sen", "nicht", "recht", "breit", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "$,", "ART", "NN", "PTKNEG", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und wann die Marilis sich an dem Fenster zeiget,", "tokens": ["Und", "wann", "die", "Ma\u00b7ri\u00b7lis", "sich", "an", "dem", "Fens\u00b7ter", "zei\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NE", "PRF", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So sieht man, wie sich gleich der straffe Ritter beuget.", "tokens": ["So", "sieht", "man", ",", "wie", "sich", "gleich", "der", "straf\u00b7fe", "Rit\u00b7ter", "beu\u00b7get", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "$,", "PWAV", "PRF", "ADV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Da denckt der arme Tropf, wie wohl er ihr gefiel;", "tokens": ["Da", "denckt", "der", "ar\u00b7me", "Tropf", ",", "wie", "wohl", "er", "ihr", "ge\u00b7fiel", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,", "PWAV", "ADV", "PPER", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Nein, Ritter, du verfehlst den rechten Zweck und Ziel,", "tokens": ["Nein", ",", "Rit\u00b7ter", ",", "du", "ver\u00b7fehlst", "den", "rech\u00b7ten", "Zweck", "und", "Ziel", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "NN", "$,", "PPER", "VVFIN", "ART", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Denn man verliebt sich nicht so gleich in Staats-Peruquen,", "tokens": ["Denn", "man", "ver\u00b7liebt", "sich", "nicht", "so", "gleich", "in", "Staats\u00b7Pe\u00b7ru\u00b7quen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "PRF", "PTKNEG", "ADV", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.8": {"text": "In einen grossen Hut, den Busch und Federn schm\u00fccken.", "tokens": ["In", "ei\u00b7nen", "gros\u00b7sen", "Hut", ",", "den", "Busch", "und", "Fe\u00b7dern", "schm\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "ART", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "In ein bordirtes Kleid und Strumpff mit Gold gestickt,", "tokens": ["In", "ein", "bor\u00b7dir\u00b7tes", "Kleid", "und", "Strumpff", "mit", "Gold", "ge\u00b7stickt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "KON", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Weil doch der Beutel offt gar mager ausgespickt.", "tokens": ["Weil", "doch", "der", "Beu\u00b7tel", "offt", "gar", "ma\u00b7ger", "aus\u00b7ge\u00b7spickt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "ADV", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Wer deine Prahlerey, dein d\u00fcrfftig Wesen kennet,", "tokens": ["Wer", "dei\u00b7ne", "Prah\u00b7le\u00b7rey", ",", "dein", "d\u00fcr\u00b7ff\u00b7tig", "We\u00b7sen", "ken\u00b7net", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "$,", "PPOSAT", "ADJD", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "Der spottet deiner Pracht, wenn Gaul und Herre rennet.", "tokens": ["Der", "spot\u00b7tet", "dei\u00b7ner", "Pracht", ",", "wenn", "Gaul", "und", "Her\u00b7re", "ren\u00b7net", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPOSAT", "NN", "$,", "KOUS", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Man wei\u00df, da\u00df nur dein Staat mit Borgen anschafft,", "tokens": ["Man", "wei\u00df", ",", "da\u00df", "nur", "dein", "Staat", "mit", "Bor\u00b7gen", "an\u00b7schafft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "KOUS", "ADV", "PPOSAT", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.14": {"text": "Worein sich Marilis vermuthlich nicht vergafft.", "tokens": ["Wo\u00b7rein", "sich", "Ma\u00b7ri\u00b7lis", "ver\u00b7muth\u00b7lich", "nicht", "ver\u00b7gafft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "NE", "ADV", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Jedoch will diese nicht, so gibts wohl andre Tocken,", "tokens": ["Je\u00b7doch", "will", "die\u00b7se", "nicht", ",", "so", "gibts", "wohl", "and\u00b7re", "To\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PDS", "PTKNEG", "$,", "ADV", "VVFIN", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Die man durch Prung und Staat wei\u00df listig anzulocken.", "tokens": ["Die", "man", "durch", "Prung", "und", "Staat", "wei\u00df", "lis\u00b7tig", "an\u00b7zu\u00b7lo\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "APPR", "NN", "KON", "NN", "VVFIN", "ADJD", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Wie mancher zeiget sich so pr\u00e4chtig unsrer Welt,", "tokens": ["Wie", "man\u00b7cher", "zei\u00b7get", "sich", "so", "pr\u00e4ch\u00b7tig", "uns\u00b7rer", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VVFIN", "PRF", "ADV", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Da\u00df jeder ihn vor reich und starck beg\u00fctert h\u00e4lt,", "tokens": ["Da\u00df", "je\u00b7der", "ihn", "vor", "reich", "und", "starck", "be\u00b7g\u00fc\u00b7tert", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "APPR", "ADJD", "KON", "ADJD", "VVPP", "VVFIN", "$,"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.19": {"text": "So lange bi\u00df das Volck, zu seinen Spott, erf\u00e4hret,", "tokens": ["So", "lan\u00b7ge", "bi\u00df", "das", "Volck", ",", "zu", "sei\u00b7nen", "Spott", ",", "er\u00b7f\u00e4h\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ART", "NN", "$,", "APPR", "PPOSAT", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Da\u00df eine Compagnie von Weibern ihn ern\u00e4hret.", "tokens": ["Da\u00df", "ei\u00b7ne", "Com\u00b7pag\u00b7nie", "von", "Wei\u00b7bern", "ihn", "er\u00b7n\u00e4h\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPR", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Geniessest du nun auch ein solch Stipendium,", "tokens": ["Ge\u00b7nies\u00b7sest", "du", "nun", "auch", "ein", "solch", "Sti\u00b7pen\u00b7di\u00b7um", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "ART", "PIAT", "NN", "$,"], "meter": "-+----+-+--+", "measure": "iambic.tetra.chol"}, "line.22": {"text": "So w\u00e4r es in der That recht abgeschmackt und tumm,", "tokens": ["So", "w\u00e4r", "es", "in", "der", "That", "recht", "ab\u00b7ge\u00b7schmackt", "und", "tumm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "ART", "NN", "ADJD", "VVPP", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Wann du die H\u00fcner liest, die dich ernehren, stehen,", "tokens": ["Wann", "du", "die", "H\u00fc\u00b7ner", "liest", ",", "die", "dich", "er\u00b7neh\u00b7ren", ",", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "VVFIN", "$,", "PRELS", "PRF", "VVINF", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Und unterdessen woltst nach frembden H\u00f6fen gehen.", "tokens": ["Und", "un\u00b7ter\u00b7des\u00b7sen", "woltst", "nach", "fremb\u00b7den", "H\u00f6\u00b7fen", "ge\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VMFIN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Mop\u00df sitzt recht straff zu Pferd, wenn er bi\u00dfweilen reit,", "tokens": ["Mop\u00df", "sitzt", "recht", "straff", "zu", "Pferd", ",", "wenn", "er", "bi\u00df\u00b7wei\u00b7len", "reit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADJD", "VVFIN", "APPR", "NN", "$,", "KOUS", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Gassen sind so schmal, die Strassen nicht recht breit,", "tokens": ["Die", "Gas\u00b7sen", "sind", "so", "schmal", ",", "die", "Stras\u00b7sen", "nicht", "recht", "breit", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN",