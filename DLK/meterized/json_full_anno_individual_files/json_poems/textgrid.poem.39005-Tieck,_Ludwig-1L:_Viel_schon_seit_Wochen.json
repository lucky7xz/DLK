{"textgrid.poem.39005": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Viel schon seit Wochen", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Viel schon seit Wochen", "tokens": ["Viel", "schon", "seit", "Wo\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Verdank' ich dir, du hohe Stiege,", "tokens": ["Ver\u00b7dank'", "ich", "dir", ",", "du", "ho\u00b7he", "Stie\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPER", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Mein freundlicher Nachbar.", "tokens": ["Mein", "freund\u00b7li\u00b7cher", "Nach\u00b7bar", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "So wie die Gl\u00e4ubigen fromm", "tokens": ["So", "wie", "die", "Gl\u00e4u\u00b7bi\u00b7gen", "fromm"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "ART", "NN", "ADJD"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Dort am Lateran", "tokens": ["Dort", "am", "La\u00b7te\u00b7ran"], "token_info": ["word", "word", "word"], "pos": ["ADV", "APPRART", "NN"], "meter": "--+-+", "measure": "anapaest.init"}, "line.6": {"text": "Auf heiliger Staffel knien,", "tokens": ["Auf", "hei\u00b7li\u00b7ger", "Staf\u00b7fel", "kni\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "So nun seit Wochen", "tokens": ["So", "nun", "seit", "Wo\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "Wandl' ich, wenn die hei\u00dfe Mittagssonne", "tokens": ["Wandl'", "ich", ",", "wenn", "die", "hei\u00b7\u00dfe", "Mit\u00b7tags\u00b7son\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "$,", "KOUS", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "Brennend nieder scheint,", "tokens": ["Bren\u00b7nend", "nie\u00b7der", "scheint", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.10": {"text": "Die edlen Stufen auf und ab,", "tokens": ["Die", "ed\u00b7len", "Stu\u00b7fen", "auf", "und", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Schau mich oben um,", "tokens": ["Schau", "mich", "o\u00b7ben", "um", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "PTKVZ", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.12": {"text": "Erblicke unter mir Rom,", "tokens": ["Er\u00b7bli\u00b7cke", "un\u00b7ter", "mir", "Rom", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "NE", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.13": {"text": "Und dort den Vatikan und Peters Dom,", "tokens": ["Und", "dort", "den", "Va\u00b7ti\u00b7kan", "und", "Pe\u00b7ters", "Dom", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "KON", "NE", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Steige wieder hinab,", "tokens": ["Stei\u00b7ge", "wie\u00b7der", "hin\u00b7ab", ","], "token_i