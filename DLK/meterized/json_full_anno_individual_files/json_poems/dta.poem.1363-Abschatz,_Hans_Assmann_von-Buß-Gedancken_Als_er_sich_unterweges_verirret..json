{"dta.poem.1363": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Bu\u00df-Gedancken/  \n Als er sich unterweges verirret.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Mein Gang verirret sich/ doch noch vielmehr mein Leben/", "tokens": ["Mein", "Gang", "ver\u00b7ir\u00b7ret", "sich", "/", "doch", "noch", "viel\u00b7mehr", "mein", "Le\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PRF", "$(", "ADV", "ADV", "ADV", "PPOSAT", "NN", "$("], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Mein ungewisser Schlu\u00df w\u00e4hlt nicht den rechten Pfad/", "tokens": ["Mein", "un\u00b7ge\u00b7wis\u00b7ser", "Schlu\u00df", "w\u00e4hlt", "nicht", "den", "rech\u00b7ten", "Pfad", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "PTKNEG", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der wanckelhaffte Sinn erkiest nicht gutten Rath:", "tokens": ["Der", "wan\u00b7ckel\u00b7haff\u00b7te", "Sinn", "er\u00b7kiest", "nicht", "gut\u00b7ten", "Rath", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PTKNEG", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Das schwache Wollen folgt des Fleisches Widerstreben/", "tokens": ["Das", "schwa\u00b7che", "Wol\u00b7len", "folgt", "des", "Flei\u00b7sches", "Wi\u00b7der\u00b7stre\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ich f\u00fchle die Vernunfft mit meiner Regung streiten/", "tokens": ["Ich", "f\u00fch\u00b7le", "die", "Ver\u00b7nunfft", "mit", "mei\u00b7ner", "Re\u00b7gung", "strei\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ich wei\u00df fast/ was ich thun und was ich meiden soll.", "tokens": ["Ich", "wei\u00df", "fast", "/", "was", "ich", "thun", "und", "was", "ich", "mei\u00b7den", "soll", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$(", "PWS", "PPER", "VVINF", "KON", "PWS", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Doch jenes f\u00e4llt mir schwer/ und di\u00df gef\u00e4llt mir wohl/", "tokens": ["Doch", "je\u00b7nes", "f\u00e4llt", "mir", "schwer", "/", "und", "di\u00df", "ge\u00b7f\u00e4llt", "mir", "wohl", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "PPER", "ADJD", "$(", "KON", "PDS", "VVFIN", "PPER", "ADV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Des Geistes Krafft entsinckt/ der Fu\u00df beginnt zu gleiten.", "tokens": ["Des", "Geis\u00b7tes", "Krafft", "ent\u00b7sinckt", "/", "der", "Fu\u00df", "be\u00b7ginnt", "zu", "glei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVPP", "$(", "ART", "NN", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Das Aug\u2019 erlustigt sich an fremd-verbotner Wahre/", "tokens": ["Das", "Aug'", "er\u00b7lus\u00b7tigt", "sich", "an", "frem\u00b7dver\u00b7bot\u00b7ner", "Wah\u00b7re", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Das Ohre nimmt mit Lust die s\u00fcsse Reitzung ein/", "tokens": ["Das", "Oh\u00b7re", "nimmt", "mit", "Lust", "die", "s\u00fcs\u00b7se", "Reit\u00b7zung", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "ART", "ADJA", "NN", "ART", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Das Hertze giebet nach/ ber\u00fcckt durch gutten Schein/", "tokens": ["Das", "Hert\u00b7ze", "gie\u00b7bet", "nach", "/", "be\u00b7r\u00fcckt", "durch", "gut\u00b7ten", "Schein", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "VVFIN", "APPR", "$(", "VVFIN", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Die matte Tugend liegt erstorben auff der Bahre.", "tokens": ["Die", "mat\u00b7te", "Tu\u00b7gend", "liegt", "ers\u00b7tor\u00b7ben", "auff", "der", "Bah\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "VVPP", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Wo aber will ich hin? Des dicken Waldes Schatten", "tokens": ["Wo", "a\u00b7ber", "will", "ich", "hin", "?", "Des", "di\u00b7cken", "Wal\u00b7des", "Schat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "VMFIN", "PPER", "PTKVZ", "$.", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Vermehrt der langen Nacht betr\u00fcbte Dunckelheit.", "tokens": ["Ver\u00b7mehrt", "der", "lan\u00b7gen", "Nacht", "be\u00b7tr\u00fcb\u00b7te", "Dun\u00b7ckel\u00b7heit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "VVFIN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Ich seh und h\u00f6re nichts/ als wie die Eule schreyt/", "tokens": ["Ich", "seh", "und", "h\u00f6\u00b7re", "nichts", "/", "als", "wie", "die", "Eu\u00b7le", "schreyt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PIS", "$(", "KOUS", "KOKOM", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Wie Kr\u00f6t und Otter pfeifft/ wo bin ich hingerathen?", "tokens": ["Wie", "Kr\u00f6t", "und", "Ot\u00b7ter", "pfeifft", "/", "wo", "bin", "ich", "hin\u00b7ge\u00b7ra\u00b7then", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NE", "VVFIN", "$(", "PWAV", "VAFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Der erst-gebahnte Weg will nach und nach verschwinden/", "tokens": ["Der", "er\u00b7stge\u00b7bahn\u00b7te", "Weg", "will", "nach", "und", "nach", "ver\u00b7schwin\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "APPR", "KON", "APPR", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Ich halte voller Furcht auff unbetretner Bahn/", "tokens": ["Ich", "hal\u00b7te", "vol\u00b7ler", "Furcht", "auff", "un\u00b7be\u00b7tret\u00b7ner", "Bahn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "In Hecken und Morast die m\u00fcden Z\u00fcgel an/", "tokens": ["In", "He\u00b7cken", "und", "Mo\u00b7rast", "die", "m\u00fc\u00b7den", "Z\u00fc\u00b7gel", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "ADV", "ART", "ADJA", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Wei\u00df weder vor mich hin/ noch hinter mich zu finden/", "tokens": ["Wei\u00df", "we\u00b7der", "vor", "mich", "hin", "/", "noch", "hin\u00b7ter", "mich", "zu", "fin\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "APPR", "PPER", "PTKVZ", "$(", "ADV", "APPR", "PPER", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Die Freude geht vorbey/ die Wollust ist verschwunden/", "tokens": ["Die", "Freu\u00b7de", "geht", "vor\u00b7bey", "/", "die", "Wol\u00b7lust", "ist", "ver\u00b7schwun\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$(", "ART", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Gelegenheit entflieht/ die schnelle Zeit flie\u00dft hin/", "tokens": ["Ge\u00b7le\u00b7gen\u00b7heit", "ent\u00b7flieht", "/", "die", "schnel\u00b7le", "Zeit", "flie\u00dft", "hin", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$(", "ART", "ADJA", "NN", "VVFIN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Furcht und Betr\u00fcbnis bleibt der leidige Gewinn", "tokens": ["Furcht", "und", "Be\u00b7tr\u00fcb\u00b7nis", "bleibt", "der", "lei\u00b7di\u00b7ge", "Ge\u00b7winn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.24": {"text": "Von aller Fr\u00f6ligkeit/ die ich zuvor empfunden.", "tokens": ["Von", "al\u00b7ler", "Fr\u00f6\u00b7lig\u00b7keit", "/", "die", "ich", "zu\u00b7vor", "emp\u00b7fun\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$(", "PRELS", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Was Rath ist/ bl\u00f6des Hertz! kanst du nicht mehr zur\u00fccke?", "tokens": ["Was", "Rath", "ist", "/", "bl\u00f6\u00b7des", "Hertz", "!", "kanst", "du", "nicht", "mehr", "zu\u00b7r\u00fc\u00b7cke", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "VAFIN", "$(", "ADJA", "NN", "$.", "VMFIN", "PPER", "PTKNEG", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "So brich zur Seiten aus/ halt dich zur rechten Hand/", "tokens": ["So", "brich", "zur", "Sei\u00b7ten", "aus", "/", "halt", "dich", "zur", "rech\u00b7ten", "Hand", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPRART", "NN", "PTKVZ", "$(", "VVFIN", "PRF", "APPRART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Vermeide falschen Grund und suche festes Land/", "tokens": ["Ver\u00b7mei\u00b7de", "fal\u00b7schen", "Grund", "und", "su\u00b7che", "fes\u00b7tes", "Land", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "KON", "ADJA", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Der Pusch und Himmel ist nicht aller Orten dicke.", "tokens": ["Der", "Pusch", "und", "Him\u00b7mel", "ist", "nicht", "al\u00b7ler", "Or\u00b7ten", "di\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VAFIN", "PTKNEG", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Die braune Nacht entweicht/ die Mehrerin der Sorgen/", "tokens": ["Die", "brau\u00b7ne", "Nacht", "ent\u00b7weicht", "/", "die", "Meh\u00b7re\u00b7rin", "der", "Sor\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$(", "ART", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Der Monde weiset mir sein erstgebornes Licht/", "tokens": ["Der", "Mon\u00b7de", "wei\u00b7set", "mir", "sein", "erst\u00b7ge\u00b7bor\u00b7nes", "Licht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.31": {"text": "Ich lerne wiederum erkennen Weg und Pflicht/", "tokens": ["Ich", "ler\u00b7ne", "wie\u00b7de\u00b7rum", "er\u00b7ken\u00b7nen", "Weg", "und", "Pflicht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJA", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Mein frohes Hertze gr\u00fc\u00dft den nunmehr nahen Morgen.", "tokens": ["Mein", "fro\u00b7hes", "Hert\u00b7ze", "gr\u00fc\u00dft", "den", "nun\u00b7mehr", "na\u00b7hen", "Mor\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "ART", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Mein Urtheil weckt nun auff die eingeschl\u00e4fften Sinnen/", "tokens": ["Mein", "Ur\u00b7theil", "weckt", "nun", "auff", "die", "ein\u00b7ge\u00b7schl\u00e4ff\u00b7ten", "Sin\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Der Geist erholet sich/ gest\u00e4rcket durch den Geist/", "tokens": ["Der", "Geist", "er\u00b7ho\u00b7let", "sich", "/", "ge\u00b7st\u00e4r\u00b7cket", "durch", "den", "Geist", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "$(", "VVPP", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Der das verirrte Volck zu rechtem Wege weist.", "tokens": ["Der", "das", "ver\u00b7irr\u00b7te", "Volck", "zu", "rech\u00b7tem", "We\u00b7ge", "weist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "HeRR/ richte meinen Fu\u00df/ und f\u00f6rdre mein Beginnen!", "tokens": ["HeRR", "/", "rich\u00b7te", "mei\u00b7nen", "Fu\u00df", "/", "und", "f\u00f6rd\u00b7re", "mein", "Be\u00b7gin\u00b7nen", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "VVFIN", "PPOSAT", "NN", "$(", "KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}