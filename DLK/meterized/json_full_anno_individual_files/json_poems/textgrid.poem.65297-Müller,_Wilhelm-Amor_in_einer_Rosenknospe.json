{"textgrid.poem.65297": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Amor in einer Rosenknospe", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Frau Venus wollte neulich", "tokens": ["Frau", "Ve\u00b7nus", "woll\u00b7te", "neu\u00b7lich"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "NE", "VMFIN", "ADV"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Ihr loses S\u00f6hnchen schlagen:", "tokens": ["Ihr", "lo\u00b7ses", "S\u00f6hn\u00b7chen", "schla\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da ist er ihr entlaufen", "tokens": ["Da", "ist", "er", "ihr", "ent\u00b7lau\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und hat sich still gekauert", "tokens": ["Und", "hat", "sich", "still", "ge\u00b7kau\u00b7ert"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PRF", "ADJD", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "In eine Rosenknospe.", "tokens": ["In", "ei\u00b7ne", "Ro\u00b7sen\u00b7knos\u00b7pe", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Kommt, ruft er, kommt, ihr M\u00e4dchen,", "tokens": ["Kommt", ",", "ruft", "er", ",", "kommt", ",", "ihr", "M\u00e4d\u00b7chen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "PPER", "$,", "VVFIN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tr