{"dta.poem.19796": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Rinaldo Rinaldini .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519168", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es wollt ein Schneider wandern,               ", "tokens": ["Es", "wollt", "ein", "Schnei\u00b7der", "wan\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NE", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Am Montag in der Fruh,", "tokens": ["Am", "Mon\u00b7tag", "in", "der", "Fruh", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Begegnet ihm der Teufel,", "tokens": ["Be\u00b7geg\u00b7net", "ihm", "der", "Teu\u00b7fel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Hat weder Str\u00fcmpf noch Schuh':", "tokens": ["Hat", "we\u00b7der", "Str\u00fcmpf", "noch", "Schuh'", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "KON", "NN", "ADV", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "He, he du Schneidergesell,", "tokens": ["He", ",", "he", "du", "Schnei\u00b7der\u00b7ge\u00b7sell", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VVFIN", "PPER", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.6": {"text": "Mu\u00dft wieder aus der H\u00f6ll,", "tokens": ["Mu\u00dft", "wie\u00b7der", "aus", "der", "H\u00f6ll", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wir brauchen nicht zu messen;", "tokens": ["Wir", "brau\u00b7chen", "nicht", "zu", "mes\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Es gehe wie es w\u00f6ll.", "tokens": ["Es", "ge\u00b7he", "wie", "es", "w\u00f6ll", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "PPER", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Nachdem er all gemessen hat,", "tokens": ["Nach\u00b7dem", "er", "all", "ge\u00b7mes\u00b7sen", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nahm er seine lange Scheer", "tokens": ["Nahm", "er", "sei\u00b7ne", "lan\u00b7ge", "Scheer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und stuzt den Teufflen d' Schw\u00e4nzlein ab", "tokens": ["Und", "stuzt", "den", "Teuf\u00b7flen", "d'", "Schw\u00e4nz\u00b7lein", "ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "NE", "NN", "PTKVZ"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Sie h\u00fcpfen hin und her.", "tokens": ["Sie", "h\u00fcp\u00b7fen", "hin", "und", "her", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "He, he du Schneiderg'sell,", "tokens": ["He", ",", "he", "du", "Schnei\u00b7der\u00b7g'\u00b7sell", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VVFIN", "PPER", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.6": {"text": "Pack dich nur aus der H\u00f6ll,", "tokens": ["Pack", "dich", "nur", "aus", "der", "H\u00f6ll", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Wir brauchen nicht das Stuzen,", "tokens": ["Wir", "brau\u00b7chen", "nicht", "das", "Stu\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Es gehe wie es w\u00f6ll.", "tokens": ["Es", "ge\u00b7he", "wie", "es", "w\u00f6ll", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "PPER", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Da zog er's B\u00fcgeleisen raus,", "tokens": ["Da", "zog", "er's", "B\u00fc\u00b7ge\u00b7lei\u00b7sen", "raus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und warf es in das Feuer,", "tokens": ["Und", "warf", "es", "in", "das", "Feu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Er streicht den Teuflen die Falten aus,", "tokens": ["Er", "streicht", "den", "Teuf\u00b7len", "die", "Fal\u00b7ten", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Sie schrieen ungeheuer:", "tokens": ["Sie", "schri\u00b7een", "un\u00b7ge\u00b7heu\u00b7er", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "He, he du Schneider G'sell,", "tokens": ["He", ",", "he", "du", "Schnei\u00b7der", "G'\u00b7sell", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VVFIN", "PPER", "NE", "NE", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.6": {"text": "Geh du nur aus der H\u00f6ll,", "tokens": ["Geh", "du", "nur", "aus", "der", "H\u00f6ll", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Wir brauchen nicht zu b\u00fcgeln,", "tokens": ["Wir", "brau\u00b7chen", "nicht", "zu", "b\u00fc\u00b7geln", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Es gehe wie es w\u00f6ll.", "tokens": ["Es", "ge\u00b7he", "wie", "es", "w\u00f6ll", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "PPER", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Er nahm den Pfriemen aus dem Sack,", "tokens": ["Er", "nahm", "den", "Pfrie\u00b7men", "aus", "dem", "Sack", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und stach sie in die K\u00f6pf,", "tokens": ["Und", "stach", "sie", "in", "die", "K\u00f6pf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er sagt, halt still, ich bin schon da,", "tokens": ["Er", "sagt", ",", "halt", "still", ",", "ich", "bin", "schon", "da", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "PTKVZ", "$,", "PPER", "VAFIN", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So sezt man bei uns Kn\u00f6pf:", "tokens": ["So", "sezt", "man", "bei", "uns", "Kn\u00f6pf", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "APPR", "PPER", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "He, he, du Schneiderg'sell,", "tokens": ["He", ",", "he", ",", "du", "Schnei\u00b7der\u00b7g'\u00b7sell", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$,", "VVFIN", "$,", "PPER", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.6": {"text": "Geh einmal aus der H\u00f6ll,", "tokens": ["Geh", "ein\u00b7mal", "aus", "der", "H\u00f6ll", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wir brauchen nicht zu kleiden,", "tokens": ["Wir", "brau\u00b7chen", "nicht", "zu", "klei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Es geh nun wie es w\u00f6ll.", "tokens": ["Es", "geh", "nun", "wie", "es", "w\u00f6ll", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "KOKOM", "PPER", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Drauf nahm er Nadl und Fingerhut,", "tokens": ["Drauf", "nahm", "er", "Nadl", "und", "Fin\u00b7ger\u00b7hut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und f\u00e4ngt zu stechen an,", "tokens": ["Und", "f\u00e4ngt", "zu", "ste\u00b7chen", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKZU", "VVINF", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er flickt den Teufeln die Nasl\u00f6cher zu.", "tokens": ["Er", "flickt", "den", "Teu\u00b7feln", "die", "Nas\u00b7l\u00f6\u00b7cher", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "So eng er immer kan:", "tokens": ["So", "eng", "er", "im\u00b7mer", "kan", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "He, he du Schneidergesell,", "tokens": ["He", ",", "he", "du", "Schnei\u00b7der\u00b7ge\u00b7sell", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VVFIN", "PPER", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.6": {"text": "Pack dich nur aus der H\u00f6ll,", "tokens": ["Pack", "dich", "nur", "aus", "der", "H\u00f6ll", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Wir k\u00f6nnen nimmer riechen,", "tokens": ["Wir", "k\u00f6n\u00b7nen", "nim\u00b7mer", "rie\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Es geh nun wie es w\u00f6ll.", "tokens": ["Es", "geh", "nun", "wie", "es", "w\u00f6ll", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "KOKOM", "PPER", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Darauf f\u00e4ngt er zu schneiden an,", "tokens": ["Da\u00b7rauf", "f\u00e4ngt", "er", "zu", "schnei\u00b7den", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PTKA", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das Ding hat ziemlich brennt,", "tokens": ["Das", "Ding", "hat", "ziem\u00b7lich", "brennt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er hat den Teuflen mit Gewalt", "tokens": ["Er", "hat", "den", "Teuf\u00b7len", "mit", "Ge\u00b7walt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Ohrlappen aufgetrennt:", "tokens": ["Die", "Ohr\u00b7lap\u00b7pen", "auf\u00b7ge\u00b7trennt", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-++-+-+", "measure": "unknown.measure.tetra"}, "line.5": {"text": "He, he, du Schneiderg'sell,", "tokens": ["He", ",", "he", ",", "du", "Schnei\u00b7der\u00b7g'\u00b7sell", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$,", "VVFIN", "$,", "PPER", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.6": {"text": "Marschir nur aus der H\u00f6ll,", "tokens": ["Mar\u00b7schir", "nur", "aus", "der", "H\u00f6ll", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Sonst brauchen wir den Bader,", "tokens": ["Sonst", "brau\u00b7chen", "wir", "den", "Ba\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Es geh nun wie es w\u00f6ll.", "tokens": ["Es", "geh", "nun", "wie", "es", "w\u00f6ll", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "KOKOM", "PPER", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Nach diesem kam der Lucifer,", "tokens": ["Nach", "die\u00b7sem", "kam", "der", "Lu\u00b7ci\u00b7fer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "VVFIN", "ART", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und sagt: es ist ein Graus,", "tokens": ["Und", "sagt", ":", "es", "ist", "ein", "Graus", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "PPER", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Kein Teufel hat kein Schw\u00e4nzerl mehr,", "tokens": ["Kein", "Teu\u00b7fel", "hat", "kein", "Schw\u00e4n\u00b7zerl", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PIAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Jagt ihn zur H\u00f6ll hinaus:", "tokens": ["Jagt", "ihn", "zur", "H\u00f6ll", "hin\u00b7aus", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "He, he, du Schneiderg'sell,", "tokens": ["He", ",", "he", ",", "du", "Schnei\u00b7der\u00b7g'\u00b7sell", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$,", "VVFIN", "$,", "PPER", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.6": {"text": "Pack dich nur aus der H\u00f6ll,", "tokens": ["Pack", "dich", "nur", "aus", "der", "H\u00f6ll", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Wir brauchen keine Kleider,", "tokens": ["Wir", "brau\u00b7chen", "kei\u00b7ne", "Klei\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Es geh nun wie es w\u00f6ll.", "tokens": ["Es", "geh", "nun", "wie", "es", "w\u00f6ll", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "KOKOM", "PPER", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Nachdem er nun hat aufgepackt,", "tokens": ["Nach\u00b7dem", "er", "nun", "hat", "auf\u00b7ge\u00b7packt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da war ihm erst recht wohl,", "tokens": ["Da", "war", "ihm", "erst", "recht", "wohl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADV", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er h\u00fcpft und springet unverzagt,", "tokens": ["Er", "h\u00fcpft", "und", "sprin\u00b7get", "un\u00b7ver\u00b7zagt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Lacht sich den Buckel voll,", "tokens": ["Lacht", "sich", "den", "Bu\u00b7ckel", "voll", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "ADJD", "$,"], "meter": "++-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ging eilends aus der H\u00f6ll,", "tokens": ["Ging", "ei\u00b7lends", "aus", "der", "H\u00f6ll", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Und blieb ein Schneiderg'sell;", "tokens": ["Und", "blieb", "ein", "Schnei\u00b7der\u00b7g'\u00b7sell", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Drum holt der Teufel kein Schneider mehr,", "tokens": ["Drum", "holt", "der", "Teu\u00b7fel", "kein", "Schnei\u00b7der", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "NN", "PIAT", "NN", "ADV", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Er stehl so viel er w\u00f6ll.", "tokens": ["Er", "stehl", "so", "viel", "er", "w\u00f6ll", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "ADV", "PPER", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}