{"dta.poem.8847": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "XxVI.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Sie reihen sich zu ganzen Schaaren an!", "tokens": ["Sie", "rei\u00b7hen", "sich", "zu", "gan\u00b7zen", "Schaa\u00b7ren", "an", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Als Unerfahrner ward ich eingeschifft,", "tokens": ["Als", "Un\u00b7er\u00b7fahr\u00b7ner", "ward", "ich", "ein\u00b7ge\u00b7schifft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und kam im Hafen unerfahren an!", "tokens": ["Und", "kam", "im", "Ha\u00b7fen", "un\u00b7er\u00b7fah\u00b7ren", "an", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wenn du besuchen willst der Liebe Markt,", "tokens": ["Wenn", "du", "be\u00b7su\u00b7chen", "willst", "der", "Lie\u00b7be", "Markt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVINF", "VMFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "So triffst du stets von meinen Waaren an;", "tokens": ["So", "triffst", "du", "stets", "von", "mei\u00b7nen", "Waa\u00b7ren", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Vertr\u00f6delt hab' ich fr\u00fcherhin das Herz,", "tokens": ["Ver\u00b7tr\u00f6\u00b7delt", "hab'", "ich", "fr\u00fc\u00b7her\u00b7hin", "das", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "D'rum fing ich sp\u00e4terhin zu sparen an.", "tokens": ["D'\u00b7rum", "fing", "ich", "sp\u00e4\u00b7ter\u00b7hin", "zu", "spa\u00b7ren", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ADV", "PTKZU", "VVINF", "PTKVZ", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.8": {"text": "O Gl\u00fcck, wenn je du kommst, so thu' es jezt,", "tokens": ["O", "Gl\u00fcck", ",", "wenn", "je", "du", "kommst", ",", "so", "thu'", "es", "jezt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "KOUS", "ADV", "PPER", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Du triffst mich noch bey jungen Jahren an!", "tokens": ["Du", "triffst", "mich", "noch", "bey", "jun\u00b7gen", "Jah\u00b7ren", "an", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}