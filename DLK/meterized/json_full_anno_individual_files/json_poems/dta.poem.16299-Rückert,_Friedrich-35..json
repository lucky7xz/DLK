{"dta.poem.16299": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "35.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ereignisse sind nicht das Wichtigste am Leben,", "tokens": ["Er\u00b7eig\u00b7nis\u00b7se", "sind", "nicht", "das", "Wich\u00b7tigs\u00b7te", "am", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PTKNEG", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn, ohne dir bewu\u00dft zu werden, sie entschweben.", "tokens": ["Wenn", ",", "oh\u00b7ne", "dir", "be\u00b7wu\u00dft", "zu", "wer\u00b7den", ",", "sie", "ent\u00b7schwe\u00b7ben", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "$,", "KOUI", "PPER", "ADJD", "PTKZU", "VAINF", "$,", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Was innerlich nur ward, wie klein es sei, ist wichtig;", "tokens": ["Was", "in\u00b7ner\u00b7lich", "nur", "ward", ",", "wie", "klein", "es", "sei", ",", "ist", "wich\u00b7tig", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "ADJD", "ADV", "VAFIN", "$,", "PWAV", "ADJD", "PPER", "VAFIN", "$,", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was \u00e4u\u00dferlich dir blieb, das Gr\u00f6ste selbst ist nichtig.", "tokens": ["Was", "\u00e4u\u00b7\u00dfer\u00b7lich", "dir", "blieb", ",", "das", "Gr\u00f6s\u00b7te", "selbst", "ist", "nich\u00b7tig", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "PPER", "VVFIN", "$,", "ART", "NN", "ADV", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Drum dr\u00e4nge nicht zuviel hinaus dich in den Braus,", "tokens": ["Drum", "dr\u00e4n\u00b7ge", "nicht", "zu\u00b7viel", "hin\u00b7aus", "dich", "in", "den", "Braus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PTKNEG", "ADV", "APZR", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "La\u00df aber unbemerkt vorbei nichts deinem Haus.", "tokens": ["La\u00df", "a\u00b7ber", "un\u00b7be\u00b7merkt", "vor\u00b7bei", "nichts", "dei\u00b7nem", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "ADJD", "ADV", "PIS", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Zieh ein Ergebnis dir aus dem, was sich begab", "tokens": ["Zieh", "ein", "Er\u00b7geb\u00b7nis", "dir", "aus", "dem", ",", "was", "sich", "be\u00b7gab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "PPER", "APPR", "ART", "$,", "PRELS", "PRF", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bedeutendes, und frag' ihm die Bedeutung ab.", "tokens": ["Be\u00b7deu\u00b7ten\u00b7des", ",", "und", "frag'", "ihm", "die", "Be\u00b7deu\u00b7tung", "ab", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "VVIMP", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Setz' ihm ein Denkmal, das dir zeig' in k\u00fcnft'gen Stunden,", "tokens": ["Setz'", "ihm", "ein", "Denk\u00b7mal", ",", "das", "dir", "zeig'", "in", "k\u00fcnft'\u00b7gen", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df der geschwundenen dir keine leer geschwunden.", "tokens": ["Da\u00df", "der", "ge\u00b7schwun\u00b7de\u00b7nen", "dir", "kei\u00b7ne", "leer", "ge\u00b7schwun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "PPER", "PIAT", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}