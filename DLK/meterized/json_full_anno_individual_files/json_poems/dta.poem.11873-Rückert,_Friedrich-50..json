{"dta.poem.11873": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "50.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Schenk' in dein Glas nicht mehr, als auf einmal zu trinken!", "tokens": ["Schenk'", "in", "dein", "Glas", "nicht", "mehr", ",", "als", "auf", "ein\u00b7mal", "zu", "trin\u00b7ken", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "PTKNEG", "ADV", "$,", "KOUS", "APPR", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gestandnes stehet ab und wird im Preise sinken.", "tokens": ["Ge\u00b7stand\u00b7nes", "ste\u00b7het", "ab", "und", "wird", "im", "Prei\u00b7se", "sin\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PTKVZ", "KON", "VAFIN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Kein andrer wird von dir die Neige trinken wollen;", "tokens": ["Kein", "an\u00b7drer", "wird", "von", "dir", "die", "Nei\u00b7ge", "trin\u00b7ken", "wol\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "VAFIN", "APPR", "PPER", "ART", "NN", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "La\u00df jeden trinken und trink immer selbst vom vollen!", "tokens": ["La\u00df", "je\u00b7den", "trin\u00b7ken", "und", "trink", "im\u00b7mer", "selbst", "vom", "vol\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PIS", "VVINF", "KON", "VVFIN", "ADV", "ADV", "APPRART", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}