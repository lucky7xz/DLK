{"dta.poem.2610": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Klag-Elegi  \n  An Hn. M. v. H. bey Beerdigung seines ei-  \n nigen Sohns im Junio 1666.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wer jener Herrligkeit h\u00f6chst-seligstes Vergn\u00fcgen", "tokens": ["Wer", "je\u00b7ner", "Herr\u00b7lig\u00b7keit", "h\u00f6chst\u00b7se\u00b7ligs\u00b7tes", "Ver\u00b7gn\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "PDAT", "NN", "ADJA", "NN"], "meter": "-+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Das unumbschribne Licht/ und unumgr\u00e4ntzte Reich/", "tokens": ["Das", "u\u00b7numbsc\u00b7hrib\u00b7ne", "Licht", "/", "und", "un\u00b7um\u00b7gr\u00e4ntz\u00b7te", "Reich", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "KON", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Des Lammes Rosen-Blut/ der Au\u00dferwehlten Sigen/", "tokens": ["Des", "Lam\u00b7mes", "Ro\u00b7sen\u00b7Blut", "/", "der", "Au\u00b7\u00dfer\u00b7wehl\u00b7ten", "Si\u00b7gen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$(", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Verkl\u00e4rter Leiber Glantz/ der Sonn und Sternen", "tokens": ["Ver\u00b7kl\u00e4r\u00b7ter", "Lei\u00b7ber", "Glantz", "/", "der", "Sonn", "und", "Ster\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "NN", "$(", "ART", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Mit Augen der Vernunfft was ernstlicher betrachtet/", "tokens": ["Mit", "Au\u00b7gen", "der", "Ver\u00b7nunfft", "was", "ernst\u00b7li\u00b7cher", "be\u00b7trach\u00b7tet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "PWS", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und denn die Fl\u00fcchtigkeit des Menschen recht bedenckt/", "tokens": ["Und", "denn", "die", "Fl\u00fcch\u00b7tig\u00b7keit", "des", "Men\u00b7schen", "recht", "be\u00b7denckt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "ART", "NN", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wie hier sein sicher Leib in strengen Martern schmachtet/", "tokens": ["Wie", "hier", "sein", "si\u00b7cher", "Leib", "in", "stren\u00b7gen", "Mar\u00b7tern", "schmach\u00b7tet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPOSAT", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wie kaum ein Blick der Zeit verfleust der ihn nicht kr\u00e4nckt/", "tokens": ["Wie", "kaum", "ein", "Blick", "der", "Zeit", "ver\u00b7fleust", "der", "ihn", "nicht", "kr\u00e4nckt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "ART", "NN", "VVFIN", "ART", "PPER", "PTKNEG", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Der merckt den Unterscheid/ den Schatten und die Sonne/", "tokens": ["Der", "merckt", "den", "Un\u00b7ter\u00b7scheid", "/", "den", "Schat\u00b7ten", "und", "die", "Son\u00b7ne", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "$(", "ART", "NN", "KON", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Das Licht und Finstern\u00fc\u00df/ den hell und tr\u00fcben Tag/", "tokens": ["Das", "Licht", "und", "Fins\u00b7ter\u00b7n\u00fc\u00df", "/", "den", "hell", "und", "tr\u00fc\u00b7ben", "Tag", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "$(", "ART", "ADJD", "KON", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Des Lebens bittern Kelch/ des Sterbens neue Wonne/", "tokens": ["Des", "Le\u00b7bens", "bit\u00b7tern", "Kelch", "/", "des", "Ster\u00b7bens", "neu\u00b7e", "Won\u00b7ne", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$(", "ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Das Thor dadurch man ein zur Freyheit gehen mag.", "tokens": ["Das", "Thor", "da\u00b7durch", "man", "ein", "zur", "Frey\u00b7heit", "ge\u00b7hen", "mag."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["ART", "NN", "PAV", "PIS", "ART", "APPRART", "NN", "VVFIN", "NE"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Wie schwartz der Todt auch scheint/ wie finster Grufft und H\u00f6le/", "tokens": ["Wie", "schwartz", "der", "Todt", "auch", "scheint", "/", "wie", "fins\u00b7ter", "Grufft", "und", "H\u00f6\u00b7le", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "ADV", "VVFIN", "$(", "KOKOM", "ADJA", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "So sind sie doch der Weg in das bestimmte Reich.", "tokens": ["So", "sind", "sie", "doch", "der", "Weg", "in", "das", "be\u00b7stimm\u00b7te", "Reich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Es zancke Fleisch und Blut mit seinem Gast der Seele/", "tokens": ["Es", "zan\u00b7cke", "Fleisch", "und", "Blut", "mit", "sei\u00b7nem", "Gast", "der", "See\u00b7le", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "APPR", "PPOSAT", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Was dorte schimmern sol das mu\u00df hier werden bleich.", "tokens": ["Was", "dor\u00b7te", "schim\u00b7mern", "sol", "das", "mu\u00df", "hier", "wer\u00b7den", "bleich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "VVINF", "VMFIN", "PDS", "VMFIN", "ADV", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Wir k\u00f6nnen nicht die Frucht noch f\u00fcr den Bl\u00fcten haben/", "tokens": ["Wir", "k\u00f6n\u00b7nen", "nicht", "die", "Frucht", "noch", "f\u00fcr", "den", "Bl\u00fc\u00b7ten", "ha\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "ART", "NN", "ADV", "APPR", "ART", "NN", "VAFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Und wenn die Schlacken weg/ so ist das Gold erst rein/", "tokens": ["Und", "wenn", "die", "Schla\u00b7cken", "weg", "/", "so", "ist", "das", "Gold", "erst", "rein", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "PTKVZ", "$(", "ADV", "VAFIN", "ART", "NN", "ADV", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Wie sehr die L\u00e4uffer sonst in ihrem Kreissen draben/", "tokens": ["Wie", "sehr", "die", "L\u00e4uf\u00b7fer", "sonst", "in", "ih\u00b7rem", "Kreis\u00b7sen", "dra\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "So wird/ ders Ziel erreicht/ nur Uberwinder seyn.", "tokens": ["So", "wird", "/", "ders", "Ziel", "er\u00b7reicht", "/", "nur", "U\u00b7ber\u00b7win\u00b7der", "seyn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "$(", "ART", "NN", "VVPP", "$(", "ADV", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Ach sichre Sterblichen/ wie k\u00f6nnen wir genesen?", "tokens": ["Ach", "sich\u00b7re", "Sterb\u00b7li\u00b7chen", "/", "wie", "k\u00f6n\u00b7nen", "wir", "ge\u00b7ne\u00b7sen", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "NN", "$(", "PWAV", "VMFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Wenn auch die Himmel selbst in dr\u00fcm\u0303ern sollen gehn.", "tokens": ["Wenn", "auch", "die", "Him\u00b7mel", "selbst", "in", "dr\u00fcm\u0303ern", "sol\u00b7len", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "ADV", "APPR", "VVINF", "VMFIN", "VVINF", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.23": {"text": "Des Lebens kurtzer Brauch ist so ein eitel Wesen", "tokens": ["Des", "Le\u00b7bens", "kurt\u00b7zer", "Brauch", "ist", "so", "ein", "ei\u00b7tel", "We\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "VAFIN", "ADV", "ART", "ADJD", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Da\u00df wir mit einem Fu\u00df stets in dem Grabe stehn.", "tokens": ["Da\u00df", "wir", "mit", "ei\u00b7nem", "Fu\u00df", "stets", "in", "dem", "Gra\u00b7be", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Ein Schiffer der die See hat hin und her durchkreutzet", "tokens": ["Ein", "Schif\u00b7fer", "der", "die", "See", "hat", "hin", "und", "her", "durch\u00b7kreut\u00b7zet"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "ART", "NN", "VAFIN", "ADV", "KON", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "W\u00fcnscht/ und erlanget auch den Hafen sichrer Ruh.", "tokens": ["W\u00fcnscht", "/", "und", "er\u00b7lan\u00b7get", "auch", "den", "Ha\u00b7fen", "sich\u00b7rer", "Ruh", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "KON", "VVFIN", "ADV", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Der Mensch/ der so erhitzt nach langem Leben geitzet/", "tokens": ["Der", "Mensch", "/", "der", "so", "er\u00b7hitzt", "nach", "lan\u00b7gem", "Le\u00b7ben", "geit\u00b7zet", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "ADV", "VVFIN", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Wei\u00df nicht aus Aberwitz/ wie er so th\u00f6richt thu;", "tokens": ["Wei\u00df", "nicht", "aus", "A\u00b7ber\u00b7witz", "/", "wie", "er", "so", "th\u00f6\u00b7richt", "thu", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "APPR", "NN", "$(", "PWAV", "PPER", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Welch Kauffman tauschet Glas/ und giebet Amethisten?", "tokens": ["Welch", "Kauf\u00b7fman", "tau\u00b7schet", "Glas", "/", "und", "gie\u00b7bet", "A\u00b7met\u00b7his\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "NN", "$(", "KON", "VVFIN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Wer wechselt den Demant umb Folgen und Cristall?", "tokens": ["Wer", "wech\u00b7selt", "den", "De\u00b7mant", "umb", "Fol\u00b7gen", "und", "Cris\u00b7tall", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+--++-+-+++", "measure": "iambic.septa.relaxed"}, "line.31": {"text": "Noch l\u00e4st ein Sterblicher zum offtern sich gel\u00fcsten/", "tokens": ["Noch", "l\u00e4st", "ein", "Sterb\u00b7li\u00b7cher", "zum", "off\u00b7tern", "sich", "ge\u00b7l\u00fcs\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPRART", "PIS", "PRF", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Da\u00df vor des Himmels Schlo\u00df er wehlt der Erden Ball.", "tokens": ["Da\u00df", "vor", "des", "Him\u00b7mels", "Schlo\u00df", "er", "wehlt", "der", "Er\u00b7den", "Ball", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "NN", "PPER", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Da keine Gleich n\u00fc\u00df doch ist irgend wo zu finden/", "tokens": ["Da", "kei\u00b7ne", "Gleich", "n\u00fc\u00df", "doch", "ist", "ir\u00b7gend", "wo", "zu", "fin\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "ADV", "VVFIN", "ADV", "VAFIN", "ADV", "PWAV", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Hier ist der D\u00f6rner Thal und dort das Rosen-Land:", "tokens": ["Hier", "ist", "der", "D\u00f6r\u00b7ner", "Thal", "und", "dort", "das", "Ro\u00b7sen\u00b7Land", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "KON", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Wer liest ihm Ketten aus vor Kron und Ehren-Binden?", "tokens": ["Wer", "liest", "ihm", "Ket\u00b7ten", "aus", "vor", "Kron", "und", "Eh\u00b7ren\u00b7Bin\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "NN", "APPR", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Der guten Acker hat der s\u00e4\u2019t nicht in den Sand.", "tokens": ["Der", "gu\u00b7ten", "A\u00b7cker", "hat", "der", "s\u00e4't", "nicht", "in", "den", "Sand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ART", "VVFIN", "PTKNEG", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Hoch-Edler/ wolte GOtt da\u00df diese Zeilen kr\u00e4fftig", "tokens": ["Hoch\u00b7Ed\u00b7ler", "/", "wol\u00b7te", "Gott", "da\u00df", "die\u00b7se", "Zei\u00b7len", "kr\u00e4ff\u00b7tig"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$(", "VMFIN", "NN", "KOUS", "PDAT", "NN", "ADJD"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Zubrechen Angst und Weh/ das seine Seele nagt!", "tokens": ["Zu\u00b7bre\u00b7chen", "Angst", "und", "Weh", "/", "das", "sei\u00b7ne", "See\u00b7le", "nagt", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "$(", "PDS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Die Feder w\u00e4r\u2019 anitzt mittleidende gesch\u00e4fftig/", "tokens": ["Die", "Fe\u00b7der", "w\u00e4r'", "a\u00b7nitzt", "mitt\u00b7lei\u00b7den\u00b7de", "ge\u00b7sch\u00e4ff\u00b7tig", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJA", "ADJD", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Und h\u00e4tte sich was mehr zu fertigen gewagt.", "tokens": ["Und", "h\u00e4t\u00b7te", "sich", "was", "mehr", "zu", "fer\u00b7ti\u00b7gen", "ge\u00b7wagt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PRF", "PWS", "PIS", "PTKZU", "VVINF", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine