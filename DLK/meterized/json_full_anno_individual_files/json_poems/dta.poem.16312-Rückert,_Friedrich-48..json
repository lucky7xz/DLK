{"dta.poem.16312": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "48.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Beklage dich nur nicht, da\u00df dir so viel mislang;", "tokens": ["Be\u00b7kla\u00b7ge", "dich", "nur", "nicht", ",", "da\u00df", "dir", "so", "viel", "mis\u00b7lang", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "PTKNEG", "$,", "KOUS", "PPER", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sieh wie dabei auch viel Ersprie\u00dfliches entsprang.", "tokens": ["Sieh", "wie", "da\u00b7bei", "auch", "viel", "Er\u00b7sprie\u00df\u00b7li\u00b7ches", "ent\u00b7sprang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "PAV", "ADV", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Reich ist an K\u00f6rnern wie an Spreu die Ernte; scheue", "tokens": ["Reich", "ist", "an", "K\u00f6r\u00b7nern", "wie", "an", "Spreu", "die", "Ern\u00b7te", ";", "scheu\u00b7e"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ADJD", "VAFIN", "APPR", "NN", "KOKOM", "APPR", "NN", "ART", "NN", "$.", "VVFIN"], "meter": "++-+-+-+-+-+-", "measure": "unknown.measure.septa"}, "line.2": {"text": "Nur nicht die M\u00fch, und lis die K\u00f6rner aus der Spreue.", "tokens": ["Nur", "nicht", "die", "M\u00fch", ",", "und", "lis", "die", "K\u00f6r\u00b7ner", "aus", "der", "Spreu\u00b7e", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "A