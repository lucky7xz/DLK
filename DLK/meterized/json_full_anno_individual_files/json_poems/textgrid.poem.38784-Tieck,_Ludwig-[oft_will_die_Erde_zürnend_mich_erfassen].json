{"textgrid.poem.38784": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "[oft will die Erde z\u00fcrnend mich erfassen]", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Oft will die Erde z\u00fcrnend mich erfassen,", "tokens": ["Oft", "will", "die", "Er\u00b7de", "z\u00fcr\u00b7nend", "mich", "er\u00b7fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "VVPP", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie Felsen steigt es st\u00fcrmend in die Seele,", "tokens": ["Wie", "Fel\u00b7sen", "steigt", "es", "st\u00fcr\u00b7mend", "in", "die", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVFIN", "PPER", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So da\u00df ich mich in grimmen Zweifeln qu\u00e4le,", "tokens": ["So", "da\u00df", "ich", "mich", "in", "grim\u00b7men", "Zwei\u00b7feln", "qu\u00e4\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "PRF", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Als m\u00fc\u00df' ich Lieb' und Herz und Himmel hassen.", "tokens": ["Als", "m\u00fc\u00df'", "ich", "Lieb'", "und", "Herz", "und", "Him\u00b7mel", "has\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VMFIN", "PPER", "NN", "KON", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Dein s\u00fcsses Bild hat furchtsam mich verlassen,