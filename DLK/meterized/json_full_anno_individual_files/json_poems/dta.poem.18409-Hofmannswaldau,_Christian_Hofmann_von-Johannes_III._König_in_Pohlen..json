{"dta.poem.18409": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Johannes  III.  K\u00f6nig in Pohlen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1703", "urn": "urn:nbn:de:kobv:b4-200905199360", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wer sich nicht gauckeley und blendwerck l\u00e4\u00dft betriegen/", "tokens": ["Wer", "sich", "nicht", "gau\u00b7cke\u00b7ley", "und", "blend\u00b7werck", "l\u00e4\u00dft", "be\u00b7trie\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "PTKNEG", "ADV", "KON", "ADJD", "VVFIN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der siehet deinen hof als eine wallstadt an/", "tokens": ["Der", "sie\u00b7het", "dei\u00b7nen", "hof", "als", "ei\u00b7ne", "wall\u00b7stadt", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPOSAT", "NN", "KOKOM", "ART", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wo geld und schmeicheley mit solchem vortheil siegen/", "tokens": ["Wo", "geld", "und", "schmei\u00b7che\u00b7ley", "mit", "sol\u00b7chem", "vor\u00b7theil", "sie\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "ADV", "APPR", "PIAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Als kaum dein helden-arm bey Chotim hat gethan.", "tokens": ["Als", "kaum", "dein", "hel\u00b7den\u00b7arm", "bey", "Cho\u00b7tim", "hat", "ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPOSAT", "NN", "APPR", "NE", "VAFIN", "VVPP", "$."], "meter": "-+-+-+