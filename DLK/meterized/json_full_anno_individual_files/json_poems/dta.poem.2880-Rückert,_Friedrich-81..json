{"dta.poem.2880": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "81.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du bist zu schwach, der Welt Ungleichheit auszugleichen,", "tokens": ["Du", "bist", "zu", "schwach", ",", "der", "Welt", "Un\u00b7gleich\u00b7heit", "aus\u00b7zu\u00b7glei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKA", "ADJD", "$,", "ART", "NN", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nicht machen kannst du rings die Armen all zu Reichen.", "tokens": ["Nicht", "ma\u00b7chen", "kannst", "du", "rings", "die", "Ar\u00b7men", "all", "zu", "Rei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVINF", "VMFIN", "PPER", "ADV", "ART", "NN", "PIAT", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Nicht jeden Thoren kannst du zu 'nem Weisen machen,", "tokens": ["Nicht", "je\u00b7den", "Tho\u00b7ren", "kannst", "du", "zu", "'nem", "Wei\u00b7sen", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PIAT", "NN", "VMFIN", "PPER", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zum Guten jeden Wicht, zum Helden jeden Schwachen;", "tokens": ["Zum", "Gu\u00b7ten", "je\u00b7den", "Wicht", ",", "zum", "Hel\u00b7den", "je\u00b7den", "Schwa\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PIAT", "NN", "$,", "APPRART", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Nicht decken jede Bl\u00f6\u00df' und jeden Fehl verh\u00fcllen,", "tokens": ["Nicht", "de\u00b7cken", "je\u00b7de", "Bl\u00f6\u00df'", "und", "je\u00b7den", "Fehl", "ver\u00b7h\u00fcl\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVINF", "PIAT", "NN", "KON", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nicht stillen jeden Durst und jeden Wunsch erf\u00fcllen.", "tokens": ["Nicht", "stil\u00b7len", "je\u00b7den", "Durst", "und", "je\u00b7den", "Wunsch", "er\u00b7f\u00fcl\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "PIAT", "NN", "KON", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Doch la\u00df soweit du kannst nur deine Liebe reichen,", "tokens": ["Doch", "la\u00df", "so\u00b7weit", "du", "kannst", "nur", "dei\u00b7ne", "Lie\u00b7be", "rei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "ADV", "PPER", "VMFIN", "ADV", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nach allen Seiten, Ungleichheiten auszugleichen;", "tokens": ["Nach", "al\u00b7len", "Sei\u00b7ten", ",", "Un\u00b7gleich\u00b7hei\u00b7ten", "aus\u00b7zu\u00b7glei\u00b7chen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}