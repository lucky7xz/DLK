{"textgrid.poem.33546": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Entrungen hat sich ihrer H\u00fclle", "tokens": ["Ent\u00b7run\u00b7gen", "hat", "sich", "ih\u00b7rer", "H\u00fcl\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "PRF", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Bl\u00fcte vom Orangenbaum,", "tokens": ["Die", "Bl\u00fc\u00b7te", "vom", "O\u00b7ran\u00b7gen\u00b7baum", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ihr Wohlgeruch in s\u00fc\u00dfer F\u00fclle", "tokens": ["Ihr", "Wohl\u00b7ge\u00b7ruch", "in", "s\u00fc\u00b7\u00dfer", "F\u00fcl\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Durchstr\u00f6mt den dunklen Gartenraum.", "tokens": ["Durch\u00b7str\u00f6mt", "den", "dunk\u00b7len", "Gar\u00b7ten\u00b7raum", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Es leuchtet aus dem Grund des Kelches,", "tokens": ["Es", "leuch\u00b7tet", "aus", "dem", "Grund", "des", "Kel\u00b7ches", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Es duftet so geheimnisvoll,", "tokens": ["Es", "duf\u00b7tet", "so", "ge\u00b7heim\u00b7nis\u00b7voll", ","], "token_info": ["word