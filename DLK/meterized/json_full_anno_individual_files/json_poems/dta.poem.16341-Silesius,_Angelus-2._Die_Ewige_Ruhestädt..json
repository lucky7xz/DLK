{"dta.poem.16341": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "2.  Die Ewige Ruhest\u00e4dt.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Es mag ein anderre sich umb sein Begr\u00e4bni\u00df kr\u00e4nken/", "tokens": ["Es", "mag", "ein", "an\u00b7der\u00b7re", "sich", "umb", "sein", "Be\u00b7gr\u00e4b\u00b7ni\u00df", "kr\u00e4n\u00b7ken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "PIS", "PRF", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+----+-+-+-", "measure": "unknown.measure.penta"}, "line.2": {"text": "Und seinen Madensak mit stoltzem Bau bed\u00e4nken.", "tokens": ["Und", "sei\u00b7nen", "Ma\u00b7den\u00b7sak", "mit", "stolt\u00b7zem", "Bau", "be\u00b7d\u00e4n\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich Sorge nicht daf\u00fcr: Mein Grab/ mein Fel\u00df und", "tokens": ["Ich", "Sor\u00b7ge", "nicht", "da\u00b7f\u00fcr", ":", "Mein", "Grab", "/", "mein", "Fel\u00df", "und"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "NN", "PTKNEG", "PAV", "$.", "PPOSAT", "NN", "$(", "PPOSAT", "NN", "KON"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In dem ich ewig Ruh/ so", "tokens": ["In", "dem", "ich", "e\u00b7wig", "Ruh", "/", "so"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["APPR", "PRELS", "PPER", "ADJD", "NN", "$(", "ADV"]