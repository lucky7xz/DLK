"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.5": {"text": "Aber sie fielen wie Steine", "tokens": ["A\u00b7ber", "sie", "fie\u00b7len", "wie", "Stei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "KOKOM", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.6": {"text": "Zur Nacht mit traurigem Laut", "tokens": ["Zur", "Nacht", "mit", "trau\u00b7ri\u00b7gem", "Laut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Und hielten im eisernen Schnabel", "tokens": ["Und", "hiel\u00b7ten", "im", "ei\u00b7ser\u00b7nen", "Schna\u00b7bel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.8": {"text": "Die Kr\u00e4nze von Stroh und Kraut.", "tokens": ["Die", "Kr\u00e4n\u00b7ze", "von", "Stroh", "und", "Kraut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Manchmal ist deine Stimme,", "tokens": ["Manch\u00b7mal", "ist", "dei\u00b7ne", "Stim\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-++-+-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Die im Winde verstreicht,", "tokens": ["Die", "im", "Win\u00b7de", "ver\u00b7streicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "VVPP", "$,"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Deine Hand, die im Traume", "tokens": ["Dei\u00b7ne", "Hand", ",", "die", "im", "Trau\u00b7me"], "token_info": ["word", "word", "punct", "word", "word", "w