{"textgrid.poem.46530": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[trauriger Ahnung Gedanken]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Trauriger Ahnung Gedanken", "tokens": ["Trau\u00b7ri\u00b7ger", "Ah\u00b7nung", "Ge\u00b7dan\u00b7ken"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Schlage sogleich danieder,", "tokens": ["Schla\u00b7ge", "sog\u00b7leich", "da\u00b7nie\u00b7der", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADV", "PAV", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Eh' sie in K\u00f6rperschranken", "tokens": ["Eh'", "sie", "in", "K\u00f6r\u00b7per\u00b7schran\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Treten und werden Lieder.", "tokens": ["Tre\u00b7ten", "und", "wer\u00b7den", "Lie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VAFIN", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.2": {"line.1": {"text": "Lebendig gewordne Gedanken", "tokens": ["Le\u00b7ben\u00b7dig", "ge\u00b7word\u00b7ne", "Ge\u00b7dan\u00b7ken"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "ADJA", "NN"], "meter": "+---+--+-", "measure": "trochaic.tri.relaxed"}, "line.2": {"text": "Sind nicht Schatten, sind Wesen.", "tokens": ["Sind", "nicht", "Schat\u00b7ten", ",", "sind", "We\u00b7sen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "NN", "$,", "VAFIN", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Da\u00df nicht sterben deine Kranken,", "tokens": ["Da\u00df", "nicht", "ster\u00b7ben", "dei\u00b7ne", "Kran\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sprich aus, da\u00df du f\u00fchlst, sie genesen.", "tokens": ["Sprich", "aus", ",", "da\u00df", "du", "f\u00fchlst", ",", "sie", "ge\u00b7ne\u00b7sen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "$,", "KOUS", "PPER", "VVFIN", "$,", "PPER", "VVPP", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.3": {"line.1": {"text": "Greifst du darum in die Schranken,", "tokens": ["Greifst", "du", "da\u00b7rum", "in", "die", "Schran\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PAV", "APPR", "ART", "NN", "$,"], "meter": "+-+++-+-", "measure": "unknown.measure.penta"}, "line.2": {"text": "Die Gott sich vorbehalten?", "tokens": ["Die", "Gott", "sich", "vor\u00b7be\u00b7hal\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Nein, wenn er eingiebt Gedanken,", "tokens": ["Nein", ",", "wenn", "er", "ein\u00b7giebt", "Ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "KOUS", "PPER", "VVFIN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "So will er sie auch halten.", "tokens": ["So", "will", "er", "sie", "auch", "hal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Trauriger Ahnung Gedanken", "tokens": ["Trau\u