{"textgrid.poem.34140": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Grundirrthum", "genre": "verse", "period": "N.A.", "pub_year": 1838, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbh\u00e4tte der R\u00fcstige nicht so viel gedichtet, er h\u00e4tte", "tokens": ["\u00bb", "h\u00e4t\u00b7te", "der", "R\u00fcs\u00b7ti\u00b7ge", "nicht", "so", "viel", "ge\u00b7dich\u00b7tet", ",", "er", "h\u00e4t\u00b7te"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["$(", "VAFIN", "ART", "NN", "PTKNEG", "ADV", "ADV", "VVPP", "$,", "PPER", "VAFIN"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.2": {"text": "H\u00f6here Fl\u00fcge gethan, h\u00e4tte die Sterne erreicht!\u00ab", "tokens": ["H\u00f6\u00b7he\u00b7re", "Fl\u00fc\u00b7ge", "ge\u00b7than", ",", "h\u00e4t\u00b7te", "die", "Ster\u00b7ne", "er\u00b7reicht", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADJA", "NN", "VVPP", "$,", "VAFIN", "ART", "NN", "VVPP", "$.", "$("], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}, "line.3": {"text": "W\u00e4re die Wiese nicht leider in Butterblumen zerflossen,", "tokens": ["W\u00e4\u00b7re", "die", "Wie\u00b7se", "nicht", "lei\u00b7der", "in", "But\u00b7ter\u00b7blu\u00b7men", "zer\u00b7flos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PTKNEG", "ADV", "APPR", "NN", "VVPP", "$,"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Eine Aloe w\u00e4r' sicher zuletzt ihr entspro\u00dft!", "tokens": ["Ei\u00b7ne", "A\u00b7loe", "w\u00e4r'", "si\u00b7cher", "zu\u00b7letzt", "ihr", "ent\u00b7spro\u00dft", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "ADV", "PPER", "VVFIN", "$."], "meter": "+-+--+--+--+", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "Giebst du das Eine nicht zu, so mu\u00df ich das And're bestreiten,", "tokens": ["Giebst", "du", "das", "Ei\u00b7ne", "nicht", "zu", ",", "so", "mu\u00df", "ich", "das", "An\u00b7d'\u00b7re", "be\u00b7strei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PDS", "ART", "PTKNEG", "PTKVZ", "$,", "ADV", "VMFIN", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.6": {"text": "Nie zerflie\u00dft ein Krystall, aber ein Tropfen zerrinnt.", "tokens": ["Nie", "zer\u00b7flie\u00dft", "ein", "Krys\u00b7tall", ",", "a\u00b7ber", "ein", "Trop\u00b7fen", "zer\u00b7rinnt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "ADV", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "\u00bbh\u00e4tte der R\u00fcstige nicht so viel gedichtet, er h\u00e4tte", "tokens": ["\u00bb", "h\u00e4t\u00b7te", "der", "R\u00fcs\u00b7ti\u00b7ge", "nicht", "so", "viel", "ge\u00b7dich\u00b7tet", ",", "er", "h\u00e4t\u00b7te"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["$(", "VAFIN", "ART", "NN", "PTKNEG", "ADV", "ADV", "VVPP", "$,", "PPER", "VAFIN"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.2": {"text": "H\u00f6here Fl\u00fcge gethan, h\u00e4tte die Sterne erreicht!\u00ab", "tokens": ["H\u00f6\u00b7he\u00b7re", "Fl\u00fc\u00b7ge", "ge\u00b7than", ",", "h\u00e4t\u00b7te", "die", "Ster\u00b7ne", "er\u00b7reicht", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", 