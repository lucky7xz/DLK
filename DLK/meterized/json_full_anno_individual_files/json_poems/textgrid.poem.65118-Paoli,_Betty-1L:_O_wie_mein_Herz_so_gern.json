{"textgrid.poem.65118": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: O wie mein Herz so gern", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O wie mein Herz so gern", "tokens": ["O", "wie", "mein", "Herz", "so", "gern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "KOKOM", "PPOSAT", "NN", "ADV", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Der gro\u00dfen Stimme lauschet,", "tokens": ["Der", "gro\u00b7\u00dfen", "Stim\u00b7me", "lau\u00b7schet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die, wie ein Gru\u00df des Herrn,", "tokens": ["Die", ",", "wie", "ein", "Gru\u00df", "des", "Herrn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "PWAV", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Aus Meerestiefen rauschet!", "tokens": ["Aus", "Mee\u00b7res\u00b7tie\u00b7fen", "rau\u00b7schet", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Sie nennt den Heimathsport,", "tokens": ["Sie", "nennt", "den", "Hei\u00b7math\u00b7sport", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Dem bang verirrten Kinde,", "tokens": ["Dem", "bang", "ver\u00b7irr\u00b7ten", "Kin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sie hat das rechte Wort", "tokens": ["Sie", "hat", "das", "rech\u00b7te", "Wort"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "F\u00fcr das, was ich empfinde;", "tokens": ["F\u00fcr", "das", ",", "was", "ich", "emp\u00b7fin\u00b7de", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Sie ruft mir tr\u00f6stend zu:", "tokens": ["Sie", "ruft", "mir", "tr\u00f6s\u00b7tend", "zu", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "\u00bbdu sturmverschlag'nes Leben!", "tokens": ["\u00bb", "du", "sturm\u00b7ver\u00b7schlag'\u00b7nes", "Le\u00b7ben", "!"], "token_info": ["punct", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die hingeschied'ne Ruh", "tokens": ["Die", "hin\u00b7ge\u00b7schie\u00b7d'\u00b7ne", "Ruh"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Will ich dir wiedergeben.", "tokens": ["Will", "ich", "dir", "wie\u00b7der\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Vertrau' dich meiner Huth!", "tokens": ["Ver\u00b7trau'", "dich", "mei\u00b7ner", "Huth", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Ich l\u00f6se deine Kette.", "tokens": ["Ich", "l\u00f6\u00b7se", "dei\u00b7ne", "Ket\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Des Friedens Kleinod ruht", "tokens": ["Des", "Frie\u00b7dens", "Klei\u00b7nod", "ruht"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "In meinem Wogenbette.", "tokens": ["In", "mei\u00b7nem", "Wo\u00b7gen\u00b7bet\u00b7te", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Der Lenz mit seiner Zier", "tokens": ["Der", "Lenz", "mit", "sei\u00b7ner", "Zier"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Mahnt dich mit seinen Prangen,", "tokens": ["Mahnt", "dich", "mit", "sei\u00b7nen", "Pran\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "An den nur, der in dir", "tokens": ["An", "den", "nur", ",", "der", "in", "dir"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "ADV", "$,", "PRELS", "APPR", "PPER"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Auf ewig hingegangen!", "tokens": ["Auf", "e\u00b7wig", "hin\u00b7ge\u00b7gan\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Und wenn zu Lust und Leid", "tokens": ["Und", "wenn", "zu", "Lust", "und", "Leid"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Die Menschen sich verbinden,", "tokens": ["Die", "Men\u00b7schen", "sich", "ver\u00b7bin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wirst deine Einsamkeit", "tokens": ["Wirst", "dei\u00b7ne", "Ein\u00b7sam\u00b7keit"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Du bitt'rer nur empfinden.", "tokens": ["Du", "bitt'\u00b7rer", "nur", "emp\u00b7fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Drum flieh' mit deinem M\u00fch'n,", "tokens": ["Drum", "flieh'", "mit", "dei\u00b7nem", "M\u00fch'n", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Wenn Schmerz in dir entbrennet,", "tokens": ["Wenn", "Schmerz", "in", "dir", "ent\u00b7bren\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Zu mir, die zwar kein Bl\u00fch'n,", "tokens": ["Zu", "mir", ",", "die", "zwar", "kein", "Bl\u00fch'n", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "PRELS", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Doch auch kein Welken kennet! \u2013", "tokens": ["Doch", "auch", "kein", "Wel\u00b7ken", "ken\u00b7net", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADV", "PIAT", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Bei mir verstummt die Pein!", "tokens": ["Bei", "mir", "ver\u00b7stummt", "die", "Pein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Sanft will ich dich umgleiten,", "tokens": ["Sanft", "will", "ich", "dich", "um\u00b7glei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Du wirst nicht einsam sein", "tokens": ["Du", "wirst", "nicht", "ein\u00b7sam", "sein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADJD", "VAINF"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "In meinen Einsamkeiten.", "tokens": ["In", "mei\u00b7nen", "Ein\u00b7sam\u00b7kei\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Mein Haus erhebet sich", "tokens": ["Mein", "Haus", "er\u00b7he\u00b7bet", "sich"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "PRF"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Aus schimmerndem Kristalle,", "tokens": ["Aus", "schim\u00b7mern\u00b7dem", "Kris\u00b7tal\u00b7le", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich w\u00f6lbe \u00fcber dich", "tokens": ["Ich", "w\u00f6l\u00b7be", "\u00fc\u00b7ber", "dich"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Die schattende Koralle!", "tokens": ["Die", "schat\u00b7ten\u00b7de", "Ko\u00b7ral\u00b7le", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Statt mit dem Rosenpaar", "tokens": ["Statt", "mit", "dem", "Ro\u00b7sen\u00b7paar"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "Von euern fl\u00fccht'gen Lenzen", "tokens": ["Von", "eu\u00b7ern", "fl\u00fccht'\u00b7gen", "Len\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Will ich dein dunkles Haar", "tokens": ["Will", "ich", "dein", "dunk\u00b7les", "Haar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Mit Perlen licht bekr\u00e4nzen!", "tokens": ["Mit", "Per\u00b7len", "licht", "be\u00b7kr\u00e4n\u00b7zen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Ich will in meinem Arm", "tokens": ["Ich", "will", "in", "mei\u00b7nem", "Arm"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Dich fest und liebend pressen,", "tokens": ["Dich", "fest", "und", "lie\u00b7bend", "pres\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKVZ", "KON", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Bis da\u00df du deinen Harm", "tokens": ["Bis", "da\u00df", "du", "dei\u00b7nen", "Harm"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "KOUS", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "In sel'gem Traum vergessen!", "tokens": ["In", "sel'\u00b7gem", "Traum", "ver\u00b7ges\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Befreit, wirst du die Gluth", "tokens": ["Be\u00b7freit", ",", "wirst", "du", "die", "Gluth"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "VAFIN", "PPER", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Der Erde hier verlernen,", "tokens": ["Der", "Er\u00b7de", "hier", "ver\u00b7ler\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und sehnend, wie die Fluth,", "tokens": ["Und", "seh\u00b7nend", ",", "wie", "die", "Fluth", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "$,", "PWAV", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Aufrauschen zu den Sternen!\u00ab", "tokens": ["Auf\u00b7rau\u00b7schen", "zu", "den", "Ster\u00b7nen", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "O wie mein Herz so gern", "tokens": ["O", "wie", "mein", "Herz", "so", "gern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "KOKOM", "PPOSAT", "NN", "ADV", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Der gro\u00dfen Stimme lauschet,", "tokens": ["Der", "gro\u00b7\u00dfen", "Stim\u00b7me", "lau\u00b7schet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die, wie ein Gru\u00df des Herrn,", "tokens": ["Die", ",", "wie", "ein", "Gru\u00df", "des", "Herrn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "PWAV", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Aus Meerestiefen rauschet!", "tokens": ["Aus", "Mee\u00b7res\u00b7tie\u00b7fen", "rau\u00b7schet", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Sie nennt den Heimathsport,", "tokens": ["Sie", "nennt", "den", "Hei\u00b7math\u00b7sport", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Dem bang verirrten Kinde,", "tokens": ["Dem", "bang", "ver\u00b7irr\u00b7ten", "Kin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sie hat das rechte Wort", "tokens": ["Sie", "hat", "das", "rech\u00b7te", "Wort"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "F\u00fcr das, was ich empfinde;", "tokens": ["F\u00fcr", "das", ",", "was", "ich", "emp\u00b7fin\u00b7de", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "Sie ruft mir tr\u00f6stend zu:", "tokens": ["Sie", "ruft", "mir", "tr\u00f6s\u00b7tend", "zu", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "