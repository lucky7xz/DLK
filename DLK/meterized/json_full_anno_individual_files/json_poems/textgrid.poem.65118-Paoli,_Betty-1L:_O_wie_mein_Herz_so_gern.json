en_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Mahnt dich mit seinen Prangen,", "tokens": ["Mahnt", "dich", "mit", "sei\u00b7nen", "Pran\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "An den nur, der in dir", "tokens": ["An", "den", "nur", ",", "der", "in", "dir"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "ADV", "$,", "PRELS", "APPR", "PPER"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Auf ewig hingegangen!", "tokens": ["Auf", "e\u00b7wig", "hin\u00b7ge\u00b7gan\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.18": {"line.1": {"text": "Und wenn zu Lust und Leid", "tokens": ["Und", "wenn", "zu", "Lust", "und", "Leid"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Die Menschen sich verbinden,", "tokens": ["Die", "Men\u00b7schen", "sich", "ver\u00b7bin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wirst deine Einsamkeit", "tokens": ["Wirst", "dei\u00b7ne", "Ein\u00b7sam\u00b7keit"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Du bitt'rer nur empfinden.", "tokens": ["Du", "bitt'\u00b7rer", "nur", "emp\u00b7fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.19": {"line.1": {"text": "Drum flieh' mit deinem M\u00fch'n,", "tokens": ["Drum", "flieh'", "mit", "dei\u00b7nem", "M\u00fch'n", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Wenn Schmerz in dir entbrennet,", "tokens": ["Wenn", "Schmerz", "in", "dir", "ent\u00b7bren\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Zu mir, die zwar kein Bl\u00fch'n,", "tokens": ["Zu", "mir", ",", "die", "zwar", "kein", "Bl\u00fch'n", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "PRELS", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Doch auch kein Welken kennet! \u2013", "tokens": ["Doch", "auch", "kein", "Wel\u00b7ken", "ken\u00b7net", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADV", "PIAT", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.20": {"line.1": {"text": "Bei mir verstummt die Pein!", "tokens": ["Bei", "mir", "ver\u00b7stummt", "die", "Pein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Sanft will ich dich umgleiten,", "tokens": ["Sanft", "will", "ich", "dich", "um\u00b7glei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Du wirst nicht einsam sein", "tokens": ["Du", "wirst", "nicht", "ein\u00b7sam", "sein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADJD", "VAINF"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "In meinen Einsamkeiten.", "tokens": ["In", "mei\u00b7nen", "Ein\u00b7sam\u00b7kei\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.21": {"line.1": {"text": "Mein Haus erhebet sich", "tokens": ["Mein", "Haus", "er\u00b7he\u00b7bet", "sich"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "PRF"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Aus schimmerndem Kristalle,", "tokens": ["Aus", "schim\u00b7mern\u00b7dem", "Kris\u00b7tal\u00b7le", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich w\u00f6lbe \u00fcber dich", "tokens": ["Ich", "w\u00f6l\u00b7be", "\u00fc\u00b7ber", "dich"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Die schattende Koralle!", "tokens": ["Die", "schat\u00b7ten\u00b7de", "Ko\u00b7ral\u00b7le", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.22": {"line.1": {"text": "Statt mit dem Rosenpaar", "tokens": ["Statt", "mit", "dem", "Ro\u00b7sen\u00b7paar"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "Von euern fl\u00fccht'gen Lenzen", "tokens": ["Von", "eu\u00b7ern", "fl\u00fccht'\u00b7gen", "Len\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Will ich dein dunkles Haar", "tokens": ["Will", "ich", "dein", "dunk\u00b7les", "Haar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Mit Perlen licht bekr\u00e4nzen!", "tokens": ["Mit", "Per\u00b7len", "licht", "be\u00b7kr\u00e4n\u00b7zen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.23": {"line.1": {"text": "Ich will in meinem Arm", "tokens": ["Ich", "will", "in", "mei\u00b7nem", "Arm"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Dich fest und liebend pressen,", "tokens": ["Dich", "fest", "und", "lie\u00b7bend", "pres\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKVZ", "KON", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Bis da\u00df du deinen Harm", "tokens": ["Bis", "da\u00df", "du", "dei\u00b7nen", "Harm"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "KOUS", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "In sel'gem Traum vergessen!", "tokens": ["In", "sel'\u00b7gem", "Traum", "ver\u00b7ges\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.24": {"line.1": {"text": "Befreit, wirst du die Gluth", "tokens": ["Be\u00b7freit", ",", "wirst", "du", "die", "Gluth"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "VAFIN", "PPER", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Der Erde hier verlernen,", "tokens": ["Der", "Er\u00b7de", "hier", "ver\u00b7ler\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und sehnend, wie die Fluth,", "tokens": ["Und", "seh\u00b7nend", ",", "wie", "die", "Fluth", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "$,", "PWAV", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Aufrauschen zu den Sternen!\u00ab", "tokens": ["Auf\u00b7rau\u00b7schen", "zu", "den", "Ster\u00b7nen", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}