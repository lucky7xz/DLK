{"dta.poem.15989": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "43.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was hat dich, Geist, vermocht aus Gott hervorzuwallen?", "tokens": ["Was", "hat", "dich", ",", "Geist", ",", "ver\u00b7mocht", "aus", "Gott", "her\u00b7vor\u00b7zu\u00b7wal\u00b7len", "?"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "$,", "NN", "$,", "VVPP", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Er hat dich nicht verbannt, du bist nicht abgefallen.", "tokens": ["Er", "hat", "dich", "nicht", "ver\u00b7bannt", ",", "du", "bist", "nicht", "ab\u00b7ge\u00b7fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PTKNEG", "VVPP", "$,", "PPER", "VAFIN", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die Liebe nur hat dich, die Liebe dich vertrieben,", "tokens": ["Die", "Lie\u00b7be", "nur", "hat", "dich", ",", "die", "Lie\u00b7be", "dich", "ver\u00b7trie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VAFIN", "PPER", "$,", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Er wollte, da\u00df er dich, da\u00df du ihn k\u00f6nntest lieben.", "tokens": ["Er", "woll\u00b7te", ",", "da\u00df", "er", "dich", ",", "da\u00df", "du", "ihn", "k\u00f6nn\u00b7test", "lie\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "KOUS", "PPER", "PRF", "$,", "KOUS", "PPER", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "W\u00e4rst du nicht au\u00dfer ihm, wie k\u00f6nntst du suchen ihn?", "tokens": ["W\u00e4rst", "du", "nicht", "au\u00b7\u00dfer", "ihm", ",", "wie", "k\u00f6nntst", "du", "su\u00b7chen", "ihn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "APPR", "PPER", "$,", "PWAV", "VMFIN", "PPER", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "W\u00e4r' er nicht au\u00dfer dir, wie k\u00f6nnt' er an dich ziehn?", "tokens": ["W\u00e4r'", "er", "nicht", "au\u00b7\u00dfer", "dir", ",", "wie", "k\u00f6nnt'", "er", "an", "dich", "ziehn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "APPR", "PPER", "$,", "PWAV", "VMFIN", "PPER", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}