{"textgrid.poem.34449": {"metadata": {"author": {"name": "Hartleben, Otto Erich", "birth": "N.A.", "death": "N.A."}, "title": "1", "genre": "verse", "period": "N.A.", "pub_year": 1886, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Von keiner Noth besiegt und keiner Liebe,", "tokens": ["Von", "kei\u00b7ner", "Noth", "be\u00b7siegt", "und", "kei\u00b7ner", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVPP", "KON", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "hab ich genossen fr\u00fch und stets genossen.", "tokens": ["hab", "ich", "ge\u00b7nos\u00b7sen", "fr\u00fch", "und", "stets", "ge\u00b7nos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "ADJD", "KON", "ADV", "VVPP", "$."], "meter": "---+-+-+-+-", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Die Eltern starben mir, und kalt verschlossen", "tokens": ["Die", "El\u00b7tern", "star\u00b7ben", "mir", ",", "und", "kalt", "ver\u00b7schlos\u00b7sen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,", "KON", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "fr\u00f6hnt ich als Kind schon eigens\u00fcchtgem Triebe.", "tokens": ["fr\u00f6hnt", "ich", "als", "Kind", "schon", "ei\u00b7gen\u00b7s\u00fccht\u00b7gem", "Trie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KOUS", "NN", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Bekam derzeit vielleicht zu wenig Hiebe", "tokens": ["Be\u00b7kam", "der\u00b7zeit", "viel\u00b7leicht", "zu", "we\u00b7nig", "Hie\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ADV", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "und ward verzogen fr\u00fch zu eitlen Possen?", "tokens": ["und", "ward", "ver\u00b7zo\u00b7gen", "fr\u00fch", "zu", "eit\u00b7len", "Pos\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "VVPP", "ADJD", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und doch hat sich der Mutter Blut ergossen", "tokens": ["Und", "doch", "hat", "sich", "der", "Mut\u00b7ter", "Blut", "er\u00b7gos\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "PRF", "ART", "NN", "NN", "VVINF"], "meter": "-+---+-+-+-", "measure": "dactylic.init"}, "line.4": {"text": "in meine Brust, dass es lebendig bliebe.", "tokens": ["in", "mei\u00b7ne", "Brust", ",", "dass", "es", "le\u00b7ben\u00b7dig", "blie\u00b7be", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "KOUS", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Mein Herz war warm und warm hat es empfunden,", "tokens": ["Mein", "Herz", "war", "warm", "und", "warm", "hat", "es", "emp\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "KON", "ADJD", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "f\u00fcr alles Edle hat mein Puls geschlagen.", "tokens": ["f\u00fcr", "al\u00b7les", "Ed\u00b7le", "hat", "mein", "Puls", "ge\u00b7schla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "VAFIN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das Leben schwoll, die D\u00e4mme sind geschwunden,", "tokens": ["Das", "Le\u00b7ben", "schwoll", ",", "die", "D\u00e4m\u00b7me", "sind", "ge\u00b7schwun\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$,", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "der Sinne Strom hat sie hinweggetragen.", "tokens": ["der", "Sin\u00b7ne", "Strom", "hat", "sie", "hin\u00b7weg\u00b7ge\u00b7tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Du, Pepi, hast das L\u00f6sungswort gefunden:", "tokens": ["Du", ",", "Pe\u00b7pi", ",", "hast", "das", "L\u00f6\u00b7sungs\u00b7wort", "ge\u00b7fun\u00b7den", ":"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NE", "$,", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Revolverle kannst du so drollig sagen.", "tokens": ["Re\u00b7vol\u00b7ver\u00b7le", "kannst", "du", "so", "drol\u00b7lig", "sa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Von keiner Noth besiegt und keiner Liebe,", "tokens": ["Von", "kei\u00b7ner", "Noth", "be\u00b7siegt", "und", "kei\u00b7ner", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVPP", "KON", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "lin