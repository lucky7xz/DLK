{"textgrid.poem.62244": {"metadata": {"author": {"name": "Kempner, Friederike", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein Sonnenuntergang, der Untergang", "genre": "verse", "period": "N.A.", "pub_year": 1868, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Sonnenuntergang, der Untergang", "tokens": ["Ein", "Son\u00b7nen\u00b7un\u00b7ter\u00b7gang", ",", "der", "Un\u00b7ter\u00b7gang"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Desjenigen Volks, das einst so hoch gestrahlt,", "tokens": ["Des\u00b7je\u00b7ni\u00b7gen", "Volks", ",", "das", "einst", "so", "hoch", "ge\u00b7strahlt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "PRELS", "ADV", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Siehst Du die Streifen, purpurrot und lang,", "tokens": ["Siehst", "Du", "die", "Strei\u00b7fen", ",", "pur\u00b7pur\u00b7rot", "und", "lang", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Den jeder Untergang am Himmel malt \u2013", "tokens": ["Den", "je\u00b7der", "Un\u00b7ter\u00b7gang", "am", "Him\u00b7mel", "malt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "APPRART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Als z\u00f6gen Schmerz und Unschuld Hand in Hand \u2013", "tokens": ["Als", "z\u00f6\u00b7gen", "Schmerz", "und", "Un\u00b7schuld", "Hand", "in", "Hand", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "KON", "NN", "NN", "APPR", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ein st\u00fcrmisch Leben, ein erhabner Tod \u2013", "tokens": ["Ein", "st\u00fcr\u00b7misch", "Le\u00b7ben", ",", "ein", "er\u00b7hab\u00b7ner", "Tod", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "$,", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ein siegreich Dulden, das nichts \u00fcberwand.", "tokens": ["Ein", "sieg\u00b7reich", "Dul\u00b7den", ",", "das", "nichts", "\u00fc\u00b7ber\u00b7wand", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "$,", "PRELS", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Welch' gro\u00dfes Bild! im Hintergrunde Tag,", "tokens": ["Welch'", "gro\u00b7\u00dfes", "Bild", "!", "im", "Hin\u00b7ter\u00b7grun\u00b7de", "Tag", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$.", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Im Vordergrunde tiefe Nacht man sieht,", "tokens": ["Im", "Vor\u00b7der\u00b7grun\u00b7de", "tie\u00b7fe", "Nacht", "man", "sieht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJA", "NN", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ein Volk, das tief im Staube kniet \u2013", "tokens": ["Ein", "Volk", ",", "das", "tief", "im", "Stau\u00b7be", "kniet", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD", "APPRART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hoch \u00fcber seinem Haupt die Pr\u00fcfung lag \u2013", "tokens": ["Hoch", "\u00fc\u00b7ber", "sei\u00b7nem", "Haupt", "die", "Pr\u00fc\u00b7fung", "lag", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPOSAT", "NN", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und Tr\u00e4nen, Dornen, Ketten, aller Art \u2013", "tokens": ["Und", "Tr\u00e4\u00b7nen", ",", "Dor\u00b7nen", ",", "Ket\u00b7ten", ",", "al\u00b7ler", "Art", "\u2013"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "NN", "$,", "NN", "$,", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und harte, gift'ge Zungen lauern dort \u2013", "tokens": ["Und", "har\u00b7te", ",", "gift'\u00b7ge", "Zun\u00b7gen", "lau\u00b7ern", "dort", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "ADJA", "NN", "VVFIN", "ADV", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und Herzen, ihnen gegen\u00fcber, hart", "tokens": ["Und", "Her\u00b7zen", ",", "ih\u00b7nen", "ge\u00b7gen\u00b7\u00fc\u00b7ber", ",", "hart"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word"], "pos": ["KON", "NN", "$,", "PPER", "PTKVZ", "$,", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wie Stein, und wie der stille Mord.", "tokens": ["Wie", "Stein", ",", "und", "wie", "der", "stil\u00b7le", "Mord", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "$,", "KON", "PWAV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Und Angst und j\u00e4he Flucht und bleiche Not", "tokens": ["Und", "Angst", "und", "j\u00e4\u00b7he", "Flucht", "und", "blei\u00b7che", "Not"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "KON", "ADJA", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mit tausend Schrecken, Qualen, wechseln ab,", "tokens": ["Mit", "tau\u00b7send", "Schre\u00b7cken", ",", "Qua\u00b7len", ",", "wech\u00b7seln", "ab", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "$,", "NN", "$,", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "\u2013 Ein Schatten-Leben und ein rascher Tod, \u2013", "tokens": ["\u2013", "Ein", "Schat\u00b7ten\u00b7Le\u00b7ben", "und", "ein", "ra\u00b7scher", "Tod", ",", "\u2013"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ART", "NN", "KON", "ART", "ADJA", "NN", "$,", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "In d\u00fcsterm Flammen-, frischem Wellengrab.", "tokens": ["In", "d\u00fcs\u00b7term", "Flam\u00b7men", ",", "fri\u00b7schem", "Wel\u00b7len\u00b7grab", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "TRUNC", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Das Volk sieht lange sich die Pr\u00fcfung an:", "tokens": ["Das", "Volk", "sieht", "lan\u00b7ge", "sich", "die", "Pr\u00fc\u00b7fung", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "PRF", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Das Ungl\u00fcck, wie es leibt und lebt und stirbt,", "tokens": ["Das", "Un\u00b7gl\u00fcck", ",", "wie", "es", "leibt", "und", "lebt", "und", "stirbt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "PPER", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und wie es, dem\u00fctig auf steilem Pfad hinan,", "tokens": ["Und", "wie", "es", ",", "de\u00b7m\u00fc\u00b7tig", "auf", "stei\u00b7lem", "Pfad", "hi\u00b7nan", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "$,", "ADJD", "APPR", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Um einen kalten Blick des Mitleids wirbt!", "tokens": ["Um", "ei\u00b7nen", "kal\u00b7ten", "Blick", "des", "Mit\u00b7leids", "wirbt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Im Vordergrunde Nacht \u2013 im Herzen Licht,", "tokens": ["Im", "Vor\u00b7der\u00b7grun\u00b7de", "Nacht", "\u2013", "im", "Her\u00b7zen", "Licht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$(", "APPRART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Im Herzen jenes Morgenrot", "tokens": ["Im", "Her\u00b7zen", "je\u00b7nes", "Mor\u00b7gen\u00b7rot"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Des Glaubens und der Zuversicht \u2013", "tokens": ["Des", "Glau\u00b7bens", "und", "der", "Zu\u00b7ver\u00b7sicht", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Erhaben \u00fcber Finsternis und Tod \u2013", "tokens": ["Er\u00b7ha\u00b7ben", "\u00fc\u00b7ber", "Fins\u00b7ter\u00b7nis", "und", "Tod", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Sie \u00fcberdauernd, \u00fcberdauernd Raum und Zeit,", "tokens": ["Sie", "\u00fc\u00b7berd\u00b7au\u00b7ernd", ",", "\u00fc\u00b7berd\u00b7au\u00b7ernd", "Raum", "und", "Zeit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVPP", "$,", "VVPP", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sie umgestaltend in den ewigen Tag \u2013", "tokens": ["Sie", "um\u00b7ge\u00b7stal\u00b7tend", "in", "den", "e\u00b7wi\u00b7gen", "Tag", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVPP", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Sie umgestaltend in Unsterblichkeit:", "tokens": ["Sie", "um\u00b7ge\u00b7stal\u00b7tend", "in", "U\u00b7nsterb\u00b7lich\u00b7keit", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVPP", "APPR", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Das gl\u00e4ub'ge Volk hofft es bei jedem Schlag;", "tokens": ["Das", "gl\u00e4ub'\u00b7ge", "Volk", "hofft", "es", "bei", "je\u00b7dem", "Schlag", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Das Volk sieht in den Abgrund tief hinab,", "tokens": ["Das", "Volk", "sieht", "in", "den", "Ab\u00b7grund", "tief", "hin\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und ruft: ich werde leben! Gott mit mir!", "tokens": ["Und", "ruft", ":", "ich", "wer\u00b7de", "le\u00b7ben", "!", "Gott", "mit", "mir", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "PPER", "VAFIN", "VVINF", "$.", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Geb't mir zur Reise um die Welt den Stab,", "tokens": ["Ge\u00b7b't", "mir", "zur", "Rei\u00b7se", "um", "die", "Welt", "den", "Stab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPRART", "NN", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "\u2013 Den Glauben \u2013 ihn allein nehm' ich mit mir.", "tokens": ["\u2013", "Den", "Glau\u00b7ben", "\u2013", "ihn", "al\u00b7lein", "nehm'", "ich", "mit", "mir", "."], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "$(", "PPER", "ADV", "VVFIN", "PPER", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Und \u00fcberall verk\u00fcnd' ich Gottes Wort,", "tokens": ["Und", "\u00fc\u00b7be\u00b7rall", "ver\u00b7k\u00fcnd'", "ich", "Got\u00b7tes", "Wort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ein Weltalls-Prediger, bew\u00e4hrt durch Tat,", "tokens": ["Ein", "Welt\u00b7alls\u00b7Pre\u00b7di\u00b7ger", ",", "be\u00b7w\u00e4hrt", "durch", "Tat", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Als Glaubensbild weil' ich an jedem Ort,", "tokens": ["Als", "Glau\u00b7bens\u00b7bild", "weil'", "ich", "an", "je\u00b7dem", "Ort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVFIN", "PPER", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ein Gotteslehrer und \u2013 der V\u00f6lker Rat. \u2013", "tokens": ["Ein", "Got\u00b7tes\u00b7leh\u00b7rer", "und", "\u2013", "der", "V\u00f6l\u00b7ker", "Rat", ".", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "KON", "$(", "ART", "NN", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.11": {"line.1": {"text": "Hier steht das Mi\u00dfgeschick, doch dicht der Glaube,", "tokens": ["Hier", "steht", "das", "Mi\u00df\u00b7ge\u00b7schick", ",", "doch", "dicht", "der", "Glau\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "ADV", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dort steht das irdsche Gl\u00fcck, mit ihm das Nichts \u2013", "tokens": ["Dort", "steht", "das", "ird\u00b7sche", "Gl\u00fcck", ",", "mit", "ihm", "das", "Nichts", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,", "APPR", "PPER", "ART", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Hier bist Du jedem irdschen Schmerz zum Raube,", "tokens": ["Hier", "bist", "Du", "je\u00b7dem", "ird\u00b7schen", "Schmerz", "zum", "Rau\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PIAT", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Allein Du bleibst ein Sohn des ew'gen Lichts \u2013!", "tokens": ["Al\u00b7lein", "Du", "bleibst", "ein", "Sohn", "des", "ew'\u00b7gen", "Lichts", "\u2013", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "PPER", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$(", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.12": {"line.1": {"text": "So rief ein Engel unter Lorbeerzweigen,", "tokens": ["So", "rief", "ein", "En\u00b7gel", "un\u00b7ter", "Lor\u00b7beer\u00b7zwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und zeigte nochmals Tag und Nacht zugleich,", "tokens": ["Und", "zeig\u00b7te", "noch\u00b7mals", "Tag", "und", "Nacht", "zu\u00b7gleich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "NN", "KON", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und todesmutig sie die H\u00e4upter neigen,", "tokens": ["Und", "to\u00b7des\u00b7mu\u00b7tig", "sie", "die", "H\u00e4up\u00b7ter", "nei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und rufen laut: ", "tokens": ["Und", "ru\u00b7fen", "laut", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.13": {"line.1": {