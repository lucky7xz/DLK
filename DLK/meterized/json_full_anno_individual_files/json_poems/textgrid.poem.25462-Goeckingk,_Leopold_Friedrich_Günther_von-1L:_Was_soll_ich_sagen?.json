fo": ["word", "punct", "word", "word"], "pos": ["NN", "$,", "PTKZU", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.61": {"text": "Im Schlafen an,", "tokens": ["Im", "Schla\u00b7fen", "an", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.62": {"text": "Da\u00df Nantchen dann,", "tokens": ["Da\u00df", "Nant\u00b7chen", "dann", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADV", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.63": {"text": "So fest sie schlief,", "tokens": ["So", "fest", "sie", "schlief", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.64": {"text": "Mich h\u00f6rt' und rief:", "tokens": ["Mich", "h\u00f6rt'", "und", "rief", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.65": {"text": "Was fehlt dir, Mann?", "tokens": ["Was", "fehlt", "dir", ",", "Mann", "?"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$,", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.66": {"text": "\u00bbach! ", "tokens": ["\u00bb", "ach", "!"], "token_info": ["punct", "word", "punct"], "pos": ["$(", "ITJ", "$."], "meter": "+", "measure": "single.up"}, "line.67": {"text": "Sieh hier! \u2013 erschossen!\u00ab", "tokens": ["Sieh", "hier", "!", "\u2013", "er\u00b7schos\u00b7sen", "!", "\u00ab"], "token_info": ["word", "word", "punct", "punct", "word", "punct", "punct"], "pos": ["NE", "ADV", "$.", "$(", "VVPP", "$.", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.68": {"text": "Im Traum' doch? Possen!", "tokens": ["Im", "Traum'", "doch", "?", "Pos\u00b7sen", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "$.", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.69": {"text": "Der Traum betr\u00fcgt!", "tokens": ["Der", "Traum", "be\u00b7tr\u00fcgt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.70": {"text": "Ermuntre dich,", "tokens": ["Er\u00b7munt\u00b7re", "dich", ","], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.71": {"text": "Und sey vergn\u00fcgt!", "tokens": ["Und", "sey", "ver\u00b7gn\u00fcgt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.72": {"text": "Denn der hier liegt,", "tokens": ["Denn", "der", "hier", "liegt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADV", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.73": {"text": "Mann! das bin ich! \u2013", "tokens": ["Mann", "!", "das", "bin", "ich", "!", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["NN", "$.", "PDS", "VAFIN", "PPER", "$.", "$("], "meter": "+-+-", "measure": "trochaic.di"}, "line.74": {"text": "\u00bbo! Gott sey Dank!", "tokens": ["\u00bb", "o", "!", "Gott", "sey", "Dank", "!"], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "FM", "$.", "NN", "VAFIN", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.75": {"text": "So lebt er? \u2013 doch,", "tokens": ["So", "lebt", "er", "?", "\u2013", "doch", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$.", "$(", "ADV", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.76": {"text": "Ist er wohl noch", "tokens": ["Ist", "er", "wohl", "noch"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "ADV"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.77": {"text": "Gesund? Nicht krank?\u00ab", "tokens": ["Ge\u00b7sund", "?", "Nicht", "krank", "?", "\u00ab"], "token_info": ["word", "punct", "word", "word", "punct", "punct"], "pos": ["NN", "$.", "PTKNEG", "ADJD", "$.", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.78": {"text": "Wie k\u00f6nnen doch", "tokens": ["Wie", "k\u00f6n\u00b7nen", "doch"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "VMFIN", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.79": {"text": "Dir Tr\u00e4ume, Kind,", "tokens": ["Dir", "Tr\u00e4u\u00b7me", ",", "Kind", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PPER", "NN", "$,", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.80": {"text": "Den Schlummer rauben,", "tokens": ["Den", "Schlum\u00b7mer", "rau\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.81": {"text": "Die, kannst mir glauben,", "tokens": ["Die", ",", "kannst", "mir", "glau\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "$,", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.82": {"text": "Nur T\u00e4uscher sind", "tokens": ["Nur", "T\u00e4u\u00b7scher", "sind"], "token_info": ["word", "word", "word"], "pos": ["ADV", "NN", "VAFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.83": {"text": "Um einen Traum", "tokens": ["Um", "ei\u00b7nen", "Traum"], "token_info": ["word", "word", "word"], "pos": ["KOUI", "ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.84": {"text": "Sich so betr\u00fcben!", "tokens": ["Sich", "so", "be\u00b7tr\u00fc\u00b7ben", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PRF", "ADV", "VVINF", "$."], "meter": "++-+-", "measure": "iambic.di"}, "line.85": {"text": "Er hat ja kaum", "tokens": ["Er", "hat", "ja", "kaum"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.86": {"text": "An dich geschrieben?", "tokens": ["An", "dich", "ge\u00b7schrie\u00b7ben", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVPP", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.87": {"text": "Wer wird gleich schier", "tokens": ["Wer", "wird", "gleich", "schier"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "ADV", "ADJD"], "meter": "-+-+", "measure": "iambic.di"}, "line.88": {"text": "Das schlimmste w\u00e4hnen?", "tokens": ["Das", "schlimms\u00b7te", "w\u00e4h\u00b7nen", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.89": {"text": "Komm'! la\u00df die Thr\u00e4nen", "tokens": ["Komm'", "!", "la\u00df", "die", "Thr\u00e4\u00b7nen"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["VVIMP", "$.", "VVIMP", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.90": {"text": "Abtrocknen dir! \u2013", "tokens": ["Ab\u00b7trock\u00b7nen", "dir", "!", "\u2013"], "token_info": ["word", "word", "punct", "punct"], "pos": ["NN", "PPER", "$.", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.91": {"text": "Nun Herz! schlaf' ein!", "tokens": ["Nun", "Herz", "!", "schlaf'", "ein", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "NN", "$.", "VVFIN", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.92": {"text": "Und ", "tokens": ["Und"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.93": {"text": "Wirst du, Gerechter", "tokens": ["Wirst", "du", ",", "Ge\u00b7rech\u00b7ter"], "token_info": ["word", "word", "punct", "word"], "pos": ["VAFIN", "PPER", "$,", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.94": {"text": "Im Himmel, seyn! \u2013", "tokens": ["Im", "Him\u00b7mel", ",", "seyn", "!", "\u2013"], "token_info": ["word", "word", "punct", "word", "punct", "punct"], "pos": ["APPRART", "NN", "$,", "VAINF", "$.", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.95": {"text": "\u00bbdas wird er seyn!", "tokens": ["\u00bb", "das", "wird", "er", "seyn", "!"], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PDS", "VAFIN", "PPER", "VAINF", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.96": {"text": "Wohlan, ihr Z\u00e4hren,", "tokens": ["Wo\u00b7hlan", ",", "ihr", "Z\u00e4h\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ADV", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.97": {"text": "So haltet ein.", "tokens": ["So", "hal\u00b7tet", "ein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.98": {"text": "Wann Gott gew\u00e4hren", "tokens": ["Wann", "Gott", "ge\u00b7w\u00e4h\u00b7ren"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "NN", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.99": {"text": "Den Wunsch mir wird,", "tokens": ["Den", "Wunsch", "mir", "wird", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VAFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.100": {"text": "Da\u00df nicht die Klinge", "tokens": ["Da\u00df", "nicht", "die", "Klin\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PTKNEG", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.101": {"text": "Des Kriegs mehr schwirrt,", "tokens": ["Des", "Kriegs", "mehr", "schwirrt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.102": {"text": "Und ich den Freund", "tokens": ["Und", "ich", "den", "Freund"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPER", "ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.103": {"text": "Wie sonst umschlinge:", "tokens": ["Wie", "sonst", "um\u00b7schlin\u00b7ge", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VVFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.104": {"text": "Dann weinet beide", "tokens": ["Dann", "wei\u00b7net", "bei\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS"], "meter": "-+-+-", "measure": "iambic.di"}, "line.105": {"text": "Ihr Augen, weint", "tokens": ["Ihr", "Au\u00b7gen", ",", "weint"], "token_info": ["word", "word", "punct", "word"], "pos": ["PPOSAT", "NN", "$,", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.106": {"text": "Gott Dank, ihm Freude!\u00ab", "tokens": ["Gott", "Dank", ",", "ihm", "Freu\u00b7de", "!", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["NN", "NN", "$,", "PPER", "NN", "$.", "$("], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}}}}