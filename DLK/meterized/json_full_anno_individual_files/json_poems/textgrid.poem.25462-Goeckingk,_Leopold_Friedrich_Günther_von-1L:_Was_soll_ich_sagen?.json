{"textgrid.poem.25462": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Was soll ich sagen?", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was soll ich sagen?", "tokens": ["Was", "soll", "ich", "sa\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "VVINF", "$."], "meter": "---+-", "measure": "unknown.measure.single"}, "line.2": {"text": "O Freund! O Freund!", "tokens": ["O", "Freund", "!", "O", "Freund", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "NE", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Seit dreien Tagen", "tokens": ["Seit", "drei\u00b7en", "Ta\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "CARD", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Hab' ich geweint.", "tokens": ["Hab'", "ich", "ge\u00b7weint", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVPP", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Die Augen gehn", "tokens": ["Die", "Au\u00b7gen", "gehn"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVINF"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Mir itzt noch \u00fcber,", "tokens": ["Mir", "itzt", "noch", "\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "APPR", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Denn dich, mein Lieber,", "tokens": ["Denn", "dich", ",", "mein", "Lie\u00b7ber", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "Nicht wiedersehn:", "tokens": ["Nicht", "wie\u00b7der\u00b7sehn", ":"], "token_info": ["word", "word", "punct"], "pos": ["PTKNEG", "VVINF", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "O! der Gedanke", "tokens": ["O", "!", "der", "Ge\u00b7dan\u00b7ke"], "token_info": ["word", "punct", "word", "word"], "pos": ["NE", "$.", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "Macht meinen Wein", "tokens": ["Macht", "mei\u00b7nen", "Wein"], "token_info": ["word", "word", "word"], "pos": ["NN", "PPOSAT", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.11": {"text": "Zum Schierlingstranke,", "tokens": ["Zum", "Schier\u00b7lings\u00b7tran\u00b7ke", ","], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.12": {"text": "Mischt Wermuth ein", "tokens": ["Mischt", "Wer\u00b7muth", "ein"], "token_info": ["word", "word", "word"], "pos": ["NN", "NN", "ART"], "meter": "+-+-", "measure": "trochaic.di"}, "line.13": {"text": "In meine Speise,", "tokens": ["In", "mei\u00b7ne", "Spei\u00b7se", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.14": {"text": "Macht meinen Schlaf", "tokens": ["Macht", "mei\u00b7nen", "Schlaf"], "token_info": ["word", "word", "word"], "pos": ["NN", "PPOSAT", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.15": {"text": "Verwirrt und leise,", "tokens": ["Ver\u00b7wirrt", "und", "lei\u00b7se", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "KON", "ADJD", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.16": {"text": "Und mich zum Greise.", "tokens": ["Und", "mich", "zum", "Grei\u00b7se", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.17": {"text": "Denn ach! dich traf,", "tokens": ["Denn", "ach", "!", "dich", "traf", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "XY", "$.", "PPER", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.18": {"text": "In meinem Traume,", "tokens": ["In", "mei\u00b7nem", "Trau\u00b7me", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.19": {"text": "Schon ein Kroat,", "tokens": ["Schon", "ein", "Kroat", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.20": {"text": "Der hinterm Baume", "tokens": ["Der", "hin\u00b7term", "Bau\u00b7me"], "token_info": ["word", "word", "word"], "pos": ["ART", "APPRART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.21": {"text": "Hervor nun trat,", "tokens": ["Her\u00b7vor", "nun", "trat", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.22": {"text": "Dich, der voll Blut", "tokens": ["Dich", ",", "der", "voll", "Blut"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["PPER", "$,", "PRELS", "ADJD", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.23": {"text": "Vom H\u00fcgel rollte,", "tokens": ["Vom", "H\u00fc\u00b7gel", "roll\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.24": {"text": "Auspl\u00fcndern wollte,", "tokens": ["Aus\u00b7pl\u00fcn\u00b7dern", "woll\u00b7te", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VMFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.25": {"text": "Und deinen Hut", "tokens": ["Und", "dei\u00b7nen", "Hut"], "token_info": ["word", "word", "word"], "pos": ["KON", "PPOSAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.26": {"text": "Schon nahm, als Wuth", "tokens": ["Schon", "nahm", ",", "als", "Wuth"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "$,", "KOUS", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.27": {"text": "Mir Adlersfl\u00fcgel", "tokens": ["Mir", "Ad\u00b7ler\u00b7sfl\u00fc\u00b7gel"], "token_info": ["word", "word"], "pos": ["NE", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.28": {"text": "Zur Rache gab.", "tokens": ["Zur", "Ra\u00b7che", "gab", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.29": {"text": "Ich sprang den H\u00fcgel", "tokens": ["Ich", "sprang", "den", "H\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.30": {"text": "Im Hui! herab,", "tokens": ["Im", "Hui", "!", "her\u00b7ab", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["APPRART", "FM", "$.", "ADV", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.31": {"text": "Ri\u00df aus der Scheide", "tokens": ["Ri\u00df", "aus", "der", "Schei\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.32": {"text": "Den Degen dir;", "tokens": ["Den", "De\u00b7gen", "dir", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.33": {"text": "Ins Eingeweide", "tokens": ["Ins", "Ein\u00b7ge\u00b7wei\u00b7de"], "token_info": ["word", "word"], "pos": ["APPRART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.34": {"text": "Stie\u00df ich mit Gier", "tokens": ["Stie\u00df", "ich", "mit", "Gier"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.35": {"text": "Ihn dem Kroaten,", "tokens": ["Ihn", "dem", "Kroa\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.36": {"text": "Da\u00df selbst ihm aus", "tokens": ["Da\u00df", "selbst", "ihm", "aus"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ADV", "PPER", "APPR"], "meter": "-+-+", "measure": "iambic.di"}, "line.37": {"text": "Dem Kopf' heraus", "tokens": ["Dem", "Kopf'", "he\u00b7raus"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "PTKVZ"], "meter": "-+-+", "measure": "iambic.di"}, "line.38": {"text": "Die Augen traten.", "tokens": ["Die", "Au\u00b7gen", "tra\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.39": {"text": "Drauf legt' ich mir", "tokens": ["Drauf", "legt'", "ich", "mir"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "PPER"], "meter": "-+-+", "measure": "iambic.di"}, "line.40": {"text": "Dich auf den Schoo\u00df,", "tokens": ["Dich", "auf", "den", "Schoo\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.41": {"text": "Und machte dir", "tokens": ["Und", "mach\u00b7te", "dir"], "token_info": ["word", "word", "word"], "pos": ["KON", "VVFIN", "PPER"], "meter": "-+-+", "measure": "iambic.di"}, "line.42": {"text": "Den Busen blo\u00df,", "tokens": ["Den", "Bu\u00b7sen", "blo\u00df", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.43": {"text": "Stopft' in die Wunde", "tokens": ["Stopft'", "in", "die", "Wun\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.44": {"text": "Mein Tuch hinein,", "tokens": ["Mein", "Tuch", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.45": {"text": "Blie\u00df mit dem Munde", "tokens": ["Blie\u00df", "mit", "dem", "Mun\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.46": {"text": "Dir Odem ein,", "tokens": ["Dir", "O\u00b7dem", "ein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "NN", "PTKVZ", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.47": {"text": "Und rief unz\u00e4hlig", "tokens": ["Und", "rief", "un\u00b7z\u00e4h\u00b7lig"], "token_info": ["word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD"], "meter": "-+-+-", "measure": "iambic.di"}, "line.48": {"text": "Bei Namen dich.", "tokens": ["Bei", "Na\u00b7men", "dich", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.49": {"text": "Da regt' allm\u00e4hlig", "tokens": ["Da", "regt'", "all\u00b7m\u00e4h\u00b7lig"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VVFIN", "ADJD"], "meter": "-+-+-", "measure": "iambic.di"}, "line.50": {"text": "Dein Auge sich;", "tokens": ["Dein", "Au\u00b7ge", "sich", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PRF", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.51": {"text": "Mit Danken blicktest", "tokens": ["Mit", "Dan\u00b7ken", "blick\u00b7test"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NN", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.52": {"text": "Noch einmal du", "tokens": ["Noch", "ein\u00b7mal", "du"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADV", "PPER"], "meter": "-+-+", "measure": "iambic.di"}, "line.53": {"text": "Mich an, und nicktest", "tokens": ["Mich", "an", ",", "und", "nick\u00b7test"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPER", "PTKVZ", "$,", "KON", "PTKNEG"], "meter": "-+-+-", "measure": "iambic.di"}, "line.54": {"text": "Leb' wohl! mir zu.", "tokens": ["Leb'", "wohl", "!", "mir", "zu", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "$.", "PPER", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.55": {"text": "Ich aber sprang", "tokens": ["Ich", "a\u00b7ber", "sprang"], "token_info": ["word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.56": {"text": "Nun auf, und ging", "tokens": ["Nun", "auf", ",", "und", "ging"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADV", "PTKVZ", "$,", "KON", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.57": {"text": "Umher, und rang", "tokens": ["Um\u00b7her", ",", "und", "rang"], "token_info": ["word", "punct", "word", "word"], "pos": ["ADV", "$,", "KON", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.58": {"text": "Die H\u00e4nd' und fing", "tokens": ["Die", "H\u00e4nd'", "und", "fing"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.59": {"text": "So laut, um deinen", "tokens": ["So", "laut", ",", "um", "dei\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADV", "ADJD", "$,", "KOUI", "PPOSAT"], "meter": "-+-+-", "measure": "iambic.di"}, "line.60": {"text": "Verlust, zu weinen", "tokens": ["Ver\u00b7lust", ",", "zu", "wei\u00b7nen"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$,", "PTKZU", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.61": {"text": "Im Schlafen an,", "tokens": ["Im", "Schla\u00b7fen", "an", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.62": {"text": "Da\u00df Nantchen dann,", "tokens": ["Da\u00df", "Nant\u00b7chen", "dann", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADV", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.63": {"text": "So fest sie schlief,", "tokens": ["So", "fest", "sie", "schlief", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.64": {"text": "Mich h\u00f6rt' und rief:", "tokens": ["Mich", "h\u00f6rt'", "und", "rief", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.65": {"text": "Was fehlt dir, Mann?", "tokens": ["Was", "fehlt", "dir", ",", "Mann", "?"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$,", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.66": {"text": "\u00bbach! ", "tokens": ["\u00bb", "ach", "!"], "token_info": ["punct", "word", "punct"], "pos": ["$(", "ITJ", "$."], "meter": "+", "measure": "single.up"}, "line.67": {"text": "Sieh hier! \u2013 erschossen!\u00ab", "tokens": ["Sieh", "hier", "!", "\u2013", "er\u00b7schos\u00b7sen", "!", "\u00ab"], "token_info": ["word", "word", "punct", "punct", "word", "punct", "punct"], "pos": ["NE", "ADV", "$.", "$(", "VVPP", "$.", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.68": {"text": "Im Traum' doch? Possen!", "tokens": ["Im", "Traum'", "doch", "?", "Pos\u00b7sen", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "$.", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.69": {"text": "Der Traum betr\u00fcgt!", "tokens": ["Der", "Traum", "be\u00b7tr\u00fcgt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.70": {"text": "Ermuntre dich,", "tokens": ["Er\u00b7munt\u00b7re", "dich", ","], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.71": {"text": "Und sey vergn\u00fcgt!", "tokens": ["Und", "sey", "ver\u00b7gn\u00fcgt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.72": {"text": "Denn der hier liegt,", "tokens": ["Denn", "der", "hier", "liegt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADV", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.73": {"text": "Mann! das bin ich! \u2013", "tokens": ["Mann", "!", "das", "bin", "ich", "!", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["NN", "$.", "PDS", "VAFIN", "PPER", "$.", "$("], "meter": "+-+-", "measure": "trochaic.di"}, "line.74": {"text": "\u00bbo! Gott sey Dank!", "tokens": ["\u00bb", "o", "!", "Gott", "sey", "Dank", "!"], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "FM", "$.", "NN", "VAFIN", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.75": {"text": "So lebt er? \u2013 doch,", "tokens": ["So", "lebt", "er", "?", "\u2013", "doch", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$.", "$(", "ADV", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.76": {"text": "Ist er wohl noch", "tokens": ["Ist", "er", "wohl", "noch"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "ADV"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.77": {"text": "Gesund? Nicht krank?\u00ab", "tokens": ["Ge\u00b7sund", "?", "Nicht", "krank", "?", "\u00ab"], "token_info": ["word", "punct", "word", "word", "punct", "punct"], "pos": ["NN", "$.", "PTKNEG", "ADJD", "$.", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.78": {"text": "Wie k\u00f6nnen doch", "tokens": ["Wie", "k\u00f6n\u00b7nen", "doch"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "VMFIN", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.79": {"text": "Dir Tr\u00e4ume, Kind,", "tokens": ["Dir", "Tr\u00e4u\u00b7me", ",", "Kind", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PPER", "NN", "$,", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.80": {"text": "Den Schlummer rauben,", "tokens": ["Den", "Schlum\u00b7mer", "rau\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.81": {"text": "Die, kannst mir glauben,", "tokens": ["Die", ",", "kannst", "mir", "glau\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "$,", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.82": {"text": "Nur T\u00e4uscher sind", "tokens": ["Nur", "T\u00e4u\u00b7scher", "sind"], "token_info": ["word", "word", "word"], "pos": ["ADV", "NN", "VAFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.83": {"text": "Um einen Traum", "tokens": ["Um", "ei\u00b7nen", "Traum"], "token_info": ["word", "word", "word"], "pos": ["KOUI", "ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.84": {"text": "Sich so betr\u00fcben!", "tokens": ["Sich", "so", "be\u00b7tr\u00fc\u00b7ben", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PRF", "ADV", "VVINF", "$."], "meter": "++-+-", "measure": "iambic.di"}, "line.85": {"text": "Er hat ja kaum", "tokens": ["Er", "hat", "ja", "kaum"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.86": {"text": "An dich geschrieben?", "tokens": ["An", "dich", "ge\u00b7schrie\u00b7ben", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVPP", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.87": {"text": "Wer wird gleich schier", "tokens": ["Wer", "wird", "gleich", "schier"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "ADV", "ADJD"], "meter": "-+-+", "measure": "iambic.di"}, "line.88": {"text": "Das schlimmste w\u00e4hnen?", "tokens": ["Das", "schlimms\u00b7te", "w\u00e4h\u00b7nen", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.89": {"text": "Komm'! la\u00df die Thr\u00e4nen", "tokens": ["Komm'", "!", "la\u00df", "die", "Thr\u00e4\u00b7nen"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["VVIMP", "$.", "VVIMP", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.90": {"text": "Abtrocknen dir! \u2013", "tokens": ["Ab\u00b7trock\u00b7nen", "dir", "!", "\u2013"], "token_info": ["word", "word", "punct", "punct"], "pos": ["NN", "PPER", "$.", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.91": {"text": "Nun Herz! schlaf' ein!", "tokens": ["Nun", "Herz", "!", "schlaf'", "ein", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "NN", "$.", "VVFIN", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.92": {"text": "Und ", "tokens": ["Und"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.93": {"text": "Wirst du, Gerechter", "tokens": ["Wirst", "du", ",", "Ge\u00b7rech\u00b7ter"], "token_info": ["word", "word", "punct", "word"], "pos": ["VAFIN", "PPER", "$,", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.94": {"text": "Im Himmel, seyn! \u2013", "tokens": ["Im", "Him\u00b7mel", ",", "seyn", "!", "\u2013"], "token_info": ["word", "word", "punct", "word", "punct", "punct"], "pos": ["APPRART", "NN", "$,", "VAINF", "$.", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.95": {"text": "\u00bbdas wird er seyn!", "tokens": ["\u00bb", "das", "wird", "er", "seyn", "!"], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PDS", "VAFIN", "PPER", "VAINF", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.96": {"text": "Wohlan, ihr Z\u00e4hren,", "tokens": ["Wo\u00b7hlan", ",", "ihr", "Z\u00e4h\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ADV", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.97": {"text": "So haltet ein.", "tokens": ["So", "hal\u00b7tet", "ein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.98": {"text": "Wann Gott gew\u00e4hren", "tokens": ["Wann", "Gott", "ge\u00b7w\u00e4h\u00b7ren"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "NN", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.99": {"text": "Den Wunsch mir wird,", "tokens": ["Den", "Wunsch", "mir", "wird", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VAFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.100": {"text": "Da\u00df nicht die Klinge", "tokens": ["Da\u00df", "nicht", "die", "Klin\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PTKNEG", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.101": {"text": "Des Kriegs mehr schwirrt,", "tokens": ["Des", "Kriegs", "mehr", "schwirrt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.102": {"text": "Und ich den Freund", "tokens": ["Und", "ich", "den", "Freund"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPER", "ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.103": {"text": "Wie sonst umschlinge:", "tokens": ["Wie", "sonst", "um\u00b7schlin\u00b7ge", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VVFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.104": {"text": "Dann weinet 