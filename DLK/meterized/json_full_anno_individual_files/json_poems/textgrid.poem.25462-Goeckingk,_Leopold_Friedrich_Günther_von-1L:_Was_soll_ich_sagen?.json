 "pos": ["PPER", "ADV", "ADV", "APPR", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Denn dich, mein Lieber,", "tokens": ["Denn", "dich", ",", "mein", "Lie\u00b7ber", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "Nicht wiedersehn:", "tokens": ["Nicht", "wie\u00b7der\u00b7sehn", ":"], "token_info": ["word", "word", "punct"], "pos": ["PTKNEG", "VVINF", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "O! der Gedanke", "tokens": ["O", "!", "der", "Ge\u00b7dan\u00b7ke"], "token_info": ["word", "punct", "word", "word"], "pos": ["NE", "$.", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "Macht meinen Wein", "tokens": ["Macht", "mei\u00b7nen", "Wein"], "token_info": ["word", "word", "word"], "pos": ["NN", "PPOSAT", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.11": {"text": "Zum Schierlingstranke,", "tokens": ["Zum", "Schier\u00b7lings\u00b7tran\u00b7ke", ","], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.12": {"text": "Mischt Wermuth ein", "tokens": ["Mischt", "Wer\u00b7muth", "ein"], "token_info": ["word", "word", "word"], "pos": ["NN", "NN", "ART"], "meter": "+-+-", "measure": "trochaic.di"}, "line.13": {"text": "In meine Speise,", "tokens": ["In", "mei\u00b7ne", "Spei\u00b7se", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.14": {"text": "Macht meinen Schlaf", "tokens": ["Macht", "mei\u00b7nen", "Schlaf"], "token_info": ["word", "word", "word"], "pos": ["NN", "PPOSAT", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.15": {"text": "Verwirrt und leise,", "tokens": ["Ver\u00b7wirrt", "und", "lei\u00b7se", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "KON", "ADJD", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.16": {"text": "Und mich zum Greise.", "tokens": ["Und", "mich", "zum", "Grei\u00b7se", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.17": {"text": "Denn ach! dich traf,", "tokens": ["Denn", "ach", "!", "dich", "traf", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "XY", "$.", "PPER", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.18": {"text": "In meinem Traume,", "tokens": ["In", "mei\u00b7nem", "Trau\u00b7me", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.19": {"text": "Schon ein Kroat,", "tokens": ["Schon", "ein", "Kroat", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.20": {"text": "Der hinterm Baume", "tokens": ["Der", "hin\u00b7term", "Bau\u00b7me"], "token_info": ["word", "word", "word"], "pos": ["ART", "APPRART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.21": {"text": "Hervor nun trat,", "tokens": ["Her\u00b7vor", "nun", "trat", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.22": {"text": "Dich, der voll Blut", "tokens": ["Dich", ",", "der", "voll", "Blut"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["PPER", "$,", "PRELS", "ADJD", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.23": {"text": "Vom H\u00fcgel rollte,", "tokens": ["Vom", "H\u00fc\u00b7gel", "roll\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.24": {"text": "Auspl\u00fcndern wollte,", "tokens": ["Aus\u00b7pl\u00fcn\u00b7dern", "woll\u00b7te", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VMFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.25": {"text": "Und deinen Hut", "tokens": ["Und", "dei\u00b7nen", "Hut"], "token_info": ["word", "word", "word"], "pos": ["KON", "PPOSAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.26": {"text": "Schon nahm, als Wuth", "tokens": ["Schon", "nahm", ",", "als", "Wuth"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "$,", "KOUS", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.27": {"text": "Mir Adlersfl\u00fcgel", "tokens": ["Mir", "Ad\u00b7ler\u00b7sfl\u00fc\u00b7gel"], "token_info": ["word", "word"], "pos": ["NE", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.28": {"text": "Zur Rache gab.", "tokens": ["Zur", "Ra\u00b7che", "gab", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.29": {"text": "Ich sprang den H\u00fcgel", "tokens": ["Ich", "sprang", "den", "H\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.30": {"text": "Im Hui! herab,", "tokens": ["Im", "Hui", "!", "her\u00b7ab", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["APPRART", "FM", "$.", "ADV", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.31": {"text": "Ri\u00df aus der Scheide", "tokens": ["Ri\u00df", "aus", "der", "Schei\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.32": {"text": "Den Degen dir;", "tokens": ["Den", "De\u00b7gen", "dir", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.33": {"text": "Ins Eingeweide", "tokens": ["Ins", "Ein\u00b7ge\u00b7wei\u00b7de"], "token_info": ["word", "word"], "pos": ["APPRART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.34": {"text": "Stie\u00df ich mit Gier", "tokens": ["Stie\u00df", "ich", "mit", "Gier"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.35": {"text": "Ihn dem Kroaten,", "tokens": ["Ihn", "dem", "Kroa\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.36": {"text": "Da\u00df selbst ihm aus", "tokens": ["Da\u00df", "selbst", "ihm", "aus"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ADV", "PPER", "APPR"], "meter": "-+-+", "measure": "iambic.di"}, "line.37": {"text": "Dem Kopf' heraus", "tokens": ["Dem", "Kopf'", "he\u00b7raus"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "PTKVZ"], "meter": "-+-+", "measure": "iambic.di"}, "line.38": {"text": "Die Augen traten.", "tokens": ["Die", "Au\u00b7gen", "tra\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.39": {"text": "Drauf legt' ich mir", "tokens": ["Drauf", "legt'", "ich", "mir"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "PPER"], "meter": "-+-+", "measure": "iambic.di"}, "line.40": {"text": "Dich auf den Schoo\u00df,", "tokens": ["Dich", "auf", "den", "Schoo\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.41": {"text": "Und machte dir", "tokens": ["Und", "mach\u00b7te", "dir"], "token_info": ["word", "word", "word"], "pos": ["KON", "VVFIN", "PPER"], "meter": "-+-+", "measure": "iambic.di"}, "line.42": {"text": "Den Busen blo\u00df,", "tokens": ["Den", "Bu\u00b7sen", "blo\u00df", ","], "token_info": ["word", "word", "w