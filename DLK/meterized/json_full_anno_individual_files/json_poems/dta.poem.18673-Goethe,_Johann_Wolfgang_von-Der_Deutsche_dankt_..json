{"dta.poem.18673": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang von", "birth": "N.A.", "death": "N.A."}, "title": "Der Deutsche dankt .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1819", "urn": "urn:nbn:de:kobv:b4-200905191692", "language": ["de:0.99"], "booktitle": "Goethe, Johann Wolfgang von: West-\u00f6stlicher Divan. Stuttgart, 1819."}, "poem": {"stanza.1": {"line.1": {"text": "Heiliger Ebusund, du hast\u2019s getroffen!", "tokens": ["Hei\u00b7li\u00b7ger", "E\u00b7bu\u00b7sund", ",", "du", "hast's", "ge\u00b7trof\u00b7fen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PPER", "VAFIN", "VVPP", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Solche Heilige w\u00fcnschet sich der Dichter:", "tokens": ["Sol\u00b7che", "Hei\u00b7li\u00b7ge", "w\u00fcn\u00b7schet", "sich", "der", "Dich\u00b7ter", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "VVFIN", "PRF", "ART", "NN", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Denn gerade jene Kleinigkeiten", "tokens": ["Denn", "ge\u00b7ra\u00b7de", "je\u00b7ne", "Klei\u00b7nig\u00b7kei\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "PDAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Ausserhalb der Gr\u00e4nze des Gesetzes,", "tokens": ["Aus\u00b7ser\u00b7halb", "der", "Gr\u00e4n\u00b7ze", "des", "Ge\u00b7set\u00b7zes", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Sind das Erbtheil wo er, \u00fcberm\u00fcthig,", "tokens": ["Sind", "das", "E\u00b7rbtheil", "wo", "er", ",", "\u00fc\u00b7ber\u00b7m\u00fct\u00b7hig", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PWAV", "PPER", "$,", "ADJD", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Selbst im Kummer lustig, sich beweget.", "tokens": ["Selbst", "im", "Kum\u00b7mer", "lus\u00b7tig", ",", "sich", "be\u00b7we\u00b7get", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "ADJD", "$,", "PRF", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Schlangengift und Theriak muss", "tokens": ["Schlan\u00b7gen\u00b7gift", "und", "The\u00b7ri\u00b7ak", "muss"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "KON", "XY", "XY"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.8": {"text": "Ihm das eine wie das andere scheinen,", "tokens": ["Ihm", "das", "ei\u00b7ne", "wie", "das", "an\u00b7de\u00b7re", "schei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "PIS", "KOKOM", "ART", "ADJA", "VVFIN", "$,"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.9": {"text": "T\u00f6dten wird nicht jenes, dies nicht heilen:", "tokens": ["T\u00f6d\u00b7ten", "wird", "nicht", "je\u00b7nes", ",", "dies", "nicht", "hei\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PTKNEG", "PDS", "$,", "PDS", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "Denn das wahre Leben ist des Handelns", "tokens": ["Denn", "das", "wah\u00b7re", "Le\u00b7ben", "ist", "des", "Han\u00b7delns"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "VAFIN", "ART", "NN"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.11": {"text": "Ew\u2019ge Unschuld, die sich so erweiset", "tokens": ["Ew'\u00b7ge", "Un\u00b7schuld", ",", "die", "sich", "so", "er\u00b7wei\u00b7set"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "PRELS", "PRF", "ADV", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.12": {"text": "Dass sie niemand schadet als sich selber.", "tokens": ["Dass", "sie", "nie\u00b7mand", "scha\u00b7det", "als", "sich", "sel\u00b7ber", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIS", "VVFIN", "KOUS", "PRF", "ADV", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.13": {"text": "Und so kann der alte Dichter hoffen", "tokens": ["Und", "so", "kann", "der", "al\u00b7te", "Dich\u00b7ter", "hof\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VMFIN", "ART", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.14": {"text": "Dass die Houris ihn im Paradiese,", "tokens": ["Dass", "die", "Hou\u00b7ris", "ihn", "im", "Pa\u00b7ra\u00b7die\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NE", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.15": {"text": "Als verkl\u00e4rten J\u00fcngling wohl empfangen,", "tokens": ["Als", "ver\u00b7kl\u00e4r\u00b7ten", "J\u00fcng\u00b7ling", "wohl", "emp\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.16": {"text": "Heiliger Ebusund, du hast\u2019s getroffen!", "tokens": ["Hei\u00b7li\u00b7ger", "E\u00b7bu\u00b7sund", ",", "du", "hast's", "ge\u00b7trof\u00b7fen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PPER", "VAFIN", "VVPP", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}}}}