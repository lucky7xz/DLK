{"textgrid.poem.46188": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "Ueber k. mayt. sigreichen abschid", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gro\u00df billich ist dein nam, als dessen wert so gro\u00df,", "tokens": ["Gro\u00df", "bil\u00b7lich", "ist", "dein", "nam", ",", "als", "des\u00b7sen", "wert", "so", "gro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "VAFIN", "PPOSAT", "VVFIN", "$,", "KOUS", "PDS", "VVFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "da\u00df fridenszeit sein haupt zu herschen und zu raten,", "tokens": ["da\u00df", "fri\u00b7dens\u00b7zeit", "sein", "haupt", "zu", "her\u00b7schen", "und", "zu", "ra\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPOSAT", "NN", "PTKZU", "VVINF", "KON", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "da\u00df kriegszeit seine faust durch gro\u00dfe heldenthaten", "tokens": ["da\u00df", "kriegs\u00b7zeit", "sei\u00b7ne", "faust", "durch", "gro\u00b7\u00dfe", "hel\u00b7den\u00b7tha\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "PPOSAT", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "der helden ph\u00f6nix ihn bewehrten stets gleichlos.", "tokens": ["der", "hel\u00b7den", "ph\u00f6\u00b7nix", "ihn", "be\u00b7wehr\u00b7ten", "stets", "gleich\u00b7los", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "VVFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Kein werters haupt noch hand, gewafnet oder blo\u00df,", "tokens": ["Kein", "wer\u00b7ters", "haupt", "noch", "hand", ",", "ge\u00b7waf\u00b7net", "o\u00b7der", "blo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "ADV", "NN", "$,", "VVPP", "KON", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "trug jemals scepter, kron, schwert, zier der potentaten;", "tokens": ["trug", "je\u00b7mals", "scep\u00b7ter", ",", "kron", ",", "schwert", ",", "zier", "der", "po\u00b7ten\u00b7ta\u00b7ten", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJD", "$,", "NN", "$,", "VVFIN", "$,", "ADV", "ART", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "von keinem edlern blut, belebend die soldaten,", "tokens": ["von", "kei\u00b7nem", "ed\u00b7lern", "blut", ",", "be\u00b7le\u00b7bend", "die", "sol\u00b7da\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "$,", "ADJD", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "sah man jemals schamrot der erden bleiche scho\u00df.", "tokens": ["sah", "man", "je\u00b7mals", "scham\u00b7rot", "der", "er\u00b7den", "blei\u00b7che", "scho\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "VVFIN", "ART", "NN", "ADJA", "VVFIN", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.9": {"text": "Auch kont kein andrer held, was er gethan, verwalten,", "tokens": ["Auch", "kont", "kein", "an\u00b7drer", "held", ",