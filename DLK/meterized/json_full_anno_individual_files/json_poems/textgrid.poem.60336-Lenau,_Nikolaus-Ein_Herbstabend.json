{"textgrid.poem.60336": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Ein Herbstabend", "genre": "verse", "period": "N.A.", "pub_year": 1838, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es weht der Wind so k\u00fchl, entlaubend rings die \u00c4ste,", "tokens": ["Es", "weht", "der", "Wind", "so", "k\u00fchl", ",", "ent\u00b7lau\u00b7bend", "rings", "die", "\u00c4s\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "ADJD", "$,", "VVPP", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Er ruft zum Wald hinein: Gut Nacht, ihr Erdeng\u00e4ste!", "tokens": ["Er", "ruft", "zum", "Wald", "hin\u00b7ein", ":", "Gut", "Nacht", ",", "ihr", "Er\u00b7den\u00b7g\u00e4s\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "PTKVZ", "$.", "ADJD", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Am H\u00fcgel strahlt der Mond, die grauen Wolken jagen", "tokens": ["Am", "H\u00fc\u00b7gel", "strahlt", "der", "Mond", ",", "die", "grau\u00b7en", "Wol\u00b7ken", "ja\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$,", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Schnell \u00fcbers Tal hinaus, wo alle W\u00e4lder klagen.", "tokens": ["Schnell", "\u00fc\u00b7bers", "Tal", "hin\u00b7aus", ",", "wo", "al\u00b7le", "W\u00e4l\u00b7der", "kla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "APZR", "$,", "PWAV", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Das B\u00e4chlein schleicht hinab, von abgestorbnen Hainen", "tokens": ["Das", "B\u00e4ch\u00b7lein", "schleicht", "hin\u00b7ab", ",", "von", "ab\u00b7ge\u00b7storb\u00b7nen", "Hai\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Tr\u00e4gt es die Bl\u00e4tter fort mit halbersticktem Weinen.", "tokens": ["Tr\u00e4gt", "es", "die", "Bl\u00e4t\u00b7ter", "fort", "mit", "hal\u00b7ber\u00b7stick\u00b7tem", "Wei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKVZ", "APPR", "ADJA", "NN", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}}, "stanza.4": {"line.1": {"text": "Nie h\u00f6rt ich einen Quell so leise traurig klingend,", "tokens": ["Nie", "h\u00f6rt", "ich", "ei\u00b7nen", "Quell", "so", "lei\u00b7se", "trau\u00b7rig", "klin\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "ADV", "ADJD", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Weid am Ufer steht, die weichen \u00c4ste ringend.", "tokens": ["Die", "Weid", "am", "U\u00b7fer", "steht", ",", "die", "wei\u00b7chen", "\u00c4s\u00b7te", "rin\u00b7gend", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVFIN", "$,", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Und eines toten Freunds gedenkend lausch ich nieder", "tokens": ["Und", "ei\u00b7nes", "to\u00b7ten", "Freunds", "ge\u00b7den\u00b7kend", "lausch", "ich", "nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "VVPP", "VVFIN", "PPER", "PTKVZ"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zum Quell, der murmelt stets: wir sehen uns nicht wieder!", "tokens": ["Zum", "Quell", ",", "der", "mur\u00b7melt", "stets", ":", "wir", "se\u00b7hen", "uns", "nicht", "wie\u00b7der", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PRELS", "VVFIN", "ADV", "$.", "PPER", "VVFIN", "PPER", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Horch! pl\u00f6tzlich in der Luft ein schnatterndes Geplauder:", "tokens": ["Horch", "!", "pl\u00f6tz\u00b7lich", "in", "der", "Luft", "ein", "schnat\u00b7tern\u00b7des", "Ge\u00b7plau\u00b7der", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "ADJD", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wildg\u00e4nse auf der Flucht vor winterlichem Schauder.", "tokens": ["Wild\u00b7g\u00e4n\u00b7se", "auf", "der", "Flucht", "vor", "win\u00b7ter\u00b7li\u00b7chem", "Schau\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Sie jagen hinter sich den Herbst mit raschen Fl\u00fcgeln,", "tokens": ["Sie", "ja\u00b7gen", "hin\u00b7ter", "sich", "den", "Herbst", "mit", "ra\u00b7schen", "Fl\u00fc\u00b7geln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PRF", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sie lassen scheu zur\u00fcck das Sterben auf den H\u00fcgeln.", "tokens": ["Sie", "las\u00b7sen", "scheu", "zu\u00b7r\u00fcck", "das", "Ster\u00b7ben", "auf", "den", "H\u00fc\u00b7geln", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PTKVZ", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Wo sind sie? ha! wie schnell sie dort vor\u00fcberstreichen", "tokens": ["Wo", "sind", "sie", "?", "ha", "!", "wie", "schnell", "sie", "dort", "vor\u00b7\u00fc\u00b7bers\u00b7trei\u00b7chen"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "PPER", "$.", "ITJ", "$.", "PWAV", "ADJD", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Am hellen Mond und jetzt unsichtbar schon entweichen;", "tokens": ["Am", "hel\u00b7len", "Mond", "und", "jetzt", "un\u00b7sicht\u00b7bar", "schon", "ent\u00b7wei\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "KON", "ADV", "ADJD", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Ihr ahnungsvoller Laut l\u00e4\u00dft sich noch immer h\u00f6ren,", "tokens": ["Ihr", "ah\u00b7nungs\u00b7vol\u00b7ler", "Laut", "l\u00e4\u00dft", "sich", "noch", "im\u00b7mer", "h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "VVFIN", "PRF", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dem Wandrer in der Brust die Wehmut aufzust\u00f6ren.", "tokens": ["Dem", "Wand\u00b7rer", "in", "der", "Brust", "die", "Weh\u00b7mut", "auf\u00b7zu\u00b7st\u00f6\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "ART", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.10": {"line.1": {"text": "S\u00fcdw\u00e4rts die V\u00f6gel ziehn mit eiligem Geschw\u00e4tze;", "tokens": ["S\u00fcd\u00b7w\u00e4rts", "die", "V\u00f6\u00b7gel", "ziehn", "mit", "ei\u00b7li\u00b7gem", "Ge\u00b7schw\u00e4t\u00b7ze", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Doch auch den S\u00fcden deckt der Tod mit seinem Netze.", "tokens": ["Doch", "auch", "den", "S\u00fc\u00b7den", "deckt", "der", "Tod", "mit", "sei\u00b7nem", "Net\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.11": {"line.1": {"text": "Natur das Ewge schaut in unruhvollen Tr\u00e4umen,", "tokens": ["Na\u00b7tur", "das", "Ew\u00b7ge", "schaut", "in", "un\u00b7ruh\u00b7vol\u00b7len", "Tr\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "F\u00e4hrt auf und will entfliehn den todverfallnen R\u00e4umen.", "tokens": ["F\u00e4hrt", "auf", "und", "will", "ent\u00b7fliehn", "den", "tod\u00b7ver\u00b7fall\u00b7nen", "R\u00e4u\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "KON", "VMFIN", "VVINF", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.12": {"line.1": {"text": "Der abgeri\u00dfne Ruf, womit Zugv\u00f6gel schweben,", "tokens": ["Der", "ab\u00b7ge\u00b7ri\u00df\u00b7ne", "Ruf", ",", "wo\u00b7mit", "Zug\u00b7v\u00f6\u00b7gel", "schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PWAV", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ist Aufschrei wirren Traums von einem ewgen Leben.", "tokens": ["Ist", "Auf\u00b7schrei", "wir\u00b7ren", "Traums", "von", "ei\u00b7nem", "ew\u00b7gen", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.13": {"line.1": {"text": "Ich h\u00f6re sie nicht mehr, schon sind sie weit von hinnen;", "tokens": ["Ich", "h\u00f6\u00b7re", "sie", "nicht", "mehr", ",", "schon", "sind", "sie", "weit", "von", "hin\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "ADV", "$,", "ADV", "VAFIN", "PPER", "ADJD", "APPR", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Zweifel in der Brust den Nachtgesang beginnen:", "tokens": ["Die", "Zwei\u00b7fel", "in", "der", "Brust", "den", "Nacht\u00b7ge\u00b7sang", "be\u00b7gin\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.14": {"line.1": {"text": "Ists Erdenleben Schein? \u2013 ist es die umgekehrte", "tokens": ["Ists", "Er\u00b7den\u00b7le\u00b7ben", "Schein", "?", "\u2013", "ist", "es", "die", "um\u00b7ge\u00b7kehr\u00b7te"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["NE", "NN", "NN", "$.", "$(", "VAFIN", "PPER", "ART", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Fata Morgana nur, des Ewgen Spiegelf\u00e4hrte?", "tokens": ["Fa\u00b7ta", "Mor\u00b7ga\u00b7na", "nur", ",", "des", "Ew\u00b7gen", "Spie\u00b7gel\u00b7f\u00e4hr\u00b7te", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ADV", "$,", "ART", "ADJA", "NN", "$."], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}}, "stanza.15": {"line.1": {"text": "Warum denn aber wird dem Erdenleben bange,", "tokens": ["Wa\u00b7rum", "denn", "a\u00b7ber", "wird", "dem", "Er\u00b7den\u00b7le\u00b7ben", "ban\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADV", "VAFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn es ein Schein nur ist, vor seinem Untergange?", "tokens": ["Wenn", "es", "ein", "Schein", "nur", "ist", ",", "vor", "sei\u00b7nem", "Un\u00b7ter\u00b7gan\u00b7ge", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "VAFIN", "$,", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.16": {"line.1": {"text": "Ist solche B\u00e4ngnis nur von dem, was wird bestehen,", "tokens": ["Ist", "sol\u00b7che", "B\u00e4ng\u00b7nis", "nur", "von", "dem", ",", "was", "wird", "be\u00b7ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "ADV", "APPR", "ART", "$,", "PWS", "VAFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein Widerglanz, da\u00df auch sein Bild nicht will vergehen?", "tokens": ["Ein", "Wi\u00b7der\u00b7glanz", ",", "da\u00df", "auch", "sein", "Bild", "nicht", "will", "ver\u00b7ge\u00b7hen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "ADV", "PPOSAT", "NN", "PTKNEG", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.17": {"line.1": {"text": "Dies Bangen auch nur Schein? \u2013 so schw\u00e4rmen die Gedanken,", "tokens": ["Dies", "Ban\u00b7gen", "auch", "nur", "Schein", "?", "\u2013", "so", "schw\u00e4r\u00b7men", "die", "Ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "ADV", "ADV", "NN", "$.", "$(", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie dort durchs \u00f6de Tal die Herbstesnebel schwanken.", "tokens": ["Wie", "dort", "durchs", "\u00f6\u00b7de", "Tal", "die", "Herbs\u00b7tes\u00b7ne\u00b7bel", "schwan\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "APPRART", "ADJA", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.18": {"line.1": {"text": "Es weht der Wind so k\u00fchl, entlaubend rings die \u00c4ste,", "tokens": ["Es", "weht", "der", "Wind", "so", "k\u00fchl", ",", "ent\u00b7lau\u00b7bend", "rings", "die", "\u0