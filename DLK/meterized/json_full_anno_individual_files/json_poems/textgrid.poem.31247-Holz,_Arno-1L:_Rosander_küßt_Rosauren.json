{"textgrid.poem.31247": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "1L: Rosander k\u00fc\u00dft Rosauren/", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Rosander k\u00fc\u00dft Rosauren/", "tokens": ["Ro\u00b7san\u00b7der", "k\u00fc\u00dft", "Ro\u00b7sau\u00b7ren", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "mich kan man st\u00e4ts nach Zehn", "tokens": ["mich", "kan", "man", "st\u00e4ts", "nach", "Zehn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PIS", "VVFIN", "APPR", "CARD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "bey Basimenen sehn.", "tokens": ["bey", "Ba\u00b7si\u00b7me\u00b7nen", "sehn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "$."], "meter": "---+-+", "measure": "unknown.measure.di"}}, "stanza.2": {"line.1": {"text": "Die Br\u00fcstgens sind ihr beyde", "tokens": ["Die", "Br\u00fcst\u00b7gens", "sind", "ihr", "bey\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PPER", "PIS"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "au\u00df ohnbeschmuzzter Kreyde/", "tokens": ["au\u00df", "ohn\u00b7be\u00b7schmuzz\u00b7ter", "Krey\u00b7de", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "ihr Glantz for meinen Sinn", "tokens": ["ihr", "Glantz", "for", "mei\u00b7nen", "Sinn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "sticht alle Sterne hin.", "tokens": ["sticht", "al\u00b7le", "Ster\u00b7ne", "hin", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "PTKVZ", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.3": {"line.1": {"text": "Ihr P\u00e4rlemutter-R\u00fckken", "tokens": ["Ihr", "P\u00e4r\u00b7le\u00b7mut\u00b7ter\u00b7R\u00fc\u00b7kken"], "token_info": ["word", "word"], "pos": ["PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "erf\u00fcllt mich mit Entz\u00fckken/", "tokens": ["er\u00b7f\u00fcllt", "mich", "mit", "Ent\u00b7z\u00fck\u00b7ken", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "auch nimbt mich for sie ein", "tokens": ["auch", "nimbt", "mich", "for", "sie", "ein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "VVFIN", "PPER", "ART"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "ihr wohl formirtes Bein.", "tokens": ["ihr", "wohl", "for\u00b7mir\u00b7tes", "Bein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Nur z\u00e4uberische Worte", "tokens": ["Nur", "z\u00e4u\u00b7be\u00b7ri\u00b7sche", "Wor\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "haucht die Rubinen-Pforte/", "tokens": ["haucht", "die", "Ru\u00b7bi\u00b7nen\u00b7P\u00b7for\u00b7te", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$("], "meter": "+-+-+--", "measure": "unknown.measure.tri"}, "line.3": {"text": "darmit geht sie mir zahrt", "tokens": ["dar\u00b7mit", "geht", "sie", "mir", "zahrt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "PPER", "VVFIN"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.4": {"text": "\u00fcmb meinen Zihgen-Bahrt.", "tokens": ["\u00fcmb", "mei\u00b7nen", "Zih\u00b7gen\u00b7Bahrt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Ein halbes Viertel-St\u00fcndgen", "tokens": ["Ein", "hal\u00b7bes", "Vier\u00b7tel\u00b7St\u00fcnd\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "lustirt mich so ihr M\u00fcndgen/", "tokens": ["lus\u00b7tirt", "mich", "so", "ihr", "M\u00fcnd\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PPOSAT", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "sordan reisst es mich fort", "tokens": ["sor\u00b7dan", "reisst", "es", "mich", "fort"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "PTKVZ"], "meter": "-+---+", "measure": "dactylic.init"}, "line.4": {"text": "an einen andren Ort.", "tokens": ["an", "ei\u00b7nen", "an\u00b7dren", "Ort", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Am Bandelir die Zitter", "tokens": ["Am", "Ban\u00b7de\u00b7lir", "die", "Zit\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "zih ich f\u00fcr manches Gitter/", "tokens": ["zih", "ich", "f\u00fcr", "man\u00b7ches", "Git\u00b7ter", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PIAT", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "denn einer gantz allein", "tokens": ["denn", "ei\u00b7ner", "gantz", "al\u00b7lein"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADV", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "kan ich nicht freundlig seyn.", "tokens": ["kan", "ich", "nicht", "freund\u00b7lig", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "ADJD", "VAINF", "$."], "meter": "---+-+", "measure": "unknown.measure.di"}}, "stanza.7": {"line.1": {"text": "Die G\u00e4ssgens auff und nihder", "tokens": ["Die", "G\u00e4ss\u00b7gens", "auff", "und", "nih\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "erschallen meine Lider/", "tokens": ["er\u00b7schal\u00b7len", "mei\u00b7ne", "Li\u00b7der", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "singt doch s\u00e4lbst meiner Treu", "tokens": ["singt", "doch", "s\u00e4lbst", "mei\u00b7ner", "Treu"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ADV", "PPOSAT", "NN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "der gr\u00fcne Bapegoy.", "tokens": ["der", "gr\u00fc\u00b7ne", "Ba\u00b7pe\u00b7goy", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Wenn eine recht gef\u00fcglig/", "tokens": ["Wenn", "ei\u00b7ne", "recht", "ge\u00b7f\u00fcg\u00b7lig", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADV", "ADJD", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "ist mir das schon gen\u00fcglig/", "tokens": ["ist", "mir", "das", "schon", "ge\u00b7n\u00fcg\u00b7lig", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PDS", "ADV", "ADJD", "$("], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "doch wenn sie nach mir brennt/", "tokens": ["doch", "wenn", "sie", "nach", "mir", "brennt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "APPR", "PPER", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "so f\u00fchl ich mich content.", "tokens": ["so", "f\u00fchl", "ich", "mich", "con\u00b7tent", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Mit silbrenen Galaunen", "tokens": ["Mit", "sil\u00b7bre\u00b7nen", "Ga\u00b7lau\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "s\u00e4zz ich sie in Erstaunen;", "tokens": ["s\u00e4zz", "ich", "sie", "in", "Er\u00b7stau\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "das W\u00e4mbstgen/ das mich zihrt/", "tokens": ["das", "W\u00e4mbst\u00b7gen", "/", "das", "mich", "zihrt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PRELS", "PPER", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "ist dikk mit Gold brodirt.", "tokens": ["ist", "dikk", "mit", "Gold", "bro\u00b7dirt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NE", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Kombt wer mir in die Qwere/", "tokens": ["Kombt", "wer", "mir", "in", "die", "Qwe\u00b7re", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PWS", "PPER", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "dan hat ihn gleich auff Ehre", "tokens": ["dan", "hat", "ihn", "gleich", "auff", "Eh\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "der alte G\u00f6zze ", "tokens": ["der", "al\u00b7te", "G\u00f6z\u00b7ze"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "der Hellen-General!", "tokens": ["der", "Hel\u00b7len\u00b7Ge\u00b7ne\u00b7ral", "!"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Durch Fehder/ Filtz und Krempe", "tokens": ["Durch", "Feh\u00b7der", "/", "Filtz", "und", "Krem\u00b7pe"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "$(", "NE", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "sto\u00df ich ihm meine Plempe/", "tokens": ["sto\u00df", "ich", "ihm", "mei\u00b7ne", "Plem\u00b7pe", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "PPOSAT", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "von jeder L\u00f6ffeley", "tokens": ["von", "je\u00b7der", "L\u00f6f\u00b7fe\u00b7ley"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PIAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "mach ich ihn durchau\u00df frey!", "tokens": ["mach", "ich", "ihn", "durch\u00b7au\u00df", "frey", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Die bleich verstellte Frazze", "tokens": ["Die", "bleich", "ver\u00b7stell\u00b7te", "Fraz\u00b7ze"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "beschnoppt ihm Hund und Kazze/", "tokens": ["be\u00b7schnoppt", "ihm", "Hund", "und", "Kaz\u00b7ze", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "KON", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "ich dreh mich nicht mal um/", "tokens": ["ich", "dreh", "mich", "nicht", "mal", "um", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "ADV", "APPR", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Hertz-Bruder/ du bleibst stumm!", "tokens": ["Hertz\u00b7Bru\u00b7der", "/", "du", "bleibst", "stumm", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$(", "PPER", "VVFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Mit Trekk statt mit Bomade", "tokens": ["Mit", "Trekk", "statt", "mit", "Bo\u00b7ma\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "APPR", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "bespr\u00fczzt bi\u00df an die Wade/", "tokens": ["be\u00b7spr\u00fczzt", "bi\u00df", "an", "die", "Wa\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "bey ", "tokens": ["bey"], "token_info": ["word"], "pos": ["APPR"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "darzu b\u00fcn ich der Mann!", "tokens": ["dar\u00b7zu", "b\u00fcn", "ich", "der", "Mann", "!"], "token_info": ["word", "word", "word", "word", "word", "pu