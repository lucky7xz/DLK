{"textgrid.poem.43025": {"metadata": {"author": {"name": "Aston, Louise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Warum mein Herz nicht freudig schl\u00e4gt", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Warum mein Herz nicht freudig schl\u00e4gt", "tokens": ["Wa\u00b7rum", "mein", "Herz", "nicht", "freu\u00b7dig", "schl\u00e4gt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPOSAT", "NN", "PTKNEG", "ADJD", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zu all' dem Jubel, diesen Festen?", "tokens": ["Zu", "all'", "dem", "Ju\u00b7bel", ",", "die\u00b7sen", "Fes\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PIS", "ART", "NN", "$,", "PDAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Mir ist's wie Ahnung stumm bewegt,", "tokens": ["Mir", "ist's", "wie", "Ah\u00b7nung", "stumm", "be\u00b7wegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "KOKOM", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich traure mit des Volkes Besten.", "tokens": ["Ich", "trau\u00b7re", "mit", "des", "Vol\u00b7kes", "Bes\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Denn wer um Freiheit muthig rang,", "tokens": ["Denn", "wer", "um", "Frei\u00b7heit", "mut\u00b7hig", "rang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "APPR", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Noch kann er sich zum Fest nicht laden;", "tokens": ["Noch", "kann", "er", "sich", "zum", "Fest", "nicht", "la\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "APPRART", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ein K\u00e4mpfer steht er, ernst und bang", "tokens": ["Ein", "K\u00e4mp\u00b7fer", "steht", "er", ",", "ernst", "und", "bang"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "An den Gedanken-Barrikaden.", "tokens": ["An", "den", "Ge\u00b7dan\u00b7ken\u00b7Bar\u00b7ri\u00b7ka\u00b7den", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und tr\u00e4gt ihn noch, den schwarzen Flor,", "tokens": ["Und", "tr\u00e4gt", "ihn", "noch", ",", "den", "schwar\u00b7zen", "Flor", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den er der alten Schmach getragen,", "tokens": ["Den", "er", "der", "al\u00b7ten", "Schmach", "ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und sieht in einem Meteor", "tokens": ["Und", "sieht", "in", "ei\u00b7nem", "Me\u00b7te\u00b7or"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Noch keine Sonne wieder tagen.", "tokens": ["Noch", "kei\u00b7ne", "Son\u00b7ne", "wie\u00b7der", "ta\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Wer in das Blut, das f\u00fcr ihn rann,", "tokens": ["Wer", "in", "das", "Blut", ",", "das", "f\u00fcr", "ihn", "rann", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "$,", "PRELS", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sein Tuch, das thr\u00e4nenfeuchte, tauchte,", "tokens": ["Sein", "Tuch", ",", "das", "thr\u00e4\u00b7nen\u00b7feuch\u00b7te", ",", "tauch\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PDS", "VVFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Auf diese rothe Fahne dann", "tokens": ["Auf", "die\u00b7se", "ro\u00b7the", "Fah\u00b7ne", "dann"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "ADJA", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Freiheit hei\u00dfe Schw\u00fcre hauchte:", "tokens": ["Der", "Frei\u00b7heit", "hei\u00b7\u00dfe", "Schw\u00fc\u00b7re", "hauch\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Der harret aus. Noch ist es nicht", "tokens": ["Der", "har\u00b7ret", "aus", ".", "Noch", "ist", "es", "nicht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PTKVZ", "$.", "ADV", "VAFIN", "PPER", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gel\u00f6st, das alte Mi\u00dfverst\u00e4ndni\u00df,", "tokens": ["Ge\u00b7l\u00f6st", ",", "das", "al\u00b7te", "Mi\u00df\u00b7ver\u00b7st\u00e4nd\u00b7ni\u00df", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Das Jahrelang dem neuen Licht", "tokens": ["Das", "Jah\u00b7re\u00b7lang", "dem", "neu\u00b7en", "Licht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Verschlossen blieb \u2013 der Welterkenntni\u00df.", "tokens": ["Ver\u00b7schlos\u00b7sen", "blieb", "\u2013", "der", "Wel\u00b7ter\u00b7kennt\u00b7ni\u00df", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$(", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Der reicht mit kindischem Vertrau'n", "tokens": ["Der", "reicht", "mit", "kin\u00b7di\u00b7schem", "Ver\u00b7trau'n"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Siegerhand nicht hin vers\u00f6hnend,", "tokens": ["Die", "Sie\u00b7ger\u00b7hand", "nicht", "hin", "ver\u00b7s\u00f6h\u00b7nend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "So lange noch herniederschau'n", "tokens": ["So", "lan\u00b7ge", "noch", "her\u00b7nie\u00b7der\u00b7schau'n"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADV", "ADV", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die alten G\u00f6tzenbilder h\u00f6hnend.", "tokens": ["Die", "al\u00b7ten", "G\u00f6t\u00b7zen\u00b7bil\u00b7der", "h\u00f6h\u00b7nend", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "So lange noch ein ", "tokens": ["So", "lan\u00b7ge", "noch", "ein"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADV", "ADV", "ART"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Geschmiedet wird den Nationen,", "tokens": ["Ge\u00b7schmie\u00b7det", "wird", "den", "Na\u00b7ti\u00b7o\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der V\u00f6lker heiligstes Problem:", "tokens": ["Der", "V\u00f6l\u00b7ker", "hei\u00b7ligs\u00b7tes", "Prob\u00b7lem", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Der Herrschsucht M\u00fchsal zu belohnen.", "tokens": ["Der", "Herrschsucht", "M\u00fch\u00b7sal", "zu", "be\u00b7loh\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "So lange ", "tokens": ["So", "lan\u00b7ge"], "token_info": ["word", "word"], "pos": ["ADV", "ADV"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "In dem politischen Capitel,", "tokens": ["In", "dem", "po\u00b7li\u00b7ti\u00b7schen", "Ca\u00b7pi\u00b7tel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "So lange nicht die Hand verdorrt,", "tokens": ["So", "lan\u00b7ge", "nicht", "die", "Hand", "ver\u00b7dorrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PTKNEG", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die frech auslangt nach Kron' und Titel.", "tokens": ["Die", "frech", "aus\u00b7langt", "nach", "Kron'", "und", "Ti\u00b7tel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "APPR", "NE", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Kein deutsches Reich, nicht Schwarz, Roth, Gold!", "tokens": ["Kein", "deut\u00b7sches", "Reich", ",", "nicht", "Schwarz", ",", "Roth", ",", "Gold", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$,", "PTKNEG", "NE", "$,", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "O werft das Spielzeug aus den H\u00e4nden.", "tokens": ["O", "werft", "das", "Spiel\u00b7zeug", "aus", "den", "H\u00e4n\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Blickt in die Zukunft! drohend grollt", "tokens": ["Blickt", "in", "die", "Zu\u00b7kunft", "!", "dro\u00b7hend", "grollt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "$.", "VVPP", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Himmel und wird Blitze senden.", "tokens": ["Der", "Him\u00b7mel", "und", "wird", "Blit\u00b7ze", "sen\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "VAFIN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Ein neues Reich, loh angefacht", "tokens": ["Ein", "neu\u00b7es", "Reich", ",", "loh", "an\u00b7ge\u00b7facht"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ADJD", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von segenbringenden Gewittern", "tokens": ["Von", "se\u00b7gen\u00b7brin\u00b7gen\u00b7den", "Ge\u00b7wit\u00b7tern"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "---+-+-+-", "measure": "unknown.measure.tri"}, "line.3": {"text": "Wird, eh' der neue Tag erwacht,", "tokens": ["Wird", ",", "eh'", "der", "neu\u00b7e", "Tag", "er\u00b7wacht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "KOUS", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die alte deutsche Nacht durchzittern.", "tokens": ["Die", "al\u00b7te", "deut\u00b7sche", "Nacht", "durch\u00b7zit\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Schon f\u00fchl' ich sein begeisternd Weh'n", "tokens": ["Schon", "f\u00fchl'", "ich", "sein", "be\u00b7geis\u00b7ternd", "Weh'n"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "VVPP", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie eines Gottes gro\u00dfe Mahnung,", "tokens": ["Wie", "ei\u00b7nes", "Got\u00b7tes", "gro\u00b7\u00dfe", "Mah\u00b7nung", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Den Sturm gewaltiger Ideen", "tokens": ["Den", "Sturm", "ge\u00b7wal\u00b7ti\u00b7ger", "I\u00b7deen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter"