{"textgrid.poem.33313": {"metadata": {"author": {"name": "Blumauer, Aloys", "birth": "N.A.", "death": "N.A."}, "title": "Unterhaltungskalender eines jungen Wiener Herrchens", "genre": "verse", "period": "N.A.", "pub_year": 1776, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Des Sonntags weid' ich mich an unsern sch\u00f6nen Hetzen,", "tokens": ["Des", "Sonn\u00b7tags", "weid'", "ich", "mich", "an", "un\u00b7sern", "sch\u00f6\u00b7nen", "Het\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PRF", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Am Montag mu\u00df mich Kasperle erg\u00f6tzen.", "tokens": ["Am", "Mon\u00b7tag", "mu\u00df", "mich", "Kas\u00b7per\u00b7le", "er\u00b7g\u00f6t\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VMFIN", "PPER", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Am Dienstag l\u00e4dt mich's deutsche Schauspiel ein,", "tokens": ["Am", "Diens\u00b7tag", "l\u00e4dt", "mich's", "deut\u00b7sche", "Schau\u00b7spiel", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "NE", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Am Mittwoch trage ich ihm nur mein Ohr hinein.", "tokens": ["Am", "Mitt\u00b7woch", "tra\u00b7ge", "ich", "ihm", "nur", "mein", "Ohr", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "PPER", "ADV", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Zeigt sich am Donnerstag nicht Stubers", "tokens": ["Zeigt", "sich", "am", "Don\u00b7ners\u00b7tag", "nicht", "Stu\u00b7bers"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "APPRART", "NN", "PTKNEG", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "So gibt es wenigstens doch eine Feuersbrunst.", "tokens": ["So", "gibt", "es", "we\u00b7nigs\u00b7tens", "doch", "ei\u00b7ne", "Feu\u00b7ers\u00b7brunst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Am Freitag kann ich fr\u00fch die Ochsentheilung sehen:", "tokens": ["Am", "Frei\u00b7tag", "kann", "ich", "fr\u00fch", "die", "Och\u00b7sen\u00b7thei\u00b7lung", "se\u00b7hen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VMFIN", "PPER", "ADJD", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Die wechsl' ich Abends dann mit Assembleen.", "tokens": ["Die", "wechsl'", "ich", "A\u00b7bends", "dann", "mit", "As\u00b7sem\u00b7bleen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Nur Samstags, ach! ist meine Lust ersch\u00f6pft,", "tokens": ["Nur", "Sams\u00b7tags", ",", "ach", "!", "ist", "mei\u00b7ne", "Lust", "er\u00b7sch\u00f6pft", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "$,", "ITJ", "$.", "VAFIN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Seitdem man nicht mehr r\u00e4dert, h\u00e4ngt und k\u00f6pft.", "tokens": ["Seit\u00b