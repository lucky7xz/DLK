{"dta.poem.10051": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Vermahnung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20086-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Auch ein thierisch Ohr kan h\u00f6ren,", "tokens": ["Auch", "ein", "thie\u00b7risch", "Ohr", "kan", "h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJD", "NN", "VMFIN", "VVINF", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Auch ein Ochsen-Auge sehn:", "tokens": ["Auch", "ein", "Och\u00b7sen\u00b7Au\u00b7ge", "sehn", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber es kann, GOTT zu Ehren,", "tokens": ["A\u00b7ber", "es", "kann", ",", "GoTT", "zu", "Eh\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "$,", "NE", "APPR", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Da\u00df es Sein Werck, nicht verstehn.", "tokens": ["Da\u00df", "es", "Sein", "Werck", ",", "nicht", "ver\u00b7stehn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "$,", "PTKNEG", "VVINF", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.5": {"text": "Es kann im Genu\u00df nicht dencken:", "tokens": ["Es", "kann", "im", "Ge\u00b7nu\u00df", "nicht", "den\u00b7cken", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPRART", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Solch ein Vorrecht hast du hie,", "tokens": ["Solch", "ein", "Vor\u00b7recht", "hast", "du", "hie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ART", "NN", "VAFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Liebster Mensch, vor allem Vieh.", "tokens": ["Liebs\u00b7ter", "Mensch", ",", "vor", "al\u00b7lem", "Vieh", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "APPR", "PIS", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ach! so thu nach deiner Pflicht.", "tokens": ["Ach", "!", "so", "thu", "nach", "dei\u00b7ner", "Pflicht", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "ADV", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sieh vern\u00fcnftig! riech und h\u00f6re!", "tokens": ["Sieh", "ver\u00b7n\u00fcnf\u00b7tig", "!", "riech", "und", "h\u00f6\u00b7re", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "$.", "ADJD", "KON", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Freue dich! stiehl ferner nicht", "tokens": ["Freu\u00b7e", "dich", "!", "stiehl", "fer\u00b7ner", "nicht"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$.", "ADV", "ADV", "PTKNEG"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Dir die Lust, und ", "tokens": ["Dir", "die", "Lust", ",", "und"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["PPER", "ART", "NN", "$,", "KON"], "meter": "+-+-", "measure": "trochaic.di"}}}}}