{"dta.poem.2957": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Einst wird die Poesie zur Kinderkrankheit werden,", "tokens": ["Einst", "wird", "die", "Poe\u00b7sie", "zur", "Kin\u00b7der\u00b7krank\u00b7heit", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "APPRART", "NN", "VAINF", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und nur Filosofie erwachsen sich geberden.", "tokens": ["Und", "nur", "Fi\u00b7lo\u00b7so\u00b7fie", "er\u00b7wach\u00b7sen", "sich", "ge\u00b7ber\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "NN", "VVFIN", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Dann wird der Knab' abthun sein Lust- und Trauerspiel,", "tokens": ["Dann", "wird", "der", "Knab'", "ab\u00b7thun", "sein", "Lust", "und", "Trau\u00b7er\u00b7spiel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVINF", "PPOSAT", "TRUNC", "KON", "NN", "$,"], "meter": "-+-+---+-+-+", "measure": "unknown.measure.penta"}, "line.2": {"text": "Mit Mannesernst dann gehn lusttrauerlos zum Ziel.", "tokens": ["Mit", "Man\u00b7ne\u00b7sernst", "dann", "gehn", "lust\u00b7trau\u00b7er\u00b7los", "zum", "Ziel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "VVFIN", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Dann wird die Menschheit sich zur h\u00f6chsten W\u00fcrd' erheben,", "tokens": ["Dann", "wird", "die", "Menschheit", "sich", "zur", "h\u00f6chs\u00b7ten", "W\u00fcrd'", "er\u00b7he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "PRF", "APPRART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Du aber freue dich die Zeit nicht zu erleben.", "tokens": ["Du", "a\u00b7ber", "freu\u00b7e", "dich", "die", "Zeit", "nicht", "zu", "er\u00b7le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PRF", "ART", "NN", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}