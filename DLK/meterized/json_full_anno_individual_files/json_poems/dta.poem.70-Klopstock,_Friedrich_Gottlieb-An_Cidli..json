{"dta.poem.70": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "An Cidli.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1771", "urn": "urn:nbn:de:kobv:b4-200905199486", "language": ["de:0.99"], "booktitle": "[Klopstock, Friedrich Gottlieb]: Oden. Hamburg, 1771."}, "poem": {"stanza.1": {"line.1": {"text": "Unerforschter, als sonst etwas den Forscher t\u00e4uscht,", "tokens": ["Un\u00b7er\u00b7forschter", ",", "als", "sonst", "et\u00b7was", "den", "For\u00b7scher", "t\u00e4uscht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "ADV", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Ist ein Herz, das die Lieb\u2019 empfand,", "tokens": ["Ist", "ein", "Herz", ",", "das", "die", "Lieb'", "emp\u00b7fand", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Sie, die wirklicher Werth, nicht der verg\u00e4ngliche", "tokens": ["Sie", ",", "die", "wirk\u00b7li\u00b7cher", "Werth", ",", "nicht", "der", "ver\u00b7g\u00e4ng\u00b7li\u00b7che"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "$,", "ART", "ADJA", "NN", "$,", "PTKNEG", "ART", "ADJA"], "meter": "+-+--++--+--", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Unsers dichtenden Traums gebahr,", "tokens": ["Un\u00b7sers", "dich\u00b7ten\u00b7den", "Traums", "ge\u00b7bahr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.5": {"text": "Jene trunkene Lust, wenn die erweinete,", "tokens": ["Je\u00b7ne", "trun\u00b7ke\u00b7ne", "Lust", ",", "wenn", "die", "er\u00b7wei\u00b7ne\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "$,", "KOUS", "PDS", "VVFIN", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.6": {"text": "Fast zu selige Stunde kommt,", "tokens": ["Fast", "zu", "se\u00b7li\u00b7ge", "Stun\u00b7de", "kommt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.7": {"text": "Die dem Liebenden sagt, da\u00df er geliebet wird!", "tokens": ["Die", "dem", "Lie\u00b7ben\u00b7den", "sagt", ",", "da\u00df", "er", "ge\u00b7lie\u00b7bet", "wird", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVFIN", "$,", "KOUS", "PPER", "VVPP", "VAFIN", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.8": {"text": "Und zwo bessere Seelen nun", "tokens": ["Und", "zwo", "bes\u00b7se\u00b7re", "See\u00b7len", "nun"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "CARD", "ADJA", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Ganz, das erstemal ganz, f\u00fchlen, wie sehr sie sind!", "tokens": ["Ganz", ",", "das", "ers\u00b7te\u00b7mal", "ganz", ",", "f\u00fch\u00b7len", ",", "wie", "sehr", "sie", "sind", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PRELS", "ADV", "ADV", "$,", "VVFIN", "$,", "PWAV", "ADV", "PPER", "VAFIN", "$."], "meter": "+-+---+--+-+", "measure": "trochaic.penta.relaxed"}, "line.10": {"text": "Und wie gl\u00fccklich! wie \u00e4hnlich sich!", "tokens": ["Und", "wie", "gl\u00fcck\u00b7lich", "!", "wie", "\u00e4hn\u00b7lich", "sich", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ADJD", "$.", "PWAV", "ADJD", "PRF", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.11": {"text": "Ach, wie gl\u00fccklich dadurch! Wer der Geliebten spricht", "tokens": ["Ach", ",", "wie", "gl\u00fcck\u00b7lich", "da\u00b7durch", "!", "Wer", "der", "Ge\u00b7lieb\u00b7ten", "spricht"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "PWAV", "ADJD", "PAV", "$.", "PWS", "ART", "NN", "VVFIN"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.12": {"text": "Diese Liebe mit Worten aus?", "tokens": ["Die\u00b7se", "Lie\u00b7be", "mit", "Wor\u00b7ten", "aus", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "APPR", "NN", "PTKVZ", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.13": {"text": "Wer mit Thr\u00e4nen? und wer mit dem verweilenden,", "tokens": ["Wer", "mit", "Thr\u00e4\u00b7nen", "?", "und", "wer", "mit", "dem", "ver\u00b7wei\u00b7len\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "NN", "$.", "KON", "PWS", "APPR", "ART", "ADJA", "$,"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.14": {"text": "Vollem Blick, und der Seele drinn?", "tokens": ["Vol\u00b7lem", "Blick", ",", "und", "der", "See\u00b7le", "drinn", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "KON", "ART", "NN", "PTKVZ", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.15": {"text": "Selbst das Trauren ist s\u00fc\u00df, das sie verk\u00fcndete,", "tokens": ["Selbst", "das", "Trau\u00b7ren", "ist", "s\u00fc\u00df", ",", "das", "sie", "ver\u00b7k\u00fcn\u00b7de\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VAFIN", "ADJD", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.16": {"text": "Eh die selige Stunde kam!", "tokens": ["Eh", "die", "se\u00b7li\u00b7ge", "Stun\u00b7de", "kam", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.17": {"text": "Wenn die\u00df Trauren umsonst Eine verk\u00fcndete;", "tokens": ["Wenn", "die\u00df", "Trau\u00b7ren", "um\u00b7sonst", "Ei\u00b7ne", "ver\u00b7k\u00fcn\u00b7de\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "NN", "ADV", "ART", "ADJA", "$."], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.18": {"text": "O dann w\u00e4hlte die Seele falsch,", "tokens": ["O", "dann", "w\u00e4hl\u00b7te", "die", "See\u00b7le", "falsch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "VVFIN", "ART", "NN", "ADJD", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.19": {"text": "Und doch w\u00fcrdig! Das webt keiner der Denker auf,", "tokens": ["Und", "doch", "w\u00fcr\u00b7dig", "!", "Das", "webt", "kei\u00b7ner", "der", "Den\u00b7ker", "auf", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "$.", "PDS", "VVFIN", "PIS", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.20": {"text": "Was vor Irren sie damals ging!", "tokens": ["Was", "vor", "Ir\u00b7ren", "sie", "da\u00b7mals", "ging", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "NN", "PPER", "ADV", "VVFIN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.21": {"text": "Selbst der kennt sie nicht ganz, welcher sie wandelte,", "tokens": ["Selbst", "der", "kennt", "sie", "nicht", "ganz", ",", "wel\u00b7cher", "sie", "wan\u00b7del\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "VVFIN", "PPER", "PTKNEG", "ADV", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.22": {"text": "Und verfehlt sie nur weniger.", "tokens": ["Und", "ver\u00b7fehlt", "sie", "nur", "we\u00b7ni\u00b7ger", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.23": {"text": "Leise redets darin: Weil du es w\u00fcrdig warst,", "tokens": ["Lei\u00b7se", "re\u00b7dets", "da\u00b7rin", ":", "Weil", "du", "es", "w\u00fcr\u00b7dig", "warst", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PAV", "$.", "KOUS", "PPER", "PPER", "ADJD", "VAFIN", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.24": {"text": "Da\u00df du liebtest, so lehrten wir", "tokens": ["Da\u00df", "du", "lieb\u00b7test", ",", "so", "lehr\u00b7ten", "wir"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "ADV", "VVFIN", "PPER"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.25": {"text": "Dich die Liebe. Du kennst alle Verwandlungen", "tokens": ["Dich", "die", "Lie\u00b7be", ".", "Du", "kennst", "al\u00b7le", "Ver\u00b7wand\u00b7lun\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "ART", "NN", "$.", "PPER", "VVFIN", "PIAT", "NN"], "meter": "+-+---+--+--", "measure": "trochaic.tetra.relaxed"}, "line.26": {"text": "Ihres m\u00e4chtigen Zauberstabs!", "tokens": ["Ih\u00b7res", "m\u00e4ch\u00b7ti\u00b7gen", "Zau\u00b7ber\u00b7stabs", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.27": {"text": "Ahm den Weisen nun nach: Handle! die Wissenschaft,", "tokens": ["Ahm", "den", "Wei\u00b7sen", "nun", "nach", ":", "Hand\u00b7le", "!", "die", "Wis\u00b7sen\u00b7schaft", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADV", "PTKVZ", "$.", "NE", "$.", "ART", "NN", "$,"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.28": {"text": "Sie nur, machte nie Gl\u00fcckliche!", "tokens": ["Sie", "nur", ",", "mach\u00b7te", "nie", "Gl\u00fcck\u00b7li\u00b7che", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "VVFIN", "ADV", "NN", "$."], "meter": "--+--+--", "measure": "anapaest.di.plus"}, "line.29": {"text": "Ich gehorche. Das Thal, Eden nur schattete,", "tokens": ["Ich", "ge\u00b7hor\u00b7che", ".", "Das", "Thal", ",", "E\u00b7den", "nur", "schat\u00b7te\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "ART", "NN", "$,", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.30": {"text": "Wie es schattet, der Lenz im Thal", "tokens": ["Wie", "es", "schat\u00b7tet", ",", "der", "Lenz", "im", "Thal"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "VVFIN", "$,", "ART", "NN", "APPRART", "NN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.31": {"text": "Wei!t dich! L\u00fcfte, wie die, welche die Himmlischen", "tokens": ["Wei", "!", "t", "dich", "!", "L\u00fcf\u00b7te", ",", "wie", "die", ",", "wel\u00b7che", "die", "Himm\u00b7li\u00b7schen"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "$.", "VVFIN", "PPER", "$.", "NN", "$,", "PWAV", "ART", "$,", "PRELS", "ART", "NN"], "meter": "+--+-+-+--+-+", "measure": "iambic.hexa.invert"}, "line.32": {"text": "Sanft umathmen, umathmen dich!", "tokens": ["Sanft", "um\u00b7ath\u00b7men", ",", "um\u00b7ath\u00b7men", "dich", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VVINF", "$,", "VVFIN", "PPER", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.33": {"text": "Rosen knospen dir auf, da\u00df sie mit s\u00fcssem Duft", "tokens": ["Ro\u00b7sen", "knos\u00b7pen", "dir", "auf", ",", "da\u00df", "sie", "mit", "s\u00fcs\u00b7sem", "Duft"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "PTKVZ", "$,", "KOUS", "PPER", "APPR", "ADJA", "NN"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.34": {"text": "Dich umstr\u00f6men! dort schlummerst du!", "tokens": ["Dich", "um\u00b7str\u00f6\u00b7men", "!", "dort", "schlum\u00b7merst", "du", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "$.", "ADV", "VVFIN", "PPER", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.35": {"text": "Wach, ich werfe sie dir leis\u2019 in die Locken hin,", "tokens": ["Wach", ",", "ich", "wer\u00b7fe", "sie", "dir", "leis'", "in", "die", "Lo\u00b7cken", "hin", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VVFIN", "PPER", "PPER", "ADJD", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+---+--+-+", "measure": "trochaic.penta.relaxed"}, "line.36": {"text": "Wach vom Thaue der Rosen auf.", "tokens": ["Wach", "vom", "Thau\u00b7e", "der", "Ro\u00b7sen", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.37": {"text": "Und \u2026 noch bebt mir mein Herz, lange daran verw\u00f6hnt,", "tokens": ["Und", "\u2026", "noch", "bebt", "mir", "mein", "Herz", ",", "lan\u00b7ge", "da\u00b7ran", "ver\u00b7w\u00f6hnt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$(", "ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$,", "ADV", "PAV", "VVPP", "$,"], "meter": "--+--++--+-+", "measure": "anapaest.di.plus"}, "line.38": {"text": "Und \u2026 o wache mir l\u00e4chelnd auf!", "tokens": ["Und", "\u2026", "o", "wa\u00b7che", "mir", "l\u00e4\u00b7chelnd", "auf", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$(", "FM", "ADJA", "PPER", "ADJD", "PTKVZ", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}}}}