{"textgrid.poem.41552": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Am dritten Sonntage nach Pfingsten", "genre": "verse", "period": "N.A.", "pub_year": 1822, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Doch zu dem Reichen", "tokens": ["Doch", "zu", "dem", "Rei\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Sprach Abraham: \u00bbUnd h\u00f6rten nie", "tokens": ["Sprach", "Ab\u00b7ra\u00b7ham", ":", "\u00bb", "Und", "h\u00f6r\u00b7ten", "nie"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word"], "pos": ["NN", "NE", "$.", "$(", "KON", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie Mosen noch Prophetenschar,", "tokens": ["Sie", "Mo\u00b7sen", "noch", "Pro\u00b7phe\u00b7ten\u00b7schar", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NE", "ADV", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dann wahrlich nimmer glauben sie,", "tokens": ["Dann", "wahr\u00b7lich", "nim\u00b7mer", "glau\u00b7ben", "sie", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Stellt sich ein Toter ihnen dar.\u00ab", "tokens": ["Stellt", "sich", "ein", "To\u00b7ter", "ih\u00b7nen", "dar", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "PPER", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So ward die Scheidewand gelegt,", "tokens": ["So", "ward", "die", "Schei\u00b7de\u00b7wand", "ge\u00b7legt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und auf den Grabstein hat gepr\u00e4gt", "tokens": ["Und", "auf", "den", "Grab\u00b7stein", "hat", "ge\u00b7pr\u00e4gt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VAFIN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Die Ewigkeit ihr stummes Zeichen.", "tokens": ["Die", "E\u00b7wig\u00b7keit", "ihr", "stum\u00b7mes", "Zei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wie br\u00fcnstig flehend", "tokens": ["Wie", "br\u00fcns\u00b7tig", "fle\u00b7hend"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "ADJD", "VVPP"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Hab' ich so oft in mancher Nacht", "tokens": ["Hab'", "ich", "so", "oft", "in", "man\u00b7cher", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "ADV", "ADV", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "An meine Toten mich gewandt!", "tokens": ["An", "mei\u00b7ne", "To\u00b7ten", "mich", "ge\u00b7wandt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie manchen Stundenschlag bewacht,", "tokens": ["Wie", "man\u00b7chen", "Stun\u00b7den\u00b7schlag", "be\u00b7wacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wenn grau und wirbelnd lag das Land!", "tokens": ["Wenn", "grau", "und", "wir\u00b7belnd", "lag", "das", "Land", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "KON", "ADJD", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und nicht ein Zeichen ward mir je,", "tokens": ["Und", "nicht", "ein", "Zei\u00b7chen", "ward", "mir", "je", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ART", "NN", "VAFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Kein Knistern in des Lagers N\u00e4h',", "tokens": ["Kein", "Knis\u00b7tern", "in", "des", "La\u00b7gers", "N\u00e4h'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Kein Schimmer l\u00e4ngs den W\u00e4nden gehend.", "tokens": ["Kein", "Schim\u00b7mer", "l\u00e4ngs", "den", "W\u00e4n\u00b7den", "ge\u00b7hend", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Hab' ich's gefunden", "tokens": ["Hab'", "ich's", "ge\u00b7fun\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "PIS", "VVPP"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Doch hart und lieblos manches Mal,", "tokens": ["Doch", "hart", "und", "lieb\u00b7los", "man\u00b7ches", "Mal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KON", "ADJD", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df das, dem ich so hei\u00df geneigt,", "tokens": ["Da\u00df", "das", ",", "dem", "ich", "so", "hei\u00df", "ge\u00b7neigt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "$,", "PRELS", "PPER", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nicht einen Laut f\u00fcr meine Qual,", "tokens": ["Nicht", "ei\u00b7nen", "Laut", "f\u00fcr", "mei\u00b7ne", "Qual", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "APPR", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Kein Zeichen hatte noch so leicht.", "tokens": ["Kein", "Zei\u00b7chen", "hat\u00b7te", "noch", "so", "leicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "An ihrer Statt, so d\u00fcnkte mich,", "tokens": ["An", "ih\u00b7rer", "Statt", ",", "so", "d\u00fcnk\u00b7te", "mich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "ADV", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "W\u00fcrd' alles, alles wagen ich,", "tokens": ["W\u00fcrd'", "al\u00b7les", ",", "al\u00b7les", "wa\u00b7gen", "ich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "$,", "PIS", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Zu lindern des Geliebten Wunden.", "tokens": ["Zu", "lin\u00b7dern", "des", "Ge\u00b7lieb\u00b7ten", "Wun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ihr konntet's nimmer!", "tokens": ["Ihr", "konn\u00b7tet's", "nim\u00b7mer", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Ausfechten sollen wir den Kampf", "tokens": ["Aus\u00b7fech\u00b7ten", "sol\u00b7len", "wir", "den", "Kampf"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VMFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und bleiben dem Geschick die Macht.", "tokens": ["Und", "blei\u00b7ben", "dem", "Ge\u00b7schick", "die", "Macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich f\u00fchl' es wohl, der Seele Krampf", "tokens": ["Ich", "f\u00fchl'", "es", "wohl", ",", "der", "See\u00b7le", "Krampf"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$,", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Zerrinnen m\u00fc\u00dfte mit der Nacht,", "tokens": ["Zer\u00b7rin\u00b7nen", "m\u00fc\u00df\u00b7te", "mit", "der", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ja mit dem letzten Nebeltraum", "tokens": ["Ja", "mit", "dem", "letz\u00b7ten", "Ne\u00b7bel\u00b7traum"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKANT", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Zerflie\u00dfen mu\u00df des B\u00f6sen Schaum,", "tokens": ["Zer\u00b7flie\u00b7\u00dfen", "mu\u00df", "des", "B\u00f6\u00b7sen", "Schaum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Drum bleibt die Wahrheit nur ein Schimmer.", "tokens": ["Drum", "bleibt", "die", "Wahr\u00b7heit", "nur", "ein", "Schim\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "NN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "O m\u00f6g' uns bleiben", "tokens": ["O", "m\u00f6g'", "uns", "blei\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "In diesem grau und tr\u00fcben Stand,", "tokens": ["In", "die\u00b7sem", "grau", "und", "tr\u00fc\u00b7ben", "Stand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJD", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wo Schatten lagern \u00fcberm Licht,", "tokens": ["Wo", "Schat\u00b7ten", "la\u00b7gern", "\u00fc\u00b7berm", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nur reiner Liebesfackel Brand;", "tokens": ["Nur", "rei\u00b7ner", "Lie\u00b7bes\u00b7fa\u00b7ckel", "Brand", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dann sind wir auch verlassen nicht!", "tokens": ["Dann", "sind", "wir", "auch", "ver\u00b7las\u00b7sen", "nicht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "VVFIN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und wie das Schiff in w\u00fcster See,", "tokens": ["Und", "wie", "das", "Schiff", "in", "w\u00fcs\u00b7ter", "See", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Vertrauend auf des Pharus N\u00e4h',", "tokens": ["Ver\u00b7trau\u00b7end", "auf", "des", "Pha\u00b7rus", "N\u00e4h'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Mag unser Kahn zum Hafen treiben.", "tokens": ["Mag", "un\u00b7ser", "Kahn", "zum", "Ha\u00b7fen", "trei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Dem reichen Manne", "tokens": ["Dem", "rei\u00b7chen", "Man\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Sprach nicht ein Wort von Zweifels Not", "tokens": ["Sprach", "nicht", "ein", "Wort", "von", "Zwei\u00b7fels", "Not"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "PTKNEG", "ART", "NN", "APPR", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die schreckliche Verdammnis aus;", "tokens": ["Die", "schreck\u00b7li\u00b7che", "Ver\u00b7damm\u00b7nis", "aus", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nein nur das ungebrochne Brod,", "tokens": ["Nein", "nur", "das", "un\u00b7ge\u00b7broch\u00b7ne", "Brod", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Als \u00e4chzend lag vor seinem Haus", "tokens": ["Als", "\u00e4ch\u00b7zend", "lag", "vor", "sei\u00b7nem", "Haus"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJD", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der Arm' und Sieche, dies allein", "tokens": ["Der", "Arm'", "und", "Sie\u00b7che", ",", "dies", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "KON", "NN", "$,", "PDS", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Hat lastend wie ein M\u00fchlenstein", "tokens": ["Hat", "las\u00b7tend", "wie", "ein", "M\u00fch\u00b7len\u00b7stein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADJD", "KOKOM", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ihn fortgew\u00e4lzt zu Pein und Banne.", "tokens": ["Ihn", "fort\u00b7ge\u00b7w\u00e4lzt", "zu", "Pein", "und", "Ban\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Hier steht die Stelle:", "tokens": ["Hier", "steht", "die", "Stel\u00b7le", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "\u00bbund als er in die Qualen kam,", "tokens": ["\u00bb", "und", "als", "er", "in", "die", "Qua\u00b7len", "kam", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da hob die Augen er empor,", "tokens": ["Da", "hob", "die", "Au\u00b7gen", "er", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sah in der Ferne Abraham,", "tokens": ["Sah", "in", "der", "Fer\u00b7ne", "Ab\u00b7ra\u00b7ham", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Umgeben von der Heil'gen Chor,", "tokens": ["Um\u00b7ge\u00b7ben", "von", "der", "Heil'\u00b7gen", "Chor", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und Lazarum in seinem Scho\u00df", "tokens": ["Und", "La\u00b7za\u00b7rum", "in", "sei\u00b7nem", "Scho\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der Schw\u00e4ren frei, der Plagen los.\u00ab", "tokens": ["Der", "Schw\u00e4\u00b7ren", "frei", ",", "der", "Pla\u00b7gen", "los", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,", "ART", "NN", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Er aber \u2013 er war in der H\u00f6lle!", "tokens": ["Er", "a\u00b7ber", "\u2013", "er", "war", "in", "der", "H\u00f6l\u00b7le", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$(", "PPER", "VAFIN", "APPR", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.8": {"line.1": {"text": "Doch zu dem Reichen", "tokens": ["Doch", "zu", "dem", "Rei\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Sprach Abraham: \u00bbUnd h\u00f6rten nie", "tokens": ["Sprach", "Ab\u00b7ra\