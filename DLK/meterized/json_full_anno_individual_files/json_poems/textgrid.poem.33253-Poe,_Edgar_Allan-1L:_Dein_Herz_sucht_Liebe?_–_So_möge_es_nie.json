{"textgrid.poem.33253": {"metadata": {"author": {"name": "Poe, Edgar Allan", "birth": "N.A.", "death": "N.A."}, "title": "1L: Dein Herz sucht Liebe? \u2013 So m\u00f6ge es nie", "genre": "verse", "period": "N.A.", "pub_year": 1829, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dein Herz sucht Liebe? \u2013 So m\u00f6ge es nie", "tokens": ["Dein", "Herz", "sucht", "Lie\u00b7be", "?", "\u2013", "So", "m\u00f6\u00b7ge", "es", "nie"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "NN", "$.", "$(", "ADV", "VMFIN", "PPER", "ADV"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Vom jetzigen Pfade weichen,", "tokens": ["Vom", "jet\u00b7zi\u00b7gen", "Pfa\u00b7de", "wei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Sei, was du bist, und wolle nie", "tokens": ["Sei", ",", "was", "du", "bist", ",", "und", "wol\u00b7le", "nie"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "$,", "PWS", "PPER", "VAFIN", "$,", "KON", "VMFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dem, was du nicht bist, gleichen \u2013", "tokens": ["Dem", ",", "was", "du", "nicht", "bist", ",", "glei\u00b7chen", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PDS", "$,", "PWS", "PPER", "PTKNEG", "VAFIN", "$,", "ADJA", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "So wird die Welt deinem sanften Sein,", "tokens": ["So", "wird", "die", "Welt", "dei\u00b7nem", "sanf\u00b7ten", "Sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Deiner Anmut ein unendlich", "tokens": ["Dei\u00b7ner", "An\u00b7mut", "ein", "un\u00b7end\u00b7lich"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ART", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und freudevolles Preislied weihn,", "tokens": ["Und", "freu\u00b7de\u00b7vol\u00b7les", "Preis\u00b7lied", "weihn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und Liebe wird selbstverst\u00e4ndlich.", "tokens": ["Und", "Lie\u00b7be", "wird", "selbst\u00b7ver\u00b7st\u00e4nd\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "ADJD", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Dein Herz sucht Liebe? \u2013 So m\u00f6ge es nie", "tokens": ["Dein", "Herz", "sucht", "Lie\u00b7be", "?", "\u2013", "So", "m\u00f6\u00b7ge", "es", "nie"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "NN", "$.", "$(", "ADV", "VMFIN", "PPER", "ADV"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Vom jetzigen Pfade weichen,", "tokens": ["Vom", "jet\u00b7zi\u00b7gen", "Pfa\u00b7de", "wei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Sei, was du bist, und wolle nie", "tokens": ["Sei", ",", "was", "du", "bist", ",", "und", "wol\u00b7le", "nie"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "$,", "PWS", "PPER", "VAFIN", "$,", "KON", "VMFIN", "ADV"], "meter":