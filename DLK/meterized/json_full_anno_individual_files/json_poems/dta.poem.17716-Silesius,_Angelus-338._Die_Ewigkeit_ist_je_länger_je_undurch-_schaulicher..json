{"dta.poem.17716": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "338.  Die Ewigkeit ist je l\u00e4nger je undurch-  \n schaulicher.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Das Meer der Ewigkeit je mehr\u2019s der Geist beschifft", "tokens": ["Das", "Meer", "der", "E\u00b7wig\u00b7keit", "je", "mehr's", "der", "Geist", "be\u00b7schifft"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "ADV", "PIS", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Je undurchfchifflicher und weiter ers betrifft.", "tokens": ["Je", "un\u00b7durch\u00b7fchiff\u00b7li\u00b7cher", "und", "wei\u00b7ter", "ers", "be\u00b7tr\u00b7ifft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADV", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+---", "measure": "unknown.measure.penta"}}}}}