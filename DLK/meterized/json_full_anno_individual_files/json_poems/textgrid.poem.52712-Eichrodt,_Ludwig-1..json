{"textgrid.poem.52712": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Oft mein ich, wenn ich in Tr\u00e4umen liege,", "tokens": ["Oft", "mein", "ich", ",", "wenn", "ich", "in", "Tr\u00e4u\u00b7men", "lie\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "PPER", "$,", "KOUS", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Es trete zu mir eine hehre Gestalt,", "tokens": ["Es", "tre\u00b7te", "zu", "mir", "ei\u00b7ne", "heh\u00b7re", "Ge\u00b7stalt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+---+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Und wenn ich mich freudvoll an sie schmiege,", "tokens": ["Und", "wenn", "ich", "mich", "freud\u00b7voll", "an", "sie", "schmie\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PRF", "ADJD", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "So k\u00fcsse sie mich mit Liebesgewalt;", "tokens": ["So", "k\u00fcs\u00b7se", "sie", "mich", "mit", "Lie\u00b7bes\u00b7ge\u00b7walt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "So spr\u00e4che sie laut, ich suchte dich lange,", "tokens": ["So", "spr\u00e4\u00b7che", "sie", "laut", ",", "ich", "such\u00b7te", "dich", "lan\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "$,", "PPER", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Und da ich dich endlich gefunden hab,", "tokens": ["Und", "da", "ich", "dich", "end\u00b7lich", "ge\u00b7fun\u00b7den", "hab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PRF", "ADV", "VVPP", "VAFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.7": {"text": "So la\u00df mich entsagen dem finstern Zwange,", "tokens": ["So", "la\u00df", "mich", "ent\u00b7sa\u00b7gen", "dem", "fins\u00b7tern", "Zwan\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.8": {"text": "So liebe du mich, du lieber Knab!", "tokens": ["So", "lie\u00b7be", "du", "mich", ",", "du", "lie\u00b7ber", "Knab", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "$,", "PPER", "ADV", "NE", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Und immer die gleiche, immer die holde", "tokens": ["Und", "im\u00b7mer", "die", "glei\u00b7che", ",", "im\u00b7mer", "die", "hol\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "ADJA", "$,", "ADV", "ART", "ADJA"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Erscheinet die liebliche Traumgestalt,", "tokens": ["Er\u00b7schei\u00b7net", "die", "lieb\u00b7li\u00b7che", "Traum\u00b7ge\u00b7stalt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Hochgr\u00fc\u00dfenden Augs, mit Locken von Golde", "tokens": ["Hoch\u00b7gr\u00fc\u00b7\u00dfen\u00b7den", "Augs", ",", "mit", "Lo\u00b7cken", "von", "Gol\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "APPR", "NN", "APPR", "NN"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die anmuthselige Schulter umwallt!", "tokens": ["Die", "an\u00b7muth\u00b7se\u00b7li\u00b7ge", "Schul\u00b7ter", "um\u00b7wallt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Woll\u00fcstig w\u00fchl ich im Golde der Locken,", "tokens": ["Wol\u00b7l\u00fcs\u00b7tig", "w\u00fchl", "ich", "im", "Gol\u00b7de", "der", "Lo\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "APPRART", "NN", "ART", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.6": {"text": "Ich presse mein Vollgl\u00fcck an die Brust,", "tokens": ["Ich", "pres\u00b7se", "mein", "Voll\u00b7gl\u00fcck", "an", "die", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Und, s\u00fc\u00df vor unendlichen Reizen erschrocken,", "tokens": ["Und", ",", "s\u00fc\u00df", "vor", "un\u00b7end\u00b7li\u00b7chen", "Rei\u00b7zen", "er\u00b7schro\u00b7cken", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ADJD", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.8": {"text": "Ersch\u00fcttert mich pl\u00f6tzlich der nahe Verlust.", "tokens": ["Er\u00b7sch\u00fct\u00b7tert", "mich", "pl\u00f6tz\u00b7lich", "der", "na\u00b7he", "Ver\u00b7lust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.3": {"line.1": {"text": "Ich frage mich oft, ich frage mich immer,", "tokens": ["Ich", "fra\u00b7ge", "mich", "oft", ",", "ich", "fra\u00b7ge", "mich", "im\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$,", "PPER", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Woher die Stimme so thr\u00e4nens\u00fc\u00df?", "tokens": ["Wo\u00b7her", "die", "Stim\u00b7me", "so", "thr\u00e4\u00b7nen\u00b7s\u00fc\u00df", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADV", "ADJD", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Woher der weihrauchwonnige Schimmer,", "tokens": ["Wo\u00b7her", "der", "weih\u00b7rauch\u00b7won\u00b7ni\u00b7ge", "Schim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wenn sie mich staunend einsam lie\u00df?", "tokens": ["Wenn", "sie", "mich", "stau\u00b7nend", "ein\u00b7sam", "lie\u00df", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADJD", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Woher der Augen leuchtende Lohe,", "tokens": ["Wo\u00b7her", "der", "Au\u00b7gen", "leuch\u00b7ten\u00b7de", "Lo\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Woher die zaubrische Liebesgewalt?", "tokens": ["Wo\u00b7her", "die", "zaub\u00b7ri\u00b7sche", "Lie\u00b7bes\u00b7ge\u00b7walt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Woher die reine, woher die hohe", "tokens": ["Wo\u00b7her", "die", "rei\u00b7ne", ",", "wo\u00b7her", "die", "ho\u00b7he"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "$,", "PWAV", "ART", "ADJA"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Woher die wunderbare Gestalt?", "tokens": ["Wo\u00b7her", "die", "wun\u00b7der\u00b7ba\u00b7re", "Ge\u00b7stalt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.4": {"line.1": {"text": "Doch \u2013 darf die n\u00fcchterne Seele fragen", "tokens": ["Doch", "\u2013", "darf", "die", "n\u00fcch\u00b7ter\u00b7ne", "See\u00b7le", "fra\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "$(", "VMFIN", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Nach dem geheimni\u00dfreichsten Wie?", "tokens": ["Nach", "dem", "ge\u00b7heim\u00b7ni\u00df\u00b7reichs\u00b7ten", "Wie", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "PWAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Von Wahngebilden ein Kluges sagen,", "tokens": ["Von", "Wahn\u00b7ge\u00b7bil\u00b7den", "ein", "Klu\u00b7ges", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Vom Spiel der m\u00fc\u00dfigen Phantasie?", "tokens": ["Vom", "Spiel", "der", "m\u00fc\u00b7\u00dfi\u00b7gen", "Phan\u00b7ta\u00b7sie", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+--", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Nein, um mich ewig begl\u00fcckt zu lassen,", "tokens": ["Nein", ",", "um", "mich", "e\u00b7wig", "be\u00b7gl\u00fcckt", "zu", "las\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "KOUI", "PRF", "ADJD", "VVPP", "PTKZU", "VVINF", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Verl\u00e4umdet mir nicht das zagende Gl\u00fcck,", "tokens": ["Ver\u00b7l\u00e4um\u00b7det", "mir", "nicht", "das", "za\u00b7gen\u00b7de", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Die Unaussprechliche w\u00fcrde mich hassen,", "tokens": ["Die", "Un\u00b7aus\u00b7sprech\u00b7li\u00b7che", "w\u00fcr\u00b7de", "mich", "has\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVINF", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Und, nimmer ach, kehrte sie mir zur\u00fcck!", "tokens": ["Und", ",", "nim\u00b7mer", "ach", ",", "kehr\u00b7te", "sie", "mir", "zu\u00b7r\u00fcck", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ADV", "ADV", "$,", "VVFIN", "PPER", "PPER", "PTKVZ", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.5": {"line.1": {"text": "Es ist kein Scheinen, kein Wahngebilde,", "tokens": ["Es", "ist", "kein", "Schei\u00b7nen", ",", "kein", "Wahn\u00b7ge\u00b7bil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "$,", "PIAT", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Kein Spiel der m\u00fc\u00dfigen Phantasie.", "tokens": ["Kein", "Spiel", "der", "m\u00fc\u00b7\u00dfi\u00b7gen", "Phan\u00b7ta\u00b7sie", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+--", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Sie schwebt aus g\u00f6ttlichem Gefilde,", "tokens": ["Sie", "schwebt", "aus", "g\u00f6tt\u00b7li\u00b7chem", "Ge\u00b7fil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das ihr so milden Reiz verlieh!", "tokens": ["Das", "ihr", "so", "mil\u00b7den", "Reiz", "ver\u00b7lieh", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ADV", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Es sind die heiligen, gl\u00fccklichen Inseln!", "tokens": ["Es", "sind", "die", "hei\u00b7li\u00b7gen", ",", "gl\u00fcck\u00b7li\u00b7chen", "In\u00b7seln", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Drauf wandelt im vollen Lebensdrang,", "tokens": ["Drauf", "wan\u00b7delt", "im", "vol\u00b7len", "Le\u00b7bens\u00b7drang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Was aus parrhasischen Meisterpinseln,", "tokens": ["Was", "aus", "par\u00b7rha\u00b7si\u00b7schen", "Meis\u00b7ter\u00b7pin\u00b7seln", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Was aus dem Haupt der Dichter sprang!", "tokens": ["Was", "aus", "dem", "Haupt", "der", "Dich\u00b7ter", "sprang", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Und sieh, und sieh! schon naht sie wieder,", "tokens": ["Und", "sieh", ",", "und", "sieh", "!", "schon", "naht", "sie", "wie\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KON", "VVIMP", "$.", "ADV", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mein Glaube wird, meine Treue belohnt,", "tokens": ["Mein", "Glau\u00b7be", "wird", ",", "mei\u00b7ne", "Treu\u00b7e", "be\u00b7lohnt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "$,", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Musik durchstr\u00f6mt die bl\u00fchenden Glieder,", "tokens": ["Mu\u00b7sik", "durch\u00b7str\u00f6mt", "die", "bl\u00fc\u00b7hen\u00b7den", "Glie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Darin der Geist des Wohlklangs wohnt.", "tokens": ["Da\u00b7rin", "der", "Geist", "des", "Wohl\u00b7klangs", "wohnt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sie l\u00e4chelt Dank, sie sinket nieder", "tokens": ["Sie", "l\u00e4\u00b7chelt", "Dank", ",", "sie", "sin\u00b7ket", "nie\u00b7der"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "$,", "PPER", "VVFIN", "PTKVZ"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wie herrliches Licht vom Maimond flie\u00dft,", "tokens": ["Wie", "herr\u00b7li\u00b7ches", "Licht", "vom", "Mai\u00b7mond", "flie\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Und mich berauschen unsterbliche Lieder,", "tokens": ["Und", "mich", "be\u00b7rau\u00b7schen", "uns\u00b7terb\u00b7li\u00b7che", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Wie sie Apollo, der Gott, genie\u00dft!", "tokens": ["Wie", "sie", "A\u00b7pol\u00b7lo", ",", "der", "Gott", ",", "ge\u00b7nie\u00dft", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "PPER", "NE", "$,", "ART", "NN", "$,", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "Oft mein ich, wenn ich in Tr\u00e4umen liege,", "tokens": ["Oft", "mein", "ich", ",", "wenn", "ich", "in", "Tr\u00e4u\u00b7men", "lie\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "PPER", "$,", "KOUS", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Es trete zu mir eine hehre Gestalt,", "to