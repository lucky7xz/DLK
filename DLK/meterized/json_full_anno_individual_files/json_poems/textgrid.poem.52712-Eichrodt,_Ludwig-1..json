, "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.8": {"text": "So liebe du mich, du lieber Knab!", "tokens": ["So", "lie\u00b7be", "du", "mich", ",", "du", "lie\u00b7ber", "Knab", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "$,", "PPER", "ADV", "NE", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.8": {"line.1": {"text": "Und immer die gleiche, immer die holde", "tokens": ["Und", "im\u00b7mer", "die", "glei\u00b7che", ",", "im\u00b7mer", "die", "hol\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "ADJA", "$,", "ADV", "ART", "ADJA"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Erscheinet die liebliche Traumgestalt,", "tokens": ["Er\u00b7schei\u00b7net", "die", "lieb\u00b7li\u00b7che", "Traum\u00b7ge\u00b7stalt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Hochgr\u00fc\u00dfenden Augs, mit Locken von Golde", "tokens": ["Hoch\u00b7gr\u00fc\u00b7\u00dfen\u00b7den", "Augs", ",", "mit", "Lo\u00b7cken", "von", "Gol\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "APPR", "NN", "APPR", "NN"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die anmuthselige Schulter umwallt!", "tokens": ["Die", "an\u00b7muth\u00b7se\u00b7li\u00b7ge", "Schul\u00b7ter", "um\u00b7wallt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Woll\