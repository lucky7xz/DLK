{"textgrid.poem.38364": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "W w Wiedhopf", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Wiedhopf ist sehr wohlgeziert, doch hat er keine Stimm,", "tokens": ["Der", "Wied\u00b7hopf", "ist", "sehr", "wohl\u00b7ge\u00b7ziert", ",", "doch", "hat", "er", "kei\u00b7ne", "Stimm", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "$,", "ADV", "VAFIN", "PPER", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "Sein Kr\u00f6nlein er stets mit sich f\u00fchrt, steckt doch nichts hinter ihm,", "tokens": ["Sein", "Kr\u00f6n\u00b7lein", "er", "stets", "mit", "sich", "f\u00fchrt", ",", "steckt", "doch", "nichts", "hin\u00b7ter", "ihm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "ADV", "APPR", "PRF", "VVFIN", "$,", "VVFIN", "ADV", "PIS", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.3": {"text": "Wie mancher hat viel Kleider, als w\u00e4re er ein Graf,", "tokens": ["Wie", "man\u00b7cher", "hat", "viel", "Klei\u00b7der", ",", "als", "w\u00e4\u00b7re", "er", "ein", "Graf", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VAFIN", "PIAT", "NN", "$,", "KOKOM", "VAFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Sein Vater ist ein Schneider, sein Bruder h\u00fct die Schaaf.", "tokens": ["Sein", "Va\u00b7ter", "ist", "ein", "Schnei\u00b7der", ",", "sein", "Bru\u00b7der", "h\u00fct", "di