{"textgrid.poem.55926": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "Die Fensterrose", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da drin: das tr\u00e4ge Treten ihrer Tatzen", "tokens": ["Da", "drin", ":", "das", "tr\u00e4\u00b7ge", "Tre\u00b7ten", "ih\u00b7rer", "Tat\u00b7zen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "PTKVZ", "$.", "ART", "ADJA", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "macht eine Stille, die dich fast verwirrt;", "tokens": ["macht", "ei\u00b7ne", "Stil\u00b7le", ",", "die", "dich", "fast", "ver\u00b7wirrt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "PRELS", "PPER", "ADV", "ADJD", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "und wie dann pl\u00f6tzlich eine von den Katzen", "tokens": ["und", "wie", "dann", "pl\u00f6tz\u00b7lich", "ei\u00b7ne", "von", "den", "Kat\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ADV", "ADJD", "ART", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "den Blick an ihr, der hin und wieder irrt,", "tokens": ["den", "Blick", "an", "ihr", ",", "der", "hin", "und", "wie\u00b7der", "irrt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "$,", "PRELS", "ADV", "KON", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "gewaltsam in ihr gro\u00dfes Auge nimmt, \u2013", "tokens": ["ge\u00b7walt\u00b7sam", "in", "ihr", "gro\u00b7\u00dfes", "Au\u00b7ge", "nimmt", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADJD", "APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "$,", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "den Blick, der, wie von eines Wirbels Kreis", "tokens": ["den", "Blick", ",", "der", ",", "wie", "von", "ei\u00b7nes", "Wir\u00b7bels", "Kreis"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "$,", "PWAV", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "ergriffen, eine kleine Weile schwimmt", "tokens": ["er\u00b7grif\u00b7fen", ",", "ei\u00b7ne", "klei\u00b7ne", "Wei\u00b7le", "schwimmt"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVPP", "$,", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "und dann versinkt und nichts mehr von sich wei\u00df,", "tokens": ["und", "dann", "ver\u00b7sinkt", "und", "nichts", "mehr", "von", "sich", "wei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "KON", "PIS", "ADV", "APPR", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "wenn dieses Auge, welches scheinbar ruht,", "tokens": ["wenn", "die\u00b7ses", "Au\u00b7ge", ",", "wel\u00b7ches", "schein\u00b7bar", "ruht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "$,", "PRELS", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "sich auftut und zusammenschl\u00e4gt mit Tosen", "tokens": ["sich", "auf\u00b7tut", "und", "zu\u00b7sam\u00b7men\u00b7schl\u00e4gt", "mit", "To\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PRF", "ADJD", "KON", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "und ihn hineinrei\u00dft bis ins rote Blut \u2013:", "tokens": ["und", "ihn", "hin\u00b7ein\u00b7rei\u00dft", "bis", "ins", "ro\u00b7te", "Blut", "\u2013", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "APPRART", "ADJA", "NN", "$(", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "So griffen einstmals aus dem Dunkelsein", "tokens": ["So", "grif\u00b7fen", "einst\u00b7mals", "aus", "dem", "Dun\u00b7kel\u00b7sein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "der Kathedralen gro\u00dfe Fensterrosen", "tokens": ["der", "Ka\u00b7thed\u00b7ra\u00b7len", "gro\u00b7\u00dfe", "Fens\u00b7ter\u00b7ro\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "ein Herz und rissen es in Gott hinein.", "tokens": ["ein", "Herz", "und", "ris\u00b7sen", "es", "in", "Gott", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "VVFIN", "PPER", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Da drin: das tr\u00e4ge Treten ihrer Tatzen", "tokens": ["Da", "drin", ":", "das", "tr\u00e4\u00b7ge", "Tre\u00b7ten", "ih\u00b7rer", "Tat\u00b7zen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "PTKVZ", "$.", "ART", "ADJA", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "macht eine Stille, die dich fast verwirrt;", "tokens": ["macht", "ei\u00b7ne", "Stil\u00b7le"