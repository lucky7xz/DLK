{"textgrid.poem.31287": {"metadata": {"author": {"name": "Ramler, Karl Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "An die Venus Urania", "genre": "verse", "period": "N.A.", "pub_year": 1761, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "G\u00f6ttin ", "tokens": ["G\u00f6t\u00b7tin"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.2": {"text": "Unsers Cineas Sohn, seinen vollendeten", "tokens": ["Un\u00b7sers", "Ci\u00b7ne\u00b7as", "Sohn", ",", "sei\u00b7nen", "voll\u00b7en\u00b7de\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NE", "NE", "NN", "$,", "PPOSAT", "ADJA"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "Tempel: Zeuch in dein Haus, Venus Urania,", "tokens": ["Tem\u00b7pel", ":", "Zeuch", "in", "dein", "Haus", ",", "Ve\u00b7nus", "U\u00b7ra\u00b7nia", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$.", "NN", "APPR", "PPOSAT", "NN", "$,", "NE", "NE", "$,"], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Erstgeborne des Himmels, ein!", "tokens": ["Erst\u00b7ge\u00b7bor\u00b7ne", "des", "Him\u00b7mels", ",", "ein", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "PTKVZ", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.2": {"line.1": {"text": "Unaufl\u00f6slich vereint folge dir, Arm in Arm", "tokens": ["Un\u00b7auf\u00b7l\u00f6s\u00b7lich", "ver\u00b7eint", "fol\u00b7ge", "dir", ",", "Arm", "in", "Arm"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "VVPP", "VVFIN", "PPER", "$,", "NN", "APPR", "NN"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Holde ", "tokens": ["Hol\u00b7de"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Unbestechliche ", "tokens": ["Un\u00b7be\u00b7stech\u00b7li\u00b7che"], "token_info": ["word"], "pos": ["NN"], "meter": "+-+--", "measure": "unknown.measure.di"}}, "stanza.3": {"line.1": {"text": "Keine reinere Hand brachte dir Weihrauch dar", "tokens": ["Kei\u00b7ne", "rei\u00b7ne\u00b7re", "Hand", "brach\u00b7te", "dir", "Weih\u00b7rauch", "dar"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "ADJA", "NN", "VVFIN", "PPER", "NN", "PTKVZ"], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.2": {"text": "Als dein Diener und Freund, mit ihm Arsinoe,", "tokens": ["Als", "dein", "Die\u00b7ner", "und", "Freund", ",", "mit", "ihm", "Ar\u00b7si\u00b7noe", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "KON", "NN", "$,", "APPR", "PPER", "NE", "$,"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Ihm an Tugenden, ihm gleich an erhabenem Geist,", "tokens": ["Ihm", "an", "Tu\u00b7gen\u00b7den", ",", "ihm", "gleich", "an", "er\u00b7ha\u00b7be\u00b7nem", "Geist", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "$,", "PPER", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "--+----+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Ihm an beiderlei Grazien.", "tokens": ["Ihm", "an", "bei\u00b7der\u00b7lei", "Gra\u00b7zi\u00b7en", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PIAT", "NN", "$."], "meter": "-+---+-+", "measure": "dactylic.init"}}, "stanza.4": {"line.1": {"text": "Keinen heiligern Sitz beut dir ein sterblich Paar:", "tokens": ["Kei\u00b7nen", "hei\u00b7li\u00b7gern", "Sitz", "beut", "dir", "ein", "sterb\u00b7lich", "Paar", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVFIN", "PPER", "ART", "ADJD", "NN", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Ihn wird schaudervoll, ihn ewig die schmeichelnde", "tokens": ["Ihn", "wird", "schau\u00b7der\u00b7voll", ",", "ihn", "e\u00b7wig", "die", "schmei\u00b7cheln\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADJD", "$,", "PPER", "ADJD", "ART", "ADJA"], "meter": "+-+-+-+--+--", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Afterg\u00f6ttin, nach dir f\u00e4lschlich genannt, und ihr", "tokens": ["Af\u00b7ter\u00b7g\u00f6t\u00b7tin", ",", "nach", "dir", "f\u00e4lschlich", "ge\u00b7nannt", ",", "und", "ihr"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "$,", "APPR", "PPER", "ADJD", "VVPP", "$,", "KON", "PPOSAT"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "Unholdinnengefolge flieh'n:", "tokens": ["Un\u00b7hol\u00b7din\u00b7nen\u00b7ge\u00b7fol\u00b7ge", "flieh'n", ":"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.5": {"line.1": {"text": "Natter, ", "tokens": ["Nat\u00b7ter", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.2": {"text": "Und mit rasendem Dolch und mit Medeischen", "tokens": ["Und", "mit", "ra\u00b7sen\u00b7dem", "Dolch", "und", "mit", "Me\u00b7dei\u00b7schen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN", "KON", "APPR", "NN"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Becher ", "tokens": ["Be\u00b7cher"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}}, "stanza.6": {"line.1": {"text": "Wann der sch\u00e4dliche Trupp aus den Hesperischen", "tokens": ["Wann", "der", "sch\u00e4d\u00b7li\u00b7che", "Trupp", "aus", "den", "Hes\u00b7pe\u00b7ri\u00b7schen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN", "APPR", "ART", "NN"], "meter": "+-+--+--+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Myrten, oder von dir, eitles Lutetien,", "tokens": ["Myr\u00b7ten", ",", "o\u00b7der", "von", "dir", ",", "eit\u00b7les", "Lu\u00b7te\u00b7ti\u00b7en", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "APPR", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "Auszeucht, oder den Weg aus dem Aucanzien-", "tokens": ["Aus\u00b7zeucht", ",", "o\u00b7der", "den", "Weg", "aus", "dem", "Au\u00b7can\u00b7zi\u00b7en"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "KON", "ART", "NN", "APPR", "ART", "TRUNC"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Hain der hei\u00dfen Iberer nimmt,", "tokens": ["Hain", "der", "hei\u00b7\u00dfen", "I\u00b7be\u00b7rer", "nimmt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.7": {"line.1": {"text": "Durch Teutonien irrt, dort ein begl\u00fccktes Volk", "tokens": ["Durch", "Teu\u00b7to\u00b7ni\u00b7en", "irrt", ",", "dort", "ein", "be\u00b7gl\u00fcck\u00b7tes", "Volk"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NE", "VVFIN", "$,", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zu verderben, das noch sittsame T\u00f6chter zeugt,", "tokens": ["Zu", "ver\u00b7der\u00b7ben", ",", "das", "noch", "sitt\u00b7sa\u00b7me", "T\u00f6ch\u00b7ter", "zeugt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "$,", "PRELS", "ADV", "ADJA", "NN", "VVFIN", "$,"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Noch vom besseren Blut Siegmars entsprossene", "tokens": ["Noch", "vom", "bes\u00b7se\u00b7ren", "Blut", "Sieg\u00b7mars", "ent\u00b7spros\u00b7se\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "ADJA", "NN", "NE", "NE"], "meter": "--+--+-+-+--", "measure": "anapaest.di.plus"}, "line.4": {"text": "Biederherzige. S\u00f6hne n\u00e4hrt,", "tokens": ["Bie\u00b7der\u00b7her\u00b7zi\u00b7ge", ".", "S\u00f6h\u00b7ne", "n\u00e4hrt", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$.", "NN", "VVFIN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.8": {"line.1": {"text": "Aber t\u00e4glich begr\u00fc\u00dft dich die Gerechtigkeit,", "tokens": ["A\u00b7ber", "t\u00e4g\u00b7lich", "be\u00b7gr\u00fc\u00dft", "dich", "die", "Ge\u00b7rech\u00b7tig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Die nun unter uns bleibt, dich die tief forschende", "tokens": ["Die", "nun", "un\u00b7ter", "uns", "bleibt", ",", "dich", "die", "tief", "for\u00b7schen\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADV", "APPR", "PPER", "VVFIN", "$,", "PRF", "ART", "ADJD", "ADJA"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "Weisheit, leichtes Gespr\u00e4chs, dich die verschwiegene", "tokens": ["Weis\u00b7heit", ",", "leich\u00b7tes", "Ge\u00b7spr\u00e4chs", ",", "dich", "die", "ver\u00b7schwie\u00b7ge\u00b7ne"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "ADJA", "NN", "$,", "PRF", "ART", "ADJA"], "meter": "+-+--+-+-+--", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Freundschaft, deinen Huldinnen gleich.", "tokens": ["Freund\u00b7schaft", ",", "dei\u00b7nen", "Hul\u00b7din\u00b7nen", "gleich", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "ADV", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.9": {"line.1": {"text": "Immer wechselnd besucht jede der Musen dich:", "tokens": ["Im\u00b7mer", "wech\u00b7selnd", "be\u00b7sucht", "je\u00b7de", "der", "Mu\u00b7sen", "dich", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "PIAT", "ART", "NN", "PPER", "$."], "meter": "+-+---+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Und zur gl\u00fccklichen Zeit eilet die helfende", "tokens": ["Und", "zur", "gl\u00fcck\u00b7li\u00b7chen", "Zeit", "ei\u00b7let", "die", "hel\u00b7fen\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "ADJA", "NN", "VVFIN", "ART", "ADJA"], "meter": "--+--++--+--", "measure": "anapaest.di.plus"}, "line.3": {"text": "Mutterg\u00f6ttin herbei, welche die Lieblinge", "tokens": ["Mut\u00b7ter\u00b7g\u00f6t\u00b7tin", "her\u00b7bei", ",", "wel\u00b7che", "die", "Lieb\u00b7lin\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "PTKVZ", "$,", "PRELS", "ART", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.4": {"text": "Deines Busens verewiget.", "tokens": ["Dei\u00b7nes", "Bu\u00b7sens", "ve\u00b7re\u00b7wi\u00b7get", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Nimm dein Heiligthum ein, Tochter des Himmels! hier", "tokens": ["Nimm", "dein", "Hei\u00b7lig\u00b7thum", "ein", ",", "Toch\u00b7ter", "des", "Him\u00b7mels", "!", "hier"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["NE", "PPOSAT", "NN", "PTKVZ", "$,", "NN", "ART", "NN", "$.", "ADV"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Sey dein erster Altar! wohne bei diesem Stamm,", "tokens": ["Sey", "dein", "ers\u00b7ter", "Al\u00b7tar", "!", "woh\u00b7ne", "bei", "die\u00b7sem", "Stamm", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "ADJA", "NN", "$.", "VVFIN", "APPR", "PDAT", "NN", "$,"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Bis im Jahrbuch der Welt ", "tokens": ["Bis", "im", "Jahr\u00b7buch", "der", "Welt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "APPRART", "NN", "ART", "NN"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Und am Himmel die Sonne stirbt.", "tokens": ["Und", "am", "Him\u00b7mel", "die", "Son\u00b7ne", "stirbt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.11": {"line.1": {"text": "G\u00f6ttin ", "tokens": ["G\u00f6t\u00b7tin"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.2": {"text": "Unsers Cineas Sohn, seinen vollendeten", "tokens": ["Un\u00b7sers", "Ci\u00b7ne\u00b7as", "Sohn", ",", "sei\u00b7nen", "voll\u00b7en\u00b7de\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NE", "NE", "NN", "$,", "PPOSAT", "ADJA"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "Tempel: Zeuch in dein Haus, Venus Urania,", "tokens": ["Tem\u00b7pel", ":", "Zeuch", "in", "dein", "Haus", ",", "Ve\u00b7nus", "U\u00b7ra