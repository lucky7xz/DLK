{"textgrid.poem.42119": {"metadata": {"author": {"name": "Weissmann, Maria Luise", "birth": "N.A.", "death": "N.A."}, "title": "Auf ein Paket mit Briefen", "genre": "verse", "period": "N.A.", "pub_year": 1914, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So jahrlang totgesagt, da\u00df ich es hob", "tokens": ["So", "jahr\u00b7lang", "tot\u00b7ge\u00b7sagt", ",", "da\u00df", "ich", "es", "hob"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VVFIN", "$,", "KOUS", "PPER", "PPER", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wie eine Aschenurne, Und gefa\u00dft", "tokens": ["Wie", "ei\u00b7ne", "A\u00b7schen\u00b7ur\u00b7ne", ",", "Und", "ge\u00b7fa\u00dft"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "ART", "NN", "$,", "KON", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df nicht der Staub aus dem Verblichnen stob", "tokens": ["Da\u00df", "nicht", "der", "Staub", "aus", "dem", "Ver\u00b7blich\u00b7nen", "stob"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PTKNEG", "ART", "NN", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wollt ich sie tragen. Doch mich bog die Last:", "tokens": ["Wollt", "ich", "sie", "tra\u00b7gen", ".", "Doch", "mich", "bog", "die", "Last", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "VVINF", "$.", "KON", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Entschwundne Himmel brachen strahlend nieder", "tokens": ["Ent\u00b7schwund\u00b7ne", "Him\u00b7mel", "bra\u00b7chen", "strah\u00b7lend", "nie\u00b7der"], "token_info": ["word", "w