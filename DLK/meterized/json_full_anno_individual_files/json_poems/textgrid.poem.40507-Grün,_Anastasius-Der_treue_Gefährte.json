{"textgrid.poem.40507": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "Der treue Gef\u00e4hrte", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich hatt' einst einen Genossen treu,", "tokens": ["Ich", "hatt'", "einst", "ei\u00b7nen", "Ge\u00b7nos\u00b7sen", "treu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "NN", "ADJD", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wo ich war, war er auch dabei;", "tokens": ["Wo", "ich", "war", ",", "war", "er", "auch", "da\u00b7bei", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VAFIN", "$,", "VAFIN", "PPER", "ADV", "PAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Blieb ich daheim, ging er auch nicht aus,", "tokens": ["Blieb", "ich", "da\u00b7heim", ",", "ging", "er", "auch", "nicht", "aus", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "VVFIN", "PPER", "ADV", "PTKNEG", "PTKVZ", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Und ging ich fort, blieb er nicht zu Haus.", "tokens": ["Und", "ging", "ich", "fort", ",", "blieb", "er", "nicht", "zu", "Haus", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKVZ", "$,", "VVFIN", "PPER", "PTKNEG", "APPR", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Er trank aus einem Glas mit mir,", "tokens": ["Er", "trank", "aus", "ei\u00b7nem", "Glas", "mit", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er schlief in einem Bett mit mir,", "tokens": ["Er", "schlief", "in", "ei\u00b7nem", "Bett", "mit", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wir trugen die Kleider nach einem Schnitt,", "tokens": ["Wir", "tru\u00b7gen", "die", "Klei\u00b7der", "nach", "ei\u00b7nem", "Schnitt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Ja selbst zum Liebchen nahm ich ihn mit.", "tokens": ["Ja", "selbst", "zum", "Lieb\u00b7chen", "nahm", "ich", "ihn", "mit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADV", "APPRART", "NN", "VVFIN", "PPER", "PPER", "PTKVZ", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.3": {"line.1": {"text": "Und als mich's j\u00fcngst zu den Bergen zog,", "tokens": ["Und", "als", "mich's", "j\u00fcngst", "zu", "den", "Ber\u00b7gen", "zog", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PIS", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und Stab und B\u00fcndel im Arm ich wog,", "tokens": ["Und", "Stab", "und", "B\u00fcn\u00b7del", "im", "Arm", "ich", "wog", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "APPRART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Da sprach der treue Geselle gleich:", "tokens": ["Da", "sprach", "der", "treu\u00b7e", "Ge\u00b7sel\u00b7le", "gleich", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "ADV", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Mit Gunsten, Freund, ich geh' mit euch!", "tokens": ["Mit", "Guns\u00b7ten", ",", "Freund", ",", "ich", "geh'", "mit", "euch", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "$,", "PPER", "VVFIN", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Wir wallten still hinaus zum Thor,", "tokens": ["Wir", "wall\u00b7ten", "still", "hin\u00b7aus", "zum", "Thor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die B\u00e4ume streben frisch empor,", "tokens": ["Die", "B\u00e4u\u00b7me", "stre\u00b7ben", "frisch", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die L\u00fcfte bringen uns warmen Gru\u00df,", "tokens": ["Die", "L\u00fcf\u00b7te", "brin\u00b7gen", "uns", "war\u00b7men", "Gru\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Da sch\u00fcttelt der Freund den Kopf mit Verdru\u00df.", "tokens": ["Da", "sch\u00fct\u00b7telt", "der", "Freund", "den", "Kopf", "mit", "Ver\u00b7dru\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "APPR", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Im Aether jauchzt ein Lerchenchor,", "tokens": ["Im", "A\u00b7e\u00b7ther", "jauchzt", "ein", "Ler\u00b7chen\u00b7chor", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Da h\u00e4lt er zugepre\u00dft sein Ohr;", "tokens": ["Da", "h\u00e4lt", "er", "zu\u00b7ge\u00b7pre\u00dft", "sein", "Ohr", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "S\u00fc\u00df duftet dort das Rosengestr\u00e4uch,", "tokens": ["S\u00fc\u00df", "duf\u00b7tet", "dort", "das", "Ro\u00b7sen\u00b7ge\u00b7str\u00e4uch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Da wird er schwindlig und todtenbleich.", "tokens": ["Da", "wird", "er", "schwind\u00b7lig", "und", "tod\u00b7ten\u00b7bleich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "Und als wir stiegen den Berg hinan,", "tokens": ["Und", "als", "wir", "stie\u00b7gen", "den", "Berg", "hi\u00b7nan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Verlor den Athem der arme Mann;", "tokens": ["Ver\u00b7lor", "den", "A\u00b7them", "der", "ar\u00b7me", "Mann", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ich wallt' empor mit leuchtendem Blick,", "tokens": ["Ich", "wallt'", "em\u00b7por", "mit", "leuch\u00b7ten\u00b7dem", "Blick", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Doch er blieb keuchend unten zur\u00fcck.", "tokens": ["Doch", "er", "blieb", "keu\u00b7chend", "un\u00b7ten", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADJD", "ADV", "PTKVZ", "$."], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}}, "stanza.7": {"line.1": {"text": "Ich aber stand jauchzend ganz allein", "tokens": ["Ich", "a\u00b7ber", "stand", "jauch\u00b7zend", "ganz", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "ADJD", "ADV", "ADV"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Am Bergesgipfel im Sonnenschein!", "tokens": ["Am", "Ber\u00b7ges\u00b7gip\u00b7fel", "im", "Son\u00b7nen\u00b7schein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPRART", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Rings gr\u00fcne Triften und Blumenduft!", "tokens": ["Rings", "gr\u00fc\u00b7ne", "Trif\u00b7ten", "und", "Blu\u00b7men\u00b7duft", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Rings wirbelnde Lerchen und Bergesluft!", "tokens": ["Rings", "wir\u00b7beln\u00b7de", "Ler\u00b7chen", "und", "Ber\u00b7ge\u00b7sluft", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "KON", "NN", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}}, "stanza.8": {"line.1": {"text": "Und als ich wieder zu Thal gewallt,", "tokens": ["Und", "als", "ich", "wie\u00b7der", "zu", "Thal", "ge\u00b7wallt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Da stie\u00df ich auf eine Leiche bald:", "tokens": ["Da", "stie\u00df", "ich", "auf", "ei\u00b7ne", "Lei\u00b7che", "bald", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "ADV", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "O weh, er ist's! Todt liegt er hier,", "tokens": ["O", "weh", ",", "er", "ist's", "!", "Todt", "liegt", "er", "hier", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$,", "PPER", "VAFIN", "$.", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der einst der treu'ste Gef\u00e4hrte mir!", "tokens": ["Der", "einst", "der", "treu'\u00b7ste", "Ge\u00b7f\u00e4hr\u00b7te", "mir", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "ADJA", "NN", "PPER", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.9": {"line.1": {"text": "Da lie\u00df ich graben ein tiefes Grab", "tokens": ["Da", "lie\u00df", "ich", "gra\u00b7ben", "ein", "tie\u00b7fes", "Grab"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und senkte die Leiche still hinab,", "tokens": ["Und", "senk\u00b7te", "die", "Lei\u00b7che", "still", "hin\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADJD", "PTKVZ", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Drauf setzt' ich einen Leichenstein", "tokens": ["Drauf", "setzt'", "ich", "ei\u00b7nen", "Lei\u00b7chen\u00b7stein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und grub die Wort' als Inschrift drein:", "tokens": ["Und", "grub", "die", "Wort'", "als", "In\u00b7schrift", "drein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "KOUS", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "\u00bbhier ruht mein treu'ster Geno\u00df im Land,", "tokens": ["\u00bb", "hier", "ruht", "mein", "treu'\u00b7ster", "Ge\u00b7no\u00df", "im", "Land", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPOSAT", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Herr Hypochonder zubenannt;", "tokens": ["Herr", "Hy\u00b7po\u00b7chon\u00b7der", "zu\u00b7be\u00b7nannt", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NE", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er starb an frischer Bergesluft,", "tokens": ["Er", "starb", "an", "fri\u00b7scher", "Ber\u00b7ge\u00b7sluft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "An Lerchenschlag und Rosenduft!", "tokens": ["An", "Ler\u00b7chen\u00b7schlag", "und", "Ro\u00b7sen\u00b7duft", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Sonst w\u00fcnsch' ich ihm alles Gl\u00fcck und Heil,", "tokens": ["Sonst", "w\u00fcn\u00b7sch'", "ich", "ihm", "al\u00b7les", "Gl\u00fcck", "und", "Heil", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "PIAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die ewige Ruh' werd' ihm zu Theil,", "tokens": ["Die", "e\u00b7wi\u00b7ge", "Ruh'", "werd'", "ihm", "zu", "Theil", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Nur wahr' mich Gott vor'm Wiedersehn", "tokens": ["Nur", "wahr'", "mich", "Gott", "vor'm", "Wie\u00b7der\u00b7sehn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und seinem fr\u00f6hlichen Auferstehn!\u00ab", "tokens": ["Und", "sei\u00b7nem", "fr\u00f6h\u00b7li\u00b7chen", "Auf\u00b7er\u00b7stehn", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "$.", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.12": {"line.1": {"text": "Ich hatt' einst einen Genossen treu,", "tokens": ["Ich", "hatt'", "einst", "ei\u00b7nen", "Ge\u00b7nos\u00b7sen", "treu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "NN", "ADJD", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wo ich war, war er auch dabei;", "tokens": ["Wo", "ich", "war", ",", "war", "er", "auch", "da\u00b7bei", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VAFIN", "$,", "VAFIN", "PPER", "ADV", "PAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Blieb ich daheim, ging er auch nicht aus,", "tokens": ["Blieb", "ich", "da\u00b7heim", ",", "ging", "er", "auch", "nicht", "aus", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "VVFIN", "PPER", "ADV", "PTKNEG", "PTKVZ", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Und ging ich fort, blieb er nicht zu Haus.", "tokens": ["Und", "ging", "ich", "fort", ",", "blieb", "er", "nicht", "zu", "Haus", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKVZ", "$,", "VVFIN", "PPER", "PTKNEG", "APPR", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.13": {"line.1": {"text": "Er trank aus einem Glas mit mir,", "tokens": ["Er", "trank", "aus", "ei\u00b7nem", "Glas", "mit", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er schlief in einem Bett mit mir,", "tokens": ["Er", "schlief", "in", "ei\u00b7nem", "Bett", "mit", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wir trugen die Kleider nach einem Schnitt,", "tokens": ["Wir", "tru\u00b7gen", "die", "Klei\u00b7der", "nach", "ei\u00b7nem", "Schnitt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Ja selbst zum Liebchen nahm ich ihn mit.", "tokens": ["Ja", "selbst", "zum", "Lieb\u00b7chen", "nahm", "ich", "ihn", "mit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADV", "APPRART", "NN", "VVFIN", "PPER", "PPER", "PTKVZ", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.14": {"line.1": {"text": "Und als mich's j\u00fcngst zu den Bergen zog,", "tokens": ["Und", "als", "mich's", "j\u00fcngst", "zu", "den", "Ber\u00b7gen", "zog", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PIS", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und Stab und B\u00fcndel im Arm ich wog,", "tokens": ["Und", "Stab", "und", "B\u00fcn\u00b7del", "im", "Arm", "ich", "wog", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "APPRART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Da sprach der treue Geselle gleich:", "tokens": ["Da", "sprach", "der", "treu\u00b7e", "Ge\u00b7sel\u00b7le", "gleich", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "ADV", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Mit Gunsten, Freund, ich geh' mit euch!", "tokens": ["Mit", "Guns\u00b7ten", ",", "Freund", ",", "ich", "geh'", "mit", "euch", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "$,", "PPER", "VVFIN", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.15": {"line.1": {"text": "Wir wallten still hinaus zum Thor,", "tokens": ["Wir", "wall\u00b7ten", "still", "hin\u00b7aus", "zum", "Thor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die B\u00e4ume streben frisch empor,", "tokens": ["Die", "B\u00e4u\u00b7me", "stre\u00b7ben", "frisch", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die L\u00fcfte bringen uns warmen Gru\u00df,", "tokens": ["Die", "L\u00fcf\u00b7te", "brin\u00b7gen", "uns", "war\u00b7men", "Gru\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Da sch\u00fcttelt der Freund den Kopf mit Verdru\u00df.", "tokens": ["Da", "sch\u00fct\u00b7telt", "der", "Freund", "den", "Kopf", "mit", "Ver\u00b7dru\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "APPR", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.16": {"line.1": {"text": "Im Aether jauchzt ein Lerchenchor,", "tokens": ["Im", "A\u00b7e\u00b7ther", "jauchzt", "ein", "Ler\u00b7chen\u00b7chor", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Da h\u00e4lt er zugepre\u00dft sein Ohr;", "tokens": ["Da", "h\u00e4lt", "er", "zu\u00b7ge\u00b7pre\u00dft", "sein", "Ohr", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "S\u00fc\u00df duftet dort das Rosengestr\u00e4uch,", "tokens": ["S\u00fc\u00df", "duf\u00b7tet", "dort", "das", "Ro\u00b7sen\u00b7ge\u00b7str\u00e4uch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Da wird er schwindlig und todtenbleich.", "tokens": ["Da", "wird", "er", "schwind\u00b7lig", "und", "tod\u00b7ten\u00b7bleich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.17": {"line.1": {"text": "Und als wir stiegen den Berg hinan,", "tokens": ["Und", "als", "wir", "stie\u00b7gen", "den", "Berg", "hi\u00b7nan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Verlor den Athem der arme Mann;", "tokens": ["Ver\u00b7lor", "den", "A\u00b7them", "der", "ar\u00b7me", "Mann", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ich wallt' empor mit leuchtendem Blick,", "tokens": ["Ich", "wallt'", "em\u00b7por", "mit", "leuch\u00b7ten\u00b7dem", "Blick", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Doch er blieb keuchend unten zur\u00fcck.", "tokens": ["Doch", "er", "blieb", "keu\u00b7c