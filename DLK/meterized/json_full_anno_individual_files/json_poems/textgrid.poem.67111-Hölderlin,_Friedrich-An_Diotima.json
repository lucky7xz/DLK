{"textgrid.poem.67111": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "An Diotima", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sch\u00f6nes Leben! du lebst, wie die zarten Bl\u00fcten im Winter,", "tokens": ["Sch\u00f6\u00b7nes", "Le\u00b7ben", "!", "du", "lebst", ",", "wie", "die", "zar\u00b7ten", "Bl\u00fc\u00b7ten", "im", "Win\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "PPER", "VVFIN", "$,", "PWAV", "ART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.2": {"text": "In der gealterten Welt bl\u00fchst du verschlossen, allein.", "tokens": ["In", "der", "ge\u00b7al\u00b7ter\u00b7ten", "Welt", "bl\u00fchst", "du", "ver\u00b7schlos\u00b7sen", ",", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "PPER", "VVPP", "$,", "ADV", "$."], "meter": "-+-+--+-+-+--+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Liebend strebst du hinaus, dich zu sonnen am Lichte des Fr\u00fchlings,", "tokens": ["Lie\u00b7bend", "strebst", "du", "hin\u00b7aus", ",", "dich", "zu", "son\u00b7nen", "am", "Lich\u00b7te", "des", "Fr\u00fch\u00b7lings", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PTKVZ", "$,", "PPER", "PTKZU", "VVINF", "APPRART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+--+--+-", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Zu erwarmen an ihr, suchst du die Jugend der Welt.", "tokens": ["Zu", "er\u00b7war\u00b7men", "an", "ihr", ",", "suchst", "du", "die", "Ju\u00b7gend", "der", "Welt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "APPR", "PPER", "$,", "VVFIN", "PPER", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.5": {"text": "Deine Sonne, die sch\u00f6nere Zeit, ist untergegangen", "tokens": ["Dei\u00b7ne", "Son\u00b7ne", ",", "die", "sch\u00f6\u00b7ne\u00b7re", "Zeit", ",", "ist", "un\u00b7ter\u00b7ge\u00b7gan\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "ART", "ADJA", "NN", "$,", "VAFIN", "VVPP"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.6": {"text": "Und in frostiger Nacht zanken Orkane sich nun.", "tokens": ["Und", "in", "fros\u00b7ti\u00b7ger", "Nacht", "zan\u00b7ken", "Or\u00b7ka\u00b7ne", "sich", "nun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "ADJA", "NN", "PRF", "ADV", "$."], "meter": "--+--++-++--+", "measure": "anapaest.di.plus"}}, "stanza.2": {"line.1": {"text": "Sch\u00f6nes Leben! du lebst, wie die zarten Bl\u00fcten im Winter,", "tokens": ["Sch\u00f6\u00b7nes", "Le\u00b7ben", "!", "du", "lebst", ",", "wie", "die", "zar\u00b7ten", "Bl\u00fc\u00b7ten", "im", "Win\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "PPER", "VVFIN", "$,", "PWAV", "ART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.2": {"text": "In der gealterten Welt bl\u00fchst du verschlossen, allein.", "tokens": ["In", "der", "ge\u00b7al\u00b7ter\u00b7ten", "Welt", "bl\u00fchst", "du", "ver\u00b7schlos\u00b7sen", ",", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "p