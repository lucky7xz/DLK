{"dta.poem.21477": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Atlantis .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Heilige Zukunftsschaaren, des S\u00e4ngers Gru\u00df euch!", "tokens": ["Hei\u00b7li\u00b7ge", "Zu\u00b7kunfts\u00b7schaa\u00b7ren", ",", "des", "S\u00e4n\u00b7gers", "Gru\u00df", "euch", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ART", "NN", "NN", "PPER", "$."], "meter": "+--+-+--+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Einen Wurfspeer schleudert er, voller Ahnung,", "tokens": ["Ei\u00b7nen", "Wurf\u00b7speer", "schleu\u00b7dert", "er", ",", "vol\u00b7ler", "Ah\u00b7nung", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.3": {"text": "Durch das Nachtgraun, lichtere Schimmer sehend,", "tokens": ["Durch", "das", "Nacht\u00b7graun", ",", "lich\u00b7te\u00b7re", "Schim\u00b7mer", "se\u00b7hend", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "Seinen Gesang hin!", "tokens": ["Sei\u00b7nen", "Ge\u00b7sang", "hin", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKVZ", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "La\u00dft der Weisheit d\u00fcstere Rabenstimme", "tokens": ["La\u00dft", "der", "Weis\u00b7heit", "d\u00fcs\u00b7te\u00b7re", "Ra\u00b7bens\u00b7tim\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "ART", "NN", "VVFIN", "NN"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Von dem niemals endenden Elend kr\u00e4chzen,", "tokens": ["Von", "dem", "nie\u00b7mals", "en\u00b7den\u00b7den", "E\u00b7lend", "kr\u00e4ch\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADV", "ADJA", "NN", "VVINF", "$,"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Was so hoffnungsfr\u00f6hlich das Herz durchzittert,", "tokens": ["Was", "so", "hoff\u00b7nungs\u00b7fr\u00f6h\u00b7lich", "das", "Herz", "durch\u00b7zit\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADJD", "ART", "NN", "VVPP", "$,"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Mu\u00df sich entfalten.             ", "tokens": ["Mu\u00df", "sich", "ent\u00b7fal\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "VVINF", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "Still und langsam windet sich hin die Raupe,", "tokens": ["Still", "und", "lang\u00b7sam", "win\u00b7det", "sich", "hin", "die", "Rau\u00b7pe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "VVFIN", "PRF", "ADV", "ART", "NN", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.2": {"text": "Aber heimlich sp\u00fcret sie schon die Fl\u00fcgel,", "tokens": ["A\u00b7ber", "heim\u00b7lich", "sp\u00fc\u00b7ret", "sie", "schon", "die", "Fl\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.3": {"text": "Die sie einst als schillernden Falter tragen", "tokens": ["Die", "sie", "einst", "als", "schil\u00b7lern\u00b7den", "Fal\u00b7ter", "tra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ADV", "KOUS", "ADJA", "NN", "VVINF"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ueber die Auen.", "tokens": ["Ue\u00b7ber", "die", "Au\u00b7en", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.4": {"line.1": {"text": "Ja, nur ein Ziel leuchte den Erdens\u00f6hnen!", "tokens": ["Ja", ",", "nur", "ein", "Ziel", "leuch\u00b7te", "den", "Er\u00b7den\u00b7s\u00f6h\u00b7nen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADV", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-++--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ha, nur ein Ziel singet, ihr heil\u2019gen Dichter:", "tokens": ["Ha", ",", "nur", "ein", "Ziel", "sin\u00b7get", ",", "ihr", "heil'\u00b7gen", "Dich\u00b7ter", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADV", "ART", "NN", "VVFIN", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "+--++--+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "\u201eedler Freiheit fl\u00fcchtig gew\u00e4hrte, goldne", "tokens": ["\u201e", "ed\u00b7ler", "Frei\u00b7heit", "fl\u00fcch\u00b7tig", "ge\u00b7w\u00e4hr\u00b7te", ",", "gold\u00b7ne"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word"], "pos": ["$(", "ADJA", "NN", "ADJD", "ADJA", "$,", "ADJA"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "F\u00fclle hienieden!\u201c", "tokens": ["F\u00fcl\u00b7le", "hien\u00b7ie\u00b7den", "!", "\u201c"], "token_info": ["word", "word", "punct", "punct"], "pos": ["NN", "ADV", "$.", "$("], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.5": {"line.1": {"text": "Nimmermehr auf anderen Lichtgefilden,", "tokens": ["Nim\u00b7mer\u00b7mehr", "auf", "an\u00b7de\u00b7ren", "Licht\u00b7ge\u00b7fil\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.2": {"text": "Welche noch kein sterblicher Blick durchmessen,", "tokens": ["Wel\u00b7che", "noch", "kein", "sterb\u00b7li\u00b7cher", "Blick", "durch\u00b7mes\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "PIAT", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.3": {"text": "Sollst du aufbl\u00fchn, s\u00fc\u00dfester Traum der Menschheit,", "tokens": ["Sollst", "du", "auf\u00b7bl\u00fchn", ",", "s\u00fc\u00b7\u00dfes\u00b7ter", "Traum", "der", "Menschheit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVINF", "$,", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Sonnig Atlantis!", "tokens": ["Son\u00b7nig", "At\u00b7lan\u00b7tis", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "NE", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.6": {"line.1": {"text": "Unter uns vom perlenden Thau des Aethers", "tokens": ["Un\u00b7ter", "uns", "vom", "per\u00b7len\u00b7den", "Thau", "des", "A\u00b7e\u00b7thers"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "APPRART", "ADJA", "NN", "ART", "NN"], "meter": "+-+-+--+-+--", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Wonnesanft umschmeichelt, erbl\u00fch\u2019 den Enkeln,", "tokens": ["Won\u00b7ne\u00b7sanft", "um\u00b7schmei\u00b7chelt", ",", "er\u00b7bl\u00fch'", "den", "En\u00b7keln", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.3": {"text": "Welche fromm wie wandelnde Blumen leben,", "tokens": ["Wel\u00b7che", "fromm", "wie", "wan\u00b7deln\u00b7de", "Blu\u00b7men", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "KOKOM", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "Bilden und schaffen!", "tokens": ["Bil\u00b7den", "und", "schaf\u00b7fen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "VVINF", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.7": {"line.1": {"text": "Aber ihr, o gl\u00fcckliche L\u00e4nder, denen", "tokens": ["A\u00b7ber", "ihr", ",", "o", "gl\u00fcck\u00b7li\u00b7che", "L\u00e4n\u00b7der", ",", "de\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["KON", "PPER", "$,", "FM", "ADJA", "NN", "$,", "PRELS"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.2": {"text": "Wir den fernher leuchtenden Port gewiesen,", "tokens": ["Wir", "den", "fern\u00b7her", "leuch\u00b7ten\u00b7den", "Port", "ge\u00b7wie\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "NE", "VVPP", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Weiht dann uns manch sch\u00e4umendes Glas, Dankthr\u00e4nen", "tokens": ["Weiht", "dann", "uns", "manch", "sch\u00e4u\u00b7men\u00b7des", "Glas", ",", "Dank\u00b7thr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["VVFIN", "ADV", "PPER", "PIAT", "ADJA", "NN", "$,", "NN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "Himmlischer Freude!", "tokens": ["Himm\u00b7li\u00b7scher", "Freu\u00b7de", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}}}}