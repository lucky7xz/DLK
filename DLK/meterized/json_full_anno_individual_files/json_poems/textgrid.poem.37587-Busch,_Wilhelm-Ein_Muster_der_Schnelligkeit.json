{"textgrid.poem.37587": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Ein Muster der Schnelligkeit", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Panzerkr\u00f6te sprach zur Schnecke:", "tokens": ["Die", "Pan\u00b7zer\u00b7kr\u00f6\u00b7te", "sprach", "zur", "Schne\u00b7cke", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Pfui! Sch\u00e4me dich!", "tokens": ["Pfui", "!", "Sch\u00e4\u00b7me", "dich", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$.", "VVFIN", "PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Du kommst ja gar nicht recht vom Flecke!", "tokens": ["Du", "kommst", "ja", "gar", "nicht", "recht", "vom", "Fle\u00b7cke", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "PTKNEG", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da sieh mal mich!!", "tokens": ["Da", "sieh", "mal", "mich", "!!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Wenn das Rhinozeros, das schlimme,", "tokens": ["Wenn", "das", "Rhi\u00b7no\u00b7ze\u00b7ros", ",", "das", "schlim\u00b7me", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,", "PDS", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dich kriegen will in seinem Grimme,", "tokens": ["Dich", "krie\u00b7gen", "will", "in", "sei\u00b7nem", "Grim\u00b7me", ","], "token_info": ["word",