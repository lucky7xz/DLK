{"dta.poem.3188": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "39.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "La\u00df deinen Arm nicht schlapp am Leibe niederhangen,", "tokens": ["La\u00df", "dei\u00b7nen", "Arm", "nicht", "schlapp", "am", "Lei\u00b7be", "nie\u00b7der\u00b7han\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "PTKNEG", "ADJD", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und la\u00df ihn auch zu weit aus in die Luft nicht langen.", "tokens": ["Und", "la\u00df", "ihn", "auch", "zu", "weit", "aus", "in", "die", "Luft", "nicht", "lan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "ADV", "PTKA", "ADJD", "APPR", "APPR", "ART", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Denn nichts erlangen wird, wer nicht den Arm ausstreckt,", "tokens": ["Denn", "nichts", "er\u00b7lan\u00b7gen", "wird", ",", "wer", "nicht", "den", "Arm", "aus\u00b7streckt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVINF", "VAFIN", "$,", "PWS", "PTKNEG", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und der verrenket ihn, wer ihn zu weit ausreckt.", "tokens": ["Und", "der", "ver\u00b7ren\u00b7ket", "ihn", ",", "wer", "ihn", "zu", "weit", "aus\u00b7reckt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"