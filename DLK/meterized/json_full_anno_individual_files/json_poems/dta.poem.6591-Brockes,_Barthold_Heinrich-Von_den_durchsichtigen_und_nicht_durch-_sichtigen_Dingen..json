{"dta.poem.6591": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den durchsichtigen und nicht durch-  \n sichtigen Dingen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "So wird, durch dichtes Laub, ein grosser Wald,", "tokens": ["So", "wird", ",", "durch", "dich\u00b7tes", "Laub", ",", "ein", "gros\u00b7ser", "Wald", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "$,", "APPR", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Selbst in der Mittags-Zeit, der Schatten Aufenthalt,", "tokens": ["Selbst", "in", "der", "Mit\u00b7tags\u00b7Zeit", ",", "der", "Schat\u00b7ten", "Auf\u00b7ent\u00b7halt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und hindert uns der Sonnen Schein zu sehen.", "tokens": ["Und", "hin\u00b7dert", "uns", "der", "Son\u00b7nen", "Schein", "zu", "se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}