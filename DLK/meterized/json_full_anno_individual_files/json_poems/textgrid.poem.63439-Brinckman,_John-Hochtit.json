ord", "punct"], "pos": ["KON", "PWS", "PTKNEG", "VVFIN", "$,", "NE", "VVFIN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Den Schulten d\u00fcst in'n Kopp dat so,", "tokens": ["Den", "Schul\u00b7ten", "d\u00fcst", "in'n", "Kopp", "dat", "so", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "ART", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Karlin de danzt noch \u00fcmmerto \u2013", "tokens": ["Kar\u00b7lin", "de", "danzt", "noch", "\u00fcm\u00b7mer\u00b7to", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ADV", "ADV", "ADV", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "juchhei, juchhei un hopsasa!", "tokens": ["juch\u00b7hei", ",", "juch\u00b7hei", "un", "hop\u00b7sa\u00b7sa", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PTKVZ", "$,", "FM", "FM", "FM", "$."], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.8": {"text": "un heidilditschen dallala!", "tokens": ["un", "hei\u00b7dil\u00b7dit\u00b7schen", "dal\u00b7la\u00b7la", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Noch is keen twintig Johr Karlin,", "tokens": ["Noch", "is", "ke\u00b7en", "twin\u00b7tig", "Johr", "Kar\u00b7lin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "FM", "VVFIN", "ADJD", "NE", "NE", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "smuck is s' un stramm as keen;", "tokens": ["smuck", "is", "s'", "un", "stramm", "as", "ke\u00b7en", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "FM", "FM", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "de Schult de k\u00fcnn ehr Vader sin", "tokens": ["de", "Schult", "de", "k\u00fcnn", "ehr", "Va\u00b7der", "sin"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "un is man swack to Been.", "tokens": ["un", "is", "man", "swack", "to", "Be\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "FM", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Na, he hett allens s\u00fcs f\u00f6r twee,", "tokens": ["Na", ",", "he", "hett", "al\u00b7lens", "s\u00fcs", "f\u00f6r", "twee", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "un wat em f\u00e4hlt, na, dat hett se \u2013", "tokens": ["un", "wat", "em", "f\u00e4hlt", ",", "na", ",", "dat", "hett", "se", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "VVFIN", "$,", "ITJ", "$,", "FM.nl", "FM.nl", "FM.nl", "$("], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.7": {"text": "juchhei, juchhei un hopsasa!", "tokens": ["juch\u00b7hei", ",", "juch\u00b7hei", "un", "hop\u00b7sa\u00b7sa", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PTKVZ", "$,", "FM", "FM", "FM", "$."], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.8": {"text": "un heidilditschen dallala!", "tokens": ["un", "hei\u00b7dil\u00b7dit\u00b7schen", "dal\u00b7la\u00b7la", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Wat plirt ji all so hen nah ehr,", "tokens": ["Wat", "plirt", "ji", "all", "so", "hen", "nah", "ehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "PIAT", "ADV", "VVFIN", "ADJD", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "wat kl\u0153tert ji dor all", "tokens": ["wat", "kl\u0153tert", "ji", "dor", "all"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "un t\u00fcschelt hen un t\u00fcschelt her \u2013", "tokens": ["un", "t\u00fc\u00b7schelt", "hen", "un", "t\u00fc\u00b7schelt", "her", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "VVFIN", "PTKVZ", "$("], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "ach, Dirns, w\u00e4st doch nich mall!", "tokens": ["ach", ",", "Dirns", ",", "w\u00e4st", "doch", "nich", "mall", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["XY", "$,", "NN", "$,", "VVFIN", "ADV", "PTKNEG", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wenn't klappert, gaht man hen to Haw'!", "tokens": ["Wenn't", "klap\u00b7pert", ",", "gaht", "man", "hen", "to", "Ha\u00b7w'", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "VVFIN", "PIS", "VVFIN", "NE", "NE", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Karlin de sitt denn achter'n Aw \u2013", "tokens": ["Kar\u00b7lin", "de", "sitt", "denn", "ach\u00b7ter'n", "Aw", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "ADV", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "juchhei, juchhei un hopsasa!", "tokens": ["juch\u00b7hei", ",", "juch\u00b7hei", "un", "hop\u00b7sa\u00b7sa", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PTKVZ", "$,", "FM", "FM", "FM", "$."], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.8": {"text": "un heidilditschen dallala!", "tokens": ["un", "hei\u00b7dil\u00b7dit\u00b7schen", "dal\u00b7la\u00b7la", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Un wenn ji nahst jug Kawels hackt,", "tokens": ["Un", "wenn", "ji", "nahst", "jug", "Ka\u00b7wels", "hackt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOUS", "PPER", "VVFIN", "PIAT", "NN", "VVFIN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "in't Fack staht, Me\u00df upslaht,", "tokens": ["in't", "Fack", "staht", ",", "Me\u00df", "ups\u00b7laht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$,", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "liggt in ehr Armstohl se un snackt", "tokens": ["liggt", "in", "ehr", "Arm\u00b7stohl", "se", "un", "snackt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "NN", "FM", "FM", "FM", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "mit K\u00f6ster un Kandat;", "tokens": ["mit", "K\u00f6s\u00b7ter", "un", "Kan\u00b7dat", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "FM", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Musch\u00fcken stippt s' t\u00fcm Koffee in,", "tokens": ["Mu\u00b7sch\u00fc\u00b7ken", "stippt", "s'", "t\u00fcm", "Kof\u00b7fee", "in", ","], "token_info": ["wo