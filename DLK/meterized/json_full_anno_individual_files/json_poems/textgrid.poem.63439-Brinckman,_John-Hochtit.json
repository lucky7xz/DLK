{"textgrid.poem.63439": {"metadata": {"author": {"name": "Brinckman, John", "birth": "N.A.", "death": "N.A."}, "title": "Hochtit", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.85", "ro:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Fleudus un Fiedel un, juchhei!", "tokens": ["Fleu\u00b7dus", "un", "Fie\u00b7del", "un", ",", "juch\u00b7hei", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "FM", "FM", "FM", "$,", "ITJ", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Klarnett, juchhei! un Ba\u00df \u2013", "tokens": ["Klar\u00b7nett", ",", "juch\u00b7hei", "!", "un", "Ba\u00df", "\u2013"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ITJ", "$.", "FM", "FM", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "dat geiht f\u00f6r dull h\u00fct, heidildei!", "tokens": ["dat", "geiht", "f\u00f6r", "dull", "h\u00fct", ",", "hei\u00b7dil\u00b7dei", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,", "PTKVZ", "$."], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.4": {"text": "Un vull is Kros un Glas.", "tokens": ["Un", "vull", "is", "Kros", "un", "Glas", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "FM", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Schultvader de wull wedder fri'n,", "tokens": ["Schult\u00b7va\u00b7der", "de", "wull", "wed\u00b7der", "fri'n", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.6": {"text": "de frigt den W\u00e4wer sin Karlin \u2013", "tokens": ["de", "frigt", "den", "W\u00