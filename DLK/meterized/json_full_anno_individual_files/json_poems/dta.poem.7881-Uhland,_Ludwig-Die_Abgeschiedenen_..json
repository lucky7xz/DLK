{"dta.poem.7881": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Die Abgeschiedenen .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.99"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "So hab\u2019 ich endlich dich gerettet", "tokens": ["So", "hab'", "ich", "end\u00b7lich", "dich", "ge\u00b7ret\u00b7tet"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "PPER", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mir aus der Menge wilden Reihn!", "tokens": ["Mir", "aus", "der", "Men\u00b7ge", "wil\u00b7den", "Reihn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du bist in meinen Arm gekettet,", "tokens": ["Du", "bist", "in", "mei\u00b7nen", "Arm", "ge\u00b7ket\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Du bist nun mein, nun einzig mein.", "tokens": ["Du", "bist", "nun", "mein", ",", "nun", "ein\u00b7zig", "mein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PPOSAT", "$,", "ADV", "ADJD", "PPOSAT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Es schlummert Alles diese Stunde,", "tokens": ["Es", "schlum\u00b7mert", "Al\u00b7les", "die\u00b7se", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Nur wir noch leben auf der Welt;", "tokens": ["Nur", "wir", "noch", "le\u00b7ben", "auf", "der", "Welt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ADV", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wie in der Wasser stillem Grunde", "tokens": ["Wie", "in", "der", "Was\u00b7ser", "stil\u00b7lem", "Grun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Der Meergott seine G\u00f6ttin h\u00e4lt.", "tokens": ["Der", "Meer\u00b7gott", "sei\u00b7ne", "G\u00f6t\u00b7tin", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Verrauscht ist all das rohe Tosen,", "tokens": ["Ver\u00b7rauscht", "ist", "all", "das", "ro\u00b7he", "To\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PIAT", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das deine Worte mir verschlang;", "tokens": ["Das", "dei\u00b7ne", "Wor\u00b7te", "mir", "ver\u00b7schlang", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dein leises, liebevolles Kosen", "tokens": ["Dein", "lei\u00b7ses", ",", "lie\u00b7be\u00b7vol\u00b7les", "Ko\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ist nun mein einz\u2019ger, s\u00fc\u00dfer Klang.", "tokens": ["Ist", "nun", "mein", "einz'\u00b7ger", ",", "s\u00fc\u00b7\u00dfer", "Klang", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPOSAT", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Erde liegt in Nacht geh\u00fcllet,", "tokens": ["Die", "Er\u00b7de", "liegt", "in", "Nacht", "ge\u00b7h\u00fcl\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Kein Licht ergl\u00e4nzt auf Flur und Teich;", "tokens": ["Kein", "Licht", "er\u00b7gl\u00e4nzt", "auf", "Flur", "und", "Teich", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVPP", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Nur dieser Lampe Schimmer f\u00fcllet", "tokens": ["Nur", "die\u00b7ser", "Lam\u00b7pe", "Schim\u00b7mer", "f\u00fcl\u00b7let"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PDAT", "NN", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Noch unsrer Liebe kleines Reich.", "tokens": ["Noch", "uns\u00b7rer", "Lie\u00b7be", "klei\u00b7nes", "Reich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}