{"dta.poem.8073": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Von den sieben Zechbr\u00fcdern .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.99"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "Ich kenne sieben lust\u2019ge Br\u00fcder,", "tokens": ["Ich", "ken\u00b7ne", "sie\u00b7ben", "lust'\u00b7ge", "Br\u00fc\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "CARD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sie sind die durstigsten im Ort,", "tokens": ["Sie", "sind", "die", "durs\u00b7tigs\u00b7ten", "im", "Ort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "APPRART", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Die schwuren h\u00f6chlich, niemals wieder", "tokens": ["Die", "schwu\u00b7ren", "h\u00f6ch\u00b7lich", ",", "nie\u00b7mals", "wie\u00b7der"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PDS", "VVFIN", "ADJD", "$,", "ADV", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zu nennen ein gewisses Wort,", "tokens": ["Zu", "nen\u00b7nen", "ein", "ge\u00b7wis\u00b7ses", "Wort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "In keinerlei Weise,", "tokens": ["In", "kei\u00b7ner\u00b7lei", "Wei\u00b7se", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Nicht laut und nicht leise.", "tokens": ["Nicht", "laut", "und", "nicht", "lei\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "KON", "PTKNEG", "ADJD", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Es ist das gute W\u00f6rtlein: ", "tokens": ["Es", "ist", "das", "gu\u00b7te", "W\u00f6rt\u00b7lein", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Darin doch sonst kein Arges steckt.", "tokens": ["Da\u00b7rin", "doch", "sonst", "kein", "Ar\u00b7ges", "steckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "ADV", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie k\u00f6mmt\u2019s nun, da\u00df die wilden Prasser", "tokens": ["Wie", "k\u00f6mmt's", "nun", ",", "da\u00df", "die", "wil\u00b7den", "Pras\u00b7ser"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "NE", "ADV", "$,", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dies schlichte Wort so m\u00e4chtig schreckt?", "tokens": ["Dies", "schlich\u00b7te", "Wort", "so", "m\u00e4ch\u00b7tig", "schreckt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "NN", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Merkt auf! ich berichte", "tokens": ["Merkt", "auf", "!", "ich", "be\u00b7rich\u00b7te"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["VVIMP", "PTKVZ", "$.", "PPER", "VVFIN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Die Wundergeschichte.", "tokens": ["Die", "Wun\u00b7der\u00b7ge\u00b7schich\u00b7te", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "Einst h\u00f6rten jene durst\u2019gen Sieben", "tokens": ["Einst", "h\u00f6r\u00b7ten", "je\u00b7ne", "dur\u00b7st'\u00b7gen", "Sie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PDAT", "ADJA", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Von einem fremden Zechkumpan,", "tokens": ["Von", "ei\u00b7nem", "frem\u00b7den", "Zech\u00b7kum\u00b7pan", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es sey am Waldgebirge dr\u00fcben", "tokens": ["Es", "sey", "am", "Wald\u00b7ge\u00b7bir\u00b7ge", "dr\u00fc\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPRART", "NN", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein neues Wirthshaus aufgethan,", "tokens": ["Ein", "neu\u00b7es", "Wirths\u00b7haus", "auf\u00b7ge\u00b7than", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da flie\u00dfen so reine,", "tokens": ["Da", "flie\u00b7\u00dfen", "so", "rei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ADJA", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "So w\u00fcrzige Weine.", "tokens": ["So", "w\u00fcr\u00b7zi\u00b7ge", "Wei\u00b7ne", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.4": {"line.1": {"text": "Um einer guten Predigt willen", "tokens": ["Um", "ei\u00b7ner", "gu\u00b7ten", "Pre\u00b7digt", "wil\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUI", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "H\u00e4tt\u2019 Keiner sich vom Platz bewegt,", "tokens": ["H\u00e4tt'", "Kei\u00b7ner", "sich", "vom", "Platz", "be\u00b7wegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PRF", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch gilt es, Gl\u00e4ser gut zu f\u00fcllen,", "tokens": ["Doch", "gilt", "es", ",", "Gl\u00e4\u00b7ser", "gut", "zu", "f\u00fcl\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "NN", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dann sind die Bursche gleich erregt.", "tokens": ["Dann", "sind", "die", "Bur\u00b7sche", "gleich", "er\u00b7regt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "\u201eauf, lasset uns wandern!\u201c", "tokens": ["\u201e", "auf", ",", "las\u00b7set", "uns", "wan\u00b7dern", "!", "\u201c"], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PTKVZ", "$,", "VVFIN", "PPER", "VVFIN", "$.", "$("], "meter": "++--+-", "measure": "trochaic.tri.relaxed"}, "line.6": {"text": "Ruft Einer dem Andern.", "tokens": ["Ruft", "Ei\u00b7ner", "dem", "An\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ART", "ADJA", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.5": {"line.1": {"text": "Sie wandern r\u00fcstig mit dem Fr\u00fchen,", "tokens": ["Sie", "wan\u00b7dern", "r\u00fcs\u00b7tig", "mit", "dem", "Fr\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Bald steigt die Sonne dr\u00fcckend hei\u00df;", "tokens": ["Bald", "steigt", "die", "Son\u00b7ne", "dr\u00fc\u00b7ckend", "hei\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "VVPP", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Zunge lechzt, die Lippen gl\u00fchen", "tokens": ["Die", "Zun\u00b7ge", "lechzt", ",", "die", "Lip\u00b7pen", "gl\u00fc\u00b7hen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und von der Stirne rinnt der Schwei\u00df:", "tokens": ["Und", "von", "der", "Stir\u00b7ne", "rinnt", "der", "Schwei\u00df", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da rieselt so helle", "tokens": ["Da", "rie\u00b7selt", "so", "hel\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "ADJA"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Vom Felsen die Quelle.", "tokens": ["Vom", "Fel\u00b7sen", "die", "Quel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.6": {"line.1": {"text": "Wie trinken sie in vollen Z\u00fcgen!", "tokens": ["Wie", "trin\u00b7ken", "sie", "in", "vol\u00b7len", "Z\u00fc\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Doch als sie kaum den Durst gestillt,", "tokens": ["Doch", "als", "sie", "kaum", "den", "Durst", "ge\u00b7stillt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bezeugen sie ihr Mi\u00dfvergn\u00fcgen,", "tokens": ["Be\u00b7zeu\u00b7gen", "sie", "ihr", "Mi\u00df\u00b7ver\u00b7gn\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df hier nicht Wein, nur Wasser, quillt:", "tokens": ["Da\u00df", "hier", "nicht", "Wein", ",", "nur", "Was\u00b7ser", ",", "quillt", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "ADV", "PTKNEG", "NN", "$,", "ADV", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "\u201eo fades Getr\u00e4nke!", "tokens": ["\u201e", "o", "fa\u00b7des", "Ge\u00b7tr\u00e4n\u00b7ke", "!"], "token_info": ["punct", "word", "word", "word", "punct"], "pos": ["$(", "FM", "ADJA", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "O \u00e4rmliche Schwenke!\u201c", "tokens": ["O", "\u00e4rm\u00b7li\u00b7che", "Schwen\u00b7ke", "!", "\u201c"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["NE", "ADJA", "NN", "$.", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.7": {"line.1": {"text": "In seine vielverwobnen G\u00e4nge", "tokens": ["In", "sei\u00b7ne", "viel\u00b7ver\u00b7wob\u00b7nen", "G\u00e4n\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nimmt jetzt der Wald die Pilger auf,", "tokens": ["Nimmt", "jetzt", "der", "Wald", "die", "Pil\u00b7ger", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da stehn sie pl\u00f6tzlich im Gedr\u00e4nge,", "tokens": ["Da", "stehn", "sie", "pl\u00f6tz\u00b7lich", "im", "Ge\u00b7dr\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Verworrnes Dickicht hemmt den Lauf;", "tokens": ["Ver\u00b7worr\u00b7nes", "Di\u00b7ckicht", "hemmt", "den", "Lauf", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sie irren, sie suchen,", "tokens": ["Sie", "ir\u00b7ren", ",", "sie", "su\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Sie zanken und fluchen.", "tokens": ["Sie", "zan\u00b7ken", "und", "flu\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "KON", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.8": {"line.1": {"text": "Derweil hat sich in finstre Wetter", "tokens": ["Der\u00b7weil", "hat", "sich", "in", "finst\u00b7re", "Wet\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PRF", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die schw\u00fcle Sonne tief verh\u00fcllt,", "tokens": ["Die", "schw\u00fc\u00b7le", "Son\u00b7ne", "tief", "ver\u00b7h\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Schon rauscht der Regen durch die Bl\u00e4tter,", "tokens": ["Schon", "rauscht", "der", "Re\u00b7gen", "durch", "die", "Bl\u00e4t\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Es zuckt der Blitz, der Donner br\u00fcllt,", "tokens": ["Es", "zuckt", "der", "Blitz", ",", "der", "Don\u00b7ner", "br\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dann k\u00f6mmt es geflossen,", "tokens": ["Dann", "k\u00f6mmt", "es", "ge\u00b7flos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVPP", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Unendlich ergossen.", "tokens": ["Un\u00b7end\u00b7lich", "er\u00b7gos\u00b7sen", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVPP", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.9": {"line.1": {"text": "Bald wird der Forst zu tausend Inseln,", "tokens": ["Bald", "wird", "der", "Forst", "zu", "tau\u00b7send", "In\u00b7seln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "APPR", "CARD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zahllose Str\u00f6me brechen vor;", "tokens": ["Zahl\u00b7lo\u00b7se", "Str\u00f6\u00b7me", "bre\u00b7chen", "vor", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PTKVZ", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Hier hilft kein Toben, hilft kein Winseln,", "tokens": ["Hier", "hilft", "kein", "To\u00b7ben", ",", "hilft", "kein", "Win\u00b7seln", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "$,", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Er mu\u00df hindurch, der edle Chor.", "tokens": ["Er", "mu\u00df", "hin\u00b7durch", ",", "der", "ed\u00b7le", "Chor", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PAV", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "O gr\u00fcndliche Taufe!", "tokens": ["O", "gr\u00fcnd\u00b7li\u00b7che", "Tau\u00b7fe", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "O k\u00f6stliche Traufe!", "tokens": ["O", "k\u00f6st\u00b7li\u00b7che", "Trau\u00b7fe", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.10": {"line.1": {"text": "Vor Alters wurden Menschenkinder", "tokens": ["Vor", "Al\u00b7ters", "wur\u00b7den", "Men\u00b7schen\u00b7kin\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "VAFIN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Verwandelt oft in Quell und Flu\u00df,", "tokens": ["Ver\u00b7wan\u00b7delt", "oft", "in", "Quell", "und", "Flu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Auch unsre sieben arme S\u00fcnder", "tokens": ["Auch", "uns\u00b7re", "sie\u00b7ben", "ar\u00b7me", "S\u00fcn\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "CARD", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Bedroht ein gleicher G\u00f6tterschlu\u00df.", "tokens": ["Be\u00b7droht", "ein", "glei\u00b7cher", "G\u00f6t\u00b7ter\u00b7schlu\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sie triefen, sie schwellen,", "tokens": ["Sie", "trie\u00b7fen", ",", "sie", "schwel\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Als w\u00fcrden sie Quellen.", "tokens": ["Als", "w\u00fcr\u00b7den", "sie", "Quel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPER", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.11": {"line.1": {"text": "So, mehr geschwommen, als gegangen,", "tokens": ["So", ",", "mehr", "ge\u00b7schwom\u00b7men", ",", "als", "ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "VVPP", "$,", "KOUS", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Gelangen sie zum Wald hinaus;", "tokens": ["Ge\u00b7lan\u00b7gen", "sie", "zum", "Wald", "hin\u00b7aus", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch keine Schenke sehn sie prangen,", "tokens": ["Doch", "kei\u00b7ne", "Schen\u00b7ke", "sehn", "sie", "pran\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sie sind auf gradem Weg nach Haus;", "tokens": ["Sie", "sind", "auf", "gra\u00b7dem", "Weg", "nach", "Haus", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ADJA", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Schon rieselt so helle", "tokens": ["Schon", "rie\u00b7selt", "so", "hel\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "ADJA"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Vom Felsen die Quelle.", "tokens": ["Vom", "Fel\u00b7sen", "die", "Quel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.12": {"line.1": {"text": "Da ist\u2019s, als ob sie rauschend spreche:", "tokens": ["Da", "ist's", ",", "als", "ob", "sie", "rau\u00b7schend", "spre\u00b7che", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "$,", "KOKOM", "KOUS", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "\u201ewillkommen, saubre Br\u00fcderschaar!", "tokens": ["\u201e", "will\u00b7kom\u00b7men", ",", "saub\u00b7re", "Br\u00fc\u00b7der\u00b7schaar", "!"], "token_info": ["punct", "word", "punct", "word", "word", "punct"], "pos": ["$(", "ADJD", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ihr habt geschm\u00e4het, th\u00f6richt Freche!", "tokens": ["Ihr", "habt", "ge\u00b7schm\u00e4\u00b7het", ",", "th\u00f6\u00b7richt", "Fre\u00b7che", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$,", "ADJD", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mein Wasser, das euch labend war.", "tokens": ["Mein", "Was\u00b7ser", ",", "das", "euch", "la\u00b7bend", "war", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "PPER", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Nun seyd ihr getr\u00e4nket,", "tokens": ["Nun", "seyd", "ihr", "ge\u00b7tr\u00e4n\u00b7ket", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Da\u00df ihr daran denket.\u201c", "tokens": ["Da\u00df", "ihr", "da\u00b7ran", "den\u00b7ket", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "PAV", "VVFIN", "$.", "$("], "meter": "-+-+--", "measure": "unknown.measure.di"}}, "stanza.13": {"line.1": {"text": "So kam es, da\u00df die sieben Br\u00fcder", "tokens": ["So", "kam", "es", ",", "da\u00df", "die", "sie\u00b7ben", "Br\u00fc\u00b7der"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "ART", "CARD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das Wasser f\u00fcrchteten hinfort,", "tokens": ["Das", "Was\u00b7ser", "f\u00fcrch\u00b7te\u00b7ten", "hin\u00b7fort", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und da\u00df sie schwuren, niemals wieder", "tokens": ["Und", "da\u00df", "sie", "schwu\u00b7ren", ",", "nie\u00b7mals", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "$,", "ADV", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zu nennen das verw\u00fcnschte Wort,", "tokens": ["Zu", "nen\u00b7nen", "das", "ver\u00b7w\u00fcnschte", "Wort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "In keinerlei Weise,", "tokens": ["In", "kei\u00b7ner\u00b7lei", "Wei\u00b7se", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Nicht laut und nicht leise.", "tokens": ["Nicht", "laut", "und", "nicht", "lei\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "KON", "PTKNEG", "ADJD", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}}}}