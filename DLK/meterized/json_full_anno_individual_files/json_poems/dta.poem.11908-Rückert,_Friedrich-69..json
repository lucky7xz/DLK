{"dta.poem.11908": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "69.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn du den Augen h\u00e4ltst das Buch so nahe vor,", "tokens": ["Wenn", "du", "den", "Au\u00b7gen", "h\u00e4ltst", "das", "Buch", "so", "na\u00b7he", "vor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "ART", "NN", "ADV", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Schwimmt die verwirrte Schrift in einem D\u00e4mmerflor.", "tokens": ["Schwimmt", "die", "ver\u00b7wirr\u00b7te", "Schrift", "in", "ei\u00b7nem", "D\u00e4m\u00b7mer\u00b7flor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Und wieder wenn du h\u00e4ltst den Augen es so fern,", "tokens": ["Und", "wie\u00b7der", "wenn", "du", "h\u00e4ltst", "den", "Au\u00b7gen", "es", "so", "fern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "KOUS", "PPER", "VVFIN", "ART", "NN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wird jeder Buchstab ein unklarer Nebelstern.", "tokens": ["Wird", "je\u00b7der", "Buch\u00b7stab", "ein", "un\u00b7kla\u00b7rer", "Ne\u00b7bels\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}}, "stanza.3": {"line.1": {"text": "Und unzufrieden wirst du leicht mit deinem Auge,", "tokens": ["Und", "un\u00b7zu\u00b7frie\u00b7den", "wirst", "du", "leicht", "mit", "dei\u00b7nem", "Au\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "PPER", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df weder fern noch nah es recht zu sehen tauge.", "tokens": ["Da\u00df", "we\u00b7der", "fern", "noch", "nah", "es", "recht", "zu", "se\u00b7hen", "tau\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KON", "ADJD", "ADV", "ADJD", "PPER", "ADJD", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Doch halte nicht zu nah und nicht zu fern das Buch,", "tokens": ["Doch", "hal\u00b7te", "nicht", "zu", "nah", "und", "nicht", "zu", "fern", "das", "Buch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "PTKA", "ADJD", "KON", "PTKNEG", "PTKA", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und leserlich nach Wunsch erscheint dir jeder Spruch.", "tokens": ["Und", "le\u00b7ser\u00b7lich", "nach", "Wunsch", "er\u00b7scheint", "dir", "je\u00b7der", "Spruch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "NN", "VVFIN", "PPER", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Nur zwischen deinem Ziel und dir mu\u00dft du dem Licht", "tokens": ["Nur", "zwi\u00b7schen", "dei\u00b7nem", "Ziel", "und", "dir", "mu\u00dft", "du", "dem", "Licht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "KON", "PPER", "VMFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Raum lassen grad soviel, als taugt f\u00fcr dein Gesicht.", "tokens": ["Raum", "las\u00b7sen", "grad", "so\u00b7viel", ",", "als", "taugt", "f\u00fcr", "dein", "Ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "PIS", "$,", "KOKOM", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Und also siehst du auch die Welt und die Natur", "tokens": ["Und", "al\u00b7so", "siehst", "du", "auch", "die", "Welt", "und", "die", "Na\u00b7tur"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In rechter Deutlichkeit bei rechtem Abstand nur;", "tokens": ["In", "rech\u00b7ter", "Deut\u00b7lich\u00b7keit", "bei", "rech\u00b7tem", "Ab\u00b7stand", "nur", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Wenn zwischen ihr und dir du l\u00e4ssest eine Weite,", "tokens": ["Wenn", "zwi\u00b7schen", "ihr", "und", "dir", "du", "l\u00e4s\u00b7sest", "ei\u00b7ne", "Wei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PPER", "KON", "PPER", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df klar im Zwischenraum sich Gottes Licht verbreite.", "tokens": ["Da\u00df", "klar", "im", "Zwi\u00b7schen\u00b7raum", "sich", "Got\u00b7tes", "Licht", "ver\u00b7brei\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "APPRART", "NN", "PRF", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Die Weite doch ist gleich f\u00fcr jedes Auge nicht,", "tokens": ["Die", "Wei\u00b7te", "doch", "ist", "gleich", "f\u00fcr", "je\u00b7des", "Au\u00b7ge", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VAFIN", "ADV", "APPR", "PIAT", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie ihm beschieden ist Fern- oder Nahgesicht.", "tokens": ["Wie", "ihm", "be\u00b7schie\u00b7den", "ist", "Fern", "o\u00b7der", "Nah\u00b7ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVPP", "VAFIN", "TRUNC", "KON", "NN", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.9": {"line.1": {"text": "Die Weite wechselt selbst mit jeder Lebenszeit,", "tokens": ["Die", "Wei\u00b7te", "wech\u00b7selt", "selbst", "mit", "je\u00b7der", "Le\u00b7bens\u00b7zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie eben wechseln mag Fern- und Nahsichtigkeit.", "tokens": ["Wie", "e\u00b7ben", "wech\u00b7seln", "mag", "Fern", "und", "Nah\u00b7sich\u00b7tig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VVINF", "VMFIN", "TRUNC", "KON", "NN", "$."], "meter": "-+-+--+----+", "measure": "iambic.tetra.relaxed"}}, "stanza.10": {"line.1": {"text": "Das wechsle nun wie's mag, wenn du nur nicht erblindest,", "tokens": ["Das", "wechs\u00b7le", "nun", "wie's", "mag", ",", "wenn", "du", "nur", "nicht", "er\u00b7blin\u00b7dest", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ADV", "VMFIN", "$,", "KOUS", "PPER", "ADV", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Noch in Verblendung dir die Augen selbst verbindest.", "tokens": ["Noch", "in", "Ver\u00b7blen\u00b7dung", "dir", "die", "Au\u00b7gen", "selbst", "ver\u00b7bin\u00b7dest", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "PPER", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.11": {"line.1": {"text": "Gebrauch dein Auge nur, wie es ist Gottes Wille", "tokens": ["Ge\u00b7brauch", "dein", "Au\u00b7ge", "nur", ",", "wie", "es", "ist", "Got\u00b7tes", "Wil\u00b7le"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "PPOSAT", "NN", "ADV", "$,", "PWAV", "PPER", "VAFIN", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und der Natur, und nie bed\u00fcrfe mir der Brille!", "tokens": ["Und", "der", "Na\u00b7tur", ",", "und", "nie", "be\u00b7d\u00fcr\u00b7fe", "mir", "der", "Bril\u00b7le", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "KON", "ADV", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}