{"dta.poem.1044": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "Die 3. Antistrophe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1618", "urn": "urn:nbn:de:kobv:b4-20628-9", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Hjezwischen gienge mit verlangen", "tokens": ["Hjez\u00b7wi\u00b7schen", "gien\u00b7ge", "mit", "ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPR", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die gantze Churf\u00fcrstliche stat/", "tokens": ["Die", "gant\u00b7ze", "Chur\u00b7f\u00fcrst\u00b7li\u00b7che", "stat", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Da der Neckar den anflu\u00df hat", "tokens": ["Da", "der", "Ne\u00b7ckar", "den", "an\u00b7flu\u00df", "hat"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ART", "NN", "VAFIN"], "meter": "--++-+-+", "measure": "anapaest.init"}, "line.4": {"text": "Jhr liebe F\u00fcrstin zuempfangen;", "tokens": ["Ihr", "lie\u00b7be", "F\u00fcrs\u00b7tin", "zu\u00b7em\u00b7pfan\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Welche mit grossem pomp vnd pracht", "tokens": ["Wel\u00b7che", "mit", "gros\u00b7sem", "pomp", "vnd", "pracht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAT", "APPR", "ADJA", "NN", "KON", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Vnder vilen Fr\u00e4wlin gefahren", "tokens": ["Vn\u00b7der", "vi\u00b7len", "Fr\u00e4w\u00b7lin", "ge\u00b7fah\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "VVPP"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.7": {"text": "Leuchtet/ wie in Nymphischen scharen", "tokens": ["Leuch\u00b7tet", "/", "wie", "in", "Nym\u00b7phi\u00b7schen", "scha\u00b7ren"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$(", "KOKOM", "APPR", "NN", "VVINF"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.8": {"text": "Diana in der hellen macht:", "tokens": ["Di\u00b7a\u00b7na", "in", "der", "hel\u00b7len", "macht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "ADJA", "VVFIN", "$."], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.9": {"text": "Von perlen vnd edelgesteinen", "tokens": ["Von", "per\u00b7len", "vnd", "e\u00b7del\u00b7ge\u00b7stei\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "VVINF", "KON", "ADJA"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.10": {"text": "Jhr haubt vnd jhre klaider scheinen/", "tokens": ["Ihr", "haubt", "vnd", "jhre", "klai\u00b7der", "schei\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "PPOSAT", "ADJA", "VVFIN", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.11": {"text": "Doch gl\u00e4ntzet jhre sch\u00f6nheit mehr/", "tokens": ["Doch", "gl\u00e4nt\u00b7zet", "jhre", "sch\u00f6n\u00b7heit", "mehr", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "ADV", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.12": {"text": "Vnd ist sie selbs jhr gr\u00f6ste ehr.", "tokens": ["Vnd", "ist", "sie", "selbs", "jhr", "gr\u00f6s\u00b7te", "ehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Gleich wie auch jhr liebliche Jugent", "tokens": ["Gleich", "wie", "auch", "jhr", "lieb\u00b7li\u00b7che", "Ju\u00b7gent"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "ADV", "PPOSAT", "ADJA", "NN"], "meter": "+---+--+-", "measure": "trochaic.tri.relaxed"}, "line.14": {"text": "Schimmert in einem guldin stuck:", "tokens": ["Schim\u00b7mert", "in", "ei\u00b7nem", "gul\u00b7din", "stuck", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.15": {"text": "Also Ehr/ Gotsforcht/ Zucht vnd Tugent", "tokens": ["Al\u00b7so", "Ehr", "/", "Gots\u00b7forcht", "/", "Zucht", "vnd", "Tu\u00b7gent"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ADV", "NN", "$(", "NN", "$(", "NN", "KON", "NN"], "meter": "---+-+-+-", "measure": "unknown.measure.tri"}, "line.16": {"text": "Seind jhrer sehlen wahrer schmuck.", "tokens": ["Seind", "jhrer", "seh\u00b7len", "wah\u00b7rer", "schmuck", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "ADV", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}