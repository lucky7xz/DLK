{"dta.poem.14245": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "An die Entfernte .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Als noch Lieb' mit mir im Bunde,", "tokens": ["Als", "noch", "Lieb'", "mit", "mir", "im", "Bun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "NN", "APPR", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hatt' ich Ruhe keine Stunde;", "tokens": ["Hatt'", "ich", "Ru\u00b7he", "kei\u00b7ne", "Stun\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "NN", "PIAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn im Schlo\u00df noch alle schliefen,", "tokens": ["Wenn", "im", "Schlo\u00df", "noch", "al\u00b7le", "schlie\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPRART", "NN", "ADV", "PIS", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "War's, als ob s\u00fc\u00df' Stimmen riefen,", "tokens": ["Wa\u00b7r's", ",", "als", "ob", "s\u00fc\u00df'", "Stim\u00b7men", "rie\u00b7fen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "KOKOM", "KOUS", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "T\u00f6nend bis zum Herzensgrunde:", "tokens": ["T\u00f6\u00b7nend", "bis", "zum", "Her\u00b7zens\u00b7grun\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "\u201eauf! schon gold'ne Strahlen dringen,", "tokens": ["\u201e", "auf", "!", "schon", "gold'\u00b7ne", "Strah\u00b7len", "drin\u00b7gen", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PTKVZ", "$.", "ADV", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Heiter funkeln Wald und Garten,", "tokens": ["Hei\u00b7ter", "fun\u00b7keln", "Wald", "und", "Gar\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Neu erquickt die V\u00f6gel singen,", "tokens": ["Neu", "er\u00b7quickt", "die", "V\u00f6\u00b7gel", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "L\u00e4\u00dft Du so Dein Liebchen warten?\u201c", "tokens": ["L\u00e4\u00dft", "Du", "so", "Dein", "Lieb\u00b7chen", "war\u00b7ten", "?", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PPOSAT", "NN", "VVINF", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Und vom Lager mu\u00dft' ich springen.", "tokens": ["Und", "vom", "La\u00b7ger", "mu\u00dft'", "ich", "sprin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}