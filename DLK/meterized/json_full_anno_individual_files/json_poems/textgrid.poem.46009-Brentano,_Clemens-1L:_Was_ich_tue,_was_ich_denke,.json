{"textgrid.poem.46009": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "1L: Was ich tue, was ich denke,", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was ich tue, was ich denke,", "tokens": ["Was", "ich", "tue", ",", "was", "ich", "den\u00b7ke", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Alles, was mit mir geschieht,", "tokens": ["Al\u00b7les", ",", "was", "mit", "mir", "ge\u00b7schieht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "PRELS", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Herr nach deinem Auge lenke", "tokens": ["Herr", "nach", "dei\u00b7nem", "Au\u00b7ge", "len\u00b7ke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Das auf meine Wege sieht.", "tokens": ["Das", "auf", "mei\u00b7ne", "We\u00b7ge", "sieht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Herr, o wolle auf mein Flehen,", "tokens": ["Herr", ",", "o", "wol\u00b7le", "auf", "mein", "Fle\u00b7hen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "FM", "VMFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn mein Blut zu Tage springt,", "tokens": ["Wenn", "mein", "Blut", "zu", "Ta\u00b7ge", "springt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Heut mich wie ein Kind ansehen", "tokens": ["Heut", "mich", "wie", "ein", "Kind", "an\u00b7se\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "KOKOM", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Das sein erstes Opfer bringt.", "tokens": ["Das", "sein", "ers\u00b7tes", "Op\u00b7fer", "bringt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Unter scharfen Marterruten,", "tokens": ["Un\u00b7ter", "schar\u00b7fen", "Mar\u00b7ter\u00b7ru\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Unter eines Richters Schwert,", "tokens": ["Un\u00b7ter", "ei\u00b7nes", "Rich\u00b7ters", "Schwert", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "M\u00f6cht' ich dir mein Heiland bluten,", "tokens": ["M\u00f6cht'", "ich", "dir", "mein", "Hei\u00b7land", "blu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "W\u00e4r' ich deiner Kronen wert.", "tokens": ["W\u00e4r'", "ich", "dei\u00b7ner", "Kro\u00b7nen", "wert", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Aber, da mir nicht verg\u00f6nnet", "tokens": ["A\u00b7ber", ",", "da", "mir", "nicht", "ver\u00b7g\u00f6n\u00b7net"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["KON", "$,", "KOUS", "PPER", "PTKNEG", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Solch ein heilender Ergu\u00df,", "tokens": ["Solch", "ein", "hei\u00b7len\u00b7der", "Er\u00b7gu\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Geb' ich, weil die F\u00fclle brennet,", "tokens": ["Geb'", "ich", ",", "weil", "die", "F\u00fcl\u00b7le", "bren\u00b7net", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ach! nichts, als den \u00dcberflu\u00df.", "tokens": ["Ach", "!", "nichts", ",", "als", "den", "\u00dc\u00b7berf\u00b7lu\u00df", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PIS", "$,", "KOUS", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Alles doch hast du gegeben,", "tokens": ["Al\u00b7les", "doch", "hast", "du", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Gott der zu den Schmerzen kam,", "tokens": ["Gott", "der", "zu", "den", "Schmer\u00b7zen", "kam", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und im Blut hingab das Leben,", "tokens": ["Und", "im", "Blut", "hin\u00b7gab", "das", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df den Tod er von uns nahm.", "tokens": ["Da\u00df", "den", "Tod", "er", "von", "uns", "nahm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Meines flie\u00dft zu meinem Heile,", "tokens": ["Mei\u00b7nes", "flie\u00dft", "zu", "mei\u00b7nem", "Hei\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Flie\u00dft zu deinem Ruhme nicht", "tokens": ["Flie\u00dft", "zu", "dei\u00b7nem", "Ruh\u00b7me", "nicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "PTKNEG"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Herr mir deinen Schmerz mitteile,", "tokens": ["Herr", "mir", "dei\u00b7nen", "Schmerz", "mit\u00b7tei\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn der Stahl die Quelle bricht.", "tokens": ["Wenn", "der", "Stahl", "die", "Quel\u00b7le", "bricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Gieb, da\u00df deines Bluts ich denke,", "tokens": ["Gieb", ",", "da\u00df", "dei\u00b7nes", "Bluts", "ich", "den\u00b7ke", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "KOUS", "PPOSAT", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn das meine niederrinnt,", "tokens": ["Wenn", "das", "mei\u00b7ne", "nie\u00b7der\u00b7rinnt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "PPOSAT", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und in deine Wunden senke", "tokens": ["Und", "in", "dei\u00b7ne", "Wun\u00b7den", "sen\u00b7ke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dein ohnm\u00e4cht'ges schwaches Kind!", "tokens": ["Dein", "ohn\u00b7m\u00e4cht'\u00b7ges", "schwa\u00b7ches", "Kind", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "La\u00df, was b\u00f6s in meinen Sinnen,", "tokens": ["La\u00df", ",", "was", "b\u00f6s", "in", "mei\u00b7nen", "Sin\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "PRELS", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Alle hei\u00dfe Erdenglut,", "tokens": ["Al\u00b7le", "hei\u00b7\u00dfe", "Er\u00b7deng\u00b7lut", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Heut aus meinen Adern rinnen,", "tokens": ["Heut", "aus", "mei\u00b7nen", "A\u00b7dern", "rin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Morgen dann gieb mir ", "tokens": ["Mor\u00b7gen", "dann", "gieb", "mir"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ADV", "VVIMP", "PPER"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.9": {"line.1": {"text": "O wie hast du's gut bestellet,", "tokens": ["O", "wie", "hast", "du's", "gut", "be\u00b7stel\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "VAFIN", "PIS", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Meine Seele fa\u00dft es kaum,", "tokens": ["Mei\u00b7ne", "See\u00b7le", "fa\u00dft", "es", "kaum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df dein Blut sich mir gesellet,", "tokens": ["Da\u00df", "dein", "Blut", "sich", "mir", "ge\u00b7sel\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PRF", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Macht das meine heut ihm Raum.", "tokens": ["Macht", "das", "mei\u00b7ne", "heut", "ihm", "Raum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "PPOSAT", "ADV", "PPER", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "All dein Blut hast du vergossen", "tokens": ["All", "dein", "Blut", "hast", "du", "ver\u00b7gos\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "PPOSAT", "NN", "VAFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mir zu tilgen das Gericht,", "tokens": ["Mir", "zu", "til\u00b7gen", "das", "Ge\u00b7richt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und es ist f\u00fcr mich geflossen,", "tokens": ["Und", "es", "ist", "f\u00fcr", "mich", "ge\u00b7flos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "APPR", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Aber ich, ich nahm es nicht.", "tokens": ["A\u00b7ber", "ich", ",", "ich", "nahm", "es", "nicht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "PPER", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Hast auch deinen Leib gegeben,", "tokens": ["Hast", "auch", "dei\u00b7nen", "Leib", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "F\u00fcr mich in des Richters Zorn,", "tokens": ["F\u00fcr", "mich", "in", "des", "Rich\u00b7ters", "Zorn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ich zage f\u00fcr das Leben,", "tokens": ["Und", "ich", "za\u00b7ge", "f\u00fcr", "das", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Trag' um dich auch keinen Dorn.", "tokens": ["Trag'", "um", "dich", "auch", "kei\u00b7nen", "Dorn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "ADV", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Und ich wei\u00df doch, es giebt Seelen", "tokens": ["Und", "ich", "wei\u00df", "doch", ",", "es", "giebt", "See\u00b7len"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ADV", "$,", "PPER", "VVFIN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Brennend so in reiner Glut,", "tokens": ["Bren\u00b7nend", "so", "in", "rei\u00b7ner", "Glut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df sie deine Wunden z\u00e4hlen", "tokens": ["Da\u00df", "sie", "dei\u00b7ne", "Wun\u00b7den", "z\u00e4h\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "An sich selbst in Wunderflut.", "tokens": ["An", "sich", "selbst", "in", "Wun\u00b7der\u00b7flut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "ADV", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Ach weil ich nicht diesen gleiche", "tokens": ["Ach", "weil", "ich", "nicht", "die\u00b7sen", "glei\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "KOUS", "PPER", "PTKNEG", "PDAT", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist wohl b\u00f6ses Blut in mir,", "tokens": ["Ist", "wohl", "b\u00f6\u00b7ses", "Blut", "in", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJA", "NN", "APPR", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Gieb da\u00df alles es entweiche,", "tokens": ["Gieb", "da\u00df", "al\u00b7les", "es", "ent\u00b7wei\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "KOUS", "PIS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Jesus dann gefall' ich dir.", "tokens": ["Je\u00b7sus", "dann", "ge\u00b7fall'", "ich", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "VVFIN", "PPER", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "Und ersetz' es geistlich wieder", "tokens": ["Und", "er\u00b7setz'", "es", "geist\u00b7lich", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Morgen mir mit deinem Blut", "tokens": ["Mor\u00b7gen", "mir", "mit", "dei\u00b7nem", "Blut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Vor dir sink' ich rein dann nieder,", "tokens": ["Vor", "dir", "sink'", "ich", "rein", "dann", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PPER", "ADJD", "ADV", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wo die B\u00fc\u00dferin geruht.", "tokens": ["Wo", "die", "B\u00fc\u00b7\u00dfe\u00b7rin", "ge\u00b7ruht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.15": {"line.1": {"text": "Herr, du wei\u00dft ich wollt' bekennen,", "tokens": ["Herr", ",", "du", "wei\u00dft", "ich", "wollt'", "be\u00b7ken\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VVFIN", "PPER", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Was die Seele niederdr\u00fcckt", "tokens": ["Was", "die", "See\u00b7le", "nie\u00b7der\u00b7dr\u00fcckt"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "ART", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Felsen von dem Quell mich trennen", "tokens": ["Fel\u00b7sen", "von", "dem", "Quell", "mich", "tren\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wo die Bu\u00dfe Gnade pfl\u00fcckt.", "tokens": ["Wo", "die", "Bu\u00b7\u00dfe", "Gna\u00b7de", "pfl\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.16": {"line.1": {"text": "Ich hab' nicht den Zaun durchbrochen,", "tokens": ["Ich", "hab'", "nicht", "den", "Zaun", "durch\u00b7bro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ART", "NN", "VVPP", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Herr vergieb uns unsre Schuld,", "tokens": ["Herr", "ver\u00b7gieb", "uns", "uns\u00b7re", "Schuld", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "W\u00e4r' durch Dornen gern gekrochen", "tokens": ["W\u00e4r'", "durch", "Dor\u00b7nen", "gern", "ge\u00b7kro\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "NN", "ADV", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Heim in deiner Kirche Huld.", "tokens": ["Heim", "in", "dei\u00b7ner", "Kir\u00b7che", "Huld", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.17": {"line.1": {"text": "Und lie\u00df ich denn meine S\u00fcnden,", "tokens": ["Und", "lie\u00df", "ich", "denn", "mei\u00b7ne", "S\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Alle heut in meinem Blut,", "tokens": ["Al\u00b7le", "heut", "in", "mei\u00b7nem", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wolle mich in ihm entbinden,", "tokens": ["Wol\u00b7le", "mich", "in", "ihm", "ent\u00b7bin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "APPR", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie die Erd' in S\u00fcndenflut.", "tokens": ["Wie", "die", "Erd'", "in", "S\u00fcn\u00b7den\u00b7flut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.18": {"line.1": {"text": "Mit dem Blute wird verschuldet,", "tokens": ["Mit", "dem", "Blu\u00b7te", "wird", "ver\u00b7schul\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mit dem Blute wird vers\u00fchnt,", "tokens": ["Mit", "dem", "Blu\u00b7te", "wird", "ver\u00b7s\u00fchnt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Du Herr hast die Pein erduldet,", "tokens": ["Du", "Herr", "hast", "die", "Pein", "er\u00b7dul\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ich, ich habe sie verdient.", "tokens": ["Ich", ",", "ich", "ha\u00b7be", "sie", "ver\u00b7dient", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PPER", "VAFIN", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.19": {"line.1": {"text": "Und so komm' ich dann im Glauben", "tokens": ["Und", "so", "komm'", "ich", "dann", "im", "Glau\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADV", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Deines Blutes Gast zu sein,", "tokens": ["Dei\u00b7nes", "Blu\u00b7tes", "Gast", "zu", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "PTKZU", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Keiner soll mir dieses rauben,", "tokens": ["Kei\u00b7ner", "soll", "mir", "die\u00b7ses", "rau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PPER", "PDS", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Du warst mein, ach, mach mich dein.", "tokens": ["Du", "warst", "mein", ",", "ach", ",", "mach", "mich", "dein", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "$,", "ITJ", "$,", "VVFIN", "PPER", "PPOSAT", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.20": {"line.1": {"text": "Was ich tue, was ich denke,", "tokens": ["Was", "ich", "tue", ",", "was", "ich", "den\u00b7ke", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Alles, was mit mir geschieht,", "tokens": ["Al\u00b7les", ",", "was", "mit", "mir", "ge\u00b7schieht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "PRELS", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Herr nach deinem Auge lenke", "tokens": ["Herr", "nach", "dei\u00b7nem", "Au\u00b7ge", "len\u00b7ke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Das auf meine Wege sieht.", "tokens": ["Das", "auf", "mei\u00b7ne", "We\u00b7ge", "sieht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.21": {"line.1": {"text": "Herr, o wolle auf mein Flehen,", "tokens": ["Herr", ",", "o", "wol\u00b7le", "auf", "mein", "Fle\u00b7hen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "FM", "VMFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn mein Blut zu Tage springt,", "tokens": ["Wenn", "mein", "Blut", "zu", "Ta\u00b7ge", "springt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Heut mich wie ein Kind ansehen", "tokens": ["Heut", "mich", "wie", "ein", "Kind", "an\u00b7se\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "KOKOM", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Das sein erstes Opfer bringt.", "tokens": ["Das", "sein", "ers\u00b7tes", "Op\u00b7fer", "bringt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.22": {"line.1": {"text": "Unter scharfen Marterruten,", "tokens": ["Un\u00b7ter", "schar\u00b7fen", "Mar\u00b7ter\u00b7ru\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Unter eines Richters Schwert,", "tokens": ["Un\u00b7ter", "ei\u00b7nes", "Rich\u00b7ters", "Schwert", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "M\u00f6cht' ich dir mein Heiland bluten,", "tokens": ["M\u00f6cht'", "ich", "dir", "mein", "Hei\u00b7land", "blu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "W\u00e4r' ich deiner Kronen wert.", "tokens": ["W\u00e4r'", "ich", "dei\u00b7ner", "Kro\u00b7nen", "wert", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.23": {"line.1": {"text": "Aber, da mir nicht verg\u00f6nnet", "tokens": ["A\u00b7ber", ",", "da", "mir", "nicht", "ver\u00b7g\u00f6n\u00b7net"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["KON", "$,", "KOUS", "PPER", "PTKNEG", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Solch ein heilender Ergu\u00df,", "tokens": ["Solch", "ein", "hei\u00b7len\u00b7der", "Er\u00b7gu\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Geb' ich, weil die F\u00fclle brennet,", "tokens": ["Geb'", "ich", ",", "weil", "die", "F\u00fcl\u00b7le", "bren\u00b7net", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ach! nichts, als den \u00dcberflu\u00df.", "tokens": ["Ach", "!", "nichts", ",", "als", "den", "\u00dc\u00b7berf\u00b7lu\u00df", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PIS", "$,", "KOUS", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.24": {"line.1": {"text": "Alles doch hast du gegeben,", "tokens": ["Al\u00b7les", "doch", "hast", "du", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Gott der zu den Schmerzen kam,", "tokens": ["Gott", "der", "zu", "den", "Schmer\u00b7zen", "kam", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und im Blut hingab das Leben,", "tokens": ["Und", "im", "Blut", "hin\u00b7gab", "das", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df den Tod er von uns nahm.", "tokens": ["Da\u00df", "den", "Tod", "er", "von", "uns", "nahm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.25": {"line.1": {"text": "Meines flie\u00dft zu meinem Heile,", "tokens": ["Mei\u00b7nes", "flie\u00dft", "zu", "mei\u00b7nem", "Hei\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Flie\u00dft zu deinem Ruhme nicht", "tokens": ["Flie\u00dft", "zu", "dei\u00b7nem", "Ruh\u00b7me", "nicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "PTKNEG"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Herr mir deinen Schmerz mitteile,", "tokens": ["Herr", "mir", "dei\u00b7nen", "Schmerz", "mit\u00b7tei\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn der Stahl die Quelle bricht.", "tokens": ["Wenn", "der", "Stahl", "die", "Quel\u00b7le", "bricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.26": {"line.1": {"text": "Gieb, da\u00df deines Bluts ich denke,", "tokens": ["Gieb", ",", "da\u00df", "dei\u00b7nes", "Bluts", "ich", "den\u00b7ke", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "KOUS", "PPOSAT", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn das meine niederrinnt,", "tokens": ["Wenn", "das", "mei\u00b7ne", "nie\u00b7der\u00b7rinnt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "PPOSAT", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und in deine Wunden senke", "tokens": ["Und", "in", "dei\u00b7ne", "Wun\u00b7den", "sen\u00b7ke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dein ohnm\u00e4cht'ges schwaches Kind!", "tokens": ["Dein", "ohn\u00b7m\u00e4cht'\u00b7ges", "schwa\u00b7ches", "Kind", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.27": {"line.1": {"text": "La\u00df, was b\u00f6s in meinen Sinnen,", "tokens": ["La\u00df", ",", "was", "b\u00f6s", "in", "mei\u00b7nen", "Sin\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "PRELS", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Alle hei\u00dfe Erdenglut,", "tokens": ["Al\u00b7le", "hei\u00b7\u00dfe", "Er\u00b7deng\u00b7lut", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Heut aus meinen Adern rinnen,", "tokens": ["Heut", "aus", "mei\u00b7nen", "A\u00b7dern", "rin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Morgen dann gieb mir ", "tokens": ["Mor\u00b7gen", "dann", "gieb", "mir"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ADV", "VVIMP", "PPER"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.28": {"line.1": {"text": "O wie hast du's gut bestellet,", "tokens": ["O", "wie", "hast", "du's", "gut", "be\u00b7stel\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "VAFIN", "PIS", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Meine Seele fa\u00dft es kaum,", "tokens": ["Mei\u00b7ne", "See\u00b7le", "fa\u00dft", "es", "kaum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df dein Blut sich mir gesellet,", "tokens": ["Da\u00df", "dein", "Blut", "sich", "mir", "ge\u00b7sel\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PRF", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Macht das meine heut ihm Raum.", "tokens": ["Macht", "das", "mei\u00b7ne", "heut", "ihm", "Raum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "PPOSAT", "ADV", "PPER", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.29": {"line.1": {"text": "All dein Blut hast du vergossen", "tokens": ["All", "dein", "Blut", "hast", "du", "ver\u00b7gos\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "PPOSAT", "NN", "VAFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mir zu tilgen das Gericht,", "tokens": ["Mir", "zu", "til\u00b7gen", "das", "Ge\u00b7richt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und es ist f\u00fcr mich geflossen,", "tokens": ["Und", "es", "ist", "f\u00fcr", "mich", "ge\u00b7flos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "APPR", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Aber ich, ich nahm es nicht.", "tokens": ["A\u00b7ber", "ich", ",", "ich", "nahm", "es", "nicht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "PPER", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.30": {"line.1": {"text": "Hast auch deinen Leib gegeben,", "tokens": ["Hast", "auch", "dei\u00b7nen", "Leib", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "F\u00fcr mich in des Richters Zorn,", "tokens": ["F\u00fcr", "mich", "in", "des", "Rich\u00b7ters", "Zorn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ich zage f\u00fcr das Leben,", "tokens": ["Und", "ich", "za\u00b7ge", "f\u00fcr", "das", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Trag' um dich auch keinen Dorn.", "tokens": ["Trag'", "um", "dich", "auch", "kei\u00b7nen", "Dorn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "ADV", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.31": {"line.1": {"text": "Und ich wei\u00df doch, es giebt Seelen", "tokens": ["Und", "ich", "wei\u00df", "doch", ",", "es", "giebt", "See\u00b7len"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ADV", "$,", "PPER", "VVFIN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Brennend so in reiner Glut,", "tokens": ["Bren\u00b7nend", "so", "in", "rei\u00b7ner", "Glut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df sie deine Wunden z\u00e4hlen", "tokens": ["Da\u00df", "sie", "dei\u00b7ne", "Wun\u00b7den", "z\u00e4h\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "An sich selbst in Wunderflut.", "tokens": ["An", "sich", "selbst", "in", "Wun\u00b7der\u00b7flut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "ADV", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.32": {"line.1": {"text": "Ach weil ich nicht diesen gleiche", "tokens": ["Ach", "weil", "ich", "nicht", "die\u00b7sen", "glei\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "KOUS", "PPER", "PTKNEG", "PDAT", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist wohl b\u00f6ses Blut in mir,", "tokens": ["Ist", "wohl", "b\u00f6\u00b7ses", "Blut", "in", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJA", "NN", "APPR", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Gieb da\u00df alles es entweiche,", "tokens": ["Gieb", "da\u00df", "al\u00b7les", "es", "ent\u00b7wei\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "KOUS", "PIS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Jesus dann gefall' ich dir.", "tokens": ["Je\u00b7sus", "dann", "ge\u00b7fall'", "ich", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "VVFIN", "PPER", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.33": {"line.1": {"text": "Und ersetz' es geistlich wieder", "tokens": ["Und", "er\u00b7setz'", "es", "geist\u00b7lich", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Morgen mir mit deinem Blut", "tokens": ["Mor\u00b7gen", "mir", "mit", "dei\u00b7nem", "Blut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Vor dir sink' ich rein dann nieder,", "tokens": ["Vor", "dir", "sink'", "ich", "rein", "dann", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PPER", "ADJD", "ADV", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wo die B\u00fc\u00dferin geruht.", "tokens": ["Wo", "die", "B\u00fc\u00b7\u00dfe\u00b7rin", "ge\u00b7ruht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.34": {"line.1": {"text": "Herr, du wei\u00dft ich wollt' bekennen,", "tokens": ["Herr", ",", "du", "wei\u00dft", "ich", "wollt'", "be\u00b7ken\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VVFIN", "PPER", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Was die Seele niederdr\u00fcckt", "tokens": ["Was", "die", "See\u00b7le", "nie\u00b7der\u00b7dr\u00fcckt"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "ART", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Felsen von dem Quell mich trennen", "tokens": ["Fel\u00b7sen", "von", "dem", "Quell", "mich", "tren\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wo die Bu\u00dfe Gnade pfl\u00fcckt.", "tokens": ["Wo", "die", "Bu\u00b7\u00dfe", "Gna\u00b7de", "pfl\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.35": {"line.1": {"text": "Ich hab' nicht den Zaun durchbrochen,", "tokens": ["Ich", "hab'", "nicht", "den", "Zaun", "durch\u00b7bro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ART", "NN", "VVPP", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Herr vergieb uns unsre Schuld,", "tokens": ["Herr", "ver\u00b7gieb", "uns", "uns\u00b7re", "Schuld", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "W\u00e4r' durch Dornen gern gekrochen", "tokens": ["W\u00e4r'", "durch", "Dor\u00b7nen", "gern", "ge\u00b7kro\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "NN", "ADV", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Heim in deiner Kirche Huld.", "tokens": ["Heim", "in", "dei\u00b7ner", "Kir\u00b7che", "Huld", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.36": {"line.1": {"text": "Und lie\u00df ich denn meine S\u00fcnden,", "tokens": ["Und", "lie\u00df", "ich", "denn", "mei\u00b7ne", "S\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Alle heut in meinem Blut,", "tokens": ["Al\u00b7le", "heut", "in", "mei\u00b7nem", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wolle mich in ihm entbinden,", "tokens": ["Wol\u00b7le", "mich", "in", "ihm", "ent\u00b7bin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "APPR", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie die Erd' in S\u00fcndenflut.", "tokens": ["Wie", "die", "Erd'", "in", "S\u00fcn\u00b7den\u00b7flut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.37": {"line.1": {"text": "Mit dem Blute wird verschuldet,", "tokens": ["Mit", "dem", "Blu\u00b7te", "wird", "ver\u00b7schul\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mit dem Blute wird vers\u00fchnt,", "tokens": ["Mit", "dem", "Blu\u00b7te", "wird", "ver\u00b7s\u00fchnt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Du Herr hast die Pein erduldet,", "tokens": ["Du", "Herr", "hast", "die", "Pein", "er\u00b7dul\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ich, ich habe sie verdient.", "tokens": ["Ich", ",", "ich", "ha\u00b7be", "sie", "ver\u00b7dient", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PPER", "VAFIN", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.38": {"line.1": {"text": "Und so komm' ich dann im Glauben", "tokens": ["Und", "so", "komm'", "ich", "dann", "im", "Glau\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADV", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Deines Blutes Gast zu sein,", "tokens": ["Dei\u00b7nes", "Blu\u00b7tes", "Gast", "zu", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "PTKZU", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Keiner soll mir dieses rauben,", "tokens": ["Kei\u00b7ner", "soll", "mir", "die\u00b7ses", "rau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PPER", "PDS", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Du warst mein, ach, mach mich dein.", "tokens": ["Du", "warst", "mein", ",", "ach", ",", "mach", "mich", "dein", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "$,", "ITJ", "$,", "VVFIN", "PPER", "PPOSAT", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}