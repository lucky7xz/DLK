N"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.2": {"text": "Jovis Stern dem Himmel, mit Wehmuth blickt er", "tokens": ["Jo\u00b7vis", "Stern", "dem", "Him\u00b7mel", ",", "mit", "Weh\u00b7muth", "blickt", "er"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NN", "ART", "NN", "$,", "APPR", "NN", "VVFIN", "PPER"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.3": {"text": "Seine Tempeltr\u00fcmmer am Capitol an,", "tokens": ["Sei\u00b7ne", "Tem\u00b7pel\u00b7tr\u00fcm\u00b7mer", "am", "Ca\u00b7pi\u00b7tol", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "Ave Maria.", "tokens": ["A\u00b7ve", "Ma\u00b7ria", "."], "token_info": ["word", "word", "punct"], "pos": ["NE", "NE", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.6": {"line.1": {"text": "Halb im Mondschein, halb in der D\u00e4mmerung schon", "tokens": ["Halb", "im", "Mond\u00b7schein", ",", "halb", "in", "der", "D\u00e4m\u00b7me\u00b7rung", "schon"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJD", "APPRART", "NN", "$,", "ADJD", "APPR", "ART", "NN", "ADV"], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Graut der Stiere s\u00e4ulenbedecktes Schuttfeld,", "tokens": ["Graut", "der", "Stie\u00b7re", "s\u00e4u\u00b7len\u00b7be\u00b7deck\u00b7tes", "Schutt\u00b7feld", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.3": {"text": "Und im Zwielicht wandelt noch eines M\u00f6nches", "tokens": ["Und", "im", "Zwie\u00b7licht", "wan\u00b7delt", "noch", "ei\u00b7nes", "M\u00f6n\u00b7ches"], "token_info": ["word", "word", "word", "word", "word", "