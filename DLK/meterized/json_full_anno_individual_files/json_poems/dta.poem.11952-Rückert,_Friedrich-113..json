{"dta.poem.11952": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "113.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wer mit Erholung recht wei\u00df Arbeit auszugleichen,", "tokens": ["Wer", "mit", "Er\u00b7ho\u00b7lung", "recht", "wei\u00df", "Ar\u00b7beit", "aus\u00b7zu\u00b7glei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "NN", "ADJD", "VVFIN", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mag ohn' Erm\u00fcdung wol ein sch\u00f6nes Ziel erreichen.", "tokens": ["Mag", "ohn'", "Er\u00b7m\u00fc\u00b7dung", "wol", "ein", "sch\u00f6\u00b7nes", "Ziel", "er\u00b7rei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "NN", "ADV", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ein Thor ist, wer, anstatt Erholung seiner Kr\u00e4fte", "tokens": ["Ein", "Thor", "ist", ",", "wer", ",", "an\u00b7statt", "Er\u00b7ho\u00b7lung", "sei\u00b7ner", "Kr\u00e4f\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "$,", "PWS", "$,", "KOUI", "NN", "PPOSAT", "NN"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Zu suchen, selber macht Erholung zum Gesch\u00e4fte.", "tokens": ["Zu", "su\u00b7chen", ",", "sel\u00b7ber", "macht", "Er\u00b7ho\u00b7lung", "zum", "Ge\u00b7sch\u00e4f\u00b7te", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "ADV", "VVFIN", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ein Weiser ist, wer Scherz und Ernst zu sondern wei\u00df,", "tokens": ["Ein", "Wei\u00b7ser", "ist", ",", "wer", "Scherz", "und", "Ernst", "zu", "son\u00b7dern", "wei\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$,", "PWS", "NN", "KON", "NN", "APPR", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und sich an heiterm Spiel neu st\u00e4rkt zu strengem Flei\u00df.", "tokens": ["Und", "sich", "an", "hei\u00b7term", "Spiel", "neu", "st\u00e4rkt", "zu", "stren\u00b7gem", "Flei\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "APPR", "ADJA", "NN", "ADJD", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Noch weiser doch ist, wer sich solch ein Spielwerk macht,", "tokens": ["Noch", "wei\u00b7ser", "doch", "ist", ",", "wer", "sich", "solch", "ein", "Spiel\u00b7werk", "macht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "ADV", "VAFIN", "$,", "PWS", "PRF", "PIAT", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wodurch sein Tagewerk selbst weiter wird gebracht.", "tokens": ["Wo\u00b7durch", "sein", "Ta\u00b7ge\u00b7werk", "selbst", "wei\u00b7ter", "wird", "ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "ADV", "ADV", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Der erste kann zu nichts, der andre weit es bringen,", "tokens": ["Der", "ers\u00b7te", "kann", "zu", "nichts", ",", "der", "and\u00b7re", "weit", "es", "brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VMFIN", "APPR", "PIS", "$,", "PRELS", "PIS", "ADJD", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch nur dem dritten wird Vorz\u00fcgliches gelingen.", "tokens": ["Doch", "nur", "dem", "drit\u00b7ten", "wird", "Vor\u00b7z\u00fcg\u00b7li\u00b7ches", "ge\u00b7lin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "ADJA", "VAFIN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}