{"textgrid.poem.59479": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Die Hochzeitsnacht", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nachts durch die stille Runde", "tokens": ["Nachts", "durch", "die", "stil\u00b7le", "Run\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN"], "meter": "++-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Rauschte des Rheines Lauf,", "tokens": ["Rauschte", "des", "Rhei\u00b7nes", "Lauf", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Ein Schifflein zog im Grunde,", "tokens": ["Ein", "Schif\u00b7flein", "zog", "im", "Grun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein Ritter stand darauf.", "tokens": ["Ein", "Rit\u00b7ter", "stand", "da\u00b7rauf", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PAV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die Blicke irre schweifen", "tokens": ["Die", "Bli\u00b7cke", "ir\u00b7re", "schwei\u00b7fen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Von seines Schiffes Rand,", "tokens": ["Von", "sei\u00b7nes", "Schif\u00b7fes", "Rand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ein blutigroter Streifen", "tokens": ["Ein", "blu\u00b7ti\u00b7gro\u00b7ter", "Strei\u00b7fen"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sich um das Haupt ihm wand.", "tokens": ["Sich", "um", "das", "Haupt", "ihm", "wand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Der sprach: \u00bbDa oben stehet", "tokens": ["Der", "sprach", ":", "\u00bb", "Da", "o\u00b7ben", "ste\u00b7het"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word"], "pos": ["PDS", "VVFIN", "$.", "$(", "ADV", "ADV", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein Schl\u00f6\u00dflein \u00fcberm Rhein,", "tokens": ["Ein", "Schl\u00f6\u00df\u00b7lein", "\u00fc\u00b7berm", "Rhein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NE", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die an dem Fenster stehet:", "tokens": ["Die", "an", "dem", "Fens\u00b7ter", "ste\u00b7het", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das ist die Liebste mein.", "tokens": ["Das", "ist", "die", "Liebs\u00b7te", "mein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "PPOSAT", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Sie hat mir Treu versprochen,", "tokens": ["Sie", "hat", "mir", "Treu", "ver\u00b7spro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Bis ich gekommen sei,", "tokens": ["Bis", "ich", "ge\u00b7kom\u00b7men", "sei", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie hat die Treu gebrochen,", "tokens": ["Sie", "hat", "die", "Treu", "ge\u00b7bro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und alles ist vorbei.\u00ab", "tokens": ["Und", "al\u00b7les", "ist", "vor\u00b7bei", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PIS", "VAFIN", "ADV", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Viel Hochzeitleute drehen", "tokens": ["Viel", "Hoch\u00b7zeit\u00b7leu\u00b7te", "dre\u00b7hen"], "token_info": ["word", "word", "word"], "pos": ["PIAT", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sich oben laut und bunt,", "tokens": ["Sich", "o\u00b7ben", "laut", "und", "bunt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie bleibet einsam stehen,", "tokens": ["Sie", "blei\u00b7bet", "ein\u00b7sam", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und lauschet in den Grund.", "tokens": ["Und", "lau\u00b7schet", "in", "den", "Grund", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Und wie sie tanzen munter,", "tokens": ["Und", "wie", "sie", "tan\u00b7zen", "mun\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und Schiff und Schiffer schwand,", "tokens": ["Und", "Schiff", "und", "Schif\u00b7fer", "schwand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Stieg sie vom Schlo\u00df herunter,", "tokens": ["Stieg", "sie", "vom", "Schlo\u00df", "her\u00b7un\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Bis sie im Garten stand.", "tokens": ["Bis", "sie", "im", "Gar\u00b7ten", "stand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Die Spielleut musizierten,", "tokens": ["Die", "Spiel\u00b7leut", "mu\u00b7si\u00b7zier\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sie sann gar mancherlei,", "tokens": ["Sie", "sann", "gar", "man\u00b7cher\u00b7lei", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PIS", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die T\u00f6ne sie so r\u00fchrten,", "tokens": ["Die", "T\u00f6\u00b7ne", "sie", "so", "r\u00fchr\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Als m\u00fc\u00dft das Herz entzwei.", "tokens": ["Als", "m\u00fc\u00dft", "das", "Herz", "ent\u00b7zwei", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VMFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Da trat ihr Br\u00e4ut'gam s\u00fc\u00dfe", "tokens": ["Da", "trat", "ihr", "Br\u00e4ut'\u00b7gam", "s\u00fc\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "NE", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zu ihr aus stiller Nacht,", "tokens": ["Zu", "ihr", "aus", "stil\u00b7ler", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So freundlich er sie gr\u00fc\u00dfte,", "tokens": ["So", "freund\u00b7lich", "er", "sie", "gr\u00fc\u00df\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da\u00df ihr das Herze lacht.", "tokens": ["Da\u00df", "ihr", "das", "Her\u00b7ze", "lacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PDS", "VVFIN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Er sprach: \u00bbWas willst du weinen,", "tokens": ["Er", "sprach", ":", "\u00bb", "Was", "willst", "du", "wei\u00b7nen", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "$(", "PWS", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Weil alle fr\u00f6hlich sein?", "tokens": ["Weil", "al\u00b7le", "fr\u00f6h\u00b7lich", "sein", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADJD", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Stern so helle scheinen,", "tokens": ["Die", "Stern", "so", "hel\u00b7le", "schei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So lustig geht der Rhein.", "tokens": ["So", "lus\u00b7tig", "geht", "der", "Rhein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "ART", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Das Kr\u00e4nzlein in den Haaren", "tokens": ["Das", "Kr\u00e4nz\u00b7lein", "in", "den", "Haa\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Steht dir so wunderfein,", "tokens": ["Steht", "dir", "so", "wun\u00b7der\u00b7fein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wir wollen etwas fahren", "tokens": ["Wir", "wol\u00b7len", "et\u00b7was", "fah\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PIS", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Hinunter auf dem Rhein.\u00ab", "tokens": ["Hin\u00b7un\u00b7ter", "auf", "dem", "Rhein", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "APPR", "ART", "NE", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Zum Kahn folgt' sie behende,", "tokens": ["Zum", "Kahn", "folgt'", "sie", "be\u00b7hen\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Setzt' sich ganz vorne hin,", "tokens": ["Setzt'", "sich", "ganz", "vor\u00b7ne", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er setzt' sich an das Ende", "tokens": ["Er", "setzt'", "sich", "an", "das", "En\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und lie\u00df das Schifflein ziehn.", "tokens": ["Und", "lie\u00df", "das", "Schif\u00b7flein", "ziehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Sie sprach: \u00bbDie Tone kommen", "tokens": ["Sie", "sprach", ":", "\u00bb", "Die", "To\u00b7ne", "kom\u00b7men"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "$(", "ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Verworren durch den Wind,", "tokens": ["Ver\u00b7wor\u00b7ren", "durch", "den", "Wind", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Fenster sind verglommen,", "tokens": ["Die", "Fens\u00b7ter", "sind", "ver\u00b7glom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wir fahren so geschwind.", "tokens": ["Wir", "fah\u00b7ren", "so", "ge\u00b7schwind", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Was sind das f\u00fcr so lange", "tokens": ["Was", "sind", "das", "f\u00fcr", "so", "lan\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "PDS", "APPR", "ADV", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Gebirge weit und breit?", "tokens": ["Ge\u00b7bir\u00b7ge", "weit", "und", "breit", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mir wird auf einmal bange", "tokens": ["Mir", "wird", "auf", "ein\u00b7mal", "ban\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "ADV", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In dieser Einsamkeit!", "tokens": ["In", "die\u00b7ser", "Ein\u00b7sam\u00b7keit", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Und fremde Leute stehen", "tokens": ["Und", "frem\u00b7de", "Leu\u00b7te", "ste\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Auf mancher Felsenwand,", "tokens": ["Auf", "man\u00b7cher", "Fel\u00b7sen\u00b7wand", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und stehen still und sehen", "tokens": ["Und", "ste\u00b7hen", "still", "und", "se\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PTKVZ", "KON", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So schwindlig \u00fcbern Rand.\u00ab \u2013", "tokens": ["So", "schwind\u00b7lig", "\u00fc\u00b7bern", "Rand", ".", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ADV", "ADJD", "ADJA", "NN", "$.", "$(", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "Der Br\u00e4ut'gam schien so traurig", "tokens": ["Der", "Br\u00e4ut'\u00b7gam", "schien", "so", "trau\u00b7rig"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NE", "VVFIN", "ADV", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und sprach kein einzig Wort,", "tokens": ["Und", "sprach", "kein", "ein\u00b7zig", "Wort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "ADJD", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Schaut in die Wellen schaurig", "tokens": ["Schaut", "in", "die", "Wel\u00b7len", "schau\u00b7rig"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und rudert immerfort.", "tokens": ["Und", "ru\u00b7dert", "im\u00b7mer\u00b7fort", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.16": {"line.1": {"text": "Sie sprach: \u00bbSchon seh ich Streifen", "tokens": ["Sie", "sprach", ":", "\u00bb", "Schon", "seh", "ich", "Strei\u00b7fen"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "$(", "ADV", "VVFIN", "PPER", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So rot im Morgen stehn,", "tokens": ["So", "rot", "im", "Mor\u00b7gen", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und Stimmen h\u00f6r ich schweifen,", "tokens": ["Und", "Stim\u00b7men", "h\u00f6r", "ich", "schwei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure