{"dta.poem.19574": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Thespestus.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-200905193933", "language": ["de:0.99"], "booktitle": "Meyer, Conrad Ferdinand: Gedichte. Leipzig, 1882."}, "poem": {"stanza.1": {"line.1": {"text": "Zwei Greise ruhten unter einer Pinie,", "tokens": ["Zwei", "Grei\u00b7se", "ruh\u00b7ten", "un\u00b7ter", "ei\u00b7ner", "Pi\u00b7nie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Stab neben Stab, an einer Quelle klarer Flut,", "tokens": ["Stab", "ne\u00b7ben", "Stab", ",", "an", "ei\u00b7ner", "Quel\u00b7le", "kla\u00b7rer", "Flut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Wo wandernd sie begegnet sich von ungef\u00e4hr.", "tokens": ["Wo", "wan\u00b7dernd", "sie", "be\u00b7geg\u00b7net", "sich", "von", "un\u00b7ge\u00b7f\u00e4hr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "VVFIN", "PRF", "APPR", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sie f\u00fchrten Zwiegespr\u00e4ch und sie behagten sich.", "tokens": ["Sie", "f\u00fchr\u00b7ten", "Zwie\u00b7ge\u00b7spr\u00e4ch", "und", "sie", "be\u00b7hag\u00b7ten", "sich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "PPER", "VVFIN", "PRF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "\u2014 \u201eMan nennt mich Eukrates, und wer, mein Freund, bist du?\u201c", "tokens": ["\u201e", "Man", "nennt", "mich", "Eu\u00b7kra\u00b7tes", ",", "und", "wer", ",", "mein", "Freund", ",", "bist", "du", "?", "\u201c"], "token_info": ["punct", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["$(", "$(", "PIS", "VVFIN", "PRF", "NE", "$,", "KON", "PWS", "$,", "PPOSAT", "NN", "$,", "VAFIN", "PPER", "$.", "$("], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.6": {"text": "\u2014 \u201eMich nannten Aridaeus lange Jahre sie,", "tokens": ["\u201e", "Mich", "nann\u00b7ten", "A\u00b7ri\u00b7dae\u00b7us", "lan\u00b7ge", "Jah\u00b7re", "sie", ","], "token_info": ["punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "$(", "PPER", "VVFIN", "NE", "ADJA", "NN", "PPER", "$,"], "meter": "+-+-+-+-+--+", "measure": "iambic.hexa.chol"}, "line.7": {"text": "Seit langen Jahren bin ich nun Thespesius.\u201c", "tokens": ["Seit", "lan\u00b7gen", "Jah\u00b7ren", "bin", "ich", "nun", "Thes\u00b7pe\u00b7sius", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ADJA", "NN", "VAFIN", "PPER", "ADV", "NE", "$.", "$("], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "\u2014 \u201eZwei Namen trugst du?\u201c \u2014 \u201eBeide Namen, Eukrates.", "tokens": ["\u201e", "Zwei", "Na\u00b7men", "trugst", "du", "?", "\u201c", "\u201e", "Bei\u00b7de", "Na\u00b7men", ",", "Eu\u00b7kra\u00b7tes", "."], "token_info": ["punct", "punct", "word", "word", "word", "word", "punct", "punct", "punct", "punct", "word", "word", "punct", "word", "punct"], "pos": ["$(", "$(", "CARD", "NN", "VVFIN", "PPER", "$.", "$(", "$(", "$(", "PIAT", "NN", "$,", "NE", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "H\u00f6r' an! Ein J\u00fcngling, peitscht' ich rasend das Gespann.", "tokens": ["H\u00f6r'", "an", "!", "Ein", "J\u00fcng\u00b7ling", ",", "peitscht'", "ich", "ra\u00b7send", "das", "Ge\u00b7spann", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$.", "ART", "NN", "$,", "VVFIN", "PPER", "VVPP", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Die Rosse flogen. Becher, Buhlen, W\u00fcrfelspiel,", "tokens": ["Die", "Ros\u00b7se", "flo\u00b7gen", ".", "Be\u00b7cher", ",", "Buh\u00b7len", ",", "W\u00fcr\u00b7fel\u00b7spiel", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Wuth, Zorn, vergossen Blut \u2014 verklagend Blut!", "tokens": ["Wuth", ",", "Zorn", ",", "ver\u00b7gos\u00b7sen", "Blut", "ver\u00b7kla\u00b7gend", "Blut", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "ADJA", "NN", "$(", "VVPP", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Dem ich entfloh, die Eumeniden hinter mir \u2014", "tokens": ["Dem", "ich", "ent\u00b7floh", ",", "die", "Eu\u00b7me\u00b7ni\u00b7den", "hin\u00b7ter", "mir"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "$,", "ART", "NN", "APPR", "PPER", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Sie folgten meiner raschen F\u00fc\u00dfe schnellstem Lauf,", "tokens": ["Sie", "folg\u00b7ten", "mei\u00b7ner", "ra\u00b7schen", "F\u00fc\u00b7\u00dfe", "schnells\u00b7tem", "Lauf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Ich warf mich in den Flu\u00df, sie sprangen jauchzend nach", "tokens": ["Ich", "warf", "mich", "in", "den", "Flu\u00df", ",", "sie", "spran\u00b7gen", "jauch\u00b7zend", "nach"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "NN", "$,", "PPER", "VVFIN", "ADJD", "APPR"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Und hoben schwimmend ihrer Fackeln d\u00fcstre Glut.", "tokens": ["Und", "ho\u00b7ben", "schwim\u00b7mend", "ih\u00b7rer", "Fa\u00b7ckeln", "d\u00fcst\u00b7re", "Glut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "PPOSAT", "NN", "VVFIN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Ich klomm bergan \u2014 verirrt st\u00fcrzt' ich von einer Wand \u2014", "tokens": ["Ich", "klomm", "ber\u00b7gan", "ver\u00b7irrt", "st\u00fcrzt'", "ich", "von", "ei\u00b7ner", "Wand"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$(", "VVPP", "VVFIN", "PPER", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Die Sinne schwanden mir. Dann lebt' ich wieder \u2014 war's", "tokens": ["Die", "Sin\u00b7ne", "schwan\u00b7den", "mir", ".", "Dann", "lebt'", "ich", "wie\u00b7der", "wa\u00b7r's"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "$.", "ADV", "VVFIN", "PPER", "ADV", "$(", "VAFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Im Traum? \u2014 und schritt auf einem weichen Wiesengr\u00fcn,", "tokens": ["Im", "Traum", "?", "und", "schritt", "auf", "ei\u00b7nem", "wei\u00b7chen", "Wie\u00b7sen\u00b7gr\u00fcn", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$.", "$(", "KON", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.19": {"text": "Wo Sel'ge \u2014 solche schienen sie \u2014 lustwandelten", "tokens": ["Wo", "Sel'\u00b7ge", "sol\u00b7che", "schie\u00b7nen", "sie", "lust\u00b7wan\u00b7del\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["PWAV", "NE", "$(", "PIS", "VVFIN", "PPER", "$(", "VVFIN"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.20": {"text": "In still bewegten Schaaren. Kr\u00e4nze trugen sie.", "tokens": ["In", "still", "be\u00b7weg\u00b7ten", "Schaa\u00b7ren", ".", "Kr\u00e4n\u00b7ze", "tru\u00b7gen", "sie", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "$.", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Den Einen kannt' ich wohl und ward von ihm erkannt:", "tokens": ["Den", "Ei\u00b7nen", "kannt'", "ich", "wohl", "und", "ward", "von", "ihm", "er\u00b7kannt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "ADV", "KON", "VAFIN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Mein Blutsverwandter, welcher j\u00fcngst geschwunden war", "tokens": ["Mein", "Bluts\u00b7ver\u00b7wand\u00b7ter", ",", "wel\u00b7cher", "j\u00fcngst", "ge\u00b7schwun\u00b7den", "war"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "ADV", "VVPP", "VAFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Aus dieser Erde Staub nach einem reinen Lauf.", "tokens": ["Aus", "die\u00b7ser", "Er\u00b7de", "Staub", "nach", "ei\u00b7nem", "rei\u00b7nen", "Lauf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Der sprach mich an: \u201eIch gr\u00fc\u00dfe dich, Thespesius!\u201c", "tokens": ["Der", "sprach", "mich", "an", ":", "\u201e", "Ich", "gr\u00fc\u00b7\u00dfe", "dich", ",", "Thes\u00b7pe\u00b7sius", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PTKVZ", "$.", "$(", "PPER", "VVFIN", "PPER", "$,", "NE", "$.", "$("], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.25": {"text": "\u201ewozu der neue Name, wundersamer Ohm?", "tokens": ["\u201e", "wo\u00b7zu", "der", "neu\u00b7e", "Na\u00b7me", ",", "wun\u00b7der\u00b7sa\u00b7mer", "Ohm", "?"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "PWAV", "ART", "ADJA", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Wie nennst du mich? Dein Aridaeus bin ich ja!\u201c", "tokens": ["Wie", "nennst", "du", "mich", "?", "Dein", "A\u00b7ri\u00b7dae\u00b7us", "bin", "ich", "ja", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PRF", "$.", "PPOSAT", "NE", "VAFIN", "PPER", "ADV", "$.", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Die Locken sch\u00fcttelt' leis er, die ambrosischen:", "tokens": ["Die", "Lo\u00b7cken", "sch\u00fct\u00b7telt'", "leis", "er", ",", "die", "am\u00b7bro\u00b7si\u00b7schen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "PPER", "$,", "PRELS", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "\u201eund abermals, ich gr\u00fc\u00dfe dich, Thespesius!\u201c \u2014", "tokens": ["\u201e", "und", "a\u00b7ber\u00b7mals", ",", "ich", "gr\u00fc\u00b7\u00dfe", "dich", ",", "Thes\u00b7pe\u00b7sius", "!", "\u201c"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "punct", "punct"], "pos": ["$(", "KON", "ADV", "$,", "PPER", "VVFIN", "PPER", "$,", "NE", "$.", "$(", "$("], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.29": {"text": "Jetzt wacht' ich wirklich auf. Am Hange lag", "tokens": ["Jetzt", "wacht'", "ich", "wirk\u00b7lich", "auf", ".", "Am", "Han\u00b7ge", "lag"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "PTKVZ", "$.", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.30": {"text": "Ich blutbedeckt, von gier'gen Raben schon umschw\u00e4rmt \u2014", "tokens": ["Ich", "blut\u00b7be\u00b7deckt", ",", "von", "gier'\u00b7gen", "Ra\u00b7ben", "schon", "um\u00b7schw\u00e4rmt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "APPR", "ADJA", "NN", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Was mehr? Ich ward ein Andrer. Nicht mit kleinem Kampf!", "tokens": ["Was", "mehr", "?", "Ich", "ward", "ein", "A\u00b7ndrer", ".", "Nicht", "mit", "klei\u00b7nem", "Kampf", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "$.", "PPER", "VAFIN", "ART", "NN", "$.", "PTKNEG", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Der Kampf ist gro\u00df! Mein neuer Name st\u00e4rkte mich,", "tokens": ["Der", "Kampf", "ist", "gro\u00df", "!", "Mein", "neu\u00b7er", "Na\u00b7me", "st\u00e4rk\u00b7te", "mich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$.", "PPOSAT", "ADJA", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Der makellose, der so rein und g\u00f6ttlich klang!", "tokens": ["Der", "ma\u00b7kel\u00b7lo\u00b7se", ",", "der", "so", "rein", "und", "g\u00f6tt\u00b7lich", "klang", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "ADJD", "KON", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Hab' gute Fahrt!\u201c \u2014 \u201eFahr' wohl auch du, Thespesius!\u201c", "tokens": ["Hab'", "gu\u00b7te", "Fahrt", "!", "\u201c", "\u201e", "Fahr'", "wohl", "auch", "du", ",", "Thes\u00b7pe\u00b7sius", "!", "\u201c"], "token_info": ["word", "word", "word", "punct", "punct", "punct", "punct", "word", "word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["NN", "ADJA", "NN", "$.", "$(", "$(", "$(", "VVIMP", "ADV", "ADV", "PPER", "$,", "NE", "$.", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}}}}