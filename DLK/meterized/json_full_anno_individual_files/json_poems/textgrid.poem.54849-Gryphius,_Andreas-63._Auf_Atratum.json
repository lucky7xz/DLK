{"textgrid.poem.54849": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "63. Auf Atratum", "genre": "verse", "period": "N.A.", "pub_year": 1640, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dein \u00fcberpr\u00e4chtig Grab/ das schwer erschunden Geld/", "tokens": ["Dein", "\u00fc\u00b7ber\u00b7pr\u00e4ch\u00b7tig", "Grab", "/", "das", "schwer", "er\u00b7schun\u00b7den", "Geld", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "$(", "ART", "ADJD", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und armer Leutte Schwei\u00df und Thr\u00e4nen auffgestelt/", "tokens": ["Und", "ar\u00b7mer", "Leut\u00b7te", "Schwei\u00df", "und", "Thr\u00e4\u00b7nen", "auff\u00b7ge\u00b7stelt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "NN", "KON", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Bricht itzt der Krig entzwey und die verfaulten Glider", "tokens": ["Bricht", "itzt", "der", "Krig", "ent\u00b7zwey", "und", "die", "ver\u00b7faul\u00b7ten", "Gli\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "NN", "PTKVZ", "KON", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Reist ein ergrimmter Hund voll Rasen hin und wider.", "tokens": ["Reist", "ein", "er\u00b7grimm\u00b7ter", "Hund", "voll", "Ra\u00b7sen", "hin", "und", "wi\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "ADJD", "NN", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.i