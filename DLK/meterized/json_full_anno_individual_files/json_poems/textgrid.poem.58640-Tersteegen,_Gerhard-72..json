{"textgrid.poem.58640": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "72.", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich wei\u00df und immer mehr erfahr', wie ich so leicht kann wanken;", "tokens": ["Ich", "wei\u00df", "und", "im\u00b7mer", "mehr", "er\u00b7fahr'", ",", "wie", "ich", "so", "leicht", "kann", "wan\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "ADV", "ADV", "VVFIN", "$,", "PWAV", "PPER", "ADV", "ADJD", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "Herr, la\u00df doch deines Geistes Zucht mich halten in den Schranken!", "tokens": ["Herr", ",", "la\u00df", "doch", "dei\u00b7nes", "Geis\u00b7tes", "Zucht", "mich", "hal\u00b7ten", "in", "den", "Schran\u00b7ken", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVIMP", "ADV", "PPOSAT", "NN", "NN", "PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.3": {"text": "Mein Licht und Vorsatz reicht nicht zu, die Kr\u00e4fte sind zu klein,", "tokens": ["Mein", "Licht", "und", "Vor\u00b7satz", "reicht", "nicht", "zu", ",", "die", "Kr\u00e4f\u00b7te", "sind", "zu", "klein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "VVFIN", "PTKNEG", "PTKVZ", "$,", "ART", "NN", "VAFIN", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "Ich schwach' und dummes Kindlein mu\u00df von dir geg\u00e4ngelt sein.", "tokens": ["Ich", "schwach'", "und", "dum\u00b7mes", "Kin\u00b7dlein", "mu\u00df", "von", "dir", "ge\u00b7g\u00e4n\u00b7gelt", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "ADJA", "NN", "VMFIN", "APPR", "PPER", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.5": {"text": "Ich \u00fcberlass' mich deiner Hand und deiner Weisheit Leiten,", "tokens": ["Ich", "\u00fc\u00b7ber\u00b7lass'", "mich", "dei\u00b7ner", "Hand", "und", "dei\u00b7ner", "Weis\u00b7heit", "Lei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.6": {"text": "Du kannst durch Kreuz und Leiden mich zu deinem Reich bereiten;", "tokens": ["Du", "kannst", "durch", "Kreuz", "und", "Lei\u00b7den", "mich", "zu", "dei\u00b7nem", "Reich", "be\u00b7rei\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "NN", "KON", "NN", "PPER", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.7": {"text": "Wohlan, so schlag und z\u00fccht'ge mich, doch wei\u00dft du Ziel und Ma\u00df,", "tokens": ["Wo\u00b7hlan", ",", "so", "schlag", "und", "z\u00fccht'\u00b7ge", "mich", ",", "doch", "wei\u00dft", "du", "Ziel", "und", "Ma\u00df", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "VVFIN", "KON", "VVFIN", "PPER", "$,", "ADV", "VVFIN", "PPER", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.8": {"text": "Herr, tu es nicht in deinem Grimm und mich nicht gar verla\u00df!", "tokens": ["Herr", ",", 