{"textgrid.poem.36180": {"metadata": {"author": {"name": "Kosegarten, Gotthard Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Im All der Sonnen walten der Kr\u00e4fte zwei.", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im All der Sonnen walten der Kr\u00e4fte zwei.", "tokens": ["Im", "All", "der", "Son\u00b7nen", "wal\u00b7ten", "der", "Kr\u00e4f\u00b7te", "zwei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "ART", "NN", "CARD", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Nach au\u00dfen dr\u00e4ngt die Eine; die Andre lockt", "tokens": ["Nach", "au\u00b7\u00dfen", "dr\u00e4ngt", "die", "Ei\u00b7ne", ";", "die", "And\u00b7re", "lockt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADV", "VVFIN", "ART", "NN", "$.", "ART", "PIS", "VVFIN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Zur\u00fcck zum Schwerpunkt. Solchem Zwist dankt", "tokens": ["Zu\u00b7r\u00fcck", "zum", "Schwer\u00b7punkt", ".", "Sol\u00b7chem", "Zwist", "dankt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "$.", "PIAT", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ordnung und Dauer der Sph\u00e4ren Prachtbau.", "tokens": ["Ord\u00b7nung", "und", "Dau\u00b7er", "der", "Sph\u00e4\u00b7ren", "Pracht\u00b7bau", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ART", "NN", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Zwei Kr\u00e4ft' auch sind es, welche dem Staat Bestand", "tokens": ["Zwei", "Kr\u00e4ft'", "auch", "sind", "es", ",", "wel\u00b7che", "dem", "Staat", "Be\u00b7stand"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["CARD", "NN", "ADV", "VAFIN", "PPER", "$,", "PRELS", "ART", "NN", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und Wohlfahrt sichern, schwebend im Gleichgewicht:", "tokens": ["Und", "Wohl\u00b7fahrt", "si\u00b7chern", ",", "schwe\u00b7bend", "im", "Gleich\u00b7ge\u00b7wicht", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVINF", "$,", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Des Helden Zorn, der Glimpf des Weisen,", "tokens": ["Des", "Hel\u00b7den", "Zorn", ",", "der", "Glimpf", "des", "Wei\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Spornend der Eine, der Andre z\u00fcgelnd.", "tokens": ["Spor\u00b7nend", "der", "Ei\u00b7ne", ",", "der", "And\u00b7re", "z\u00fc\u00b7gelnd", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "PRELS", "PIS", "VVPP", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Wem die\u00df, wie jenes liebend beschied der Gott,", "tokens": ["Wem", "die\u00df", ",", "wie", "je\u00b7nes", "lie\u00b7bend", "be\u00b7schied", "der", "Gott", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PDS", "$,", "PWAV", "PDS", "ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Heil solchem Reiche! Heil dir, Borussia!", "tokens": ["Heil", "sol\u00b7chem", "Rei\u00b7che", "!", "Heil", "dir", ",", "Bo\u00b7rus\u00b7sia", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NN", "PIAT", "NE", "$.", "NN", "PPER", "$,", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Gest\u00fctzt auf ", "tokens": ["Ge\u00b7st\u00fctzt", "auf"], "token_info": ["word", "word"], "pos": ["VVPP", "APPR"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Weicht nicht der K\u00f6nig vom Pfad der Mitte.", "tokens": ["Weicht", "nicht", "der", "K\u00f6\u00b7nig", "vom", "Pfad", "der", "Mit\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "ART", "NN", "APPRART", "NN", "ART", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.4": {"line.1": {"text": "Derselbe, wie auch wechs'le der Loose Fall,", "tokens": ["Der\u00b7sel\u00b7be", ",", "wie", "auch", "wechs'\u00b7le", "der", "Loo\u00b7se", "Fall", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "$,", "PWAV", "ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ruhig, ob schmeichl', ob drohe der Horizont,", "tokens": ["Ru\u00b7hig", ",", "ob", "schmeichl'", ",", "ob", "dro\u00b7he", "der", "Ho\u00b7ri\u00b7zont", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KOUS", "ADJD", "$,", "KOUS", "ADV", "ART", "NN", "$,"], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Ehrt Er den Thatendurst des Feldherrn,", "tokens": ["Ehrt", "Er", "den", "Tha\u00b7ten\u00b7durst", "des", "Feld\u00b7herrn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Achtend zugleich auf des Staatsmanns Mahnung.", "tokens": ["Ach\u00b7tend", "zu\u00b7gleich", "auf", "des", "Staats\u00b7manns", "Mah\u00b7nung", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "ADV", "APPR", "ART", "NN", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.5": {"line.1": {"text": "Sey uns gegr\u00fc\u00dft dann, weiser Canzlar des Reichs!", "tokens": ["Sey", "uns", "ge\u00b7gr\u00fc\u00dft", "dann", ",", "wei\u00b7ser", "Canz\u00b7lar", "des", "Reichs", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "ADV", "$,", "ADJA", "NN", "ART", "NN", "$."], "meter": "+--+-+-+--+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Ein wack'rer Steurer hast ", "tokens": ["Ein", "wack'\u00b7rer", "Steu\u00b7rer", "hast"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das schwerarbeitende, zum Hafen", "tokens": ["Das", "schwer\u00b7ar\u00b7bei\u00b7ten\u00b7de", ",", "zum", "Ha\u00b7fen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "$,", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Kr\u00e4ftig gelenkt durch Geklipp und Brandung.", "tokens": ["Kr\u00e4f\u00b7tig", "ge\u00b7lenkt", "durch", "Ge\u00b7klipp", "und", "Bran\u00b7dung", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "APPR", "NN", "KON", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.6": {"line.1": {"text": "Was Kaiser Friedrich ", "tokens": ["Was", "Kai\u00b7ser", "Fried\u00b7rich"], "token_info": ["word", "word", "word"], "pos": ["PWS", "NN", "NE"], "meter": "-+---", "measure": "dactylic.init"}, "line.2": {"text": "Was seinem Ludwig ", "tokens": ["Was", "sei\u00b7nem", "Lud\u00b7wig"], "token_info": ["word", "word", "word"], "pos": ["PWS", "PPOSAT", "NE"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Was ", "tokens": ["Was"], "token_info": ["word"], "pos": ["PWS"], "meter": "-", "measure": "single.down"}, "line.4": {"text": "Warst ", "tokens": ["Warst"], "token_info": ["word"], "pos": ["VAFIN"], "meter": "+", "measure": "single.up"}}, "stanza.7": {"line.1": {"text": "Bis auch der Wunden letzte zur Narbe wird,", "tokens": ["Bis", "auch", "der", "Wun\u00b7den", "letz\u00b7te", "zur", "Nar\u00b7be", "wird", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ART", "NN", "ADJA", "APPRART", "NN", "VAFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Daraus Europa blutet; bis Ost und West", "tokens": ["Da\u00b7raus", "Eu\u00b7ro\u00b7pa", "blu\u00b7tet", ";", "bis", "Ost", "und", "West"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PAV", "NE", "VVFIN", "$.", "APPR", "NN", "KON", "NN"], "meter": "---+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Des m\u00fcden Welttheils tiefaufathmend", "tokens": ["Des", "m\u00fc\u00b7den", "Welt\u00b7theils", "tie\u00b7fauf\u00b7ath\u00b7mend"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Schlummern im Schatten des ", "tokens": ["Schlum\u00b7mern", "im", "Schat\u00b7ten", "des"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPRART", "NN", "ART"], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.8": {"line.1": {"text": "Inde\u00df sey hold uns, die wir das deutsche Herz", "tokens": ["In\u00b7de\u00df", "sey", "hold", "uns", ",", "die", "wir", "das", "deut\u00b7sche", "Herz"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "ADJD", "PPER", "$,", "PRELS", "PPER", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Sorgsam bewahrten, sa\u00df gleich der K\u00f6nig uns", "tokens": ["Sorg\u00b7sam", "be\u00b7wahr\u00b7ten", ",", "sa\u00df", "gleich", "der", "K\u00f6\u00b7nig", "uns"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "$,", "VVFIN", "ADV", "ART", "NN", "PPER"], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Jenseit der Wasser, lag gleich fern uns", "tokens": ["Jen\u00b7seit", "der", "Was\u00b7ser", ",", "lag", "gleich", "fern", "uns"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "VVFIN", "ADV", "ADJD", "PPER"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Frohes und Tr\u00fcbes der Bruderst\u00e4mme.", "tokens": ["Fro\u00b7hes", "und", "Tr\u00fc\u00b7bes", "der", "Bru\u00b7der\u00b7st\u00e4m\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "KON", "NN", "ART", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.9": {"line.1": {"text": "Sey hold uns, ", "tokens": ["Sey", "hold", "uns", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "PPER", "$,"], "meter": "+--", "measure": "dactylic.init"}, "line.2": {"text": "Rathspendend sitzest! Werd' uns ein Genius!", "tokens": ["Rath\u00b7spen\u00b7dend", "sit\u00b7zest", "!", "Werd'", "uns", "ein", "Ge\u00b7nius", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$.", "VAFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Zum B\u00fcrger adle den Privatmann!", "tokens": ["Zum", "B\u00fcr\u00b7ger", "ad\u00b7le", "den", "Pri\u00b7vat\u00b7mann", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Steigr' uns die Heimath zum Vaterlande!", "tokens": ["Steigr'", "uns", "die", "Hei\u00b7math", "zum", "Va\u00b7ter\u00b7lan\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPRART", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.10": {"line.1": {"text": "Samml' um des Thrones Stufen den V\u00f6lkerrath!", "tokens": ["Samml'", "um", "des", "Thro\u00b7nes", "Stu\u00b7fen", "den", "V\u00f6l\u00b7ker\u00b7rath", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Gut, Ehr' und Leben sichr' uns das ", "tokens": ["Gut", ",", "Ehr'", "und", "Le\u00b7ben", "sichr'", "uns", "das"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "$,", "NN", "KON", "NN", "VVFIN", "PPER", "ART"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Oeffn' aller Kraft der Thaten Rennbahn!", "tokens": ["Oeffn'", "al\u00b7ler", "Kraft", "der", "Tha\u00b7ten", "Renn\u00b7bahn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Keinem Verdienst sey versagt die Palme!", "tokens": ["Kei\u00b7nem", "Ver\u00b7dienst", "sey", "ver\u00b7sagt", "die", "Pal\u00b7me", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "VVPP", "ART", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.11": {"line.1": {"text": "Der Boden eigne dem, der den Boden baut!", "tokens": ["Der", "Bo\u00b7den", "eig\u00b7ne", "dem", ",", "der", "den", "Bo\u00b7den", "baut", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Die Marken h\u00fcte, welchen die Mark ges\u00e4ugt!", "tokens": ["Die", "Mar\u00b7ken", "h\u00fc\u00b7te", ",", "wel\u00b7chen", "die", "Mark", "ge\u00b7s\u00e4ugt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PWAT", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Heer sey das Volk; die Schule Stoa;", "tokens": ["Heer", "sey", "das", "Volk", ";", "die", "Schu\u00b7le", "Stoa", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$.", "ART", "NN", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Areopagus die Pfalz des Rechtes!", "tokens": ["A\u00b7reo\u00b7pa\u00b7gus", "die", "Pfalz", "des", "Rech\u00b7tes", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Der Geister Aufschwung l\u00e4hme kein Machtgebot!", "tokens": ["Der", "Geis\u00b7ter", "Auf\u00b7schwung", "l\u00e4h\u00b7me", "kein", "Macht\u00b7ge\u00b7bot", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Die freie Type fess'le kein Zwanggesetz!", "tokens": ["Die", "frei\u00b7e", "Ty\u00b7pe", "fess'\u00b7le", "kein", "Zwang\u00b7ge\u00b7setz", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Dem H\u00f6hern huld'ge Kron' und Inful!", "tokens": ["Dem", "H\u00f6\u00b7hern", "huld'\u00b7ge", "Kron'", "und", "In\u00b7ful", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Innigst durchdringe sich Staat und Kirche! ...", "tokens": ["In\u00b7nigst", "durch\u00b7drin\u00b7ge", "sich", "Staat", "und", "Kir\u00b7che", "!", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PRF", "NN", "KON", "NN", "$.", "$("], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.13": {"line.1": {"text": "Der Vorwelt Aeren, g\u00fcld'ne Jahrhunderte", "tokens": ["Der", "Vor\u00b7welt", "A\u00b7e\u00b7ren", ",", "g\u00fcld'\u00b7ne", "Jahr\u00b7hun\u00b7der\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "NN", "$,", "ADJA", "NN"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Des, dem Athen, dem Roma den Glanz verdankt,", "tokens": ["Des", ",", "dem", "A\u00b7then", ",", "dem", "Ro\u00b7ma", "den", "Glanz", "ver\u00b7dankt", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "ART", "NE", "$,", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "Dem Bagdad, dem die sch\u00f6ne Florenz,", "tokens": ["Dem", "Bag\u00b7dad", ",", "dem", "die", "sch\u00f6\u00b7ne", "Flo\u00b7renz", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "$,", "PRELS", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+