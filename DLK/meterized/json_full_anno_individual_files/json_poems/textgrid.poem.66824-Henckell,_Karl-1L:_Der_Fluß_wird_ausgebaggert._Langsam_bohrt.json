{"textgrid.poem.66824": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der Flu\u00df wird ausgebaggert. Langsam bohrt", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Flu\u00df wird ausgebaggert. Langsam bohrt", "tokens": ["Der", "Flu\u00df", "wird", "aus\u00b7ge\u00b7bag\u00b7gert", ".", "Lang\u00b7sam", "bohrt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$.", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der leeren Kupferkiepen Rollradkette", "tokens": ["Der", "lee\u00b7ren", "Kup\u00b7fer\u00b7kie\u00b7pen", "Roll\u00b7rad\u00b7ket\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sich in der seichten Flut verschlammtes Bette,", "tokens": ["Sich", "in", "der", "seich\u00b7ten", "Flut", "ver\u00b7schlamm\u00b7tes", "Bet\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Es kreischt und quietscht, es rasselt und rumort.", "tokens": ["Es", "kreischt", "und", "quietscht", ",", "es", "ras\u00b7selt", "und", "ru\u00b7mort", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "$,", "PPER", "VVFIN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Mit Kies gef\u00fcllt steigt Korb nach Korb und wirft", "tokens": ["Mit", "Kies", "ge\u00b7f\u00fcllt", "steigt", "Korb", "nach", "Korb", "und", "wirft"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVPP", "VVFIN", "NN", "APPR", "NN", "KON", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Die Steinlast in die breite Bretterrinne,", "tokens": ["Die", "Stein\u00b7last", "in", "die", "brei\u00b7te", "Bret\u00b7ter\u00b7rin\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Die in das Frachtboot l\u00e4uft. Und wieder sch\u00fcrft", "tokens": ["Die", "in", "das", "Fracht\u00b7boot", "l\u00e4uft", ".", "Und", "wie\u00b7der", "sch\u00fcrft"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "$.", "KON", "ADV", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Und w\u00fchlt's im Grund, und wieder schleppt's zur Zinne.", "tokens": ["Und", "w\u00fchlt's", "im", "Grund", ",", "und", "wie\u00b7der", "schleppt's", "zur", "Zin\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "$,", "KON", "ADV", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Der Eisenschornstein raucht. Und Kohlen schaufelt", "tokens": ["Der", "Ei\u00b7sen\u00b7schorn\u00b7stein", "raucht", ".", "Und", "Koh\u00b7len", "schau\u00b7felt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$.", "KON", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Der Heizer in den Ofen der Maschine.", "tokens": ["Der", "Hei\u00b7zer", "in", "den", "O\u00b7fen", "der", "Ma\u00b7schi\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Der Vorrat schmilzt. Aus flachem Lastkahn haufelt", "tokens": ["Der", "Vor\u00b7rat", "schmilzt", ".", "Aus", "fla\u00b7chem", "Last\u00b7kahn", "hau\u00b7felt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$.", "APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Er frische Kohlen auf, der schwarze H\u00fcne ...", "tokens": ["Er", "fri\u00b7sche", "Koh\u00b7len", "auf", ",", "der", "schwar\u00b7ze", "H\u00fc\u00b7ne", "..."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "PTKVZ", "$,", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Was wird da mit dem Schlamm und mit den Steinen", "tokens": ["Was", "wird", "da", "mit", "dem", "Schlamm", "und", "mit", "den", "Stei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "ADV", "APPR", "ART", "NN", "KON", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dem k\u00fchlen Scho\u00df und Schlummer nicht geraubt!", "tokens": ["Dem", "k\u00fch\u00b7len", "Scho\u00df", "und", "Schlum\u00b7mer", "nicht", "ge\u00b7raubt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "NN", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ein alter Feldstuhl mit zerbrochnen Beinen,", "tokens": ["Ein", "al\u00b7ter", "Feld\u00b7stuhl", "mit", "zer\u00b7broch\u00b7nen", "Bei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein Kaffeek\u00e4nnchen, ohne Henk und Haupt.", "tokens": ["Ein", "Kaf\u00b7fee\u00b7k\u00e4nn\u00b7chen", ",", "oh\u00b7ne", "Henk", "und", "Haupt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUI", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ein ", "tokens": ["Ein"], "token_info": ["word"], "pos": ["ART"], "meter": "+", "measure": "single.up"}, "line.6": {"text": "Wer warf den wohl so mitten durch entzwei?", "tokens": ["Wer", "warf", "den", "wohl", "so", "mit\u00b7ten", "durch", "ent\u00b7zwei", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "ADV", "ADV", "ADV", "APPR", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Ging's einem Ehmann damit an den Kragen?", "tokens": ["Ging's", "ei\u00b7nem", "Eh\u00b7mann", "da\u00b7mit", "an", "den", "Kra\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "PAV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Sardellenb\u00fcchsen, Kruken wirft der Brei.", "tokens": ["Sar\u00b7del\u00b7len\u00b7b\u00fcch\u00b7sen", ",", "Kru\u00b7ken", "wirft", "der", "Brei", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.9": {"text": "Ein Schirm in Fetzen, dem die Stangen fehlen,", "tokens": ["Ein", "Schirm", "in", "Fet\u00b7zen", ",", "dem", "die", "Stan\u00b7gen", "feh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Ein eingetriebener Zylinderhut ...", "tokens": ["Ein", "ein\u00b7ge\u00b7trie\u00b7be\u00b7ner", "Zy\u00b7lin\u00b7der\u00b7hut", "..."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$("], "meter": "-+-+---+-+", "measure": "zehnsilber"}, "line.11": {"text": "Den k\u00f6nnte ein Hanswurst sich passend stehlen,", "tokens": ["Den", "k\u00f6nn\u00b7te", "ein", "Hans\u00b7wurst", "sich", "pas\u00b7send", "steh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "ART", "NN", "PRF", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Getrocknet w\u00e4r' er noch zum Fasching gut.", "tokens": ["Ge\u00b7trock\u00b7net", "w\u00e4r'", "er", "noch", "zum", "Fa\u00b7sching", "gut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "ADV", "APPRART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Ein Bruchband, Hosentr\u00e4ger, Scherben, Dr\u00e4hte", "tokens": ["Ein", "Bruch\u00b7band", ",", "Ho\u00b7sen\u00b7tr\u00e4\u00b7ger", ",", "Scher\u00b7ben", ",", "Dr\u00e4h\u00b7te"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["ART", "NN", "$,", "NN", "$,", "NN", "$,", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Und all dergleichen Leib- und Hausger\u00e4te ...", "tokens": ["Und", "all", "derg\u00b7lei\u00b7chen", "Leib", "und", "Haus\u00b7ge\u00b7r\u00e4\u00b7te", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "PIS", "TRUNC", "KON", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Die Leute, eh sie zum Gesch\u00e4fte gehn,", "tokens": ["Die", "Leu\u00b7te", ",", "eh", "sie", "zum", "Ge\u00b7sch\u00e4f\u00b7te", "gehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "PPER", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "H\u00f6ren den L\u00e4rm und bleiben gern mal stehn.", "tokens": ["H\u00f6\u00b7ren", "den", "L\u00e4rm", "und", "blei\u00b7ben", "gern", "mal", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "KON", "VVFIN", "ADV", "ADV", "VVINF", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Besonders wenn die Mittagssonne scheint", "tokens": ["Be\u00b7son\u00b7ders", "wenn", "die", "Mit\u00b7tags\u00b7son\u00b7ne", "scheint"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und mit dem Erdgew\u00fcrm es gn\u00e4dig meint.", "tokens": ["Und", "mit", "dem", "Erd\u00b7ge\u00b7w\u00fcrm", "es", "gn\u00e4\u00b7dig", "meint", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Das Schurzfell lehnt sich eine kleine Zeit", "tokens": ["Das", "Schurz\u00b7fell", "lehnt", "sich", "ei\u00b7ne", "klei\u00b7ne", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PRF", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Ans Steingel\u00e4nder mit gekreuzten Beinen;", "tokens": ["Ans", "Stein\u00b7ge\u00b7l\u00e4n\u00b7der", "mit", "ge\u00b7kreuz\u00b7ten", "Bei\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Der Gl\u00fcckliche, der Geld auf Zinsen leiht,", "tokens": ["Der", "Gl\u00fcck\u00b7li\u00b7che", ",", "der", "Geld", "auf", "Zin\u00b7sen", "leiht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Schl\u00e4gt Schneckenschritt an, mit ihm seine Kleinen,", "tokens": ["Schl\u00e4gt", "Schne\u00b7cken\u00b7schritt", "an", ",", "mit", "ihm", "sei\u00b7ne", "Klei\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "PTKVZ", "$,", "APPR", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Ein Schulbub, der Mechanik lernen soll,", "tokens": ["Ein", "Schul\u00b7bub", ",", "der", "Me\u00b7cha\u00b7nik", "ler\u00b7nen", "soll", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Betrachtet aufmerksam das Seilgeroll ...", "tokens": ["Be\u00b7trach\u00b7tet", "auf\u00b7merk\u00b7sam", "das", "Seil\u00b7ge\u00b7roll", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "NN", "$("], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.4": {"line.1": {"text": "Ich klopfe mir den Staub vom Ellenbogen", "tokens": ["Ich", "klop\u00b7fe", "mir", "den", "Staub", "vom", "El\u00b7len\u00b7bo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und trolle mich so recht gemach nach Haus,", "tokens": ["Und", "trol\u00b7le", "mich", "so", "recht", "ge\u00b7mach", "nach", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADJD", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Symbolisiere, da\u00df die Locken wogen,", "tokens": ["Sym\u00b7bo\u00b7li\u00b7sie\u00b7re", ",", "da\u00df", "die", "Lo\u00b7cken", "wo\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Von Seelenschlamm und baggre lustig aus.", "tokens": ["Von", "See\u00b7len\u00b7schlamm", "und", "bagg\u00b7re", "lus\u00b7tig", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "VVFIN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "\u00bbder sch\u00f6ne Flu\u00df des Lebens ist versandet,", "tokens": ["\u00bb", "der", "sch\u00f6\u00b7ne", "Flu\u00df", "des", "Le\u00b7bens", "ist", "ver\u00b7san\u00b7det", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "ADJA", "NN", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Der Heizer heizt. Die Reinigung beginnt.", "tokens": ["Der", "Hei\u00b7zer", "heizt", ".", "Die", "Rei\u00b7ni\u00b7gung", "be\u00b7ginnt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Bald ist der Krimskrams 'raus. Und jeder landet", "tokens": ["Bald", "ist", "der", "Krims\u00b7krams", "'raus", ".", "Und", "je\u00b7der", "lan\u00b7det"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "NE", "$.", "KON", "PIS", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und dampft nach Ufern, die ihm teuer sind.\u00ab", "tokens": ["Und", "dampft", "nach", "U\u00b7fern", ",", "die", "ihm", "teu\u00b7er", "sind", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "$,", "PRELS", "PPER", "ADJD", "VAFIN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Der Flu\u00df wird ausgebaggert. Langsam bohrt", "tokens": ["Der", "Flu\u00df", "wird", "aus\u00b7ge\u00b7bag\u00b7gert", ".", "Lang\u00b7sam", "bohrt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$.", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der leeren Kupferkiepen Rollradkette", "tokens": ["Der", "lee\u00b7ren", "Kup\u00b7fer\u00b7kie\u00b7pen", "Roll\u00b7rad\u00b7ket\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sich in der seichten Flut verschlammtes Bette,", "tokens": ["Sich", "in", "der", "seich\u00b7ten", "Flut", "ver\u00b7schlamm\u00b7tes", "Bet\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Es kreischt und quietscht, es rasselt und rumort.", "tokens": ["Es", "kreischt", "und", "quietscht", ",", "es", "ras\u00b7selt", "und", "ru\u00b7mort", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "$,", "PPER", "VVFIN", "KON", "NN", "$."], "meter": "