{"textgrid.poem.33375": {"metadata": {"author": {"name": "Blumauer, Aloys", "birth": "N.A.", "death": "N.A."}, "title": "Die Wahl", "genre": "verse", "period": "N.A.", "pub_year": 1776, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Einst stritten Cybele und Zeus sich in die Wette:", "tokens": ["Einst", "strit\u00b7ten", "Cy\u00b7be\u00b7le", "und", "Zeus", "sich", "in", "die", "Wet\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "KON", "NE", "PRF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wer an der Menschen Gl\u00fcck mehr Antheil h\u00e4tte,", "tokens": ["Wer", "an", "der", "Men\u00b7schen", "Gl\u00fcck", "mehr", "An\u00b7theil", "h\u00e4t\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "NN", "PIAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Gattin, oder Mutter? Jupiter", "tokens": ["Die", "Gat\u00b7tin", ",", "o\u00b7der", "Mut\u00b7ter", "?", "Ju\u00b7pi\u00b7ter"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ART", "NN", "$,", "KON", "NN", "$.", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "War f\u00fcr die gute Gattin mehr,", "tokens": ["War", "f\u00fcr", "die", "gu\u00b7te", "Gat\u00b7tin", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und Cybele ereiferte sich sehr,", "tokens": ["Und", "Cy\u00b7be\u00b7le", "er\u00b7ei\u00b7fer\u00b7te", "sich", "sehr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VVFIN", "PRF", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Da\u00df f\u00fcr der Menschheit Gl\u00fcck und Ehre", "tokens": ["Da\u00df", "f\u00fcr", "der", "Menschheit", "Gl\u00fcck", "und", "Eh\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "NN", "KON", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Die gute Mutter wichtiger,", "tokens": ["Die", "gu\u00b7te", "Mut\u00b7ter", "wich\u00b7ti\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Als eine gute Gattin w\u00e4re.", "tokens": ["Als", "ei\u00b7ne", "gu\u00b7te", "Gat\u00b7tin", "w\u00e4\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Nun gut, sprach Jupiter, wir wollen seh'n,", "tokens": ["Nun", "gut", ",", "sprach", "Ju\u00b7pi\u00b7ter", ",", "wir", "wol\u00b7len", "seh'n", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "VVFIN", "NE", "$,", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Wer Recht beh\u00e4lt! So la\u00df uns denn", "tokens": ["Wer", "Recht", "be\u00b7h\u00e4lt", "!", "So", "la\u00df", "uns", "denn"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "NN", "VVFIN", "$.", "ADV", "VVIMP", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Von neugebornen M\u00e4dchenseelen", "tokens": ["Von", "neu\u00b7ge\u00b7bor\u00b7nen", "M\u00e4d\u00b7chen\u00b7see\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Ein Paar der Besten zum Versuche w\u00e4hlen;", "tokens": ["Ein", "Paar", "der", "Bes\u00b7ten", "zum", "Ver\u00b7su\u00b7che", "w\u00e4h\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Ich suche mir das beste M\u00e4dchen aus,", "tokens": ["Ich", "su\u00b7che", "mir", "das", "bes\u00b7te", "M\u00e4d\u00b7chen", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Und mache dir die beste Frau daraus;", "tokens": ["Und", "ma\u00b7che", "dir", "die", "bes\u00b7te", "Frau", "da\u00b7raus", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "ADJA", "NN", "PAV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Und meines soll, rief Cybele, die Gaben", "tokens": ["Und", "mei\u00b7nes", "soll", ",", "rief", "Cy\u00b7be\u00b7le", ",", "die", "Ga\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KON", "PPOSAT", "VMFIN", "$,", "VVFIN", "NE", "$,", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Der besten Mutter alle haben. \u2013", "tokens": ["Der", "bes\u00b7ten", "Mut\u00b7ter", "al\u00b7le", "ha\u00b7ben", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "PIS", "VAINF", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Sie w\u00e4hlten beide aus der M\u00e4dchen Zahl,", "tokens": ["Sie", "w\u00e4hl\u00b7ten", "bei\u00b7de", "aus", "der", "M\u00e4d\u00b7chen", "Zahl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Doch wu\u00dfte keines von des andern Wahl:", "tokens": ["Doch", "wu\u00df\u00b7te", "kei\u00b7nes", "von", "des", "an\u00b7dern", "Wahl", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Und jedes sah mit innigem Vergn\u00fcgen", "tokens": ["Und", "je\u00b7des", "sah", "mit", "in\u00b7ni\u00b7gem", "Ver\u00b7gn\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.20": {"text": "Den Keim des Sieg's in der Gew\u00e4hlten liegen.", "tokens": ["Den", "Keim", "des", "Sieg's", "in", "der", "Ge\u00b7w\u00e4hl\u00b7ten", "lie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Zur besten Frau f\u00fcr einen edlen Mann", "tokens": ["Zur", "bes\u00b7ten", "Frau", "f\u00fcr", "ei\u00b7nen", "ed\u00b7len", "Mann"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Wuchs Zeus Gew\u00e4hlte nun heran,", "tokens": ["Wuchs", "Zeus", "Ge\u00b7w\u00e4hl\u00b7te", "nun", "he\u00b7ran", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Und die der Cybele versprach nicht minder", "tokens": ["Und", "die", "der", "Cy\u00b7be\u00b7le", "ver\u00b7sprach", "nicht", "min\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ART", "NN", "VVFIN", "PTKNEG", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "Die beste Mutter ihrer Kinder.", "tokens": ["Die", "bes\u00b7te", "Mut\u00b7ter", "ih\u00b7rer", "Kin\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Ob dem Erfolg, den beider Wahl verhie\u00df,", "tokens": ["Ob", "dem", "Er\u00b7folg", ",", "den", "bei\u00b7der", "Wahl", "ver\u00b7hie\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,", "ART", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.26": {"text": "War jedes seines Sieg's gewi\u00df.", "tokens": ["War", "je\u00b7des", "sei\u00b7nes", "Sieg's", "ge\u00b7wi\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Kaum war nun Zeus gew\u00e4hlte Frau,", "tokens": ["Kaum", "war", "nun", "Zeus", "ge\u00b7w\u00e4hl\u00b7te", "Frau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "NE", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "So f\u00fchrt' er Cybelen, von stolzem Selbstvertrauen,", "tokens": ["So", "f\u00fchrt'", "er", "Cy\u00b7be\u00b7len", ",", "von", "stol\u00b7zem", "Selbst\u00b7ver\u00b7trau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+---+-+-+-", "measure": "unknown.measure.penta"}, "line.29": {"text": "Zu seinem Meisterst\u00fccke hin zur Schau,", "tokens": ["Zu", "sei\u00b7nem", "Meis\u00b7ter\u00b7st\u00fc\u00b7cke", "hin", "zur", "Schau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.30": {"text": "Und wies ihr da das Muster aller Frauen.", "tokens": ["Und", "wies", "ihr", "da", "das", "Mus\u00b7ter", "al\u00b7ler", "Frau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ART", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.31": {"text": "Die G\u00f6ttin fuhr zur\u00fcck mit sch\u00fcchternen Geberden,", "tokens": ["Die", "G\u00f6t\u00b7tin", "fuhr", "zu\u00b7r\u00fcck", "mit", "sch\u00fcch\u00b7ter\u00b7nen", "Ge\u00b7ber\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+---+-", "measure": "unknown.measure.penta"}, "line.32": {"text": "Doch fa\u00dfte sie sich bald und sprach:", "tokens": ["Doch", "fa\u00df\u00b7te", "sie", "sich", "bald", "und", "sprach", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PRF", "ADV", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "La\u00df erst die Meine Mutter werden,", "tokens": ["La\u00df", "erst", "die", "Mei\u00b7ne", "Mut\u00b7ter", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "ART", "PPOSAT", "NN", "VAINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.34": {"text": "Eh' geb' ich, Stolzer, dir nicht nach!", "tokens": ["Eh'", "geb'", "ich", ",", "Stol\u00b7zer", ",", "dir", "nicht", "nach", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$,", "NN", "$,", "PPER", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.35": {"text": "Sie ward's; da f\u00fchrte, voll Entz\u00fccken,", "tokens": ["Sie", "ward's", ";", "da", "f\u00fchr\u00b7te", ",", "voll", "Ent\u00b7z\u00fc\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$.", "ADV", "VVFIN", "$,", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.36": {"text": "Die G\u00f6ttin Jupiter'n dahin,", "tokens": ["Die", "G\u00f6t\u00b7tin", "Ju\u00b7pi\u00b7ter'n", "da\u00b7hin", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.37": {"text": "Und zeigte l\u00e4chelnd seinen Blicken", "tokens": ["Und", "zeig\u00b7te", "l\u00e4\u00b7chelnd", "sei\u00b7nen", "Bli\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.38": {"text": "Die holde Kinderpflegerin.", "tokens": ["Die", "hol\u00b7de", "Kin\u00b7der\u00b7pfle\u00b7ge\u00b7rin", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.39": {"text": "Wer, sprach sie, hat nun hier verloren,", "tokens": ["Wer", ",", "sprach", "sie", ",", "hat", "nun", "hier", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "$,", "VVFIN", "PPER", "$,", "VAFIN", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.40": {"text": "Du siehst in Einer Beydes hier;", "tokens": ["Du", "siehst", "in", "Ei\u00b7ner", "Bey\u00b7des", "hier", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.41": {"text": "Die du zur Gattin dir erkohren,", "tokens": ["Die", "du", "zur", "Gat\u00b7tin", "dir", "er\u00b7koh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.42": {"text": "W\u00e4hlt' ich zur besten Mutter mir! \u2013", "tokens": ["W\u00e4hlt'", "ich", "zur", "bes\u00b7ten", "Mut\u00b7ter", "mir", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "APPRART", "ADJA", "NN", "PPER", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.43": {"text": "Und beide sah'n im sch\u00f6nsten Bunde", "tokens": ["Und", "bei\u00b7de", "sah'n", "im", "sch\u00f6ns\u00b7ten", "Bun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.44": {"text": "Die Mutter mit der Frau vereint,", "tokens": ["Die", "Mut\u00b7ter", "mit", "der", "Frau", "ver\u00b7eint", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.45": {"text": "Und beide segneten die Stunde,", "tokens": ["Und", "bei\u00b7de", "seg\u00b7ne\u00b7ten", "die", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.46": {"text": "Die ihre Wahl in Dir vereint.", "tokens": ["Die", "ih\u00b7re", "Wahl", "in", "Dir", "ver\u00b7eint", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Einst stritten Cybele und Zeus sich in die Wette:", "tokens": ["Einst", "strit\u00b7ten", "Cy\u00b7be\u00b7le", "und", "Zeus", "sich", "in", "die", "Wet\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "KON", "NE", "PRF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wer an der Menschen Gl\u00fcck mehr Antheil h\u00e4tte,", "tokens": ["Wer", "an", "der", "Men\u00b7schen", "Gl\u00fcck", "mehr", "An\u00b7theil", "h\u00e4t\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "NN", "PIAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Gattin, oder Mutter? Jupiter", "tokens": ["Die", "Gat\u00b7tin", ",", "o\u00b7der", "Mut\u00b7ter", "?", "Ju\u00b7pi\u00b7ter"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ART", "NN", "$,", "KON", "NN", "$.", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "War f\u00fcr die gute Gattin mehr,", "tokens": ["War", "f\u00fcr", "die", "gu\u00b7te", "Gat\u00b7tin", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und Cybele ereiferte sich sehr,", "tokens": ["Und", "Cy\u00b7be\u00b7le", "er\u00b7ei\u00b7fer\u00b7te", "sich", "sehr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON