{"dta.poem.20173": {"metadata": {"author": {"name": "Neumark, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Der Lieb-erfreute  \n  FJLAMON .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1666", "urn": "urn:nbn:de:kobv:b4-20424-1", "language": ["de:0.99"], "booktitle": "Neumark, Georg: Poetisch-Historischer Lustgarten. Frankfurt (Main), 1666."}, "poem": {"stanza.1": {"line.1": {"text": "Solstu Cupido im Himmels-Ban sitzen", "tokens": ["Sols\u00b7tu", "Cu\u00b7pi\u00b7do", "im", "Him\u00b7mels\u00b7Ban", "sit\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "NE", "APPRART", "NN", "VVINF"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Unter der G\u00f6tter unsterblichen Schaar/", "tokens": ["Un\u00b7ter", "der", "G\u00f6t\u00b7ter", "uns\u00b7terb\u00b7li\u00b7chen", "Schaar", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$("], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Sol ich das gl\u00e4uben? es ist ja nicht war.", "tokens": ["Sol", "ich", "das", "gl\u00e4u\u00b7ben", "?", "es", "ist", "ja", "nicht", "war", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PDS", "VVINF", "$.", "PPER", "VAFIN", "ADV", "PTKNEG", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Sitzestu nicht mit dem Bogen und Spitzen/", "tokens": ["Sit\u00b7ze\u00b7stu", "nicht", "mit", "dem", "Bo\u00b7gen", "und", "Spit\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "APPR", "ART", "NN", "KON", "NN", "$("], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.5": {"text": "Welche mit Liebe die Hertzen erhitzen/", "tokens": ["Wel\u00b7che", "mit", "Lie\u00b7be", "die", "Hert\u00b7zen", "er\u00b7hit\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "APPR", "NN", "ART", "NN", "VVINF", "$("], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.6": {"text": "(wie ich befunden mit grosser Gefahr)", "tokens": ["(", "wie", "ich", "be\u00b7fun\u00b7den", "mit", "gros\u00b7ser", "Ge\u00b7fahr", ")"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "PPER", "VVPP", "APPR", "ADJA", "NN", "$("], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "In der Bellifloren Aeugelein-Paar/", "tokens": ["In", "der", "Bell\u00b7if\u00b7lo\u00b7ren", "A\u00b7eu\u00b7ge\u00b7lein\u00b7Paar", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$("], "meter": "--+-+-+---+", "measure": "anapaest.init"}, "line.8": {"text": "Welche der Sonnen gleich gl\u00e4ntzen und blitzen?", "tokens": ["Wel\u00b7che", "der", "Son\u00b7nen", "gleich", "gl\u00e4nt\u00b7zen", "und", "blit\u00b7zen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "ADV", "VVINF", "KON", "VVFIN", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.9": {"text": "So es nicht wahr ist wie kommet es dann", "tokens": ["So", "es", "nicht", "wahr", "ist", "wie", "kom\u00b7met", "es", "dann"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "PTKNEG", "ADJD", "VAFIN", "KOKOM", "VVFIN", "PPER", "ADV"], "meter": "---+--+--+", "measure": "iambic.tri.relaxed"}, "line.10": {"text": "Wenn mich Belliflora blikket nur an/", "tokens": ["Wenn", "mich", "Bell\u00b7if\u00b7lo\u00b7ra", "blik\u00b7ket", "nur", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NE", "VVFIN", "ADV", "PTKVZ", "$("], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.11": {"text": "Da\u00df ich dann allezeit werde geschossen?", "tokens": ["Da\u00df", "ich", "dann", "al\u00b7le\u00b7zeit", "wer\u00b7de", "ge\u00b7schos\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Zwar in dem Himmel wol g\u00f6ttlich du bist.", "tokens": ["Zwar", "in", "dem", "Him\u00b7mel", "wol", "g\u00f6tt\u00b7lich", "du", "bist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ADV", "ADJD", "PPER", "VAFIN", "$."], "meter": "-+-+-+---+", "measure": "zehnsilber"}, "line.13": {"text": "In ihren Aeugelein aber die List", "tokens": ["In", "ih\u00b7ren", "A\u00b7e\u00b7u\u00b7ge\u00b7lein", "a\u00b7ber", "die", "List"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "ART", "NN"], "meter": "-+-+-+-++--+", "measure": "iambic.hexa.chol"}, "line.14": {"text": "Menschen zu fallen/ ist stetes verschlossen.", "tokens": ["Men\u00b7schen", "zu", "fal\u00b7len", "/", "ist", "ste\u00b7tes", "ver\u00b7schlos\u00b7sen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VVINF", "$(", "VAFIN", "ADJA", "VVPP", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}}}}}