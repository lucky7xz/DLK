{"dta.poem.4262": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die  Assembl\u00e9e.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "So bald man in das W\u00e4ldchen tritt, liegt eine Laube", "tokens": ["So", "bald", "man", "in", "das", "W\u00e4ld\u00b7chen", "tritt", ",", "liegt", "ei\u00b7ne", "Lau\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADV", "PIS", "APPR", "ART", "NN", "VVFIN", "$,", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "linker Hand,", "tokens": ["lin\u00b7ker", "Hand", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Die, wegen ihres weiten Umfangs, die ", "tokens": ["Die", ",", "we\u00b7gen", "ih\u00b7res", "wei\u00b7ten", "Um\u00b7fangs", ",", "die"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["ART", "$,", "APPR", "PPOSAT", "ADJA", "NN", "$,", "PRELS"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.4": {"text": "genannt,", "tokens": ["ge\u00b7nannt", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.5": {"text": "Jm Schatten vieler grossen Eichen, wovon an einer son-", "tokens": ["Jm", "Schat\u00b7ten", "vie\u00b7ler", "gros\u00b7sen", "Ei\u00b7chen", ",", "wo\u00b7von", "an", "ei\u00b7ner", "son"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "PIAT", "ADJA", "NN", "$,", "PWAV", "APPR", "ART", "TRUNC"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.6": {"text": "derlich", "tokens": ["der\u00b7lich"], "token_info": ["word"], "pos": ["ADV"], "meter": "--", "measure": "unknown.measure.zero"}, "line.7": {"text": "Die knorrichten bemoo\u00dften Aeste, in einer weiten R\u00fcnde,", "tokens": ["Die", "knor\u00b7rich\u00b7ten", "be\u00b7moo\u00df\u00b7ten", "A\u00b7es\u00b7te", ",", "in", "ei\u00b7ner", "wei\u00b7ten", "R\u00fcn\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-++--+-+-+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}, "line.8": {"text": "sich,", "tokens": ["sich", ","], "token_info": ["word", "punct"], "pos": ["PRF", "$,"], "meter": "-", "measure": "single.down"}, "line.9": {"text": "Mit tausend Wend- und Biegungen, gewunden und gedre-", "tokens": ["Mit", "tau\u00b7send", "Wen\u00b7d", "und", "Bie\u00b7gun\u00b7gen", ",", "ge\u00b7wun\u00b7den", "und", "ge\u00b7dre"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "CARD", "TRUNC", "KON", "NN", "$,", "ADJD", "KON", "TRUNC"], "meter": "-+-+--+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.10": {"text": "het strecken,", "tokens": ["het", "stre\u00b7cken", ","], "token_info": ["word", "word", "punct"], "pos": ["VAFIN", "VVINF", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.11": {"text": "Und eine runde Rasen-Bank, am Fu\u00df der runden Laube,", "tokens": ["Und", "ei\u00b7ne", "run\u00b7de", "Ra\u00b7sen\u00b7Bank", ",", "am", "Fu\u00df", "der", "run\u00b7den", "Lau\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$,", "APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.12": {"text": "decken.", "tokens": ["de\u00b7cken", "."], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.13": {"text": "Rings um derselben weiten Cirkel steht um der B\u00e4ume", "tokens": ["Rings", "um", "der\u00b7sel\u00b7ben", "wei\u00b7ten", "Cir\u00b7kel", "steht", "um", "der", "B\u00e4u\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "PDAT", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "+--+-+-+-+--+-", "measure": "hexameter"}, "line.14": {"text": "Stamm gewunden,", "tokens": ["Stamm", "ge\u00b7wun\u00b7den", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VAPP", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.15": {"text": "Geflochtnes Caprifolium, das sich von ungefehr gefunden,", "tokens": ["Ge\u00b7flocht\u00b7nes", "Cap\u00b7ri\u00b7fo\u00b7li\u00b7um", ",", "das", "sich", "von", "un\u00b7ge\u00b7fehr", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PRELS", "PRF", "APPR", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.16": {"text": "Jm Kreis hier recht empor gewachsen, nur blo\u00df ein wenig", "tokens": ["Jm", "Kreis", "hier", "recht", "em\u00b7por", "ge\u00b7wach\u00b7sen", ",", "nur", "blo\u00df", "ein", "we\u00b7nig"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADV", "ADJD", "PTKVZ", "VAPP", "$,", "ADV", "ADV", "ART", "PIS"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.17": {"text": "ausgebreitet,", "tokens": ["aus\u00b7ge\u00b7brei\u00b7tet", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.18": {"text": "Und ohne M\u00fch\u2019, auch ohne Kosten, auf Stangen rings", "tokens": ["Und", "oh\u00b7ne", "M\u00fch'", ",", "auch", "oh\u00b7ne", "Kos\u00b7ten", ",", "auf", "Stan\u00b7gen", "rings"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "$,", "ADV", "APPR", "NN", "$,", "APPR", "NN", "ADV"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.19": {"text": "herum geleitet.", "tokens": ["he\u00b7rum", "ge\u00b7lei\u00b7tet", "."], "token_info": ["word", "word", "punct"], "pos": ["APZR", "VVPP", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.20": {"text": "Hier kann, beschirmt von hohen B\u00e4umen, und auch zugleich", "tokens": ["Hier", "kann", ",", "be\u00b7schirmt", "von", "ho\u00b7hen", "B\u00e4u\u00b7men", ",", "und", "auch", "zu\u00b7gleich"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VMFIN", "$,", "VVPP", "APPR", "ADJA", "NN", "$,", "KON", "ADV", "ADV"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.21": {"text": "in niedern B\u00fcschen,", "tokens": ["in", "nie\u00b7dern", "B\u00fc\u00b7schen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.22": {"text": "Sich eine fr\u00f6hliche Gesellschaft mit Lust gemeinschaftlich", "tokens": ["Sich", "ei\u00b7ne", "fr\u00f6h\u00b7li\u00b7che", "Ge\u00b7sell\u00b7schaft", "mit", "Lust", "ge\u00b7mein\u00b7schaft\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PRF", "ART", "ADJA", "NN", "APPR", "NN", "ADJD"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.23": {"text": "erfrischen,", "tokens": ["er\u00b7fri\u00b7schen", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.24": {"text": "Und, mitten in der Sonnen-Hitze, im schw\u00fchlen Sonnen-", "tokens": ["Und", ",", "mit\u00b7ten", "in", "der", "Son\u00b7nen\u00b7Hit\u00b7ze", ",", "im", "schw\u00fch\u00b7len", "Son\u00b7nen"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "$,", "ADV", "APPR", "ART", "NN", "$,", "APPRART", "ADJA", "TRUNC"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.25": {"text": "Strahl, im K\u00fchlen", "tokens": ["Strahl", ",", "im", "K\u00fch\u00b7len"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$,", "APPRART", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.26": {"text": "Ein angenehm Vergn\u00fcgen nehmen, Ergetzen und Erquic-", "tokens": ["Ein", "an\u00b7ge\u00b7nehm", "Ver\u00b7gn\u00fc\u00b7gen", "neh\u00b7men", ",", "Er\u00b7get\u00b7zen", "und", "Er\u00b7quic"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJD", "NN", "VVINF", "$,", "NN", "KON", "TRUNC"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.27": {"text": "kung f\u00fchlen.", "tokens": ["kung", "f\u00fch\u00b7len", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$."], "meter": "-+-", "measure": "amphibrach.single"}}}}}