{"dta.poem.15617": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "50.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du fragst, wie Ewigkeit du dir auf Erden dichtest?", "tokens": ["Du", "fragst", ",", "wie", "E\u00b7wig\u00b7keit", "du", "dir", "auf", "Er\u00b7den", "dich\u00b7test", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "NN", "PPER", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nicht anders als indem du Zeit und Raum vernichtest.", "tokens": ["Nicht", "an\u00b7ders", "als", "in\u00b7dem", "du", "Zeit", "und", "Raum", "ver\u00b7nich\u00b7test", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "KOKOM", "KOUS", "PPER", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die Zeit vernichtest du, wenn selig du vergissest", "tokens": ["Die", "Zeit", "ver\u00b7nich\u00b7test", "du", ",", "wenn", "se\u00b7lig", "du", "ver\u00b7gis\u00b7sest"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,", "KOUS", "ADJD", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vergangenes, und nicht Zuk\u00fcnftiges ermissest.", "tokens": ["Ver\u00b7gan\u00b7ge\u00b7nes", ",", "und", "nicht", "Zu\u00b7k\u00fcnf\u00b7ti\u00b7ges", "er\u00b7mis\u00b7sest", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "PTKNEG", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Den Raum vernichtest du, wenn, wo du bist, du bleibst", "tokens": ["Den", "Raum", "ver\u00b7nich\u00b7test", "du", ",", "wenn", ",", "wo", "du", "bist", ",", "du", "bleibst"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,", "KOUS", "$,", "PWAV", "PPER", "VAFIN", "$,", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In Frieden, dich nicht um in fremden Kreisen treibst.", "tokens": ["In", "Frie\u00b7den", ",", "dich", "nicht", "um", "in", "frem\u00b7den", "Krei\u00b7sen", "treibst", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PPER", "PTKNEG", "APPR", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Dadurch vernichtest du nicht v\u00f6llig Zeit und Raum,", "tokens": ["Da\u00b7durch", "ver\u00b7nich\u00b7test", "du", "nicht", "v\u00f6l\u00b7lig", "Zeit", "und", "Raum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PTKNEG", "ADJD", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch ist, was \u00fcbrig bleibt, dir nur ein leichter Traum.", "tokens": ["Doch", "ist", ",", "was", "\u00fcb\u00b7rig", "bleibt", ",", "dir", "nur", "ein", "leich\u00b7ter", "Traum", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "$,", "PRELS", "ADJD", "VVFIN", "$,", "PPER", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Aus diesem Traume la\u00df vom Wachen dich nicht st\u00f6ren;", "tokens": ["Aus", "die\u00b7sem", "Trau\u00b7me", "la\u00df", "vom", "Wa\u00b7chen", "dich", "nicht", "st\u00f6\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVFIN", "APPRART", "NN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was hast du auf der Welt zu sehn noch und zu h\u00f6ren?", "tokens": ["Was", "hast", "du", "auf", "der", "Welt", "zu", "sehn", "noch", "und", "zu", "h\u00f6\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "APPR", "ART", "NN", "PTKZU", "VVINF", "ADV", "KON", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Und was du h\u00f6ren mu\u00dft und sehn, dir ist gegeben", "tokens": ["Und", "was", "du", "h\u00f6\u00b7ren", "mu\u00dft", "und", "sehn", ",", "dir", "ist", "ge\u00b7ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PWS", "PPER", "VVINF", "VMFIN", "KON", "VVINF", "$,", "PPER", "VAFIN", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Kunst, es deinem Traum unst\u00f6rend einzuweben.", "tokens": ["Die", "Kunst", ",", "es", "dei\u00b7nem", "Traum", "un\u00b7st\u00f6\u00b7rend", "ein\u00b7zu\u00b7we\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPER", "PPOSAT", "NN", "ADJD", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}