{"dta.poem.968": {"metadata": {"author": {"name": "George, Stefan", "birth": "N.A.", "death": "N.A."}, "title": "R\u00fcckkehr", "genre": "Lyrik", "period": "N.A.", "pub_year": "1897", "urn": "urn:nbn:de:kobv:b4-200905191530", "language": ["de:0.99"], "booktitle": "George, Stefan: Das Jahr der Seele. Berlin, 1897."}, "poem": {"stanza.1": {"line.1": {"text": "Ich fahre heim auf reichem kahne                 ", "tokens": ["Ich", "fah\u00b7re", "heim", "auf", "rei\u00b7chem", "kah\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das ziel erwacht im abendrot                 ", "tokens": ["Das", "ziel", "er\u00b7wacht", "im", "a\u00b7ben\u00b7drot"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Vom maste weht die weisse fahne                 ", "tokens": ["Vom", "mas\u00b7te", "weht", "die", "weis\u00b7se", "fah\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wir &#252;bereilen manches boot               ", "tokens": ["Wir", "&#252;", "be\u00b7rei\u00b7len", "man\u00b7ches", "boot"], "token_info": ["word", "XML_entity", "word", "word", "word"], "pos": ["PPER", "$(", "VVFIN", "PIS", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Die alten ufer und geb&#228;ude                 ", "tokens": ["Die", "al\u00b7ten", "u\u00b7fer", "und", "geb", "&#228;", "u\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "XML_entity", "word"], "pos": ["ART", "ADJA", "NN", "KON", "VVFIN", "$(", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die alten glocken neu mir sind                 ", "tokens": ["Die", "al\u00b7ten", "glo\u00b7cken", "neu", "mir", "sind"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADJD", "PPER", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit der verheissung neuer freude                 ", "tokens": ["Mit", "der", "ver\u00b7heis\u00b7sung", "neu\u00b7er", "freu\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Bereden mich die winde lind               ", "tokens": ["Be\u00b7re\u00b7den", "mich", "die", "win\u00b7de", "lind"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Da taucht aus gr&#252;nen wogenk&#228;mmen                 ", "tokens": ["Da", "taucht", "aus", "gr", "&#252;", "nen", "wo\u00b7genk", "&#228;", "mmen"], "token_info": ["word", "word", "word", "word", "XML_entity", "word", "word", "XML_entity", "word"], "pos": ["ADV", "VVFIN", "APPR", "NE", "$(", "VVINF", "VVFIN", "$(", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein wort ein rosenes gesicht:                 ", "tokens": ["Ein", "wort", "ein", "ro\u00b7se\u00b7nes", "ge\u00b7sicht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du wohntest lang bei fremden st&#228;mmen                 ", "tokens": ["Du", "wohn\u00b7test", "lang", "bei", "frem\u00b7den", "st", "&#228;", "mmen"], "token_info": ["word", "word", "word", "word", "word", "word", "XML_entity", "word"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "ADJA", "VVFIN", "$(", "VVINF"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Doch unsre liebe starb dir nicht               ", "tokens": ["Doch", "uns\u00b7re", "lie\u00b7be", "starb", "dir", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "VVFIN", "PPER", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Du fuhrest aus im morgengrauen                 ", "tokens": ["Du", "fuh\u00b7rest", "aus", "im", "mor\u00b7gen\u00b7grau\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und als ob einen tag nur fern                 ", "tokens": ["Und", "als", "ob", "ei\u00b7nen", "tag", "nur", "fern"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOKOM", "KOUS", "ART", "NN", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Begr&#252;ssen dich die wellenfrauen                 ", "tokens": ["Be\u00b7gr", "&#252;", "ssen", "dich", "die", "wel\u00b7len\u00b7frau\u00b7en"], "token_info": ["word", "XML_entity", "word", "word", "word", "word"], "pos": ["NN", "$(", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die ufer und der erste stern.               ", "tokens": ["Die", "u\u00b7fer", "und", "der", "ers\u00b7te", "stern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}