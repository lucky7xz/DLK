{"textgrid.poem.48439": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "10. Auf eines guten Freundes Geburtstag", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da\u00df wir, geehrter Freund, euch dieses Brieflein senden,", "tokens": ["Da\u00df", "wir", ",", "ge\u00b7ehr\u00b7ter", "Freund", ",", "euch", "die\u00b7ses", "Brie\u00b7flein", "sen\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "ADJA", "NN", "$,", "PPER", "PDAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "das wird man, hoffen wir, uns nicht zum Argen wenden.", "tokens": ["das", "wird", "man", ",", "hof\u00b7fen", "wir", ",", "uns", "nicht", "zum", "Ar\u00b7gen", "wen\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PIS", "$,", "VVFIN", "PPER", "$,", "PPER", "PTKNEG", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sein lieber Namenstag, der sch\u00f6ne, hat gemacht,", "tokens": ["Sein", "lie\u00b7ber", "Na\u00b7mens\u00b7tag", ",", "der", "sch\u00f6\u00b7ne", ",", "hat", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "ART", "ADJA", "$,", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "da\u00df wir ihm auf ein Band gewesen sein bedacht,", "tokens": ["da\u00df", "wir", "ihm", "auf", "ein", "Band", "ge\u00b7we\u00b7sen", "sein", "be\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "APPR", "ART", "NN", "VAPP", "VAINF", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "doch mehr auf einen Wundsch. Wir lassen Andre binden", "tokens": ["doch", "mehr", "auf", "ei\u00b7nen", "Wund\u00b7sch", ".", "Wir", "las\u00b7sen", "And\u00b7re", "bin\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "ART", "NN", "$.", "PPER", "VVFIN", "PIS", "VVINF"], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "mit B\u00e4ndern mancher Art. Nichts solches ist zu finden", "tokens": ["mit", "B\u00e4n\u00b7dern", "man\u00b7cher", "Art", ".", "Nichts", "sol\u00b7ches", "ist", "zu", "fin\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "PIAT", "NN", "$.", "PIS", "PIS", "VAFIN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "bei Leuten, wie wir sein. Wer besser binden will,", "tokens": ["bei", "Leu\u00b7ten", ",", "wie", "wir", "sein", ".", "Wer", "bes\u00b7ser", "bin\u00b7den", "will", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PWAV", "PPER", "VAINF", "$.", "PWS", "ADJD", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "der such' und seh' ihm aus gar viel ein ander Ziel,", "tokens": ["der", "such'", "und", "seh'", "ihm", "aus", "gar", "viel", "ein", "an\u00b7der", "Ziel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VVFIN", "KON", "VVFIN", "PPER", "APPR", "ADV", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "als da\u00df er mit der Hand und Bande wolle scherzen,", "tokens": ["als", "da\u00df", "er", "mit", "der", "Hand", "und", "Ban\u00b7de", "wol\u00b7le", "scher\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOKOM", "KOUS", "PPER", "APPR", "ART", "NN", "KON", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "und binde, gleich als wir, das Herze mit dem Herzen!", "tokens": ["und", "bin\u00b7de", ",", "gleich", "als", "wir", ",", "das", "Her\u00b7ze", "mit", "dem", "Her\u00b7zen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "ADV", "KOUS", "PPER", "$,", "PDS", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Da\u00df wir, geehrter Freund, euch dieses Brieflein senden,", "tokens": ["Da\u00df", "wir", ",", "ge\u00b7ehr\u00b7ter", "Freund", ",", "euch", "die\u00b7ses", "Brie\u00b7flein", "sen\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "ADJA", "NN", "$,", "PPER", "PDAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "das wird man, hoffen wir, uns nicht zum Argen wenden.", "tokens": ["das", "wird", "man", ",", "hof\u00b7fen", "wir", ",", "uns", "nicht", "zum", "Ar\u00b7gen", "wen\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PIS", "$,", "VVFIN", "PPER", "$,", "PPER", "PTKNEG", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sein lieber Namenstag, der sch\u00