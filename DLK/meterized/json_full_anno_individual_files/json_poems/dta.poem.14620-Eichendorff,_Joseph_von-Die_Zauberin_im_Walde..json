{"dta.poem.14620": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Die Zauberin im Walde.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "\u201ein der Fern' liegt jetzt mein Leben,", "tokens": ["\u201e", "in", "der", "Fern'", "liegt", "jetzt", "mein", "Le\u00b7ben", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "ART", "NN", "VVFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Breitend sich wie junge Tr\u00e4ume,", "tokens": ["Brei\u00b7tend", "sich", "wie", "jun\u00b7ge", "Tr\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PRF", "KOKOM", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Schimmert stets so seltsam lockend", "tokens": ["Schim\u00b7mert", "stets", "so", "selt\u00b7sam", "lo\u00b7ckend"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ADV", "ADJD", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Durch die alten, dunklen B\u00e4ume.\u201c", "tokens": ["Durch", "die", "al\u00b7ten", ",", "dunk\u00b7len", "B\u00e4u\u00b7me", ".", "\u201c"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "ADJA", "$,", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}