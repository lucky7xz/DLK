{"dta.poem.8829": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "ViII.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Wer, was er liebt, im Auge hat, und dennoch nach", "tokens": ["Wer", ",", "was", "er", "liebt", ",", "im", "Au\u00b7ge", "hat", ",", "und", "den\u00b7noch", "nach"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "$,", "PWS", "PPER", "VVFIN", "$,", "APPRART", "NN", "VAFIN", "$,", "KON", "ADV", "APPR"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es hat der S\u00e4mann ausges\u00e4t, doch fri\u00dft der Rost die", "tokens": ["Es", "hat", "der", "S\u00e4\u00b7mann", "aus\u00b7ge\u00b7s\u00e4t", ",", "doch", "fri\u00dft", "der", "Rost", "die"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVPP", "$,", "ADV", "VVFIN", "ART", "NN", "ART"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Des Schnitters Arme sind zu schlaff, was hilft es, ob", "tokens": ["Des", "Schnit\u00b7ters", "Ar\u00b7me", "sind", "zu", "schlaff", ",", "was", "hilft", "es", ",", "ob"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "NN", "VAFIN", "PTKA", "ADJD", "$,", "PWS", "VVFIN", "PPER", "$,", "KOUS"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die welken Bl\u00e4tter les't ihr auf, da st\u00fcrmisch der", "tokens": ["Die", "wel\u00b7ken", "Bl\u00e4t\u00b7ter", "les't", "ihr", "auf", ",", "da", "st\u00fcr\u00b7misch", "der"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "PTKVZ", "$,", "KOUS", "ADJD", "ART"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "O pfl\u00fccktet Bl\u00fcthen ihr im Mai, wenn aus dem Laub", "tokens": ["O", "pfl\u00fcck\u00b7tet", "Bl\u00fc\u00b7then", "ihr", "im", "Mai", ",", "wenn", "aus", "dem", "Laub"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "NN", "PPOSAT", "APPRART", "NN", "$,", "KOUS", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Nur Der vermag, wie Titus einst, zu rufen: Ich ge-", "tokens": ["Nur", "Der", "ver\u00b7mag", ",", "wie", "Ti\u00b7tus", "einst", ",", "zu", "ru\u00b7fen", ":", "Ich", "ge"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ART", "VVFIN", "$,", "PWAV", "NE", "ADV", "$,", "PTKZU", "VVINF", "$.", "PPER", "TRUNC"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wer einen s\u00fc\u00dfen Mund ber\u00fchrt, an einen sch\u00f6nen Arm", "tokens": ["Wer", "ei\u00b7nen", "s\u00fc\u00b7\u00dfen", "Mund", "be\u00b7r\u00fchrt", ",", "an", "ei\u00b7nen", "sch\u00f6\u00b7nen", "Arm"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "ART", "ADJA", "NN", "VVPP", "$,", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.8": {"text": "Die Lehre zwar ist alt, ich wei\u00df; doch hat sie Mancher", "tokens": ["Die", "Leh\u00b7re", "zwar", "ist", "alt", ",", "ich", "wei\u00df", ";", "doch", "hat", "sie", "Man\u00b7cher"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "VAFIN", "ADJD", "$,", "PPER", "VVFIN", "$.", "ADV", "VAFIN", "PPER", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "De\u00df Grab sich nun im Lenz beros't, de\u00df Grab sich nun", "tokens": ["De\u00df", "Grab", "sich", "nun", "im", "Lenz", "be\u00b7ros't", ",", "de\u00df", "Grab", "sich", "nun"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "PRF", "ADV", "APPRART", "NN", "VVFIN", "$,", "ART", "NN", "PRF", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}