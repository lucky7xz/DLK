{"textgrid.poem.34007": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Sch\u00f6nheit", "genre": "verse", "period": "N.A.", "pub_year": 1845, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das Loos der G\u00f6tter ist auch dir gefallen;", "tokens": ["Das", "Loos", "der", "G\u00f6t\u00b7ter", "ist", "auch", "dir", "ge\u00b7fal\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VAFIN", "ADV", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Denn du bist sch\u00f6n, du brauchst dich nur zu zeigen,", "tokens": ["Denn", "du", "bist", "sch\u00f6n", ",", "du", "brauchst", "dich", "nur", "zu", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADJD", "$,", "PPER", "VVFIN", "PPER", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So wird sogar von Lippen, welche schweigen,", "tokens": ["So", "wird", "so\u00b7gar", "von", "Lip\u00b7pen", ",", "wel\u00b7che", "schwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "APPR", "NN", "$,", "PRELS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wenn Jeder jauchzt, dir Lob und Preis erschallen.", "tokens": ["Wenn", "Je\u00b7der", "jauchzt", ",", "dir", "Lob", "und", "Preis", "er\u00b7schal\u00b7len", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVFIN", "$,", "PPER", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Denn, die als unerreichbar vorschwebt Allen,", "tokens": ["Denn", ",", "die", "als", "un\u00b7er\u00b7reich\u00b7bar", "vor\u00b7schwebt", "Al\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PRELS", "KOUS", "ADJD", "VVFIN", "NE", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Harmonie, ist deinem Wesen eigen,", "tokens": ["Die", "Har\u00b7mo\u00b7nie", ",", "ist", "dei\u00b7nem", "We\u00b7sen", "ei\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VAFIN", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie sollte dich, wo du erscheinst, ein Reigen", "tokens": ["Wie", "soll\u00b7te", "dich", ",", "wo", "du", "er\u00b7scheinst", ",", "ein", "Rei\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "VMFIN", "PPER", "$,", "PWAV", "PPER", "VVFIN", "$,", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Von trunkenen Verehrern nicht umwallen!", "tokens": ["Von", "trun\u00b7ke\u00b7nen", "Ver\u00b7eh\u00b7rern", "nicht", "um\u00b7wal\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Zwar werden wir's nur schmerzlicher empfinden,", "tokens": ["Zwar", "wer\u00b7den", "wir's", "nur", "schmerz\u00b7li\u00b7cher", "emp\u00b7fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAINF", "VAFIN", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie viel uns mangelt, wenn wir auf dich schauen,", "tokens": ["Wie", "viel", "uns", "man\u00b7gelt", ",", "wenn", "wir", "auf", "dich", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPER", "VVFIN", "$,", "KOUS", "PPER", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Allein du bist uns doch verwandt geblieben;", "tokens": ["Al\u00b7lein", "du", "bist", "uns", "doch", "ver\u00b7wandt", "ge\u00b7blie\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VAFIN", "PPER", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Drum dienst du, uns dem H\u00f6chsten zu verbinden,", "tokens": ["Drum", "dienst", "du", ",", "uns", "dem", "H\u00f6chs\u00b7ten", "zu", "ver\u00b7bin\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "$,", "PPER", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wir stehen ihm nicht l\u00e4nger fern mit Grauen,", "tokens": ["Wir", "ste\u00b7hen", "ihm", "nicht", "l\u00e4n\u00b7ger", "fern", "mit", "Grau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "ADJD", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Es tritt uns nah' in dir, wir k\u00f6nnen's lieben!", "tokens": ["Es", "tritt", "uns", "nah'", "in", "dir", ",", "wir", "k\u00f6n\u00b7nen's", "lie\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVFIN", "APPR", "PPER", "$,", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Das Loos der G\u00f6tter ist au