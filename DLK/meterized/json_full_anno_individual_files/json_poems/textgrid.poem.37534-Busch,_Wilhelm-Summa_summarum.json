{"textgrid.poem.37534": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Summa summarum", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sag, wie w\u00e4r es, alter Schragen,", "tokens": ["Sag", ",", "wie", "w\u00e4r", "es", ",", "al\u00b7ter", "Schra\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "VAFIN", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn du mal die Brille putztest,", "tokens": ["Wenn", "du", "mal", "die", "Bril\u00b7le", "putz\u00b7test", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Um ein wenig nachzuschlagen,", "tokens": ["Um", "ein", "we\u00b7nig", "nach\u00b7zu\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "PIS", "VVIZU", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie du deine Zeit benutztest.", "tokens": ["Wie", "du", "dei\u00b7ne", "Zeit", "be\u00b7nutz\u00b7test", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Oft wohl h\u00e4tten dich so gerne", "tokens": ["Oft", "wohl", "h\u00e4t\u00b7ten", "dich", "so", "ger\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "ADV", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Weiche Arme warm gebettet;", "tokens": ["Wei\u00b7che", "Ar\u00b7me", "warm", "ge\u00b7bet\u00b7tet",