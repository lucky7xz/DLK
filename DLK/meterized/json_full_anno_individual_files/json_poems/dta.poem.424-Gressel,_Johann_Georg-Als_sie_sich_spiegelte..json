{"dta.poem.424": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Als sie sich spiegelte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Sie siehet/ sch\u00f6nstes Kind/ und wird alsbald gesehen/", "tokens": ["Sie", "sie\u00b7het", "/", "sch\u00f6ns\u00b7tes", "Kind", "/", "und", "wird", "als\u00b7bald", "ge\u00b7se\u00b7hen", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "ADJA", "NN", "$(", "KON", "VAFIN", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn sie ihr Auge l\u00e5st in einen Spiegel gehen/", "tokens": ["Wenn", "sie", "ihr", "Au\u00b7ge", "l\u00e5st", "in", "ei\u00b7nen", "Spie\u00b7gel", "ge\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Darinnen zeiget sich der Sch\u00f6nheit beste Pracht/", "tokens": ["Da\u00b7rin\u00b7nen", "zei\u00b7get", "sich", "der", "Sch\u00f6n\u00b7heit", "bes\u00b7te", "Pracht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PRF", "ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Zu der Vollkommenheit mit vollen Munde lacht.", "tokens": ["Zu", "der", "Voll\u00b7kom\u00b7men\u00b7heit", "mit", "vol\u00b7len", "Mun\u00b7de", "lacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word"