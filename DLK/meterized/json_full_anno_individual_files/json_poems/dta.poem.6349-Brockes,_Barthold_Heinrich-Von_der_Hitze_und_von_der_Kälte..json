{"dta.poem.6349": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von der Hitze und von der K\u00e4lte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.85", "af:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Bewundert man nun dieses sehr,", "tokens": ["Be\u00b7wun\u00b7dert", "man", "nun", "die\u00b7ses", "sehr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "PDAT", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auf welche Weise doch zwo Feuchtigkeiten", "tokens": ["Auf", "wel\u00b7che", "Wei\u00b7se", "doch", "zwo", "Feuch\u00b7tig\u00b7kei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PWAT", "NN", "ADV", "CARD", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die, wenn sie einzeln kalt, als wenn es feurig w\u00e4r,", "tokens": ["Die", ",", "wenn", "sie", "ein\u00b7zeln", "kalt", ",", "als", "wenn", "es", "feu\u00b7rig", "w\u00e4r", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "KOUS", "PPER", "ADJD", "ADJD", "$,", "KOKOM", "KOUS", "PPER", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sich von sich selbst erhitzen, brudeln, streiten", "tokens": ["Sich", "von", "sich", "selbst", "er\u00b7hit\u00b7zen", ",", "bru\u00b7deln", ",", "strei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["PRF", "APPR", "PRF", "ADV", "VVINF", "$,", "VVFIN", "$,", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "In einem Augenblick, wo kommt das Feuer her,", "tokens": ["In", "ei\u00b7nem", "Au\u00b7gen\u00b7blick", ",", "wo", "kommt", "das", "Feu\u00b7er", "her", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PWAV", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wor\u00fcber man sich so verwundern mu\u00df?", "tokens": ["Wo\u00b7r\u00fc\u00b7ber", "man", "sich", "so", "ver\u00b7wun\u00b7dern", "mu\u00df", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "PRF", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Daher, dieweil der rundeu C\u00f6rper Flu\u00df,", "tokens": ["Da\u00b7her", ",", "die\u00b7weil", "der", "run\u00b7deu", "C\u00f6r\u00b7per", "Flu\u00df", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "KOUS", "ART", "NE", "NE", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Die sich begegnen, ihre G\u00e4nge", "tokens": ["Die", "sich", "be\u00b7geg\u00b7nen", ",", "ih\u00b7re", "G\u00e4n\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "PRF", "VVINF", "$,", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Verstopffen, hindern, und sie enge", "tokens": ["Ver\u00b7stopf\u00b7fen", ",", "hin\u00b7dern", ",", "und", "sie", "en\u00b7ge"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["VVPP", "$,", "VVINF", "$,", "KON", "PPER", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Und schwierig machen: Da sie dann,", "tokens": ["Und", "schwie\u00b7rig", "ma\u00b7chen", ":", "Da", "sie", "dann", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVINF", "$.", "KOUS", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "So viel ein jeder immer kan,", "tokens": ["So", "viel", "ein", "je\u00b7der", "im\u00b7mer", "kan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "PIS", "ADV", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Was sie umgiebet, von sich dr\u00e4ngen.", "tokens": ["Was", "sie", "um\u00b7gie\u00b7bet", ",", "von", "sich", "dr\u00e4n\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "APPR", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Nichts kan so dann sich durch dieselbe zw\u00e4ngen,", "tokens": ["Nichts", "kan", "so", "dann", "sich", "durch", "die\u00b7sel\u00b7be", "zw\u00e4n\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "ADV", "ADV", "PRF", "APPR", "PDAT", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Als blos das erste Element,", "tokens": ["Als", "blos", "das", "ers\u00b7te", "E\u00b7le\u00b7ment", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Wodurch die kleine C\u00f6rperlein", "tokens": ["Wo\u00b7durch", "die", "klei\u00b7ne", "C\u00f6r\u00b7per\u00b7lein"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Beweget seyn:", "tokens": ["Be\u00b7we\u00b7get", "seyn", ":"], "token_info": ["word", "word", "punct"], "pos": ["VVPP", "VAINF", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.17": {"text": "Und durch die St\u00f6sse, die sie sich", "tokens": ["Und", "durch", "die", "St\u00f6s\u00b7se", ",", "die", "sie", "sich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "$,", "PRELS", "PPER", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Einander so gar hefftiglich", "tokens": ["Ein\u00b7an\u00b7der", "so", "gar", "heff\u00b7tig\u00b7lich"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADV", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Und mit so strengem Triebe geben,", "tokens": ["Und", "mit", "so", "stren\u00b7gem", "Trie\u00b7be", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADV", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Erregen sie in dieser kalten Fluth", "tokens": ["Er\u00b7re\u00b7gen", "sie", "in", "die\u00b7ser", "kal\u00b7ten", "Fluth"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Solch eine schnelle Gluht.", "tokens": ["Solch", "ei\u00b7ne", "schnel\u00b7le", "Gluht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}