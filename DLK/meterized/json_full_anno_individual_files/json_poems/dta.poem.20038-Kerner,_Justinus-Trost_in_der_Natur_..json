{"dta.poem.20038": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "Trost in der Natur .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905193111", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Das Schicksal hat verschlagen", "tokens": ["Das", "Schick\u00b7sal", "hat", "ver\u00b7schla\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mich an so manchen Ort,", "tokens": ["Mich", "an", "so", "man\u00b7chen", "Ort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wo andre unter Klagen", "tokens": ["Wo", "and\u00b7re", "un\u00b7ter", "Kla\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "PIS", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Bald w\u00e4ren weiter fort.", "tokens": ["Bald", "w\u00e4\u00b7ren", "wei\u00b7ter", "fort", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ich doch blieb mit Vergn\u00fcgen", "tokens": ["Ich", "doch", "blieb", "mit", "Ver\u00b7gn\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sah ich nur einen Baum,", "tokens": ["Sah", "ich", "nur", "ei\u00b7nen", "Baum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sah ich nur V\u00f6gel fliegen,", "tokens": ["Sah", "ich", "nur", "V\u00f6\u00b7gel", "flie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "F\u00fchlt' ich mein Leiden kaum.", "tokens": ["F\u00fchlt'", "ich", "mein", "Lei\u00b7den", "kaum", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und trug ich Schmerz und Wunden,", "tokens": ["Und", "trug", "ich", "Schmerz", "und", "Wun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ich klagte nimmer laut,", "tokens": ["Ich", "klag\u00b7te", "nim\u00b7mer", "laut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Konnt' immer noch gesunden", "tokens": ["Konnt'", "im\u00b7mer", "noch", "ge\u00b7sun\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "ADV", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Im Lenz bey Gras und Kraut.", "tokens": ["Im", "Lenz", "bey", "Gras", "und", "Kraut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ich hab mich stets gehalten", "tokens": ["Ich", "hab", "mich", "stets", "ge\u00b7hal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "An die Natur so warm,", "tokens": ["An", "die", "Na\u00b7tur", "so", "warm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Menschen lie\u00df ich schalten,", "tokens": ["Die", "Men\u00b7schen", "lie\u00df", "ich", "schal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Gott! \u2014 die sind kalt und arm.", "tokens": ["Gott", "!", "die", "sind", "kalt", "und", "arm", "."], "token_info": ["word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "$(", "PDS", "VAFIN", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}