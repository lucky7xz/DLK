{"textgrid.poem.37809": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Das Lied vom Ringe", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es waren drey Soldaten,", "tokens": ["Es", "wa\u00b7ren", "drey", "Sol\u00b7da\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "CARD", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dabey ein junges Blut,", "tokens": ["Da\u00b7bey", "ein", "jun\u00b7ges", "Blut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie hatten sich vergangen,", "tokens": ["Sie", "hat\u00b7ten", "sich", "ver\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Graf nahm sie gefangen,", "tokens": ["Der", "Graf", "nahm", "sie", "ge\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Setzt sie bis auf den Tod.", "tokens": ["Setzt", "sie", "bis", "auf", "den", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Es war ein wackres M\u00e4delein", "tokens": ["Es", "war", "ein", "wack\u00b7res", "M\u00e4\u00b7de\u00b7lein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dazu aus fremdem Land,", "tokens": ["Da\u00b7zu", "aus", "frem\u00b7dem", "Land", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie lief in aller Eilen", "tokens": ["Sie", "lief", "in", "al\u00b7ler", "Ei\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Des Tags wohl zehen Meilen", "tokens": ["Des", "Tags", "wohl", "ze\u00b7hen", "Mei\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "CARD", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Bis zu dem Grafen hin.", "tokens": ["Bis", "zu", "dem", "Gra\u00b7fen", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "\u00bbgott gr\u00fc\u00df Euch, edler Herre mein,", "tokens": ["\u00bb", "gott", "gr\u00fc\u00df", "Euch", ",", "ed\u00b7ler", "Her\u00b7re", "mein", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "VVFIN", "PPER", "$,", "ADJA", "NN", "PPOSAT", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich w\u00fcnsch Euch guten Tag,", "tokens": ["Ich", "w\u00fcnsch", "Euch", "gu\u00b7ten", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ach! wolt Ihr mein gedenken", "tokens": ["Ach", "!", "wolt", "Ihr", "mein", "ge\u00b7den\u00b7ken"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "VMFIN", "PPER", "PPOSAT", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Den Gefangnen mir zu schenken", "tokens": ["Den", "Ge\u00b7fang\u00b7nen", "mir", "zu", "schen\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PPER", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ja schenken zu der Eh.\u00ab", "tokens": ["Ja", "schen\u00b7ken", "zu", "der", "Eh", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PTKANT", "VVFIN", "APPR", "ART", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "\u00bbach nein, mein liebes M\u00e4delein,", "tokens": ["\u00bb", "ach", "nein", ",", "mein", "lie\u00b7bes", "M\u00e4\u00b7de\u00b7lein", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "XY", "PTKANT", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das kann und mag nicht sein,", "tokens": ["Das", "kann", "und", "mag", "nicht", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "KON", "VMFIN", "PTKNEG", "VAINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Gefangne der mu\u00df sterben,", "tokens": ["Der", "Ge\u00b7fang\u00b7ne", "der", "mu\u00df", "ster\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gott's Gnad mu\u00df er ererben", "tokens": ["Gott's", "Gnad", "mu\u00df", "er", "er\u00b7er\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "NN", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wie er verdienet hat.\u00ab", "tokens": ["Wie", "er", "ver\u00b7die\u00b7net", "hat", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "VAFIN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Das M\u00e4del drehet sich herum", "tokens": ["Das", "M\u00e4\u00b7del", "dre\u00b7het", "sich", "he\u00b7rum"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PRF", "APZR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und weinet bitterlich,", "tokens": ["Und", "wei\u00b7net", "bit\u00b7ter\u00b7lich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie lief in aller Eilen", "tokens": ["Sie", "lief", "in", "al\u00b7ler", "Ei\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Des Tags wohl zwanzig Meilen,", "tokens": ["Des", "Tags", "wohl", "zwan\u00b7zig", "Mei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "CARD", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Bis zu dem tiefen Thurm.", "tokens": ["Bis", "zu", "dem", "tie\u00b7fen", "Thurm", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "\u00bbgott gr\u00fc\u00df Euch ihr Gefangnen mein,", "tokens": ["\u00bb", "gott", "gr\u00fc\u00df", "Euch", "ihr", "Ge\u00b7fang\u00b7nen", "mein", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "VVFIN", "PPER", "PPOSAT", "NN", "PPOSAT", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich w\u00fcnsch Euch guten Tag!", "tokens": ["Ich", "w\u00fcnsch", "Euch", "gu\u00b7ten", "Tag", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich hab f\u00fcr Euch gebeten,", "tokens": ["Ich", "hab", "f\u00fcr", "Euch", "ge\u00b7be\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ich kann Euch nicht erretten,", "tokens": ["Ich", "kann", "Euch", "nicht", "er\u00b7ret\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Es hilft nicht Gut noch Geld.\u00ab", "tokens": ["Es", "hilft", "nicht", "Gut", "noch", "Geld", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADJD", "ADV", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Was hat sie unter ihrem Sch\u00fcrzelein?", "tokens": ["Was", "hat", "sie", "un\u00b7ter", "ih\u00b7rem", "Sch\u00fcr\u00b7zel\u00b7ein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ein Hemdlein war schneewei\u00df,", "tokens": ["Ein", "Hemd\u00b7lein", "war", "schnee\u00b7wei\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbdas nimm du Allerliebster mein,", "tokens": ["\u00bb", "das", "nimm", "du", "Al\u00b7ler\u00b7liebs\u00b7ter", "mein", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PDS", "VVFIN", "PPER", "NN", "PPOSAT", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Es soll von mir dein Brauthemd sein,", "tokens": ["Es", "soll", "von", "mir", "dein", "Brau\u00b7themd", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PPER", "PPOSAT", "NN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Darin lieg du im Tod.\u00ab", "tokens": ["Da\u00b7rin", "lieg", "du", "im", "Tod", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PAV", "VVFIN", "PPER", "APPRART", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Was zog er von dem Finger sein?", "tokens": ["Was", "zog", "er", "von", "dem", "Fin\u00b7ger", "sein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "APPR", "ART", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Ringlein, war von Gold,", "tokens": ["Ein", "Rin\u00b7glein", ",", "war", "von", "Gold", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VAFIN", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbdas nimm du H\u00fcbsche, du Feine,", "tokens": ["\u00bb", "das", "nimm", "du", "H\u00fcb\u00b7sche", ",", "du", "Fei\u00b7ne", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "PDS", "VVFIN", "PPER", "NN", "$,", "PPER", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Du Allerliebste meine,", "tokens": ["Du", "Al\u00b7ler\u00b7liebs\u00b7te", "mei\u00b7ne", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Das soll dein Trauring sein.\u00ab", "tokens": ["Das", "soll", "dein", "Trau\u00b7ring", "sein", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "VMFIN", "PPOSAT", "NN", "VAINF", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "\u00bbwas soll ich mit dem Ringlein thun,", "tokens": ["\u00bb", "was", "soll", "ich", "mit", "dem", "Rin\u00b7glein", "thun", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VMFIN", "PPER", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn ichs nicht tragen kann?\u00ab", "tokens": ["Wenn", "ichs", "nicht", "tra\u00b7gen", "kann", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PIS", "PTKNEG", "VVINF", "VMFIN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbleg es in Kisten und Kasten,", "tokens": ["\u00bb", "leg", "es", "in", "Kis\u00b7ten", "und", "Kas\u00b7ten", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "PPER", "APPR", "NN", "KON", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Und la\u00df es ruhen und rasten", "tokens": ["Und", "la\u00df", "es", "ru\u00b7hen", "und", "ras\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVIMP", "PPER", "VVINF", "KON", "VVFIN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Bis an den j\u00fcngsten Tag.\u00ab", "tokens": ["Bis", "an", "den", "j\u00fcng\u00b7sten", "Tag", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "APPR", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "\u00bbund wenn ich \u00fcber Kisten und Kasten komm,", "tokens": ["\u00bb", "und", "wenn", "ich", "\u00fc\u00b7ber", "Kis\u00b7ten", "und", "Kas\u00b7ten", "komm", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "KOUS", "PPER", "APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und sehe das Ringlein an,", "tokens": ["Und", "se\u00b7he", "das", "Rin\u00b7glein", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Da darf ichs nicht anstecken,", "tokens": ["Da", "darf", "ichs", "nicht", "an\u00b7ste\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIS", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das Herz m\u00f6cht mir zerbrechen,", "tokens": ["Das", "Herz", "m\u00f6cht", "mir", "zer\u00b7bre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Weil ichs nicht \u00e4ndern kann.\u00ab", "tokens": ["Weil", "ichs", "nicht", "\u00e4n\u00b7dern", "kann", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PIS", "PTKNEG", "VVINF", "VMFIN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Es waren drey Soldaten,", "tokens": ["Es", "wa\u00b7ren", "drey", "Sol\u00b7da\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "CARD", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dabey ein junges Blut,", "tokens": ["Da\u00b7bey", "ein", "jun\u00b7ges", "Blut", ","], "token_info": ["word", "word"