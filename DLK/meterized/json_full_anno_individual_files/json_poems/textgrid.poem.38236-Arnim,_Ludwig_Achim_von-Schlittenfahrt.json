{"textgrid.poem.38236": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Schlittenfahrt", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da\u00df uns der Winter nicht steht will seyn,", "tokens": ["Da\u00df", "uns", "der", "Win\u00b7ter", "nicht", "steht", "will", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PTKNEG", "VVFIN", "VMFIN", "VAINF", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Des trauren die M\u00e4dlein gar sehre;", "tokens": ["Des", "trau\u00b7ren", "die", "M\u00e4d\u00b7lein", "gar", "seh\u00b7re", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Weil uns der Schnee nit bleiben will,", "tokens": ["Weil", "uns", "der", "Schnee", "nit", "blei\u00b7ben", "will", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PTKNEG", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und ander gut Gesellen mehre.", "tokens": ["Und", "an\u00b7der", "gut", "Ge\u00b7sel\u00b7len", "meh\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADJD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Heut ist trocken, morgen ist na\u00df,", "tokens": ["Heut", "ist", "tro\u00b7cken", ",", "mor\u00b7gen", "ist", "na\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADJD", "$,", "ADV", "VAFIN", "ADJD", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Da hat uns der Teufel den Winter herbracht;", "tokens": ["Da", "hat", "uns", "der", "Teu\u00b7fel", "den", "Win\u00b7ter", "her\u00b7bracht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.7": {"text": "Der Winter thut sich biegen,", "tokens": ["Der", "Win\u00b7ter", "thut", "sich", "bie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die Lerchen thun sich schmiegen,", "tokens": ["Die", "Ler\u00b7chen", "thun", "sich", "schmie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Die Schlitten thun sie \u00fcben.", "tokens": ["Die", "Schlit\u00b7ten", "thun", "sie", "\u00fc\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ach feins mein Lieb, so sey mir hold,", "tokens": ["Ach", "feins", "mein", "Lieb", ",", "so", "sey", "mir", "hold", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "PPOSAT", "NN", "$,", "ADV", "VAFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Um Eins will ich dich bitten,", "tokens": ["Um", "Eins", "will", "ich", "dich", "bit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "VMFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Kauf du mir ein gespiegeltes Ro\u00df,", "tokens": ["Kauf", "du", "mir", "ein", "ge\u00b7spie\u00b7gel\u00b7tes", "Ro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "---+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Dazu ein gemahlten Schlitten.", "tokens": ["Da\u00b7zu", "ein", "ge\u00b7mahl\u00b7ten", "Schlit\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "So fahren wir mit Schallen,", "tokens": ["So", "fah\u00b7ren", "wir", "mit", "Schal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "So fahren wir mit Schallen,", "tokens": ["So", "fah\u00b7ren", "wir", "mit", "Schal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "So fahren wir mit Schallen,", "tokens": ["So", "fah\u00b7ren", "wir", "mit", "Schal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die G\u00e4\u00dflein allenthalben,", "tokens": ["Die", "G\u00e4\u00df\u00b7lein", "al\u00b7len\u00b7thal\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Feins Lieb, la\u00df dirs gefallen.", "tokens": ["Feins", "Lieb", ",", "la\u00df", "dirs", "ge\u00b7fal\u00b7len", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "VVFIN", "PIS", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ach feins mein Lieb, so spar mich nit,", "tokens": ["Ach", "feins", "mein", "Lieb", ",", "so", "spar", "mich", "nit", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "PPOSAT", "NN", "$,", "ADV", "ADJD", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich bin darzu gewachsen.", "tokens": ["Ich", "bin", "dar\u00b7zu", "ge\u00b7wach\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PAV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Nimm nur dein M\u00fcfflein in die Hand,", "tokens": ["Nimm", "nur", "dein", "M\u00fcf\u00b7flein", "in", "die", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "PPOSAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich schau dir \u00fcber die Achsel,", "tokens": ["Ich", "schau", "dir", "\u00fc\u00b7ber", "die", "Ach\u00b7sel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wei\u00df zugeschneites Osterlamm,", "tokens": ["Wei\u00df", "zu\u00b7ge\u00b7schnei\u00b7tes", "Os\u00b7ter\u00b7lamm", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mein R\u00f6\u00dflein rasselt mit dem Kamm,", "tokens": ["Mein", "R\u00f6\u00df\u00b7lein", "ras\u00b7selt", "mit", "dem", "Kamm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "So fahren wir mit Schallen,", "tokens": ["So", "fah\u00b7ren", "wir", "mit", "Schal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die G\u00e4\u00dflein allenthalben,", "tokens": ["Die", "G\u00e4\u00df\u00b7lein", "al\u00b7len\u00b7thal\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Feins Lieb la\u00df dirs gefallen.", "tokens": ["Feins", "Lieb", "la\u00df", "dirs", "ge\u00b7fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "PIS", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ach feins mein Lieb, nun spitz die F\u00fc\u00df,", "tokens": ["Ach", "feins", "mein", "Lieb", ",", "nun", "spitz", "die", "F\u00fc\u00df", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "PPOSAT", "NN", "$,", "ADV", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wohl auf mit mir zum Tanze,", "tokens": ["Wohl", "auf", "mit", "mir", "zum", "Tan\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "APPR", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Zieh mir die R\u00e4dlein um und um,", "tokens": ["Zieh", "mir", "die", "R\u00e4d\u00b7lein", "um", "und", "um", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPR", "KON", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit deinem Schleppenschwanze;", "tokens": ["Mit", "dei\u00b7nem", "Schlep\u00b7pen\u00b7schwan\u00b7ze", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und schwenkst du mirs nit in die Sporn,", "tokens": ["Und", "schwenkst", "du", "mirs", "nit", "in", "die", "Sporn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "NE", "PTKNEG", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.6": {"text": "Setz ich ein Kranz dir auf die Ohr'n,", "tokens": ["Setz", "ich", "ein", "Kranz", "dir", "auf", "die", "Ohr'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "So fahren wir mit Schallen", "tokens": ["So", "fah\u00b7ren", "wir", "mit", "Schal\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die G\u00e4\u00dflein allenthalben,", "tokens": ["Die", "G\u00e4\u00df\u00b7lein", "al\u00b7len\u00b7thal\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Feins Lieb la\u00df dirs gefallen.", "tokens": ["Feins", "Lieb", "la\u00df", "dirs", "ge\u00b7fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "PIS", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Ob einer k\u00e4m, der murren wollt,", "tokens": ["Ob", "ei\u00b7ner", "k\u00e4m", ",", "der", "mur\u00b7ren", "wollt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVFIN", "$,", "PRELS", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wir wollen nichts drum geben,", "tokens": ["Wir", "wol\u00b7len", "nichts", "drum", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIS", "PAV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Es mu\u00df vorbei gestochen seyn,", "tokens": ["Es", "mu\u00df", "vor\u00b7bei", "ge\u00b7sto\u00b7chen", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und kost es Leib und Leben,", "tokens": ["Und", "kost", "es", "Leib", "und", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "So fahren wir \u00fcber die Heide,", "tokens": ["So", "fah\u00b7ren", "wir", "\u00fc\u00b7ber", "die", "Hei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "So fahren wir \u00fcber die Heide,", "tokens": ["So", "fah\u00b7ren", "wir", "\u00fc\u00b7ber", "die", "Hei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.7": {"text": "So fahren wir \u00fcber die Heide,", "tokens": ["So", "fah\u00b7ren", "wir", "\u00fc\u00b7ber", "die", "Hei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.8": {"text": "Wohl manchem Mann zu Leide,",