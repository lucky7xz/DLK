{"dta.poem.7496": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Grabschrift.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192430", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Sein oder Nichtsein ist hier keine Frage;", "tokens": ["Sein", "o\u00b7der", "Nich\u00b7tsein", "ist", "hier", "kei\u00b7ne", "Fra\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "KON", "NN", "VAFIN", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich bin gewesen, was ich konnte sein.", "tokens": ["Ich", "bin", "ge\u00b7we\u00b7sen", ",", "was", "ich", "konn\u00b7te", "sein", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VAPP", "$,", "PWS", "PPER", "VMFIN", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Kein Schelm und Schuft, bei Gott! ein Narr allein,", "tokens": ["Kein", "Schelm", "und", "Schuft", ",", "bei", "Gott", "!", "ein", "Narr", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "NN", "$,", "APPR", "NN", "$.", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der auch sein L\u00e4mpchen brannt' am hellen Tage.", "tokens": ["Der", "auch", "sein", "L\u00e4mp\u00b7chen", "brannt'", "am", "hel\u00b7len", "Ta\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PPOSAT", "NN", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Kein Turner, aber doch von deutschem Schlage;", "tokens": ["Kein", "Tur\u00b7ner", ",", "a\u00b7ber", "doch", "von", "deut\u00b7schem", "Schla\u00b7ge", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "ADV", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und w\u00e4r' mein Vers wie meine H\u00e4nde, rein,", "tokens": ["Und", "w\u00e4r'", "mein", "Vers", "wie", "mei\u00b7ne", "H\u00e4n\u00b7de", ",", "rein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VAFIN", "PPOSAT", "NN", "KOKOM", "PPOSAT", "NN", "$,", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "So ruhete di\u00df dichterlich Gebein", "tokens": ["So", "ru\u00b7he\u00b7te", "di\u00df", "dich\u00b7ter\u00b7lich", "Ge\u00b7bein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PDS", "ADJD", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Dereinst in einem stolzen Sarkophage.", "tokens": ["De\u00b7reinst", "in", "ei\u00b7nem", "stol\u00b7zen", "Sar\u00b7ko\u00b7pha\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ich nahm das Leben f\u00fcr ein W\u00fcrfelspiel,", "tokens": ["Ich", "nahm", "das", "Le\u00b7ben", "f\u00fcr", "ein", "W\u00fcr\u00b7fel\u00b7spiel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Das Keinem seine stete Gunst geschworen,", "tokens": ["Das", "Kei\u00b7nem", "sei\u00b7ne", "ste\u00b7te", "Gunst", "ge\u00b7schwo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "PPOSAT", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch oft hatt' ich der Augen noch zu viel;", "tokens": ["Doch", "oft", "hatt'", "ich", "der", "Au\u00b7gen", "noch", "zu", "viel", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ART", "NN", "ADV", "PTKA", "PIS", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ich trieb's, ein Thor, wie tausend andre Thoren,", "tokens": ["Ich", "trie\u00b7b's", ",", "ein", "Thor", ",", "wie", "tau\u00b7send", "and\u00b7re", "Tho\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ART", "NN", "$,", "PWAV", "ADJD", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und, gl\u00fccklicher als weiland Freund Schlemihl,", "tokens": ["Und", ",", "gl\u00fcck\u00b7li\u00b7cher", "als", "wei\u00b7land", "Freund", "Schle\u00b7mihl", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ADJD", "KOKOM", "VVFIN", "NN", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Hab' niemals meinen Schatten ich verloren.", "tokens": ["Hab'", "nie\u00b7mals", "mei\u00b7nen", "Schat\u00b7ten", "ich", "ver\u00b7lo\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PPOSAT", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}