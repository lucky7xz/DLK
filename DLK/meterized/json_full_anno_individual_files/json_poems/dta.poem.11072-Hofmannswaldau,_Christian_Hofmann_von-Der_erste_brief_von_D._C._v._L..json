{"dta.poem.11072": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Der erste brief von D. C. v. L.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nicht scheue dich, mein kind! di\u00df siegel aufzumachen,", "tokens": ["Nicht", "scheu\u00b7e", "dich", ",", "mein", "kind", "!", "di\u00df", "sie\u00b7gel", "auf\u00b7zu\u00b7ma\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "PPER", "$,", "PPOSAT", "NN", "$.", "PDS", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die du den schl\u00fcssel selbst zu meiner seelen hast.", "tokens": ["Die", "du", "den", "schl\u00fcs\u00b7sel", "selbst", "zu", "mei\u00b7ner", "see\u00b7len", "hast", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "ADV", "APPR", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Was hier geheimnis ist, sind dir bekannte sachen,", "tokens": ["Was", "hier", "ge\u00b7heim\u00b7nis", "ist", ",", "sind", "dir", "be\u00b7kann\u00b7te", "sa\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVPP", "VAFIN", "$,", "VAFIN", "PPER", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mein antlitz hat dir l\u00e4ngst verrathen meine last.", "tokens": ["Mein", "ant\u00b7litz", "hat", "dir", "l\u00e4ngst", "ver\u00b7ra\u00b7then", "mei\u00b7ne", "last", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "ADV", "VVFIN", "PPOSAT", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die asch auf Aetnens klufft lehrt, da\u00df da feuer stecket,", "tokens": ["Die", "asch", "auf", "A\u00b7e\u00b7tnens", "klufft", "lehrt", ",", "da\u00df", "da", "feu\u00b7er", "ste\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "NN", "ADJD", "VVFIN", "$,", "KOUS", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Und meine bl\u00e4sse zeigt, da\u00df lieb im hertzen glimmt.", "tokens": ["Und", "mei\u00b7ne", "bl\u00e4s\u00b7se", "zeigt", ",", "da\u00df", "lieb", "im", "hert\u00b7zen", "glimmt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "$,", "KOUS", "ADJD", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Nicht frage, wer in mir so s\u00fcsse glut erwecket,", "tokens": ["Nicht", "fra\u00b7ge", ",", "wer", "in", "mir", "so", "s\u00fcs\u00b7se", "glut", "er\u00b7we\u00b7cket", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "$,", "PWS", "APPR", "PPER", "ADV", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Dein eignes auge f\u00fchlt, wo sie den ursprung nimmt,", "tokens": ["Dein", "eig\u00b7nes", "au\u00b7ge", "f\u00fchlt", ",", "wo", "sie", "den", "ur\u00b7sprung", "nimmt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$,", "PWAV", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Weil heisse sonnen ja nicht leer vom brand seyn m\u00fcssen,", "tokens": ["Weil", "heis\u00b7se", "son\u00b7nen", "ja", "nicht", "leer", "vom", "brand", "seyn", "m\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "ADV", "PTKNEG", "ADJD", "APPRART", "NN", "VAINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Aus kalten adern nicht ein warmer brunn entspringt;", "tokens": ["Aus", "kal\u00b7ten", "a\u00b7dern", "nicht", "ein", "war\u00b7mer", "brunn", "ent\u00b7springt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKNEG", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Doch wilst du deinen sieg selbst von dem sclaven wissen,", "tokens": ["Doch", "wilst", "du", "dei\u00b7nen", "sieg", "selbst", "von", "dem", "scla\u00b7ven", "wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PPOSAT", "NN", "ADV", "APPR", "ART", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "So duld\u2019 es auch dein ohr, wenn itzt sein fessel klingt.", "tokens": ["So", "duld'", "es", "auch", "dein", "ohr", ",", "wenn", "itzt", "sein", "fes\u00b7sel", "klingt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PPOSAT", "NN", "$,", "KOUS", "ADV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Ich liebe dich, mein kind! mit unzertheiltem hertzen,", "tokens": ["Ich", "lie\u00b7be", "dich", ",", "mein", "kind", "!", "mit", "un\u00b7zer\u00b7theil\u00b7tem", "hert\u00b7zen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PPOSAT", "NN", "$.", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Nicht lasse dir das wort unglaublich kommen f\u00fcr.", "tokens": ["Nicht", "las\u00b7se", "dir", "das", "wort", "un\u00b7glaub\u00b7lich", "kom\u00b7men", "f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "PPER", "ART", "NN", "ADJD", "VVFIN", "APPR", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Die flammen unsrer eh sind ausgeleschte kertzen,", "tokens": ["Die", "flam\u00b7men", "uns\u00b7rer", "eh", "sind", "aus\u00b7ge\u00b7leschte", "kert\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPOSAT", "NN", "VAFIN", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.16": {"text": "Ja unser\u2019 erste flamm entsteht, mein licht! aus dir.", "tokens": ["Ja", "un\u00b7ser'", "ers\u00b7te", "flamm", "ent\u00b7steht", ",", "mein", "licht", "!", "aus", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKANT", "PPOSAT", "ADJA", "NN", "VVFIN", "$,", "PPOSAT", "NN", "$.", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Ich hab\u2019 erst, seit ich dir geopffert meine seele,", "tokens": ["Ich", "hab'", "erst", ",", "seit", "ich", "dir", "ge\u00b7opf\u00b7fert", "mei\u00b7ne", "see\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "$,", "KOUS", "PPER", "PPER", "VVPP", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Was lieb\u2019 und liebens-wehrt, mein kind! von dir gelernt.", "tokens": ["Was", "lieb'", "und", "lie\u00b7bens\u00b7wehrt", ",", "mein", "kind", "!", "von", "dir", "ge\u00b7lernt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "KON", "VVFIN", "$,", "PPOSAT", "NN", "$.", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Das ew\u2019ge feuer brennt nicht in jedweder h\u00f6le,", "tokens": ["Das", "ew'\u00b7ge", "feu\u00b7er", "brennt", "nicht", "in", "jed\u00b7we\u00b7der", "h\u00f6\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PTKNEG", "APPR", "PIAT", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Du weist, da\u00df offt der schnee wie eine Venus sternt.", "tokens": ["Du", "weist", ",", "da\u00df", "offt", "der", "schnee", "wie", "ei\u00b7ne", "Ve\u00b7nus", "sternt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "ADV", "ART", "NN", "KOKOM", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Es soll\u2019n ja wohl corall\u2019n seyn der gemahlin lippen,", "tokens": ["Es", "soll'n", "ja", "wohl", "corall'n", "seyn", "der", "ge\u00b7mah\u00b7lin", "lip\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "ADJD", "VAINF", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.22": {"text": "Kein liebreitz, kein magnet begeistert aber sie;", "tokens": ["Kein", "lieb\u00b7reitz", ",", "kein", "mag\u00b7net", "be\u00b7geis\u00b7tert", "a\u00b7ber", "sie", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PIAT", "ADJD", "VVPP", "KON", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "L\u00e4st sich nun stahl nicht ziehn von unbeseelten klippen,", "tokens": ["L\u00e4st", "sich", "nun", "stahl", "nicht", "ziehn", "von", "un\u00b7be\u00b7seel\u00b7ten", "klip\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "ADV", "PTKNEG", "VVINF", "APPR", "ADJA", "VVINF", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.24": {"text": "Wer schilt, da\u00df ich an ihr mich nicht zu kleben m\u00fch\u2019.", "tokens": ["Wer", "schilt", ",", "da\u00df", "ich", "an", "ihr", "mich", "nicht", "zu", "kle\u00b7ben", "m\u00fch'", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "KOUS", "PPER", "APPR", "PPER", "PRF", "PTKNEG", "PTKZU", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Die augen sind zwar schwartz, doch ausgeleschte kohlen,", "tokens": ["Die", "au\u00b7gen", "sind", "zwar", "schwartz", ",", "doch", "aus\u00b7ge\u00b7leschte", "koh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "$,", "ADV", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.26": {"text": "Von denen schwefel sich selbst nicht entz\u00fcnden kan.", "tokens": ["Von", "de\u00b7nen", "schwe\u00b7fel", "sich", "selbst", "nicht", "ent\u00b7z\u00fcn\u00b7den", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "ADJD", "PRF", "ADV", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Sie r\u00fchmt sich, ihr geruch besch\u00e4me die violen,", "tokens": ["Sie", "r\u00fchmt", "sich", ",", "ihr", "ge\u00b7ruch", "be\u00b7sch\u00e4\u00b7me", "die", "vi\u00b7o\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "$,", "PPER", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Was aber n\u00fctzt zibeth, der uns nicht bisamt an?", "tokens": ["Was", "a\u00b7ber", "n\u00fctzt", "zi\u00b7beth", ",", "der", "uns", "nicht", "bi\u00b7samt", "an", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADJD", "VVFIN", "$,", "PRELS", "PPER", "PTKNEG", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Die wangen sind an ihr granaten ohne kerne,", "tokens": ["Die", "wan\u00b7gen", "sind", "an", "ihr", "gra\u00b7na\u00b7ten", "oh\u00b7ne", "ker\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "PPOSAT", "NN", "APPR", "NN", "$,"], "meter": "-+-++--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.30": {"text": "Gebl\u00fcme, das nicht re\u00fccht, ein feld von rosen leer.", "tokens": ["Ge\u00b7bl\u00fc\u00b7me", ",", "das", "nicht", "re\u00fccht", ",", "ein", "feld", "von", "ro\u00b7sen", "leer", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PTKNEG", "VVFIN", "$,", "ART", "NN", "APPR", "NE", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Die br\u00fcste regt kein trieb entflammter liebes-sterne,", "tokens": ["Die", "br\u00fcs\u00b7te", "regt", "kein", "trieb", "ent\u00b7flamm\u00b7ter", "lie\u00b7bes\u00b7ster\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "PIAT", "VVFIN", "ADJD", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Sie sind von Zemblens ei\u00df ein zugefroren meer.", "tokens": ["Sie", "sind", "von", "Zem\u00b7blens", "ei\u00df", "ein", "zu\u00b7ge\u00b7fro\u00b7ren", "meer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NE", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Sie schleppt der heyrath band wie eine sclaven-kette;", "tokens": ["Sie", "schleppt", "der", "hey\u00b7rath", "band", "wie", "ei\u00b7ne", "scla\u00b7ven\u00b7ket\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVFIN", "KOKOM", "ART", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Sie fleucht, als legten sich mit mir ihr schlangen bey.", "tokens": ["Sie", "fleucht", ",", "als", "leg\u00b7ten", "sich", "mit", "mir", "ihr", "schlan\u00b7gen", "bey", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "VVFIN", "PRF", "APPR", "PPER", "PPER", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Ja find\u2019 ich einst noch platz in der gemahlin bette,", "tokens": ["Ja", "find'", "ich", "einst", "noch", "platz", "in", "der", "ge\u00b7mah\u00b7lin", "bet\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "VVFIN", "PPER", "ADV", "ADV", "VVIMP", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "So glaub\u2019, da\u00df sie mehr kalt als salamander sey.", "tokens": ["So", "glaub'", ",", "da\u00df", "sie", "mehr", "kalt", "als", "sa\u00b7la\u00b7man\u00b7der", "sey", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "KOUS", "PPER", "ADV", "ADJD", "KOKOM", "ADJD", "VAFIN", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.37": {"text": "Kein feur, darinnen Zevs in fliessend gold zerronnen,", "tokens": ["Kein", "feur", ",", "da\u00b7rin\u00b7nen", "Zevs", "in", "flies\u00b7send", "gold", "zer\u00b7ron\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "ADV", "NE", "APPR", "ADJD", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Kein lodernd seufftzen fl\u00f6\u00dft ihr laulicht lieben ein,", "tokens": ["Kein", "lo\u00b7dernd", "seufft\u00b7zen", "fl\u00f6\u00dft", "ihr", "lau\u00b7licht", "lie\u00b7ben", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJD", "VVINF", "VVFIN", "PPER", "ADJD", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Der nord-stern w\u00e4rmt den Belt mehr, als mich ihre sonnen,", "tokens": ["Der", "nord\u00b7stern", "w\u00e4rmt", "den", "Belt", "mehr", ",", "als", "mich", "ih\u00b7re", "son\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "ADV", "$,", "KOUS", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Sie m\u00fcht sich steinerner als Niobe zu seyn.", "tokens": ["Sie", "m\u00fcht", "sich", "stei\u00b7ner\u00b7ner", "als", "Ni\u00b7o\u00b7be", "zu", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADJD", "KOKOM", "NE", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Will ich zum opffer ihr gleich meine seel anz\u00fcnden,", "tokens": ["Will", "ich", "zum", "opf\u00b7fer", "ihr", "gleich", "mei\u00b7ne", "seel", "an\u00b7z\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPRART", "NN", "PPER", "ADV", "PPOSAT", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "So d\u00fcnckt sie sich doch mir vor ein altar zu gut:", "tokens": ["So", "d\u00fcnckt", "sie", "sich", "doch", "mir", "vor", "ein", "al\u00b7tar", "zu", "gut", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ADV", "PPER", "APPR", "ART", "NN", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Das f\u00fchlen, glaub ich, hat bey ihr selbst kein empfinden,", "tokens": ["Das", "f\u00fch\u00b7len", ",", "glaub", "ich", ",", "hat", "bey", "ihr", "selbst", "kein", "emp\u00b7fin\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "VVFIN", "PPER", "$,", "VAFIN", "APPR", "PPER", "ADV", "PIAT", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Und unser liebes-\u00f6l braucht sie vor kalte fluth.", "tokens": ["Und", "un\u00b7ser", "lie\u00b7bes\u00b7\u00f6l", "braucht", "sie", "vor", "kal\u00b7te", "fluth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.45": {"text": "Doch w\u00fcrde der geduld noch dieser unmuth weichen,", "tokens": ["Doch", "w\u00fcr\u00b7de", "der", "ge\u00b7duld", "noch", "die\u00b7ser", "un\u00b7muth", "wei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "ADV", "PDAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "W\u00e4r\u2019 er mit eyfersucht und hochmuth nicht vergallt.", "tokens": ["W\u00e4r'", "er", "mit", "ey\u00b7fer\u00b7sucht", "und", "hoch\u00b7muth", "nicht", "ver\u00b7gallt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "NN", "KON", "NN", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "Sie ist dem rosen-strauch im winter zu vergleichen,", "tokens": ["Sie", "ist", "dem", "ro\u00b7sen\u00b7strauch", "im", "win\u00b7ter", "zu", "ver\u00b7glei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPRART", "ADJA", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.48": {"text": "Der keine rosen tr\u00e4gt, und doch den dorn beh\u00e4lt.", "tokens": ["Der", "kei\u00b7ne", "ro\u00b7sen", "tr\u00e4gt", ",", "und", "doch", "den", "dorn", "be\u00b7h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "$,", "KON", "ADV", "ART", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.49": {"text": "Urtheile nun, mein kind! ob wir verliebt seyn m\u00f6gen?", "tokens": ["Ur\u00b7thei\u00b7le", "nun", ",", "mein", "kind", "!", "ob", "wir", "ver\u00b7liebt", "seyn", "m\u00f6\u00b7gen", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "PPOSAT", "NN", "$.", "KOUS", "PPER", "VVPP", "VAINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.50": {"text": "Selbst Aetna leschet aus, wenn zunder ihm gebricht,", "tokens": ["Selbst", "A\u00b7et\u00b7na", "le\u00b7schet", "aus", ",", "wenn", "zun\u00b7der", "ihm", "ge\u00b7bricht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "VVFIN", "PTKVZ", "$,", "KOUS", "ADV", "PPER", "VVFIN", "$,"], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.51": {"text": "Der wahn ehrt g\u00f6tzen nur, die einem sind entgegen,", "tokens": ["Der", "wahn", "ehrt", "g\u00f6t\u00b7zen", "nur", ",", "die", "ei\u00b7nem", "sind", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NE", "ADV", "$,", "PRELS", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.52": {"text": "Die lieb erkennt den ha\u00df vor keinen abgott nicht;", "tokens": ["Die", "lieb", "er\u00b7kennt", "den", "ha\u00df", "vor", "kei\u00b7nen", "ab\u00b7gott", "nicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "ART", "NN", "APPR", "PIAT", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.53": {"text": "Die Anmuth aber hat den sitz bey dir erwehlet,", "tokens": ["Die", "An\u00b7muth", "a\u00b7ber", "hat", "den", "sitz", "bey", "dir", "er\u00b7weh\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VAFIN", "ART", "NN", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.54": {"text": "Der unhold w\u00fcsteney ist weit von dir verbannt.", "tokens": ["Der", "un\u00b7hold", "w\u00fcs\u00b7te\u00b7ney", "ist", "weit", "von", "dir", "ver\u00b7bannt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VAFIN", "VAFIN", "ADJD", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.55": {"text": "Mit deinen sitten hat der liebreitz sich verm\u00e4hlet,", "tokens": ["Mit", "dei\u00b7nen", "sit\u00b7ten", "hat", "der", "lieb\u00b7reitz", "sich", "ver\u00b7m\u00e4h\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.56": {"text": "Dein sch\u00f6n-seyn hat ein garn der freyheit ausgespannt,", "tokens": ["Dein", "sch\u00f6n\u00b7seyn", "hat", "ein", "garn", "der", "frey\u00b7heit", "aus\u00b7ge\u00b7spannt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.57": {"text": "In diesem siehst du mich hier deinen sclaven liegen,", "tokens": ["In", "die\u00b7sem", "siehst", "du", "mich", "hier", "dei\u00b7nen", "scla\u00b7ven", "lie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "VVFIN", "PPER", "PRF", "ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.58": {"text": "Versto\u00df nun diesen nicht, den du selbst fesselst an.", "tokens": ["Ver\u00b7sto\u00df", "nun", "die\u00b7sen", "nicht", ",", "den", "du", "selbst", "fes\u00b7selst", "an", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PDAT", "PTKNEG", "$,", "PRELS", "PPER", "ADV", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.59": {"text": "La\u00df mich von deiner hand kein sch\u00e4rffer urtheil kriegen,", "tokens": ["La\u00df", "mich", "von", "dei\u00b7ner", "hand", "kein", "sch\u00e4rf\u00b7fer", "ur\u00b7theil", "krie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "PPOSAT", "NN", "PIAT", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.60": {"text": "Als dein liebreitzend aug\u2019 uns prophezeyen kan.", "tokens": ["Als", "dein", "lieb\u00b7reit\u00b7zend", "aug'", "uns", "pro\u00b7phe\u00b7ze\u00b7yen", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJD", "VVFIN", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.61": {"text": "Die sonne, welche du zweyfach in augen tr\u00e4gest,", "tokens": ["Die", "son\u00b7ne", ",", "wel\u00b7che", "du", "zwey\u00b7fach", "in", "au\u00b7gen", "tr\u00e4\u00b7gest", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.62": {"text": "Pflantzt, wie du mir, den trieb den sonnenwenden ein;", "tokens": ["Pflantzt", ",", "wie", "du", "mir", ",", "den", "trieb", "den", "son\u00b7nen\u00b7wen\u00b7den", "ein", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "PPER", "PPER", "$,", "ART", "VVFIN", "ART", "ADJA", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.63": {"text": "Die hold, so du selbst feil auf deinem mund auslegest,", "tokens": ["Die", "hold", ",", "so", "du", "selbst", "feil", "auf", "dei\u00b7nem", "mund", "aus\u00b7le\u00b7gest", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "$,", "ADV", "PPER", "ADV", "ADJD", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.64": {"text": "Kan meiner seele nicht verbothne waare seyn.", "tokens": ["Kan", "mei\u00b7ner", "see\u00b7le", "nicht", "ver\u00b7both\u00b7ne", "waa\u00b7re", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "VVFIN", "PTKNEG", "VVFIN", "ADJA", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.65": {"text": "Das kauff-geld, das ich dir vor deine lieb\u2019 erlege,", "tokens": ["Das", "kauff\u00b7geld", ",", "das", "ich", "dir", "vor", "dei\u00b7ne", "lieb'", "er\u00b7le\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.66": {"text": "Soll seyn mein bester schatz, mein hertz\u2019, an diesem ist", "tokens": ["Soll", "seyn", "mein", "bes\u00b7ter", "schatz", ",", "mein", "hertz'", ",", "an", "die\u00b7sem", "ist"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["VMFIN", "VAINF", "PPOSAT", "ADJA", "NN", "$,", "PPOSAT", "PTKVZ", "$,", "APPR", "PDS", "VAFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.67": {"text": "Dis treue schrot und korn, du selbst bist das gepr\u00e4ge,", "tokens": ["Dis", "treu\u00b7e", "schrot", "und", "korn", ",", "du", "selbst", "bist", "das", "ge\u00b7pr\u00e4\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADJD", "KON", "NN", "$,", "PPER", "ADV", "VAFIN", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.68": {"text": "Ich wei\u00df, da\u00df du mit nichts sonst zu erkauffen bist.", "tokens": ["Ich", "wei\u00df", ",", "da\u00df", "du", "mit", "nichts", "sonst", "zu", "er\u00b7kauf\u00b7fen", "bist", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "APPR", "PIS", "ADV", "PTKZU", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.69": {"text": "Dir aber wird ja m\u00fcntz, auf der dein bild steht, gelten,", "tokens": ["Dir", "a\u00b7ber", "wird", "ja", "m\u00fcntz", ",", "auf", "der", "dein", "bild", "steht", ",", "gel\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "ADV", "ADJD", "$,", "APPR", "ART", "PPOSAT", "NN", "VVFIN", "$,", "VVFIN", "$,"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.70": {"text": "Ob dich, unsch\u00e4tzbare, kein schatz gleich zahlen kan.", "tokens": ["Ob", "dich", ",", "un\u00b7sch\u00e4tz\u00b7ba\u00b7re", ",", "kein", "schatz", "gleich", "zah\u00b7len", "kan", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "ADJA", "$,", "PIAT", "NN", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.71": {"text": "Nicht sorge, da\u00df ein mensch wird deine liebe schelten,", "tokens": ["Nicht", "sor\u00b7ge", ",", "da\u00df", "ein", "mensch", "wird", "dei\u00b7ne", "lie\u00b7be", "schel\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "$,", "KOUS", "ART", "ADJD", "VAFIN", "PPOSAT", "ADJA", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.72": {"text": "Man nimmt mit fug in dienst versto\u00dfne diener an.", "tokens": ["Man", "nimmt", "mit", "fug", "in", "dienst", "ver\u00b7sto\u00df\u00b7ne", "die\u00b7ner", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "NN", "APPR", "PDAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.73": {"text": "Es mag auch alle welt, wie ich dich liebe, wissen,", "tokens": ["Es", "mag", "auch", "al\u00b7le", "welt", ",", "wie", "ich", "dich", "lie\u00b7be", ",", "wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PIAT", "NN", "$,", "PWAV", "PPER", "PRF", "VVFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.74": {"text": "Denn heimlich buhlen ist zwar s\u00fcsse, selten rein.", "tokens": ["Denn", "heim\u00b7lich", "buh\u00b7len", "ist", "zwar", "s\u00fcs\u00b7se", ",", "sel\u00b7ten", "rein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVINF", "VAFIN", "ADV", "VVFIN", "$,", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.75": {"text": "Mein vorsatz hat in sich ein redliches entschl\u00fcssen,", "tokens": ["Mein", "vor\u00b7satz", "hat", "in", "sich", "ein", "red\u00b7li\u00b7ches", "ent\u00b7schl\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "APPR", "PRF", "ART", "ADJA", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.76": {"text": "Das auch der kirche selbst nicht kan verdammlich seyn.", "tokens": ["Das", "auch", "der", "kir\u00b7che", "selbst", "nicht", "kan", "ver\u00b7damm\u00b7lich", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ART", "NN", "ADV", "PTKNEG", "VMFIN", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.77": {"text": "Kein h\u00e4\u00dflich rabe zeucht an meinem liebes-wagen,", "tokens": ["Kein", "h\u00e4\u00df\u00b7lich", "ra\u00b7be", "zeucht", "an", "mei\u00b7nem", "lie\u00b7bes\u00b7wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJD", "VVFIN", "VVFIN", "APPR", "PPOSAT", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.78": {"text": "Die schwanen keuscher lust ziehn meinen geist zu dir,", "tokens": ["Die", "schwa\u00b7nen", "keu\u00b7scher", "lust", "ziehn", "mei\u00b7nen", "geist", "zu", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.79": {"text": "Von reinen lilgen soll dein haupt den braut-krantz tragen,", "tokens": ["Von", "rei\u00b7nen", "lil\u00b7gen", "soll", "dein", "haupt", "den", "braut\u00b7krantz", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VMFIN", "PPOSAT", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.80": {"text": "Mit hochzeit-fackeln uns die tugend leuchten f\u00fcr.", "tokens": ["Mit", "hoch\u00b7zeit\u00b7fa\u00b7ckeln", "uns", "die", "tu\u00b7gend", "leuch\u00b7ten", "f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "ART", "NN", "VVFIN", "APPR", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.81": {"text": "Die hand des priesters soll dir selbst den trau-ring geben,", "tokens": ["Die", "hand", "des", "pries\u00b7ters", "soll", "dir", "selbst", "den", "trau\u00b7ring", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VMFIN", "PPER", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.82": {"text": "Weil freylich du zu hoch f\u00fcr eine Hagar bist.", "tokens": ["Weil", "frey\u00b7lich", "du", "zu", "hoch", "f\u00fcr", "ei\u00b7ne", "Ha\u00b7gar", "bist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPER", "PTKA", "ADJD", "APPR", "ART", "NE", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.83": {"text": "Kurtz: keine raupe soll an unsern myrthen kleben,", "tokens": ["Kurtz", ":", "kei\u00b7ne", "rau\u00b7pe", "soll", "an", "un\u00b7sern", "myr\u00b7then", "kle\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$.", "PIAT", "VVFIN", "VMFIN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.84": {"text": "Die deiner ehr\u2019 abbricht, und unsern nachruhm frist.", "tokens": ["Die", "dei\u00b7ner", "ehr'", "ab\u00b7bricht", ",", "und", "un\u00b7sern", "nach\u00b7ruhm", "frist", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "$,", "KON", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.85": {"text": "Kommt dir di\u00df seltsam f\u00fcr, verm\u00e4hlten sich verm\u00e4hlen,", "tokens": ["Kommt", "dir", "di\u00df", "selt\u00b7sam", "f\u00fcr", ",", "ver\u00b7m\u00e4hl\u00b7ten", "sich", "ver\u00b7m\u00e4h\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PDS", "ADJD", "APPR", "$,", "VVFIN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.86": {"text": "Weil keine Sara mehr den mann zu andern weist,", "tokens": ["Weil", "kei\u00b7ne", "Sa\u00b7ra", "mehr", "den", "mann", "zu", "an\u00b7dern", "weist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "ADV", "ART", "NN", "APPR", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.87": {"text": "Wo wehrts der himmel uns, zwey seelen zu erwehlen,", "tokens": ["Wo", "wehrts", "der", "him\u00b7mel", "uns", ",", "zwey", "see\u00b7len", "zu", "er\u00b7weh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "PPER", "$,", "CARD", "VVINF", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.88": {"text": "Bevor wenn eine selbst das band in st\u00fccke reist?", "tokens": ["Be\u00b7vor", "wenn", "ei\u00b7ne", "selbst", "das", "band", "in", "st\u00fc\u00b7cke", "reist", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "ART", "ADV", "PDS", "VVFIN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.89": {"text": "Was t\u00e4glich nicht geschicht, ist nicht bald zu verdammen.", "tokens": ["Was", "t\u00e4g\u00b7lich", "nicht", "ge\u00b7schicht", ",", "ist", "nicht", "bald", "zu", "ver\u00b7dam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "PTKNEG", "VVPP", "$,", "VAFIN", "PTKNEG", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.90": {"text": "Zu dem, der gr\u00f6ste theil der menschen spricht es recht:", "tokens": ["Zu", "dem", ",", "der", "gr\u00f6s\u00b7te", "theil", "der", "men\u00b7schen", "spricht", "es", "recht", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$,", "ART", "ADJA", "NN", "ART", "NN", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.91": {"text": "Die vorwelt labte sich bey zwey und mehrern flammen:", "tokens": ["Die", "vor\u00b7welt", "lab\u00b7te", "sich", "bey", "zwey", "und", "meh\u00b7rern", "flam\u00b7men", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "PRF", "APPR", "CARD", "KON", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.92": {"text": "Ein f\u00fcrst ist auch nicht stracks gemeiner ordnung knecht.", "tokens": ["Ein", "f\u00fcrst", "ist", "auch", "nicht", "stracks", "ge\u00b7mei\u00b7ner", "ord\u00b7nung", "knecht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VAFIN", "ADV", "PTKNEG", "ADV", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.93": {"text": "Kein eyfrend auge wird dir scheele blicke geben,", "tokens": ["Kein", "ey\u00b7frend", "au\u00b7ge", "wird", "dir", "schee\u00b7le", "bli\u00b7cke", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJD", "NN", "VAFIN", "PPER", "VVFIN", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.94": {"text": "Weil, die ihr hertz entfernt, sich selbst zu trennen sinnt,", "tokens": ["Weil", ",", "die", "ihr", "hertz", "ent\u00b7fernt", ",", "sich", "selbst", "zu", "tren\u00b7nen", "sinnt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "PRELS", "PPOSAT", "NN", "VVPP", "$,", "PRF", "ADV", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.95": {"text": "Mich wirst du durch ein ja ins paradie\u00df erheben,", "tokens": ["Mich", "wirst", "du", "durch", "ein", "ja", "ins", "pa\u00b7ra\u00b7die\u00df", "er\u00b7he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "APPR", "ART", "ADV", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.96": {"text": "Darinnen aber auch vor dich was s\u00fcsses rinnt.", "tokens": ["Da\u00b7rin\u00b7nen", "a\u00b7ber", "auch", "vor", "dich", "was", "s\u00fcs\u00b7ses", "rinnt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "APPR", "PPER", "PWS", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.97": {"text": "An meine lincke hand wird man dich zwar nur trauen;", "tokens": ["An", "mei\u00b7ne", "lin\u00b7cke", "hand", "wird", "man", "dich", "zwar", "nur", "trau\u00b7en", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VAFIN", "PIS", "PRF", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.98": {"text": "Solch kummer aber f\u00e4llt, wenn sie, mein schatz! versteht,", "tokens": ["Solch", "kum\u00b7mer", "a\u00b7ber", "f\u00e4llt", ",", "wenn", "sie", ",", "mein", "schatz", "!", "ver\u00b7steht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "VVFIN", "$,", "KOUS", "PPER", "$,", "PPOSAT", "NN", "$.", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.99": {"text": "Da\u00df man mit mehrer pracht der rechten pflegt zu freyen,", "tokens": ["Da\u00df", "man", "mit", "meh\u00b7rer", "pracht", "der", "rech\u00b7ten", "pflegt", "zu", "frey\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "PIAT", "NN", "ART", "ADJA", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.100": {"text": "Doch da\u00df die lincke nur von treuem hertzen geht.", "tokens": ["Doch", "da\u00df", "die", "lin\u00b7cke", "nur", "von", "treu\u00b7em", "hert\u00b7zen", "geht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "ADJA", "ADV", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}