{"textgrid.poem.45970": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Bei Christian Grafen von Stolbergs Tod zu St. Amand, in der Schlacht de la belle Alliance, den 19. Juni 1815", "genre": "verse", "period": "N.A.", "pub_year": 1815, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Krieg zog aus, zu kaufen", "tokens": ["Der", "Krieg", "zog", "aus", ",", "zu", "kau\u00b7fen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein ungewisses Los,", "tokens": ["Ein", "un\u00b7ge\u00b7wis\u00b7ses", "Los", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und wirft zu ganzen Haufen", "tokens": ["Und", "wirft", "zu", "gan\u00b7zen", "Hau\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dem Sieg die h\u00f6chsten G\u00fcter in den Scho\u00df.", "tokens": ["Dem", "Sieg", "die", "h\u00f6chs\u00b7ten", "G\u00fc\u00b7ter", "in", "den", "Scho\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "In freudigem Vertrauen", "tokens": ["In", "freu\u00b7di\u00b7gem", "Ver\u00b7trau\u00b7en"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Kr\u00e4nzt sich, wer \u00fcbrig blieb,", "tokens": ["Kr\u00e4nzt", "sich", ",", "wer", "\u00fcb\u00b7rig", "blieb", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "$,", "PWS", "ADJD", "VVFIN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Und ich mu\u00df niederschauen,", "tokens": ["Und", "ich", "mu\u00df", "nie\u00b7der\u00b7schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Denn Einer sank, er war den Besten lieb.", "tokens": ["Denn", "Ei\u00b7ner", "sank", ",", "er", "war", "den", "Bes\u00b7ten", "lieb", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "$,", "PPER", "VAFIN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Du Spiegel aller G\u00fcte,", "tokens": ["Du", "Spie\u00b7gel", "al\u00b7ler", "G\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Du frommes Jugendblut,", "tokens": ["Du", "from\u00b7mes", "Ju\u00b7gend\u00b7blut", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du sankst, du Adelsbl\u00fcte,", "tokens": ["Du", "sankst", ",", "du", "A\u00b7dels\u00b7bl\u00fc\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mein Stolberg, o wir waren dir so gut.", "tokens": ["Mein", "Stol\u00b7berg", ",", "o", "wir", "wa\u00b7ren", "dir", "so", "gut", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "FM", "PPER", "VAFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "So stark, so frei, so t\u00fcchtig,", "tokens": ["So", "stark", ",", "so", "frei", ",", "so", "t\u00fcch\u00b7tig", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADV", "ADJD", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So kindlich, freudig, fromm,", "tokens": ["So", "kind\u00b7lich", ",", "freu\u00b7dig", ",", "fromm", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADJD", "$,", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So mutig und so z\u00fcchtig,", "tokens": ["So", "mu\u00b7tig", "und", "so", "z\u00fcch\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADV", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mein Stolberg war im Himmel recht willkomm.", "tokens": ["Mein", "Stol\u00b7berg", "war", "im", "Him\u00b7mel", "recht", "will\u00b7komm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "APPRART", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Sagt! wer verdient zu siegen,", "tokens": ["Sagt", "!", "wer", "ver\u00b7dient", "zu", "sie\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "PWS", "VVPP", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wer gilt so hohen Preis,", "tokens": ["Wer", "gilt", "so", "ho\u00b7hen", "Preis", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wenn solche Opfer liegen", "tokens": ["Wenn", "sol\u00b7che", "Op\u00b7fer", "lie\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PIAT", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zu des Triumphes blut'gem Ehrengleis.", "tokens": ["Zu", "des", "Tri\u00b7um\u00b7phes", "blut'\u00b7gem", "Eh\u00b7ren\u00b7gleis", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.6": {"line.1": {"text": "Drum horcht, ihr Siegesmeister,", "tokens": ["Drum", "horcht", ",", "ihr", "Sie\u00b7ges\u00b7meis\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wenn man die Fahne schwingt,", "tokens": ["Wenn", "man", "die", "Fah\u00b7ne", "schwingt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Drin rauschen edle Geister,", "tokens": ["Drin", "rau\u00b7schen", "ed\u00b7le", "Geis\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die keine L\u00fcgenkunst je wiederbringt.", "tokens": ["Die", "kei\u00b7ne", "L\u00fc\u00b7gen\u00b7kunst", "je", "wie\u00b7der\u00b7bringt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Wi\u00dft, da\u00df ein Tag mu\u00df kommen", "tokens": ["Wi\u00dft", ",", "da\u00df", "ein", "Tag", "mu\u00df", "kom\u00b7men"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "KOUS", "ART", "NN", "VMFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In Volks-, in Gotteskraft,", "tokens": ["In", "Volks", ",", "in", "Got\u00b7tes\u00b7kraft", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "TRUNC", "$,", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wo Rechnung wird genommen", "tokens": ["Wo", "Rech\u00b7nung", "wird", "ge\u00b7nom\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "NN", "VAFIN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "F\u00fcr alle, die der Sieg hat hingerafft.", "tokens": ["F\u00fcr", "al\u00b7le", ",", "die", "der", "Sieg", "hat", "hin\u00b7ge\u00b7rafft", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "$,", "PRELS", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Der Braunschweig ist gestorben,", "tokens": ["Der", "Braun\u00b7schweig", "ist", "ge\u00b7stor\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der hat sich ausgel\u00f6st", "tokens": ["Der", "hat", "sich", "aus\u00b7ge\u00b7l\u00f6st"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "PRF", "VVPP"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Rechtfertigung erworben,", "tokens": ["Recht\u00b7fer\u00b7ti\u00b7gung", "er\u00b7wor\u00b7ben", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Von allen F\u00fcrstenschulden sich entbl\u00f6\u00dft.", "tokens": ["Von", "al\u00b7len", "F\u00fcrs\u00b7ten\u00b7schul\u00b7den", "sich", "ent\u00b7bl\u00f6\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Es stirbt durch Rosses Hufen", "tokens": ["Es", "stirbt", "durch", "Ros\u00b7ses", "Hu\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Kein H\u00e4lmlein in dem Feld,", "tokens": ["Kein", "H\u00e4lm\u00b7lein", "in", "dem", "Feld", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df, der den Reuter gerufen,", "tokens": ["Da\u00df", ",", "der", "den", "Reu\u00b7ter", "ge\u00b7ru\u00b7fen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "PRELS", "ART", "NE", "VVPP", "$,"], "meter": "++-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Nicht werde drum in das Gericht gestellt.", "tokens": ["Nicht", "wer\u00b7de", "drum", "in", "das", "Ge\u00b7richt", "ge\u00b7stellt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VAFIN", "PAV", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Die Saaten sollt ihr h\u00fcten,", "tokens": ["Die", "Saa\u00b7ten", "sollt", "ihr", "h\u00fc\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die frommes Blut getr\u00e4nkt,", "tokens": ["Die", "from\u00b7mes", "Blut", "ge\u00b7tr\u00e4nkt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dem Vaterland verg\u00fcten,", "tokens": ["Dem", "Va\u00b7ter\u00b7land", "ver\u00b7g\u00fc\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das Leben, das der Opfernde ihm schenkt.", "tokens": ["Das", "Le\u00b7ben", ",", "das", "der", "Op\u00b7fern\u00b7de", "ihm", "schenkt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+---+", "measure": "zehnsilber"}}, "stanza.11": {"line.1": {"text": "Nur darum ist gefallen", "tokens": ["Nur", "da\u00b7rum", "ist", "ge\u00b7fal\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "PAV", "VAFIN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Stolberg aus freiem Mut,", "tokens": ["Stol\u00b7berg", "aus", "frei\u00b7em", "Mut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ADJA", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Da\u00df den Gerechten allen", "tokens": ["Da\u00df", "den", "Ge\u00b7rech\u00b7ten", "al\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "PIAT"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Sein freies teutsches Leben komm zu gut.", "tokens": ["Sein", "frei\u00b7es", "teut\u00b7sches", "Le\u00b7ben", "komm", "zu", "gut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "ADJA", "NN", "VVFIN", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.12": {"line.1": {"text": "Dies ist der letzte Willen", "tokens": ["Dies", "ist", "der", "letz\u00b7te", "Wil\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Bei jedes Helden Tod,", "tokens": ["Bei", "je\u00b7des", "Hel\u00b7den", "Tod", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und diesen zu erf\u00fcllen,", "tokens": ["Und", "die\u00b7sen", "zu", "er\u00b7f\u00fcl\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das tut euch F\u00fcrsten, und dir Teutschland not.", "tokens": ["Das", "tut", "euch", "F\u00fcrs\u00b7ten", ",", "und", "dir", "Teutschland", "not", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "NN", "$,", "KON", "PPER", "NE", "NE", "$."], "meter": "-+-+--+++", "measure": "iambic.penta.relaxed"}}, "stanza.13": {"line.1": {"text": "Mit seines Vaters Segen", "tokens": ["Mit", "sei\u00b7nes", "Va\u00b7ters", "Se\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und mit dem Ku\u00df der Braut,", "tokens": ["Und", "mit", "dem", "Ku\u00df", "der", "Braut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und mit dem teutschen Degen", "tokens": ["Und", "mit", "dem", "teut\u00b7schen", "De\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Hat seinen ganzen Schatz er Gott vertraut.", "tokens": ["Hat", "sei\u00b7nen", "gan\u00b7zen", "Schatz", "er", "Gott", "ver\u00b7traut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "ADJA", "NN", "PPER", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.14": {"line.1": {"text": "Der hat ihn hingenommen", "tokens": ["Der", "hat", "ihn", "hin\u00b7ge\u00b7nom\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Aus dieser wilden Welt,", "tokens": ["Aus", "die\u00b7ser", "wil\u00b7den", "Welt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Den Starken, Reinen, Frommen", "tokens": ["Den", "Star\u00b7ken", ",", "Rei\u00b7nen", ",", "From\u00b7men"], "token_info": ["word", "word", "punct", "word", "punct", "word"], "pos": ["ART", "NN", "$,", "NN", "$,", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dort bessern Kampfes Siegern zugesellt.", "tokens": ["Dort", "bes\u00b7sern", "Kamp\u00b7fes", "Sie\u00b7gern", "zu\u00b7ge\u00b7sellt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.15": {"line.1": {"text": "So ihr den Sieg nicht ehret", "tokens": ["So", "ihr", "den", "Sieg", "nicht", "eh\u00b7ret"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "ART", "NN", "PTKNEG", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Den solches Blut erkauft,", "tokens": ["Den", "sol\u00b7ches", "Blut", "er\u00b7kauft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So ihr zum B\u00f6sen kehret", "tokens": ["So", "ihr", "zum", "B\u00f6\u00b7sen", "keh\u00b7ret"