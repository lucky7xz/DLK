{"dta.poem.11739": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "25.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wer in den Spiegel sieht, und sieht sich sch\u00f6n darin,", "tokens": ["Wer", "in", "den", "Spie\u00b7gel", "sieht", ",", "und", "sieht", "sich", "sch\u00f6n", "da\u00b7rin", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "VVFIN", "$,", "KON", "VVFIN", "PRF", "ADJD", "PAV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der spreche: Mache Gott mich gut, wie sch\u00f6n ich bin.", "tokens": ["Der", "spre\u00b7che", ":", "Ma\u00b7che", "Gott", "mich", "gut", ",", "wie", "sch\u00f6n", "ich", "bin", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$.", "ADJA", "NN", "PPER", "ADJD", "$,", "PWAV", "ADJD", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Und wer den Spiegel sieht und sieht darin sich h\u00e4\u00dflich,", "tokens": ["Und", "wer", "den", "Spie\u00b7gel", "sieht", "und", "sieht", "da\u00b7rin", "sich", "h\u00e4\u00df\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "VVFIN", "KON", "VVFIN", "PAV", "PRF", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der denke, G\u00fcte sei ihm doppelt unerl\u00e4\u00dflich.", "tokens": ["Der", "den\u00b7ke", ",", "G\u00fc\u00b7te", "sei", "ihm", "dop\u00b7pelt", "un\u00b7er\u00b7l\u00e4\u00df\u00b7lich", "."], "token_info": ["word"