{"dta.poem.9893": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich bin wie ein vogel schnell/ eines alters mit der erden;", "tokens": ["Ich", "bin", "wie", "ein", "vo\u00b7gel", "schnell", "/", "ei\u00b7nes", "al\u00b7ters", "mit", "der", "er\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "KOKOM", "ART", "NN", "ADJD", "$(", "ART", "ADJA", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "Doch ein solch zerst\u00f6rer auch/ als nur kan gefunden werden.", "tokens": ["Doch", "ein", "solch", "zer\u00b7st\u00f6\u00b7rer", "auch", "/", "als", "nur", "kan", "ge\u00b7fun\u00b7den", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PIAT", "ADJA", "ADV", "$(", "KOKOM", "ADV", "VMFIN", "VVPP", "VAINF", "$."], "meter": "-+--+--+-+-+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Keiner ist/ der mich nicht kennet auf der see/ in meinem reich/", "tokens": ["Kei\u00b7ner", "ist", "/", "der", "mich", "nicht", "ken\u00b7net", "auf", "der", "see", "/", "in", "mei\u00b7nem", "reich", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "$(", "PRELS", "PPER", "PTKNEG", "VVFIN", "APPR", "ART", "NN", "$(", "APPR", "PPOSAT", "ADJD", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Ob er mich nicht h\u00f6rt und siehet/ scheut er dennoch meine streich.", "tokens": ["Ob", "er", "mich", "nicht", "h\u00f6rt", "und", "sie\u00b7het", "/", "scheut", "er", "den\u00b7noch", "mei\u00b7ne", "streich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "PTKNEG", "VVFIN", "KON", "VVFIN", "$(", "VVFIN", "PPER", "ADV", "PPOSAT", "ADJD", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}}}}