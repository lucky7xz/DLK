{"dta.poem.9906": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "15.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es geschiehet nicht aus armuth/ sondern aus bequemligkeit/", "tokens": ["Es", "ge\u00b7schie\u00b7het", "nicht", "aus", "ar\u00b7muth", "/", "son\u00b7dern", "aus", "be\u00b7quem\u00b7lig\u00b7keit", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "APPR", "NE", "$(", "KON", "APPR", "ADJD", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Da\u00df ich meistens geh entbl\u00f6sset/ ohne deck und ohne kleid.", "tokens": ["Da\u00df", "ich", "meis\u00b7tens", "geh", "ent\u00b7bl\u00f6s\u00b7set", "/", "oh\u00b7ne", "deck", "und", "oh\u00b7ne", "kleid", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVFIN", "VVFIN", "$(", "APPR", "NE", "KON", "APPR", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Eben so machts meine schwester/ sie h\u00e4lt es f\u00fcr keine schand/", "tokens": ["E\u00b7ben", "so", "machts", "mei\u00b7ne", "schwes\u00b7ter", "/", "sie", "h\u00e4lt", "es", "f\u00fcr", "kei\u00b7ne", "schand", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPOSAT", "ADJA", "$(", "PPER", "VVFIN", "PPER", "APPR", "PIAT", "VVFIN", "$("], "meter": "+--+--+--+--+-+", "measure": "diphilius"}, "line.4": {"text": "Ob sie schon vor allen leuten nackend wird/ wie ich/ erkant.", "tokens": ["Ob", "sie", "schon", "vor", "al\u00b7len", "leu\u00b7ten", "na\u00b7ckend", "wird", "/", "wie", "ich", "/", "er\u00b7kant", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "PIAT", "NN", "VVPP", "VAFIN", "$(", "PWAV", "PPER", "$(", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}}}}