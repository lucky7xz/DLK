{"dta.poem.6021": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von der Sonne, den Planeten,  \n und vom Firmament.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Doch bleibet diese Quell von Flammen, Licht und Schein,", "tokens": ["Doch", "blei\u00b7bet", "die\u00b7se", "Quell", "von", "Flam\u00b7men", ",", "Licht", "und", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PDAT", "NN", "APPR", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nicht stets gleich unbefleckt und rein:", "tokens": ["Nicht", "stets", "gleich", "un\u00b7be\u00b7fleckt", "und", "rein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Man kan auf ihre Stirn offt mehr, als einen Flecken,", "tokens": ["Man", "kan", "auf", "ih\u00b7re", "Stirn", "offt", "mehr", ",", "als", "ei\u00b7nen", "Fle\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "APPR", "PPOSAT", "NN", "ADV", "ADV", "$,", "KOUS", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Gantz eigentlich entdecken,", "tokens": ["Gantz", "ei\u00b7gent\u00b7lich", "ent\u00b7de\u00b7cken", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVINF", "$,"], "meter": "-+---+-", "measure": "dactylic.init"}, "line.5": {"text": "Und die gemess\u2019ne Zeit, so sie verbirgt und weiset,", "tokens": ["Und", "die", "ge\u00b7mess'\u00b7ne", "Zeit", ",", "so", "sie", "ver\u00b7birgt", "und", "wei\u00b7set", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$,", "ADV", "PPER", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Zeigt, da\u00df sie ihren Creys in dreyssig Tagen reiset.", "tokens": ["Zeigt", ",", "da\u00df", "sie", "ih\u00b7ren", "Creys", "in", "dreys\u00b7sig", "Ta\u00b7gen", "rei\u00b7set", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "PPER", "PPOSAT", "NN", "APPR", "CARD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}