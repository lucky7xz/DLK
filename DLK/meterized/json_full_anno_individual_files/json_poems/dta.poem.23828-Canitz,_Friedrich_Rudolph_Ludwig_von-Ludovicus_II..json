{"dta.poem.23828": {"metadata": {"author": {"name": "Canitz, Friedrich Rudolph Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "Ludovicus II.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1700", "urn": "urn:nbn:de:kobv:b4-200905197532", "language": ["de:0.99"], "booktitle": "[Canitz, Friedrich Rudolph Ludwig von]: Neben-Stunden Unterschiedener Gedichte. [Hrsg. v. Joachim Lange]. Berlin, 1700."}, "poem": {"stanza.1": {"line.1": {"text": "Italien/ das war mein Eigenthum/", "tokens": ["I\u00b7ta\u00b7li\u00b7en", "/", "das", "war", "mein", "Ei\u00b7gen\u00b7thum", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "PDS", "VAFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Dabey ich auch den K\u00e4ysers Titel f\u00fchrte/", "tokens": ["Da\u00b7bey", "ich", "auch", "den", "K\u00e4y\u00b7sers", "Ti\u00b7tel", "f\u00fchr\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "ADV", "ART", "NN", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Durch Tapfferkeit/ die mancher Feind versp\u00fcrte/", "tokens": ["Durch", "Tapf\u00b7fer\u00b7keit", "/", "die", "man\u00b7cher", "Feind", "ver\u00b7sp\u00fcr\u00b7te", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$(", "ART", "PIAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und durch Verstand erwarb ich grossen Ruhm.", "tokens": ["Und", "durch", "Ver\u00b7stand", "er\u00b7warb", "ich", "gros\u00b7sen", "Ruhm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}