{"dta.poem.7299": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Mein Beruf.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-20090519994", "language": ["de:0.99"], "booktitle": "Droste-H\u00fclshoff, Annette von: Gedichte. Stuttgart u. a., 1844."}, "poem": {"stanza.1": {"line.1": {"text": "\u201ewas meinem Kreise mich enttrieb,", "tokens": ["\u201e", "was", "mei\u00b7nem", "Krei\u00b7se", "mich", "ent\u00b7trieb", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "PPOSAT", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Kammer friedlichem Gelasse?\u201c", "tokens": ["Der", "Kam\u00b7mer", "fried\u00b7li\u00b7chem", "Ge\u00b7las\u00b7se", "?", "\u201c"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Das fragt ihr mich als sey, ein Dieb,", "tokens": ["Das", "fragt", "ihr", "mich", "als", "sey", ",", "ein", "Dieb", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PRF", "KOKOM", "VAFIN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich eingebrochen am Parnasse.", "tokens": ["Ich", "ein\u00b7ge\u00b7bro\u00b7chen", "am", "Par\u00b7nas\u00b7se", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVPP", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "So h\u00f6rt denn, h\u00f6rt, weil ihr gefragt:", "tokens": ["So", "h\u00f6rt", "denn", ",", "h\u00f6rt", ",", "weil", "ihr", "ge\u00b7fragt", ":"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "$,", "VVFIN", "$,", "KOUS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Bei der Geburt bin ich geladen,", "tokens": ["Bei", "der", "Ge\u00b7burt", "bin", "ich", "ge\u00b7la\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Mein Recht soweit der Himmel tagt,", "tokens": ["Mein", "Recht", "so\u00b7weit", "der", "Him\u00b7mel", "tagt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und meine Macht von Gottes Gnaden.", "tokens": ["Und", "mei\u00b7ne", "Macht", "von", "Got\u00b7tes", "Gna\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "APPR", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Jetzt wo hervor der todte Schein", "tokens": ["Jetzt", "wo", "her\u00b7vor", "der", "tod\u00b7te", "Schein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PWAV", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sich dr\u00e4ngt am modervollen Stumpfe,", "tokens": ["Sich", "dr\u00e4ngt", "am", "mo\u00b7der\u00b7vol\u00b7len", "Stump\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wo sich der sch\u00f6nste Blumenrain", "tokens": ["Wo", "sich", "der", "sch\u00f6ns\u00b7te", "Blu\u00b7men\u00b7rain"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PRF", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wiegt \u00fcber dem erstorbnen Sumpfe,", "tokens": ["Wiegt", "\u00fc\u00b7ber", "dem", "ers\u00b7torb\u00b7nen", "Sump\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Der Geist, ein blutlos Meteor,", "tokens": ["Der", "Geist", ",", "ein", "blut\u00b7los", "Me\u00b7te\u00b7or", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Entflammt und lischt im Moorgeschwehle,", "tokens": ["Ent\u00b7flammt", "und", "lischt", "im", "Mo\u00b7or\u00b7ge\u00b7schweh\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Jetzt ruft die Stunde: \u201etritt hervor,", "tokens": ["Jetzt", "ruft", "die", "Stun\u00b7de", ":", "\u201e", "tritt", "her\u00b7vor", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$.", "$(", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Mann oder Weib, lebend'ge Seele!", "tokens": ["Mann", "o\u00b7der", "Weib", ",", "le\u00b7ben\u00b7d'\u00b7ge", "See\u00b7le", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "ADJA", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "\u201etritt zu dem Tr\u00e4umer, den am Rand", "tokens": ["\u201e", "tritt", "zu", "dem", "Tr\u00e4u\u00b7mer", ",", "den", "am", "Rand"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "VVFIN", "APPR", "ART", "NN", "$,", "PRELS", "APPRART", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Entschl\u00e4fert der Datura Odem,", "tokens": ["Ent\u00b7schl\u00e4\u00b7fert", "der", "Da\u00b7tu\u00b7ra", "O\u00b7dem", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NE", "$,"], "meter": "-+---+-+-", "measure": "dactylic.init"}, "line.3": {"text": "Der, langsam gleitend von der Wand,", "tokens": ["Der", ",", "lang\u00b7sam", "glei\u00b7tend", "von", "der", "Wand", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "ADJD", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Noch zucket gen den Zauberbrodem.", "tokens": ["Noch", "zu\u00b7cket", "gen", "den", "Zau\u00b7ber\u00b7bro\u00b7dem", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+---+-+-", "measure": "dactylic.init"}, "line.5": {"text": "Und wo ein Mund zu l\u00e4cheln wei\u00df", "tokens": ["Und", "wo", "ein", "Mund", "zu", "l\u00e4\u00b7cheln", "wei\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "NN", "PTKZU", "VVINF", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Im Traum, ein Auge noch zu weinen,", "tokens": ["Im", "Traum", ",", "ein", "Au\u00b7ge", "noch", "zu", "wei\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "ART", "NN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Da schmettre laut, da fl\u00fcstre leis,", "tokens": ["Da", "schmett\u00b7re", "laut", ",", "da", "fl\u00fcst\u00b7re", "leis", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "$,", "ADV", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Trompetensto\u00df und West in Hainen!", "tokens": ["Trom\u00b7pe\u00b7ten\u00b7sto\u00df", "und", "West", "in", "Hai\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "\u201etritt n\u00e4her, wo die Sinnenlust", "tokens": ["\u201e", "tritt", "n\u00e4\u00b7her", ",", "wo", "die", "Sin\u00b7nen\u00b7lust"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "VVFIN", "ADJD", "$,", "PWAV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Als Liebe giebt ihr w\u00fcstes Ringen,", "tokens": ["Als", "Lie\u00b7be", "giebt", "ihr", "w\u00fcs\u00b7tes", "Rin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und durch der eignen Mutter Brust", "tokens": ["Und", "durch", "der", "eig\u00b7nen", "Mut\u00b7ter", "Brust"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den Pfeil zum Ziele m\u00f6chte bringen,", "tokens": ["Den", "Pfeil", "zum", "Zie\u00b7le", "m\u00f6ch\u00b7te", "brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wo selbst die Schande flattert auf,", "tokens": ["Wo", "selbst", "die", "Schan\u00b7de", "flat\u00b7tert", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ein lustiges Panier zum Siege,", "tokens": ["Ein", "lus\u00b7ti\u00b7ges", "Pa\u00b7nier", "zum", "Sie\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Da r\u00fcttle hart: \u201ewach auf, wach auf,", "tokens": ["Da", "r\u00fctt\u00b7le", "hart", ":", "\u201e", "wach", "auf", ",", "wach", "auf", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "$.", "$(", "ADJD", "PTKVZ", "$,", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Unsel'ger, denk an deine Wiege!\u201c", "tokens": ["Un\u00b7sel'\u00b7ger", ",", "denk", "an", "dei\u00b7ne", "Wie\u00b7ge", "!", "\u201c"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADJD", "$,", "PRELS", "APPR", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "\u201edenk an das Aug', das \u00fcberwacht", "tokens": ["\u201e", "denk", "an", "das", "Aug'", ",", "das", "\u00fc\u00b7ber\u00b7wacht"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["$(", "VVFIN", "APPR", "ART", "NN", "$,", "PRELS", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Noch eine Freude dir bereitet,", "tokens": ["Noch", "ei\u00b7ne", "Freu\u00b7de", "dir", "be\u00b7rei\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Denk an die Hand, die manche Nacht", "tokens": ["Denk", "an", "die", "Hand", ",", "die", "man\u00b7che", "Nacht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "$,", "PRELS", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dein Schmerzenslager dir gebreitet,", "tokens": ["Dein", "Schmer\u00b7zens\u00b7la\u00b7ger", "dir", "ge\u00b7brei\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Des Herzens denk, das einzig wund", "tokens": ["Des", "Her\u00b7zens", "denk", ",", "das", "ein\u00b7zig", "wund"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "PRELS", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und einzig selig deinetwegen,", "tokens": ["Und", "ein\u00b7zig", "se\u00b7lig", "dei\u00b7net\u00b7we\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und dann knie nieder auf den Grund", "tokens": ["Und", "dann", "knie", "nie\u00b7der", "auf", "den", "Grund"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PTKVZ", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und fleh' um deiner Mutter Segen!\u201c", "tokens": ["Und", "fleh'", "um", "dei\u00b7ner", "Mut\u00b7ter", "Se\u00b7gen", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "NN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "\u201eund wo sich tr\u00e4umen wie in Haft", "tokens": ["\u201e", "und", "wo", "sich", "tr\u00e4u\u00b7men", "wie", "in", "Haft"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "KON", "PWAV", "PRF", "VVINF", "KOKOM", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zwei einst so gl\u00fch ersehnte Wesen,", "tokens": ["Zwei", "einst", "so", "gl\u00fch", "er\u00b7sehn\u00b7te", "We\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "ADV", "ADV", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Als hab' ein Priesterwort die Kraft", "tokens": ["Als", "hab'", "ein", "Pries\u00b7ter\u00b7wort", "die", "Kraft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "VAFIN", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Banne seligsten zu l\u00f6sen,", "tokens": ["Der", "Ban\u00b7ne", "se\u00b7ligs\u00b7ten", "zu", "l\u00f6\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "PTKZU", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Da fl\u00fcstre leise: \u201ewacht, o wacht!", "tokens": ["Da", "fl\u00fcst\u00b7re", "lei\u00b7se", ":", "\u201e", "wacht", ",", "o", "wacht", "!"], "token_info": ["word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "$.", "$(", "VVFIN", "$,", "FM", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Schaut in das Auge euch, das tr\u00fcbe,", "tokens": ["Schaut", "in", "das", "Au\u00b7ge", "euch", ",", "das", "tr\u00fc\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "PPER", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Wo d\u00e4mmernd sich Erinnrung facht,", "tokens": ["Wo", "d\u00e4m\u00b7mernd", "sich", "E\u00b7rinn\u00b7rung", "facht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVPP", "PRF", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und dann: wach auf, o heil'ge Liebe!\u201c", "tokens": ["Und", "dann", ":", "wach", "auf", ",", "o", "heil'\u00b7ge", "Lie\u00b7be", "!", "\u201c"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADV", "$.", "ADJD", "PTKVZ", "$,", "FM", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "\u201eund wo im Schlafe zitternd noch", "tokens": ["\u201e", "und", "wo", "im", "Schla\u00b7fe", "zit\u00b7ternd", "noch"], "token_info": ["punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "KON", "PWAV", "APPRART", "NN", "VVPP", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vom Opiat die Pulse klopfen,", "tokens": ["Vom", "O\u00b7piat", "die", "Pul\u00b7se", "klop\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Das Auge d\u00fcrr, und g\u00e4be doch", "tokens": ["Das", "Au\u00b7ge", "d\u00fcrr", ",", "und", "g\u00e4\u00b7be", "doch"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "$,", "KON", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sein Sonnenlicht um einen Tropfen, \u2014", "tokens": ["Sein", "Son\u00b7nen\u00b7licht", "um", "ei\u00b7nen", "Trop\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN", "$,", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "O, r\u00fcttle sanft! \u201eVerarmter, senk'", "tokens": ["O", ",", "r\u00fctt\u00b7le", "sanft", "!", "\u201e", "Ver\u00b7arm\u00b7ter", ",", "senk'"], "token_info": ["word", "punct", "word", "word", "punct", "punct", "word", "punct", "word"], "pos": ["NE", "$,", "VVFIN", "ADJD", "$.", "$(", "NN", "$,", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Blicke in des Aethers Sch\u00f6ne,", "tokens": ["Die", "Bli\u00b7cke", "in", "des", "A\u00b7e\u00b7thers", "Sch\u00f6\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Kos' einem blonden Kind und denk'", "tokens": ["Kos'", "ei\u00b7nem", "blon\u00b7den", "Kind", "und", "denk'"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "ART", "ADJA", "NN", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "An der Begeistrung erste Thr\u00e4ne.\u201c", "tokens": ["An", "der", "Be\u00b7geis\u00b7trung", "ers\u00b7te", "Thr\u00e4\u00b7ne", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "So rief die Zeit, so ward mein Amt", "tokens": ["So", "rief", "die", "Zeit", ",", "so", "ward", "mein", "Amt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "ADV", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von Gottes Gnaden mir gegeben,", "tokens": ["Von", "Got\u00b7tes", "Gna\u00b7den", "mir", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "So mein Beruf mir angestammt,", "tokens": ["So", "mein", "Be\u00b7ruf", "mir", "an\u00b7ge\u00b7stammt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Im frischen Muth, im warmen Leben;", "tokens": ["Im", "fri\u00b7schen", "Muth", ",", "im", "war\u00b7men", "Le\u00b7ben", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ich frage nicht ob ihr mich nennt,", "tokens": ["Ich", "fra\u00b7ge", "nicht", "ob", "ihr", "mich", "nennt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "KOUS", "PPER", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Nicht fr\u00f6hnen mag ich kurzem Ruhme,", "tokens": ["Nicht", "fr\u00f6h\u00b7nen", "mag", "ich", "kur\u00b7zem", "Ruh\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVINF", "VMFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Doch wi\u00dft: wo die Sahara brennt,", "tokens": ["Doch", "wi\u00dft", ":", "wo", "die", "Sa\u00b7ha\u00b7ra", "brennt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "PWAV", "ART", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Im W\u00fcstensand, steht eine Blume,", "tokens": ["Im", "W\u00fcs\u00b7ten\u00b7sand", ",", "steht", "ei\u00b7ne", "Blu\u00b7me", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Farblos und Duftes baar, nichts wei\u00df", "tokens": ["Farb\u00b7los", "und", "Duf\u00b7tes", "baar", ",", "nichts", "wei\u00df"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "KON", "NN", "ADJD", "$,", "PIS", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie als den frommen Thau zu h\u00fcten,", "tokens": ["Sie", "als", "den", "from\u00b7men", "Thau", "zu", "h\u00fc\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "KOUS", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und dem Verschmachtenden ihn leis", "tokens": ["Und", "dem", "Ver\u00b7schmach\u00b7ten\u00b7den", "ihn", "leis"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "PPER", "ADJD"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "In ihrem Kelche anzubieten.", "tokens": ["In", "ih\u00b7rem", "Kel\u00b7che", "an\u00b7zu\u00b7bie\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Vor\u00fcber schl\u00fcpft die Schlange scheu", "tokens": ["Vor\u00b7\u00fc\u00b7ber", "schl\u00fcpft", "die", "Schlan\u00b7ge", "scheu"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und Pfeile ihre Blicke regnen,", "tokens": ["Und", "Pfei\u00b7le", "ih\u00b7re", "Bli\u00b7cke", "reg\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Vor\u00fcber rauscht der stolze Leu,", "tokens": ["Vor\u00b7\u00fc\u00b7ber", "rauscht", "der", "stol\u00b7ze", "Leu", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Allein der Pilger wird sie segnen.", "tokens": ["Al\u00b7lein", "der", "Pil\u00b7ger", "wird", "sie", "seg\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VAFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}