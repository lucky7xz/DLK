{"textgrid.poem.36626": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Als Kayser Joseph II. gestorben war", "genre": "verse", "period": "N.A.", "pub_year": 1761, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Er schlug, ein Herkules! ein Menschheitspatriot!", "tokens": ["Er", "schlug", ",", "ein", "Her\u00b7ku\u00b7les", "!", "ein", "Menschheits\u00b7pa\u00b7tri\u00b7ot", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ART", "NN", "$.", "ART", "NN", "$."], "meter": "-+-+---+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Das ungeheure Thier, den Aberglauben todt!", "tokens": ["Das", "un\u00b7ge\u00b7heu\u00b7re", "Thier", ",", "den", "A\u00b7berg\u00b7lau\u00b7ben", "todt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und alle H\u00f6llengeister standen,", "tokens": ["Und", "al\u00b7le", "H\u00f6l\u00b7len\u00b7geis\u00b7ter", "stan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Um's Aas des Thiers, und alle fanden,", "tokens": ["Um's", "Aas", "des", "Thiers", ",", "und", "al\u00b7le", "fan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ART", "NN", "$,", "KON", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "In ihm noch Leben, sahn's, in seinem Drachenschwanz,", "tokens": ["In", "ihm", "noch", "Le\u00b7ben", ",", "sahn's", ",", "in", "sei\u00b7nem", "Dra\u00b7chen\u00b7schwanz", ","], "token_in