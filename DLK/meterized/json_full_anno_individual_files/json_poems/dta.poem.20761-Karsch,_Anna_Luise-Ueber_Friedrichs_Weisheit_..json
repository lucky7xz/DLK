{"dta.poem.20761": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "Ueber  \n Friedrichs Weisheit .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1792", "urn": "urn:nbn:de:kobv:b4-200905193016", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Vorsicht giebt mit Flei\u00df an Trauben oftmals               ", "tokens": ["Die", "Vor\u00b7sicht", "giebt", "mit", "Flei\u00df", "an", "Trau\u00b7ben", "oft\u00b7mals"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "APPR", "NN", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "wenig,", "tokens": ["we\u00b7nig", ","], "token_info": ["word", "punct"], "pos": ["ADV", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Um desto besser schmeckt der Wein:", "tokens": ["Um", "des\u00b7to", "bes\u00b7ser", "schmeckt", "der", "Wein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADV", "ADJD", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So sparsam handelt stets der gro\u00dfe weise K\u00f6nig,", "tokens": ["So", "spar\u00b7sam", "han\u00b7delt", "stets", "der", "gro\u00b7\u00dfe", "wei\u00b7se", "K\u00f6\u00b7nig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "ADV", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Denn er will ihr Nachahmer seyn;", "tokens": ["Denn", "er", "will", "ihr", "Nac\u00b7hah\u00b7mer", "seyn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PPOSAT", "NN", "VAINF", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.6": {"text": "Indem sein Officier und Diener klagen mu\u00df,", "tokens": ["In\u00b7dem", "sein", "Of\u00b7fi\u00b7cier", "und", "Die\u00b7ner", "kla\u00b7gen", "mu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "KON", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Bewahrt er beide vor dem satten Ueberdru\u00df.", "tokens": ["Be\u00b7wahrt", "er", "bei\u00b7de", "vor", "dem", "sat\u00b7ten", "Ue\u00b7berd\u00b7ru\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIS", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}