{"dta.poem.14094": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Der Tanzmeister.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "So vom Augenblick \u2014 geschlagen,", "tokens": ["So", "vom", "Au\u00b7gen\u00b7blick", "ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$(", "VVPP", "$,"], "meter": "--+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Kniet' ich Armer vor ihr hin,", "tokens": ["Kniet'", "ich", "Ar\u00b7mer", "vor", "ihr", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hatt' kein Herz nun, ihr zu sagen,", "tokens": ["Hatt'", "kein", "Herz", "nun", ",", "ihr", "zu", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "ADV", "$,", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df ich ihr Entherzter bin.", "tokens": ["Da\u00df", "ich", "ihr", "En\u00b7therz\u00b7ter", "bin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}