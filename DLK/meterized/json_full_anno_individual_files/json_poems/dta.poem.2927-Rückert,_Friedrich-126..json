{"dta.poem.2927": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "126.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Erkennest du, wohin auf oder niederstrebt", "tokens": ["Er\u00b7ken\u00b7nest", "du", ",", "wo\u00b7hin", "auf", "o\u00b7der", "nie\u00b7der\u00b7strebt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "PWAV", "APPR", "KON", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Zeitgang, gib nur nach, o Herz, das widerstrebt!", "tokens": ["Der", "Zeit\u00b7gang", ",", "gib", "nur", "nach", ",", "o", "Herz", ",", "das", "wi\u00b7der\u00b7strebt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVIMP", "ADV", "PTKVZ", "$,", "FM", "NN", "$,", "PDS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Kein Widerstreben hilft; du mu\u00dft dich ihm bequemen,", "tokens": ["Kein", "Wi\u00b7der\u00b7stre\u00b7ben", "hilft", ";", "du", "mu\u00dft", "dich", "ihm", "be\u00b7que\u00b7men", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "$.", "PPER", "VMFIN", "PPER", "PPER", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wo nicht, mit deinem Thun vom Schauplatz Abschied nehmen.", "tokens": ["Wo", "nicht", ",", "mit", "dei\u00b7nem", "Thun", "vom", "Schau\u00b7platz", "Ab\u00b7schied", "neh\u00b7men", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PTKNEG", "$,", "APPR", "PPOSAT", "NN", "APPRART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "In jeder Jahreszeit kommt andres an die Reihe;", "tokens": ["In", "je\u00b7der", "Jah\u00b7res\u00b7zeit", "kommt", "and\u00b7res", "an", "die", "Rei\u00b7he", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "PIS", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Begehre nicht, da\u00df man nur Wetter dir verleihe!", "tokens": ["Be\u00b7geh\u00b7re", "nicht", ",", "da\u00df", "man", "nur", "Wet\u00b7ter", "dir", "ver\u00b7lei\u00b7he", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "$,", "KOUS", "PIS", "ADV", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Wenn du im Wetter, das nun kommt, nicht bl\u00fchen kannst,", "tokens": ["Wenn", "du", "im", "Wet\u00b7ter", ",", "das", "nun", "kommt", ",", "nicht", "bl\u00fc\u00b7hen", "kannst", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "$,", "PRELS", "ADV", "VVFIN", "$,", "PTKNEG", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So freue dich da\u00df du schon deine Frucht gewannst.", "tokens": ["So", "freu\u00b7e", "dich", "da\u00df", "du", "schon", "dei\u00b7ne", "Frucht", "ge\u00b7wannst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "KOUS", "PPER", "ADV", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Das worin du erstickst, ist andern Lebensluft;", "tokens": ["Das", "wo\u00b7rin", "du", "er\u00b7stickst", ",", "ist", "an\u00b7dern", "Le\u00b7bens\u00b7luft", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "PWAV", "PPER", "VVFIN", "$,", "VAFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Zukunft Odem weht aus des Vergangnen Gruft.", "tokens": ["Der", "Zu\u00b7kunft", "O\u00b7dem", "weht", "aus", "des", "Ver\u00b7gang\u00b7nen", "Gruft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Was also bleibet dir? theilnehmende Betrachtung,", "tokens": ["Was", "al\u00b7so", "blei\u00b7bet", "dir", "?", "theil\u00b7neh\u00b7men\u00b7de", "Be\u00b7trach\u00b7tung", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "PPER", "$.", "ADJA", "NN", "$,"], "meter": "-+-+-+-+---+-", "measure": "unknown.measure.penta"}, "line.2": {"text": "Dem Werden zuzusehn ohn' Aerger und Verachtung.", "tokens": ["Dem", "Wer\u00b7den", "zu\u00b7zu\u00b7sehn", "ohn'", "A\u00b7er\u00b7ger", "und", "Ver\u00b7ach\u00b7tung", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVIZU", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.7": {"line.1": {"text": "Gl\u00fcckselig ein Gem\u00fcth, in dessen Heiligthumen", "tokens": ["Gl\u00fcck\u00b7se\u00b7lig", "ein", "Ge\u00b7m\u00fcth", ",", "in", "des\u00b7sen", "Hei\u00b7lig\u00b7thu\u00b7men"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "ART", "NN", "$,", "APPR", "PRELAT", "NN"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Jedwede Jahreszeit hervorbringt Himmelsblumen.", "tokens": ["Jed\u00b7we\u00b7de", "Jah\u00b7res\u00b7zeit", "her\u00b7vor\u00b7bringt", "Him\u00b7mels\u00b7blu\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}