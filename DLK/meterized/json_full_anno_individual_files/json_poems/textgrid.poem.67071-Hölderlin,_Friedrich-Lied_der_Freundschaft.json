"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Schaffen Ruhe nach dem Harm.", "tokens": ["Schaf\u00b7fen", "Ru\u00b7he", "nach", "dem", "Harm", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.24": {"line.1": {"text": "Rauscht ihm dann des Todes Fl\u00fcgel,", "tokens": ["Rauscht", "ihm", "dann", "des", "To\u00b7des", "Fl\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schl\u00e4ft er ruhig unterm H\u00fcgel,", "tokens": ["Schl\u00e4ft", "er", "ru\u00b7hig", "un\u00b7term", "H\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo sein Bund den Kranz ihm flicht,", "tokens": ["Wo", "sein", "Bund", "den", "Kranz", "ihm", "flicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "In die Locken seiner Br\u00fcder", "tokens": ["In", "die", "Lo\u00b7cken", "sei\u00b7ner", "Br\u00fc\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "S\u00e4uselt noch sein Geist hernieder,", "tokens": ["S\u00e4u\u00b7selt", "noch", "sein", "Geist", "her\u00b7nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Lispelt leis: Verge\u00dft mic