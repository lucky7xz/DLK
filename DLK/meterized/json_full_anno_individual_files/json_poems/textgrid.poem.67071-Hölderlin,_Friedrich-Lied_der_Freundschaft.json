{"textgrid.poem.67071": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Lied der Freundschaft", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Frei, wie G\u00f6tter an dem Mahle,", "tokens": ["Frei", ",", "wie", "G\u00f6t\u00b7ter", "an", "dem", "Mah\u00b7le", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Singen wir um die Pokale,", "tokens": ["Sin\u00b7gen", "wir", "um", "die", "Po\u00b7ka\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo der edle Trank ergl\u00fcht,", "tokens": ["Wo", "der", "ed\u00b7le", "Trank", "er\u00b7gl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Voll von Schauern, ernst und stille,", "tokens": ["Voll", "von", "Schau\u00b7ern", ",", "ernst", "und", "stil\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "$,", "ADJD", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "In des Dunkels heilger H\u00fclle", "tokens": ["In", "des", "Dun\u00b7kels", "heil\u00b7ger", "H\u00fcl\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Singen wir der Freundschaft Lied.", "tokens": ["Sin\u00b7gen", "wir", "der", "Freund\u00b7schaft", "Lied", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Schwebt herab aus k\u00fchlen L\u00fcften,", "tokens": ["Schwebt", "her\u00b7ab", "aus", "k\u00fch\u00b7len", "L\u00fcf\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schwebet aus den Schlummergr\u00fcften,", "tokens": ["Schwe\u00b7bet", "aus", "den", "Schlum\u00b7mer\u00b7gr\u00fcf\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Helden der Vergangenheit!", "tokens": ["Hel\u00b7den", "der", "Ver\u00b7gan\u00b7gen\u00b7heit", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Kommt in unsern Kreis hernieder,", "tokens": ["Kommt", "in", "un\u00b7sern", "Kreis", "her\u00b7nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Staunt und sprecht: Da ist sie wieder,", "tokens": ["Staunt", "und", "sprecht", ":", "Da", "ist", "sie", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "VVFIN", "$.", "ADV", "VAFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Unsre deutsche Herzlichkeit.", "tokens": ["Uns\u00b7re", "deut\u00b7sche", "Herz\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Singe von ihr Jubellieder,", "tokens": ["Sin\u00b7ge", "von", "ihr", "Ju\u00b7bel\u00b7lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Von der Wonne deutscher Br\u00fcder,", "tokens": ["Von", "der", "Won\u00b7ne", "deut\u00b7scher", "Br\u00fc\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Chronos! in dem ewgen Lauf;", "tokens": ["Chro\u00b7nos", "!", "in", "dem", "ew\u00b7gen", "Lauf", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Singe, Sohn der Afterzeiten!", "tokens": ["Sin\u00b7ge", ",", "Sohn", "der", "Af\u00b7ter\u00b7zei\u00b7ten", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Sing: Elysens Herrlichkeiten", "tokens": ["Sing", ":", "E\u00b7ly\u00b7sens", "Herr\u00b7lich\u00b7kei\u00b7ten"], "token_info": ["word", "punct", "word", "word"], "pos": ["NE", "$.", "NE", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wog ein deutscher Handschlag auf.", "tokens": ["Wog", "ein", "deut\u00b7scher", "Hand\u00b7schlag", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Ha! der hohen G\u00f6tterstunden!", "tokens": ["Ha", "!", "der", "ho\u00b7hen", "G\u00f6t\u00b7ter\u00b7stun\u00b7den", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wann der Edle sich gefunden,", "tokens": ["Wann", "der", "Ed\u00b7le", "sich", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "PRF", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Der f\u00fcr unser Herz geh\u00f6rt;", "tokens": ["Der", "f\u00fcr", "un\u00b7ser", "Herz", "ge\u00b7h\u00f6rt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "So begeisternd zu den H\u00f6hen,", "tokens": ["So", "be\u00b7geis\u00b7ternd", "zu", "den", "H\u00f6\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Die um uns, wie Riesen, stehen!", "tokens": ["Die", "um", "uns", ",", "wie", "Rie\u00b7sen", ",", "ste\u00b7hen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "APPR", "PPER", "$,", "PWAV", "NN", "$,", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "So des deutschen J\u00fcnglings wert!", "tokens": ["So", "des", "deut\u00b7schen", "J\u00fcng\u00b7lings", "wert", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Froher schl\u00e4gt das Herz, und freier!", "tokens": ["Fro\u00b7her", "schl\u00e4gt", "das", "Herz", ",", "und", "frei\u00b7er", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,", "KON", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Reichet zu des Bundes Feier", "tokens": ["Rei\u00b7chet", "zu", "des", "Bun\u00b7des", "Fei\u00b7er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Uns der Freund den Becher dar;", "tokens": ["Uns", "der", "Freund", "den", "Be\u00b7cher", "dar", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ohne Freuden, ohne Leben", "tokens": ["Oh\u00b7ne", "Freu\u00b7den", ",", "oh\u00b7ne", "Le\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "$,", "KOUI", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Erntet' er Ly\u00e4us Reben,", "tokens": ["Ern\u00b7tet'", "er", "Ly\u00b7\u00e4us", "Re\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NE", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Als er ohne Freunde war.", "tokens": ["Als", "er", "oh\u00b7ne", "Freun\u00b7de", "war", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "St\u00e4rke, wenn Verleumder schreien,", "tokens": ["St\u00e4r\u00b7ke", ",", "wenn", "Ver\u00b7leum\u00b7der", "schrei\u00b7en", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wahrheit, wenn Despoten dr\u00e4uen,", "tokens": ["Wahr\u00b7heit", ",", "wenn", "Des\u00b7po\u00b7ten", "dr\u00e4u\u00b7en", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "M\u00e4nnermut im Mi\u00dfgeschick,", "tokens": ["M\u00e4n\u00b7ner\u00b7mut", "im", "Mi\u00df\u00b7ge\u00b7schick", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Duldung, wenn die Schwachen sinken,", "tokens": ["Dul\u00b7dung", ",", "wenn", "die", "Schwa\u00b7chen", "sin\u00b7ken", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Liebe, Duldung, W\u00e4rme trinken", "tokens": ["Lie\u00b7be", ",", "Dul\u00b7dung", ",", "W\u00e4r\u00b7me", "trin\u00b7ken"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Freunde von des Freundes Blick.", "tokens": ["Freun\u00b7de", "von", "des", "Freun\u00b7des", "Blick", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Sanfter atmen Fr\u00fchlingsl\u00fcfte,", "tokens": ["Sanf\u00b7ter", "at\u00b7men", "Fr\u00fch\u00b7lings\u00b7l\u00fcf\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "S\u00fc\u00dfer sind der Linde D\u00fcfte,", "tokens": ["S\u00fc\u00b7\u00dfer", "sind", "der", "Lin\u00b7de", "D\u00fcf\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NE", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "K\u00fchliger der Eichenhain,", "tokens": ["K\u00fch\u00b7li\u00b7ger", "der", "Ei\u00b7chen\u00b7hain", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn bekr\u00e4nzt mit jungen Rosen", "tokens": ["Wenn", "be\u00b7kr\u00e4nzt", "mit", "jun\u00b7gen", "Ro\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "VVPP", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Freunde bei den Bechern kosen,", "tokens": ["Freun\u00b7de", "bei", "den", "Be\u00b7chern", "ko\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Freunde sich des Abends freun.", "tokens": ["Freun\u00b7de", "sich", "des", "A\u00b7bends", "freun", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Br\u00fcder! la\u00dft die Toren sinnen,", "tokens": ["Br\u00fc\u00b7der", "!", "la\u00dft", "die", "To\u00b7ren", "sin\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie sie F\u00fcrstengunst gewinnen,", "tokens": ["Wie", "sie", "F\u00fcrs\u00b7ten\u00b7gunst", "ge\u00b7win\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "H\u00e4ufen m\u00f6gen Gut und Gold;", "tokens": ["H\u00e4u\u00b7fen", "m\u00f6\u00b7gen", "Gut", "und", "Gold", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "ADJD", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "L\u00e4chelnd kanns der Edle missen,", "tokens": ["L\u00e4\u00b7chelnd", "kanns", "der", "Ed\u00b7le", "mis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VMFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Sich geliebt, geliebt zu wissen,", "tokens": ["Sich", "ge\u00b7liebt", ",", "ge\u00b7liebt", "zu", "wis\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PRF", "VVPP", "$,", "VVPP", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Dies ist seiner Taten Sold.", "tokens": ["Dies", "ist", "sei\u00b7ner", "Ta\u00b7ten", "Sold", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "NN", "NE", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}, "stanza.9": {"line.1": {"text": "Schmettert aus der trauten Halle", "tokens": ["Schmet\u00b7tert", "aus", "der", "trau\u00b7ten", "Hal\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Auch die Auserw\u00e4hlten alle", "tokens": ["Auch", "die", "Au\u00b7ser\u00b7w\u00e4hl\u00b7ten", "al\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "PIAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "In die Ferne das Geschick,", "tokens": ["In", "die", "Fer\u00b7ne", "das", "Ge\u00b7schick", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wandelt er mit Schmerz beladen", "tokens": ["Wan\u00b7delt", "er", "mit", "Schmerz", "be\u00b7la\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Nun auf freundelosen Pfaden,", "tokens": ["Nun", "auf", "freun\u00b7de\u00b7lo\u00b7sen", "Pfa\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Schwarzen Gram im bangen Blick,", "tokens": ["Schwar\u00b7zen", "Gram", "im", "ban\u00b7gen", "Blick", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Wankt er, wenn sich Wolken t\u00fcrmen,", "tokens": ["Wankt", "er", ",", "wenn", "sich", "Wol\u00b7ken", "t\u00fcr\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "PRF", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wankt er nun in Winterst\u00fcrmen", "tokens": ["Wankt", "er", "nun", "in", "Win\u00b7ter\u00b7st\u00fcr\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ohne Leiter, ohne Stab,", "tokens": ["Oh\u00b7ne", "Lei\u00b7ter", ",", "oh\u00b7ne", "Stab", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "KOUI", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Lauscht er abgebleicht und d\u00fcster", "tokens": ["Lauscht", "er", "ab\u00b7ge\u00b7bleicht", "und", "d\u00fcs\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "VVPP", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Bangem Mitternachtsgefl\u00fcster", "tokens": ["Ban\u00b7gem", "Mit\u00b7ter\u00b7nachts\u00b7ge\u00b7fl\u00fcs\u00b7ter"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ahndungsvoll am frischen Grab,", "tokens": ["Ahn\u00b7dungs\u00b7voll", "am", "fri\u00b7schen", "Grab", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "O da kehren all die Stunden,", "tokens": ["O", "da", "keh\u00b7ren", "all", "die", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "VVFIN", "PIAT", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "So in Freundesarm verschwunden,", "tokens": ["So", "in", "Freun\u00b7des\u00b7arm", "ver\u00b7schwun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Unter Schw\u00fcren, wahr, und warm,", "tokens": ["Un\u00b7ter", "Schw\u00fc\u00b7ren", ",", "wahr", ",", "und", "warm", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "ADJD", "$,", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "All umfa\u00dft mit sanftem Sehnen", "tokens": ["All", "um\u00b7fa\u00dft", "mit", "sanf\u00b7tem", "Seh\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Seine Seele, s\u00fc\u00dfe Tr\u00e4nen", "tokens": ["Sei\u00b7ne", "See\u00b7le", ",", "s\u00fc\u00b7\u00dfe", "Tr\u00e4\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Schaffen Ruhe nach dem Harm.", "tokens": ["Schaf\u00b7fen", "Ru\u00b7he", "nach", "dem", "Harm", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Rauscht ihm dann des Todes Fl\u00fcgel,", "tokens": ["Rauscht", "ihm", "dann", "des", "To\u00b7des", "Fl\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schl\u00e4ft er ruhig unterm H\u00fcgel,", "tokens": ["Schl\u00e4ft", "er", "ru\u00b7hig", "un\u00b7term", "H\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo sein Bund den Kranz ihm flicht,", "tokens": ["Wo", "sein", "Bund", "den", "Kranz", "ihm", "flicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "In die Locken seiner Br\u00fcder", "tokens": ["In", "die", "Lo\u00b7cken", "sei\u00b7ner", "Br\u00fc\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "S\u00e4uselt noch sein Geist hernieder,", "tokens": ["S\u00e4u\u00b7selt", "noch", "sein", "Geist", "her\u00b7nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Lispelt leis: Verge\u00dft mich nicht!", "tokens": ["Lis\u00b7pelt", "leis", ":", "Ver\u00b7ge\u00dft", "mich", "nicht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$.", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Frei, wie G\u00f6tter an dem Mahle,", "tokens": ["Frei", ",", "wie", "G\u00f6t\u00b7ter", "an", "dem", "Mah\u00b7le", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Singen wir um die Pokale,", "tokens": ["Sin\u00b7gen", "wir", "um", "die", "Po\u00b7ka\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo der edle Trank ergl\u00fcht,", "tokens": ["Wo", "der", "ed\u00b7le", "Trank", "er\u00b7gl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Voll von Schauern, ernst und stille,", "tokens": ["Voll", "von", "Schau\u00b7ern", ",", "ernst", "und", "stil\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "$,", "ADJD", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "In des Dunkels heilger H\u00fclle", "tokens": ["In", "des", "Dun\u00b7kels", "heil\u00b7ger", "H\u00fcl\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Singen wir der Freundschaft Lied.", "tokens": ["Sin\u00b7gen", "wir", "der", "Freund\u00b7schaft", "Lied", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "Schwebt herab aus k\u00fchlen L\u00fcften,", "tokens": ["Schwebt", "her\u00b7ab", "aus", "k\u00fch\u00b7len", "L\u00fcf\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schwebet aus den Schlummergr\u00fcften,", "tokens": ["Schwe\u00b7bet", "aus", "den", "Schlum\u00b7mer\u00b7gr\u00fcf\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Helden der Vergangenheit!", "tokens": ["Hel\u00b7den", "der", "Ver\u00b7gan\u00b7gen\u00b7heit", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Kommt in unsern Kreis hernieder,", "tokens": ["Kommt", "in", "un\u00b7sern", "Kreis", "her\u00b7nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Staunt und sprecht: Da ist sie wieder,", "tokens": ["Staunt", "und", "sprecht", ":", "Da", "ist", "sie", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "VVFIN", "$.", "ADV", "VAFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Unsre deutsche Herzlichkeit.", "tokens": ["Uns\u00b7re", "deut\u00b7sche", "Herz\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.15": {"line.1": {"text": "Singe von ihr Jubellieder,", "tokens": ["Sin\u00b7ge", "von", "ihr", "Ju\u00b7bel\u00b7lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Von der Wonne deutscher Br\u00fcder,", "tokens": ["Von", "der", "Won\u00b7ne", "deut\u00b7scher", "Br\u00fc\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Chronos! in dem ewgen Lauf;", "tokens": ["Chro\u00b7nos", "!", "in", "dem", "ew\u00b7gen", "Lauf", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Singe, Sohn der Afterzeiten!", "tokens": ["Sin\u00b7ge", ",", "Sohn", "der", "Af\u00b7ter\u00b7zei\u00b7ten", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Sing: Elysens Herrlichkeiten", "tokens": ["Sing", ":", "E\u00b7ly\u00b7sens", "Herr\u00b7lich\u00b7kei\u00b7ten"], "token_info": ["word", "punct", "word", "word"], "pos": ["NE", "$.", "NE", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wog ein deutscher Handschlag auf.", "tokens": ["Wog", "ein", "deut\u00b7scher", "Hand\u00b7schlag", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.16": {"line.1": {"text": "Ha! der hohen G\u00f6tterstunden!", "tokens": ["Ha", "!", "der", "ho\u00b7hen", "G\u00f6t\u00b7ter\u00b7stun\u00b7den", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wann der Edle sich gefunden,", "tokens": ["Wann", "der", "Ed\u00b7le", "sich", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "PRF", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Der f\u00fcr unser Herz geh\u00f6rt;", "tokens": ["Der", "f\u00fcr", "un\u00b7ser", "Herz", "ge\u00b7h\u00f6rt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "So begeisternd zu den H\u00f6hen,", "tokens": ["So", "be\u00b7geis\u00b7ternd", "zu", "den", "H\u00f6\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Die um uns, wie Riesen, stehen!", "tokens": ["Die", "um", "uns", ",", "wie", "Rie\u00b7sen", ",", "ste\u00b7hen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "APPR", "PPER", "$,", "PWAV", "NN", "$,", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "So des deutschen J\u00fcnglings wert!", "tokens": ["So", "des", "deut\u00b7schen", "J\u00fcng\u00b7lings", "wert", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.17": {"line.1": {"text": "Froher schl\u00e4gt das Herz, und freier!", "tokens": ["Fro\u00b7her", "schl\u00e4gt", "das", "Herz", ",", "und", "frei\u00b7er", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,", "KON", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Reichet zu des Bundes Feier", "tokens": ["Rei\u00b7chet", "zu", "des", "Bun\u00b7des", "Fei\u00b7er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Uns der Freund den Becher dar;", "tokens": ["Uns", "der", "Freund", "den", "Be\u00b7cher", "dar", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ohne Freuden, ohne Leben", "tokens": ["Oh\u00b7ne", "Freu\u00b7den", ",", "oh\u00b7ne", "Le\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "$,", "KOUI", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Erntet' er Ly\u00e4us Reben,", "tokens": ["Ern\u00b7tet'", "er", "Ly\u00b7\u00e4us", "Re\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NE", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Als er ohne Freunde war.", "tokens": ["Als", "er", "oh\u00b7ne", "Freun\u00b7de", "war", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.18": {"line.1": {"text": "St\u00e4rke, wenn Verleumder schreien,", "tokens": ["St\u00e4r\u00b7ke", ",", "wenn", "Ver\u00b7leum\u00b7der", "schrei\u00b7en", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wahrheit, wenn Despoten dr\u00e4uen,", "tokens": ["Wahr\u00b7heit", ",", "wenn", "Des\u00b7po\u00b7ten", "dr\u00e4u\u00b7en", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "M\u00e4nnermut im Mi\u00dfgeschick,", "tokens": ["M\u00e4n\u00b7ner\u00b7mut", "im", "Mi\u00df\u00b7ge\u00b7schick", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Duldung, wenn die Schwachen sinken,", "tokens": ["Dul\u00b7dung", ",", "wenn", "die", "Schwa\u00b7chen", "sin\u00b7ken", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Liebe, Duldung, W\u00e4rme trinken", "tokens": ["Lie\u00b7be", ",", "Dul\u00b7dung", ",", "W\u00e4r\u00b7me", "trin\u00b7ken"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Freunde von des Freundes Blick.", "tokens": ["Freun\u00b7de", "von", "des", "Freun\u00b7des", "Blick", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.19": {"line.1": {"text": "Sanfter atmen Fr\u00fchlingsl\u00fcfte,", "tokens": ["Sanf\u00b7ter", "at\u00b7men", "Fr\u00fch\u00b7lings\u00b7l\u00fcf\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "S\u00fc\u00dfer sind der Linde D\u00fcfte,", "tokens": ["S\u00fc\u00b7\u00dfer", "sind", "der", "Lin\u00b7de", "D\u00fcf\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NE", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "K\u00fchliger der Eichenhain,", "tokens": ["K\u00fch\u00b7li\u00b7ger", "der", "Ei\u00b7chen\u00b7hain", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn bekr\u00e4nzt mit jungen Rosen", "tokens": ["Wenn", "be\u00b7kr\u00e4nzt", "mit", "jun\u00b7gen", "Ro\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "VVPP", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Freunde bei den Bechern kosen,", "tokens": ["Freun\u00b7de", "bei", "den", "Be\u00b7chern", "ko\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Freunde sich des Abends freun.", "tokens": ["Freun\u00b7de", "sich", "des", "A\u00b7bends", "freun", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.20": {"line.1": {"text": "Br\u00fcder! la\u00dft die Toren sinnen,", "tokens": ["Br\u00fc\u00b7der", "!", "la\u00dft", "die", "To\u00b7ren", "sin\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie sie F\u00fcrstengunst gewinnen,", "tokens": ["Wie", "sie", "F\u00fcrs\u00b7ten\u00b7gunst", "ge\u00b7win\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "H\u00e4ufen m\u00f6gen Gut und Gold;", "tokens": ["H\u00e4u\u00b7fen", "m\u00f6\u00b7gen", "Gut", "und", "Gold", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "ADJD", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "L\u00e4chelnd kanns der Edle missen,", "tokens": ["L\u00e4\u00b7chelnd", "kanns", "der", "Ed\u00b7le", "mis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VMFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Sich geliebt, geliebt zu wissen,", "tokens": ["Sich", "ge\u00b7liebt", ",", "ge\u00b7liebt", "zu", "wis\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PRF", "VVPP", "$,", "VVPP", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Dies ist seiner Taten Sold.", "tokens": ["Dies", "ist", "sei\u00b7ner", "Ta\u00b7ten", "Sold", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "NN", "NE", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}, "stanza.21": {"line.1": {"text": "Schmettert aus der trauten Halle", "tokens": ["Schmet\u00b7tert", "aus", "der", "trau\u00b7ten", "Hal\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Auch die Auserw\u00e4hlten alle", "tokens": ["Auch", "die", "Au\u00b7ser\u00b7w\u00e4hl\u00b7ten", "al\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "PIAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "In die Ferne das Geschick,", "tokens": ["In", "die", "Fer\u00b7ne", "das", "Ge\u00b7schick", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wandelt er mit Schmerz beladen", "tokens": ["Wan\u00b7delt", "er", "mit", "Schmerz", "be\u00b7la\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Nun auf freundelosen Pfaden,", "tokens": ["Nun", "auf", "freun\u00b7de\u00b7lo\u00b7sen", "Pfa\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Schwarzen Gram im bangen Blick,", "tokens": ["Schwar\u00b7zen", "Gram", "im", "ban\u00b7gen", "Blick", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.22": {"line.1": {"text": "Wankt er, wenn sich Wolken t\u00fcrmen,", "tokens": ["Wankt", "er", ",", "wenn", "sich", "Wol\u00b7ken", "t\u00fcr\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "PRF", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wankt er nun in Winterst\u00fcrmen", "tokens": ["Wankt", "er", "nun", "in", "Win\u00b7ter\u00b7st\u00fcr\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ohne Leiter, ohne Stab,", "tokens": ["Oh\u00b7ne", "Lei\u00b7ter", ",", "oh\u00b7ne", "Stab", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "KOUI", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Lauscht er abgebleicht und d\u00fcster", "tokens": ["Lauscht", "er", "ab\u00b7ge\u00b7bleicht", "und", "d\u00fcs\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "VVPP", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Bangem Mittern