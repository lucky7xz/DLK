{"dta.poem.2127": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Viola Mariana.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Zwar die Figur von einem Stern, wovon doch jedes", "tokens": ["Zwar", "die", "Fi\u00b7gur", "von", "ei\u00b7nem", "Stern", ",", "wo\u00b7von", "doch", "je\u00b7des"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "APPR", "ART", "NN", "$,", "PWAV", "ADV", "PIAT"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "hinterwerts,", "tokens": ["hin\u00b7ter\u00b7werts", ","], "token_info": ["word", "punct"], "pos": ["ADV", "$,"], "meter": "+--", "measure": "dactylic.init"}, "line.3": {"text": "Gemeinschaftlich mit seinem Nachbar, fast an Gestalt ein", "tokens": ["Ge\u00b7mein\u00b7schaft\u00b7lich", "mit", "sei\u00b7nem", "Nach\u00b7bar", ",", "fast", "an", "Ge\u00b7stalt", "ein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "PPOSAT", "NN", "$,", "ADV", "APPR", "NN", "ART"], "meter": "-+-+-+-+-++-+-", "measure": "unknown.measure.septa"}, "line.4": {"text": "gr\u00fcnes Herz,", "tokens": ["gr\u00fc\u00b7nes", "Herz", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Das hol, und an dem Rand gekr\u00fcmmt verwunderlich for-", "tokens": ["Das", "hol", ",", "und", "an", "dem", "Rand", "ge\u00b7kr\u00fcmmt", "ver\u00b7wun\u00b7der\u00b7lich", "for"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "$,", "KON", "APPR", "ART", "NN", "VVPP", "ADJD", "TRUNC"], "meter": "-+-+-+-+-+-++", "measure": "unknown.measure.septa"}, "line.6": {"text": "miret hat.", "tokens": ["mi\u00b7ret", "hat", "."], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "VAFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Recht da wo sich die Bl\u00e4tter biegen,", "tokens": ["Recht", "da", "wo", "sich", "die", "Bl\u00e4t\u00b7ter", "bie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PWAV", "PRF", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Wenn man sie in der Queer durchschnitten, sieht man, wie", "tokens": ["Wenn", "man", "sie", "in", "der", "Que\u00b7er", "durch\u00b7schnit\u00b7ten", ",", "sieht", "man", ",", "wie"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["KOUS", "PIS", "PPER", "APPR", "ART", "NN", "VVFIN", "$,", "VVFIN", "PIS", "$,", "PWAV"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "im gevierten Platz,", "tokens": ["im", "ge\u00b7vier\u00b7ten", "Platz", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.10": {"text": "In zierlich abgetheilten F\u00e4chern, als einen da verborgnen", "tokens": ["In", "zier\u00b7lich", "ab\u00b7ge\u00b7theil\u00b7ten", "F\u00e4\u00b7chern", ",", "als", "ei\u00b7nen", "da", "ver\u00b7borg\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ADJD", "ADJA", "NN", "$,", "KOUS", "ART", "ADV", "VVINF"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.11": {"text": "Schatz,", "tokens": ["Schatz", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+", "measure": "single.up"}, "line.12": {"text": "Den ungez\u00e4hlten Samen liegen;", "tokens": ["Den", "un\u00b7ge\u00b7z\u00e4hl\u00b7ten", "Sa\u00b7men", "lie\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Der, wegen seiner Kleinheit, werth, da\u00df man denselbigen", "tokens": ["Der", ",", "we\u00b7gen", "sei\u00b7ner", "Klein\u00b7heit", ",", "werth", ",", "da\u00df", "man", "den\u00b7sel\u00b7bi\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ART", "$,", "APPR", "PPOSAT", "NN", "$,", "ADJD", "$,", "KOUS", "PIS", "VVINF"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.14": {"text": "beachtet", "tokens": ["be\u00b7ach\u00b7tet"], "token_info": ["word"], "pos": ["VVPP"], "meter": "-+-", "measure": "amphibrach.single"}, "line.15": {"text": "Und, da\u00df solch eine gro\u00dfe Staud\u2019 in ihm verborgen liegt,", "tokens": ["Und", ",", "da\u00df", "solch", "ei\u00b7ne", "gro\u00b7\u00dfe", "Staud'", "in", "ihm", "ver\u00b7bor\u00b7gen", "liegt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PIAT", "ART", "ADJA", "NN", "APPR", "PPER", "VVPP", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.16": {"text": "betrachtet,", "tokens": ["be\u00b7trach\u00b7tet", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.17": {"text": "Nebst so viel Bl\u00e4ttern, Stengeln, Blumen, Kein mensch-", "tokens": ["Nebst", "so", "viel", "Bl\u00e4t\u00b7tern", ",", "Sten\u00b7geln", ",", "Blu\u00b7men", ",", "Kein", "men\u00b7sch"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word"], "pos": ["APPR", "ADV", "PIAT", "NN", "$,", "NN", "$,", "NN", "$,", "PIAT", "TRUNC"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.18": {"text": "licher Verstand kann fassen,", "tokens": ["li\u00b7cher", "Ver\u00b7stand", "kann", "fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VMFIN", "VVINF", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.19": {"text": "Wie doch so viel und gro\u00dfe Theile so enge sich verschr\u00e4n-", "tokens": ["Wie", "doch", "so", "viel", "und", "gro\u00b7\u00dfe", "Thei\u00b7le", "so", "en\u00b7ge", "sich", "ver\u00b7schr\u00e4n"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ADV", "ADV", "KON", "ADJA", "NN", "ADV", "ADJA", "PRF", "TRUNC"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.20": {"text": "ken lassen.", "tokens": ["ken", "las\u00b7sen", "."], "token_info": ["word", "word", "punct"], "pos": ["VVINF", "VVINF", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.21": {"text": "Man sollte, da die\u00df unbegreiflich, wohl gar auf die Ge-", "tokens": ["Man", "soll\u00b7te", ",", "da", "die\u00df", "un\u00b7be\u00b7greif\u00b7lich", ",", "wohl", "gar", "auf", "die", "Ge"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PIS", "VMFIN", "$,", "KOUS", "PDS", "ADJD", "$,", "ADV", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+--", "measure": "unknown.measure.hexa"}, "line.22": {"text": "danken kommen,", "tokens": ["dan\u00b7ken", "kom\u00b7men", ","], "token_info": ["word", "word", "punct"], "pos": ["PAV", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.23": {"text": "Es w\u00fcrd\u2019, im Wachsen, aller Stoff von ihnen aus der Luft", "tokens": ["Es", "w\u00fcrd'", ",", "im", "Wach\u00b7sen", ",", "al\u00b7ler", "Stoff", "von", "ih\u00b7nen", "aus", "der", "Luft"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "$,", "APPRART", "NN", "$,", "PIAT", "NN", "APPR", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.24": {"text": "genommen,", "tokens": ["ge\u00b7nom\u00b7men", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.25": {"text": "Und, auf Magneten Art, gezogen; indem die\u00df m\u00f6glicher", "tokens": ["Und", ",", "auf", "Mag\u00b7ne\u00b7ten", "Art", ",", "ge\u00b7zo\u00b7gen", ";", "in\u00b7dem", "die\u00df", "m\u00f6g\u00b7li\u00b7cher"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["KON", "$,", "APPR", "ADJA", "NN", "$,", "VVPP", "$.", "KOUS", "PDS", "ADJA"], "meter": "--+--+-+-++-+--", "measure": "anapaest.di.plus"}, "line.26": {"text": "noch scheint,", "tokens": ["noch", "scheint", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.27": {"text": "Da\u00df eine Menge vieler Theile mit ihr von au\u00dfen sich", "tokens": ["Da\u00df", "ei\u00b7ne", "Men\u00b7ge", "vie\u00b7ler", "Thei\u00b7le", "mit", "ihr", "von", "au\u00b7\u00dfen", "sich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "PIAT", "NN", "APPR", "PPOSAT", "APPR", "ADV", "PRF"], "meter": "-+-+-+-+-+--+-+", "measure": "iambic.septa.relaxed"}, "line.28": {"text": "vereint,", "tokens": ["ver\u00b7eint", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.29": {"text": "Als da\u00df sie in ihr eingeschlossen, in solchem engen Raum", "tokens": ["Als", "da\u00df", "sie", "in", "ihr", "ein\u00b7ge\u00b7schlos\u00b7sen", ",", "in", "sol\u00b7chem", "en\u00b7gen", "Raum"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "PPER", "APPR", "PPER", "VVPP", "$,", "APPR", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.30": {"text": "verschr\u00e4nket,", "tokens": ["ver\u00b7schr\u00e4n\u00b7ket", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.31": {"text": "Ganz unbegreiflich liegen sollten. Allein, wie tief man", "tokens": ["Ganz", "un\u00b7be\u00b7greif\u00b7lich", "lie\u00b7gen", "soll\u00b7ten", ".", "Al\u00b7lein", ",", "wie", "tief", "man"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADJD", "VVINF", "VMFIN", "$.", "ADV", "$,", "PWAV", "ADJD", "PIS"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.32": {"text": "immer denket,", "tokens": ["im\u00b7mer", "den\u00b7ket", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}}}}}