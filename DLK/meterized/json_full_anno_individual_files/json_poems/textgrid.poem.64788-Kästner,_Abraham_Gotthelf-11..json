{"textgrid.poem.64788": {"metadata": {"author": {"name": "K\u00e4stner, Abraham Gotthelf", "birth": "N.A.", "death": "N.A."}, "title": "11.", "genre": "verse", "period": "N.A.", "pub_year": 1759, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Noch kurze Zeit darf ich dich meine nennen,", "tokens": ["Noch", "kur\u00b7ze", "Zeit", "darf", "ich", "dich", "mei\u00b7ne", "nen\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VMFIN", "PPER", "PRF", "PPOSAT", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Eh' meinem Arm ein st\u00e4rkrer dich entrei\u00dft.", "tokens": ["Eh'", "mei\u00b7nem", "Arm", "ein", "st\u00e4r\u00b7krer", "dich", "ent\u00b7rei\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ART", "ADJA", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Noch kannst du mir ein z\u00e4rtlich Lied verg\u00f6nnen,", "tokens": ["Noch", "kannst", "du", "mir", "ein", "z\u00e4rt\u00b7lich", "Lied", "ver\u00b7g\u00f6n\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPER", "ART", "ADJD", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Eh' dich mein Lied voll Ehrfurcht selig preist.", "tokens": ["Eh'", "dich", "mein", "Lied", "voll", "Ehr\u00b7furcht", "se\u00b7lig", "preist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "ADJD", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wie? st\u00f6rt mein Reim nicht mit zu eitlen Trieben", "tokens": ["Wie", "?", "st\u00f6rt", "mein", "Reim", "nicht", "mit", "zu", "eit\u00b7len", "Trie\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "$.", "VVFIN", "PPOSAT", "NN", "PTKNEG", "APPR", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Dich, die bereits nur an den Himmel denkt?", "tokens": ["Dich", ",", "die", "be\u00b7reits", "nur", "an", "den", "Him\u00b7mel", "denkt", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "ADV", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Nein, wie du liebst, kannst du mich auch noch lieben,", "tokens": ["Nein", ",", "wie", "du", "liebst", ",", "kannst", "du", "mich", "auch", "noch", "lie\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PWAV", "PPER", "VVFIN", "$,", "VMFIN", "PPER", "PRF", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Wenn sich dein Aug', ach! brechend auf mich lenkt.", "tokens": ["Wenn", "sich", "dein", "Aug'", ",", "ach", "!", "bre\u00b7chend", "auf", "mich", "lenkt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "PPOSAT", "NN", "$,", "ITJ", "$.", "ADJD", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "So starke Gluth ein menschlich Herz empfindet,", "tokens": ["So", "star\u00b7ke", "Gluth", "ein", "menschlich", "Herz", "emp\u00b7fin\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "ART", "ADJD", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "So starke Gluth hat dich f\u00fcr mich ger\u00fchrt:", "tokens": ["So", "star\u00b7ke", "Gluth", "hat", "dich", "f\u00fcr", "mich", "ge\u00b7r\u00fchrt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VAFIN", "PRF", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "So reine Huld, als Geister nur verbindet,", "tokens": ["So", "rei\u00b7ne", "Huld", ",", "als", "Geis\u00b7ter", "nur", "ver\u00b7bin\u00b7det", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$,", "KOUS", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "So reine Huld hat deine Gluth regiert.", "tokens": ["So", "rei\u00b7ne", "Huld", "hat", "dei\u00b7ne", "Gluth", "re\u00b7giert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VAFIN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Auf Erden nur durch dich mich zu begl\u00fccken,", "tokens": ["Auf", "Er\u00b7den", "nur", "durch", "dich", "mich", "zu", "be\u00b7gl\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "APPR", "PPER", "PRF", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Der Endzweck war f\u00fcr deine Gunst zu klein,", "tokens": ["Der", "End\u00b7zweck", "war", "f\u00fcr", "dei\u00b7ne", "Gunst", "zu", "klein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "PPOSAT", "NN", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Ein gr\u00f6\u00dfer Wohl zeigt sich erhabnern Blicken,", "tokens": ["Ein", "gr\u00f6\u00b7\u00dfer", "Wohl", "zeigt", "sich", "er\u00b7hab\u00b7nern", "Bli\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PRF", "ADJA", "NN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.8": {"text": "Da wolltest du auch ewig bey mir seyn.", "tokens": ["Da", "woll\u00b7test", "du", "auch", "e\u00b7wig", "bey", "mir", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ADJD", "APPR", "PPER", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Doch dich soll schon das gr\u00f6\u00dfre Gl\u00fcck erg\u00f6tzen,", "tokens": ["Doch", "dich", "soll", "schon", "das", "gr\u00f6\u00df\u00b7re", "Gl\u00fcck", "er\u00b7g\u00f6t\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "ADV", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und mir wird nicht das irdische geg\u00f6nnt!", "tokens": ["Und", "mir", "wird", "nicht", "das", "ir\u00b7di\u00b7sche", "ge\u00b7g\u00f6nnt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PTKNEG", "ART", "ADJA", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und wird vielleicht, Gedanke voll Entsetzen!", "tokens": ["Und", "wird", "viel\u00b7leicht", ",", "Ge\u00b7dan\u00b7ke", "voll", "Ent\u00b7set\u00b7zen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "$,", "NN", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dein Geist von mir auf ewig jetzt getrennt?", "tokens": ["Dein", "Geist", "von", "mir", "auf", "e\u00b7wig", "jetzt", "ge\u00b7trennt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PPER", "APPR", "ADJD", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Nein, der uns schuf, verlangt nicht mein Verderben;", "tokens": ["Nein", ",", "der", "uns", "schuf", ",", "ver\u00b7langt", "nicht", "mein", "Ver\u00b7der\u00b7ben", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PRELS", "PPER", "VVFIN", "$,", "VVFIN", "PTKNEG", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Auch mir hat Er dein Gl\u00fccke zugedacht;", "tokens": ["Auch", "mir", "hat", "Er", "dein", "Gl\u00fc\u00b7cke", "zu\u00b7ge\u00b7dacht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VAFIN", "PPER", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Ich soll darum nur l\u00e4nger zitternd werben,", "tokens": ["Ich", "soll", "da\u00b7rum", "nur", "l\u00e4n\u00b7ger", "zit\u00b7ternd", "wer\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PAV", "ADV", "ADJD", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und geh' zu dir nach kurzer Jahre Nacht.", "tokens": ["Und", "geh'", "zu", "dir", "nach", "kur\u00b7zer", "Jah\u00b7re", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPER", "APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Da werden wir als Geister uns noch kennen,", "tokens": ["Da", "wer\u00b7den", "wir", "als", "Geis\u00b7ter", "uns", "noch", "ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "KOUS", "NN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Begl\u00fcckter weit, als wir uns hier gekannt,", "tokens": ["Be\u00b7gl\u00fcck\u00b7ter", "weit", ",", "als", "wir", "uns", "hier", "ge\u00b7kannt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "$,", "KOUS", "PPER", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und gegen Gott in sel'ger Liebe brennen,", "tokens": ["Und", "ge\u00b7gen", "Gott", "in", "sel'\u00b7ger", "Lie\u00b7be", "bren\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Vereint vor Ihm, wie Er uns hier verband.", "tokens": ["Ver\u00b7eint", "vor", "Ihm", ",", "wie", "Er", "uns", "hier", "ver\u00b7band", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "$,", "PWAV", "PPER", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Noch, weil die Welt hier meine Sinnen r\u00fchret,", "tokens": ["Noch", ",", "weil", "die", "Welt", "hier", "mei\u00b7ne", "Sin\u00b7nen", "r\u00fch\u00b7ret", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "ART", "NN", "ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Soll stets vor mir dein strahlend Bildni\u00df stehn.", "tokens": ["Soll", "stets", "vor", "mir", "dein", "strah\u00b7lend", "Bild\u00b7ni\u00df", "stehn", "."], "token_inf