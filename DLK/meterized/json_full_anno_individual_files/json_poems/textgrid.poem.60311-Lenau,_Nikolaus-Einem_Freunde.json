{"textgrid.poem.60311": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Einem Freunde", "genre": "verse", "period": "N.A.", "pub_year": 1843, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sp\u00e4t hab ich dich gefunden", "tokens": ["Sp\u00e4t", "hab", "ich", "dich", "ge\u00b7fun\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "PPER", "PRF", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und mu\u00df das Los beklagen,", "tokens": ["Und", "mu\u00df", "das", "Los", "be\u00b7kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das nicht in Jugendtagen", "tokens": ["Das", "nicht", "in", "Ju\u00b7gend\u00b7ta\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "PTKNEG", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mein Herz an deins gebunden.", "tokens": ["Mein", "Herz", "an", "deins", "ge\u00b7bun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "NE", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Verklungen sind die Feste,", "tokens": ["Ver\u00b7klun\u00b7gen", "sind", "die", "Fes\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Jugendtr\u00e4ume ferne;", "tokens": ["Die", "Ju\u00b7gend\u00b7tr\u00e4u\u00b7me", "fer\u00b7ne", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wie h\u00e4lt ich sie so gerne", "tokens": ["Wie", "h\u00e4lt", "ich", "sie", "so", "ger\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "PPER", "ADV", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mit dir geteilt, das Beste!", "tokens": ["Mit", "dir", "ge\u00b7teilt", ",", "das", "Bes\u00b7te", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVPP", "$,", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und k\u00f6nnt uns nicht vereinen", "tokens": ["Und", "k\u00f6nnt", "uns", "nicht", "ver\u00b7ei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Lenz in seinen Bl\u00fcten,", "tokens": ["Der", "Lenz", "in", "sei\u00b7nen", "Bl\u00fc\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So wills der Herbst verg\u00fcten", "tokens": ["So", "wills", "der", "Herbst", "ver\u00b7g\u00fc\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In seinen welken Hainen.", "tokens": ["In", "sei\u00b7nen", "wel\u00b7ken", "Hai\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Der Luft entbl\u00e4tternd Wehen,", "tokens": ["Der", "Luft", "ent\u00b7bl\u00e4t\u00b7ternd", "We\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Himmel, k\u00fchler, tr\u00fcber,", "tokens": ["Der", "Him\u00b7mel", ",", "k\u00fch\u00b7ler", ",", "tr\u00fc\u00b7ber", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "$,", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Macht, da\u00df wir nicht vor\u00fcber", "tokens": ["Macht", ",", "da\u00df", "wir", "nicht", "vor\u00b7\u00fc\u00b7ber"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "KOUS", "PPER", "PTKNEG", "PTKVZ"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Am warmen Herzen gehen.", "tokens": ["Am", "war\u00b7men", "Her\u00b7zen", "ge\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Sp\u00e4t hab ich dich gefunden", "tokens": ["Sp\u00e4t", "hab", "ich", "dich", "ge\u00b7fun\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "PPER", "PRF", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und mu\u00df das Los beklagen,", "tokens": ["Und", "mu\u00df", "das", "Los", "be\u00b7kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das nicht in Jugendtagen", "tokens": ["Das", "nicht", "in", "Ju\u00b7gend\u00b7ta\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "PTKNEG", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mein Herz an deins gebunden.", "tokens": ["Mein", "Herz", "an", "deins", "ge\u00b7bun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "NE", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"li