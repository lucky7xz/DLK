{"textgrid.poem.60197": {"metadata": {"author": {"name": "Karsch, Anna Louisa", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der du mit lachendem Auge,", "genre": "verse", "period": "N.A.", "pub_year": 1761, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der du mit lachendem Auge,", "tokens": ["Der", "du", "mit", "la\u00b7chen\u00b7dem", "Au\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Ansahst den st\u00fcrzenden Flug", "tokens": ["An\u00b7sahst", "den", "st\u00fcr\u00b7zen\u00b7den", "Flug"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Zum Wagen, welcher mich eilig", "tokens": ["Zum", "Wa\u00b7gen", ",", "wel\u00b7cher", "mich", "ei\u00b7lig"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "PRELS", "PPER", "ADJD"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Des Elbstrohms Ufer enttrug.", "tokens": ["Des", "E\u00b7lbstrohms", "U\u00b7fer", "ent\u00b7trug", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "NN", "VVFIN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Freund, w\u00fcst' ich die lyrischen Th\u00f6ne", "tokens": ["Freund", ",", "w\u00fcst'", "ich", "die", "ly\u00b7ri\u00b7schen", "Th\u00f6\u00b7ne"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Von Utz und Wei\u00dfen gespielt,", "tokens": ["Von", "Utz", "und", "Wei\u00b7\u00dfen", "ge\u00b7spielt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "NN", "VVPP", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Dann w\u00fcrd im s\u00fcssem Gesange", "tokens": ["Dann", "w\u00fcrd", "im", "s\u00fcs\u00b7sem", "Ge\u00b7san\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Dir h\u00f6rbar, was ich gef\u00fchlt,", "tokens": ["Dir", "h\u00f6r\u00b7bar", ",", "was", "ich", "ge\u00b7f\u00fchlt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "$,", "PWS", "PPER", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Dort, bey dem S\u00e4nger, der feurig", "tokens": ["Dort", ",", "bey", "dem", "S\u00e4n\u00b7ger", ",", "der", "feu\u00b7rig"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "$,", "APPR", "ART", "NN", "$,", "PRELS", "ADJD"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Gesungen Schlachten und Sieg;", "tokens": ["Ge\u00b7sun\u00b7gen", "Schlach\u00b7ten", "und", "Sieg", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Bey dem, mit welchem ich h\u00fcpfend", "tokens": ["Bey", "dem", ",", "mit", "wel\u00b7chem", "ich", "h\u00fcp\u00b7fend"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "$,", "APPR", "PRELS", "PPER", "VVPP"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Den Berg der Musen bestieg.", "tokens": ["Den", "Berg", "der", "Mu\u00b7sen", "be\u00b7stieg", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.4": {"line.1": {"text": "In drey\u00dfig l\u00e4chelnden Tagen", "tokens": ["In", "drey\u00b7\u00dfig", "l\u00e4\u00b7cheln\u00b7den", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "CARD", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "War mein Gesch\u00e4fte die Lust.", "tokens": ["War", "mein", "Ge\u00b7sch\u00e4f\u00b7te", "die", "Lust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Sanft brausend str\u00f6mten Ges\u00e4nge", "tokens": ["Sanft", "brau\u00b7send", "str\u00f6m\u00b7ten", "Ge\u00b7s\u00e4n\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "ADJD", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Empor aus f\u00fchlender Brust.", "tokens": ["Em\u00b7por", "aus", "f\u00fch\u00b7len\u00b7der", "Brust", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.5": {"line.1": {"text": "Ach! wie ist alles verg\u00e4nglich!", "tokens": ["Ach", "!", "wie", "ist", "al\u00b7les", "ver\u00b7g\u00e4ng\u00b7lich", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PWAV", "VAFIN", "PIS", "ADJD", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "O unerbittlich Geschick!", "tokens": ["O", "un\u00b7er\u00b7bitt\u00b7lich", "Ge\u00b7schick", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJD", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Mit wiederkommenden R\u00e4dern,", "tokens": ["Mit", "wie\u00b7der\u00b7kom\u00b7men\u00b7den", "R\u00e4\u00b7dern", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Ri\u00df michs eilfertig zur\u00fcck!", "tokens": ["Ri\u00df", "michs", "eil\u00b7fer\u00b7tig", "zu\u00b7r\u00fcck", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PIS", "ADJD", "PTKVZ", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.6": {"line.1": {"text": "So rei\u00dft von jedem Vergn\u00fcgen", "tokens": ["So", "rei\u00dft", "von", "je\u00b7dem", "Ver\u00b7gn\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PIAT", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Mich der starkarmichte Feind,", "tokens": ["Mich", "der", "star\u00b7kar\u00b7mich\u00b7te", "Feind", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "An einem k\u00fcnftigen Tage,", "tokens": ["An", "ei\u00b7nem", "k\u00fcnf\u00b7ti\u00b7gen", "Ta\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und Klagen redet der Freund!", "tokens": ["Und", "Kla\u00b7gen", "re\u00b7det", "der", "Freund", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.7": {"line.1": {"text": "Mein Leben, schneller als R\u00e4der,", "tokens": ["Mein", "Le\u00b7ben", ",", "schnel\u00b7ler", "als", "R\u00e4\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADJD", "KOKOM", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Eilt an das wartende Grab;", "tokens": ["Eilt", "an", "das", "war\u00b7ten\u00b7de", "Grab", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Da senken diese Gebeine", "tokens": ["Da", "sen\u00b7ken", "die\u00b7se", "Ge\u00b7bei\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PDAT", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Acht M\u00e4nner traurig hinab,", "tokens": ["Acht", "M\u00e4n\u00b7ner", "trau\u00b7rig", "hin\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.8": {"line.1": {"text": "Und werfen hurtig ein jeder", "tokens": ["Und", "wer\u00b7fen", "hur\u00b7tig", "ein", "je\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD", "ART", "PIAT"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Auf mich drey H\u00e4nde voll Staub.", "tokens": ["Auf", "mich", "drey", "H\u00e4n\u00b7de", "voll", "Staub", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "CARD", "NN", "ADJD", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Da lieg ich unter dem H\u00fcgel,", "tokens": ["Da", "lieg", "ich", "un\u00b7ter", "dem", "H\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Der W\u00fcrmer ruhiger Raub,", "tokens": ["Der", "W\u00fcr\u00b7mer", "ru\u00b7hi\u00b7ger", "Raub", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.9": {"line.1": {"text": "Bis zehen tausend mahl tausend", "tokens": ["Bis", "ze\u00b7hen", "tau\u00b7send", "mahl", "tau\u00b7send"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "CARD", "CARD", "ADV", "CARD"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Hochfahrende Wagen daher", "tokens": ["Hoch\u00b7fah\u00b7ren\u00b7de", "Wa\u00b7gen", "da\u00b7her"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "PAV"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Gekommen mit dem Erwecker,", "tokens": ["Ge\u00b7kom\u00b7men", "mit", "dem", "Er\u00b7we\u00b7cker", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und Gluth verschlucket das Meer,", "tokens": ["Und", "Gluth", "ver\u00b7schluc\u00b7ket", "das", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.10": {"line.1": {"text": "Und Himmel trotzende Berge", "tokens": ["Und", "Him\u00b7mel", "trot\u00b7zen\u00b7de", "Ber\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NN", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Staub werden, und die Natur", "tokens": ["Staub", "wer\u00b7den", ",", "und", "die", "Na\u00b7tur"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VAINF", "$,", "KON", "ART", "NN"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Aufh\u00f6rt den Wagen zu lenken,", "tokens": ["Auf\u00b7h\u00f6rt", "den", "Wa\u00b7gen", "zu", "len\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Der schnell mit Tagen entfuh