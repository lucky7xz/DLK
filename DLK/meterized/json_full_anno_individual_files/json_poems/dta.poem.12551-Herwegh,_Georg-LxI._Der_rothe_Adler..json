{"dta.poem.12551": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "LxI.  \n  Der rothe Adler.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1843", "urn": "urn:nbn:de:kobv:b4-200905192440", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201eals Preu\u00dfen einst \u2014 Dank jener Knute! \u2014", "tokens": ["\u201e", "als", "Preu\u00b7\u00dfen", "einst", "Dank", "je\u00b7ner", "Knu\u00b7te", "!"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "KOUS", "NN", "ADV", "$(", "APPR", "PDAT", "NN", "$.", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Beim gro\u00dfen Raube mitgeerbt:", "tokens": ["Beim", "gro\u00b7\u00dfen", "Rau\u00b7be", "mit\u00b7ge\u00b7erbt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da haben sie in Polens Blute", "tokens": ["Da", "ha\u00b7ben", "sie", "in", "Po\u00b7lens", "Blu\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Auch meine Schwingen roth gef\u00e4rbt.", "tokens": ["Auch", "mei\u00b7ne", "Schwin\u00b7gen", "roth", "ge\u00b7f\u00e4rbt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "An goldner Kette schmacht' ich hier", "tokens": ["An", "gold\u00b7ner", "Ket\u00b7te", "schmacht'", "ich", "hier"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und bin der Bote ihrer Witze:", "tokens": ["Und", "bin", "der", "Bo\u00b7te", "ih\u00b7rer", "Wit\u00b7ze", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "O Zeus, nimm deinen Aar zu dir,", "tokens": ["O", "Zeus", ",", "nimm", "dei\u00b7nen", "Aar", "zu", "dir", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "VVIMP", "PPOSAT", "NN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und gieb ihm wieder deine Blitze!\u201c", "tokens": ["Und", "gieb", "ihm", "wie\u00b7der", "dei\u00b7ne", "Blit\u00b7ze", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVIMP", "PPER", "ADV", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}