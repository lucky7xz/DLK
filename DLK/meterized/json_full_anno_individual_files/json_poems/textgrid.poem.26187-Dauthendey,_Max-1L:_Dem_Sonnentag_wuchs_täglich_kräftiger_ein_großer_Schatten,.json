{"textgrid.poem.26187": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "1L: Dem Sonnentag wuchs t\u00e4glich kr\u00e4ftiger ein gro\u00dfer Schatten,", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dem Sonnentag wuchs t\u00e4glich kr\u00e4ftiger ein gro\u00dfer Schatten,", "tokens": ["Dem", "Son\u00b7nen\u00b7tag", "wuchs", "t\u00e4g\u00b7lich", "kr\u00e4f\u00b7ti\u00b7ger", "ein", "gro\u00b7\u00dfer", "Schat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "Liegt breit vom Weinberg an den Flu\u00df herab,", "tokens": ["Liegt", "breit", "vom", "Wein\u00b7berg", "an", "den", "Flu\u00df", "her\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPRART", "NN", "APPR", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Als zieht er jeden Baum zu sich hinab,", "tokens": ["Als", "zieht", "er", "je\u00b7den", "Baum", "zu", "sich", "hin\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPER", "PIAT", "NN", "APPR", "PRF", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Es m\u00fcssen die Bl\u00e4tter einmal von seinem Gewicht ermatten.", "tokens": ["Es", "m\u00fcs\u00b7sen", "die", "Bl\u00e4t\u00b7ter", "ein\u00b7mal", "von", "sei\u00b7nem", "Ge\u00b7wicht", "er\u00b7mat\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+--+-+--+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Er liegt wie eine Grube abgr\u00fcndig bei den Hecken,", "tokens": ["Er", "liegt", "wie", "ei\u00b7ne", "Gru\u00b7be", "ab\u00b7gr\u00fcn\u00b7dig", "bei", "den", "He\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "ART", "NN", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Er fliegt mit den Wolken und dehnt sich aus Verstecken.", "tokens": ["Er", "fliegt", "mit", "den", "Wol\u00b7ken", "und", "dehnt", "sich", "aus", "Ver\u00b7ste\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "KON", "VVFIN", "PRF", "APPR", "NN", "$."], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.7": {"text": "Der Schatten sieht die G\u00e4rten bedrohlich finster an,", "tokens": ["Der", "Schat\u00b7ten", "sieht", "die", "G\u00e4r\u00b7ten", "be\u00b7droh\u00b7lich", "fins\u00b7ter", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "ADJD", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Als ob er sich anschicken kann zu einer Macht,", "tokens": ["Als", "ob", "er", "sich", "an\u00b7schi\u00b7cken", "kann", "zu", "ei\u00b7ner", "Macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "PRF", "VVINF", "VMFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Und nur Verliebte schreckt nicht in seinen Blicken die ewige Nacht.", "tokens": ["Und", "nur", "Ver\u00b7lieb\u00b7te", "schreckt", "nicht", "in", "sei