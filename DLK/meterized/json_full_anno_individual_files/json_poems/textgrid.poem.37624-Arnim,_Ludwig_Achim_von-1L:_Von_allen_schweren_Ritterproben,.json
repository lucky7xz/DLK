{"textgrid.poem.37624": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Von allen schweren Ritterproben,", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Von allen schweren Ritterproben,", "tokens": ["Von", "al\u00b7len", "schwe\u00b7ren", "Rit\u00b7ter\u00b7pro\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die einst dem J\u00fcngling auferlegt,", "tokens": ["Die", "einst", "dem", "J\u00fcng\u00b7ling", "auf\u00b7er\u00b7legt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gilt ", "tokens": ["Gilt"], "token_info": ["word"], "pos": ["NE"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Ist jeder, der sie fest ertr\u00e4gt.", "tokens": ["Ist", "je\u00b7der", ",", "der", "sie", "fest", "er\u00b7tr\u00e4gt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "$,", "PRELS", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Zwar hei\u00dfe That ist's nicht zu nennen,", "tokens": ["Zwar", "hei\u00b7\u00dfe", "That", "ist's", "nicht", "zu", "nen\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VAFIN", "PTKNEG", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Doch hat sich mancher dran verbrannt;", "tokens": ["Doch", "hat", "sich", "man\u00b7cher", "dran", "ver\u00b7brannt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PRF", "PIS", "PAV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Weil Worte auf der Zunge brennen,", "tokens": ["Weil", "Wor\u00b7te", "auf", "der", "Zun\u00b7ge", "bren\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wird ", "tokens": ["Wird"], "token_info": ["word"], "pos": ["VAFIN"], "meter": "+", "measure": "single.up"}}, "stanza.3": {"line.1": {"text": "Zur Probe \u00f6ffnet Nacht die Pforte,", "tokens": ["Zur", "Pro\u00b7be", "\u00f6ff\u00b7net", "Nacht", "die", "Pfor\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Geheimni\u00df in den Sternen strahlt,", "tokens": ["Ge\u00b7heim\u00b7ni\u00df", "in", "den", "Ster\u00b7nen", "strahlt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "S\u00fc\u00df t\u00f6nen sch\u00f6ner Frauen Worte,", "tokens": ["S\u00fc\u00df", "t\u00f6\u00b7nen", "sch\u00f6\u00b7ner", "Frau\u00b7en", "Wor\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Womit so gern der J\u00fcngling prahlt.", "tokens": ["Wo\u00b7mit", "so", "gern", "der", "J\u00fcng\u00b7ling", "prahlt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Doch jede legt auf ihre Lippen", "tokens": ["Doch", "je\u00b7de", "legt", "auf", "ih\u00b7re", "Lip\u00b7pen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Den Zeigefinger, eh sie spricht,", "tokens": ["Den", "Zei\u00b7ge\u00b7fin\u00b7ger", ",", "eh", "sie", "spricht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es zeiget jede ihm die Klippen,", "tokens": ["Es", "zei\u00b7get", "je\u00b7de", "ihm", "die", "Klip\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Woran der Ehre Woge bricht.", "tokens": ["Wo\u00b7ran", "der", "Eh\u00b7re", "Wo\u00b7ge", "bricht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Die Liebste schm\u00fcckt ihm Lieblingspl\u00e4tze", "tokens": ["Die", "Liebs\u00b7te", "schm\u00fcckt", "ihm", "Lieb\u00b7lings\u00b7pl\u00e4t\u00b7ze"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit ihrer Kr\u00e4nze buntem Glanz,", "tokens": ["Mit", "ih\u00b7rer", "Kr\u00e4n\u00b7ze", "bun\u00b7tem", "Glanz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es reihet sich zu dem Geschw\u00e4tze", "tokens": ["Es", "rei\u00b7het", "sich", "zu", "dem", "Ge\u00b7schw\u00e4t\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Der H\u00e4ndedruck im heitern Tanz.", "tokens": ["Der", "H\u00e4n\u00b7de\u00b7druck", "im", "hei\u00b7tern", "Tanz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Zu bald, \u2013 als Morgenlicht erschienen,", "tokens": ["Zu", "bald", ",", "\u2013", "als", "Mor\u00b7gen\u00b7licht", "er\u00b7schie\u00b7nen", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "$,", "$(", "KOUS", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Er kehret heim in Seligkeit;", "tokens": ["Er", "keh\u00b7ret", "heim", "in", "Se\u00b7lig\u00b7keit", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es lacht die Welt der frohen Mienen", "tokens": ["Es", "lacht", "die", "Welt", "der", "fro\u00b7hen", "Mie\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und fr\u00e4gt ihn aus, was ihn erfreut.", "tokens": ["Und", "fr\u00e4gt", "ihn", "aus", ",", "was", "ihn", "er\u00b7freut", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKVZ", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Da\u00df ihm allein dies Gl\u00fcck sei eigen,", "tokens": ["Da\u00df", "ihm", "al\u00b7lein", "dies", "Gl\u00fcck", "sei", "ei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PDS", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Es dr\u00fcckt ihm fast die Seele ab,", "tokens": ["Es", "dr\u00fcckt", "ihm", "fast", "die", "See\u00b7le", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch wollt' er sich als Ritter zeigen,", "tokens": ["Doch", "wollt'", "er", "sich", "als", "Rit\u00b7ter", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PRF", "KOUS", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "M\u00fc\u00df er's verschweigen bis in's Grab.", "tokens": ["M\u00fc\u00df", "er's", "ver\u00b7schwei\u00b7gen", "bis", "in's", "Grab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "APPR", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Beim ersten Wort, das er gesprochen,", "tokens": ["Beim", "ers\u00b7ten", "Wort", ",", "das", "er", "ge\u00b7spro\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Schl\u00e4gt zu die Pforte seiner Lust,", "tokens": ["Schl\u00e4gt", "zu", "die", "Pfor\u00b7te", "sei\u00b7ner", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Die Sterne haben sich verkrochen,", "tokens": ["Die", "Ster\u00b7ne", "ha\u00b7ben", "sich", "ver\u00b7kro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Des Ritters Stern f\u00e4llt von der Brust.", "tokens": ["Des", "Rit\u00b7ters", "Stern", "f\u00e4llt", "von", "der", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Er reitet auf den h\u00f6lzern Schranken,", "tokens": ["Er", "rei\u00b7tet", "auf", "den", "h\u00f6l\u00b7zern", "Schran\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Statt auf dem hohen Rittergaul! \u2013", "tokens": ["Statt", "auf", "dem", "ho\u00b7hen", "Rit\u00b7ter\u00b7gaul", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Darum sei gl\u00fccklich in Gedanken,", "tokens": ["Da\u00b7rum", "sei", "gl\u00fcck\u00b7lich", "in", "Ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sei gl\u00fccklich, J\u00fcngling, halt das Maul! \u2013", "tokens": ["Sei", "gl\u00fcck\u00b7lich", ",", "J\u00fcng\u00b7ling", ",", "halt", "das", "Maul", "!", "\u2013"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["VAIMP", "ADJD", "$,", "NN", "$,", "VVFIN", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iamb