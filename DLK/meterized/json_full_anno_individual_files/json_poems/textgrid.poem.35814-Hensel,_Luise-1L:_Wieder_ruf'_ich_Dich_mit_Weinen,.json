ens": ["Lust", "und", "Schmer\u00b7zen", ",", "Fried'", "und", "Streit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Will ich Dir nun ganz ergeben:", "tokens": ["Will", "ich", "Dir", "nun", "ganz", "er\u00b7ge\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "ADV", "ADV", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "F\u00fchre mich, ich bin bereit.", "tokens": ["F\u00fch\u00b7re", "mich", ",", "ich", "bin", "be\u00b7reit", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PPER", "VAFIN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.16": {"line.1": {"text": "Ja, Du f\u00fchrst in Lieb' und Gnade", "tokens": ["Ja", ",", "Du", "f\u00fchrst", "in", "Lieb'", "und", "Gna\u00b7de"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "PPER", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mich an Deiner treuen Hand,", "tokens": ["Mich", "an", "Dei\u00b7ner", "treu\u00b7en", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "F\u00fchrest mich auf sicherm Pfade", "tokens": ["F\u00fch\u00b7rest", "mich", "auf", "si\u00b7cherm", "Pfa\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "APPR", "ADJA", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "In Dein ewig Friedensland. Amen.", "tokens": ["In", "Dein", "e\u00b7wig", "Frie\u00b7dens\u00b7land", ".", "A\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJD", "NN", "$.", "NN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}}}}}