{"dta.poem.4098": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Die b\u00f6se Farbe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1821", "urn": "urn:nbn:de:kobv:b4-200905194209", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Hinaus in die weite Welt,", "tokens": ["Hin\u00b7aus", "in", "die", "wei\u00b7te", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "----+-+", "measure": "unknown.measure.di"}, "line.2": {"text": "Wenn's nur so gr\u00fcn, so gr\u00fcn nicht w\u00e4r'", "tokens": ["Wenn's", "nur", "so", "gr\u00fcn", ",", "so", "gr\u00fcn", "nicht", "w\u00e4r'"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ADV", "ADJD", "$,", "ADV", "ADJD", "PTKNEG", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da drau\u00dfen in Wald und Feld!", "tokens": ["Da", "drau\u00b7\u00dfen", "in", "Wald", "und", "Feld", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "NN", "KON", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Ich m\u00f6chte die gr\u00fcnen Bl\u00e4tter all'", "tokens": ["Ich", "m\u00f6ch\u00b7te", "die", "gr\u00fc\u00b7nen", "Bl\u00e4t\u00b7ter", "all'"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ART", "ADJA", "NN", "PIAT"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Pfl\u00fccken von jedem Zweig,", "tokens": ["Pfl\u00fc\u00b7cken", "von", "je\u00b7dem", "Zweig", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PIAT", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Ich m\u00f6chte die gr\u00fcnen Gr\u00e4ser all'", "tokens": ["Ich", "m\u00f6ch\u00b7te", "die", "gr\u00fc\u00b7nen", "Gr\u00e4\u00b7ser", "all'"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ART", "ADJA", "NN", "PIAT"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Weinen ganz todtenbleich.", "tokens": ["Wei\u00b7nen", "ganz", "tod\u00b7ten\u00b7bleich", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADJD", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.3": {"line.1": {"text": "Ach Gr\u00fcn, du b\u00f6se Farbe du,", "tokens": ["Ach", "Gr\u00fcn", ",", "du", "b\u00f6\u00b7se", "Far\u00b7be", "du", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "$,", "PPER", "ADJA", "NN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was siehst mich immer an,", "tokens": ["Was", "siehst", "mich", "im\u00b7mer", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So stolz, so keck, so schadenfroh,", "tokens": ["So", "stolz", ",", "so", "keck", ",", "so", "scha\u00b7den\u00b7froh", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADV", "ADJD", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mich armen wei\u00dfen Mann?", "tokens": ["Mich", "ar\u00b7men", "wei\u00b7\u00dfen", "Mann", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ich m\u00f6chte liegen vor ihrer Th\u00fcr,", "tokens": ["Ich", "m\u00f6ch\u00b7te", "lie\u00b7gen", "vor", "ih\u00b7rer", "Th\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "In Sturm und Regen und Schnee,", "tokens": ["In", "Sturm", "und", "Re\u00b7gen", "und", "Schnee", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "KON", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Und singen ganz leise bei Tag und Nacht", "tokens": ["Und", "sin\u00b7gen", "ganz", "lei\u00b7se", "bei", "Tag", "und", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "ADJD", "APPR", "NN", "KON", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Das eine W\u00f6rtchen Ade!", "tokens": ["Das", "ei\u00b7ne", "W\u00f6rt\u00b7chen", "A\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Horch, wenn im Wald ein Jagdhorn ruft,", "tokens": ["Horch", ",", "wenn", "im", "Wald", "ein", "Jagd\u00b7horn", "ruft", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KOUS", "APPRART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Da klingt ihr Fensterlein,", "tokens": ["Da", "klingt", "ihr", "Fens\u00b7ter\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und schaut sie auch nach mir nicht aus,", "tokens": ["Und", "schaut", "sie", "auch", "nach", "mir", "nicht", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPR", "PPER", "PTKNEG", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Darf ich doch schauen hinein.", "tokens": ["Darf", "ich", "doch", "schau\u00b7en", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "VVFIN", "PTKVZ", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.6": {"line.1": {"text": "O binde von der Stirn dir ab", "tokens": ["O", "bin\u00b7de", "von", "der", "Stirn", "dir", "ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "APPR", "ART", "NN", "PPER", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das gr\u00fcne, gr\u00fcne Band,", "tokens": ["Das", "gr\u00fc\u00b7ne", ",", "gr\u00fc\u00b7ne", "Band", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ade, Ade! und reiche mir", "tokens": ["A\u00b7de", ",", "A\u00b7de", "!", "und", "rei\u00b7che", "mir"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "NN", "$.", "KON", "VVFIN", "PPER"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Zum Abschied deine Hand!", "tokens": ["Zum", "Ab\u00b7schied", "dei\u00b7ne", "Hand", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}