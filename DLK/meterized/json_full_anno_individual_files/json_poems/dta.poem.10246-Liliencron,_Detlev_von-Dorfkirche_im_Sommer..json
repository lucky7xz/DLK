{"dta.poem.10246": {"metadata": {"author": {"name": "Liliencron, Detlev von", "birth": "N.A.", "death": "N.A."}, "title": "Dorfkirche im Sommer.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1883", "urn": "urn:nbn:de:kobv:b4-200905197184", "language": ["de:0.99"], "booktitle": "Liliencron, Detlev von: Adjutantenritte und andere Gedichte. Leipzig, [1883]."}, "poem": {"stanza.1": {"line.1": {"text": "Schl\u00e4frig singt der K\u00fcster vor,", "tokens": ["Schl\u00e4f\u00b7rig", "singt", "der", "K\u00fcs\u00b7ter", "vor", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Schl\u00e4frig singt auch die Gemeinde,", "tokens": ["Schl\u00e4f\u00b7rig", "singt", "auch", "die", "Ge\u00b7mein\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Auf der Kanzel der Pastor", "tokens": ["Auf", "der", "Kan\u00b7zel", "der", "Pas\u00b7tor"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN"], "meter": "+-++--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Betet still f\u00fcr seine Feinde.", "tokens": ["Be\u00b7tet", "still", "f\u00fcr", "sei\u00b7ne", "Fein\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Dann die Predigt, wunderbar,", "tokens": ["Dann", "die", "Pre\u00b7digt", ",", "wun\u00b7der\u00b7bar", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Eine Predigt ohne Gleichen.", "tokens": ["Ei\u00b7ne", "Pre\u00b7digt", "oh\u00b7ne", "Glei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Die Baronin weint sogar", "tokens": ["Die", "Ba\u00b7ro\u00b7nin", "weint", "so\u00b7gar"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.4": {"text": "Im Gest\u00fchl, dem wappenreichen.", "tokens": ["Im", "Ge\u00b7st\u00fchl", ",", "dem", "wap\u00b7pen\u00b7rei\u00b7chen", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PRELS", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Amen, Segen, Th\u00fcren weit,", "tokens": ["A\u00b7men", ",", "Se\u00b7gen", ",", "Th\u00fc\u00b7ren", "weit", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Orgelton und letzter Psalter.", "tokens": ["Or\u00b7gel\u00b7ton", "und", "letz\u00b7ter", "Psal\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Durch die Sommerherrlichkeit", "tokens": ["Durch", "die", "Som\u00b7mer\u00b7herr\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Schwirren Schwalben, flattern Falter.", "tokens": ["Schwir\u00b7ren", "Schwal\u00b7ben", ",", "flat\u00b7tern", "Fal\u00b7ter", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}