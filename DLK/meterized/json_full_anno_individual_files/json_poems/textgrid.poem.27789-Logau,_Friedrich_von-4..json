{"textgrid.poem.27789": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "4.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gl\u00fcck zu, du \u00f6des Feld! Gl\u00fcck zu, ihr w\u00fcsten Auen!", "tokens": ["Gl\u00fcck", "zu", ",", "du", "\u00f6\u00b7des", "Feld", "!", "Gl\u00fcck", "zu", ",", "ihr", "w\u00fcs\u00b7ten", "Au\u00b7en", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "PPER", "ADJA", "NN", "$.", "NN", "PTKVZ", "$,", "PPER", "VVFIN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die ich, wann ich euch seh, mit Threnen mu\u00df betauen,", "tokens": ["Die", "ich", ",", "wann", "ich", "euch", "seh", ",", "mit", "Thre\u00b7nen", "mu\u00df", "be\u00b7tau\u00b7en", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "$,", "PWAV", "PPER", "PPER", "VVFIN", "$,", "APPR", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Weil ihr nicht mehr seyd ihr; so gar hat euren Stand", "tokens": ["Weil", "ihr", "nicht", "mehr", "seyd", "ihr", ";", "so", "gar", "hat", "eu\u00b7ren", "Stand"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PTKNEG", "ADV", "VAFIN", "PPER", "$.", "ADV", "ADV", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der freche Mord-Gott Mars grund au\u00df herum gewand.", "tokens": ["Der", "fre\u00b7che", "Mord\u00b7Gott", "Mars", "grund", "au\u00df", "he\u00b7rum", "ge\u00b7wand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NE", "NN", "APPR", "APZR", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Seyd aber doch gegr\u00fcst, seyd dennoch f\u00fcrgesetzet", "tokens": ["Seyd", "a\u00b7ber", "doch", "ge\u00b7gr\u00fcst", ",", "seyd", "den\u00b7noch", "f\u00fcr\u00b7ge\u00b7set\u00b7zet"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAIMP", "ADV", "ADV", "VVPP", "$,", "VAFIN", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Dem allem, was die Stat f\u00fcr sch\u00f6n und k\u00f6stlich sch\u00e4tzet.", "tokens": ["Dem", "al\u00b7lem", ",", "was", "die", "Stat", "f\u00fcr", "sch\u00f6n", "und", "k\u00f6st\u00b7lich", "sch\u00e4t\u00b7zet", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "$,", "PRELS", "ART", "NN", "APPR", "ADJD", "KON", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ihr wart mir lieb; ihr seyd, ihr bleibt mir lieb und werth;", "tokens": ["Ihr", "wart", "mir", "lieb", ";", "ihr", "seyd", ",", "ihr", "bleibt", "mir", "lieb", "und", "werth", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "$.", "PPER", "VAFIN", "$,", "PPER", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ich bin, ob ihr verkehrt, noch dennoch nicht verkehrt.", "tokens": ["Ich", "bin", ",", "ob", "ihr", "ver\u00b7kehrt", ",", "noch", "den\u00b7noch", "nicht", "ver\u00b7kehrt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "KOUS", "PPER", "VVPP", "$,", "ADV", "ADV", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ich bin, der ich war vor. Ob ihr seyd sehr vernichtet,", "tokens": ["Ich", "bin", ",", "der", "ich", "war", "vor", ".", "Ob", "ihr", "seyd", "sehr", "ver\u00b7nich\u00b7tet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "PRELS", "PPER", "VAFIN", "PTKVZ", "$.", "KOUS", "PPER", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "So bleib ich dennoch euch zu voller Gunst verpflichtet,", "tokens": ["So", "bleib", "ich", "den\u00b7noch", "euch", "zu", "vol\u00b7ler", "Gunst", "ver\u00b7pflich\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PPER", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "So lang ich ich kan seyn. Wann dann mein seyn vergeht,", "tokens": ["So", "lang", "ich", "ich", "kan", "seyn", ".", "Wann", "dann", "mein", "seyn", "ver\u00b7geht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "PPER", "VMFIN", "VAINF", "$.", "PWAV", "ADV", "PPOSAT", "VAINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Kans sein, da\u00df Musa wo an meiner Stelle steht.", "tokens": ["Kans", "sein", ",", "da\u00df", "Mu\u00b7sa", "wo", "an", "mei\u00b7ner", "Stel\u00b7le", "steht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VAINF", "$,", "KOUS", "NE", "PWAV", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Gehab dich wol, o Stadt! die du in deinen Zinnen", "tokens": ["Ge\u00b7hab", "dich", "wol", ",", "o", "Stadt", "!", "die", "du", "in", "dei\u00b7nen", "Zin\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "ADV", "$,", "FM", "NN", "$.", "PRELS", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Hast meinen Leib gehabt, nicht aber meine Sinnen.", "tokens": ["Hast", "mei\u00b7nen", "Leib", "ge\u00b7habt", ",", "nicht", "a\u00b7ber", "mei\u00b7ne", "Sin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "VAPP", "$,", "PTKNEG", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Gehab dich wol! mein Leib ist nun vom Kerker los;", "tokens": ["Ge\u00b7hab", "dich", "wol", "!", "mein", "Leib", "ist", "nun", "vom", "Ker\u00b7ker", "los", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "$.", "PPOSAT", "NN", "VAFIN", "ADV", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Ich darff nun nicht mehr seyn, wo mich zu seyn verdro\u00df.", "tokens": ["Ich", "darff", "nun", "nicht", "mehr", "seyn", ",", "wo", "mich", "zu", "seyn", "ver\u00b7dro\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PTKNEG", "ADV", "VAINF", "$,", "PWAV", "PPER", "PTKZU", "VAINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Ich habe dich, du mich, du s\u00fcsse Vater-Erde!", "tokens": ["Ich", "ha\u00b7be", "dich", ",", "du", "mich", ",", "du", "s\u00fcs\u00b7se", "Va\u00b7ter\u00b7Er\u00b7de", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "$,", "PPER", "PRF", "$,", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Mein Feuer gl\u00e4ntzt nunmehr auff meinem eignen Herde.", "tokens": ["Mein", "Feu\u00b7er", "gl\u00e4ntzt", "nun\u00b7mehr", "auff", "mei\u00b7nem", "eig\u00b7nen", "Her\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Ich geh, ich steh, ich sitz, ich schlaf, ich wach umbsonst;", "tokens": ["Ich", "geh", ",", "ich", "steh", ",", "ich", "sitz", ",", "ich", "schlaf", ",", "ich", "wach", "um\u00b7bsonst", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "$,", "PPER", "NE", "$,", "PPER", "VVFIN", "$,", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Was theuer mir dort war, das hab ich hier aus Gunst", "tokens": ["Was", "theu\u00b7er", "mir", "dort", "war", ",", "das", "hab", "ich", "hier", "aus", "Gunst"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "ADJD", "PPER", "ADV", "VAFIN", "$,", "PDS", "VAFIN", "PPER", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Des Herren der Natur um Habe-Dank zu nissen", "tokens": ["Des", "Her\u00b7ren", "der", "Na\u00b7tur", "um", "Ha\u00b7be\u00b7Dank", "zu", "nis\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "APPR", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Und um gesunden Schweiss; darff nichts hingegen wissen", "tokens": ["Und", "um", "ge\u00b7sun\u00b7den", "Schweiss", ";", "darff", "nichts", "hin\u00b7ge\u00b7gen", "wis\u00b7sen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN", "$.", "VMFIN", "PIS", "ADV", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Von Vortel und Betrug, von Hinterlist und Neid,", "tokens": ["Von", "Vor\u00b7tel", "und", "Be\u00b7trug", ",", "von", "Hin\u00b7ter\u00b7list", "und", "Neid", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Und wo man sonst sich durch schickt etwan in die Zeit.", "tokens": ["Und", "wo", "man", "sonst", "sich", "durch", "schickt", "et\u00b7wan", "in", "die", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PIS", "ADV", "PRF", "APPR", "VVFIN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+--+-+--+", "measure": "iambic.penta.relaxed"}, "line.25": {"text": "Ich ess' ein selig Brot, mit Schwei\u00df zwar eingeteiget,", "tokens": ["Ich", "ess'", "ein", "se\u00b7lig", "Brot", ",", "mit", "Schwei\u00df", "zwar", "ein\u00b7ge\u00b7tei\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJD", "NN", "$,", "APPR", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Doch da\u00df durch Beckers Kunst und Hefen hoch nicht steiget,", "tokens": ["Doch", "da\u00df", "durch", "Be\u00b7ckers", "Kunst", "und", "He\u00b7fen", "hoch", "nicht", "stei\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "APPR", "NE", "NN", "KON", "NN", "ADJD", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Das zwar Gesichte nicht, den Magen aber f\u00fcllt", "tokens": ["Das", "zwar", "Ge\u00b7sich\u00b7te", "nicht", ",", "den", "Ma\u00b7gen", "a\u00b7ber", "f\u00fcllt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "ADV", "NN", "PTKNEG", "$,", "ART", "NN", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Und dient mehr, da\u00df es n\u00e4hrt, als da\u00df es Heller gilt.", "tokens": ["Und", "dient", "mehr", ",", "da\u00df", "es", "n\u00e4hrt", ",", "als", "da\u00df", "es", "Hel\u00b7ler", "gilt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "$,", "KOUS", "PPER", "VVFIN", "$,", "KOKOM", "KOUS", "PPER", "NE", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Mein Trinken ist nicht falsch; ich darf mir nicht gedenken,", "tokens": ["Mein", "Trin\u00b7ken", "ist", "nicht", "falsch", ";", "ich", "darf", "mir", "nicht", "ge\u00b7den\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PTKNEG", "ADJD", "$.", "PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Es sei gebrauen zwier, vom Br\u00e4uer und vom Schenken.", "tokens": ["Es", "sei", "ge\u00b7brau\u00b7en", "zwier", ",", "vom", "Br\u00e4u\u00b7er", "und", "vom", "Schen\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJA", "NN", "$,", "APPRART", "NN", "KON", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Mir schmeckt der klare Safft; mir schmeckt das reine Na\u00df,", "tokens": ["Mir", "schmeckt", "der", "kla\u00b7re", "Safft", ";", "mir", "schmeckt", "das", "rei\u00b7ne", "Na\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$.", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Das ohne Keller frisch, das gut bleibt ohne Va\u00df,", "tokens": ["Das", "oh\u00b7ne", "Kel\u00b7ler", "frisch", ",", "das", "gut", "bleibt", "oh\u00b7ne", "Va\u00df", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "NN", "ADJD", "$,", "PRELS", "ADJD", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Drum nicht die Nymphen erst mit Ceres d\u00fcrffen k\u00e4mpffen,", "tokens": ["Drum", "nicht", "die", "Nym\u00b7phen", "erst", "mit", "Ce\u00b7res", "d\u00fcrf\u00b7fen", "k\u00e4mpf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PTKNEG", "ART", "NN", "ADV", "APPR", "NE", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Wer Meister dr\u00fcber sei, das nichts bedarff zum d\u00e4mpffen,", "tokens": ["Wer", "Meis\u00b7ter", "dr\u00fc\u00b7ber", "sei", ",", "das", "nichts", "be\u00b7darff", "zum", "d\u00e4mpf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "PAV", "VAFIN", "$,", "PRELS", "PIS", "ADJD", "APPRART", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Weils keinen Schweffelrauch noch sonsten Einschlag hat,", "tokens": ["Weils", "kei\u00b7nen", "Schwef\u00b7fel\u00b7rauch", "noch", "sons\u00b7ten", "Ein\u00b7schlag", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "ADV", "ADJA", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Das ohne Geld steht feil, das keine frevle That", "tokens": ["Das", "oh\u00b7ne", "Geld", "steht", "feil", ",", "das", "kei\u00b7ne", "frev\u00b7le", "That"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "APPR", "NN", "VVFIN", "PTKVZ", "$,", "PRELS", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Hat den iemals gelehrt, der dran ihm lie\u00df gen\u00fcgen.", "tokens": ["Hat", "den", "ie\u00b7mals", "ge\u00b7lehrt", ",", "der", "dran", "ihm", "lie\u00df", "ge\u00b7n\u00fc\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADV", "VVPP", "$,", "PRELS", "PAV", "PPER", "VVFIN", "VVINF", "$."], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}, "line.38": {"text": "Der Kr\u00e4mer fruchtbar Schwur und ihr geni\u00dflich L\u00fcgen", "tokens": ["Der", "Kr\u00e4\u00b7mer", "frucht\u00b7bar", "Schwur", "und", "ihr", "ge\u00b7ni\u00df\u00b7lich", "L\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "VVFIN", "KON", "PPER", "ADJD", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Hat nimmer Ernt' um mich; der viel-geplagte Lein", "tokens": ["Hat", "nim\u00b7mer", "Ernt'", "um", "mich", ";", "der", "viel\u00b7ge\u00b7plag\u00b7te", "Lein"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "ADV", "NN", "APPR", "PPER", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Der mu\u00df, der kan mir auch anstat der Seiden seyn.", "tokens": ["Der", "mu\u00df", ",", "der", "kan", "mir", "auch", "an\u00b7stat", "der", "Sei\u00b7den", "seyn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "$,", "PRELS", "VMFIN", "PPER", "ADV", "APPR", "ART", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Bewegung ist mein Artzt. Die kr\u00e4uterreichen W\u00e4lde", "tokens": ["Be\u00b7we\u00b7gung", "ist", "mein", "Artzt", ".", "Die", "kr\u00e4u\u00b7ter\u00b7rei\u00b7chen", "W\u00e4l\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Sind Apotheks genug. Geld, Gold w\u00e4chst auch im Felde;", "tokens": ["Sind", "A\u00b7pot\u00b7heks", "ge\u00b7nug", ".", "Geld", ",", "Gold", "w\u00e4chst", "auch", "im", "Fel\u00b7de", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NE", "ADV", "$.", "NN", "$,", "NN", "VVFIN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Was mangelt alsdann mehr? Wer Gott zum Freunde hat", "tokens": ["Was", "man\u00b7gelt", "als\u00b7dann", "mehr", "?", "Wer", "Gott", "zum", "Freun\u00b7de", "hat"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "ADV", "ADV", "$.", "PWS", "NN", "APPRART", "NN", "VAFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Und hat ein eignes Feld, fragt wenig nach der Stat,", "tokens": ["Und", "hat", "ein", "eig\u00b7nes", "Feld", ",", "fragt", "we\u00b7nig", "nach", "der", "Stat", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "ADJA", "NN", "$,", "VVFIN", "PIS", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Der vortelhafften Stat, da Nahrung zu gewinnen,", "tokens": ["Der", "vor\u00b7tel\u00b7haff\u00b7ten", "Stat", ",", "da", "Nah\u00b7rung", "zu", "ge\u00b7win\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "KOUS", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "Fast ieder mu\u00df auff List, auff T\u00fcck', auff R\u00e4nke sinnen.", "tokens": ["Fast", "ie\u00b7der", "mu\u00df", "auff", "List", ",", "auff", "T\u00fcck", ",", "auff", "R\u00e4n\u00b7ke", "sin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VMFIN", "APPR", "NE", "$,", "APPR", "NN", "$,", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "Drum hab dich wol, o Stat! Wenn ich dich habe, Feld,", "tokens": ["Drum", "hab", "dich", "wol", ",", "o", "Stat", "!", "Wenn", "ich", "dich", "ha\u00b7be", ",", "Feld", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "ADV", "$,", "FM", "NN", "$.", "KOUS", "PPER", "PPER", "VAFIN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.48": {"text": "So hab ich Haus und Kost, Kleid, Ruh, Gesundheit, Geld.", "tokens": ["So", "hab", "ich", "Haus", "und", "Kost", ",", "Kleid", ",", "Ruh", ",", "Ge\u00b7sund\u00b7heit", ",", "Geld", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "KON", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Gl\u00fcck zu, du \u00f6des Feld! Gl\u00fcck zu, ihr w\u00fcsten Auen!", "tokens": ["Gl\u00fcck", "zu", ",", "du", "\u00f6\u00b7des", "Feld", "!", "Gl\u00fcck", "zu", ",", "ihr", "w\u00fcs\u00b7ten", "Au\u00b7en", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "PPER", "ADJA", "NN", "$.", "NN", "PTKVZ", "$,", "PPER", "VVFIN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die ich, wann ich euch seh, mit Threnen mu\u00df betauen,", "tokens": ["Die", "ich", ",", "wann", "ich", "euch", "seh", ",", "mit", "Thre\u00b7nen", "mu\u00df", "be\u00b7tau\u00b7en", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "$,", "PWAV", "PPER", "PPER", "VVFIN", "$,", "APPR", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Weil ihr nicht mehr seyd ihr; so gar hat euren Stand", "tokens": ["Weil", "ihr", "nicht", "mehr", "seyd", "ihr", ";", "so", "gar", "hat", "eu\u00b7ren", "Stand"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PTKNEG", "ADV", "VAFIN", "PPER", "$.", "ADV", "ADV", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der freche Mord-Gott Mars grund au\u00df herum gewand.", "tokens": ["Der", "fre\u00b7che", "Mord\u00b7Gott", "Mars", "grund", "au\u00df", "he\u00b7rum", "ge\u00b7wand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NE", "NN", "APPR", "APZR", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Seyd aber doch gegr\u00fcst, seyd dennoch f\u00fcrgesetzet", "tokens": ["Seyd", "a\u00b7ber", "doch", "ge\u00b7gr\u00fcst", ",", "seyd", "den\u00b7noch", "f\u00fcr\u00b7ge\u00b7set\u00b7zet"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAIMP", "ADV", "ADV", "VVPP", "$,", "VAFIN", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Dem allem, was die Stat f\u00fcr sch\u00f6n und k\u00f6stlich sch\u00e4tzet.", "tokens": ["Dem", "al\u00b7lem", ",", "was", "die", "Stat", "f\u00fcr", "sch\u00f6n", "und", "k\u00f6st\u00b7lich", "sch\u00e4t\u00b7zet", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "$,", "PRELS", "ART", "NN", "APPR", "ADJD", "KON", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ihr wart mir lieb; ihr seyd, ihr bleibt mir lieb und werth;", "tokens": ["Ihr", "wart", "mir", "lieb", ";", "ihr", "seyd", ",", "ihr", "bleibt", "mir", "lieb", "und", "werth", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "$.", "PPER", "VAFIN", "$,", "PPER", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ich bin, ob ihr verkehrt, noch dennoch nicht verkehrt.", "tokens": ["Ich", "bin", ",", "ob", "ihr", "ver\u00b7kehrt", ",", "noch", "den\u00b7noch", "nicht", "ver\u00b7kehrt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "KOUS", "PPER", "VVPP", "$,", "ADV", "ADV", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ich bin, der ich war vor. Ob ihr seyd sehr vernichtet,", "tokens": ["Ich", "bin", ",", "der", "ich", "war", "vor", ".", "Ob", "ihr", "seyd", "sehr", "ver\u00b7nich\u00b7tet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "PRELS", "PPER", "VAFIN", "PTKVZ", "$.", "KOUS", "PPER", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "So bleib ich dennoch euch zu voller Gunst verpflichtet,", "tokens": ["So", "bleib", "ich", "den\u00b7noch", "euch", "zu", "vol\u00b7ler", "Gunst", "ver\u00b7pflich\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PPER", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "So lang ich ich kan seyn. Wann dann mein seyn vergeht,", "tokens": ["So", "lang", "ich", "ich", "kan", "seyn", ".", "Wann", "dann", "mein", "seyn", "ver\u00b7geht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "PPER", "VMFIN", "VAINF", "$.", "PWAV", "ADV", "PPOSAT", "VAINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Kans sein, da\u00df Musa wo an meiner Stelle steht.", "tokens": ["Kans", "sein", ",", "da\u00df", "Mu\u00b7sa", "wo", "an", "mei\u00b7ner", "Stel\u00b7le", "steht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VAINF", "$,", "KOUS", "NE", "PWAV", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Gehab dich wol, o Stadt! die du in deinen Zinnen", "tokens": ["Ge\u00b7hab", "dich", "wol", ",", "o", "Stadt", "!", "die", "du", "in", "dei\u00b7nen", "Zin\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "ADV", "$,", "FM", "NN", "$.", "PRELS", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Hast meinen Leib gehabt, nicht aber meine Sinnen.", "tokens": ["Hast", "mei\u00b7nen", "Leib", "ge\u00b7habt", ",", "nicht", "a\u00b7ber", "mei\u00b7ne", "Sin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "VAPP", "$,", "PTKNEG", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Gehab dich wol! mein Leib ist nun vom Kerker los;", "tokens": ["Ge\u00b7hab", "dich", "wol", "!", "mein", "Leib", "ist", "nun", "vom", "Ker\u00b7ker", "los", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "$.", "PPOSAT", "NN", "VAFIN", "ADV", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Ich