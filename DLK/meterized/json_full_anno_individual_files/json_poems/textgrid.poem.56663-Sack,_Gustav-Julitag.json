{"textgrid.poem.56663": {"metadata": {"author": {"name": "Sack, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Julitag", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Heut aber lief der Tag sich wild!", "tokens": ["Heut", "a\u00b7ber", "lief", "der", "Tag", "sich", "wild", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "ART", "NN", "PRF", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was hat er nur gejagt, da\u00df ihm", "tokens": ["Was", "hat", "er", "nur", "ge\u00b7jagt", ",", "da\u00df", "ihm"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWS", "VAFIN", "PPER", "ADV", "VVPP", "$,", "KOUS", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "gleich einem Hunde, dem die Zunge rot schlottern?", "tokens": ["gleich", "ei\u00b7nem", "Hun\u00b7de", ",", "dem", "die", "Zun\u00b7ge", "rot", "schlot\u00b7tern", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "PRELS", "ART", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "und lechzend aus dem Halse h\u00e4ngt,", "tokens": ["und", "lech\u00b7zend", "aus", "dem", "Hal\u00b7se", "h\u00e4ngt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "die Bl\u00e4tter welk und d\u00fcrstend an den B\u00e4umen", "tokens": ["die", "Bl\u00e4t\u00b7ter", "welk", "und", "d\u00fcrs\u00b7tend", "an", "den", "B\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "A