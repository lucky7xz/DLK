{"textgrid.poem.53294": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "Abend-Lied", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Christe, Schutz-Herr deiner Glieder,", "tokens": ["O", "Chris\u00b7te", ",", "Schutz\u00b7Herr", "dei\u00b7ner", "Glie\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Du Arbeits-Trost, Du Gott der Ruh!", "tokens": ["Du", "Ar\u00b7beits\u00b7Trost", ",", "Du", "Gott", "der", "Ruh", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "PPER", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du schickest durch die Nacht vns wieder", "tokens": ["Du", "schi\u00b7ckest", "durch", "die", "Nacht", "vns", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "PPER", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den Schlaff, der Sorgen Anstand, zu,", "tokens": ["Den", "Schlaff", ",", "der", "Sor\u00b7gen", "An\u00b7stand", ",", "zu", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "NN", "$,", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Hilffst mit newen Kr\u00e4fften", "tokens": ["Hilffst", "mit", "ne\u00b7wen", "Kr\u00e4ff\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Vnsern Ampts-gesch\u00e4fften", "tokens": ["Vn\u00b7sern", "Ampt\u00b7sge\u00b7sch\u00e4ff\u00b7ten"], "token_info": ["word", "word"], "pos": ["KON", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Folgends auff den Tag,", "tokens": ["Fol\u00b7gends", "auff", "den", "Tag", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.8": {"text": "Stehest zu verh\u00fcten,", "tokens": ["Ste\u00b7hest", "zu", "ver\u00b7h\u00fc\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.9": {"text": "Da\u00df kein Fall noch w\u00fcten", "tokens": ["Da\u00df", "kein", "Fall", "noch", "w\u00fc\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PIAT", "NN", "ADV", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.10": {"text": "Vns betreten mag.", "tokens": ["Vns", "be\u00b7tre\u00b7ten", "mag."], "token_info": ["word", "word", "abbreviation"], "pos": ["PPER", "VVFIN", "NE"], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.2": {"line.1": {"text": "Erkenne, was wir Dir von wegen", "tokens": ["Er\u00b7ken\u00b7ne", ",", "was", "wir", "Dir", "von", "we\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "PRELS", "PPER", "PPER", "APPR", "APPR"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der diesen Tag erzeigten Hut", "tokens": ["Der", "die\u00b7sen", "Tag", "er\u00b7zeig\u00b7ten", "Hut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PDAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "F\u00fcr Ehre, Lob vnd Danck ablegen,", "tokens": ["F\u00fcr", "Eh\u00b7re", ",", "Lob", "vnd", "Danck", "ab\u00b7le\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Was bey vns Mund vnd Seele thut!", "tokens": ["Was", "bey", "vns", "Mund", "vnd", "See\u00b7le", "thut", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "PPER", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wir gestehn vnd sagen:", "tokens": ["Wir", "ge\u00b7stehn", "vnd", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVPP", "KON", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Da\u00df Du vns von Plagen", "tokens": ["Da\u00df", "Du", "vns", "von", "Pla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "APPR", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Gn\u00e4diglich befreyht,", "tokens": ["Gn\u00e4\u00b7di\u00b7glich", "be\u00b7freyht", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVPP", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.8": {"text": "Da\u00df kein Grimm der Hellen", "tokens": ["Da\u00df", "kein", "Grimm", "der", "Hel\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PIAT", "NE", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.9": {"text": "Vns hat k\u00f6nnen f\u00e4llen,", "tokens": ["Vns", "hat", "k\u00f6n\u00b7nen", "f\u00e4l\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.10": {"text": "Macht, Herr, dein Geleit.", "tokens": ["Macht", ",", "Herr", ",", "dein", "Ge\u00b7leit", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Vergieb die S\u00fcnd' vnd schn\u00f6de Sachen,", "tokens": ["Ver\u00b7gieb", "die", "S\u00fcnd'", "vnd", "schn\u00f6\u00b7de", "Sa\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die heute wieder Dich geschehn,", "tokens": ["Die", "heu\u00b7te", "wie\u00b7der", "Dich", "ge\u00b7schehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "La\u00df deinen Sohn das richtig machen,", "tokens": ["La\u00df", "dei\u00b7nen", "Sohn", "das", "rich\u00b7tig", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "ART", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wa\u00df wir bald hie bald da versehn!", "tokens": ["Wa\u00df", "wir", "bald", "hie", "bald", "da", "ver\u00b7sehn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sch\u00fctz' auch, Herr, daneben", "tokens": ["Sch\u00fctz'", "auch", ",", "Herr", ",", "da\u00b7ne\u00b7ben"], "token_info": ["word", "word", "punct", "word", "punct", "word"], "pos": ["NN", "ADV", "$,", "NN", "$,", "PAV"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Vnser schwaches Leben", "tokens": ["Vn\u00b7ser", "schwa\u00b7ches", "Le\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Folgends diese Nacht!", "tokens": ["Fol\u00b7gends", "die\u00b7se", "Nacht", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PDAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.8": {"text": "Denn was hilfft vn\u00df Armen,", "tokens": ["Denn", "was", "hilfft", "vn\u00df", "Ar\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.9": {"text": "Wo nicht dein Erbarmen", "tokens": ["Wo", "nicht", "dein", "Er\u00b7bar\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "PTKNEG", "PPOSAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.10": {"text": "Ferner f\u00fcr vns wacht?", "tokens": ["Fer\u00b7ner", "f\u00fcr", "vns", "wacht", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Gib, da\u00df wir niemals ohne Glauben", "tokens": ["Gib", ",", "da\u00df", "wir", "nie\u00b7mals", "oh\u00b7ne", "Glau\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "$,", "KOUS", "PPER", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Hie anzutreffen m\u00f6gen sein!", "tokens": ["Hie", "an\u00b7zu\u00b7tref\u00b7fen", "m\u00f6\u00b7gen", "sein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "VMFIN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Nacht sol zwar das Licht vns rauben,", "tokens": ["Die", "Nacht", "sol", "zwar", "das", "Licht", "vns", "rau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ADV", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Doch nie des Geistes Krafft vnd Schein;", "tokens": ["Doch", "nie", "des", "Geis\u00b7tes", "Krafft", "vnd", "Schein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Lass vns vnsre Leuchten", "tokens": ["Lass", "vns", "vns\u00b7re", "Leuch\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "PPOSAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Stets mit Oele feuchten", "tokens": ["Stets", "mit", "O\u00b7e\u00b7le", "feuch\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "NN", "ADJA"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.7": {"text": "Vnd bereitet stehn,", "tokens": ["Vnd", "be\u00b7rei\u00b7tet", "stehn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "VVINF", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.8": {"text": "Da\u00df wir an dem Ende,", "tokens": ["Da\u00df", "wir", "an", "dem", "En\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.9": {"text": "Wann Du kompst, behende", "tokens": ["Wann", "Du", "kompst", ",", "be\u00b7hen\u00b7de"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["PWAV", "PPER", "VVFIN", "$,", "ADJA"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.10": {"text": "Mit Dir k\u00f6nnen gehn!", "tokens": ["Mit", "Dir", "k\u00f6n\u00b7nen", "gehn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VMFIN", "VVINF", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "O Christe, Schutz-Herr deiner Glieder,", "tokens": ["O", "Chris\u00b7te", ",", "Schutz\u00b7Herr", "dei\u00b7ner", "Glie\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct