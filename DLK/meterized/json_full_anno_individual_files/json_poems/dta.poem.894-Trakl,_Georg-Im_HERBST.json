{"dta.poem.894": {"metadata": {"author": {"name": "Trakl, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Im HERBST", "genre": "Lyrik", "period": "N.A.", "pub_year": "1913", "urn": "urn:nbn:de:kobv:b4-30357-9", "language": ["de:0.99"], "booktitle": "Trakl, Georg: Gedichte. Leipzig, 1913."}, "poem": {"stanza.1": {"line.1": {"text": "Die Sonnenblumen leuchten am Zaun,", "tokens": ["Die", "Son\u00b7nen\u00b7blu\u00b7men", "leuch\u00b7ten", "am", "Zaun", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Still sitzen Kranke im Sonnenschein.", "tokens": ["Still", "sit\u00b7zen", "Kran\u00b7ke", "im", "Son\u00b7nen\u00b7schein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Im Acker m\u00fchn sich singend die Frau\u2019n,", "tokens": ["Im", "A\u00b7cker", "m\u00fchn", "sich", "sin\u00b7gend", "die", "Frau'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VMFIN", "PRF", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Die Klosterglocken l\u00e4uten darein.", "tokens": ["Die", "Klos\u00b7ter\u00b7glo\u00b7cken", "l\u00e4u\u00b7ten", "da\u00b7rein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PAV", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.2": {"line.1": {"text": "Die V\u00f6gel sagen dir ferne M\u00e4r\u2019,", "tokens": ["Die", "V\u00f6\u00b7gel", "sa\u00b7gen", "dir", "fer\u00b7ne", "M\u00e4r'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die Klosterglocken l\u00e4uten darein.", "tokens": ["Die", "Klos\u00b7ter\u00b7glo\u00b7cken", "l\u00e4u\u00b7ten", "da\u00b7rein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PAV", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Vom Hof t\u00f6nt sanft die Geige her.", "tokens": ["Vom", "Hof", "t\u00f6nt", "sanft", "die", "Gei\u00b7ge", "her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ADJD", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Heut keltern sie den braunen Wein.", "tokens": ["Heut", "kel\u00b7tern", "sie", "den", "brau\u00b7nen", "Wein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Da zeigt der Mensch sich froh und lind.", "tokens": ["Da", "zeigt", "der", "Mensch", "sich", "froh", "und", "lind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PRF", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Heut keltern sie den braunen Wein.", "tokens": ["Heut", "kel\u00b7tern", "sie", "den", "brau\u00b7nen", "Wein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Weit offen die Totenkammern sind", "tokens": ["Weit", "of\u00b7fen", "die", "To\u00b7ten\u00b7kam\u00b7mern", "sind"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "ADJD", "ART", "NN", "VAFIN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und sch\u00f6n bemalt vom Sonnenschein.", "tokens": ["Und", "sch\u00f6n", "be\u00b7malt", "vom", "Son\u00b7nen\u00b7schein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}