{"dta.poem.16017": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "71.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "O glaube nicht, da\u00df du nicht seiest mitgez\u00e4hlt;", "tokens": ["O", "glau\u00b7be", "nicht", ",", "da\u00df", "du", "nicht", "sei\u00b7est", "mit\u00b7ge\u00b7z\u00e4hlt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PTKNEG", "$,", "KOUS", "PPER", "PTKNEG", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Weltzahl ist nicht voll, wenn deine Ziffer fehlt.", "tokens": ["Die", "Welt\u00b7zahl", "ist", "nicht", "voll", ",", "wenn", "dei\u00b7ne", "Zif\u00b7fer", "fehlt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKNEG", "ADJD", "$,", "KOUS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die gro\u00dfe Rechnung zwar ist ohne dich gemacht,", "tokens": ["Die", "gro\u00b7\u00dfe", "Rech\u00b7nung", "zwar", "ist", "oh\u00b7ne", "dich", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VAFIN", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Allein du selber bist in Rechnung mit gebracht.", "tokens": ["Al\u00b7lein", "du", "sel\u00b7ber", "bist", "in", "Rech\u00b7nung", "mit", "ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ADV", "VAFIN", "APPR", "NN", "APPR", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ja mitgerechnet ist auf dich in alle Weise;", "tokens": ["Ja", "mit\u00b7ge\u00b7rech\u00b7net", "ist", "auf", "dich", "in", "al\u00b7le", "Wei\u00b7se", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "VVPP", "VAFIN", "APPR", "PRF", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dein kleiner Ring greift ein in jene gr\u00f6\u00dfern Kreise.", "tokens": ["Dein", "klei\u00b7ner", "Ring", "greift", "ein", "in", "je\u00b7ne", "gr\u00f6\u00b7\u00dfern", "Krei\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "ART", "APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Zum Guten Sch\u00f6nen will vom Mangelhaften B\u00f6sen", "tokens": ["Zum", "Gu\u00b7ten", "Sch\u00f6\u00b7nen", "will", "vom", "Man\u00b7gel\u00b7haf\u00b7ten", "B\u00f6\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "VMFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Welt erl\u00f6st seyn, und du sollst sie miterl\u00f6sen.", "tokens": ["Die", "Welt", "er\u00b7l\u00f6st", "seyn", ",", "und", "du", "sollst", "sie", "mi\u00b7ter\u00b7l\u00f6\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "VAINF", "$,", "KON", "PPER", "VMFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Vom B\u00f6sen mache dich, vom Mangelhaften frei;", "tokens": ["Vom", "B\u00f6\u00b7sen", "ma\u00b7che", "dich", ",", "vom", "Man\u00b7gel\u00b7haf\u00b7ten", "frei", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "$,", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zur G\u00fct' und Sch\u00f6ne so der Welten tr\u00e4gst du bei.", "tokens": ["Zur", "G\u00fct'", "und", "Sch\u00f6\u00b7ne", "so", "der", "Wel\u00b7ten", "tr\u00e4gst", "du", "bei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "NN", "ADV", "ART", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}