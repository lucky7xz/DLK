{"dta.poem.13288": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Z w\u00f6lfftes  B uch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201enun habt ihr\u201e, war sein Wort, in diesem Saal erfahren,", "tokens": ["\u201e", "nun", "habt", "ihr", "\u201e", ",", "war", "sein", "Wort", ",", "in", "die\u00b7sem", "Saal", "er\u00b7fah\u00b7ren", ","], "token_info": ["punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VAFIN", "PPER", "$(", "$,", "VAFIN", "PPOSAT", "NN", "$,", "APPR", "PDAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "\u201ewas ein Geheimnis war. Ich k\u00f6nnt euch offenbaren", "tokens": ["\u201e", "was", "ein", "Ge\u00b7heim\u00b7nis", "war", ".", "Ich", "k\u00f6nnt", "euch", "of\u00b7fen\u00b7ba\u00b7ren"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "PWS", "ART", "NN", "VAFIN", "$.", "PPER", "VVFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "\u201ewas noch verborgen ist. Der Saz bestrickte mich", "tokens": ["\u201e", "was", "noch", "ver\u00b7bor\u00b7gen", "ist", ".", "Der", "Saz", "be\u00b7strick\u00b7te", "mich"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "PWS", "ADV", "VVPP", "VAFIN", "$.", "ART", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So mehr, als dieser Mann der G\u00fcte selber glich.", "tokens": ["So", "mehr", ",", "als", "die\u00b7ser", "Mann", "der", "G\u00fc\u00b7te", "sel\u00b7ber", "glich", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "KOUS", "PDAT", "NN", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "55Sein holdes Angesicht und liebreichs Augen-wenden", "tokens": ["hol\u00b7des", "An\u00b7ge\u00b7sicht", "und", "lieb\u00b7reichs", "Au\u00b7gen\u00b7wen\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "KON", "ADJA", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.6": {"text": "Erweckte mein Gem\u00fcth: ich nahm ihn bey den H\u00e4nden,", "tokens": ["Er\u00b7weck\u00b7te", "mein", "Ge\u00b7m\u00fcth", ":", "ich", "nahm", "ihn", "bey", "den", "H\u00e4n\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$.", "PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ich dr\u00fcckt, ich k\u00fc\u00dfte sie, und dachte: wer er sey,", "tokens": ["Ich", "dr\u00fcckt", ",", "ich", "k\u00fc\u00df\u00b7te", "sie", ",", "und", "dach\u00b7te", ":", "wer", "er", "sey", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "PPER", "$,", "KON", "VVFIN", "$.", "PWS", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Er scheint die Anmuth selbst zu seyn, ich frag\u2019 ihn frey;", "tokens": ["Er", "scheint", "die", "An\u00b7muth", "selbst", "zu", "seyn", ",", "ich", "frag'", "ihn", "frey", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "PTKZU", "VAINF", "$,", "PPER", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "So bat\u2019 ich ihn, da\u00df er mir die geheimen Sachen,", "tokens": ["So", "bat'", "ich", "ihn", ",", "da\u00df", "er", "mir", "die", "ge\u00b7hei\u00b7men", "Sa\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "$,", "KOUS", "PPER", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "60Von welchen er erst sprach, zu wissen wolle machen.", "tokens": ["wel\u00b7chen", "er", "erst", "sprach", ",", "zu", "wis\u00b7sen", "wol\u00b7le", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "PPER", "ADV", "VVFIN", "$,", "PTKZU", "VVINF", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.11": {"text": "Da fuhr er l\u00e4chelnd fort: \u201eSieh diese Mahlerey!", "tokens": ["Da", "fuhr", "er", "l\u00e4\u00b7chelnd", "fort", ":", "\u201e", "Sieh", "die\u00b7se", "Mah\u00b7le\u00b7rey", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "PTKVZ", "$.", "$(", "NE", "PDAT", "NN", "$."], "meter": "-+-+-+---+-+", "measure": "unknown.measure.penta"}, "line.12": {"text": "(er zeigte nach der Wand) urtheile, was es sey!", "tokens": ["(", "er", "zeig\u00b7te", "nach", "der", "Wand", ")", "ur\u00b7thei\u00b7le", ",", "was", "es", "sey", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "APPR", "ART", "NN", "$(", "VVFIN", "$,", "PWS", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}