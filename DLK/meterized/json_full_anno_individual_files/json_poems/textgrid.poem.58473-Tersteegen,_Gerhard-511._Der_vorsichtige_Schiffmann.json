{"textgrid.poem.58473": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "511. Der vorsichtige Schiffmann", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gott schaut in mich hinein,", "tokens": ["Gott", "schaut", "in", "mich", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Und ich auf ihn allein;", "tokens": ["Und", "ich", "auf", "ihn", "al\u00b7lein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mein Ruder und Kompa\u00df", "tokens": ["Mein", "Ru\u00b7der", "und", "Kom\u00b7pa\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ist sonsten nichts als das.", "tokens": ["Ist", "sons\u00b7ten", "nichts", "als", "das", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PIS", "KOKOM", "PDS", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ach, wenn es so nur stehet,", "tokens": ["Ach", ",", "wenn", "es", "so", "nur", "ste\u00b7het", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "KOUS", "PPER", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Mein Schifflein sicher gehet;", "tokens": ["Mein", "Schif\u00b7flein", "si\u00b7cher", "ge\u00b7het", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Es st\u00fcrme Wind und Wellen,", "tokens": ["Es", "st\u00fcr\u00b7me", "Wind", "und", "Wel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Sie werden mich nicht f\u00e4llen.", "tokens": ["Sie", "wer\u00b7den", "mich", "nicht", "f\u00e4l\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Kleinglaub' und Untreu macht,", "tokens": ["Klein\u00b7glaub'", "und", "Un\u00b7treu", "macht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Da\u00df ich oft nicht drauf acht'", "tokens": ["Da\u00df", "ich", "oft", "nicht", "drauf", "acht'"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "PTKNEG", "PAV", "XY"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Und auf mich selber seh;", "tokens": ["Und", "auf", "mich", "sel\u00b7ber", "seh", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "Dann hei\u00dft es: Ich vergeh!", "tokens": ["Dann", "hei\u00dft", "es", ":", "Ich", "ver\u00b7geh", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$.", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "Doch Jesus, der mich liebet,", "tokens": ["Doch", "Je\u00b7sus", ",", "der", "mich", "lie\u00b7bet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NE", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "mea