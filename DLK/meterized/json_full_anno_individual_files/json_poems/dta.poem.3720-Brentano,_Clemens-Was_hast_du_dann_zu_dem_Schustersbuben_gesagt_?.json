{"dta.poem.3720": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Was hast du dann zu dem Schustersbuben  \n gesagt ?", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519172", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Schusterbue!               ", "tokens": ["Schus\u00b7ter\u00b7bue", "!"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Flick mir die Schuh,", "tokens": ["Flick", "mir", "die", "Schuh", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Gieb mirs Leder auch dazu,", "tokens": ["Gieb", "mirs", "Le\u00b7der", "auch", "da\u00b7zu", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADJA", "NN", "ADV", "PAV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Es ist kein Gerber in der Stadt,", "tokens": ["Es", "ist", "kein", "Ger\u00b7ber", "in", "der", "Stadt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der ein solches Leder hat.", "tokens": ["Der", "ein", "sol\u00b7ches", "Le\u00b7der", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "PIAT", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ein lustiger Bu", "tokens": ["Ein", "lus\u00b7ti\u00b7ger", "Bu"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Braucht oft ein Paar Schuh,", "tokens": ["Braucht", "oft", "ein", "Paar", "Schuh", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "NN", "$,"], "meter": "-+-++", "measure": "unknown.measure.tri"}, "line.3": {"text": "Ein trauriger Narr,", "tokens": ["Ein", "trau\u00b7ri\u00b7ger", "Narr", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Hat lang an eim Paar.", "tokens": ["Hat", "lang", "an", "eim", "Paar", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}}}}