{"dta.poem.6960": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von der Vereinigung und dem Unter-  \n scheid der Seelen und des C\u00f6rpers.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.85", "af:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Dies ist die Seele nicht, die man vern\u00fcnfftig heisst,", "tokens": ["Dies", "ist", "die", "See\u00b7le", "nicht", ",", "die", "man", "ver\u00b7n\u00fcnff\u00b7tig", "heisst", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "PTKNEG", "$,", "PRELS", "PIS", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein unverg\u00e4nglich Seyn, ein Geist.", "tokens": ["Ein", "un\u00b7ver\u00b7g\u00e4ng\u00b7lich", "Seyn", ",", "ein", "Geist", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJD", "PPOSAT", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn er untheilbar ist, wenn er unc\u00f6rperlich;", "tokens": ["Wenn", "er", "un\u00b7theil\u00b7bar", "ist", ",", "wenn", "er", "un\u00b7c\u00f6r\u00b7per\u00b7lich", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VAFIN", "$,", "KOUS", "PPER", "ADJD", "$."], "meter": "+-+-+-+-+---", "measure": "unknown.measure.penta"}, "line.4": {"text": "Ist er ohn Aendrung und unzerst\u00f6hrlich,", "tokens": ["Ist", "er", "ohn", "A\u00b7en\u00b7drung", "und", "un\u00b7zer\u00b7st\u00f6hr\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "NN", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und folglich w\u00e4hrt er unaufh\u00f6rlich,", "tokens": ["Und", "folg\u00b7lich", "w\u00e4hrt", "er", "un\u00b7auf\u00b7h\u00f6r\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wie die unendliche, selbst-st\u00e4ndig\u2019 ew\u2019ge Macht,", "tokens": ["Wie", "die", "un\u00b7end\u00b7li\u00b7che", ",", "selbst\u00b7st\u00e4n\u00b7dig'", "ew'\u00b7ge", "Macht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "$,", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die ihn hervorgebracht.", "tokens": ["Die", "ihn", "her\u00b7vor\u00b7ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}