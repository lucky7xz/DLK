{"dta.poem.11903": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "64.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mag meine Seele, die im Wachen aufwerts steigt", "tokens": ["Mag", "mei\u00b7ne", "See\u00b7le", ",", "die", "im", "Wa\u00b7chen", "auf\u00b7werts", "steigt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPOSAT", "NN", "$,", "PRELS", "APPRART", "NN", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zum Himmel, und sich nie im Traum zur Erde neigt,", "tokens": ["Zum", "Him\u00b7mel", ",", "und", "sich", "nie", "im", "Traum", "zur", "Er\u00b7de", "neigt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "KON", "PRF", "ADV", "APPRART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Mag meine Seele rein ein Licht aus jenem Licht,", "tokens": ["Mag", "mei\u00b7ne", "See\u00b7le", "rein", "ein", "Licht", "aus", "je\u00b7nem", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "ADJD", "ART", "NN", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit ihm vereinigt seyn in froher Zuversicht!", "tokens": ["Mit", "ihm", "ver\u00b7ei\u00b7nigt", "seyn", "in", "fro\u00b7her", "Zu\u00b7ver\u00b7sicht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVPP", "VAINF", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Mag meine Seele, die des Leibes Opferschale", "tokens": ["Mag", "mei\u00b7ne", "See\u00b7le", ",", "die", "des", "Lei\u00b7bes", "Op\u00b7fer\u00b7scha\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VMFIN", "PPOSAT", "NN", "$,", "PRELS", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "F\u00fcllt, bis ergossen sie wird seyn zum Opfermale,", "tokens": ["F\u00fcllt", ",", "bis", "er\u00b7gos\u00b7sen", "sie", "wird", "seyn", "zum", "Op\u00b7fer\u00b7ma\u00b7le", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KON", "VVFIN", "PPER", "VAFIN", "PPOSAT", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Mag meine Seele rein aus jenem Thau ein Thauen,", "tokens": ["Mag", "mei\u00b7ne", "See\u00b7le", "rein", "aus", "je\u00b7nem", "Thau", "ein", "Thau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "ADJD", "APPR", "PDAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit ihm vereinigt seyn in Sehnsucht und Vertrauen!", "tokens": ["Mit", "ihm", "ver\u00b7ei\u00b7nigt", "seyn", "in", "Sehn\u00b7sucht", "und", "Ver\u00b7trau\u00b7en", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVPP", "VAINF", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Mag meine Seele, die das Spiel der Kr\u00e4fte treibt", "tokens": ["Mag", "mei\u00b7ne", "See\u00b7le", ",", "die", "das", "Spiel", "der", "Kr\u00e4f\u00b7te", "treibt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPOSAT", "NN", "$,", "PRELS", "ART", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Planeten gleich, und wie die Sonn' in Ruhe bleibt,", "tokens": ["Pla\u00b7ne\u00b7ten", "gleich", ",", "und", "wie", "die", "Sonn'", "in", "Ru\u00b7he", "bleibt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "KON", "PWAV", "ART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}}, "stanza.6": {"line.1": {"text": "Mag meine Seele rein ein Trieb von jenem Triebe,", "tokens": ["Mag", "mei\u00b7ne", "See\u00b7le", "rein", "ein", "Trieb", "von", "je\u00b7nem", "Trie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "ADJD", "ART", "NN", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit ihm vereinigt seyn in Seligkeit und Liebe!", "tokens": ["Mit", "ihm", "ver\u00b7ei\u00b7nigt", "seyn", "in", "Se\u00b7lig\u00b7keit", "und", "Lie\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVPP", "VAINF", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Mag meine Seele, die bewu\u00dftvoll h\u00e4lt umfangen,", "tokens": ["Mag", "mei\u00b7ne", "See\u00b7le", ",", "die", "be\u00b7wu\u00dft\u00b7voll", "h\u00e4lt", "um\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "$,", "PRELS", "ADJD", "VVFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was gegenw\u00e4rtig hie, was k\u00fcnftig und vergangen,", "tokens": ["Was", "ge\u00b7gen\u00b7w\u00e4r\u00b7tig", "hie", ",", "was", "k\u00fcnf\u00b7tig", "und", "ver\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "ADV", "$,", "PRELS", "ADJD", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Mag meine Seele rein, dem Ew'gen nicht zu rauben,", "tokens": ["Mag", "mei\u00b7ne", "See\u00b7le", "rein", ",", "dem", "Ew'\u00b7gen", "nicht", "zu", "rau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "ADJD", "$,", "ART", "NN", "PTKNEG", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit ihm vereinigt seyn in Ewigkeit, im Glauben!", "tokens": ["Mit", "ihm", "ver\u00b7ei\u00b7nigt", "seyn", "in", "E\u00b7wig\u00b7keit", ",", "im", "Glau\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVPP", "VAINF", "APPR", "NN", "$,", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Mag meine Seele, die sich wie mit Flammendochten", "tokens": ["Mag", "mei\u00b7ne", "See\u00b7le", ",", "die", "sich", "wie", "mit", "Flam\u00b7men\u00b7doch\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPOSAT", "NN", "$,", "PRELS", "PRF", "KOKOM", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit lichter Harmonie des Weltalls hat durchflochten,", "tokens": ["Mit", "lich\u00b7ter", "Har\u00b7mo\u00b7nie", "des", "Welt\u00b7alls", "hat", "durch\u00b7floch\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.10": {"line.1": {"text": "Mag meine Seele, rein durcht\u00f6nt vom Sch\u00f6pfungswort,", "tokens": ["Mag", "mei\u00b7ne", "See\u00b7le", ",", "rein", "durch\u00b7t\u00f6nt", "vom", "Sch\u00f6p\u00b7fungs\u00b7wort", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "$,", "ADJD", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit ihm vereinigt seyn in Andacht fort und fort!", "tokens": ["Mit", "ihm", "ver\u00b7ei\u00b7nigt", "seyn", "in", "An\u00b7dacht", "fort", "und", "fort", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVPP", "VAINF", "APPR", "NN", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}