{"textgrid.poem.56158": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "17.", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wo, in welchen immer selig bew\u00e4sserten Garten, an welchen", "tokens": ["Wo", ",", "in", "wel\u00b7chen", "im\u00b7mer", "se\u00b7lig", "be\u00b7w\u00e4s\u00b7ser\u00b7ten", "Gar\u00b7ten", ",", "an", "wel\u00b7chen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "$,", "APPR", "PWAT", "ADV", "ADJD", "ADJA", "NN", "$,", "APPR", "PWAT"], "meter": "+-+-+-+--+--+--+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "B\u00e4umen, aus welchen z\u00e4rtlich entbl\u00e4tterten Bl\u00fcten-Kelchen", "tokens": ["B\u00e4u\u00b7men", ",", "aus", "wel\u00b7chen", "z\u00e4rt\u00b7lich", "ent\u00b7bl\u00e4t\u00b7ter\u00b7ten", "Bl\u00fc\u00b7ten\u00b7Kel\u00b7chen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "APPR", "PWAT", "ADJD", "ADJA", "NN"], "meter": "+----+--+--+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "reifen die fremdartigen Fr\u00fcchte der Tr\u00f6stung? Diese", "tokens": ["rei\u00b7fen", "die", "fremd\u00b7ar\u00b7ti\u00b7gen", "Fr\u00fcch\u00b7te", "der", "Tr\u00f6s\u00b7tung", "?", "Die\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN", "ART", "NN", "$.", "NN"], "meter": "+---+--+--+-+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "k\u00f6stlichen, deren du eine vielleicht in der zertretenen Wiese", "tokens": ["k\u00f6st\u00b7li\u00b7chen", ",", "de\u00b7ren", "du", "ei\u00b7ne", "viel\u00b7leicht", "in", "der", "zer\u00b7tre\u00b7te\u00b7nen", "Wie\u00b7se"], "token_info": ["word", "punct", "word", "word", "word", "wo