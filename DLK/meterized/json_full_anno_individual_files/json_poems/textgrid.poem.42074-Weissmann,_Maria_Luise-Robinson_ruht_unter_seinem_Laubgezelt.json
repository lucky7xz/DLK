{"textgrid.poem.42074": {"metadata": {"author": {"name": "Weissmann, Maria Luise", "birth": "N.A.", "death": "N.A."}, "title": "Robinson ruht unter seinem Laubgezelt", "genre": "verse", "period": "N.A.", "pub_year": 1914, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Aber mit einem Male erstrahlen", "tokens": ["A\u00b7ber", "mit", "ei\u00b7nem", "Ma\u00b7le", "er\u00b7strah\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVINF"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Tage der N\u00e4he wie selige Segel,", "tokens": ["Ta\u00b7ge", "der", "N\u00e4\u00b7he", "wie", "se\u00b7li\u00b7ge", "Se\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "KOKOM", "ADJA", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.3": {"text": "Die auf dem Blau des Wassers sich malen.", "tokens": ["Die", "auf", "dem", "Blau", "des", "Was\u00b7sers", "sich", "ma\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "ART", "NN", "PRF", "VVINF", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Aber der Gl\u00fcckliche kennt nur Beharren.", "tokens": ["A\u00b7ber", "der", "Gl\u00fcck\u00b7li\u00b7che", "kennt", "nur", "Be\u00b7har\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ADV", "NN", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.5": {"text": "Ach, er verga\u00df ganz die Sehnsucht der Tage", "tokens": ["Ach", ",", "er", "ver\u00b7ga\u00df", "ganz", "die", "Sehn\u00b7sucht", "der", "Ta\u00b7ge"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "PPER", "VVFIN", "ADV", "ART", "NN", "ART", "NN"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.6": {"text": "Gestern und vorher, die Jahre gehegte.", "tokens": ["Ge\u00b7stern", "und", "vor\u00b7her", ",", "die", "Jah\u00b7re", "ge\u00b7heg\u00b7te", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADV", "$,", "ART", "NN", "VVFIN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Ach, ihm erstarb ganz die brennende Frage", "tokens": ["Ach", ",", "ihm", "er\u00b7starb", "ganz", "die", "bren\u00b7nen\u00b7de", "Fra\u00b7ge"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "PPER", "VVFIN", "ADV", "ART", "ADJA", "NN"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.8": {"text": "Wann? Und er sieht die Errettung verweilen,", "tokens": ["Wann", "?", "Und", "er", "sieht", "die", "Er\u00b7ret\u00b7tung", "ver\u00b7wei\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "KON", "PPER", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.9": {"text": "Aber vom Gl\u00fcck?! \u2013 Und tr\u00e4umend entgleiten", "tokens": ["A\u00b7ber", "vom", "Gl\u00fcck", "?!", "\u2013", "Und", "tr\u00e4u\u00b7mend", "ent\u00b7glei\u00b7ten"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "$.", "$(", "KON", "ADJD", "VVINF"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.10": {"text": "Sieht er die Tage, die Segel enteilen", "tokens": ["Sieht", "er", "die", "Ta\u00b7ge", ",", "die", "Se\u00b7gel", "ent\u00b7ei\u00b7len"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "ART", "NN", "VVINF"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "li