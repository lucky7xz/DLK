{"dta.poem.6536": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den Farben.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Durch das lebendige Bewegen,", "tokens": ["Durch", "das", "le\u00b7ben\u00b7di\u00b7ge", "Be\u00b7we\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "So uns des Lichtes Glantz und sch\u00f6ne Theil\u2019 erregen,", "tokens": ["So", "uns", "des", "Lich\u00b7tes", "Glantz", "und", "sch\u00f6\u00b7ne", "Theil'", "er\u00b7re\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ART", "NN", "NN", "KON", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Bewegt sich alles, lacht, und schimmert in der Welt:", "tokens": ["Be\u00b7wegt", "sich", "al\u00b7les", ",", "lacht", ",", "und", "schim\u00b7mert", "in", "der", "Welt", ":"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PIS", "$,", "VVFIN", "$,", "KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Allein die Farben sinds, durch deren sanften Krafft", "tokens": ["Al\u00b7lein", "die", "Far\u00b7ben", "sinds", ",", "durch", "de\u00b7ren", "sanf\u00b7ten", "Krafft"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "VAFIN", "$,", "APPR", "PRELAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Gemildertes Gemisch\u2019 uns alles erst gef\u00e4llt,", "tokens": ["Ge\u00b7mil\u00b7der\u00b7tes", "Ge\u00b7misch'", "uns", "al\u00b7les", "erst", "ge\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPER", "PIS", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und das der Vorw\u00fcrff\u2019 Eigenschafft", "tokens": ["Und", "das", "der", "Vor\u00b7w\u00fcrff'", "Ei\u00b7gen\u00b7schafft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PDS", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und Unterscheid uns zeiget,", "tokens": ["Und", "Un\u00b7ter\u00b7scheid", "uns", "zei\u00b7get", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Durch sie begiebt man sich und steiget:", "tokens": ["Durch", "sie", "be\u00b7giebt", "man", "sich", "und", "stei\u00b7get", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PIS", "PRF", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Von einer Sch\u00f6nheit zu der andern,", "tokens": ["Von", "ei\u00b7ner", "Sch\u00f6n\u00b7heit", "zu", "der", "an\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Durch deren Aenderung und liebliches Verbinden", "tokens": ["Durch", "de\u00b7ren", "A\u00b7en\u00b7de\u00b7rung", "und", "lieb\u00b7li\u00b7ches", "Ver\u00b7bin\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELAT", "NN", "KON", "ADJA", "NN"], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.11": {"text": "Wir tausendfachen Reitz in ihrer Menge finden.", "tokens": ["Wir", "tau\u00b7send\u00b7fa\u00b7chen", "Reitz", "in", "ih\u00b7rer", "Men\u00b7ge", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}