{"dta.poem.325": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Die wunderlichen Arten der Liebe.  \n Aus dem Frantz\u00f6sischen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Ach ich seuffze/ mir ist bange!", "tokens": ["Ach", "ich", "seuff\u00b7ze", "/", "mir", "ist", "ban\u00b7ge", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "PPER", "VVFIN", "$(", "PPER", "VAFIN", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ich qu\u00e4hle mich so Tag als Nacht/", "tokens": ["Ich", "qu\u00e4h\u00b7le", "mich", "so", "Tag", "als", "Nacht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "NN", "KOUS", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich erschrecke! ich verlange!", "tokens": ["Ich", "er\u00b7schre\u00b7cke", "!", "ich", "ver\u00b7lan\u00b7ge", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die Liebe hat mich kranck gemacht.", "tokens": ["Die", "Lie\u00b7be", "hat", "mich", "kranck", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Eure heissen Augen-Strahlen", "tokens": ["Eu\u00b7re", "heis\u00b7sen", "Au\u00b7gen\u00b7Strah\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Die haben meinen Geist entz\u00fcndt/", "tokens": ["Die", "ha\u00b7ben", "mei\u00b7nen", "Geist", "ent\u00b7z\u00fcndt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Da\u00df die vielen Liebes-Quaalen", "tokens": ["Da\u00df", "die", "vie\u00b7len", "Lie\u00b7bes\u00b7Quaa\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Gar nicht von mir zu hassen sind.", "tokens": ["Gar", "nicht", "von", "mir", "zu", "has\u00b7sen", "sind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "APPR", "PPER", "PTKZU", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "La\u00dft uns diese Losung geben/", "tokens": ["La\u00dft", "uns", "die\u00b7se", "Lo\u00b7sung", "ge\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PDAT", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "So offt man uns zu reden zwingt/", "tokens": ["So", "offt", "man", "uns", "zu", "re\u00b7den", "zwingt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PIS", "PPER", "PTKZU", "VVINF", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Recht begl\u00fccket ist das Leben/", "tokens": ["Recht", "be\u00b7gl\u00fc\u00b7cket", "ist", "das", "Le\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "VAFIN", "ART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Das man verliebt zu Ende bringt.", "tokens": ["Das", "man", "ver\u00b7liebt", "zu", "En\u00b7de", "bringt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "VVFIN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Furcht und Seuffzen kommt den Seelen", "tokens": ["Furcht", "und", "Seuff\u00b7zen", "kommt", "den", "See\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Fast immerfort zu einer Zeit/", "tokens": ["Fast", "im\u00b7mer\u00b7fort", "zu", "ei\u00b7ner", "Zeit", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und aus diesen stillen Qu\u00e4hlen", "tokens": ["Und", "aus", "die\u00b7sen", "stil\u00b7len", "Qu\u00e4h\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PDAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Erh\u00e4lt der Liebe Hefftigkeit.", "tokens": ["Er\u00b7h\u00e4lt", "der", "Lie\u00b7be", "Heff\u00b7tig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Warum wollen wir denn d\u00e4mpffen", "tokens": ["Wa\u00b7rum", "wol\u00b7len", "wir", "denn", "d\u00e4mpf\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VMFIN", "PPER", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der Seelen heisse Liebes-Gluth?", "tokens": ["Der", "See\u00b7len", "heis\u00b7se", "Lie\u00b7bes\u00b7Gluth", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und die s\u00fcsse Pein bek\u00e4mpffen?", "tokens": ["Und", "die", "s\u00fcs\u00b7se", "Pein", "be\u00b7k\u00e4mpf\u00b7fen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da uns die Quaal so sanffte thut.", "tokens": ["Da", "uns", "die", "Qua\u00b7al", "so", "sanff\u00b7te", "thut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "ADJA", "VVFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "La\u00dft uns diese Losung geben", "tokens": ["La\u00dft", "uns", "die\u00b7se", "Lo\u00b7sung", "ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "PDAT", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "So offt man uns zu reden zwingt", "tokens": ["So", "offt", "man", "uns", "zu", "re\u00b7den", "zwingt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "PIS", "PPER", "PTKZU", "VVINF", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Recht begl\u00fccket ist das Leben/", "tokens": ["Recht", "be\u00b7gl\u00fc\u00b7cket", "ist", "das", "Le\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "VAFIN", "ART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Das man verliebt zu Ende bringt.", "tokens": ["Das", "man", "ver\u00b7liebt", "zu", "En\u00b7de", "bringt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "VVFIN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}