{"textgrid.poem.32278": {"metadata": {"author": {"name": "Lessing, Gotthold Ephraim", "birth": "N.A.", "death": "N.A."}, "title": "Das Paradies", "genre": "verse", "period": "N.A.", "pub_year": 1755, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sein Gl\u00fcck f\u00fcr einen Apfel geben,", "tokens": ["Sein", "Gl\u00fcck", "f\u00fcr", "ei\u00b7nen", "Ap\u00b7fel", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "O Adam, welche L\u00fcsternheit!", "tokens": ["O", "A\u00b7dam", ",", "wel\u00b7che", "L\u00fcs\u00b7tern\u00b7heit", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PWAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Statt deiner h\u00e4tt' ich sollen leben,", "tokens": ["Statt", "dei\u00b7ner", "h\u00e4tt'", "ich", "sol\u00b7len", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "VAFIN", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So w\u00e4r' das Paradies noch heut. \u2013", "tokens": ["So", "w\u00e4r'", "das", "Pa\u00b7ra\u00b7dies", "noch", "heut", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADV", "ADV", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wie aber, wenn alsdann die Traube", "tokens": ["Wie", "a\u00b7ber", ",", "wenn", "als\u00b7dann", "die", "Trau\u00b7be"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "$,", "KOUS", "ADV", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"