{"textgrid.poem.49859": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Der Schatten", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Von Dienern wimmelt's fr\u00fch vor Tag,", "tokens": ["Von", "Die\u00b7nern", "wim\u00b7melt's", "fr\u00fch", "vor", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von Lichtern, in des Grafen Schlo\u00df.", "tokens": ["Von", "Lich\u00b7tern", ",", "in", "des", "Gra\u00b7fen", "Schlo\u00df", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Reiter warten sein am Tor,", "tokens": ["Die", "Rei\u00b7ter", "war\u00b7ten", "sein", "am", "Tor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Es wiehert morgendlich sein Ro\u00df.", "tokens": ["Es", "wie\u00b7hert", "mor\u00b7gend\u00b7lich", "sein", "Ro\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.2": {"line.1": {"text": "Doch er bei seiner Frauen steht", "tokens": ["Doch", "er", "bei", "sei\u00b7ner", "Frau\u00b7en", "steht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Alleine noch im hohen Saal:", "tokens": ["Al\u00b7lei\u00b7ne", "noch", "im", "ho\u00b7hen", "Saal", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit Augen gramvoll pr\u00fcft er sie,", "tokens": ["Mit", "Au\u00b7gen", "gram\u00b7voll", "pr\u00fcft", "er", "sie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "VVFIN", "PPER", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Er spricht sie an zum letztenmal.", "tokens": ["Er", "spricht", "sie", "an", "zum", "letz\u00b7ten\u00b7mal", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "APPRART", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "\u00bbwirst du, derweil ich ferne bin", "tokens": ["\u00bb", "wirst", "du", ",", "der\u00b7weil", "ich", "fer\u00b7ne", "bin"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "VAFIN", "PPER", "$,", "KOUS", "PPER", "ADV", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bei des Erl\u00f6sers Grab, o Weib,", "tokens": ["Bei", "des", "Er\u00b7l\u00f6\u00b7sers", "Grab", ",", "o", "Weib", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,", "FM", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In Z\u00fcchten leben und getreu", "tokens": ["In", "Z\u00fcch\u00b7ten", "le\u00b7ben", "und", "ge\u00b7treu"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVINF", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mir sparen deinen jungen Leib?", "tokens": ["Mir", "spa\u00b7ren", "dei\u00b7nen", "jun\u00b7gen", "Leib", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Wirst du verschlie\u00dfen T\u00fcr und Tor", "tokens": ["Wirst", "du", "ver\u00b7schlie\u00b7\u00dfen", "T\u00fcr", "und", "Tor"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dem Manne, der uns lang entzweit,", "tokens": ["Dem", "Man\u00b7ne", ",", "der", "uns", "lang", "ent\u00b7zweit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wirst meines Hauses Ehre sein,", "tokens": ["Wirst", "mei\u00b7nes", "Hau\u00b7ses", "Eh\u00b7re", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "NN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie du nicht warest jederzeit?\u00ab", "tokens": ["Wie", "du", "nicht", "wa\u00b7rest", "je\u00b7der\u00b7zeit", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "PPER", "PTKNEG", "VAFIN", "ADV", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Sie nickt; da spricht er: \u00bbSchw\u00f6re denn!\u00ab", "tokens": ["Sie", "nickt", ";", "da", "spricht", "er", ":", "\u00bb", "Schw\u00f6\u00b7re", "denn", "!", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "$.", "ADV", "VVFIN", "PPER", "$.", "$(", "VVFIN", "ADV", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und z\u00f6gernd hebt sie auf die Hand.", "tokens": ["Und", "z\u00f6\u00b7gernd", "hebt", "sie", "auf", "die", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da sieht er bei der Lampe Schein", "tokens": ["Da", "sieht", "er", "bei", "der", "Lam\u00b7pe", "Schein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Des Weibes Schatten an der Wand.", "tokens": ["Des", "Wei\u00b7bes", "Schat\u00b7ten", "an", "der", "Wand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Ein Schauer ihn bef\u00e4llt \u2013 er sinnt,", "tokens": ["Ein", "Schau\u00b7er", "ihn", "be\u00b7f\u00e4llt", "\u2013", "er", "sinnt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVFIN", "$(", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er seufzt und wendet sich zumal.", "tokens": ["Er", "seufzt", "und", "wen\u00b7det", "sich", "zu\u00b7mal", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PRF", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er winkt ihr einen Scheidegru\u00df,", "tokens": ["Er", "winkt", "ihr", "ei\u00b7nen", "Schei\u00b7de\u00b7gru\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und l\u00e4sset sie allein im Saal.", "tokens": ["Und", "l\u00e4s\u00b7set", "sie", "al\u00b7lein", "im", "Saal", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Elf Tage war er auf der Fahrt,", "tokens": ["Elf", "Ta\u00b7ge", "war", "er", "auf", "der", "Fahrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VAFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ritt krank ins welsche Land hinein:", "tokens": ["Ritt", "krank", "ins", "wel\u00b7sche", "Land", "hin\u00b7ein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPRART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Frau Hilde gab den Tod ihm mit", "tokens": ["Frau", "Hil\u00b7de", "gab", "den", "Tod", "ihm", "mit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "NE", "VVFIN", "ART", "NN", "PPER", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In einem giftigen Becher Wein.", "tokens": ["In", "ei\u00b7nem", "gif\u00b7ti\u00b7gen", "Be\u00b7cher", "Wein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.8": {"line.1": {"text": "Es liegt eine Herberg an der Stra\u00df,", "tokens": ["Es", "liegt", "ei\u00b7ne", "Her\u00b7berg", "an", "der", "Stra\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Im wilden Tal, hei\u00dft Mutintal,", "tokens": ["Im", "wil\u00b7den", "Tal", ",", "hei\u00dft", "Mu\u00b7tin\u00b7tal", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da fiel er hin in Todesnot,", "tokens": ["Da", "fiel", "er", "hin", "in", "To\u00b7des\u00b7not", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und seine Seele Gott befahl.", "tokens": ["Und", "sei\u00b7ne", "See\u00b7le", "Gott", "be\u00b7fahl", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Dieselbe Nacht Frau Hilde lauscht,", "tokens": ["Die\u00b7sel\u00b7be", "Nacht", "Frau", "Hil\u00b7de", "lauscht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "NN", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Frau Hilde luget vom Altan:", "tokens": ["Frau", "Hil\u00b7de", "lu\u00b7get", "vom", "Al\u00b7tan", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nach ihrem Buhlen schaut sie aus,", "tokens": ["Nach", "ih\u00b7rem", "Buh\u00b7len", "schaut", "sie", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das Pf\u00f6rtlein war ihm aufgetan.", "tokens": ["Das", "Pf\u00f6rt\u00b7lein", "war", "ihm", "auf\u00b7ge\u00b7tan", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Es tut einen Schlag am vordern Tor,", "tokens": ["Es", "tut", "ei\u00b7nen", "Schlag", "am", "vor\u00b7dern", "Tor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und aber einen Schlag, da\u00df es dr\u00f6hnt und hallt;", "tokens": ["Und", "a\u00b7ber", "ei\u00b7nen", "Schlag", ",", "da\u00df", "es", "dr\u00f6hnt", "und", "hallt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "$,", "KOUS", "PPER", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Im Burghof mitten steht der Graf \u2013", "tokens": ["Im", "Burg\u00b7hof", "mit\u00b7ten", "steht", "der", "Graf", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vom Turm der W\u00e4chter kennt ihn bald.", "tokens": ["Vom", "Turm", "der", "W\u00e4ch\u00b7ter", "kennt", "ihn", "bald", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NE", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Und Vogt und Zofen auf dem Gang", "tokens": ["Und", "Vogt", "und", "Zo\u00b7fen", "auf", "dem", "Gang"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "KON", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den toten Herrn mit Grausen sehn,", "tokens": ["Den", "to\u00b7ten", "Herrn", "mit", "Grau\u00b7sen", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sehn ihn die Stiegen stracks herauf", "tokens": ["Sehn", "ihn", "die", "Stie\u00b7gen", "stracks", "her\u00b7auf"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADV", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nach seiner Frauen Kammer gehn.", "tokens": ["Nach", "sei\u00b7ner", "Frau\u00b7en", "Kam\u00b7mer", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Man h\u00f6rt sie schreien und st\u00fcrzen hin,", "tokens": ["Man", "h\u00f6rt", "sie", "schrei\u00b7en", "und", "st\u00fcr\u00b7zen", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "VVFIN", "KON", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und eine j\u00e4he Stille war.", "tokens": ["Und", "ei\u00b7ne", "j\u00e4\u00b7he", "Stil\u00b7le", "war", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das Gesinde, das flieht, auf die Zinnen es flieht:", "tokens": ["Das", "Ge\u00b7sin\u00b7de", ",", "das", "flieht", ",", "auf", "die", "Zin\u00b7nen", "es", "flieht", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PDS", "VVFIN", "$,", "APPR", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "--+---+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Da scheinen am Himmel die Sterne so klar.", "tokens": ["Da", "schei\u00b7nen", "am", "Him\u00b7mel", "die", "Ster\u00b7ne", "so", "klar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "ART", "NN", "ADV", "ADJD", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.13": {"line.1": {"text": "Und als vergangen war die Nacht,", "tokens": ["Und", "als", "ver\u00b7gan\u00b7gen", "war", "die", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "VVPP", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und stand am Wald das Morgenrot,", "tokens": ["Und", "stand", "am", "Wald", "das", "Mor\u00b7gen\u00b7rot", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie fanden das Weib in dem Gemach", "tokens": ["Sie", "fan\u00b7den", "das", "Weib", "in", "dem", "Ge\u00b7mach"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Am Bettfu\u00df unten liegen tot.", "tokens": ["Am", "Bett\u00b7fu\u00df", "un\u00b7ten", "lie\u00b7gen", "tot", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.14": {"line.1": {"text": "Und als sie treten in den Saal,", "tokens": ["Und", "als", "sie", "tre\u00b7ten", "in", "den", "Saal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "O Wunder! steht an we