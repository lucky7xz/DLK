{"textgrid.poem.61429": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Im Fr\u00fchling", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie geht nun, da sich brach der St\u00fcrme W\u00fcten,", "tokens": ["Wie", "geht", "nun", ",", "da", "sich", "brach", "der", "St\u00fcr\u00b7me", "W\u00fc\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ADV", "$,", "KOUS", "PRF", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Durchs Fr\u00fchlingstal ein wundervolles Weben!", "tokens": ["Durchs", "Fr\u00fch\u00b7ling\u00b7stal", "ein", "wun\u00b7der\u00b7vol\u00b7les", "We\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Es wei\u00df in jugendlichem Freudebeben", "tokens": ["Es", "wei\u00df", "in", "ju\u00b7gend\u00b7li\u00b7chem", "Freu\u00b7de\u00b7be\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Kein Wesen mehr sein Innerstes zu h\u00fcten.", "tokens": ["Kein", "We\u00b7sen", "mehr", "sein", "In\u00b7ners\u00b7tes", "zu", "h\u00fc\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Des Baumes Seele dringt hervor in Bl\u00fcten,", "tokens": ["Des", "Bau\u00b7mes", "See\u00b7le", "dringt", "her\u00b7vor", "in", "Bl\u00fc\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "PTKVZ", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Blume l\u00e4\u00dft den Geist als Duft entschweben,", "tokens": ["Die", "Blu\u00b7me", "l\u00e4\u00dft", "den", "Geist", "als", "Duft", "ent\u00b7schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "KOUS", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Zum Liede wird des Vogels tiefstes Leben,", "tokens": ["Zum", "Lie\u00b7de", "wird", "des", "Vo\u00b7gels", "tiefs\u00b7tes", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und licht in Flammen schmilzt der Wolke Br\u00fcten.", "tokens": ["Und", "licht", "in", "Flam\u00b7men", "schmilzt", "der", "Wol\u00b7ke", "Br\u00fc\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "NN", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Mir ist es oft in diesen lichten Tagen,", "tokens": ["Mir", "ist", "es", "oft", "in", "die\u00b7sen", "lich\u00b7ten", "Ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Als r\u00e4nge die Natur in heil'gem Triebe,", "tokens": ["Als", "r\u00e4n\u00b7ge", "die", "Na\u00b7tur", "in", "heil'\u00b7gem", "Trie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "po