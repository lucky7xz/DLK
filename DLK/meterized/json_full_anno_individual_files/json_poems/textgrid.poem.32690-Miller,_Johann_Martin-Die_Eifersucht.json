{"textgrid.poem.32690": {"metadata": {"author": {"name": "Miller, Johann Martin", "birth": "N.A.", "death": "N.A."}, "title": "Die Eifersucht", "genre": "verse", "period": "N.A.", "pub_year": 1772, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich habe sie gesehen!", "tokens": ["Ich", "ha\u00b7be", "sie", "ge\u00b7se\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sie stand am Rosenbusch mit ihm.", "tokens": ["Sie", "stand", "am", "Ro\u00b7sen\u00b7busch", "mit", "ihm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Vertraulich war", "tokens": ["Ver\u00b7trau\u00b7lich", "war"], "token_info": ["word", "word"], "pos": ["ADJD", "VAFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Ihr Blick, und Z\u00e4rtlichkeit darin.", "tokens": ["Ihr", "Blick", ",", "und", "Z\u00e4rt\u00b7lich\u00b7keit", "da\u00b7rin", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "KON", "NN", "PAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Da bebt' ich bla\u00df vor\u00fcber;", "tokens": ["Da", "bebt'", "ich", "bla\u00df", "vor\u00b7\u00fc\u00b7ber", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die falsche Seele barg sich mir,", "tokens": ["Die", "fal\u00b7sche", "See\u00b7le", "barg", "sich", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PRF", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ihr Busen schlug,", "tokens": ["Ihr", "Bu\u00b7sen", "schlug", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Wie er nur S\u00fcndern schlagen kann.", "tokens": ["Wie", "er", "nur", "S\u00fcn\u00b7dern", "schla\u00b7gen", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "O k\u00f6nnt' ich sie vergessen!", "tokens": ["O", "k\u00f6nnt'", "ich", "sie", "ver\u00b7ges\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Doch an ihr Herz gebunden ist", "tokens": ["Doch", "an", "ihr", "Herz", "ge\u00b7bun\u00b7den", "ist"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN", "VVPP", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das meinige,", "tokens": ["Das", "mei\u00b7ni\u00b7ge", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "PPOSS", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Und sie nur ri\u00df sich grausam los.", "tokens": ["Und", "sie", "nur", "ri\u00df", "sich", "grau\u00b7sam", "los", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "VVFIN", "PRF", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Und Rache! \u2013 Ja, beim Himmel!", "tokens": ["Und", "Ra\u00b7che", "!", "\u2013", "Ja", ",", "beim", "Him\u00b7mel", "!"], "token_info": ["word", "word", "punct", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KON", "NN",