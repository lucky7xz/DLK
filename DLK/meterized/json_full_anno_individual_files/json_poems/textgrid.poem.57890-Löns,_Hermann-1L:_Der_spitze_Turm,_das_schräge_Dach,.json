{"textgrid.poem.57890": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der spitze Turm, das schr\u00e4ge Dach,", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der spitze Turm, das schr\u00e4ge Dach,", "tokens": ["Der", "spit\u00b7ze", "Turm", ",", "das", "schr\u00e4\u00b7ge", "Dach", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "War deutsche Bauart von je;", "tokens": ["War", "deut\u00b7sche", "Bau\u00b7art", "von", "je", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "NN", "APPR", "ADV", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Doch seh' ich mir heute die H\u00e4user an,", "tokens": ["Doch", "seh'", "ich", "mir", "heu\u00b7te", "die", "H\u00e4u\u00b7ser", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPER", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Dann wird mir wirklich weh'.", "tokens": ["Dann", "wird", "mir", "wirk\u00b7lich", "weh'", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Nicht morgenl\u00e4ndischer kann es wohl", "tokens": ["Nicht", "mor\u00b7gen\u00b7l\u00e4n\u00b7di\u00b7scher", "kann", "es", "wohl"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADV", "VMFIN", "PPER", "ADV"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "In Kairo und Bagdad a