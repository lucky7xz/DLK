{"dta.poem.669": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "2. Gegensatz.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Sje werffen weg/ sie strewen auff den acker/ ", "tokens": ["Sje", "werf\u00b7fen", "weg", "/", "sie", "stre\u00b7wen", "auff", "den", "ac\u00b7ker", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PTKVZ", "$(", "PPER", "VVFIN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Was sie durch so viel noth vnd sorgen", "tokens": ["Was", "sie", "durch", "so", "viel", "noth", "vnd", "sor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "APPR", "ADV", "PIAT", "NN", "KON", "NN"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Erschwitzt/ worvber sie der morgen ", "tokens": ["Er\u00b7schwitzt", "/", "worv\u00b7ber", "sie", "der", "mor\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVPP", "$(", "PWAV", "PPER", "ART", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Vnd abendr\u00f6t/ vnd mitternacht fandt wacker.", "tokens": ["Vnd", "a\u00b7bend\u00b7r\u00f6t", "/", "vnd", "mit\u00b7ter\u00b7nacht", "fandt", "wa\u00b7cker", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$(", "KON", "NN", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Doch seidt getrost/ der schaad ist nicht zu achten!", "tokens": ["Doch", "seidt", "ge\u00b7trost", "/", "der", "schaad", "ist", "nicht", "zu", "ach\u00b7ten", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "VVPP", "$(", "ART", "ADJD", "VAFIN", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Was jtzundt nimbt wirdt wieder geben/", "tokens": ["Was", "jt\u00b7zundt", "nimbt", "wirdt", "wie\u00b7der", "ge\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "VAFIN", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Was jtzundt stirbt/ wirdt wieder leben.", "tokens": ["Was", "jt\u00b7zundt", "stirbt", "/", "wirdt", "wie\u00b7der", "le\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "$(", "VAFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Jhr solt nicht ewig also schmachten.", "tokens": ["Ihr", "solt", "nicht", "e\u00b7wig", "al\u00b7so", "schmach\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "ADJD", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}