{"textgrid.poem.54072": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Saisonarbeiter", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zehntausend polnische Schnitter,", "tokens": ["Zehn\u00b7tau\u00b7send", "pol\u00b7ni\u00b7sche", "Schnit\u00b7ter", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["CARD", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "die kommen nach Deutschland hinein;", "tokens": ["die", "kom\u00b7men", "nach", "Deutschland", "hin\u00b7ein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VVFIN", "APPR", "NE", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "es wollen die pommerschen Ritter", "tokens": ["es", "wol\u00b7len", "die", "pom\u00b7mer\u00b7schen", "Rit\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ART", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "billig bedienet sein.", "tokens": ["bil\u00b7lig", "be\u00b7die\u00b7net", "sein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "VAINF", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Die beschimpfen den deutschen Arbeitsmann,", "tokens": ["Die", "be\u00b7schimp\u00b7fen", "den", "deut\u00b7schen", "Ar\u00b7beits\u00b7mann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "weil der ihr Fressen nicht essen kann", "tokens": ["weil", "der", "ihr", "Fres\u00b7sen", "nicht", "es\u00b7sen", "kann"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "PPOSAT", "NN", "PTKNEG", "VVINF", "VMFIN"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.7": {"text": "und nicht wohnt in dem Lumpenquartier.", "tokens": ["und", "nicht", "wohnt", "in", "dem", "Lum\u00b7pen\u00b7quar\u00b7tier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.8": {"text": "Und es erl\u00e4\u00dft der Herr Aristokrat", "tokens": ["Und", "es", "er\u00b7l\u00e4\u00dft", "der", "Herr", "A\u00b7ris\u00b7to\u00b7krat"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "NE"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "ein Landarbeiter-Inserat", "tokens": ["ein", "Land\u00b7a\u00b7rbei\u00b7ter\u00b7In\u00b7se\u00b7rat"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "auf Zeitungspapier,", "tokens": ["auf", "Zei\u00b7tungs\u00b7pa\u00b7pier", ","], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.11": {"text": "auf Zeitungspapier,", "tokens": ["auf", "Zei\u00b7tungs\u00b7pa\u00b7pier", ","], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.12": {"text": "auf polnischem Zeitungspapier.", "tokens": ["auf", "pol\u00b7ni\u00b7schem", "Zei\u00b7tungs\u00b7pa\u00b7pier", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.2": {"line.1": {"text": "Zehntausend \u00c4rmste der Armen,", "tokens": ["Zehn\u00b7tau\u00b7send", "\u00c4rms\u00b7te", "der", "Ar\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "die treten zur Arbeit an,", "tokens": ["die", "tre\u00b7ten", "zur", "Ar\u00b7beit", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "bewacht von den Gendarmen,", "tokens": ["be\u00b7wacht", "von", "den", "Gen\u00b7dar\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "da\u00df keiner auskneifen kann.", "tokens": ["da\u00df", "kei\u00b7ner", "aus\u00b7knei\u00b7fen", "kann", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVINF", "VMFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Sie schuften f\u00fcr ein paar Zettel Geld.", "tokens": ["Sie", "schuf\u00b7ten", "f\u00fcr", "ein", "paar", "Zet\u00b7tel", "Geld", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "PIAT", "NN", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Bringt die Arbeitersfrau ein Kind zur Welt,", "tokens": ["Bringt", "die", "Ar\u00b7bei\u00b7ters\u00b7frau", "ein", "Kind", "zur", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-----+-+-+", "measure": "unknown.measure.tri"}, "line.7": {"text": "dann n\u00e4selt der Kavalier:", "tokens": ["dann", "n\u00e4\u00b7selt", "der", "Ka\u00b7va\u00b7lier", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "\u00bbwas sind denn das f\u00fcr Schweinerein?", "tokens": ["\u00bb", "was", "sind", "denn", "das", "f\u00fcr", "Schwei\u00b7ne\u00b7rein", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VAFIN", "ADV", "ART", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "W\u00e4sche \u2013? Wickeln Sie das doch ein", "tokens": ["W\u00e4\u00b7sche", "\u2013", "?", "Wi\u00b7ckeln", "Sie", "das", "doch", "ein"], "token_info": ["word", "punct", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$(", "$.", "NN", "PPER", "PDS", "ADV", "ART"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.10": {"text": "in Zeitungspapier,", "tokens": ["in", "Zei\u00b7tungs\u00b7pa\u00b7pier", ","], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.11": {"text": "in Zeitungspapier,", "tokens": ["in", "Zei\u00b7tungs\u00b7pa\u00b7pier", ","], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.12": {"text": "in schlesisches Zeitungspapier \u2013!\u00ab", "tokens": ["in", "schle\u00b7si\u00b7sches", "Zei\u00b7tungs\u00b7pa\u00b7pier", "\u2013", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct", "punct"], "pos": ["APPR", "ADJA", "NN", "$(", "$.", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.3": {"line.1": {"text": "So werden Proleten betrogen,", "tokens": ["So", "wer\u00b7den", "Pro\u00b7le\u00b7ten", "be\u00b7tro\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NN", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "so werden Kinder gemacht.", "tokens": ["so", "wer\u00b7den", "Kin\u00b7der", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NN", "VVPP", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Sie liegen in Zeitungsbogen", "tokens": ["Sie", "lie\u00b7gen", "in", "Zei\u00b7tungs\u00b7bo\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "und br\u00fcllen die ganze Nacht.", "tokens": ["und", "br\u00fcl\u00b7len", "die", "gan\u00b7ze", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Und k\u00f6nnten sie lesen, so l\u00e4sen sie gleich", "tokens": ["Und", "k\u00f6nn\u00b7ten", "sie", "le\u00b7sen", ",", "so", "l\u00e4\u00b7sen", "sie", "gleich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "VVINF", "$,", "ADV", "VVFIN", "PPER", "ADV"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "von dem herrlichen, dem Deutschen Reich", "tokens": ["von", "dem", "herr\u00b7li\u00b7chen", ",", "dem", "Deut\u00b7schen", "Reich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "$,", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "und von proletarischer Gier.", "tokens": ["und", "von", "pro\u00b7le\u00b7ta\u00b7ri\u00b7scher", "Gier", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.8": {"text": "Wirf, Arbeiter, aus deinem Haus,", "tokens": ["Wirf", ",", "Ar\u00b7bei\u00b7ter", ",", "aus", "dei\u00b7nem", "Haus", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.9": {"text": "die arbeiterfeindliche Presse heraus!", "to