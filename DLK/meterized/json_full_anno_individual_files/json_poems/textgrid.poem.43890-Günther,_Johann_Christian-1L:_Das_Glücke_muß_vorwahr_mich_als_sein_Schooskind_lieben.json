{"textgrid.poem.43890": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Das Gl\u00fccke mu\u00df vorwahr mich als sein Schooskind lieben", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das Gl\u00fccke mu\u00df vorwahr mich als sein Schooskind lieben", "tokens": ["Das", "Gl\u00fc\u00b7cke", "mu\u00df", "vor\u00b7wahr", "mich", "als", "sein", "Scho\u00b7os\u00b7kind", "lie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "ADV", "PPER", "KOUS", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-++-+-", "measure": "unknown.measure.septa"}, "line.2": {"text": "Und das Verh\u00e4ngn\u00fc\u00df mich zu qu\u00e4len m\u00fcde seyn,", "tokens": ["Und", "das", "Ver\u00b7h\u00e4ng\u00b7n\u00fc\u00df", "mich", "zu", "qu\u00e4\u00b7len", "m\u00fc\u00b7de", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PPER", "PTKZU", "VVINF", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Weil du, getreues Kind, mir nach so mancher Pein", "tokens": ["Weil", "du", ",", "ge\u00b7treu\u00b7es", "Kind", ",", "mir", "nach", "so", "man\u00b7cher", "Pein"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "$,", "ADJA", "NN", "$,", "PPER", "APPR", "ADV", "PIAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dein unverf\u00e4lschtes Herz zum Eigenthum verschrieben.", "tokens": ["Dein", "un\u00b7ver\u00b7f\u00e4lschtes", "Herz", "zum", "Ei\u00b7gen\u00b7thum", "ver\u00b7schrie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Mein Schif, das Wind und Meer an manchen Fels getrieben,", "tokens": ["Mein", "Schif", ",", "das", "Wind", "und", "Meer", "an", "man\u00b7chen", "Fels", "ge\u00b7trie\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ART", "NN", "KON", "NN", "APPR", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Lauft den Vergn\u00fcgungsport mit vollen Seegeln ein,", "tokens": ["Lauft", "den", "Ver\u00b7gn\u00fc\u00b7gungs\u00b7port", "mit", "vol\u00b7len", "See\u00b7geln", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Und meine Hofnung kan sich schon im Geiste freun,", "tokens": ["Und", "mei\u00b7ne", "Hof\u00b7nung", "kan", "sich", "schon", "im", "Geis\u00b7te", "freun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VMFIN", "PRF", "ADV", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Nachdem dein freyes Ja den Zweifel aufgerieben.", "tokens": ["Nach\u00b7dem", "dein", "frey\u00b7es", "Ja", "den", "Zwei\u00b7fel", "auf\u00b7ge\u00b7rie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Versiegle nun den Bund durch einen feuchten Ku\u00df,", "tokens": ["Ver\u00b7sieg\u00b7le", "nun", "den", "Bund", "durch", "ei\u00b7nen", "feuch\u00b7ten", "Ku\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bis dich des Priesters Hand mir v\u00f6llig \u00fcberreiche,", "tokens": ["Bis", "dich", "des", "Pries\u00b7ters", "Hand", "mir", "v\u00f6l\u00b7lig", "\u00fc\u00b7berr\u00b7ei\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "ART", "NN", "NN", "PPER", "ADJD", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und glaube, da\u00df mich selbst der Himmel strafen mu\u00df,", "tokens": ["Und", "glau\u00b7be", ",", "da\u00df", "mich", "selbst", "der", "Him\u00b7mel", "stra\u00b7fen", "mu\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PPER", "ADV", "ART", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wofern mein Wanckelmuth dein Bild in mir verstreiche.", "tokens": ["Wo\u00b7fern", "mein", "Wan\u00b7ckel\u00b7muth", "dein", "Bild", "in", "mir", "ver\u00b7strei\u00b7che", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPOSAT", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Drum liebe nur getrost; denn die Best\u00e4ndigkeit", "tokens": ["Drum", "lie\u00b7be", "nur", "ge\u00b7trost", ";", "denn", "die", "Be\u00b7st\u00e4n\u00b7dig\u00b7keit"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PAV", "VVFIN", "ADV", "VVPP", "$.", "KON", "ART", "NN"], "meter": "-+-+-