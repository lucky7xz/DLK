{"dta.poem.297": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Er will die Schmiedin nicht mehr lieben.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Ich habe mich darauf verschworen/", "tokens": ["Ich", "ha\u00b7be", "mich", "da\u00b7rauf", "ver\u00b7schwo\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PRF", "PAV", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df ich sie nicht mehr lieben will/", "tokens": ["Da\u00df", "ich", "sie", "nicht", "mehr", "lie\u00b7ben", "will", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "PTKNEG", "ADV", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mein Hertze ruhet in ", "tokens": ["Mein", "Hert\u00b7ze", "ru\u00b7het", "in"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "VVFIN", "VVFIN", "APPR"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und liegt auch bey ", "tokens": ["Und", "liegt", "auch", "bey"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "APPR"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Allein der Schmiedin Liebes-Banden", "tokens": ["Al\u00b7lein", "der", "Schmie\u00b7din", "Lie\u00b7bes\u00b7Ban\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Sind in der Brust nicht mehr verhanden.", "tokens": ["Sind", "in", "der", "Brust", "nicht", "mehr", "ver\u00b7han\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "PTKNEG", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Mich m\u00fcste ja der Teuffel plagen", "tokens": ["Mich", "m\u00fcs\u00b7te", "ja", "der", "Teuf\u00b7fel", "pla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dem M\u00e4dgen lieb und hold zu seyn/", "tokens": ["Dem", "M\u00e4d\u00b7gen", "lieb", "und", "hold", "zu", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "KON", "ADJD", "PTKZU", "VAINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das allen Leuten pflegt zu sagen/?", "tokens": ["Das", "al\u00b7len", "Leu\u00b7ten", "pflegt", "zu", "sa\u00b7gen", "/", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "PTKZU", "VVINF", "$(", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ich st\u00fcrbe fast vor Liebes-Pein/", "tokens": ["Ich", "st\u00fcr\u00b7be", "fast", "vor", "Lie\u00b7bes\u00b7Pein", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da/ wenn es meinen Sinn recht kennte/", "tokens": ["Da", "/", "wenn", "es", "mei\u00b7nen", "Sinn", "recht", "kenn\u00b7te", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "KOUS", "PPER", "PPOSAT", "NN", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Es sich die Finger nicht verbrennte.", "tokens": ["Es", "sich", "die", "Fin\u00b7ger", "nicht", "ver\u00b7brenn\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PRF", "ART", "NN", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Mach\u2019 ich zu weilen ", "tokens": ["Mach'", "ich", "zu", "wei\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKZU", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Und f\u00e4lt das Stellen mir nicht schwer/", "tokens": ["Und", "f\u00e4lt", "das", "Stel\u00b7len", "mir", "nicht", "schwer", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PPER", "PTKNEG", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df alle Leute sagen k\u00f6nten/", "tokens": ["Da\u00df", "al\u00b7le", "Leu\u00b7te", "sa\u00b7gen", "k\u00f6n\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df ich im Ernst verliebet w\u00e4r/", "tokens": ["Da\u00df", "ich", "im", "Ernst", "ver\u00b7lie\u00b7bet", "w\u00e4r", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "VVFIN", "VAFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So sind sie doch beym Licht betrogen/", "tokens": ["So", "sind", "sie", "doch", "beym", "Licht", "be\u00b7tro\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "APPRART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und ihre Meynung hat gelogen.", "tokens": ["Und", "ih\u00b7re", "Mey\u00b7nung", "hat", "ge\u00b7lo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ich liebe zwar/ doch nur mit Mienen/", "tokens": ["Ich", "lie\u00b7be", "zwar", "/", "doch", "nur", "mit", "Mie\u00b7nen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$(", "ADV", "ADV", "APPR", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die nicht von treuen Hertzen gehn/", "tokens": ["Die", "nicht", "von", "treu\u00b7en", "Hert\u00b7zen", "gehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "APPR", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich sage woll ich will euch dienen/", "tokens": ["Ich", "sa\u00b7ge", "woll", "ich", "will", "euch", "die\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "VMFIN", "PPER", "VMFIN", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Doch ihr m\u00fc\u00dft mir zu Dienste stehn;", "tokens": ["Doch", "ihr", "m\u00fc\u00dft", "mir", "zu", "Diens\u00b7te", "stehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PPER", "APPR", "NN", "VVINF", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Denn eh\u2019 ich mich zum Sclaven mache/", "tokens": ["Denn", "eh'", "ich", "mich", "zum", "Scla\u00b7ven", "ma\u00b7che", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PRF", "APPRART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ich alle eure Lust verlache.", "tokens": ["Ich", "al\u00b7le", "eu\u00b7re", "Lust", "ver\u00b7la\u00b7che", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Ich h\u00f6re ohne dem nach Flandern", "tokens": ["Ich", "h\u00f6\u00b7re", "oh\u00b7ne", "dem", "nach", "Flan\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und kan nicht wol best\u00e4ndig seyn;", "tokens": ["Und", "kan", "nicht", "wol", "be\u00b7st\u00e4n\u00b7dig", "seyn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PTKNEG", "ADV", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch diese mu\u00df vor allen wandern/", "tokens": ["Doch", "die\u00b7se", "mu\u00df", "vor", "al\u00b7len", "wan\u00b7dern", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VMFIN", "APPR", "PIS", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die sich die Herrschafft bildet ein.", "tokens": ["Die", "sich", "die", "Herr\u00b7schafft", "bil\u00b7det", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der aber bleib ich mehr ergeben/", "tokens": ["Der", "a\u00b7ber", "bleib", "ich", "mehr", "er\u00b7ge\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "PPER", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die mich in Freyheit l\u00e4sset leben.", "tokens": ["Die", "mich", "in", "Frey\u00b7heit", "l\u00e4s\u00b7set", "le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NN", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Victore will ich ewig lieben/", "tokens": ["Vic\u00b7to\u00b7re", "will", "ich", "e\u00b7wig", "lie\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PPER", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die gleich wie ich gesinnet ist/", "tokens": ["Die", "gleich", "wie", "ich", "ge\u00b7sin\u00b7net", "ist", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "KOKOM", "PPER", "VVPP", "VAFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Clarinden kan ich nicht betr\u00fcben/", "tokens": ["Cla\u00b7rin\u00b7den", "kan", "ich", "nicht", "be\u00b7tr\u00fc\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PPER", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ob ich schon ", "tokens": ["Ob", "ich", "schon"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPER", "ADV"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Bey diesen M\u00e4dgens will ich bleiben/", "tokens": ["Bey", "die\u00b7sen", "M\u00e4d\u00b7gens", "will", "ich", "blei\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VMFIN", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Weil sie sich auch von Flandern schreiben.", "tokens": ["Weil", "sie", "sich", "auch", "von", "Flan\u00b7dern", "schrei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Wer kan mich nun verliebet nennen?", "tokens": ["Wer", "kan", "mich", "nun", "ver\u00b7lie\u00b7bet", "nen\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "ADV", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da ich so wanckelm\u00fchtig bin/", "tokens": ["Da", "ich", "so", "wan\u00b7ckel\u00b7m\u00fch\u00b7tig", "bin", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "VAFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mir deucht es wird vor Liebe brennen", "tokens": ["Mir", "deucht", "es", "wird", "vor", "Lie\u00b7be", "bren\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "VAFIN", "APPR", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die unerfahrne Sch\u00fclerin/", "tokens": ["Die", "un\u00b7er\u00b7fahr\u00b7ne", "Sch\u00fc\u00b7le\u00b7rin", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und da\u00df sie um sich zu besch\u00f6nen", "tokens": ["Und", "da\u00df", "sie", "um", "sich", "zu", "be\u00b7sch\u00f6\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "APPR", "PRF", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Nur meine Flammen will verh\u00f6hnen.", "tokens": ["Nur", "mei\u00b7ne", "Flam\u00b7men", "will", "ver\u00b7h\u00f6h\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}