{"dta.poem.10085": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Aenderung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20086-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Anmuth, die man fr\u00fch, (durch s\u00fcssen Schlaff er-", "tokens": ["Die", "An\u00b7muth", ",", "die", "man", "fr\u00fch", ",", "(", "durch", "s\u00fcs\u00b7sen", "Schlaff", "er"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "PIS", "ADJD", "$,", "$(", "APPR", "ADJA", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "frischt", "tokens": ["frischt"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Und halb verj\u00fcngt) versp\u00fchrt; die Anmuth, die uns nehrt,", "tokens": ["Und", "halb", "ver\u00b7j\u00fcngt", ")", "ver\u00b7sp\u00fchrt", ";", "die", "An\u00b7muth", ",", "die", "uns", "nehrt", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVPP", "$(", "VVPP", "$.", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wann uns so vielerley wird aufgetischt,", "tokens": ["Wann", "uns", "so", "vie\u00b7ler\u00b7ley", "wird", "auf\u00b7ge\u00b7tischt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "PIAT", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wie zweymahl ordentlich uns t\u00e4glich wiederf\u00e4hrt;", "tokens": ["Wie", "zwey\u00b7mahl", "or\u00b7dent\u00b7lich", "uns", "t\u00e4g\u00b7lich", "wie\u00b7der\u00b7f\u00e4hrt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADJD", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die Wollust, wann der Mensch die m\u00fcden Glieder", "tokens": ["Die", "Wol\u00b7lust", ",", "wann", "der", "Mensch", "die", "m\u00fc\u00b7den", "Glie\u00b7der"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PWAV", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Des Abends wiederum, mit vieler Anmuth, nieder", "tokens": ["Des", "A\u00b7bends", "wie\u00b7de\u00b7rum", ",", "mit", "vie\u00b7ler", "An\u00b7muth", ",", "nie\u00b7der"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "ADV", "$,", "APPR", "PIAT", "NN", "$,", "PTKVZ"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Auf weiche Federn streckt; wodurch die lange Zeit", "tokens": ["Auf", "wei\u00b7che", "Fe\u00b7dern", "streckt", ";", "wo\u00b7durch", "die", "lan\u00b7ge", "Zeit"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$.", "PWAV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Verk\u00fcrtzt, der Arbeit Last, und andere Beschwerden", "tokens": ["Ver\u00b7k\u00fcrtzt", ",", "der", "Ar\u00b7beit", "Last", ",", "und", "an\u00b7de\u00b7re", "Be\u00b7schwer\u00b7den"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVPP", "$,", "ART", "NN", "NE", "$,", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Best\u00e4ndig unterbrochen werden,", "tokens": ["Be\u00b7st\u00e4n\u00b7dig", "un\u00b7ter\u00b7bro\u00b7chen", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Sind alle Tage da. Wer aber denckt daran,", "tokens": ["Sind", "al\u00b7le", "Ta\u00b7ge", "da", ".", "Wer", "a\u00b7ber", "denckt", "da\u00b7ran", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "PTKVZ", "$.", "PWS", "ADV", "VVFIN", "PAV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Da\u00df GOTT, der Sch\u00f6pfer, uns in diesem Leben,", "tokens": ["Da\u00df", "GoTT", ",", "der", "Sch\u00f6p\u00b7fer", ",", "uns", "in", "die\u00b7sem", "Le\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "$,", "ART", "NN", "$,", "PPER", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Selbst in der Aenderung, so manche Lust gegeben?", "tokens": ["Selbst", "in", "der", "A\u00b7en\u00b7de\u00b7rung", ",", "so", "man\u00b7che", "Lust", "ge\u00b7ge\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,", "ADV", "PIAT", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}}}}}