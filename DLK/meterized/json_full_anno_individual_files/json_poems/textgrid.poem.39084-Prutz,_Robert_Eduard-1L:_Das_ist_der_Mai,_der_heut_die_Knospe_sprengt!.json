{"textgrid.poem.39084": {"metadata": {"author": {"name": "Prutz, Robert Eduard", "birth": "N.A.", "death": "N.A."}, "title": "1L: Das ist der Mai, der heut die Knospe sprengt!", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das ist der Mai, der heut die Knospe sprengt!", "tokens": ["Das", "ist", "der", "Mai", ",", "der", "heut", "die", "Knos\u00b7pe", "sprengt", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "PRELS", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Das ist der Lenz, der holde Liebesbote,", "tokens": ["Das", "ist", "der", "Lenz", ",", "der", "hol\u00b7de", "Lie\u00b7bes\u00b7bo\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "der jauchzend heut dem jungen Morgenrote", "tokens": ["der", "jauch\u00b7zend", "heut", "dem", "jun\u00b7gen", "Mor\u00b7gen\u00b7ro\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "aus Bl\u00fcten sich entgegendr\u00e4ngt!", "tokens": ["aus", "Bl\u00fc\u00b7ten", "sich", "ent\u00b7ge\u00b7gen\u00b7dr\u00e4ngt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Durch alle B\u00e4ume geht ein leises Rauschen,", "tokens": ["Durch", "al\u00b7le", "B\u00e4u\u00b7me", "geht", "ein", "lei\u00b7ses", "Rau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "und alle Ohren neigen sich und lauschen,", "tokens": ["und", "al\u00b7le", "Oh\u00b7ren", "nei\u00b7gen", "sich", "und", "lau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "PRF", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "und alle Herzen f\u00fchlen warm und frei", "tokens": ["und", "al\u00b7le", "Her\u00b7zen", "f\u00fch\u00b7len", "warm", "und", "frei"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "VVFIN", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "und gr\u00fc\u00dfen dich, o Bl\u00fctenk\u00f6nig Mai! \u2013", "tokens": ["und", "gr\u00fc\u00b7\u00dfen", "dich", ",", "o", "Bl\u00fc\u00b7ten\u00b7k\u00f6\u00b7nig", "Mai", "!", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "FM", "NN", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Doch nicht ein Mai f\u00fcr Gras und B\u00e4ume blo\u00df:", "tokens": ["Doch", "nicht", "ein", "Mai", "f\u00fcr", "Gras", "und", "B\u00e4u\u00b7me", "blo\u00df", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ART", "NN", "APPR", "NN", "KON", "NN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ein andrer rei\u00dft, ersehnt mit tausend Schmerzen,", "tokens": ["Ein", "an\u00b7drer", "rei\u00dft", ",", "er\u00b7sehnt", "mit", "tau\u00b7send", "Schmer\u00b7zen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "$,", "VVFIN", "APPR", "CARD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "ein Fr\u00fchling heut der Geister und der Herzen", "tokens": ["ein", "Fr\u00fch\u00b7ling", "heut", "der", "Geis\u00b7ter", "und", "der", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "sich aus dem Grund der Zeiten los.", "tokens": ["sich", "aus", "dem", "Grund", "der", "Zei\u00b7ten", "los", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Er gr\u00fc\u00dft uns auch mit Nachtigallenschl\u00e4gen,", "tokens": ["Er", "gr\u00fc\u00dft", "uns", "auch", "mit", "Nach\u00b7ti\u00b7gal\u00b7len\u00b7schl\u00e4\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "er streckt uns auch ein sprossend Reis entgegen:", "tokens": ["er", "streckt", "uns", "auch", "ein", "spros\u00b7send", "Reis", "ent\u00b7ge\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ART", "ADJD", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Nun was sich jung und kr\u00e4ftig f\u00fchlt, herbei,", "tokens": ["Nun", "was", "sich", "jung", "und", "kr\u00e4f\u00b7tig", "f\u00fchlt", ",", "her\u00b7bei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "PRELS", "PRF", "ADJD", "KON", "ADJD", "VVFIN", "$,", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "zum Opferdienst dem Geisterk\u00f6nig Mai!", "tokens": ["zum", "Op\u00b7fer\u00b7dienst", "dem", "Geis\u00b7ter\u00b7k\u00f6\u00b7nig", "Mai", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Zwar wissen wir, der Sommer ist es nicht,", "tokens": ["Zwar", "wis\u00b7sen", "wir", ",", "der", "Som\u00b7mer", "ist", "es", "nicht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ART", "NN", "VAFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "noch Gr\u00f6\u00dfres bleibt die k\u00fcnft'ge Zeit uns schuldig,", "tokens": ["noch", "Gr\u00f6\u00df\u00b7res", "bleibt", "die", "k\u00fcnft'\u00b7ge", "Zeit", "uns", "schul\u00b7dig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVFIN", "ART", "ADJA", "NN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "nach dem das Herz sich sehnet ungeduldig,", "tokens": ["nach", "dem", "das", "Herz", "sich", "seh\u00b7net", "un\u00b7ge\u00b7dul\u00b7dig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "PRF", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "gleichwie das Auge nach dem Licht.", "tokens": ["gleich\u00b7wie", "das", "Au\u00b7ge", "nach", "dem", "Licht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Doch kommt auch dies! Die Knospe mu\u00df ja reifen,", "tokens": ["Doch", "kommt", "auch", "dies", "!", "Die", "Knos\u00b7pe", "mu\u00df", "ja", "rei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PDS", "$.", "ART", "NN", "VMFIN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "es mu\u00df der Kern die H\u00fclle von sich streifen,", "tokens": ["es", "mu\u00df", "der", "Kern", "die", "H\u00fcl\u00b7le", "von", "sich", "strei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "ART", "NN", "APPR", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "der Wille f\u00fchrt die Taten doch herbei:", "tokens": ["der", "Wil\u00b7le", "f\u00fchrt", "die", "Ta\u00b7ten", "doch", "her\u00b7bei", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Und immerdar ein Sommer folgt dem Mai! \u2013", "tokens": ["Und", "im\u00b7mer\u00b7dar", "ein", "Som\u00b7mer", "folgt", "dem", "Mai", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VVFIN", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Auf seine Fr\u00fcchte deute dieser Kranz!", "tokens": ["Auf", "sei\u00b7ne", "Fr\u00fcch\u00b7te", "deu\u00b7te", "die\u00b7ser", "Kranz", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Voll junger Knospen, sprossender Gedanken,", "tokens": ["Voll", "jun\u00b7ger", "Knos\u00b7pen", ",", "spros\u00b7sen\u00b7der", "Ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "soll er sich k\u00fchlend um die Schl\u00e4fe ranken", "tokens": ["soll", "er", "sich", "k\u00fch\u00b7lend", "um", "die", "Schl\u00e4\u00b7fe", "ran\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PRF", "ADJD", "APPR", "ART", "NN", "VVINF"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "des wundgetretnen Vaterlands;", "tokens": ["des", "wund\u00b7ge\u00b7tret\u00b7nen", "Va\u00b7ter\u00b7lands", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "ein Weihefr\u00fchling, wird er ausgesendet,", "tokens": ["ein", "Wei\u00b7he\u00b7fr\u00fch\u00b7ling", ",", "wird", "er", "aus\u00b7ge\u00b7sen\u00b7det", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "bis sich die Zeit, die nahende vollendet:", "tokens": ["bis", "sich", "die", "Zeit", ",", "die", "na\u00b7hen\u00b7de", "voll\u00b7en\u00b7det", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "ART", "NN", "$,", "ART", "ADJA", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Drum was sich jung und kr\u00e4ftig f\u00fchlt, herbei!", "tokens": ["Drum", "was", "sich", "jung", "und", "kr\u00e4f\u00b7tig", "f\u00fchlt", ",", "her\u00b7bei", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PAV", "PRELS", "PRF", "ADJD", "KON", "ADJD", "VVFIN", "$,", "PTKVZ", "$."], "m