{"dta.poem.2778": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Ejusdem. Desiderium VI.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ach ja ich bin es werth/ ich hab es auch verdienet/", "tokens": ["Ach", "ja", "ich", "bin", "es", "werth", "/", "ich", "hab", "es", "auch", "ver\u00b7die\u00b7net", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "PPER", "VAFIN", "PPER", "ADJD", "$(", "PPER", "VAFIN", "PPER", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich mu\u00df mein Laster nur mit vollem Hal\u00df ausschreyn/", "tokens": ["Ich", "mu\u00df", "mein", "Las\u00b7ter", "nur", "mit", "vol\u00b7lem", "Hal\u00df", "aus\u00b7schreyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "ADV", "APPR", "ADJA", "NN", "VVIZU", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Kein F\u00fcrspruch hilft mich hier/ denn was ich mich erk\u00fchnet", "tokens": ["Kein", "F\u00fcr\u00b7spruch", "hilft", "mich", "hier", "/", "denn", "was", "ich", "mich", "er\u00b7k\u00fch\u00b7net"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "ADV", "$(", "KON", "PWS", "PPER", "PRF", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ist eine solche Schuld die leicht nicht zu ver zeihn.", "tokens": ["Ist", "ei\u00b7ne", "sol\u00b7che", "Schuld", "die", "leicht", "nicht", "zu", "ver", "zeihn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "PIAT", "NN", "ART", "ADJD", "PTKNEG", "APPR", "NE", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ich habe mich an dir/ O h\u00f6chster GOtt vergriffen/", "tokens": ["Ich", "ha\u00b7be", "mich", "an", "dir", "/", "O", "h\u00f6chs\u00b7ter", "Gott", "ver\u00b7grif\u00b7fen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "APPR", "PPER", "$(", "NE", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und dieses Schandmal kan verm\u00e4nteln keine Kunst/", "tokens": ["Und", "die\u00b7ses", "Schand\u00b7mal", "kan", "ver\u00b7m\u00e4n\u00b7teln", "kei\u00b7ne", "Kunst", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "VMFIN", "VVINF", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ich wei\u00df da\u00df \u00fcber mich dein Rach-Schwerdt schon geschliffen/", "tokens": ["Ich", "wei\u00df", "da\u00df", "\u00fc\u00b7ber", "mich", "dein", "Rach\u00b7Schwerdt", "schon", "ge\u00b7schlif\u00b7fen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOUS", "APPR", "PPER", "PPOSAT", "NN", "ADV", "VVPP", "$("], "meter": "-+-+-+-++--+-", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Drumb ist die Furcht der That wie auch die Scham umsonst.", "tokens": ["Drumb", "ist", "die", "Furcht", "der", "That", "wie", "auch", "die", "Scham", "um\u00b7sonst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "ART", "NN", "ART", "NN", "KOKOM", "ADV", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ich falle dir zu Fu\u00df mit aufgehobnen H\u00e4nden/", "tokens": ["Ich", "fal\u00b7le", "dir", "zu", "Fu\u00df", "mit", "auf\u00b7ge\u00b7hob\u00b7nen", "H\u00e4n\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "NN", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Benenne nur die Straf in der ich b\u00fcssen mu\u00df/", "tokens": ["Be\u00b7nen\u00b7ne", "nur", "die", "Straf", "in", "der", "ich", "b\u00fcs\u00b7sen", "mu\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ART", "NN", "APPR", "PRELS", "PPER", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Soll ich in meiner Brust den blossen Dolch umbwenden/", "tokens": ["Soll", "ich", "in", "mei\u00b7ner", "Brust", "den", "blos\u00b7sen", "Dolch", "um\u00b7bwen\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "PPOSAT", "NN", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Ein Denckmal aufzubau\u2019n vor dieses Urtheils Schlu\u00df.", "tokens": ["Ein", "Denck\u00b7mal", "auf\u00b7zu\u00b7bau'n", "vor", "die\u00b7ses", "Ur\u00b7theils", "Schlu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PDAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Jtztleg\u2019 auf dein Altar viel hundert Opffer-Kuchen/", "tokens": ["Jtzt\u00b7leg'", "auf", "dein", "Al\u00b7tar", "viel", "hun\u00b7dert", "Opf\u00b7fer\u00b7Ku\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "ADV", "CARD", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Der fette Weyrauch soll bi\u00df an die Sterne gehn/", "tokens": ["Der", "fet\u00b7te", "Wey\u00b7rauch", "soll", "bi\u00df", "an", "die", "Ster\u00b7ne", "gehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "ADV", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.15": {"text": "Ja wilstu gar mein Blut zu dem Besprengen suchen/", "tokens": ["Ja", "wils\u00b7tu", "gar", "mein", "Blut", "zu", "dem", "Be\u00b7spren\u00b7gen", "su\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "VMFIN", "ADV", "PPOSAT", "NN", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Fur meine Laster will ich dir ein Opffer stehn.", "tokens": ["Fur", "mei\u00b7ne", "Las\u00b7ter", "will", "ich", "dir", "ein", "Opf\u00b7fer", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "VMFIN", "PPER", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Ach k\u00f6nt\u2019 ich Opffer seyn f\u00fcr so ein gro\u00df Verbrechen/", "tokens": ["Ach", "k\u00f6nt'", "ich", "Opf\u00b7fer", "seyn", "f\u00fcr", "so", "ein", "gro\u00df", "Ver\u00b7bre\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "NN", "VAINF", "APPR", "ADV", "ART", "ADJD", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Denn alle Straffe ist f\u00fcr meine Schuld zu klein/", "tokens": ["Denn", "al\u00b7le", "Straf\u00b7fe", "ist", "f\u00fcr", "mei\u00b7ne", "Schuld", "zu", "klein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VAFIN", "APPR", "PPOSAT", "NN", "PTKA", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Doch wirstu so ergrimmt di\u00df Thun nicht wollen r\u00e4chen/", "tokens": ["Doch", "wirs\u00b7tu", "so", "er\u00b7grimmt", "di\u00df", "Thun", "nicht", "wol\u00b7len", "r\u00e4\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "VVFIN", "PDS", "NN", "PTKNEG", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Da\u00df Blut und ein Altar hier werden n\u00f6thig seyn.", "tokens": ["Da\u00df", "Blut", "und", "ein", "Al\u00b7tar", "hier", "wer\u00b7den", "n\u00f6\u00b7thig", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "ART", "NN", "ADV", "VAFIN", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Wie oft hat nicht dein Stuhl den schw\u00e4chern Theil verziehen/", "tokens": ["Wie", "oft", "hat", "nicht", "dein", "Stuhl", "den", "schw\u00e4\u00b7chern", "Theil", "ver\u00b7zie\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VAFIN", "PTKNEG", "PPOSAT", "NN", "ART", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Und von dem Feinde Ruhm/ den du verschont gehabt.", "tokens": ["Und", "von", "dem", "Fein\u00b7de", "Ruhm", "/", "den", "du", "ver\u00b7schont", "ge\u00b7habt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "NN", "$(", "ART", "PPER", "VVPP", "VAPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Dein sanft Gem\u00fcthe kan von solchem Zorn nicht gl\u00fchen/", "tokens": ["Dein", "sanft", "Ge\u00b7m\u00fc\u00b7the", "kan", "von", "sol\u00b7chem", "Zorn", "nicht", "gl\u00fc\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "VMFIN", "APPR", "PIAT", "NN", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Da\u00df es sich nach Verdienst zu straffen je gelabt/", "tokens": ["Da\u00df", "es", "sich", "nach", "Ver\u00b7dienst", "zu", "straf\u00b7fen", "je", "ge\u00b7labt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "APPR", "NN", "PTKZU", "VVFIN", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Ach Schutz der Sterblichen und Heil der weiten Erden/", "tokens": ["Ach", "Schutz", "der", "Sterb\u00b7li\u00b7chen", "und", "Heil", "der", "wei\u00b7ten", "Er\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "ART", "NN", "KON", "NN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Zu dem die gantze Welt viel tausend W\u00fcnsche schickt.", "tokens": ["Zu", "dem", "die", "gant\u00b7ze", "Welt", "viel", "tau\u00b7send", "W\u00fcn\u00b7sche", "schickt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "ADJA", "NN", "ADV", "CARD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Zeig auch in deinem Zorn sanfftm\u00fcthige Geberden/", "tokens": ["Zeig", "auch", "in", "dei\u00b7nem", "Zorn", "sanfft\u00b7m\u00fct\u00b7hi\u00b7ge", "Ge\u00b7ber\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "APPR", "PPOSAT", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+---+-", "measure": "unknown.measure.penta"}, "line.28": {"text": "Die Hand die Schwerdte tr\u00e4gt von der werd\u2019 H\u00fclf erblickt/", "tokens": ["Die", "Hand", "die", "Schwerd\u00b7te", "tr\u00e4gt", "von", "der", "werd'", "H\u00fclf", "er\u00b7blickt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "APPR", "ART", "VAFIN", "NE", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Erlaubstu mir so viel (obschon die kl\u00fcgsten Worte/", "tokens": ["Er\u00b7laubs\u00b7tu", "mir", "so", "viel", "(", "ob\u00b7schon", "die", "kl\u00fcgs\u00b7ten", "Wor\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "$(", "KOUS", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Und der beredste Mund nichts meiner Sache n\u00fctzt)", "tokens": ["Und", "der", "be\u00b7reds\u00b7te", "Mund", "nichts", "mei\u00b7ner", "Sa\u00b7che", "n\u00fctzt", ")"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "PIS", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Da\u00df ich was weniges f\u00fcrbring\u2019 an deinen Ort/", "tokens": ["Da\u00df", "ich", "was", "we\u00b7ni\u00b7ges", "f\u00fcr\u00b7bring'", "an", "dei\u00b7nen", "Ort", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PWS", "PIS", "VVFIN", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "(so w\u00fcnsch ich) da\u00df zugleich dein Recht auch sey besch\u00fctzt.", "tokens": ["(", "so", "w\u00fcnsch", "ich", ")", "da\u00df", "zu\u00b7gleich", "dein", "Recht", "auch", "sey", "be\u00b7sch\u00fctzt", "."], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ADJD", "PPER", "$(", "KOUS", "ADV", "PPOSAT", "NN", "ADV", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Ach so vernein\u2019 ichs nicht/ ich bin befleckt mit S\u00fcnden/", "tokens": ["Ach", "so", "ver\u00b7nein'", "ichs", "nicht", "/", "ich", "bin", "be\u00b7fleckt", "mit", "S\u00fcn\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "VVFIN", "PIS", "PTKNEG", "$(", "PPER", "VAFIN", "VVPP", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Denn der gemeine Wahn der hat mich so verf\u00fchrt.", "tokens": ["Denn", "der", "ge\u00b7mei\u00b7ne", "Wahn", "der", "hat", "mich", "so", "ver\u00b7f\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "ART", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Jtzt will man alle Schuld mir auf den R\u00fccken binden/", "tokens": ["Jtzt", "will", "man", "al\u00b7le", "Schuld", "mir", "auf", "den", "R\u00fc\u00b7cken", "bin\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIS", "PIAT", "NN", "PPER", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Da jeder doch das Feur in Kohlen aufger\u00fchrt.", "tokens": ["Da", "je\u00b7der", "doch", "das", "Feur", "in", "Koh\u00b7len", "auf\u00b7ge\u00b7r\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "ART", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Wenn der gantze Rasende in Weine sich besoffen/", "tokens": ["Wenn", "der", "gant\u00b7ze", "Ra\u00b7sen\u00b7de", "in", "Wei\u00b7ne", "sich", "be\u00b7sof\u00b7fen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "APPR", "NN", "PRF", "VVPP", "$("], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.38": {"text": "Ein ander tolle Glut der Geilheit bey sich trug/", "tokens": ["Ein", "an\u00b7der", "tol\u00b7le", "Glut", "der", "Geil\u00b7heit", "bey", "sich", "trug", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "ART", "NN", "APPR", "PRF", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "So h\u00e4tten sie f\u00fcrs Schwerdt den Oele-Zweig zu hoffen/", "tokens": ["So", "h\u00e4t\u00b7ten", "sie", "f\u00fcrs", "Schwerdt", "den", "O\u00b7ele\u00b7Zweig", "zu", "hof\u00b7fen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPRART", "NN", "ART", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Und da\u00df die Donner-Hand nicht drauf mit Keulen schlug.", "tokens": ["Und", "da\u00df", "die", "Don\u00b7ner\u00b7Hand", "nicht", "drauf", "mit", "Keu\u00b7len", "schlug", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "PTKNEG", "PAV", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Soll ich denn ewiglich im Zwiekampf mit dir leben/", "tokens": ["Soll", "ich", "denn", "e\u00b7wig\u00b7lich", "im", "Zwie\u00b7kampf", "mit", "dir", "le\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADV", "APPRART", "NN", "APPR", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Wilstu ein Fechter stets mich zu ermorden seyn/", "tokens": ["Wils\u00b7tu", "ein", "Fech\u00b7ter", "stets", "mich", "zu", "er\u00b7mor\u00b7den", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "VVFIN", "PPER", "PTKZU", "VVINF", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Ists nicht genug da\u00df ich dir will ", "tokens": ["Ists", "nicht", "ge\u00b7nug", "da\u00df", "ich", "dir", "will"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "PTKNEG", "ADV", "KOUS", "PPER", "PPER", "VMFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.44": {"text": "Und sagen meine Schuld verdienet Zorn und Pein.", "tokens": ["Und", "sa\u00b7gen", "mei\u00b7ne", "Schuld", "ver\u00b7die\u00b7net", "Zorn", "und", "Pein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "VVFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Werd\u2019 ich dir nicht zu gleich den sch\u00f6nsten Anla\u00df machen/", "tokens": ["Werd'", "ich", "dir", "nicht", "zu", "gleich", "den", "sch\u00f6ns\u00b7ten", "An\u00b7la\u00df", "ma\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "PTKNEG", "PTKA", "ADV", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "Da\u00df meine Missethat dir noch zum Ruhm gedeyt/", "tokens": ["Da\u00df", "mei\u00b7ne", "Mis\u00b7se\u00b7that", "dir", "noch", "zum", "Ruhm", "ge\u00b7deyt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "ADV", "APPRART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "H\u00e4tt ich mich nicht befleckt/ verg\u00e4bstu nicht die Sachen/", "tokens": ["H\u00e4tt", "ich", "mich", "nicht", "be\u00b7fleckt", "/", "ver\u00b7g\u00e4bs\u00b7tu", "nicht", "die", "Sa\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "PTKNEG", "VVPP", "$(", "VVFIN", "PTKNEG", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.48": {"text": "Und niemand priese dich ob deiner G\u00fctigkeit.", "tokens": ["Und", "nie\u00b7mand", "prie\u00b7se", "dich", "ob", "dei\u00b7ner", "G\u00fc\u00b7tig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "PPER", "KOUS", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}