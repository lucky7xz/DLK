{"dta.poem.18810": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang von", "birth": "N.A.", "death": "N.A."}, "title": "Geheimschrift .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1819", "urn": "urn:nbn:de:kobv:b4-200905191692", "language": ["de:0.99"], "booktitle": "Goethe, Johann Wolfgang von: West-\u00f6stlicher Divan. Stuttgart, 1819."}, "poem": {"stanza.1": {"line.1": {"text": "Lasst euch, o Diplomaten!", "tokens": ["Lasst", "euch", ",", "o", "Dip\u00b7lo\u00b7ma\u00b7ten", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "FM", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Recht angelegen seyn,", "tokens": ["Recht", "an\u00b7ge\u00b7le\u00b7gen", "seyn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVPP", "VAINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und eure Potentaten", "tokens": ["Und", "eu\u00b7re", "Po\u00b7ten\u00b7ta\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["KON", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Berathet rein und fein.", "tokens": ["Be\u00b7ra\u00b7thet", "rein", "und", "fein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Geheimer Chiffern Sendung", "tokens": ["Ge\u00b7hei\u00b7mer", "Chif\u00b7fern", "Sen\u00b7dung"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Besch\u00e4ftige die Welt,", "tokens": ["Be\u00b7sch\u00e4f\u00b7ti\u00b7ge", "die", "Welt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Bis endlich jede Wendung", "tokens": ["Bis", "end\u00b7lich", "je\u00b7de", "Wen\u00b7dung"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADV", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Sich selbst in\u2019s Gleiche stellt.", "tokens": ["Sich", "selbst", "in's", "Glei\u00b7che", "stellt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Mir von der Herrinn s\u00fcsse", "tokens": ["Mir", "von", "der", "Her\u00b7rinn", "s\u00fcs\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "APPR", "ART", "NN", "VVFIN"], "meter": "++-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Chiffer ist zur Hand,", "tokens": ["Die", "Chif\u00b7fer", "ist", "zur", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Woran ich schon geniesse,", "tokens": ["Wo\u00b7ran", "ich", "schon", "ge\u00b7nies\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Weil sie die Kunst erfand.", "tokens": ["Weil", "sie", "die", "Kunst", "er\u00b7fand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Es ist die Liebesf\u00fclle", "tokens": ["Es", "ist", "die", "Lie\u00b7bes\u00b7f\u00fcl\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Im lieblichsten Revier,", "tokens": ["Im", "lieb\u00b7lichs\u00b7ten", "Re\u00b7vier", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+---+", "measure": "dactylic.init"}, "line.7": {"text": "Der holde, treue Wille", "tokens": ["Der", "hol\u00b7de", ",", "treu\u00b7e", "Wil\u00b7le"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Wie zwischen mir und ihr.", "tokens": ["Wie", "zwi\u00b7schen", "mir", "und", "ihr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "PPER", "KON", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Von abertausend Bl\u00fcten", "tokens": ["Von", "a\u00b7bert\u00b7au\u00b7send", "Bl\u00fc\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJD", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ist es ein bunter Strauss,", "tokens": ["Ist", "es", "ein", "bun\u00b7ter", "Strauss", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "---+-+", "measure": "unknown.measure.di"}, "line.3": {"text": "Von englischen Gem\u00fcthen", "tokens": ["Von", "eng\u00b7li\u00b7schen", "Ge\u00b7m\u00fc\u00b7then"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein vollbewohntes Haus;", "tokens": ["Ein", "voll\u00b7be\u00b7wohn\u00b7tes", "Haus", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Von buntesten Gefiedern", "tokens": ["Von", "bun\u00b7tes\u00b7ten", "Ge\u00b7fie\u00b7dern"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der Himmel \u00fcbers\u00e4\u2019t,", "tokens": ["Der", "Him\u00b7mel", "\u00fc\u00b7ber\u00b7s\u00e4't", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ein klingend Meer von Liedern", "tokens": ["Ein", "klin\u00b7gend", "Meer", "von", "Lie\u00b7dern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "NN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Geruchvoll \u00fcberweht.", "tokens": ["Ge\u00b7ruch\u00b7voll", "\u00fc\u00b7ber\u00b7weht", "."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ist unbedingten Strebens", "tokens": ["Ist", "un\u00b7be\u00b7ding\u00b7ten", "Stre\u00b7bens"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Geheime Doppelschrift,", "tokens": ["Ge\u00b7hei\u00b7me", "Dop\u00b7pel\u00b7schrift", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die in das Mark des Lebens", "tokens": ["Die", "in", "das", "Mark", "des", "Le\u00b7bens"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wie Pfeil um Pfeile trifft.", "tokens": ["Wie", "Pfeil", "um", "Pfei\u00b7le", "trifft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Was ich euch offenbaret", "tokens": ["Was", "ich", "euch", "of\u00b7fen\u00b7ba\u00b7ret"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "PPER", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "War l\u00e4ngst ein frommer Brauch,", "tokens": ["War", "l\u00e4ngst", "ein", "from\u00b7mer", "Brauch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und wenn ihr es gewahret,", "tokens": ["Und", "wenn", "ihr", "es", "ge\u00b7wah\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "So schweigt und nutzt es auch.", "tokens": ["So", "schweigt", "und", "nutzt", "es", "auch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}