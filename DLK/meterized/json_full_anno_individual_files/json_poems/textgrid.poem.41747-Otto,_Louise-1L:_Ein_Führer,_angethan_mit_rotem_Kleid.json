{"textgrid.poem.41747": {"metadata": {"author": {"name": "Otto, Louise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein F\u00fchrer, angethan mit rotem Kleid", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein F\u00fchrer, angethan mit rotem Kleid", "tokens": ["Ein", "F\u00fch\u00b7rer", ",", "an\u00b7ge\u00b7than", "mit", "ro\u00b7tem", "Kleid"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "VVPP", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Zeigt mir gesch\u00e4ftiglich des Domes Pracht,", "tokens": ["Zeigt", "mir", "ge\u00b7sch\u00e4f\u00b7tig\u00b7lich", "des", "Do\u00b7mes", "Pracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die funkelnde, die gold'ne Herrlichkeit,", "tokens": ["Die", "fun\u00b7keln\u00b7de", ",", "die", "gold'\u00b7ne", "Herr\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die manch Jahrhundert sorgsam hier bewacht.", "tokens": ["Die", "manch", "Jahr\u00b7hun\u00b7dert", "sorg\u00b7sam", "hier", "be\u00b7wacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "ADJD", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Da stehn die Heiligen aus lauterm Gold", "tokens": ["Da", "stehn", "die", "Hei\u00b7li\u00b7gen", "aus", "lau\u00b7term", "Gold"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und die Madonna tr\u00e4gt ein Prachtgewand,", "tokens": ["Und", "die", "Ma\u00b7don\u00b7na", "tr\u00e4gt", "ein", "Pracht\u00b7ge\u00b7wand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Aus vielen Bildern gr\u00fc\u00dft sie sch\u00f6n und hold,", "tokens": ["Aus", "vie\u00b7len", "Bil\u00b7dern", "gr\u00fc\u00dft", "sie", "sch\u00f6n", "und", "hold", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Auf ihrem Arm der Welterl\u00f6sung Pfand.", "tokens": ["Auf", "ih\u00b7rem", "Arm", "der", "Wel\u00b7ter\u00b7l\u00f6\u00b7sung", "Pfand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ein Murmeln durch die hohen R\u00e4ume klang,", "tokens": ["Ein", "Mur\u00b7meln", "durch", "die", "ho\u00b7hen", "R\u00e4u\u00b7me", "klang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Es war ein Flehen von zerlumpten Frauen,", "tokens": ["Es", "war", "ein", "Fle\u00b7hen", "von", "zer\u00b7lump\u00b7ten", "Frau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die bl\u00f6d' gefolgt des Elends dumpfem Drang,", "tokens": ["Die", "bl\u00f6d'", "ge\u00b7folgt", "des", "E\u00b7lends", "dum\u00b7pfem", "Drang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Durch lautes Beten hier sich zu erbauen.", "tokens": ["Durch", "lau\u00b7tes", "Be\u00b7ten", "hier", "sich", "zu", "er\u00b7bau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "PRF", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Es war ihr Antlitz bleich und abgezehrt,", "tokens": ["Es", "war", "ihr", "Ant\u00b7litz", "bleich", "und", "ab\u00b7ge\u00b7zehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "ADJD", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Drauf las man viel von bittrer Pein und Not,", "tokens": ["Drauf", "las", "man", "viel", "von", "bit\u00b7trer", "Pein", "und", "Not", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PIS", "ADV", "APPR", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und jenes Flehn, das ihnen nicht erh\u00f6rt,", "tokens": ["Und", "je\u00b7nes", "Flehn", ",", "das", "ih\u00b7nen", "nicht", "er\u00b7h\u00f6rt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVINF", "$,", "PRELS", "PPER", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Die hei\u00dfe Bitte: \u2013 \u00bbGieb uns unser Brot!\u00ab", "tokens": ["Die", "hei\u00b7\u00dfe", "Bit\u00b7te", ":", "\u2013", "\u00bb", "Gieb", "uns", "un\u00b7ser", "Brot", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "$(", "$(", "VVIMP", "PPER", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ihr Armen, die Madonna hilft Euch nicht,", "tokens": ["Ihr", "Ar\u00b7men", ",", "die", "Ma\u00b7don\u00b7na", "hilft", "Euch", "nicht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ART", "NN", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Kein Herz schl\u00e4gt unterm Sammet und Brocat \u2013", "tokens": ["Kein", "Herz", "schl\u00e4gt", "un\u00b7term", "Sam\u00b7met", "und", "Bro\u00b7cat", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "APPRART", "NN", "KON", "NN", "$("], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und schaut Ihr zu der ew'gen Lampe Licht \u2013", "tokens": ["Und", "schaut", "Ihr", "zu", "der", "ew'\u00b7gen", "Lam\u00b7pe", "Licht", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ach, auch von dorther keine Rettung naht.", "tokens": ["Ach", ",", "auch", "von", "dor\u00b7ther", "kei\u00b7ne", "Ret\u00b7tung", "naht", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADV", "APPR", "ADV", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Umsonst blickt Ihr zu dieser Heil'gen Schar,", "tokens": ["Um\u00b7sonst", "blickt", "Ihr", "zu", "die\u00b7ser", "Heil'\u00b7gen", "Schar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Habt ihr Verehrung br\u00fcnstiglich gezollt,", "tokens": ["Habt", "ihr", "Ver\u00b7eh\u00b7rung", "br\u00fcns\u00b7tig\u00b7lich", "ge\u00b7zollt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Umsonst zu diesem prunkenden Altar,", "tokens": ["Um\u00b7sonst", "zu", "die\u00b7sem", "prun\u00b7ken\u00b7den", "Al\u00b7tar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Umsonst \u2013 ", "tokens": ["Um\u00b7sonst", "\u2013"], "token_info": ["word", "punct"], "pos": ["ADV", "$("], "meter": "-+", "measure": "iambic.single"}}, "stanza.4": {"line.1": {"text": "Ja, diese Pracht und diese Herrlichkeit", "tokens": ["Ja", ",", "die\u00b7se", "Pracht", "und", "die\u00b7se", "Herr\u00b7lich\u00b7keit"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "PDAT", "NN", "KON", "PDAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sind Eures M\u00fchens Schwei\u00df, sind Euer Blut!", "tokens": ["Sind", "Eu\u00b7res", "M\u00fc\u00b7hens", "Schwei\u00df", ",", "sind", "Eu\u00b7er", "Blut", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "NN", "$,", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Kein Segen ist's, ein Fluch aus alter Zeit,", "tokens": ["Kein", "Se\u00b7gen", "ist's", ",", "ein", "Fluch", "aus", "al\u00b7ter", "Zeit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "$,", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der f\u00fcr das Volk in solchen Tempel ruht.", "tokens": ["Der", "f\u00fcr", "das", "Volk", "in", "sol\u00b7chen", "Tem\u00b7pel", "ruht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Das ist ein G\u00f6tzendienst, ein Frevelspiel", "tokens": ["Das", "ist", "ein", "G\u00f6t\u00b7zen\u00b7dienst", ",", "ein", "Fre\u00b7vel\u00b7spiel"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Mit Menschenhoheit und mit Menschenrecht,", "tokens": ["Mit", "Men\u00b7schen\u00b7ho\u00b7heit", "und", "mit", "Men\u00b7schen\u00b7recht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Verdummung nur ist dieses Prunkes Ziel,", "tokens": ["Ver\u00b7dum\u00b7mung", "nur", "ist", "die\u00b7ses", "Prun\u00b7kes", "Ziel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VAFIN", "PDAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Damit der Fromme werd' ein Pfaffenknecht.", "tokens": ["Da\u00b7mit", "der", "From\u00b7me", "werd'", "ein", "Pfaf\u00b7fen\u00b7knecht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Hinaus, hinaus aus diesem kalten Dom", "tokens": ["Hin\u00b7aus", ",", "hin\u00b7aus", "aus", "die\u00b7sem", "kal\u00b7ten", "Dom"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "ADV", "APPR", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und seiner weihrauchvollen, d\u00fcstern Nacht!", "tokens": ["Und", "sei\u00b7ner", "weih\u00b7rauch\u00b7vol\u00b7len", ",", "d\u00fcs\u00b7tern", "Nacht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "$,", "VVFIN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Hinweg, hinweg von diesem alten Rom,", "tokens": ["Hin\u00b7weg", ",", "hin\u00b7weg", "von", "die\u00b7sem", "al\u00b7ten", "Rom", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "APPR", "PDAT", "ADJA", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Da\u00df unserm Deutschland stets nur Schmach gebracht.", "tokens": ["Da\u00df", "un\u00b7serm", "Deutschland", "stets", "nur", "Schmach", "ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "ADV", "NN", "VVPP", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "O da\u00df Ihr doch das hohe Wort vern\u00e4hmt:", "tokens": ["O", "da\u00df", "Ihr", "doch", "das", "ho\u00b7he", "Wort", "ver\u00b7n\u00e4hmt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOUS", "PPER", "ADV", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Das Vaterland die Menschheit l\u00e4uft Gefahr,", "tokens": ["Das", "Va\u00b7ter\u00b7land", "die", "Menschheit", "l\u00e4uft", "Ge\u00b7fahr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.7": {"text": "Wenn Ihr Euch nicht jedweder Knechtschaft sch\u00e4mt", "tokens": ["Wenn", "Ihr", "Euch", "nicht", "jed\u00b7we\u00b7der", "Knecht\u00b7schaft", "sch\u00e4mt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "PTKNEG", "PIAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Als eine treue, deutsche K\u00e4mpferschar.", "tokens": ["Als", "ei\u00b7ne", "treu\u00b7e", ",", "deut\u00b7sche", "K\u00e4mp\u00b7fer\u00b7schar", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Erst\u00fcndet Ihr mit einem Jubelschrei", "tokens": ["Er\u00b7st\u00fcn\u00b7det", "Ihr", "mit", "ei\u00b7nem", "Ju\u00b7bel\u00b7schrei"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Vernichtet w\u00e4r' das finstre Pfaffenreich,", "tokens": ["Ver\u00b7nich\u00b7tet", "w\u00e4r'", "das", "finst\u00b7re", "Pfaf\u00b7fen\u00b7reich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Vers\u00e4nk' in Nacht \u2013 Ihr aber w\u00e4ret ", "tokens": ["Ver\u00b7s\u00e4nk'", "in", "Nacht", "\u2013", "Ihr", "a\u00b7ber", "w\u00e4\u00b7ret"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "APPR", "NN", "$(", "PPER", "ADV", "VAFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ihr w\u00e4r't noch mehr, Ihr w\u00e4ret alle ", "tokens": ["Ihr", "w\u00e4r't", "noch", "mehr", ",", "Ihr", "w\u00e4\u00b7ret", "al\u00b7le"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "$,", "PPER", "VAFIN", "PIAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Kein Priester mehr, kein zwingendes Gebot,", "tokens": ["Kein", "Pries\u00b7ter", "mehr", ",", "kein", "zwin\u00b7gen\u00b7des", "Ge\u00b7bot", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "$,", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Kein G\u00f6tzendienst zu einer Kirche Ruhm,", "tokens": ["Kein", "G\u00f6t\u00b7zen\u00b7dienst", "zu", "ei\u00b7ner", "Kir\u00b7che", "Ruhm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Ein F\u00fchrer, angethan mit rotem Kleid", "tokens": ["Ein", "F\u00fch\u00b7rer", ",", "an\u00b7ge\u00b7than", "mit", "ro\u00b7tem", "Kleid"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "VVPP", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Zeigt mir gesch\u00e4ftiglich des Domes Pracht,", "tokens": ["Zeigt", "mir", "ge\u00b7sch\u00e4f\u00b7tig\u00b7lich", "des", "Do\u00b7mes", "Pracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die funkelnde, die gold'ne Herrlichkeit,", "tokens": ["Die", "fun\u00b7keln\u00b7de", ",", "die", "gold'\u00b7ne", "Herr\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ART", "ADJ