{"dta.poem.11208": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Kleinod in gestalt eines schwanes.  \n R.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mjr fehlt die stimm: Jedoch wird man ein kleinod bringen,", "tokens": ["Mjr", "fehlt", "die", "stimm", ":", "Je\u00b7doch", "wird", "man", "ein", "klei\u00b7nod", "brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "ADJD", "$.", "ADV", "VAFIN", "PIS", "ART", "NN", "VVINF", "$,"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Das sch\u00f6ner ist als ich; solt ihr mich h\u00f6ren singen.", "tokens": ["Das", "sch\u00f6\u00b7ner", "ist", "als", "ich", ";", "solt", "ihr", "mich", "h\u00f6\u00b7ren", "sin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VAFIN", "KOUS", "PPER", "$.", "VMFIN", "PPER", "PRF", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}