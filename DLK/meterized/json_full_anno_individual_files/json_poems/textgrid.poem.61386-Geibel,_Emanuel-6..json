{"textgrid.poem.61386": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "6.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Durch die erstorbnen Gassen,", "tokens": ["Durch", "die", "ers\u00b7torb\u00b7nen", "Gas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Die kalt im fahlen Mondenschimmer liegen,", "tokens": ["Die", "kalt", "im", "fah\u00b7len", "Mon\u00b7den\u00b7schim\u00b7mer", "lie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Durch Pfeilerhallen, \u00fcber Marmorstiegen", "tokens": ["Durch", "Pfei\u00b7ler\u00b7hal\u00b7len", ",", "\u00fc\u00b7ber", "Mar\u00b7mor\u00b7stie\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "$,", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Schweif' ich umher verlassen", "tokens": ["Schweif'", "ich", "um\u00b7her", "ver\u00b7las\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "PTKVZ", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und denk' in Gram versenket", "tokens": ["Und", "denk'", "in", "Gram", "ver\u00b7sen\u00b7ket"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "NE", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "An dich, die meiner nimmermehr gedenket.", "tokens": ["An", "dich", ",", "die", "mei\u00b7ner", "nim\u00b7mer\u00b7mehr", "ge\u00b7den\u00b7ket", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "PRELS", "PPOSAT", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wie unter schweren Lasten", "tokens": ["Wie", "un\u00b7ter", "schwe\u00b7ren", "Las\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein Mann vom Holzschlag keucht auf Waldespfaden,", "tokens": ["Ein", "Mann", "vom", "Holz\u00b7schlag", "keucht", "auf", "Wal\u00b7des\u00b7pfa\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So seufz' ich mit des Kummers Wucht beladen,", "tokens": ["So", "seuf\u00b7z'", "ich", "mit", "des", "Kum\u00b7mers", "Wucht", "be\u00b7la\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "NN", "VVPP", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Der nicht verg\u00f6nnt zu rasten", "tokens": ["Der", "nicht", "ver\u00b7g\u00f6nnt", "zu", "ras\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PTKNEG", "VVPP", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und weiter ohn' Ermatten", "tokens": ["Und", "wei\u00b7ter", "ohn'", "Er\u00b7mat\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Mich forttreibt, umzugehn, mein eigner Schatten.", "tokens": ["Mich", "fort\u00b7treibt", ",", "um\u00b7zu\u00b7gehn", ",", "mein", "eig\u00b7ner", "Schat\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVIZU", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und f\u00fchrt zu deiner Schwelle", "tokens": ["Und", "f\u00fchrt", "zu", "dei\u00b7ner", "Schwel\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mein Weg mich, der da wei\u00df von keinem Ziele:", "tokens": ["Mein", "Weg", "mich", ",", "der", "da", "wei\u00df", "von", "kei\u00b7nem", "Zie\u00b7le", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "$,", "PRELS", "ADV", "VVFIN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Rankt meine Seele sich in leerem Spiele", "tokens": ["Rankt", "mei\u00b7ne", "See\u00b7le", "sich", "in", "lee\u00b7rem", "Spie\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "PRF", "APPR", "ADJA", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Um die geliebte Stelle;", "tokens": ["Um", "die", "ge\u00b7lieb\u00b7te", "Stel\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ich steh' gebannt und weine", "tokens": ["Ich", "steh'", "ge\u00b7bannt", "und", "wei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "KON", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Brennende Tr\u00e4nen auf die kalten Steine.", "tokens": ["Bren\u00b7nen\u00b7de", "Tr\u00e4\u00b7nen", "auf", "die", "kal\u00b7ten", "Stei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos"