{"dta.poem.239": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "An die Nacht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Komm schwartze Nacht/ du stille Finsterni\u00df/", "tokens": ["Komm", "schwart\u00b7ze", "Nacht", "/", "du", "stil\u00b7le", "Fins\u00b7ter\u00b7ni\u00df", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "$(", "PPER", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Umh\u00fclle mich mit deinem braunen Schatten/", "tokens": ["Um\u00b7h\u00fcl\u00b7le", "mich", "mit", "dei\u00b7nem", "brau\u00b7nen", "Schat\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Du blasser Mond zeig mir mein g\u00fcldnes Vlie\u00df/", "tokens": ["Du", "blas\u00b7ser", "Mond", "zeig", "mir", "mein", "g\u00fcld\u00b7nes", "Vlie\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "La\u00df ungest\u00f6hrt mich mit ", "tokens": ["La\u00df", "un\u00b7ge\u00b7st\u00f6hrt", "mich", "mit"], "token_info": ["word", "word", "word", "word"], "pos": ["VVIMP", "ADJD", "PRF", "APPR"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Jhr Sternen z\u00fcndt die hellen Fackeln an/", "tokens": ["Ihr", "Ster\u00b7nen", "z\u00fcndt", "die", "hel\u00b7len", "Fa\u00b7ckeln", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Da\u00df ich den Port der Wollust finden kan.", "tokens": ["Da\u00df", "ich", "den", "Port", "der", "Wol\u00b7lust", "fin\u00b7den", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Mein Geist verlangt den Haven bald zu sehn/", "tokens": ["Mein", "Geist", "ver\u00b7langt", "den", "Ha\u00b7ven", "bald", "zu", "sehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN", "ADV", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Den Alabaster und Corallen zieren;", "tokens": ["Den", "A\u00b7la\u00b7bas\u00b7ter", "und", "Co\u00b7ral\u00b7len", "zie\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Enge/ wo nur kan ein Schifflein gehn/", "tokens": ["Die", "En\u00b7ge", "/", "wo", "nur", "kan", "ein", "Schif\u00b7flein", "gehn", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PWAV", "ADV", "VMFIN", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Soll ihn ins Land der s\u00fc\u00dften L\u00fcste f\u00fchren/", "tokens": ["Soll", "ihn", "ins", "Land", "der", "s\u00fc\u00df\u00b7ten", "L\u00fcs\u00b7te", "f\u00fch\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPRART", "NN", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Der ", "tokens": ["Der"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.6": {"text": "So f\u00e4hret er vergn\u00fcgt zum Haven ein.", "tokens": ["So", "f\u00e4h\u00b7ret", "er", "ver\u00b7gn\u00fcgt", "zum", "Ha\u00b7ven", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVPP", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wie/ winckt mein Licht nicht albereits von fern/", "tokens": ["Wie", "/", "winckt", "mein", "Licht", "nicht", "al\u00b7be\u00b7reits", "von", "fern", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$(", "VVFIN", "PPOSAT", "NN", "PTKNEG", "ADV", "APPR", "ADJD", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und rufft mir zu/ im Seegeln fort zu eilen/", "tokens": ["Und", "rufft", "mir", "zu", "/", "im", "See\u00b7geln", "fort", "zu", "ei\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKZU", "$(", "APPRART", "NN", "PTKVZ", "PTKZU", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ja! nun wohlan! ich folge diesem Stern/", "tokens": ["Ja", "!", "nun", "wo\u00b7hlan", "!", "ich", "fol\u00b7ge", "die\u00b7sem", "Stern", "/"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "ADV", "ADV", "$.", "PPER", "VVFIN", "PDAT", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der Tag will auch nicht l\u00e4nger mehr verweilen/", "tokens": ["Der", "Tag", "will", "auch", "nicht", "l\u00e4n\u00b7ger", "mehr", "ver\u00b7wei\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ADV", "PTKNEG", "ADJD", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Die Sonne sucht im Meere ihre Ruh/", "tokens": ["Die", "Son\u00b7ne", "sucht", "im", "Mee\u00b7re", "ih\u00b7re", "Ruh", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und ich/ ich eile nach ", "tokens": ["Und", "ich", "/", "ich", "ei\u00b7le", "nach"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "$(", "PPER", "VVFIN", "APPR"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ich f\u00fchle schon die Liljen weiche Hand/", "tokens": ["Ich", "f\u00fch\u00b7le", "schon", "die", "Lil\u00b7jen", "wei\u00b7che", "Hand", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ich seh die Brust mit Wollust-Rosen spielen/", "tokens": ["Ich", "seh", "die", "Brust", "mit", "Wol\u00b7lust\u00b7Ro\u00b7sen", "spie\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und ferner fort das angenehme Land/", "tokens": ["Und", "fer\u00b7ner", "fort", "das", "an\u00b7ge\u00b7neh\u00b7me", "Land", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PTKVZ", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Das meine Brunst ist willig abzuk\u00fchlen.", "tokens": ["Das", "mei\u00b7ne", "Brunst", "ist", "wil\u00b7lig", "ab\u00b7zu\u00b7k\u00fch\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "NN", "VAFIN", "ADJD", "VVIZU", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Das/ was mein Schiff nun eingeladen hat/", "tokens": ["Das", "/", "was", "mein", "Schiff", "nun", "ein\u00b7ge\u00b7la\u00b7den", "hat", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$(", "PWS", "PPOSAT", "NN", "ADV", "VVPP", "VAFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Das bleibet dir ", "tokens": ["Das", "blei\u00b7bet", "dir"], "token_info": ["word", "word", "word"], "pos": ["PDS", "VVFIN", "PPER"], "meter": "-+-+", "measure": "iambic.di"}}}}}