{"dta.poem.7433": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Schlechter Trost.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192430", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du wirst ein sch\u00f6ner Leben schauen,", "tokens": ["Du", "wirst", "ein", "sch\u00f6\u00b7ner", "Le\u00b7ben", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und ewig, ewig bleibt es Dein;", "tokens": ["Und", "e\u00b7wig", ",", "e\u00b7wig", "bleibt", "es", "Dein", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "ADJD", "VVFIN", "PPER", "PPOSAT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Man wird Dir goldne Schl\u00f6sser bauen,", "tokens": ["Man", "wird", "Dir", "gold\u00b7ne", "Schl\u00f6s\u00b7ser", "bau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PPER", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nur \u2014 mu\u00dft Du erst gestorben sein!", "tokens": ["Nur", "mu\u00dft", "Du", "erst", "ge\u00b7stor\u00b7ben", "sein", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "VMFIN", "PPER", "ADV", "VVPP", "VAINF", "$."], "meter": "----+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Du wirst bis zu den Sternen dringen,", "tokens": ["Du", "wirst", "bis", "zu", "den", "Ster\u00b7nen", "drin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und stellen Dich in ihre Reihn,", "tokens": ["Und", "stel\u00b7len", "Dich", "in", "ih\u00b7re", "Reihn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Von Welten Dich zu Welten schwingen,", "tokens": ["Von", "Wel\u00b7ten", "Dich", "zu", "Wel\u00b7ten", "schwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nur \u2014 mu\u00dft Du erst gestorben sein.", "tokens": ["Nur", "mu\u00dft", "Du", "erst", "ge\u00b7stor\u00b7ben", "sein", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "VMFIN", "PPER", "ADV", "VVPP", "VAINF", "$."], "meter": "----+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "Du wirst, ein freier Brutus, wallen", "tokens": ["Du", "wirst", ",", "ein", "frei\u00b7er", "Bru\u00b7tus", ",", "wal\u00b7len"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["PPER", "VAFIN", "$,", "ART", "ADJA", "NE", "$,", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit Brutussen noch im Verein,", "tokens": ["Mit", "Bru\u00b7tus\u00b7sen", "noch", "im", "Ver\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "APPRART", "NN", "$,"], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.3": {"text": "All' Deine Ketten werden fallen,", "tokens": ["All'", "Dei\u00b7ne", "Ket\u00b7ten", "wer\u00b7den", "fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "NN", "VAFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nur \u2014 mu\u00dft Du erst gestorben sein.", "tokens": ["Nur", "mu\u00dft", "Du", "erst", "ge\u00b7stor\u00b7ben", "sein", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "VMFIN", "PPER", "ADV", "VVPP", "VAINF", "$."], "meter": "----+--+", "measure": "iambic.di.chol"}}, "stanza.4": {"line.1": {"text": "Wenn S\u00fcnder in der H\u00f6lle braten,", "tokens": ["Wenn", "S\u00fcn\u00b7der", "in", "der", "H\u00f6l\u00b7le", "bra\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So gehest Du zum Himmel ein;", "tokens": ["So", "ge\u00b7hest", "Du", "zum", "Him\u00b7mel", "ein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du wirst gek\u00fc\u00dft und nicht verrathen,", "tokens": ["Du", "wirst", "ge\u00b7k\u00fc\u00dft", "und", "nicht", "ver\u00b7ra\u00b7then", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "KON", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nur \u2014 mu\u00dft Du erst gestorben sein. \u2014 \u2014", "tokens": ["Nur", "mu\u00dft", "Du", "erst", "ge\u00b7stor\u00b7ben", "sein", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ADV", "$(", "VMFIN", "PPER", "ADV", "VVPP", "VAINF", "$.", "$(", "$("], "meter": "----+--+", "measure": "iambic.di.chol"}}, "stanza.5": {"line.1": {"text": "Ob ihm der Ost die Segel bl\u00e4he,", "tokens": ["Ob", "ihm", "der", "Ost", "die", "Se\u00b7gel", "bl\u00e4\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Was hilft's dem morschen, lecken Kahn?", "tokens": ["Was", "hilft's", "dem", "mor\u00b7schen", ",", "le\u00b7cken", "Kahn", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Was hilft dem Vogel die Sonnenn\u00e4he,", "tokens": ["Was", "hilft", "dem", "Vo\u00b7gel", "die", "Son\u00b7nen\u00b7n\u00e4\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Den ", "tokens": ["Den"], "token_info": ["word"], "pos": ["ART"], "meter": "+", "measure": "single.up"}}}}}