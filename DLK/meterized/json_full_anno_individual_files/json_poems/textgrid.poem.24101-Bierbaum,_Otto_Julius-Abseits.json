{"textgrid.poem.24101": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Abseits", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn, stolzes M\u00e4dchen, du mich fragen w\u00fcrdest,", "tokens": ["Wenn", ",", "stol\u00b7zes", "M\u00e4d\u00b7chen", ",", "du", "mich", "fra\u00b7gen", "w\u00fcr\u00b7dest", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "ADJA", "NN", "$,", "PPER", "PRF", "VVINF", "VAFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das nie mich fragen wird: Was giebst du mir,", "tokens": ["Das", "nie", "mich", "fra\u00b7gen", "wird", ":", "Was", "giebst", "du", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "PPER", "VVINF", "VAFIN", "$.", "PWS", "VVFIN", "PPER", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df ich dir meine Jugend schenken soll,", "tokens": ["Da\u00df", "ich", "dir", "mei\u00b7ne", "Ju\u00b7gend", "schen\u00b7ken", "soll", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "PPOSAT", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mein M\u00e4dchenleben und dies reine Herz ...?", "tokens": ["Mein", "M\u00e4d\u00b7chen\u00b7le\u00b7ben", "und", "dies", "rei\u00b7ne", "Herz", "...", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "KON", "PDS", "ADJA", "NN", "$(", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Was k\u00f6nnt ich sagen?! Ach, ich habe nichts.", "tokens": ["Was", "k\u00f6nnt", "ich", "sa\u00b7gen", "?!", "Ach", ",", "ich", "ha\u00b7be", "nichts", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "VVINF", "$.", "ITJ", "$,", "PPER", "VAFIN", "PIS", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Bin allzulange schon kein Knabe mehr,", "tokens": ["Bin", "all\u00b7zu\u00b7lan\u00b7ge", "schon", "kein", "Kna\u00b7be", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "PIAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Nicht sch\u00f6n, nicht reich, kein gro\u00dfer Herr und Held,", "tokens": ["Nicht", "sch\u00f6n", ",", "nicht", "reich", ",", "kein", "gro\u00b7\u00dfer", "Herr", "und", "Held", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "$,", "PTKNEG", "ADJD", "$,", "PIAT", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Doch auch kein wunderlicher, feiner Narr;", "tokens": ["Doch", "auch", "kein", "wun\u00b7der\u00b7li\u00b7cher", ",", "fei\u00b7ner", "Narr", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "PIAT", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Trag keinen Orden, hab kein hohes Amt,", "tokens": ["Trag", "kei\u00b7nen", "Or\u00b7den", ",", "hab", "kein", "ho\u00b7hes", "Amt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "$,", "VAFIN", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Und kam nicht unbeschmutzt aus diesem Kampf,", "tokens": ["Und", "kam", "nicht", "un\u00b7be\u00b7schmutzt", "aus", "die\u00b7sem", "Kampf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "ADJD", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Der mein Geschick war, und in dem mein Herz", "tokens": ["Der", "mein", "Ge\u00b7schick", "war", ",", "und", "in", "dem", "mein", "Herz"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "PPOSAT", "NN", "VAFIN", "$,", "KON", "APPR", "ART", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Hart ward und grimmig als ein Kriegerherz.", "tokens": ["Hart", "ward", "und", "grim\u00b7mig", "als", "ein", "Krie\u00b7ger\u00b7herz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "KON", "ADJD", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Auch bin ich m\u00fcde und kein T\u00e4nzer mehr,", "tokens": ["Auch", "bin", "ich", "m\u00fc\u00b7de", "und", "kein", "T\u00e4n\u00b7zer", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "KON", "PIAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Ein L\u00e4chler ward ich, der ein Lacher war,", "tokens": ["Ein", "L\u00e4ch\u00b7ler", "ward", "ich", ",", "der", "ein", "La\u00b7cher", "war", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "$,", "PRELS", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Und manchmal ist mein L\u00e4cheln gar nicht gut.", "tokens": ["Und", "manch\u00b7mal", "ist", "mein", "L\u00e4\u00b7cheln", "gar", "nicht", "gut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPOSAT", "NN", "ADV", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Ja, stolzes Kind, f\u00fcr eine K\u00f6nigin,", "tokens": ["Ja", ",", "stol\u00b7zes", "Kind", ",", "f\u00fcr", "ei\u00b7ne", "K\u00f6\u00b7ni\u00b7gin", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADJA", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Wie du es bist, bin ich ein armer Tropf;", "tokens": ["Wie", "du", "es", "bist", ",", "bin", "ich", "ein", "ar\u00b7mer", "Tropf", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "VAFIN", "$,", "VAFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Im Hofstaat deiner Sch\u00f6nheit war mein Platz", "tokens": ["Im", "Hof\u00b7staat", "dei\u00b7ner", "Sch\u00f6n\u00b7heit", "war", "mein", "Platz"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Im finstern Winkel, wo die Bettler stehn,", "tokens": ["Im", "fins\u00b7tern", "Win\u00b7kel", ",", "wo", "die", "Bett\u00b7ler", "stehn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "PWAV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Die ihre welke, abgeh\u00e4rmte Hand", "tokens": ["Die", "ih\u00b7re", "wel\u00b7ke", ",", "ab\u00b7ge\u00b7h\u00e4rm\u00b7te", "Hand"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "PPOSAT", "NN", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Vergeblich strecken, da\u00df von all dem Glanz", "tokens": ["Ver\u00b7geb\u00b7lich", "stre\u00b7cken", ",", "da\u00df", "von", "all", "dem", "Glanz"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "$,", "KOUS", "APPR", "PIAT", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Ein Schimmer darauf falle. Bettelarm,", "tokens": ["Ein", "Schim\u00b7mer", "da\u00b7rauf", "fal\u00b7le", ".", "Bet\u00b7tel\u00b7arm", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "PAV", "VVFIN", "$.", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.23": {"text": "Nimm dieses Wort mit aller Schandenlast,", "tokens": ["Nimm", "die\u00b7ses", "Wort", "mit", "al\u00b7ler", "Schan\u00b7den\u00b7last", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PDAT", "NN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "W\u00e4r ich bei dir, \u2013 drum will ich ferne sein.", "tokens": ["W\u00e4r", "ich", "bei", "dir", ",", "\u2013", "drum", "will", "ich", "fer\u00b7ne", "sein", "."], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "PPER", "$,", "$(", "PAV", "VMFIN", "PPER", "ADV", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Denn sieh: Ich hab ein Reich, drin bin ", "tokens": ["Denn", "sieh", ":", "Ich", "hab", "ein", "Reich", ",", "drin", "bin"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVIMP", "$.", "PPER", "VAFIN", "ART", "NN", "$,", "ADV", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Reich und Gl\u00fcck, das ist so voller Glanz,", "tokens": ["Ein", "Reich", "und", "Gl\u00fcck", ",", "das", "ist", "so", "vol\u00b7ler", "Glanz", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "$,", "PDS", "VAFIN", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df deine Sch\u00f6nheit selbst davor verbleicht.", "tokens": ["Da\u00df", "dei\u00b7ne", "Sch\u00f6n\u00b7heit", "selbst", "da\u00b7vor", "ver\u00b7bleicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "PAV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Dies Reich ist mein, weil ich sein Sch\u00f6pfer bin,", "tokens": ["Dies", "Reich", "ist", "mein", ",", "weil", "ich", "sein", "Sch\u00f6p\u00b7fer", "bin", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "VAFIN", "PPOSAT", "$,", "KOUS", "PPER", "PPOSAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ein Himmelreich mit mir als seinem Gott;", "tokens": ["Ein", "Him\u00b7mel\u00b7reich", "mit", "mir", "als", "sei\u00b7nem", "Gott", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "KOUS", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Du selber bist darin nur Kreatur.", "tokens": ["Du", "sel\u00b7ber", "bist", "da\u00b7rin", "nur", "Kre\u00b7a\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "PAV", "ADV", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "In grader S\u00e4ule steigt der Opferrauch", "tokens": ["In", "gra\u00b7der", "S\u00e4u\u00b7le", "steigt", "der", "Op\u00b7fer\u00b7rauch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Von meinem Betherd, der der Sch\u00f6nheit dampft,", "tokens": ["Von", "mei\u00b7nem", "Be\u00b7therd", ",", "der", "der", "Sch\u00f6n\u00b7heit", "dampft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.9": {"text": "Und, steh ich hier, ein Priester und ein Herr,", "tokens": ["Und", ",", "steh", "ich", "hier", ",", "ein", "Pries\u00b7ter", "und", "ein", "Herr", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VVFIN", "PPER", "ADV", "$,", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Sehn meine Augen bis zum tiefsten Grund", "tokens": ["Sehn", "mei\u00b7ne", "Au\u00b7gen", "bis", "zum", "tiefs\u00b7ten", "Grund"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "APPRART", "ADJA", "NN"], "meter": "++-+-+-+-+", "measure": "iambic.penta.spondeus"}, "line.11": {"text": "Des gro\u00dfen Meeres, das ihr Leben hei\u00dft.", "tokens": ["Des", "gro\u00b7\u00dfen", "Mee\u00b7res", ",", "das", "ihr", "Le\u00b7ben", "hei\u00dft", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Dich, K\u00f6nigin, erkenn ich und das Kind,", "tokens": ["Dich", ",", "K\u00f6\u00b7ni\u00b7gin", ",", "er\u00b7kenn", "ich", "und", "das", "Kind", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "$,", "KOUS", "PPER", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Das bleiche, das im Gassenkot verkommt,", "tokens": ["Das", "blei\u00b7che", ",", "das", "im", "Gas\u00b7sen\u00b7kot", "ver\u00b7kommt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "PRELS", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Das gro\u00dfe Weltrund und den Primelnkelch,", "tokens": ["Das", "gro\u00b7\u00dfe", "Welt\u00b7rund", "und", "den", "Pri\u00b7meln\u00b7kelch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und mir ist alles gleich verwandt und hold.", "tokens": ["Und", "mir", "ist", "al\u00b7les", "gleich", "ver\u00b7wandt", "und", "hold", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PIS", "ADV", "VVPP", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Umfassung ist mein Gl\u00fcck in diesem Reich,", "tokens": ["Um\u00b7fas\u00b7sung", "ist", "mein", "Gl\u00fcck", "in", "die\u00b7sem", "Reich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Die ganze Welt zieh ich an meine Brust,", "tokens": ["Die", "gan\u00b7ze", "Welt", "zieh", "ich", "an", "mei\u00b7ne", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "In die ein Gott mir eine Sonne gab,", "tokens": ["In", "die", "ein", "Gott", "mir", "ei\u00b7ne", "Son\u00b7ne", "gab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "ART", "NN", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Um die sich alles selig drehen mu\u00df.", "tokens": ["Um", "die", "sich", "al\u00b7les", "se\u00b7lig", "dre\u00b7hen", "mu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "PRF", "PIS", "ADJD", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Du l\u00e4chelst, wie ihr M\u00e4dchen l\u00e4cheln k\u00f6nnt,", "tokens": ["Du", "l\u00e4\u00b7chelst", ",", "wie", "ihr", "M\u00e4d\u00b7chen", "l\u00e4\u00b7cheln", "k\u00f6nnt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "PPOSAT", "NN", "VVFIN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die grausam wie das liebe Leben sind,", "tokens": ["Die", "grau\u00b7sam", "wie", "das", "lie\u00b7be", "Le\u00b7ben", "sind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "KOKOM", "ART", "ADJA", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und denen gern Verachtung stolz und fein", "tokens": ["Und", "de\u00b7nen", "gern", "Ver\u00b7ach\u00b7tung", "stolz", "und", "fein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PDS", "ADV", "NN", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die sch\u00f6nen Lippen sch\u00fcrzt. \u2013 Ich seh auch das"