{"dta.poem.19403": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "136.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du findest im Besitz Gen\u00fcge nimmermehr;", "tokens": ["Du", "fin\u00b7dest", "im", "Be\u00b7sitz", "Ge\u00b7n\u00fc\u00b7ge", "nim\u00b7mer\u00b7mehr", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Denn es begehrt dein Herz entweder immer mehr,", "tokens": ["Denn", "es", "be\u00b7gehrt", "dein", "Herz", "ent\u00b7we\u00b7der", "im\u00b7mer", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPOSAT", "NN", "KON", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Oder, hast du genug, so f\u00fcrchtest du Verlust;", "tokens": ["O\u00b7der", ",", "hast", "du", "ge\u00b7nug", ",", "so", "f\u00fcrch\u00b7test", "du", "Ver\u00b7lust", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VAFIN", "PPER", "ADV", "$,", "ADV", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und dort ist so wie hier der Stachel in der Brust.", "tokens": ["Und", "dort", "ist", "so", "wie", "hier", "der", "Sta\u00b7chel", "in", "der", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "ADV", "KOKOM", "ADV", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}