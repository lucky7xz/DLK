{"dta.poem.3768": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Von Adel und Tadel .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519172", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein silberne Scheide,               ", "tokens": ["Ein", "sil\u00b7ber\u00b7ne", "Schei\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Ein goldene Kling,", "tokens": ["Ein", "gol\u00b7de\u00b7ne", "Kling", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Mein Schatz ist von Adel,", "tokens": ["Mein", "Schatz", "ist", "von", "A\u00b7del", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "APPR", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Wie freut mich das Ding.", "tokens": ["Wie", "freut", "mich", "das", "Ding", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Kreidewei\u00dfe Haare,", "tokens": ["Krei\u00b7de\u00b7wei\u00b7\u00dfe", "Haa\u00b7re", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Schwarz gewichste Schuh,", "tokens": ["Schwarz", "ge\u00b7wichs\u00b7te", "Schuh", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Ein Degen an der Seite,", "tokens": ["Ein", "De\u00b7gen", "an", "der", "Sei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein Goldst\u00fcck dazu.", "tokens": ["Ein", "Gold\u00b7st\u00fcck", "da\u00b7zu", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Mein Schatz ist von Adel,", "tokens": ["Mein", "Schatz", "ist", "von", "A\u00b7del", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "APPR", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Von Adel ist er,", "tokens": ["Von", "A\u00b7del", "ist", "er", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PPER", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Was hat er f\u00fcr einen Tadel?", "tokens": ["Was", "hat", "er", "f\u00fcr", "ei\u00b7nen", "Ta\u00b7del", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Kein Waden hat er.", "tokens": ["Kein", "Wa\u00b7den", "hat", "er", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PPER", "$."], "meter": "-+-+-", "measure": "iambic.di"}}}}}