{"dta.poem.20309": {"metadata": {"author": {"name": "Hofmann von Hofmannswaldau, Christian", "birth": "N.A.", "death": "N.A."}, "title": "An das ihr im spiel abgel\u00f6\u00dfte  \n knie-band.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1695", "urn": "urn:nbn:de:kobv:b4-200905197751", "language": ["de:0.85", "nl:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du sanffces band/ das meinen geist bestrickt/", "tokens": ["Du", "sanff\u00b7ces", "band", "/", "das", "mei\u00b7nen", "geist", "be\u00b7strickt", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$(", "ART", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und meine freyheit bindet/", "tokens": ["Und", "mei\u00b7ne", "frey\u00b7heit", "bin\u00b7det", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich werde stets durch frische glut entz\u00fcndet/", "tokens": ["Ich", "wer\u00b7de", "stets", "durch", "fri\u00b7sche", "glut", "ent\u00b7z\u00fcn\u00b7det", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "So offt mein aug auff deine sch\u00f6nheit blickt.", "tokens": ["So", "offt", "mein", "aug", "auff", "dei\u00b7ne", "sch\u00f6n\u00b7heit", "blickt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ich liebe dich/ nicht weil du seiden bist/", "tokens": ["Ich", "lie\u00b7be", "dich", "/", "nicht", "weil", "du", "sei\u00b7den", "bist", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$(", "PTKNEG", "KOUS", "PPER", "VAINF", "VAFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Noch weil die kunst des webers dich geschlagen/", "tokens": ["Noch", "weil", "die", "kunst", "des", "we\u00b7bers", "dich", "ge\u00b7schla\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "ART", "NN", "ART", "NN", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Nein. Sondern weil dein attla\u00df w\u00fcrdig ist/", "tokens": ["Nein", ".", "Son\u00b7dern", "weil", "dein", "att\u00b7la\u00df", "w\u00fcr\u00b7dig", "ist", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "KON", "KOUS", "PPOSAT", "NN", "ADJD", "VAFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Da\u00df Phillis ihn an ihrem knie getragen.", "tokens": ["Da\u00df", "Phil\u00b7lis", "ihn", "an", "ih\u00b7rem", "knie", "ge\u00b7tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "PPER", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ich l\u00f6\u00dfte dich/ da mir das gl\u00fccke rieff/", "tokens": ["Ich", "l\u00f6\u00df\u00b7te", "dich", "/", "da", "mir", "das", "gl\u00fc\u00b7cke", "rieff", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$(", "KOUS", "PPER", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Von ihren zarten waden/", "tokens": ["Von", "ih\u00b7ren", "zar\u00b7ten", "wa\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die die natur mit warmen schnee beladen/", "tokens": ["Die", "die", "na\u00b7tur", "mit", "war\u00b7men", "schnee", "be\u00b7la\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "APPR", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ob ich schon selbst in mein verderben lieff.", "tokens": ["Ob", "ich", "schon", "selbst", "in", "mein", "ver\u00b7der\u00b7ben", "lieff", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "APPR", "PPOSAT", "VVFIN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ihr fu\u00df ward frey/ und meine hand verschr\u00e4nckt/", "tokens": ["Ihr", "fu\u00df", "ward", "frey", "/", "und", "mei\u00b7ne", "hand", "ver\u00b7schr\u00e4nckt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKVZ", "VAFIN", "ADJD", "$(", "KON", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Ja/ was noch mehr/ mein hertze selbst gefangen.", "tokens": ["Ja", "/", "was", "noch", "mehr", "/", "mein", "hert\u00b7ze", "selbst", "ge\u00b7fan\u00b7gen", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$(", "PWS", "ADV", "ADV", "$(", "PPOSAT", "NN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Doch freut es sich/ wenn es an dich gedenckt/", "tokens": ["Doch", "freut", "es", "sich", "/", "wenn", "es", "an", "dich", "ge\u00b7denckt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PRF", "$(", "KOUS", "PPER", "APPR", "PPER", "VVFIN", "$("], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Und w\u00fcnschet nur in deinem schmuck zu prangen.", "tokens": ["Und", "w\u00fcn\u00b7schet", "nur", "in", "dei\u00b7nem", "schmuck", "zu", "pran\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "PPOSAT", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ich halte dich dann h\u00f6her als demant/", "tokens": ["Ich", "hal\u00b7te", "dich", "dann", "h\u00f6\u00b7her", "als", "de\u00b7mant", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADJD", "KOKOM", "ADJD", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Als perlen und rubinen.", "tokens": ["Als", "per\u00b7len", "und", "ru\u00b7bi\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVINF", "KON", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Du must des nachts mir statt des k\u00fcssens dienen/", "tokens": ["Du", "must", "des", "nachts", "mir", "statt", "des", "k\u00fcs\u00b7sens", "die\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "APPR", "PPER", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Des tages trag ich dich an meiner hand/", "tokens": ["Des", "ta\u00b7ges", "trag", "ich", "dich", "an", "mei\u00b7ner", "hand", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PRF", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Im traume red ich eintzig nur von dir/", "tokens": ["Im", "trau\u00b7me", "red", "ich", "eint\u00b7zig", "nur", "von", "dir", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ADJD", "ADV", "APPR", "PPER", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und wach ich dann/ so bist du mein erg\u00f6tzen;", "tokens": ["Und", "wach", "ich", "dann", "/", "so", "bist", "du", "mein", "er\u00b7g\u00f6t\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "$(", "ADV", "VAFIN", "PPER", "PPOSAT", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Denn ohne dich und deiner Phillis zier", "tokens": ["Denn", "oh\u00b7ne", "dich", "und", "dei\u00b7ner", "Phil\u00b7lis", "zier"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPER", "KON", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Kan sonsten nichts mich in vergn\u00fcgung setzen.", "tokens": ["Kan", "sons\u00b7ten", "nichts", "mich", "in", "ver\u00b7gn\u00fc\u00b7gung", "set\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PIS", "PRF", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}