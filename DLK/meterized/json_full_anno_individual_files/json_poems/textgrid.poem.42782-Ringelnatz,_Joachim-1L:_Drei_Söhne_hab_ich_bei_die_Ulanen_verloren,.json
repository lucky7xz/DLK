{"textgrid.poem.42782": {"metadata": {"author": {"name": "Ringelnatz, Joachim", "birth": "N.A.", "death": "N.A."}, "title": "1L: Drei S\u00f6hne hab ich bei die Ulanen verloren,", "genre": "verse", "period": "N.A.", "pub_year": 1908, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Drei S\u00f6hne hab ich bei die Ulanen verloren,", "tokens": ["Drei", "S\u00f6h\u00b7ne", "hab", "ich", "bei", "die", "U\u00b7la\u00b7nen", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VAFIN", "PPER", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+---+-", "measure": "unknown.measure.penta"}, "line.2": {"text": "Mein Mann fiel aus dem dritten Stock.", "tokens": ["Mein", "Mann", "fiel", "aus", "dem", "drit\u00b7ten", "Stock", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Aber \u2013 es wird lustig weitergeboren!", "tokens": ["A\u00b7ber", "\u2013", "es", "wird", "lus\u00b7tig", "wei\u00b7ter\u00b7ge\u00b7bo\u00b7ren", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$(", "PPER", "VAFIN", "ADJD", "VVINF", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.4": {"text": "Ich habe nur noch den einen, den Umstandsrock.", "tokens": ["Ich", "ha\u00b7be", "nur", "noch", "den", "ei\u00b7nen", ",", "den", "Um\u00b7stands\u00b7rock", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ART", "ART", "$,", "ART", "NN", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Macht es mir nach: Werdet schwanger, ihr Weiber!", "tokens": ["Macht", "es", "mir", "nach", ":", "Wer\u00b7det", "schwan\u00b7ger", ",", "ihr", "Wei\u00b7ber", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPER", "PPER", "PTKVZ", "$.", "VAFIN", "ADJD", "$,", "PPOSAT", "NN", "$."], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Alle Weiber m\u00fcssen schwanger sein.", "tokens": ["Al\u00b7le", "Wei\u00b7ber", "m\u00fcs\u00b7sen", "schwan\u00b7ger", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "ADJD", "VAINF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Dann springen die M\u00e4nner vor eure geschwollenen Leiber", "tokens": ["Dann", "sprin\u00b7gen", "die", "M\u00e4n\u00b7ner", "vor", "eu\u00b7re", "ge\u00b7schwol\u00b7le\u00b7nen", "Lei\u00b7ber"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+--+--+--+--+-", "measure": "amphibrach.penta.plus"}, "line.4": {"text": "Links und rechts beiseite und sind ganz klein.", "tokens": ["Links", "und", "rechts", "bei\u00b7sei\u00b7te", "und", "sind", "ganz", "klein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ADV", "VVFIN", "KON", "VAFIN", "ADV", "ADJD", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Aller Anfang ist schwer.", "tokens": ["Al\u00b7ler", "An\u00b7fang", "ist", "schwer", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "ADJD", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Pfeift auf die Fehlgeburten und Mi\u00dfgeburten. \u2013", "tokens": ["Pfeift", "auf", "die", "Fehl\u00b7ge\u00b7bur\u00b7ten", "und", "Mi\u00df\u00b7ge\u00b7bur\u00b7ten", ".", "\u2013"], "token_info": ["w