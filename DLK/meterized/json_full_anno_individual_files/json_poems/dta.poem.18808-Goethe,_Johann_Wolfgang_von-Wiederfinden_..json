{"dta.poem.18808": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang von", "birth": "N.A.", "death": "N.A."}, "title": "Wiederfinden .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1819", "urn": "urn:nbn:de:kobv:b4-200905191692", "language": ["de:0.99"], "booktitle": "Goethe, Johann Wolfgang von: West-\u00f6stlicher Divan. Stuttgart, 1819."}, "poem": {"stanza.1": {"line.1": {"text": "Ist es m\u00f6glich, Stern der Sterne,", "tokens": ["Ist", "es", "m\u00f6g\u00b7lich", ",", "Stern", "der", "Ster\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "$,", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dr\u00fcck\u2019 ich wieder dich ans Herz!", "tokens": ["Dr\u00fcck", "ich", "wie\u00b7der", "dich", "ans", "Herz", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "PPER", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ach! was ist die Nacht der Ferne", "tokens": ["Ach", "!", "was", "ist", "die", "Nacht", "der", "Fer\u00b7ne"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "PWS", "VAFIN", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "F\u00fcr ein Abgrund, f\u00fcr ein Schmerz.", "tokens": ["F\u00fcr", "ein", "Ab\u00b7grund", ",", "f\u00fcr", "ein", "Schmerz", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ja du bist es! meiner Freuden", "tokens": ["Ja", "du", "bist", "es", "!", "mei\u00b7ner", "Freu\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PTKANT", "PPER", "VAFIN", "PPER", "$.", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "S\u00fcsser, lieber Widerpart;", "tokens": ["S\u00fcs\u00b7ser", ",", "lie\u00b7ber", "Wi\u00b7der\u00b7part", ";"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Eingedenk vergangner Leiden", "tokens": ["Ein\u00b7ge\u00b7denk", "ver\u00b7gang\u00b7ner", "Lei\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Schaudr\u2019 ich vor der Gegenwart.", "tokens": ["Schaudr'", "ich", "vor", "der", "Ge\u00b7gen\u00b7wart", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Als die Welt im tiefsten Grunde", "tokens": ["Als", "die", "Welt", "im", "tiefs\u00b7ten", "Grun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lag an Gottes ew\u2019ger Brust,", "tokens": ["Lag", "an", "Got\u00b7tes", "ew'\u00b7ger", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ordnet\u2019 er die erste Stunde", "tokens": ["Ord\u00b7net'", "er", "die", "ers\u00b7te", "Stun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Mit erhabner Sch\u00f6pfungslust,", "tokens": ["Mit", "er\u00b7hab\u00b7ner", "Sch\u00f6p\u00b7fungs\u00b7lust", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und er sprach das Wort: Es werde!", "tokens": ["Und", "er", "sprach", "das", "Wort", ":", "Es", "wer\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "$.", "PPER", "VAFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Da erklang ein schmerzlich Ach!", "tokens": ["Da", "er\u00b7klang", "ein", "schmerz\u00b7lich", "Ach", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJD", "ITJ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Als das All, mit Machtgeb\u00e4rde,", "tokens": ["Als", "das", "All", ",", "mit", "Macht\u00b7ge\u00b7b\u00e4r\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "In die Wirklichkeiten brach.", "tokens": ["In", "die", "Wirk\u00b7lich\u00b7kei\u00b7ten", "brach", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Auf that sich das Licht! sich trennte", "tokens": ["Auf", "that", "sich", "das", "Licht", "!", "sich", "trenn\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "VVFIN", "PRF", "ART", "NN", "$.", "PRF", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Scheu die Finsterniss von ihm,", "tokens": ["Scheu", "die", "Fins\u00b7ter\u00b7niss", "von", "ihm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und sogleich die Elemente", "tokens": ["Und", "sog\u00b7leich", "die", "E\u00b7le\u00b7men\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "NN"], "meter": "--+--+--", "measure": "anapaest.di.plus"}, "line.4": {"text": "Scheidend auseinander fliehn.", "tokens": ["Schei\u00b7dend", "aus\u00b7ein\u00b7an\u00b7der", "fliehn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Rasch, in wilden w\u00fcsten Tr\u00e4umen,", "tokens": ["Rasch", ",", "in", "wil\u00b7den", "w\u00fcs\u00b7ten", "Tr\u00e4u\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "APPR", "ADJA", "VVFIN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Jedes nach der Weite rang,", "tokens": ["Je\u00b7des", "nach", "der", "Wei\u00b7te", "rang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Starr, in ungemessnen R\u00e4umen,", "tokens": ["Starr", ",", "in", "un\u00b7ge\u00b7mess\u00b7nen", "R\u00e4u\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Ohne Sehnsucht, ohne Klang.", "tokens": ["Oh\u00b7ne", "Sehn\u00b7sucht", ",", "oh\u00b7ne", "Klang", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "KOUI", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}