{"dta.poem.889": {"metadata": {"author": {"name": "Trakl, Georg", "birth": "N.A.", "death": "N.A."}, "title": "An DEN KNABEN ELIS", "genre": "Lyrik", "period": "N.A.", "pub_year": "1913", "urn": "urn:nbn:de:kobv:b4-30357-9", "language": ["de:0.99"], "booktitle": "Trakl, Georg: Gedichte. Leipzig, 1913."}, "poem": {"stanza.1": {"line.1": {"text": "Elis, wenn die Amsel im schwarzen Wald ruft,", "tokens": ["E\u00b7lis", ",", "wenn", "die", "Am\u00b7sel", "im", "schwar\u00b7zen", "Wald", "ruft", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KOUS", "ART", "NN", "APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+---+-+-+-", "measure": "dactylic.init"}, "line.2": {"text": "Dieses ist dein Untergang.", "tokens": ["Die\u00b7ses", "ist", "dein", "Un\u00b7ter\u00b7gang", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Deine Lippen trinken die K\u00fchle des blauen Felsenquells.", "tokens": ["Dei\u00b7ne", "Lip\u00b7pen", "trin\u00b7ken", "die", "K\u00fch\u00b7le", "des", "blau\u00b7en", "Fel\u00b7sen\u00b7quells", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+--+--+-+-+", "measure": "trochaic.septa.relaxed"}}, "stanza.2": {"line.1": {"text": "La\u00df, wenn deine Stirne leise blutet", "tokens": ["La\u00df", ",", "wenn", "dei\u00b7ne", "Stir\u00b7ne", "lei\u00b7se", "blu\u00b7tet"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "$,", "KOUS", "PPOSAT", "NN", "ADJD", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Uralte Legenden", "tokens": ["Ur\u00b7al\u00b7te", "Le\u00b7gen\u00b7den"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+-+--", "measure": "unknown.measure.di"}, "line.3": {"text": "Und dunkle Deutung des Vogelflugs.", "tokens": ["Und", "dunk\u00b7le", "Deu\u00b7tung", "des", "Vo\u00b7gel\u00b7flugs", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Du aber gehst mit weichen Schritten in die Nacht,", "tokens": ["Du", "a\u00b7ber", "gehst", "mit", "wei\u00b7chen", "Schrit\u00b7ten", "in", "die", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "APPR", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die voll purpurner Trauben h\u00e4ngt", "tokens": ["Die", "voll", "pur\u00b7pur\u00b7ner", "Trau\u00b7ben", "h\u00e4ngt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und du regst die Arme sch\u00f6ner im Blau.", "tokens": ["Und", "du", "regst", "die", "Ar\u00b7me", "sch\u00f6\u00b7ner", "im", "Blau", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "ADJD", "APPRART", "NN", "$."], "meter": "--+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.4": {"line.1": {"text": "Ein Dornenbusch t\u00f6nt,", "tokens": ["Ein", "Dor\u00b7nen\u00b7busch", "t\u00f6nt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Wo deine mondenen Augen sind.", "tokens": ["Wo", "dei\u00b7ne", "mon\u00b7de\u00b7nen", "Au\u00b7gen", "sind", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "O, wie lange bist, Elis, du verstorben.", "tokens": ["O", ",", "wie", "lan\u00b7ge", "bist", ",", "E\u00b7lis", ",", "du", "ver\u00b7stor\u00b7ben", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "ADV", "VAFIN", "$,", "NE", "$,", "PPER", "VVINF", "$."], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}}, "stanza.5": {"line.1": {"text": "Dein Leib ist eine Hyazinthe,", "tokens": ["Dein", "Leib", "ist", "ei\u00b7ne", "Hy\u00b7a\u00b7zin\u00b7the", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In die ein M\u00f6nch die w\u00e4chsernen Finger taucht.", "tokens": ["In", "die", "ein", "M\u00f6nch", "die", "w\u00e4ch\u00b7ser\u00b7nen", "Fin\u00b7ger", "taucht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Eine schwarze H\u00f6hle ist unser Schweigen,", "tokens": ["Ei\u00b7ne", "schwar\u00b7ze", "H\u00f6h\u00b7le", "ist", "un\u00b7ser", "Schwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}}, "stanza.6": {"line.1": {"text": "Daraus bisweilen ein sanftes Tier tritt", "tokens": ["Da\u00b7raus", "bis\u00b7wei\u00b7len", "ein", "sanf\u00b7tes", "Tier", "tritt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "ADV", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und langsam die schweren Lider senkt.", "tokens": ["Und", "lang\u00b7sam", "die", "schwe\u00b7ren", "Li\u00b7der", "senkt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Auf deine Schl\u00e4fen tropft schwarzer Tau,", "tokens": ["Auf", "dei\u00b7ne", "Schl\u00e4\u00b7fen", "tropft", "schwar\u00b7zer", "Tau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.7": {"line.1": {"text": "Das letzte Gold verfallener Sterne.", "tokens": ["Das", "letz\u00b7te", "Gold", "ver\u00b7fal\u00b7le\u00b7ner", "Ster\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}}}}