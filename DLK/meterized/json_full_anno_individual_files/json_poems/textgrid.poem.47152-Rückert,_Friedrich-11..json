{"textgrid.poem.47152": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "11.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer in der Liebsten Auge blickt,", "tokens": ["Wer", "in", "der", "Liebs\u00b7ten", "Au\u00b7ge", "blickt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der hat die Welt vergessen.", "tokens": ["Der", "hat", "die", "Welt", "ver\u00b7ges\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der kann nicht, wen ihr Arm umstrickt,", "tokens": ["Der", "kann", "nicht", ",", "wen", "ihr", "Arm", "um\u00b7strickt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PTKNEG", "$,", "PWS", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Was drau\u00dfen liegt, ermessen.", "tokens": ["Was", "drau\u00b7\u00dfen", "liegt", ",", "er\u00b7mes\u00b7sen", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "$,", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ich halt' in meinem Arm ein Gl\u00fcck,", "tokens": ["Ich", "halt'", "in", "mei\u00b7nem", "Arm", "ein", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wer kann es mir entziehen?", "tokens": ["Wer", "kann", "es", "mir", "ent\u00b7zie\u00b7hen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und n\u00e4hm' es morgen Gott zur\u00fcck,", "tokens": ["Und", "n\u00e4hm'", "es", "mor\u00b7gen", "Gott", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "War's heut mir doch geliehen.", "tokens": ["Wa\u00b7r's", "heut", "mir", "doch", "ge\u00b7lie\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPER", "ADV", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Verlangen kann ein Menschenherz", "tokens": ["Ver\u00b7lan\u00b7gen", "kann", "ein", "Men\u00b7schen\u00b7herz"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VMFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nichts Besseres auf Erden,", "tokens": ["Nichts", "Bes\u00b7se\u00b7res", "auf", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Als f\u00fchlen Liebeslust und Schmerz", "tokens": ["Als", "f\u00fch\u00b7len", "Lie\u00b7bes\u00b7lust", "und", "Schmerz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und dann begraben werden.", "tokens": ["Und", "dann", "be\u00b7gra\u00b7ben", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVPP", "VAINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Wer in der Liebst