{"textgrid.poem.60077": {"metadata": {"author": {"name": "Jacobi, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ruhn in Frieden alle Seelen,", "genre": "verse", "period": "N.A.", "pub_year": 1777, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ruhn in Frieden alle Seelen,", "tokens": ["Ruhn", "in", "Frie\u00b7den", "al\u00b7le", "See\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "NN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die vollbracht ein banges Qu\u00e4len,", "tokens": ["Die", "voll\u00b7bracht", "ein", "ban\u00b7ges", "Qu\u00e4\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Die vollendet s\u00fc\u00dfen Traum,", "tokens": ["Die", "voll\u00b7en\u00b7det", "s\u00fc\u00b7\u00dfen", "Traum", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Lebenssatt, gebohren kaum,", "tokens": ["Le\u00b7bens\u00b7satt", ",", "ge\u00b7boh\u00b7ren", "kaum", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "VVPP", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Aus der Welt hin\u00fcber schieden:", "tokens": ["Aus", "der", "Welt", "hin\u00b7\u00fc\u00b7ber", "schie\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Alle Seelen ruhn in Frieden!", "tokens": ["Al\u00b7le", "See\u00b7len", "ruhn", "in", "Frie\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVINF", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Die sich hier Gespielen suchten,", "tokens": ["Die", "sich", "hier", "Ge\u00b7spie\u00b7len", "such\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "ADV", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Oefter weinten, nimmer fluchten,", "tokens": ["Oef\u00b7ter", "wein\u00b7ten", ",", "nim\u00b7mer", "fluch\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn von ihrer treuen Hand", "tokens": ["Wenn", "von", "ih\u00b7rer", "treu\u00b7en", "Hand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Keiner je den Druck verstand:", "tokens": ["Kei\u00b7ner", "je", "den", "Druck", "ver\u00b7stand", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Alle, die von hinnen schieden,", "tokens": ["Al\u00b7le", ",", "die", "von", "hin\u00b7nen", "schie\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "PRELS", "APPR", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Alle Seelen ruhn in Frieden!", "tokens": ["Al\u00b7le", "See\u00b7len", "ruhn", "in", "Frie\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVINF", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Liebevoller M\u00e4dchen Seelen,", "tokens": ["Lie\u00b7be\u00b7vol\u00b7ler", "M\u00e4d\u00b7chen", "See\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Deren Thr\u00e4nen nicht zu z\u00e4hlen,", "tokens": ["De\u00b7ren", "Thr\u00e4\u00b7nen", "nicht", "zu", "z\u00e4h\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "PTKNEG", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Die ein falscher Freund verlie\u00df,", "tokens": ["Die", "ein", "fal\u00b7scher", "Freund", "ver\u00b7lie\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und die blinde Welt verstie\u00df:", "tokens": ["Und", "die", "blin\u00b7de", "Welt", "ver\u00b7stie\u00df", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Alle, die von hinnen schieden,", "tokens": ["Al\u00b7le", ",", "die", "von", "hin\u00b7nen", "schie\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "PRELS", "APPR", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Alle Seelen ruhn in Frieden!", "tokens": ["Al\u00b7le", "See\u00b7len", "ruhn", "in", "Frie\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVINF", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Und der J\u00fcngling, dem, verborgen", "tokens": ["Und", "der", "J\u00fcng\u00b7ling", ",", "dem", ",", "ver\u00b7bor\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "$,", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Seine Braut am fr\u00fchen Morgen,", "tokens": ["Sei\u00b7ne", "Braut", "am", "fr\u00fc\u00b7hen", "Mor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Weil ihn Lieb' ins Grab gelegt,", "tokens": ["Weil", "ihn", "Lieb'", "ins", "Grab", "ge\u00b7legt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Auf sein Grab die Kerze tr\u00e4gt:", "tokens": ["Auf", "sein", "Grab", "die", "Ker\u00b7ze", "tr\u00e4gt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Alle, die von hinnen schieden,", "tokens": ["Al\u00b7le", ",", "die", "von", "hin\u00b7nen", "schie\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "PRELS", "APPR", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Alle Seelen ruhn in Frieden!", "tokens": ["Al\u00b7le", "See\u00b7len", "ruhn", "in", "Frie\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVINF", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Alle Geister, die voll Klarheit,", "tokens": ["Al\u00b7le", "Geis\u00b7ter", ",", "die", "voll", "Klar\u00b7heit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PRELS", "ADJD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wurden M\u00e4rtyrer der Wahrheit,", "tokens": ["Wur\u00b7den", "M\u00e4r\u00b7ty\u00b7rer", "der", "Wahr\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "K\u00e4mpften f\u00fcr das Heiligthum,", "tokens": ["K\u00e4mpf\u00b7ten", "f\u00fcr", "das", "Hei\u00b7lig\u00b7thum", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Suchten nicht der Marter Ruhm:", "tokens": ["Such\u00b7ten", "nicht", "der", "Mar\u00b7ter", "Ruhm", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Alle, die von hinnen schieden,", "tokens": ["Al\u00b7le", ",", "die", "von", "hin\u00b7nen", "schie\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "PRELS", "APPR", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Alle Seelen ruhn in Frieden!", "tokens": ["Al\u00b7le", "See\u00b7len", "ruhn", "in", "Frie\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVINF", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Und die nie der Sonne lachten,", "tokens": ["Und", "die", "nie", "der", "Son\u00b7ne", "lach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Unterm Mond auf Dornen wachten,", "tokens": ["Un\u00b7term", "Mond", "auf", "Dor\u00b7nen", "wach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Gott, im reinen Himmels-Licht,", "tokens": ["Gott", ",", "im", "rei\u00b7nen", "Him\u00b7mels\u00b7Licht", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Einst zu sehn von Angesicht:", "tokens": ["Einst", "zu", "sehn", "von", "An\u00b7ge\u00b7sicht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKZU", "VVINF", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Alle, die von hinnen schieden,", "tokens": ["Al\u00b7le", ",", "die", "von", "hin\u00b7nen", "schie\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "PRELS", "APPR", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Alle Seelen ruhn in Frieden!", "tokens": ["Al\u00b7le", "See\u00b7len", "ruhn", "in", "Frie\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVINF", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Und die gern im Rosen-Garten", "tokens": ["Und", "die", "gern", "im", "Ro\u00b7sen\u00b7Gar\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADV", "APPRART", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Bey dem Freuden-Becher harrten,", "tokens": ["Bey", "dem", "Freu\u00b7den\u00b7Be\u00b7cher", "harr\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber dann, zur b\u00f6sen Zeit,", "tokens": ["A\u00b7ber", "dann", ",", "zur", "b\u00f6\u00b7sen", "Zeit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Schmeckten seine Bitterkeit:", "tokens": ["Schmeck\u00b7ten", "sei\u00b7ne", "Bit\u00b7ter\u00b7keit", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Alle, die von hinnen schieden,", "tokens": ["Al\u00b7le", ",", "die", "von", "hin\u00b7nen", "schie\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "PRELS", "APPR", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Alle Seelen ruhn in Frieden!", "tokens": ["Al\u00b7le", "See\u00b7len", "ruhn", "in", "Frie\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVINF", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Auch, die keinen Frieden kannten,", "tokens": ["Auch", ",", "die", "kei\u00b7nen", "Frie\u00b7den", "kann\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PRELS", "PIAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Aber Muth und St\u00e4rke sandten", "tokens": ["A\u00b7ber", "Muth", "und", "St\u00e4r\u00b7ke", "sand\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "NN", "KON", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ueber leichenvolles Feld", "tokens": ["Ue\u00b7ber", "lei\u00b7chen\u00b7vol\u00b7les", "Feld"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic