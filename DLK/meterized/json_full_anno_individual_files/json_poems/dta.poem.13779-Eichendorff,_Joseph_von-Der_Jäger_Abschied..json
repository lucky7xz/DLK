{"dta.poem.13779": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Der J\u00e4ger Abschied.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Wer hat dich du sch\u00f6ner Wald", "tokens": ["Wer", "hat", "dich", "du", "sch\u00f6\u00b7ner", "Wald"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "PPER", "PPER", "ADJA", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Aufgebaut so hoch da droben?", "tokens": ["Auf\u00b7ge\u00b7baut", "so", "hoch", "da", "dro\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJD", "ADV", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wohl den Meister will ich loben,", "tokens": ["Wohl", "den", "Meis\u00b7ter", "will", "ich", "lo\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "So lang noch mein' Stimm' erschallt.", "tokens": ["So", "lang", "noch", "mein'", "Stimm'", "er\u00b7schallt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Lebe wohl,", "tokens": ["Le\u00b7be", "wohl", ","], "token_info": ["word", "word", "punct"], "pos": ["VVIMP", "ADV", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Lebe wohl, du sch\u00f6ner Wald!", "tokens": ["Le\u00b7be", "wohl", ",", "du", "sch\u00f6\u00b7ner", "Wald", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "$,", "PPER", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}