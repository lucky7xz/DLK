{"dta.poem.12087": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "9.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein heiteres Gem\u00fct ist gleich in jeder Lage,", "tokens": ["Ein", "hei\u00b7te\u00b7res", "Ge\u00b7m\u00fct", "ist", "gleich", "in", "je\u00b7der", "La\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch lieblich wechselnd, wie der See am sch\u00f6nen Tage;", "tokens": ["Doch", "lieb\u00b7lich", "wech\u00b7selnd", ",", "wie", "der", "See", "am", "sch\u00f6\u00b7nen", "Ta\u00b7ge", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADJD", "$,", "PWAV", "ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Der amethysten scheint, smaragden und saffieren,", "tokens": ["Der", "a\u00b7met\u00b7hys\u00b7ten", "scheint", ",", "sma\u00b7rag\u00b7den", "und", "saf\u00b7fie\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ADJA", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In Farben spielend, die in Farben sich verlieren.", "tokens": ["In", "Far\u00b7ben", "spie\u00b7lend", ",", "die", "in", "Far\u00b7ben", "sich", "ver\u00b7lie\u00b7ren", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,", "PRELS", "APPR", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wie ihn die Sonn' anregt, wie ihn ein Hauch bewegt,", "tokens": ["Wie", "ihn", "die", "Sonn'", "an\u00b7regt", ",", "wie", "ihn", "ein", "Hauch", "be\u00b7wegt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "VVFIN", "$,", "PWAV", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ist er mit anderen Juwelen \u00fcberlegt.", "tokens": ["Ist", "er", "mit", "an\u00b7de\u00b7ren", "Ju\u00b7we\u00b7len", "\u00fc\u00b7ber\u00b7legt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Nach der Verschiedenheit vom Ufer und vom Grund,", "tokens": ["Nach", "der", "Ver\u00b7schie\u00b7den\u00b7heit", "vom", "U\u00b7fer", "und", "vom", "Grund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPRART", "NN", "KON", "APPRART", "NN", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Thut dir sein fl\u00fcssiges Gestein sich anders kund.", "tokens": ["Thut", "dir", "sein", "fl\u00fcs\u00b7si\u00b7ges", "Ge\u00b7stein", "sich", "an\u00b7ders", "kund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPOSAT", "ADJA", "NN", "PRF", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Und jedes Wellchen, das der Flut von Edelsteinen", "tokens": ["Und", "je\u00b7des", "Well\u00b7chen", ",", "das", "der", "Flut", "von", "E\u00b7del\u00b7stei\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "$,", "PRELS", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Entsteigt, l\u00e4\u00dft auf der Stirn ein Demantfl\u00e4mmchen scheinen.", "tokens": ["Ent\u00b7steigt", ",", "l\u00e4\u00dft", "auf", "der", "Stirn", "ein", "De\u00b7mant\u00b7fl\u00e4mm\u00b7chen", "schei\u00b7nen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Doch wo des Ruders Schlag den Spiegel bricht, erfreut", "tokens": ["Doch", "wo", "des", "Ru\u00b7ders", "Schlag", "den", "Spie\u00b7gel", "bricht", ",", "er\u00b7freut"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "PWAV", "ART", "NN", "NN", "ART", "NN", "VVFIN", "$,", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dich eine Demantsaat, verschwendrisch ausgestreut.", "tokens": ["Dich", "ei\u00b7ne", "De\u00b7mant\u00b7saat", ",", "ver\u00b7schwend\u00b7risch", "aus\u00b7ge\u00b7streut", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "$,", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}