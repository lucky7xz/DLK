{"textgrid.poem.33635": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "Antwort", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vertraue! Z\u00fcrne nicht mit mir!", "tokens": ["Ver\u00b7trau\u00b7e", "!", "Z\u00fcr\u00b7ne", "nicht", "mit", "mir", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "NN", "PTKNEG", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie k\u00f6nnt' ich dich verraten!", "tokens": ["Wie", "k\u00f6nnt'", "ich", "dich", "ver\u00b7ra\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Was deine Blicke baten,", "tokens": ["Was", "dei\u00b7ne", "Bli\u00b7cke", "ba\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Erf\u00fcll' ich dir.", "tokens": ["Er\u00b7f\u00fcll'", "ich", "dir", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Die Sehnsucht wogt und st\u00fcrmt in mir.", "tokens": ["Die", "Sehn\u00b7sucht", "wogt", "und", "st\u00fcrmt", "in", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich hab' im Traum gesehen", "tokens": ["Ich", "hab'", "im", "Traum", "ge\u00b7se\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPRART", "NN", "VVPP"], "m