{"textgrid.poem.61703": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Durch die w\u00fcste, weite Heide", "tokens": ["Durch", "die", "w\u00fcs\u00b7te", ",", "wei\u00b7te", "Hei\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "VVFIN", "$,", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Tr\u00e4gt mein Ro\u00df mit meinem Leide", "tokens": ["Tr\u00e4gt", "mein", "Ro\u00df", "mit", "mei\u00b7nem", "Lei\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Matt mich fort, der Abend graut.", "tokens": ["Matt", "mich", "fort", ",", "der", "A\u00b7bend", "graut", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PTKVZ", "$,", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "\u00dcber mir die Wolken schweifen,", "tokens": ["\u00dc\u00b7ber", "mir", "die", "Wol\u00b7ken", "schwei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und der Wind mit hohlem Pfeifen", "tokens": ["Und", "der", "Wind", "mit", "hoh\u00b7lem", "Pfei\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wandert durch das Heidekraut.", "tokens": ["Wan\u00b7dert", "durch", "das", "Hei\u00b7de\u00b7kraut", "."], "token_info": ["word", "word"