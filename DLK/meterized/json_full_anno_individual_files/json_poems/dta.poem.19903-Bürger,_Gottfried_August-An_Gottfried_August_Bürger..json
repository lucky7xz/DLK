{"dta.poem.19903": {"metadata": {"author": {"name": "B\u00fcrger, Gottfried August", "birth": "N.A.", "death": "N.A."}, "title": "An  \n  Gottfried August B\u00fcrger.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1778", "urn": "urn:nbn:de:kobv:b4-20090519672", "language": ["de:0.99"], "booktitle": "B\u00fcrger, Gottfried August: Gedichte. G\u00f6ttingen, 1778."}, "poem": {"stanza.1": {"line.1": {"text": "Fried' und Freude dem S\u00e4nger zuvor und             ", "tokens": ["Fried'", "und", "Freu\u00b7de", "dem", "S\u00e4n\u00b7ger", "zu\u00b7vor", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "KON", "NN", "ART", "NN", "ADV", "KON"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "traulichen Handschlag!", "tokens": ["trau\u00b7li\u00b7chen", "Hand\u00b7schlag", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Sieh, ich habe dein Z\u00fcrnen vernommen am fer-", "tokens": ["Sieh", ",", "ich", "ha\u00b7be", "dein", "Z\u00fcr\u00b7nen", "ver\u00b7nom\u00b7men", "am", "fer"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "PPER", "VAFIN", "PPOSAT", "NN", "VVPP", "APPRART", "TRUNC"], "meter": "+-+--+--+--+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "nen Gestade;", "tokens": ["nen", "Ge\u00b7sta\u00b7de", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "--+-", "measure": "anapaest.init"}, "line.5": {"text": "H\u00f6rte den Fl\u00fcgelschlag deines Gesangs; melodi-", "tokens": ["H\u00f6r\u00b7te", "den", "Fl\u00fc\u00b7gel\u00b7schlag", "dei\u00b7nes", "Ge\u00b7sangs", ";", "me\u00b7lo\u00b7di"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["VVFIN", "ART", "NN", "PPOSAT", "NN", "$.", "TRUNC"], "meter": "+--+--+--+-+-", "measure": "dactylic.tri.plus"}, "line.6": {"text": "sche St\u00fcrme", "tokens": ["sche", "St\u00fcr\u00b7me"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "Deiner Leier erhuben ihn hoch; ein Riesenadler", "tokens": ["Dei\u00b7ner", "Lei\u00b7er", "er\u00b7hu\u00b7ben", "ihn", "hoch", ";", "ein", "Rie\u00b7se\u00b7nad\u00b7ler"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ADJD", "$.", "ART", "NN"], "meter": "+-+--+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.8": {"text": "Steht er vor mir mit dr\u00e4uender Klaue, mit r\u00fc-", "tokens": ["Steht", "er", "vor", "mir", "mit", "dr\u00e4u\u00b7en\u00b7der", "Klau\u00b7e", ",", "mit", "r\u00fc"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "PPER", "APPR", "ADJA", "NN", "$,", "APPR", "TRUNC"], "meter": "+-+--+--+--+", "measure": "trochaic.penta.relaxed"}, "line.9": {"text": "stigem Fittig;", "tokens": ["sti\u00b7gem", "Fit\u00b7tig", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.10": {"text": "Und schon z\u00fcrnt\u2019 ich entgegen. Da faste mich", "tokens": ["Und", "schon", "z\u00fcrnt'", "ich", "ent\u00b7ge\u00b7gen", ".", "Da", "fas\u00b7te", "mich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PTKVZ", "$.", "ADV", "VVFIN", "PPER"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.11": {"text": "Pallas Ath\u00e4n\u00e4", "tokens": ["Pal\u00b7las", "A\u00b7th\u00e4\u00b7n\u00e4"], "token_info": ["word", "word"], "pos": ["NE", "NE"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.12": {"text": "Bei den goldnen Locken; ich wandte mich str\u00e4u-", "tokens": ["Bei", "den", "gold\u00b7nen", "Lo\u00b7cken", ";", "ich", "wand\u00b7te", "mich", "str\u00e4u"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "$.", "PPER", "VVFIN", "PRF", "TRUNC"], "meter": "--+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.13": {"text": "bend; mein Auge", "tokens": ["bend", ";", "mein", "Au\u00b7ge"], "token_info": ["word", "punct", "word", "word"], "pos": ["VVPP", "$.", "PPOSAT", "NN"], "meter": "--+-", "measure": "anapaest.init"}, "line.14": {"text": "Staunte zur\u00fcck, vom Blize der g\u00f6ttlichen Augen", "tokens": ["Staun\u00b7te", "zu\u00b7r\u00fcck", ",", "vom", "Bli\u00b7ze", "der", "g\u00f6tt\u00b7li\u00b7chen", "Au\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PTKVZ", "$,", "APPRART", "NN", "ART", "ADJA", "NN"], "meter": "+--+-+--+--+-", "measure": "iambic.penta.invert"}, "line.15": {"text": "getroffen.", "tokens": ["ge\u00b7trof\u00b7fen", "."], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.16": {"text": "Sieh, ich bebte nicht dir; ich bebte der furcht-", "tokens": ["Sieh", ",", "ich", "beb\u00b7te", "nicht", "dir", ";", "ich", "beb\u00b7te", "der", "furcht"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "PPER", "VVFIN", "PTKNEG", "PPER", "$.", "PPER", "VVFIN", "ART", "TRUNC"], "meter": "+-+-++-+--+", "measure": "iambic.hexa.chol"}, "line.17": {"text": "baren G\u00f6ttin.", "tokens": ["ba\u00b7ren", "G\u00f6t\u00b7tin", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.18": {"text": "Sie verschwand; da war mir, als athmet\u2019 ich", "tokens": ["Sie", "ver\u00b7schwand", ";", "da", "war", "mir", ",", "als", "ath\u00b7met'", "ich"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "ADV", "VAFIN", "PPER", "$,", "KOKOM", "VVFIN", "PPER"], "meter": "--+-+--++-", "measure": "iambic.tetra.relaxed"}, "line.19": {"text": "liebliche D\u00fcfte,", "tokens": ["lieb\u00b7li\u00b7che", "D\u00fcf\u00b7te", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.20": {"text": "L\u00e4g\u2019 am blumigen Hange des Helikon, unter der", "tokens": ["L\u00e4g'", "am", "blu\u00b7mi\u00b7gen", "Han\u00b7ge", "des", "He\u00b7li\u00b7kon", ",", "un\u00b7ter", "der"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "APPRART", "ADJA", "NN", "ART", "NN", "$,", "APPR", "ART"], "meter": "+-+--+--+--+--", "measure": "trochaic.penta.relaxed"}, "line.21": {"text": "K\u00fchlung", "tokens": ["K\u00fch\u00b7lung"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.22": {"text": "Wehender Schatten, an Aganippens Silberges\u00e4usel.", "tokens": ["We\u00b7hen\u00b7der", "Schat\u00b7ten", ",", "an", "A\u00b7gan\u00b7ip\u00b7pens", "Sil\u00b7ber\u00b7ge\u00b7s\u00e4u\u00b7sel", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "APPR", "NE", "NN", "$."], "meter": "+--+---+--+--+-", "measure": "dactylic.di.plus"}, "line.23": {"text": "Nun erwacht\u2019 ich, und z\u00fcrnte nun wieder, und", "tokens": ["Nun", "er\u00b7wacht'", "ich", ",", "und", "z\u00fcrn\u00b7te", "nun", "wie\u00b7der", ",", "und"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KON", "VVFIN", "ADV", "ADV", "$,", "KON"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.24": {"text": "grif zu der Leier.", "tokens": ["grif", "zu", "der", "Lei\u00b7er", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.25": {"text": "Aber es hatte die j\u00fcngste der Musen die Leier", "tokens": ["A\u00b7ber", "es", "hat\u00b7te", "die", "j\u00fcngs\u00b7te", "der", "Mu\u00b7sen", "die", "Lei\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "ART", "ADJA", "ART", "NN", "ART", "NN"], "meter": "+--+--+--+--+-", "measure": "dactylic.penta"}, "line.26": {"text": "gestimmet,", "tokens": ["ge\u00b7stim\u00b7met", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.27": {"text": "Da\u00df sie nicht t\u00f6nte wie sonst, wie Donner, wie", "tokens": ["Da\u00df", "sie", "nicht", "t\u00f6n\u00b7te", "wie", "sonst", ",", "wie", "Don\u00b7ner", ",", "wie"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["KOUS", "PPER", "PTKNEG", "VVFIN", "KOKOM", "ADV", "$,", "PWAV", "NN", "$,", "PWAV"], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.28": {"text": "Stimmen der Meere,", "tokens": ["Stim\u00b7men", "der", "Mee\u00b7re", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.29": {"text": "Sondern wie Lispel des wankenden Schilfes,", "tokens": ["Son\u00b7dern", "wie", "Lis\u00b7pel", "des", "wan\u00b7ken\u00b7den", "Schil\u00b7fes", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "NE", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.30": {"text": "wie z\u00e4rtliche Klagen", "tokens": ["wie", "z\u00e4rt\u00b7li\u00b7che", "Kla\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.31": {"text": "Junger Nachtigallen auf bl\u00fchenden Zweigen der", "tokens": ["Jun\u00b7ger", "Nach\u00b7ti\u00b7gal\u00b7len", "auf", "bl\u00fc\u00b7hen\u00b7den", "Zwei\u00b7gen", "der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "APPR", "ADJA", "NN", "ART"], "meter": "+-+-+--+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.32": {"text": "Myrten.", "tokens": ["Myr\u00b7ten", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.33": {"text": "Und mir kehrte die Weisheit zur\u00fck; sie pfl\u00fckte den", "tokens": ["Und", "mir", "kehr\u00b7te", "die", "Weis\u00b7heit", "zu\u00b7r\u00fck", ";", "sie", "pfl\u00fck\u00b7te", "den"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "PTKVZ", "$.", "PPER", "VVFIN", "ART"], "meter": "--+--+--+-+-+", "measure": "anapaest.tri.plus"}, "line.34": {"text": "Oelzweig,", "tokens": ["O\u00b7el\u00b7zweig", ","], "token_info": ["word", "punct"], "pos": ["NE", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.35": {"text": "Den ich dir reiche; sie redet durch mich; vernim", "tokens": ["Den", "ich", "dir", "rei\u00b7che", ";", "sie", "re\u00b7det", "durch", "mich", ";", "ver\u00b7nim"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["NE", "PPER", "PPER", "VVFIN", "$.", "PPER", "VVFIN", "APPR", "PPER", "$.", "XY"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "und sey weise!", "tokens": ["und", "sey", "wei\u00b7se", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.37": {"text": "Siehe, zwar kr\u00e4nzen uns Locken der Jugend, doch", "tokens": ["Sie\u00b7he", ",", "zwar", "kr\u00e4n\u00b7zen", "uns", "Lo\u00b7cken", "der", "Ju\u00b7gend", ",", "doch"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["VVIMP", "$,", "ADV", "VVFIN", "PPER", "NN", "ART", "NN", "$,", "ADV"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.38": {"text": "rauschet der Lorbeer", "tokens": ["rau\u00b7schet", "der", "Lor\u00b7beer"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.39": {"text": "Ueber den Locken, es k\u00fchlet die Palme den Schweis", "tokens": ["Ue\u00b7ber", "den", "Lo\u00b7cken", ",", "es", "k\u00fch\u00b7let", "die", "Pal\u00b7me", "den", "Schweis"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "ART", "NN"], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.40": {"text": "an der Stirne.", "tokens": ["an", "der", "Stir\u00b7ne", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.41": {"text": "Fr\u00fch betraten wir beide den Pfad des ewigen", "tokens": ["Fr\u00fch", "be\u00b7tra\u00b7ten", "wir", "bei\u00b7de", "den", "Pfad", "des", "e\u00b7wi\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "PIS", "ART", "NN", "ART", "ADJA"], "meter": "+-+--+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.42": {"text": "Ruhmes,", "tokens": ["Ruh\u00b7mes", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.43": {"text": "Fr\u00fch erreichten wir beide das Ziel. Auf trozenden", "tokens": ["Fr\u00fch", "er\u00b7reich\u00b7ten", "wir", "bei\u00b7de", "das", "Ziel", ".", "Auf", "tro\u00b7zen\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "PIS", "ART", "NN", "$.", "APPR", "ADJA"], "meter": "+-+--+--+-+--", "measure": "trochaic.penta.relaxed"}, "line.44": {"text": "Felsen", "tokens": ["Fel\u00b7sen"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.45": {"text": "Stehn wir und l\u00e4cheln entgegen dem Strome der", "tokens": ["Stehn", "wir", "und", "l\u00e4\u00b7cheln", "ent\u00b7ge\u00b7gen", "dem", "Stro\u00b7me", "der"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "KON", "VVFIN", "APPR", "ART", "NN", "ART"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.46": {"text": "kommenden Zeiten.", "tokens": ["kom\u00b7men\u00b7den", "Zei\u00b7ten", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.47": {"text": "Hier besuchen uns oft Kronions liebliche T\u00f6chter,", "tokens": ["Hier", "be\u00b7su\u00b7chen", "uns", "oft", "Kro\u00b7nions", "lieb\u00b7li\u00b7che", "T\u00f6ch\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "NE", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+--+-", "measure": "hexameter"}, "line.48": {"text": "Lehren uns oft die eigne Leier zu stimmen, und", "tokens": ["Leh\u00b7ren", "uns", "oft", "die", "eig\u00b7ne", "Lei\u00b7er", "zu", "stim\u00b7men", ",", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["NN", "PPER", "ADV", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$,", "KON"], "meter": "+--+-+-+--+-+", "measure": "iambic.hexa.invert"}, "line.49": {"text": "bringen", "tokens": ["brin\u00b7gen"], "token_info": ["word"], "pos": ["VVINF"], "meter": "+-", "measure": "trochaic.single"}, "line.50": {"text": "Oft herab vom Olympos die Harfe des M\u00e4o-", "tokens": ["Oft", "her\u00b7ab", "vom", "O\u00b7lym\u00b7pos", "die", "Har\u00b7fe", "des", "M\u00e4o"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPRART", "NN", "ART", "NN", "ART", "TRUNC"], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.51": {"text": "niden.", "tokens": ["ni\u00b7den", "."], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.52": {"text": "Las uns beide den Harfengesang des g\u00f6ttlichen", "tokens": ["Las", "uns", "bei\u00b7de", "den", "Har\u00b7fen\u00b7ge\u00b7sang", "des", "g\u00f6tt\u00b7li\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "PPER", "PIS", "ART", "NN", "ART", "ADJA"], "meter": "+-+--+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.53": {"text": "Greisen", "tokens": ["Grei\u00b7sen"], "token_info": ["word"], "pos": ["NE"], "meter": "+-", "measure": "trochaic.single"}, "line.54": {"text": "Unserm Volke singen; wir lieben den G\u00f6ttlichen", "tokens": ["Un\u00b7serm", "Vol\u00b7ke", "sin\u00b7gen", ";", "wir", "lie\u00b7ben", "den", "G\u00f6tt\u00b7li\u00b7chen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVINF", "$.", "PPER", "VVFIN", "ART", "NN"], "meter": "+-+-+--+--+--", "measure": "trochaic.penta.relaxed"}, "line.55": {"text": "beide!", "tokens": ["bei\u00b7de", "!"], "token_info": ["word", "punct"], "pos": ["PIS", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.56": {"text": "Freund, gehabe dich wol! Ich kenne die rufende", "tokens": ["Freund", ",", "ge\u00b7ha\u00b7be", "dich", "wol", "!", "Ich", "ken\u00b7ne", "die", "ru\u00b7fen\u00b7de"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "VVFIN", "PPER", "ADV", "$.", "PPER", "VVFIN", "ART", "NN"], "meter": "+-+--+-+--+--", "measure": "trochaic.penta.relaxed"}, "line.57": {"text": "Stimme,", "tokens": ["Stim\u00b7me", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.58": {"text": "H\u00f6re wiehern die feurigen Ross\u2019 am flammenden", "tokens": ["H\u00f6\u00b7re", "wie\u00b7hern", "die", "feu\u00b7ri\u00b7gen", "Ross'", "am", "flam\u00b7men\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ART", "ADJA", "NN", "APPRART", "ADJA"], "meter": "+-+--+--+-+--", "measure": "trochaic.penta.relaxed"}, "line.59": {"text": "Wagen;", "tokens": ["Wa\u00b7gen", ";"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.60": {"text": "Siehe, mir winkt die Mus\u2019, ich folge der win-", "tokens": ["Sie\u00b7he", ",", "mir", "winkt", "die", "Mus'", ",", "ich", "fol\u00b7ge", "der", "win"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVIMP", "$,", "PPER", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "ART", "TRUNC"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.61": {"text": "kenden G\u00f6ttin.", "tokens": ["ken\u00b7den", "G\u00f6t\u00b7tin", "."], "token_info": ["word", "word", "punct"], "pos": ["PIAT", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}}}}