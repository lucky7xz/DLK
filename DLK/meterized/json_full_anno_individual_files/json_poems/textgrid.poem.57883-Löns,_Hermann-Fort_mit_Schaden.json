{"textgrid.poem.57883": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Fort mit Schaden", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was von den V\u00e4tern wir geerbt,", "tokens": ["Was", "von", "den", "V\u00e4\u00b7tern", "wir", "ge\u00b7erbt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ach, Plunder ist's zumeist,", "tokens": ["Ach", ",", "Plun\u00b7der", "ist's", "zu\u00b7meist", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie im Kolleg mit k\u00fchnem Mut", "tokens": ["Wie", "im", "Kol\u00b7leg", "mit", "k\u00fch\u00b7nem", "Mut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPRART", "NN", "APPR", "ADJA", "NN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Herr Borchers uns beweist.", "tokens": ["Herr", "Bor\u00b7chers", "uns", "be\u00b7weist", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "\u00bbholzgraben, das ist abgeschmackt,\u00ab", "tokens": ["\u00bb", "holz\u00b7gra\u00b7ben", ",", "das", "ist", "ab\u00b7ge\u00b7schmackt", ",", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVINF", "$,", "PDS", "VAFIN", "VVPP", "$,", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So sprach der Stadtpapa;", "tokens": ["So", "sprach", "der", "Stadt\u00b7pa\u00b7pa", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbich sehe keinen Graben dort,", "tokens": ["\u00bb", "ich", "se\u00b7he", "kei\u00b7nen", "Gra\u00b7ben", "dort", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PIAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und Holz ist auch nicht da!\u00ab", "tokens": ["Und", "Holz", "ist", "auch", "nicht", "da", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "NN", "VAFIN", "ADV", "PTKNEG", "ADV", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und wenn Senator Plathner auch", "tokens": ["Und", "wenn", "Se\u00b7na\u00b7tor", "Plath\u00b7ner", "auch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "NN", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dem Redner widerspricht,", "tokens": ["Dem", "Red\u00b7ner", "wi\u00b7der\u00b7spricht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Was k\u00fcmmert die Historie uns?", "tokens": ["Was", "k\u00fcm\u00b7mert", "die", "His\u00b7to\u00b7rie", "uns", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So was geniert uns nicht!", "tokens": ["So", "was", "ge\u00b7niert", "uns", "nicht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWS", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Damit der Redner auch was hat", "tokens": ["Da\u00b7mit", "der", "Red\u00b7n