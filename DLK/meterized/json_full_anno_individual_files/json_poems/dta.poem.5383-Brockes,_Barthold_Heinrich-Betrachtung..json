{"dta.poem.5383": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Betrachtung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mit innern Freuden steh ich hier,", "tokens": ["Mit", "in\u00b7nern", "Freu\u00b7den", "steh", "ich", "hier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Beschaue der Gesch\u00f6pfe Zier,", "tokens": ["Be\u00b7schau\u00b7e", "der", "Ge\u00b7sch\u00f6p\u00b7fe", "Zier", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und denck\u2019 an den, der sie gemacht.", "tokens": ["Und", "denck", "an", "den", ",", "der", "sie", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "APPR", "ART", "$,", "PRELS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mich deucht dabey, da\u00df ihre Pracht,", "tokens": ["Mich", "deucht", "da\u00b7bey", ",", "da\u00df", "ih\u00b7re", "Pracht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PAV", "$,", "KOUS", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die meine Seel durchs Aug\u2019 empfindet,", "tokens": ["Die", "mei\u00b7ne", "Seel", "durchs", "Aug'", "emp\u00b7fin\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Sie nicht zum daucken nur entz\u00fcndet;", "tokens": ["Sie", "nicht", "zum", "dau\u00b7cken", "nur", "ent\u00b7z\u00fcn\u00b7det", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "APPRART", "VVFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Nein, da\u00df sie gleichsam in der Lust", "tokens": ["Nein", ",", "da\u00df", "sie", "gleich\u00b7sam", "in", "der", "Lust"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "KOUS", "PPER", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Zum Sch\u00f6pfer eine Th\u00fcre findet,", "tokens": ["Zum", "Sch\u00f6p\u00b7fer", "ei\u00b7ne", "Th\u00fc\u00b7re", "fin\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Ja, da\u00df sich GOTT mit unsrer Brust,", "tokens": ["Ja", ",", "da\u00df", "sich", "GoTT", "mit", "uns\u00b7rer", "Brust", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "KOUS", "PRF", "NE", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Durch dieses Mittel, selbst verbindet.", "tokens": ["Durch", "die\u00b7ses", "Mit\u00b7tel", ",", "selbst", "ver\u00b7bin\u00b7det", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}