{"textgrid.poem.40498": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Einsam stand ein grauer Felsen", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Einsam stand ein grauer Felsen", "tokens": ["Ein\u00b7sam", "stand", "ein", "grau\u00b7er", "Fel\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mitten in das Meer ges\u00e4t;", "tokens": ["Mit\u00b7ten", "in", "das", "Meer", "ge\u00b7s\u00e4t", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Fast schon wollt' ich ihn beneiden,", "tokens": ["Fast", "schon", "wollt'", "ich", "ihn", "be\u00b7nei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VMFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df er einsam, fest doch steht.", "tokens": ["Da\u00df", "er", "ein\u00b7sam", ",", "fest", "doch", "steht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "$,", "VVFIN", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Einsam auf dem grauen Felsen", "tokens": ["Ein\u00b7sam", "auf", "dem", "grau\u00b7en", "Fel\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gr\u00fcnt' ein Baum, gar stolz und k\u00fchn;", "tokens": ["Gr\u00fcnt'", "ein", "Baum", ",", "gar", "stolz", "und", "k\u00fchn", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Fast schien mir der Baum zu loben,", "tokens": ["Fast", "schien", "mir", "der", "Baum", "zu", "lo\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Da\u00df er einsam, doch so gr\u00fcn.", "tokens": ["Da\u00df", "er", "ein\u00b7sam", ",", "doch", "so", "gr\u00fcn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "$,", "ADV", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Einsam kreist' um Baum und Felsen", "tokens": ["Ein\u00b7sam", "kreist'", "um", "Baum", "und", "Fel\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "APPR", "NE", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Eine Lerche leichtbeschwingt;", "tokens": ["Ei\u00b7ne", "Ler\u00b7che", "leicht\u00b7be\u00b7schwingt", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Fast wollt' ich sie gl\u00fccklich preisen,", "tokens": ["Fast", "wollt'", "ich", "sie", "gl\u00fcck\u00b7lich", "prei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPER", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df sie noch so fr\u00f6hlich singt.", "tokens": ["Da\u00df", "sie", "noch", "so", "fr\u00f6h\u00b7lich", "singt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Aber Felsen, Baum und Lerche,", "tokens": ["A\u00b7ber", "Fel\u00b7sen", ",", "Baum", "und", "Ler\u00b7che", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "NE", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Jetzt beneid' ich euch nicht sehr!", "tokens": ["Jetzt", "be\u00b7neid'", "ich", "euch", "nicht", "sehr", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "PTKNEG", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn es warf ein Sto\u00df des Windes", "tokens": ["Denn", "es", "warf", "ein", "Sto\u00df", "des", "Win\u00b7des"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schnell den einzlen Baum ins Meer.", "tokens": ["Schnell", "den", "einz\u00b7len", "Baum", "ins", "Meer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "M\u00fcd' ins Wasser sank die Lerche,", "tokens": ["M\u00fcd'", "ins", "Was\u00b7ser", "sank", "die", "Ler\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Eh' die Schwestern sie erreicht;", "tokens": ["Eh'", "die", "Schwes\u00b7tern", "sie", "er\u00b7reicht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und die Fluthen unterw\u00fchlten", "tokens": ["Und", "die", "Flut\u00b7hen", "un\u00b7ter\u00b7w\u00fchl\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Selbst den Fels, den einzlen, leicht!", "tokens": ["Selbst", "den", "Fels", ",", "den", "einz\u00b7len", ",", "leicht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "ART", "VVINF", "$,", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Ach, da mu\u00dft' ich euer denken,", "tokens": ["Ach", ",", "da", "mu\u00dft'", "ich", "eu\u00b7er", "den\u00b7ken", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "KOUS", "VMFIN", "PPER", "PPOSAT", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dichter meines Vaterlands,", "tokens": ["Dich\u00b7ter", "mei\u00b7nes", "Va\u00b7ter\u00b7lands", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da ihr einzeln, fern den Br\u00fcdern,", "tokens": ["Da", "ihr", "ein\u00b7zeln", ",", "fern", "den", "Br\u00fc\u00b7dern", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "$,", "ADJD", "ART", "NN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "W\u00e4hnt zu pfl\u00fccken euren Kranz.", "tokens": ["W\u00e4hnt", "zu", "pfl\u00fc\u00b7cken", "eu\u00b7ren", "Kranz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKZU", "VVINF", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Gegen Nord und S\u00fcd und Osten", "tokens": ["Ge\u00b7gen", "Nord", "und", "S\u00fcd", "und", "Os\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "KON", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Steht ihr sehnend hingewandt,", "tokens": ["Steht", "ihr", "seh\u00b7nend", "hin\u00b7ge\u00b7wandt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ach, doch Manche mit dem R\u00fccken", "tokens": ["Ach", ",", "doch", "Man\u00b7che", "mit", "dem", "R\u00fc\u00b7cken"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "ADV", "PIS", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gen das eigne Vaterland!", "tokens": ["Gen", "das", "eig\u00b7ne", "Va\u00b7ter\u00b7land", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Einzle Felsen nur im Meere,", "tokens": ["Einz\u00b7le", "Fel\u00b7sen", "nur", "im", "Mee\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Einzle B\u00e4ume seid ihr nur,", "tokens": ["Einz\u00b7le", "B\u00e4u\u00b7me", "seid", "ihr", "nur", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Einzle Lerchen, einsam singend", "tokens": ["Einz\u00b7le", "Ler\u00b7chen", ",", "ein\u00b7sam", "sin\u00b7gend"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PIAT", "NN", "$,", "ADJD", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In dem \u00f6den Luftazur.", "tokens": ["In", "dem", "\u00f6\u00b7den", "Luf\u00b7ta\u00b7zur", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Trotz'ge Felsen, r\u00fcckt zusammen!", "tokens": ["Trotz'\u00b7ge", "Fel\u00b7sen", ",", "r\u00fcckt", "zu\u00b7sam\u00b7men", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Irre Lerchen, sammelt euch!", "tokens": ["Ir\u00b7re", "Ler\u00b7chen", ",", "sam\u00b7melt", "euch", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Stolze B\u00e4um', umrankt, umschlinget", "tokens": ["Stol\u00b7ze", "B\u00e4um'", ",", "um\u00b7rankt", ",", "um\u00b7schlin\u00b7get"], "token_info": ["word", "word", "punct", "word", "punct", "word"], "pos": ["ADJA", "NN", "$,", "VVFIN", "$,", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Euch in Zweig' und Wurzeln reich!", "tokens": ["Euch", "in", "Zweig'", "und", "Wur\u00b7zeln", "reich", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "KON", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "La\u00dft uns sein ein Wall von Felsen,", "tokens": ["La\u00dft", "uns", "sein", "ein", "Wall", "von", "Fel\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPOSAT", "ART", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der als Damm, gar stolz und fest,", "tokens": ["Der", "als", "Damm", ",", "gar", "stolz", "und", "fest", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "KOUS", "NN", "$,", "ADV", "ADJD", "KON", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Von dem Meere der Gemeinheit", "tokens": ["Von", "dem", "Mee\u00b7re", "der", "Ge\u00b7mein\u00b7heit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sich nicht unterw\u00fchlen l\u00e4\u00dft!", "tokens": ["Sich", "nicht", "un\u00b7ter\u00b7w\u00fch\u00b7len", "l\u00e4\u00dft", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "PTKNEG", "VVINF", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "La\u00dft uns sein ein Wald von B\u00e4umen,", "tokens": ["La\u00dft", "uns", "sein", "ein", "Wald", "von", "B\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPOSAT", "ART", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Im Vereine doppelt gr\u00fcn;", "tokens": ["Im", "Ver\u00b7ei\u00b7ne", "dop\u00b7pelt", "gr\u00fcn", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ueber den verschlung'nen Wipfeln", "tokens": ["Ue\u00b7ber", "den", "ver\u00b7schlung'\u00b7nen", "Wip\u00b7feln"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Rauscht der Sturm ohnm\u00e4chtig hin!", "tokens": ["Rauscht", "der", "Sturm", "ohn\u00b7m\u00e4ch\u00b7tig", "hin", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "La\u00dft uns sein ein Chor von Lerchen,", "tokens": ["La\u00dft", "uns", "sein", "ein", "Chor", "von", "Ler\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPOSAT", "ART", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "O dann klingt er doppelt sch\u00f6n", "tokens": ["O", "dann", "klingt", "er", "dop\u00b7pelt", "sch\u00f6n"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "ADV", "VVFIN", "PPER", "ADJD", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Der Gesang von hundert Kehlen,", "tokens": ["Der", "Ge\u00b7sang", "von", "hun\u00b7dert", "Keh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "CARD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wirbelnd in die Sonnenh\u00f6hn!", "tokens": ["Wir\u00b7belnd", "in", "die", "Son\u00b7nen\u00b7h\u00f6hn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.13": {"line.1": {"text": "Einsam stand ein grauer Felsen", "tokens": ["Ein\u00b7sam", "stand", "ein", "grau\u00b7er", "Fel\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mitten in das Meer ges\u00e4t;", "tokens": ["Mit\u00b7ten", "in", "das", "Meer", "ge\u00b7s\u00e4t", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Fast schon wollt' ich ihn beneiden,", "tokens": ["Fast", "schon", "wollt'", "ich", "ihn", "be\u00b7nei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VMFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df er einsam, fest doch steht.", "tokens": ["Da\u00df", "er", "ein\u00b7sam", ",", "fest", "doch", "steht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "$,", "VVFIN", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "Einsam auf dem grauen Felsen", "tokens": ["Ein\u00b7sam", "auf", "dem", "grau\u00b7en", "Fel\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gr\u00fcnt' ein Baum, gar stolz und k\u00fchn;", "tokens": ["Gr\u00fcnt'", "ein", "Baum", ",", "gar", "stolz", "und", "k\u00fchn", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Fast schien mir der Baum zu loben,", "tokens": ["Fast", "schien", "mir", "der", "Baum", "zu", "lo\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Da\u00df er einsam, doch so gr\u00fcn.", "tokens": ["Da\u00df", "er", "ein\u00b7sam", ",", "doch", "so", "gr\u00fcn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "$,", "ADV", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "tro