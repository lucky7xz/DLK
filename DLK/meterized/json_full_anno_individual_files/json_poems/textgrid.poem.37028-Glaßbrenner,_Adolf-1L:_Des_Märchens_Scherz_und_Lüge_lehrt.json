{"textgrid.poem.37028": {"metadata": {"author": {"name": "Gla\u00dfbrenner, Adolf", "birth": "N.A.", "death": "N.A."}, "title": "1L: Des M\u00e4rchens Scherz und L\u00fcge lehrt", "genre": "verse", "period": "N.A.", "pub_year": 1843, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Des M\u00e4rchens Scherz und L\u00fcge lehrt", "tokens": ["Des", "M\u00e4r\u00b7chens", "Scherz", "und", "L\u00fc\u00b7ge", "lehrt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "KON", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dir Ernst und Wahrheit klar;", "tokens": ["Dir", "Ernst", "und", "Wahr\u00b7heit", "klar", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NE", "KON", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Spiegel zeigt Dein Bild verkehrt", "tokens": ["Der", "Spie\u00b7gel", "zeigt", "Dein", "Bild", "ver\u00b7kehrt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und doch so klar und wahr!", "tokens": ["Und", "doch", "so", "klar", "und", "wahr", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Nein, was ist doch Das f\u00fcr ein wunderlich Reich!", "tokens": ["Nein", ",", "was", "ist", "doch", "Das", "f\u00fcr", "ein", "wun\u00b7der\u00b7lich", "Reich", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PWS", "VAFIN", "ADV", "PDS", "APPR", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Steinhart sind die Betten, das Pflaster ist weich!", "tokens": ["Stein\u00b7hart", "sind", "die", "Bet\u00b7ten", ",", "das", "Pflas\u00b7ter", "ist", "weich", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$,", "ART", "NN", "VAFIN", "ADJD", "$."], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Champagnerwein stellet auf Kohlen man hei\u00df,", "tokens": ["Cham\u00b7pag\u00b7ner\u00b7wein", "stel\u00b7let", "auf", "Koh\u00b7len", "man", "hei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "NN", "PIS", "ADJD", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Den Thee und den Plumpudding aber in Eis!", "tokens": ["Den", "Thee", "und", "den", "Plum\u00b7pud\u00b7ding", "a\u00b7ber", "in", "Eis", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "ADV", "APPR", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Steigt geputzt in das Bad, sielt nackt sich im Sand,", "tokens": ["Steigt", "ge\u00b7putzt", "in", "das", "Bad", ",", "sielt", "nackt", "sich", "im", "Sand", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "APPR", "ART", "NE", "$,", "VVFIN", "ADJD", "PRF", "APPRART", "NN", "$,"], "meter": "+-+--+-+--+", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "K\u00fc\u00dft der reizendsten Weiblein und Jungfrauen \u2013 Hand!", "tokens": ["K\u00fc\u00dft", "der", "rei\u00b7zends\u00b7ten", "Weib\u00b7lein", "und", "Jung\u00b7frau\u00b7en", "\u2013", "Hand", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "KON", "NN", "$(", "NN", "$."], "meter": "+-+--+--+--+", "measure": "trochaic.penta.relaxed"}, "line.7": {"text": "Spuckt's Fleisch weg vom Steine der Kirsche,", "tokens": ["Spuckt's", "Fleisch", "weg", "vom", "Stei\u00b7ne", "der", "Kir\u00b7sche", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ADV", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.8": {"text": "Und benutzt nur die H\u00f6rner vom Hirsche!", "tokens": ["Und", "be\u00b7nutzt", "nur", "die", "H\u00f6r\u00b7ner", "vom", "Hir\u00b7sche", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "NN", "APPRART", "NN", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.9": {"text": "Stellt in's Wasser der Blumen sich viele", "tokens": ["Stellt", "in's", "Was\u00b7ser", "der", "Blu\u00b7men", "sich", "vie\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPRART", "NN", "ART", "NN", "PRF", "PIAT"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.10": {"text": "Nach unten! nach oben die Stiele.", "tokens": ["Nach", "un\u00b7ten", "!", "nach", "o\u00b7ben", "die", "Stie\u00b7le", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "$.", "APPR", "ADV", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.3": {"line.1": {"text": "Man g\u00e4hnt in dem g\u00f6ttlichen Tempel: Natur!", "tokens": ["Man", "g\u00e4hnt", "in", "dem", "g\u00f6tt\u00b7li\u00b7chen", "Tem\u00b7pel", ":", "Na\u00b7tur", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "ART", "ADJA", "NN", "$.", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Und erbaut in dem eigen erbauten sich nur;", "tokens": ["Und", "er\u00b7baut", "in", "dem", "ei\u00b7gen", "er\u00b7bau\u00b7ten", "sich", "nur", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "ADJD", "VVFIN", "PRF", "ADV", "$."], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.3": {"text": "Wichst Stiefel und Schuh' sich mit Caviar blank,", "tokens": ["Wichst", "Stie\u00b7fel", "und", "Schuh'", "sich", "mit", "Ca\u00b7vi\u00b7ar", "blank", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NN", "PRF", "APPR", "NE", "PTKVZ", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Und tractirt sich mit Wichse und schmierigem Trank!", "tokens": ["Und", "trac\u00b7tirt", "sich", "mit", "Wich\u00b7se", "und", "schmie\u00b7ri\u00b7gem", "Trank", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+---+--+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Ist zum Gr\u00fcbeln nach Unsinn allst\u00fcndlich bereit;", "tokens": ["Ist", "zum", "Gr\u00fc\u00b7beln", "nach", "Un\u00b7sinn", "all\u00b7st\u00fcnd\u00b7lich", "be\u00b7reit", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPRART", "NN", "APPR", "NN", "ADJD", "ADJD", "$."], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.6": {"text": "Zw\u00e4ngt Liebe und Lust in gemessene Zeit!", "tokens": ["Zw\u00e4ngt", "Lie\u00b7be", "und", "Lust", "in", "ge\u00b7mes\u00b7se\u00b7ne", "Zeit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.7": {"text": "Schmei\u00dft's Gl\u00fcck aus der Th\u00fcr um halb Zw\u00f6lfe;", "tokens": ["Schmei\u00dft's", "Gl\u00fcck", "aus", "der", "Th\u00fcr", "um", "halb", "Zw\u00f6l\u00b7fe", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "APPR", "ART", "NN", "APPR", "ADJD", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.8": {"text": "N\u00e4hrt am Busen sich Schlangen und W\u00f6lfe;", "tokens": ["N\u00e4hrt", "am", "Bu\u00b7sen", "sich", "Schlan\u00b7gen", "und", "W\u00f6l\u00b7fe", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "PRF", "NN", "KON", "NN", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.9": {"text": "Wirft die Auster beim leckeren Mahle", "tokens": ["Wirft", "die", "Aus\u00b7ter", "beim", "le\u00b7cke\u00b7ren", "Mah\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "Auf die Erde und kaut an der Schaale!", "tokens": ["Auf", "die", "Er\u00b7de", "und", "kaut", "an", "der", "Schaa\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Lammdem\u00fcthig seufzt man im h\u00e4rtesten Fluch,", "tokens": ["Lamm\u00b7de\u00b7m\u00fct\u00b7hig", "seufzt", "man", "im", "h\u00e4r\u00b7tes\u00b7ten", "Fluch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PIS", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Und martert und mordet sich oft um ein Buch!", "tokens": ["Und", "mar\u00b7tert", "und", "mor\u00b7det", "sich", "oft", "um", "ein", "Buch", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "PRF", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Und schnuppert nach Mist, und hat Ekel vor Duft,", "tokens": ["Und", "schnup\u00b7pert", "nach", "Mist", ",", "und", "hat", "E\u00b7kel", "vor", "Duft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "$,", "KON", "VAFIN", "NN", "APPR", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Hat die Fr\u00fcchte vor'm Maule und bei\u00dft in die Luft!", "tokens": ["Hat", "die", "Fr\u00fcch\u00b7te", "vor'm", "Mau\u00b7le", "und", "bei\u00dft", "in", "die", "Luft", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "APPRART", "NN", "KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.5": {"text": "Tritt die bl\u00fchende Rose mit F\u00fc\u00dfen", "tokens": ["Tritt", "die", "bl\u00fc\u00b7hen\u00b7de", "Ro\u00b7se", "mit", "F\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN", "APPR", "NN"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "Um am Dorne die S\u00fcnde zu b\u00fc\u00dfen!", "tokens": ["Um", "am", "Dor\u00b7ne", "die", "S\u00fcn\u00b7de", "zu", "b\u00fc\u00b7\u00dfen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "APPRART", "NN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.7": {"text": "Und verochst sich die herrliche Jugend!", "tokens": ["Und", "ve\u00b7rochst", "sich", "die", "herr\u00b7li\u00b7che", "Ju\u00b7gend", "!"], "token_inf