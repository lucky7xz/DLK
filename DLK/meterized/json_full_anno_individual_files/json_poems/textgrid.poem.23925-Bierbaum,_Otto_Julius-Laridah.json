{"textgrid.poem.23925": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Laridah", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach, mein Schatz ist durchgegangen,", "tokens": ["Ach", ",", "mein", "Schatz", "ist", "durch\u00b7ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPOSAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Laridah!", "tokens": ["La\u00b7ri\u00b7dah", "!"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Erst wollt ich ihn wiederfangen,", "tokens": ["Erst", "wollt", "ich", "ihn", "wie\u00b7der\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPER", "VVFIN", "$,"], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.4": {"text": "Laridah!", "tokens": ["La\u00b7ri\u00b7dah", "!"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Doch dann hab ich mich besonnen:", "tokens": ["Doch", "dann", "hab", "ich", "mich", "be\u00b7son\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "PRF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Laridah!", "tokens": ["La\u00b7ri\u00b7dah", "!"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Manch Verloren ist Gewonnen.", "tokens": ["Manch", "Ver\u00b7lo\u00b7ren", "ist", "Ge\u00b7won\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Laridah!", "tokens": ["La\u00b7ri\u00b7dah", "!"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.2": {"line.1": {"text": "Zwar es war ein s\u00fc\u00dfes M\u00e4dchen,", "tokens": ["Zwar", "es", "war", "ein", "s\u00fc\u00b7\u00dfes", "M\u00e4d\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Laridah!", "tokens": ["La\u00b7ri\u00b7dah", "!"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Und wir hatten manches Beetchen,", "tokens": ["Und", "wir", "hat\u00b7ten", "man\u00b7ches", "Beet\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Laridah!", "tokens": ["La\u00b7ri\u00b7dah", "!"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Nicht blo\u00df Veilchen, Tulpen, Rosen,", "tokens": ["Nicht", "blo\u00df", "Veil\u00b7chen", ",", "Tul\u00b7pen", ",", "Ro\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PTKNEG", "ADV", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Laridah!", "tokens": ["La\u00b7ri\u00b7dah", "!"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Auch zwei St\u00e4mmchen Aprikosen.", "tokens": ["Auch", "zwei", "St\u00e4mm\u00b7chen", "Ap\u00b7ri\u00b7ko\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "CARD", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Laridah!", "tokens": ["La\u00b7ri\u00b7dah", "!"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.3": {"line.1": {"text": "Und wir wohnten ganz alleine,", "tokens": ["Und", "wir", "wohn\u00b7ten", "ganz", "al\u00b7lei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Laridah!", "tokens": ["La\u00b7ri\u00b7dah", "!"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Hatten Nachbarinnen keine,", "tokens": ["Hat\u00b7ten", "Nach\u00b7ba\u00b7rin\u00b7nen", "kei\u00b7ne", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "PIAT", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Laridah!", "tokens": ["La\u00b7ri\u00b7dah", "!"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Unser Nest war zungensicher,", "tokens": ["Un\u00b7ser", "Nest", "war", "zun\u00b7gen\u00b7si\u00b7cher", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Laridah!", "tokens": ["La\u00b7ri\u00b7dah", "!"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Vor Gekeife und Gekicher.", "tokens": ["Vor", "Ge\u00b7kei\u00b7fe", "und", "Ge\u00b7ki\u00b7cher", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Laridah!", "tokens": ["La\u00b7ri\u00b7dah", "!"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.4": {"line.1": {"text": "Ach, nun sing ich all das Holde,", "tokens": ["Ach", ",", "nun", "sing", "ich", "all", "das", "Hol\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADV", "VVFIN", "PPER", "PIAT", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Laridah!", "tokens": ["La\u00b7ri\u00b7dah", "!"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Was ich doch vergessen wollte.", "tokens": ["Was", "ich", "doch", "ver\u00b7ges\u00b7sen", "woll\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Laridah!", "tokens": ["La\u00b7ri\u00b7dah", "!"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Nein, das hei\u00dft nicht klug gesungen,", "tokens": ["Nein", ",", "das", "hei\u00dft", "nicht", "klug", "ge\u00b7sun\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PDS", "VVFIN", "PTKNEG", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Laridah!", "tokens": ["La\u00b7ri\u00b7dah", "!"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Denn der Hase ist entsprungen.", "tokens": ["Denn", "der", "Ha\u00b7se", "ist", "ent\u00b7sprun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Laridah!", "tokens": ["La\u00b7ri\u00b7dah", "!"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.5": {"line.1": {"text": "Treue war nicht seine Sache,", "tokens": ["Treu\u00b7e", "war", "nicht", "sei\u00b7ne", "Sa\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PTKNEG", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Laridah!", "tokens": ["La\u00b7ri\u00b7dah", "!"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Drum ists besser, da\u00df ich lache:", "tokens": ["Drum", "ists", "bes\u00b7ser", ",", "da\u00df", "ich", "la\u00b7che", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "ADJD", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Laridah!", "tokens": ["La\u00b7ri\u00b7dah", "!"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "W\u00e4rs nicht gestern mir geschehen,", "tokens": ["W\u00e4rs", "nicht", "ge\u00b7stern", "mir", "ge\u00b7sche\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "ADV", "PPER", "VVPP", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.6": {"text": "Laridah!", "tokens": ["La\u00b7ri\u00b7dah", "!"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "M\u00fc\u00dft ich's morgen mich versehen.", "tokens": ["M\u00fc\u00dft", "ich's", "mor\u00b7gen", "mich", "ver\u00b7se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Laridah!", "tokens": ["La\u00b7ri\u00b7dah", "!"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.6": {"line.1": {"text": "Also, Herze, sei zufrieden,", "tokens": ["Al\u00b7so", ",", "Her\u00b7ze", ",", "sei", "zu\u00b7frie\u00b7den", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "$,", "VVFIN", "$,", "VAFIN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Laridah!", "tokens": ["La\u00b7ri\u00b7dah", "!"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Viele Hasen giebts hienieden,", "tokens": ["Vie\u00b7le", "Ha\u00b7sen", "giebts", "hien\u00b7ie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Laridah!", "tokens": ["La\u00b7ri\u00b7dah", "!"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Ist der eine dir entlaufen,", "tokens": ["Ist", "der", "ei\u00b7ne", "dir", "ent\u00b7lau\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ART", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Laridah!", "tokens": ["La\u00b7ri\u00b7dah", "!"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Kannst du einen andern kaufen.", "tokens": ["Kannst", "du", "ei\u00b7nen", "an\u00b7dern", "kau\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "ADJA", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Laridah!", "tokens": ["La\u00b7ri\u00b7dah", "!"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.7": {"line.1": {"text": "Einen sch\u00f6nen, weichen, wei\u00dfen,", "tokens": ["Ei\u00b7nen", "sch\u00f6\u00b7nen", ",", "wei\u00b7chen", ",", "wei\u00b7\u00dfen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "$,", "VVINF", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Laridah!", "tokens": ["La\u00b7ri\u00b7dah", "!"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Mucki-Nucki soll er hei\u00dfen,", "tokens": ["Mucki\u00b7Nucki", "soll", "er", "hei\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Laridah!", "tokens": ["La\u00b7ri\u00b7dah", "!"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Ach, wie schl\u00e4gt das Herz mir schnelle,", "tokens": ["Ach", ",", "wie", "schl\u00e4gt", "das", "Herz", "mir", "schnel\u00b7le", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWAV", "VVFIN", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Laridah!", "tokens": ["La\u00b7ri\u00b7dah", "!"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Springt er \u00fcber meine Schwelle,", "tokens": ["Springt", "er", "\u00fc\u00b7ber", "mei\u00b7ne", "Schwel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Laridah!", "tokens": ["La\u00b7ri\u00b7dah", "!"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.8": {"line.1": {"text": "Ach, mein Schatz ist durchgegangen,", "tokens": ["Ach", ",", "mein", "Schatz", "ist", "durch\u00b7ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPOSAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Laridah!", "tokens": ["La\u00b7ri\u00b7dah", "!"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Erst wollt ich ihn wiederfangen,", "tokens": ["Erst", "wollt", "ich", "ihn", "wie\u00b7der\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPER", "VVFIN", "$,"], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.4": {"text": "Laridah!", "tokens": ["La\u00b7ri\u00b7dah", "!"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Doch dann hab ich mich besonnen:", "tokens": ["Doch", "dann", "hab", "ich", "mich", "be\u00b7son\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "PRF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Laridah!", "tokens": ["La\u00b7ri\u00b7dah", "!"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Manch Verloren ist Gewonnen.", "tokens": ["Manch", "Ver\u00b7lo\u00b7ren", "ist", "Ge\u00b7won\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Laridah!", "tokens": ["La\u00b7ri\u00b7dah", "!"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.9": {"line.1": {"text": "Zwar es war ein s\u00fc\u00dfes M\u00e4dchen,", "tokens": ["Zwar", "es", "war", "ein", "s\u00fc\u00b7\u00dfes", "M\u00e4d\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Laridah!", "tokens": ["La\u00b7ri\u00b7dah", "!"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Und wir hatten manches Beetchen,", "tokens": ["Und", "wir", "hat\u00b7ten", "man\u00b7ches", "Beet\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Laridah!", "tokens": ["La\u00b7ri\u00b7dah", "!"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Nicht blo\u00df Veilchen, Tulpen, Rosen,", "tokens": ["Nicht", "blo\u00df", "Veil\u00b7chen", ",", "Tul\u00b7pen", ",", "Ro\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PTKNEG", "ADV", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Laridah!", "tokens": ["La\u00b7ri\u00b7dah", "!"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Auch zwei St\u00e4mmchen Aprikosen.", "tokens": ["Auch", "zwei", "St\u00e4mm\u00b7chen", "Ap\u00b7ri\u00b7ko\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "CARD", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Laridah!", "tokens": ["La\u00b7ri\u00b7dah", "!"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.10": {"line.1": {"text": "Und wir wohnten ganz alleine,", "tokens": ["Und", "wir", "wohn\u00b7ten", "ganz", "al\u00b7lei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Laridah!", "tokens": ["La\u00b7ri\u00b7dah", "!"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Hatten Nachbarinnen keine,", "tokens": ["Hat\u00b7ten", "Nach\u00b7ba\u00b7rin\u00b7nen", "kei\u00b7ne", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "PIAT", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Laridah!", "tokens": ["La\u00b7ri\u00b7dah", "!"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Unser Nest war zungensicher,", "tokens": ["Un\u00b7ser", "Nest", "war", "zun\u00b7gen\u00b7si\u00b7cher", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Laridah!", "tokens": ["La\u00b7ri\u00b7dah", "!"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Vor Gekeife und Gekicher.", "tokens": ["Vor", "Ge\u00b7kei\u00b7fe", "und", "Ge\u00b7ki\u00b7cher", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Laridah!", "tokens": ["La\u00b7ri\u00b7dah", "!"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.11": {"line.1": {"text": "Ach, nun sing ich all das Holde,", "tokens": ["Ach", ",", "nun", "sing", "ich", "all", "das", "Hol\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADV", "VVFIN", "PPER", "PIAT", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Laridah!", "tokens": ["La\u00b7ri\u00b7dah", "!"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Was ich doch vergessen wollte.", "tokens": ["Was", "ich", "doch", "ver\u00b7ges\u00b7sen", "woll\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Laridah!", "tokens": ["La\u00b7ri\u00b7dah", "!"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Nein, das hei\u00dft nicht klug gesungen,", "tokens": ["Nein", ",", "das", "hei\u00dft", "nicht", "klug", "ge\u00b7sun\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PDS", "VVFIN", "PTKNEG", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Laridah!", "tokens": ["La\u00b7ri\u00b7dah", "!"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Denn der Hase ist entsprungen.", "tokens": ["Denn", "der", "Ha\u00b7se", "ist", "ent\u00b7sprun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Laridah!", "tokens": ["La\u00b7ri\u00b7dah", "!"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.12": {"line.1": {"text": "Treue war nicht seine Sache,", "tokens": ["Treu\u00b7e", "war", "nicht", "sei\u00b7ne", "Sa\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PTKNEG", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Laridah!", "tokens": ["La\u00b7ri\u00b7dah", "!"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Drum ists besser, da\u00df ich lache:", "tokens": ["Drum", "ists", "bes\u00b7ser", ",", "da\u00df", "ich", "la\u00b7che", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "ADJD", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Laridah!", "tokens": ["La\u00b7ri\u00b7dah", "!"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "W\u00e4rs nicht gestern mir geschehen,", "tokens": ["W\u00e4rs", "nicht", "ge\u00b7stern", "mir", "ge\u00b7sche\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "ADV", "PPER", "VVPP", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.6": {"text": "Laridah!", "tokens": ["La\u00b7ri\u00b7dah", "!"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "M\u00fc\u00dft ich's morgen mich versehen.", "tokens": ["M\u00fc\u00dft", "ich's", "mor\u00b7gen", "mich", "ver\u00b7se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Laridah!", "tokens": ["La\u00b7ri\u00b7dah", "!"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.13": {"line.1": {"text": "Also, Herze, sei zufrieden,", "tokens": ["Al\u00b7so", ",", "Her\u00b7ze", ",", "sei", "zu\u00b7frie\u00b7den", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "$,", "VVFIN", "$,", "VAFIN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Laridah!", "tokens": ["La\u00b7ri\u00b7dah", "!"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Viele Hasen giebts hienieden,", "tokens": ["Vie\u00b7le", "Ha\u00b7sen", "giebts", "hien\u00b7ie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Laridah!", "tokens": ["La\u00b7ri\u00b7dah", "!"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Ist der eine dir entlaufen,", "tokens": ["Ist", "der", "ei\u00b7ne", "dir", "ent\u00b7lau\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ART", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Laridah!", "tokens": ["La\u00b7ri\u00b7dah", "!"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Kannst du einen andern kaufen.", "tokens": ["Kannst", "du", "ei\u00b7nen", "an\u00b7dern", "kau\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "ADJA", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Laridah!", "tokens": ["La\u00b7ri\u00b7dah", "!"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.14": {"line.1": {"text": "Einen sch\u00f6nen, weichen, wei\u00dfen,", "tokens": ["Ei\u00b7nen", "sch\u00f6\u00b7nen", ",", "wei\u00b7chen", ",", "wei\u00b7\u00dfen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "$,", "VVINF", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Laridah!", "tokens": ["La\u00b7ri\u00b7dah", "!"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Mucki-Nucki soll er hei\u00dfen,", "tokens": ["Mucki\u00b7Nucki", "soll", "er", "hei\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Laridah!", "tokens": ["La\u00b7ri\u00b7dah", "!"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Ach, wie schl\u00e4gt das Herz mir schnelle,", "tokens": ["Ach", ",", "wie", "schl\u00e4gt", "das", "Herz", "mir", "schnel\u00b7le", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWAV", "VVFIN", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Laridah!", "tokens": ["La\u00b7ri\u00b7dah", "!"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Springt er \u00fcber meine Schwelle,", "tokens": ["Springt", "er", "\u00fc\u00b7ber", "mei\u00b7ne", "Schwel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Laridah!", "tokens": ["La\u00b7ri\u00b7dah", "!"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+-+", "measure": "trochaic.di"}}}}}