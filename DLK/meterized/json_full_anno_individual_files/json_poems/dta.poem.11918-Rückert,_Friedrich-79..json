{"dta.poem.11918": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "79.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Zur H\u00fclle diene dir das Kleid, wol auch zum Schmucke,", "tokens": ["Zur", "H\u00fcl\u00b7le", "die\u00b7ne", "dir", "das", "Kleid", ",", "wol", "auch", "zum", "Schmu\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ART", "NN", "$,", "ADV", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nie zur Behinderung der Glieder, noch zum Drucke.", "tokens": ["Nie", "zur", "Be\u00b7hin\u00b7de\u00b7rung", "der", "Glie\u00b7der", ",", "noch", "zum", "Dru\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "ART", "NN", "$,", "ADV", "APPRART", "NN", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}}, "stanza.2": {"line.1": {"text": "So n\u00fctze dir zum Schutz das Wissen, auch zum Putz;", "tokens": ["So", "n\u00fct\u00b7ze", "dir", "zum", "Schutz", "das", "Wis\u00b7sen", ",", "auch", "zum", "Putz", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN", "ART", "NN", "$,", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nur Wissen, das den Geist beschweret, ist nichts nutz.", "tokens": ["Nur", "Wis\u00b7sen", ",", "das", "den", "Geist", "be\u00b7schwe\u00b7ret", ",", "ist", "nichts", "nutz", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "wor