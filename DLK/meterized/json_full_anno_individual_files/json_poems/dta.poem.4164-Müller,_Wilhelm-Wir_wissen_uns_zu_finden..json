{"dta.poem.4164": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Wir wissen uns zu finden.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1821", "urn": "urn:nbn:de:kobv:b4-200905194209", "language": ["de:0.85", "nl:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Kam der Sommer hergezogen,", "tokens": ["Kam", "der", "Som\u00b7mer", "her\u00b7ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Rosenbl\u00fcthchen war dabei,", "tokens": ["Ro\u00b7sen\u00b7bl\u00fcth\u00b7chen", "war", "da\u00b7bei", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PAV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bin ich hinterdrein geflogen,", "tokens": ["Bin", "ich", "hin\u00b7ter\u00b7drein", "ge\u00b7flo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wu\u00dfte nicht, ob's schicklich sei.", "tokens": ["Wu\u00df\u00b7te", "nicht", ",", "ob's", "schick\u00b7lich", "sei", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "$,", "KOUS", "ADJD", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Rosenbl\u00fcthchen, woll' mir geben", "tokens": ["Ro\u00b7sen\u00b7bl\u00fcth\u00b7chen", ",", "woll'", "mir", "ge\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "PWAV", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Nur ein Bl\u00e4ttchen, drauf zu leben!", "tokens": ["Nur", "ein", "Bl\u00e4tt\u00b7chen", ",", "drauf", "zu", "le\u00b7ben", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "PAV", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Sprach es: Klein ist dein Bewerben,", "tokens": ["Sprach", "es", ":", "Klein", "ist", "dein", "Be\u00b7wer\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$.", "NE", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Doch gar schnell mein Duft verweht.", "tokens": ["Doch", "gar", "schnell", "mein", "Duft", "ver\u00b7weht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "PPOSAT", "NN", "VVFIN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.9": {"text": "Sprach ich: Mit dir will ich sterben,", "tokens": ["Sprach", "ich", ":", "Mit", "dir", "will", "ich", "ster\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$.", "APPR", "PPER", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}