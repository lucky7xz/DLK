{"textgrid.poem.60778": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "28. An Karl den Zehnten", "genre": "verse", "period": "N.A.", "pub_year": 1830, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Aus deiner Ahnherrn bl\u00fchendem Reiche zogst", "tokens": ["Aus", "dei\u00b7ner", "Ahn\u00b7herrn", "bl\u00fc\u00b7hen\u00b7dem", "Rei\u00b7che", "zogst"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NE", "VVFIN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Umblickend oft auf l\u00e4ssigem Zelter du,", "tokens": ["Um\u00b7bli\u00b7ckend", "oft", "auf", "l\u00e4s\u00b7si\u00b7gem", "Zel\u00b7ter", "du", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "ADV", "APPR", "ADJA", "NN", "PPER", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "O zehnter Karl, von deiner S\u00f6hne", "tokens": ["O", "zehn\u00b7ter", "Karl", ",", "von", "dei\u00b7ner", "S\u00f6h\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "ADJA", "NE", "$,", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Frauen umjammert, der letzte Ritter!", "tokens": ["Frau\u00b7en", "um\u00b7jam\u00b7mert", ",", "der", "letz\u00b7te", "Rit\u00b7ter", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "$,", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Nicht lehrte Weisheit dich das erblichne Haar!", "tokens": ["Nicht", "lehr\u00b7te", "Weis\u00b7heit", "dich", "das", "er\u00b7blich\u00b7ne", "Haar", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "NN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Nicht sendet nach weichherzige Seufzer dir", "tokens": ["Nicht", "sen\u00b7det", "nach", "weich\u00b7her\u00b7zi\u00b7ge", "Seuf\u00b7zer", "dir"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "VVFIN", "APPR", "ADJA", "NN", "PPER"], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Frankreich, es weint dir nicht des Mitleids", "tokens": ["Fran\u00b7kreich", ",", "es", "weint", "dir", "nicht", "des", "Mit\u00b7leids"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "PPER", "VVFIN", "PPER", "PTKNEG", "ART", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Gastliche Tr\u00e4ne der stolze Brite.", "tokens": ["Gast\u00b7li\u00b7che", "Tr\u00e4\u00b7ne", "der", "stol\u00b7ze", "Bri\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Dein eignes Volk mi\u00dfkennend, und was die Zeit", "tokens": ["Dein", "eig\u00b7nes", "Volk", "mi\u00df\u00b7ken\u00b7nend", ",", "und", "was", "die", "Zeit"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVPP", "$,", "KON", "PWS", "ART", "NN"], "meter": "-+-+----+-+", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Umst\u00fcrzte, kalt aufn\u00f6tigend, hieltest du's", "tokens": ["Um\u00b7st\u00fcrz\u00b7te", ",", "kalt", "auf\u00b7n\u00f6\u00b7ti\u00b7gend", ",", "hiel\u00b7test", "du's"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "$,", "ADJD", "VVPP", "$,", "VVFIN", "NE"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Barbaren Gleich, die fern im S\u00fcdost", "tokens": ["Bar\u00b7ba\u00b7ren", "Gleich", ",", "die", "fern", "im", "S\u00fcd\u00b7ost"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "ADV", "$,", "PRELS", "ADJD", "APPRART", "NN"], "meter": "---+-+-+-", "measure": "unknown.measure.tri"}, "line.4": {"text": "Keuchen am Joch und das Joch beklatschen?", "tokens": ["Keu\u00b7chen", "am", "Joch", "und", "das", "Joch", "be\u00b7klat\u00b7schen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "KON", "ART", "NN", "VVINF", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.4": {"line.1": {"text": "Nicht fleu\u00dft in Frankreichs Adern Kroatenblut!", "tokens": ["Nicht", "fleu\u00dft", "in", "Fran\u00b7kreichs", "A\u00b7dern", "Kroa\u00b7ten\u00b7blut", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "APPR", "NE", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Freudvoll begr\u00fc\u00dft dreifarbige Wimpel schon", "tokens": ["Freud\u00b7voll", "be\u00b7gr\u00fc\u00dft", "drei\u00b7far\u00b7bi\u00b7ge", "Wim\u00b7pel", "schon"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVPP", "ADJA", "NN", "ADV"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Europa, m\u00e4nnlich aufgerichtet,", "tokens": ["Eu\u00b7ro\u00b7pa", ",", "m\u00e4nn\u00b7lich", "auf\u00b7ge\u00b7rich\u00b7tet", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ja, bis in Afrika jauchzt das Echo!", "tokens": ["Ja", ",", "bis", "in", "Af\u00b7ri\u00b7ka", "jauchzt", "das", "E\u00b7cho", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "KOUS", "APPR", "NE", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "L\u00e4ngst sind der Zeit blutd\u00fcrstige Greul ges\u00fchnt:", "tokens": ["L\u00e4ngst", "sind", "der", "Zeit", "blut\u00b7d\u00fcrs\u00b7ti\u00b7ge", "Greul", "ge\u00b7s\u00fchnt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Blut flo\u00df von jeher, wann die verj\u00fcngte Welt", "tokens": ["Blut", "flo\u00df", "von", "je\u00b7her", ",", "wann", "die", "ver\u00b7j\u00fcng\u00b7te", "Welt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPR", "ADV", "$,", "PWAV", "ART", "ADJA", "NN"], "meter": "++-+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Neukr\u00e4ftig aufwuchs, blutig siegte", "tokens": ["Neu\u00b7kr\u00e4f\u00b7tig", "auf\u00b7wuchs", ",", "blu\u00b7tig", "sieg\u00b7te"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJD", "VVFIN", "$,", "ADJD", "VVFIN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Christus und blutig erk\u00e4mpfte Luther", "tokens": ["Chris\u00b7tus", "und", "blu\u00b7tig", "er\u00b7k\u00e4mpf\u00b7te", "Lu\u00b7ther"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "KON", "ADJD", "ADJA", "NN"], "meter": "---+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Wahrheiten. Nicht mehr rufe die Manen an", "tokens": ["Wahr\u00b7hei\u00b7ten", ".", "Nicht", "mehr", "ru\u00b7fe", "die", "Ma\u00b7nen", "an"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$.", "PTKNEG", "ADV", "VVFIN", "ART", "NN", "APPR"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Des Bruders, der klagw\u00fcrdig und edel fiel,", "tokens": ["Des", "Bru\u00b7ders", ",", "der", "klag\u00b7w\u00fcr\u00b7dig", "und", "e\u00b7del", "fiel", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD", "KON", "ADJD", "VVFIN", "$,"], "meter": "-+--++--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Nicht aber schuldlos, seine Schwachheit", "tokens": ["Nicht", "a\u00b7ber", "schuld\u00b7los", ",", "sei\u00b7ne", "Schwach\u00b7heit"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PTKNEG", "ADV", "ADJD", "$,", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Tr\u00e4gt des Geschehenen schwerste H\u00e4lfte.", "tokens": ["Tr\u00e4gt", "des", "Ge\u00b7sche\u00b7he\u00b7nen", "schwers\u00b7te", "H\u00e4lf\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.7": {"line.1": {"text": "Uralte Blutschuld lastete lange schon", "tokens": ["Ur\u00b7al\u00b7te", "Blut\u00b7schuld", "las\u00b7te\u00b7te", "lan\u00b7ge", "schon"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "ADV", "ADV"], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Auf Capets Haus, seitdem den erlauchten Spro\u00df", "tokens": ["Auf", "Ca\u00b7pets", "Haus", ",", "seit\u00b7dem", "den", "er\u00b7lauch\u00b7ten", "Spro\u00df"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NE", "NN", "$,", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Ruhmvoller Kaiser einst der schn\u00f6de", "tokens": ["Ruhm\u00b7vol\u00b7ler", "Kai\u00b7ser", "einst", "der", "schn\u00f6\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "ADV", "ART", "ADJA"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Bruder des heiligen Ludwigs abhieb.", "tokens": ["Bru\u00b7der", "des", "hei\u00b7li\u00b7gen", "Lud\u00b7wigs", "ab\u00b7hieb", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NE", "VVFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.8": {"line.1": {"text": "Lern aus der Welt Jahrb\u00fcchern Gerechtigkeit,", "tokens": ["Lern", "aus", "der", "Welt", "Jahr\u00b7b\u00fc\u00b7chern", "Ge\u00b7rech\u00b7tig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "NN", "$,"], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Und stirb vers\u00f6hnt! Dein sonstiges Volk, es sei", "tokens": ["Und", "stirb", "ver\u00b7s\u00f6hnt", "!", "Dein", "sons\u00b7ti\u00b7ges", "Volk", ",", "es", "sei"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ADJD", "VVPP", "$.", "PPOSAT", "ADJA", "NN", "$,", "PPER", "VAFIN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Bollwerk der Freiheit k\u00fcnftighin uns,", "tokens": ["Boll\u00b7werk", "der", "Frei\u00b7heit", "k\u00fcnf\u00b7tig\u00b7hin", "uns", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVFIN", "PPER", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Gl\u00e4nzendes Edelgestein Europas!", "tokens": ["Gl\u00e4n\u00b7zen\u00b7des", "E\u00b7del\u00b7ge\u00b7stein", "Eu\u00b7ro\u00b7pas", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NE", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}, "stanza.9": {"line.1": {"text": "Nie reiz es mehr blindw\u00fctender Frevel auf,", "tokens": ["Nie", "reiz", "es", "mehr", "blind\u00b7w\u00fc\u00b7ten\u00b7der", "Fre\u00b7vel", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und K\u00f6nig Philipp herrsche gerecht und gut!", "tokens": ["Und", "K\u00f6\u00b7nig", "Phi\u00b7lipp", "herr\u00b7sche", "ge\u00b7recht", "und", "gut", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "NE", "VVFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+---+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Viel hangt an ihm! Nie war so heilig", "tokens": ["Viel", "hangt", "an", "ihm", "!", "Nie", "war", "so", "hei\u00b7lig"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PPER", "$.", "ADV", "VAFIN", "ADV", "ADJD"], "meter": "-+--++-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Irgend ein f\u00fcrstliches Haupt, wie seins ist.", "tokens": ["Ir\u00b7gend", "ein", "f\u00fcrst\u00b7li\u00b7ches", "Haupt", ",", "wie", "seins", "ist", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$,", "PWAV", "PPER", "VAFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.10": {"line.1": {"text": "Aus deiner Ahnherrn bl\u00fchendem Reiche zogst", "tokens": ["Aus", "dei\u00b7ner", "Ahn\u00b7herrn", "bl\u00fc\u00b7hen\u00b7dem", "Rei\u00b7che", "zogst"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NE", "VVFIN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Umblickend oft auf l\u00e4ssigem Zelter du,", "tokens": ["Um\u00b7bli\u00b7ckend", "oft", "auf", "l\u00e4s\u00b7si\u00b7gem", "Zel\u00b7ter", "du", ","], "token_in