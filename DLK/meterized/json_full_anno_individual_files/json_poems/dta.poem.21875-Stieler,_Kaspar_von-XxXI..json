{"dta.poem.21875": {"metadata": {"author": {"name": "Stieler, Kaspar von", "birth": "N.A.", "death": "N.A."}, "title": "XxXI.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1660", "urn": "urn:nbn:de:kobv:b4-20586-2", "language": ["de:0.99"], "booktitle": "Filidor der Dorfferer [i. e. Stieler, Kaspar von]: Die Geharnschte Venus. Hamburg, 1660."}, "poem": {"stanza.1": {"line.1": {"text": "Verr\u00e4htrisch Licht/ du hast mich zwar betro-", "tokens": ["Ver\u00b7r\u00e4h\u00b7trisch", "Licht", "/", "du", "hast", "mich", "zwar", "be\u00b7tro"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJD", "NN", "$(", "PPER", "VAFIN", "PPER", "ADV", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "da\u00df ich so offt den K\u00fcrzeren gezogen:", "tokens": ["da\u00df", "ich", "so", "offt", "den", "K\u00fcr\u00b7ze\u00b7ren", "ge\u00b7zo\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "doch di\u00dfmahl ist dein Feuer spat entbrant.", "tokens": ["doch", "di\u00df\u00b7mahl", "ist", "dein", "Feu\u00b7er", "spat", "ent\u00b7brant", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PPOSAT", "NN", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ich hab\u2019 es schon/ worauff ich ging/ empfan-", "tokens": ["Ich", "hab'", "es", "schon", "/", "wo\u00b7rauff", "ich", "ging", "/", "emp\u00b7fan"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "$(", "PWAV", "PPER", "VVFIN", "$(", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Nu hinderts nicht/ da\u00df du bist angegangen.", "tokens": ["Nu", "hin\u00b7derts", "nicht", "/", "da\u00df", "du", "bist", "an\u00b7ge\u00b7gan\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PTKNEG", "$(", "KOUS", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ich schlaffe fest. Sie hat sich umgewandt.", "tokens": ["Ich", "schlaf\u00b7fe", "fest", ".", "Sie", "hat", "sich", "um\u00b7ge\u00b7wandt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$.", "PPER", "VAFIN", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}