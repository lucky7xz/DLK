{"dta.poem.7571": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "70.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn du der Au\u00dfenwelt verschlie\u00dfest deine Sinne,", "tokens": ["Wenn", "du", "der", "Au\u00b7\u00dfen\u00b7welt", "ver\u00b7schlie\u00b7\u00dfest", "dei\u00b7ne", "Sin\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wirst du in dir das Welt- und Gottgeheimnis inne.", "tokens": ["Wirst", "du", "in", "dir", "das", "Welt", "und", "Gott\u00b7ge\u00b7heim\u00b7nis", "in\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "PPER", "ART", "TRUNC", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Nimm von der Welt nicht ein, was deinen Geist zerstreut,", "tokens": ["Nimm", "von", "der", "Welt", "nicht", "ein", ",", "was", "dei\u00b7nen", "Geist", "zer\u00b7streut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "PTKNEG", "PTKVZ", "$,", "PRELS", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nur soviel da\u00df daran dein Denken sich erneut.", "tokens": ["Nur", "so\u00b7viel", "da\u00df", "da\u00b7ran", "dein", "Den\u00b7ken", "sich", "er\u00b7neut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "KOUS", "PAV", "PPOSAT", "NN", "PRF", "ADJD", "$."], "meter": "+-+-+--+-+-+", "measure": "trochaic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "Nur einen Schimmer l\u00e4\u00dft ins dunkle Zimmer streifen,", "tokens": ["Nur", "ei\u00b7nen", "Schim\u00b7mer", "l\u00e4\u00dft", "ins", "dunk\u00b7le", "Zim\u00b7mer", "strei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wer in dem Strale will das ganze Licht begreifen.", "tokens": ["Wer", "in", "dem", "Stra\u00b7le", "will", "das", "gan\u00b7ze", "Licht", "be\u00b7grei\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "VMFIN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Dann mach das Fenster auf, damit du auch erkennst,", "tokens": ["Dann", "mach", "das", "Fens\u00b7ter", "auf", ",", "da\u00b7mit", "du", "auch", "er\u00b7kennst", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PTKVZ", "$,", "KOUS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das Licht ist mehr noch als sein farbiges Gespenst.", "tokens": ["Das", "Licht", "ist", "mehr", "noch", "als", "sein", "far\u00b7bi\u00b7ges", "Ge\u00b7spenst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADV", "KOKOM", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+--+-+-+--+", "measure": "iambic.penta.relaxed"}}}}}