{"dta.poem.9608": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Antwort der alten.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Jhr n\u00e4rrchen thut gemach/ was bildet ihr euch ein?", "tokens": ["Ihr", "n\u00e4rr\u00b7chen", "thut", "ge\u00b7mach", "/", "was", "bil\u00b7det", "ihr", "euch", "ein", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "VVFIN", "ADV", "$(", "PWS", "VVFIN", "PPER", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was habt ihr thoren viel vom alter zu gedencken?", "tokens": ["Was", "habt", "ihr", "tho\u00b7ren", "viel", "vom", "al\u00b7ter", "zu", "ge\u00b7den\u00b7cken", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "VVFIN", "ADV", "APPRART", "ADJA", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ist dieses euch verhast/ so last euch jung auffhencken/", "tokens": ["Ist", "die\u00b7ses", "euch", "ver\u00b7hast", "/", "so", "last", "euch", "jung", "auff\u00b7hen\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDAT", "PPER", "VVFIN", "$(", "ADV", "VVFIN", "PPER", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+++-", "measure": "unknown.measure.septa"}, "line.4": {"text": "Halt eure zung im zaum und last das spotten seyn.", "tokens": ["Halt", "eu\u00b7re", "zung", "im", "zaum", "und", "last", "das", "spot\u00b7ten", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "APPRART", "NN", "KON", "VVFIN", "PDS", "VVFIN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Mit eurer prahlerey wird wenig au\u00dfgericht/", "tokens": ["Mit", "eu\u00b7rer", "prah\u00b7le\u00b7rey", "wird", "we\u00b7nig", "au\u00df\u00b7ge\u00b7richt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "PIS", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Jhr stellt euch allzu stoltz und \u00fcberaus verwegen/", "tokens": ["Ihr", "stellt", "euch", "all\u00b7zu", "stoltz", "und", "\u00fc\u00b7be\u00b7raus", "ver\u00b7we\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKA", "ADJD", "KON", "ADV", "ADJD", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ach lernt das ding zuvor vern\u00fcnfftig \u00fcberlegen/", "tokens": ["Ach", "lernt", "das", "ding", "zu\u00b7vor", "ver\u00b7n\u00fcnff\u00b7tig", "\u00fc\u00b7berl\u00b7e\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "ART", "NN", "ADV", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Was gilts/ ob euer mund hernach nicht anders spricht.", "tokens": ["Was", "gilts", "/", "ob", "eu\u00b7er", "mund", "her\u00b7nach", "nicht", "an\u00b7ders", "spricht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$(", "KOUS", "PPOSAT", "NN", "ADV", "PTKNEG", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Da\u00df ihr uns alte nennt/ das thut ihr nur aus neyd/", "tokens": ["Da\u00df", "ihr", "uns", "al\u00b7te", "nennt", "/", "das", "thut", "ihr", "nur", "aus", "neyd", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADJA", "VVFIN", "$(", "PDS", "VVFIN", "PPER", "ADV", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Dieweil wir nicht mit euch so kindisch t\u00e4ndeln wollen/", "tokens": ["Die\u00b7weil", "wir", "nicht", "mit", "euch", "so", "kin\u00b7disch", "t\u00e4n\u00b7deln", "wol\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "APPR", "PPER", "ADV", "ADJD", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Wir w\u00e4ren sonst f\u00fcrwar noch \u00e4rger als die tollen/", "tokens": ["Wir", "w\u00e4\u00b7ren", "sonst", "f\u00fcr\u00b7war", "noch", "\u00e4r\u00b7ger", "als", "die", "tol\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ADV", "ADJD", "KOKOM", "ART", "ADJA", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Es wundert uns/ da\u00df ihr noch stets so kindisch seyd;", "tokens": ["Es", "wun\u00b7dert", "uns", "/", "da\u00df", "ihr", "noch", "stets", "so", "kin\u00b7disch", "seyd", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$(", "KOUS", "PPER", "ADV", "ADV", "ADV", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Zu dem so geht was alt sehr offt dem jungen vor.", "tokens": ["Zu", "dem", "so", "geht", "was", "alt", "sehr", "offt", "dem", "jun\u00b7gen", "vor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADV", "VVFIN", "PIS", "ADJD", "ADV", "ADV", "ART", "ADJA", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Die jungen schoten sind zwar allzeit angenehmer;", "tokens": ["Die", "jun\u00b7gen", "scho\u00b7ten", "sind", "zwar", "all\u00b7zeit", "an\u00b7ge\u00b7neh\u00b7mer", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "VAFIN", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Doch sind die alten schon zum stecken viel bequemer/", "tokens": ["Doch", "sind", "die", "al\u00b7ten", "schon", "zum", "ste\u00b7cken", "viel", "be\u00b7que\u00b7mer", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "ADJA", "ADV", "APPRART", "VVFIN", "ADV", "ADJD", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Denn ihre keime schiebt den sch\u00f6nsten strauch empor.", "tokens": ["Denn", "ih\u00b7re", "kei\u00b7me", "schiebt", "den", "sch\u00f6ns\u00b7ten", "strauch", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Der alte wein ist ja viel besser als der most/", "tokens": ["Der", "al\u00b7te", "wein", "ist", "ja", "viel", "bes\u00b7ser", "als", "der", "most", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV", "ADV", "ADJD", "KOKOM", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Die m\u00fcrben mispeln sind doch immerfort die besten/", "tokens": ["Die", "m\u00fcr\u00b7ben", "mis\u00b7peln", "sind", "doch", "im\u00b7mer\u00b7fort", "die", "bes\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV", "ADV", "ART", "ADJA", "$("], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.19": {"text": "Die sch\u00f6nsten fr\u00fcchte stehn offt an den \u00e4ltsten \u00e4sten/", "tokens": ["Die", "sch\u00f6ns\u00b7ten", "fr\u00fcch\u00b7te", "stehn", "offt", "an", "den", "\u00e4lts\u00b7ten", "\u00e4s\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "APPR", "ART", "ADJA", "ADJA", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Und alter honig wird zuletzt zur zucker-kost.", "tokens": ["Und", "al\u00b7ter", "ho\u00b7nig", "wird", "zu\u00b7letzt", "zur", "zu\u00b7cke\u00b7rkost", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VAFIN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Was aber sagt ihr viel von eurer sch\u00f6nheit pracht/", "tokens": ["Was", "a\u00b7ber", "sagt", "ihr", "viel", "von", "eu\u00b7rer", "sch\u00f6n\u00b7heit", "pracht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Die ihr bi\u00dfweilen doch mit flor die flecken decket?", "tokens": ["Die", "ihr", "bi\u00df\u00b7wei\u00b7len", "doch", "mit", "flor", "die", "fle\u00b7cken", "de\u00b7cket", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADV", "APPR", "VVFIN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Meint ihr/ man wisse nicht/ da\u00df ihr den schalck verstecket/", "tokens": ["Meint", "ihr", "/", "man", "wis\u00b7se", "nicht", "/", "da\u00df", "ihr", "den", "schalck", "ver\u00b7ste\u00b7cket", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$(", "PIS", "VVFIN", "PTKNEG", "$(", "KOUS", "PPER", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Und eure glatte haut mit schmincke sch\u00f6ner macht?", "tokens": ["Und", "eu\u00b7re", "glat\u00b7te", "haut", "mit", "schmin\u00b7cke", "sch\u00f6\u00b7ner", "macht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "VVFIN", "APPR", "VVFIN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Doch seht euch flei\u00dfig f\u00fcr/ da\u00df euer ruhm nicht s\u00e4llt/", "tokens": ["Doch", "seht", "euch", "flei\u00b7\u00dfig", "f\u00fcr", "/", "da\u00df", "eu\u00b7er", "ruhm", "nicht", "s\u00e4llt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "APPR", "$(", "KOUS", "PPOSAT", "NN", "PTKNEG", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Die sch\u00f6nste rose wird am zeitigsten gebrochen/", "tokens": ["Die", "sch\u00f6ns\u00b7te", "ro\u00b7se", "wird", "am", "zei\u00b7tigs\u00b7ten", "ge\u00b7bro\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "APPRART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Die s\u00fc\u00dfste frucht wird offt von einem wurm gestochen.", "tokens": ["Die", "s\u00fc\u00df\u00b7ste", "frucht", "wird", "offt", "von", "ei\u00b7nem", "wurm", "ge\u00b7sto\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Wohl der/ die ihren glantz stets unbefleckt beh\u00e4lt!", "tokens": ["Wohl", "der", "/", "die", "ih\u00b7ren", "glantz", "stets", "un\u00b7be\u00b7fleckt", "be\u00b7h\u00e4lt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "$(", "ART", "PPOSAT", "NN", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Wenn man euch engel nennt/ so nehmt nicht lieber an/", "tokens": ["Wenn", "man", "euch", "en\u00b7gel", "nennt", "/", "so", "nehmt", "nicht", "lie\u00b7ber", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "NE", "VVFIN", "$(", "ADV", "VVFIN", "PTKNEG", "ADV", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Wie etwan geister thun/ so leiber angenommen/", "tokens": ["Wie", "et\u00b7wan", "geis\u00b7ter", "thun", "/", "so", "lei\u00b7ber", "an\u00b7ge\u00b7nom\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADJD", "VVINF", "$(", "ADV", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Das spucken d\u00f6rfft euch sonst nicht allzu wol bekommen.", "tokens": ["Das", "spu\u00b7cken", "d\u00f6rfft", "euch", "sonst", "nicht", "all\u00b7zu", "wol", "be\u00b7kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVINF", "VVFIN", "PPER", "ADV", "PTKNEG", "PTKA", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Um engel/ wie ihr seyd/ ists allzubald gethan.", "tokens": ["Um", "en\u00b7gel", "/", "wie", "ihr", "seyd", "/", "ists", "all\u00b7zu\u00b7bald", "ge\u00b7than", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "$(", "PWAV", "PPER", "VAFIN", "$(", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Die liebsten ruffen euch: Mein allerliebstes kind!", "tokens": ["Die", "liebs\u00b7ten", "ruf\u00b7fen", "euch", ":", "Mein", "al\u00b7ler\u00b7liebs\u00b7tes", "kind", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "PPER", "$.", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Was gilts/ ihr krieget auch der liebe milch zu saugen/", "tokens": ["Was", "gilts", "/", "ihr", "krie\u00b7get", "auch", "der", "lie\u00b7be", "milch", "zu", "sau\u00b7gen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$(", "PPER", "VVFIN", "ADV", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Jhr seyd ein blumenfeld in eurer buhler augen/", "tokens": ["Ihr", "seyd", "ein", "blu\u00b7men\u00b7feld", "in", "eu\u00b7rer", "buh\u00b7ler", "au\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "So schaut/ da\u00df sich kein wurm zur zucker-rose findt.", "tokens": ["So", "schaut", "/", "da\u00df", "sich", "kein", "wurm", "zur", "zu\u00b7cke\u00b7rro\u00b7se", "findt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$(", "KOUS", "PRF", "PIAT", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Der euch so sch\u00e4tzchen nennt/ der gr\u00e4bt gewi\u00dflich nach/", "tokens": ["Der", "euch", "so", "sch\u00e4tz\u00b7chen", "nennt", "/", "der", "gr\u00e4bt", "ge\u00b7wi\u00df\u00b7lich", "nach", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADJA", "VVFIN", "$(", "ART", "VVFIN", "ADV", "APPR", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Bi\u00df da\u00df er seinen schatz/ und euer bergwerck funden.", "tokens": ["Bi\u00df", "da\u00df", "er", "sei\u00b7nen", "schatz", "/", "und", "eu\u00b7er", "berg\u00b7werck", "fun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PPER", "PPOSAT", "NN", "$(", "KON", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Was seyd ihr vor ein stern/ wenn euer glantz verschwunden?", "tokens": ["Was", "seyd", "ihr", "vor", "ein", "stern", "/", "wenn", "eu\u00b7er", "glantz", "ver\u00b7schwun\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "APPR", "ART", "VVINF", "$(", "KOUS", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "So euer licht verl\u00f6scht/ wird euer schein zur schmach.", "tokens": ["So", "eu\u00b7er", "licht", "ver\u00b7l\u00f6scht", "/", "wird", "eu\u00b7er", "schein", "zur", "schmach", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVPP", "$(", "VAFIN", "PPOSAT", "ADJD", "APPRART", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Drumb stellt/ beth\u00f6rte/ nur das freche prahlen ein/", "tokens": ["Drumb", "stellt", "/", "be\u00b7th\u00f6r\u00b7te", "/", "nur", "das", "fre\u00b7che", "prah\u00b7len", "ein", "/"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "$(", "VVFIN", "$(", "ADV", "ART", "ADJA", "NN", "ART", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Begehrt ihr ie mit uns nicht weiter umzugehen/", "tokens": ["Be\u00b7gehrt", "ihr", "ie", "mit", "uns", "nicht", "wei\u00b7ter", "um\u00b7zu\u00b7ge\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "PPER", "PTKNEG", "ADV", "VVIZU", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "So packt euch immer hin/ wir werden euch nicht flehen;", "tokens": ["So", "packt", "euch", "im\u00b7mer", "hin", "/", "wir", "wer\u00b7den", "euch", "nicht", "fle\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PTKVZ", "$(", "PPER", "VAFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Wir bleiben dennoch wol/ die wir gewesen seyn.", "tokens": ["Wir", "blei\u00b7ben", "den\u00b7noch", "wol", "/", "die", "wir", "ge\u00b7we\u00b7se