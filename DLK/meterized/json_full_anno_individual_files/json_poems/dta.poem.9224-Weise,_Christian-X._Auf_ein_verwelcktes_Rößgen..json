{"dta.poem.9224": {"metadata": {"author": {"name": "Weise, Christian", "birth": "N.A.", "death": "N.A."}, "title": "X.  \n Auf ein verwelcktes R\u00f6\u00dfgen.", "genre": "Lyrik; Drama; Prosa", "period": "N.A.", "pub_year": "1701", "urn": "urn:nbn:de:kobv:b4-25043-0", "language": ["de:0.99"], "booktitle": "Weise, Christian: \u00dcberfl\u00fc\u00dfige Gedancken Der gr\u00fcnenden jugend. Leipzig, 1701."}, "poem": {"stanza.1": {"line.1": {"text": "Ach mein r\u00f6\u00dfgen ist verwelckt!", "tokens": ["Ach", "mein", "r\u00f6\u00df\u00b7gen", "ist", "ver\u00b7welckt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PPOSAT", "ADJA", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Welches meiner augen weide/", "tokens": ["Wel\u00b7ches", "mei\u00b7ner", "au\u00b7gen", "wei\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Meine wollust/ meine freude/", "tokens": ["Mei\u00b7ne", "wol\u00b7lust", "/", "mei\u00b7ne", "freu\u00b7de", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "PPOSAT", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Welches durch das gantzejahr", "tokens": ["Wel\u00b7ches", "durch", "das", "gant\u00b7ze\u00b7jahr"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Meine liebste farbe war/", "tokens": ["Mei\u00b7ne", "liebs\u00b7te", "far\u00b7be", "war", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Dieses eilet so behende", "tokens": ["Die\u00b7ses", "ei\u00b7let", "so", "be\u00b7hen\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ADV", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Zu dem unverhofften ende/", "tokens": ["Zu", "dem", "un\u00b7ver\u00b7hoff\u00b7ten", "en\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Ach mein r\u00f6\u00dfgen ist verwelckt!", "tokens": ["Ach", "mein", "r\u00f6\u00df\u00b7gen", "ist", "ver\u00b7welckt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PPOSAT", "ADJA", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "2. Ach/ mein r\u00f6\u00dfgen ist verwelckt!", "tokens": ["Ach", "/", "mein", "r\u00f6\u00df\u00b7gen", "ist", "ver\u00b7welckt", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$(", "PPOSAT", "ADJA", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wann ich etwas am geruche/", "tokens": ["Wann", "ich", "et\u00b7was", "am", "ge\u00b7ru\u00b7che", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "APPRART", "ADJA", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "An der krafft und sch\u00f6nheit suche/", "tokens": ["An", "der", "krafft", "und", "sch\u00f6n\u00b7heit", "su\u00b7che", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KON", "ADJD", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Find ich nur ein d\u00fcrrrs blat/", "tokens": ["Find", "ich", "nur", "ein", "d\u00fcrrrs", "blat", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Welches schlechte reitzung hat/", "tokens": ["Wel\u00b7ches", "schlech\u00b7te", "reit\u00b7zung", "hat", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "NN", "VAFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Gleichwohl konte mein verlangen", "tokens": ["Gleich\u00b7wohl", "kon\u00b7te", "mein", "ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPOSAT", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Gestern in derselben prangen/", "tokens": ["Ge\u00b7stern", "in", "der\u00b7sel\u00b7ben", "pran\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PDAT", "VVFIN", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Ach mein r\u00f6\u00dfgen ist verwelckt.", "tokens": ["Ach", "mein", "r\u00f6\u00df\u00b7gen", "ist", "ver\u00b7welckt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PPOSAT", "ADJA", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "3. Ach mein v\u00f6\u00dfgen ist verwelckt!", "tokens": ["Ach", "mein", "v\u00f6\u00df\u00b7gen", "ist", "ver\u00b7welckt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PPOSAT", "ADJA", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und die zeit/ die alten dingen/", "tokens": ["Und", "die", "zeit", "/", "die", "al\u00b7ten", "din\u00b7gen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$(", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Mu\u00df ihr letztes urtheil bringen/", "tokens": ["Mu\u00df", "ihr", "letz\u00b7tes", "ur\u00b7theil", "brin\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "ADJA", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Raubt mir auch das sch\u00f6ne pfand", "tokens": ["Raubt", "mir", "auch", "das", "sch\u00f6\u00b7ne", "pfand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Gar zu zeitlich aus der hand/", "tokens": ["Gar", "zu", "zeit\u00b7lich", "aus", "der", "hand", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKA", "ADJD", "APPR", "ART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Da\u00df ich von dem edlen st\u00fccke", "tokens": ["Da\u00df", "ich", "von", "dem", "ed\u00b7len", "st\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Kaum den schatten noch erblicke.", "tokens": ["Kaum", "den", "schat\u00b7ten", "noch", "er\u00b7bli\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Ach mein r\u00f6\u00dfgen ist verwelckt!", "tokens": ["Ach", "mein", "r\u00f6\u00df\u00b7gen", "ist", "ver\u00b7welckt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PPOSAT", "ADJA", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "4. Ach mein r\u00f6\u00dfgen ist verwelckt!", "tokens": ["Ach", "mein", "r\u00f6\u00df\u00b7gen", "ist", "ver\u00b7welckt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PPOSAT", "ADJA", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Die verliebte fr\u00fchlings-blume/", "tokens": ["Die", "ver\u00b7lieb\u00b7te", "fr\u00fch\u00b7lings\u00b7blu\u00b7me", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Welche vor dem sch\u00f6nen ruhme", "tokens": ["Wel\u00b7che", "vor", "dem", "sch\u00f6\u00b7nen", "ruh\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAT", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Das ged\u00e4chtni\u00df in der welt", "tokens": ["Das", "ge\u00b7d\u00e4cht\u00b7ni\u00df", "in", "der", "welt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Sonsten auch nichts mehr beh\u00e4lt/", "tokens": ["Sons\u00b7ten", "auch", "nichts", "mehr", "be\u00b7h\u00e4lt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PIS", "ADV", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Alldieweil die sch\u00f6nen gaben", "tokens": ["All\u00b7die\u00b7weil", "die", "sch\u00f6\u00b7nen", "ga\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Sich zu weit verh\u00fcllet haben/", "tokens": ["Sich", "zu", "weit", "ver\u00b7h\u00fcl\u00b7let", "ha\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "PTKA", "ADJD", "VVPP", "VAINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Ach mein r\u00f6\u00dfgen ist verwelckt!", "tokens": ["Ach", "mein", "r\u00f6\u00df\u00b7gen", "ist", "ver\u00b7welckt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PPOSAT", "ADJA", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "5. Ach mein r\u00f6\u00dfgen ist verwelckt!", "tokens": ["Ach", "mein", "r\u00f6\u00df\u00b7gen", "ist", "ver\u00b7welckt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PPOSAT", "ADJA", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und in dem ich sie betrachte/", "tokens": ["Und", "in", "dem", "ich", "sie", "be\u00b7trach\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PRELS", "PPER", "PPER", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "So empfind ich still und sachte", "tokens": ["So", "emp\u00b7find", "ich", "still", "und", "sach\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "KON", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mein gewisses ebenbild", "tokens": ["Mein", "ge\u00b7wis\u00b7ses", "e\u00b7ben\u00b7bild"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "In dem leichnam eingeh\u00fclt", "tokens": ["In", "dem", "leich\u00b7nam", "ein\u00b7ge\u00b7h\u00fclt"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJD", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Da\u00df ich bald auch werde m\u00fcssen", "tokens": ["Da\u00df", "ich", "bald", "auch", "wer\u00b7de", "m\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADV", "VAFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Meine junge zeit beschliessen/", "tokens": ["Mei\u00b7ne", "jun\u00b7ge", "zeit", "be\