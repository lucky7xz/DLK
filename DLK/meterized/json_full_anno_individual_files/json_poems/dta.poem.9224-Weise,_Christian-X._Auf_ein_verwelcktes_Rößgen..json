ure": "trochaic.tetra"}, "line.6": {"text": "Drum/ in dem ich sie beklage/", "tokens": ["Drum", "/", "in", "dem", "ich", "sie", "be\u00b7kla\u00b7ge", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$(", "APPR", "PRELS", "PPER", "PPER", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Kan ich nicht/ als da\u00df ich sage/", "tokens": ["Kan", "ich", "nicht", "/", "als", "da\u00df", "ich", "sa\u00b7ge", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "$(", "KOKOM", "KOUS", "PPER", "VVFIN", "$("], "meter": "+--+-+--", "measure": "iambic.tri.invert"}, "line.8": {"text": "Ach mein r\u00f6\u00dfgen ist verwelckt!", "tokens": ["Ach", "mein", "r\u00f6\u00df\u00b7gen", "ist", "ver\u00b7welckt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PPOSAT", "ADJA", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}