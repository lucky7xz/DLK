nd", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Zeit ist m\u00e4chtig! Jede Fessel f\u00e4llt!", "tokens": ["Zeit", "ist", "m\u00e4ch\u00b7tig", "!", "Je\u00b7de", "Fes\u00b7sel", "f\u00e4llt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADJD", "$.", "PIAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Nur die Erde schlie\u00dft und h\u00e4lt.\u00ab", "tokens": ["Nur", "die", "Er\u00b7de", "schlie\u00dft", "und", "h\u00e4lt", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "KON", "VVFIN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "K\u00f6nig Hans, aus seinem Traum", "tokens": ["K\u00f6\u00b7nig", "Hans", ",", "aus", "sei\u00b7nem", "Traum"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NE", "$,", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Blickt er auf und sieht des Zeltes Raum,", "tokens": ["Blickt", "er", "auf", "und", "sieht", "des", "Zel\u00b7tes", "Raum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "KON", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Und in geisterbleichem Angesicht", "tokens": ["Und", "in", "geis\u00b7ter\u00b7blei\u00b7chem", "An\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Zweier schwarzer Augen Licht.", "tokens": ["Zwei\u00b7er", "schwar\u00b7zer", "Au\u00b