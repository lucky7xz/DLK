d", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Horch! Es triumphiert!\u00ab Der Sieger spricht:", "tokens": ["Horch", "!", "Es", "tri\u00b7um\u00b7phiert", "!", "\u00ab", "Der", "Sie\u00b7ger", "spricht", ":"], "token_info": ["word", "punct", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PPER", "VVFIN", "$.", "$(", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "\u00bbk\u00f6nig, nein. Es jubelt nicht.", "tokens": ["\u00bb", "k\u00f6\u00b7nig", ",", "nein", ".", "Es", "ju\u00b7belt", "nicht", "."], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "$,", "PTKANT", "$.", "PPER", "VVFIN", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.16": {"line.1": {"text": "Ich bin eine kurze Kraft,", "tokens": ["Ich", "bin", "ei\u00b7ne", "kur\u00b7ze", "Kraft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "----+-+", "measure": "unknown.measure.di"}, "line.2": {"text": "Heut geharnischt, morgen weggerafft!", "tokens": ["Heut", "ge\u00b7har\u00b7nischt", ",", "mor\u00b7gen", "weg\u00b7ge\u00b7rafft", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVPP", "$,", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Fr\u00fche Stunde lost' ich wie Achill,", "tokens": ["Fr\u00fc\u00b7he", "Stun\u00b7de", "lost'", "ich", "wie", "A\u00b7chill", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "PPER", "KOKOM", "NE", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Meinem Lose halt ich still.\u00ab", "tokens": ["Mei\u00b7nem", "Lo\u00b7se", "halt", "ich", "still", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "PTKVZ", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}