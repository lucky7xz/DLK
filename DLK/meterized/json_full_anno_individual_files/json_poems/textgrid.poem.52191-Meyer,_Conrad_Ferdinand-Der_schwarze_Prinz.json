{"textgrid.poem.52191": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Der schwarze Prinz", "genre": "verse", "period": "N.A.", "pub_year": 1861, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schwarzer Prinz und K\u00f6nig Hans", "tokens": ["Schwar\u00b7zer", "Prinz", "und", "K\u00f6\u00b7nig", "Hans"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "KON", "NN", "NE"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ma\u00dfen sich in raschem Waffentanz,", "tokens": ["Ma\u00b7\u00dfen", "sich", "in", "ra\u00b7schem", "Waf\u00b7fen\u00b7tanz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Bis der Prinz den K\u00f6nig \u00fcberwand", "tokens": ["Bis", "der", "Prinz", "den", "K\u00f6\u00b7nig", "\u00fc\u00b7ber\u00b7wand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Mit der erzgeschienten Hand.", "tokens": ["Mit", "der", "erz\u00b7ge\u00b7schien\u00b7ten", "Hand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ins Gezelt nahm er den Raub,", "tokens": ["Ins", "Ge\u00b7zelt", "nahm", "er", "den", "Raub", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wusch den Wunden rein von Blut und Staub,", "tokens": ["Wusch", "den", "Wun\u00b7den", "rein", "von", "Blut", "und", "Staub", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADJD", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Bog das Knie und bot den Labetrank", "tokens": ["Bog", "das", "Knie", "und", "bot", "den", "La\u00b7be\u00b7trank"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "KON", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Ihm, der tief in Gram versank.", "tokens": ["Ihm", ",", "der", "tief", "in", "Gram", "ver\u00b7sank", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "ADJD", "APPR", "NE", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Frankreichs armer K\u00f6nig tr\u00e4umt", "tokens": ["Fran\u00b7kreichs", "ar\u00b7mer", "K\u00f6\u00b7nig", "tr\u00e4umt"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Also schwer, da\u00df er den Wein vers\u00e4umt,", "tokens": ["Al\u00b7so", "schwer", ",", "da\u00df", "er", "den", "Wein", "ver\u00b7s\u00e4umt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "KOUS", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Ihn ermahnt der Prinz wie er's vermag:", "tokens": ["Ihn", "er\u00b7mahnt", "der", "Prinz", "wie", "er's", "ver\u00b7mag", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "KOKOM", "PIS", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "\u00bbherr, es ist des Schicksals Tag!", "tokens": ["\u00bb", "herr", ",", "es", "ist", "des", "Schick\u00b7sals", "Tag", "!"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PTKVZ", "$,", "PPER", "VAFIN", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Manchen hattet Ihr gestreckt,", "tokens": ["Man\u00b7chen", "hat\u00b7tet", "Ihr", "ge\u00b7streckt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Da Ihr sanket, Herr, mich hat's erschreckt,", "tokens": ["Da", "Ihr", "san\u00b7ket", ",", "Herr", ",", "mich", "hat's", "er\u00b7schreckt", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "NN", "$,", "PPER", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Doch man lebt, und blieb nur Ehre heil,", "tokens": ["Doch", "man", "lebt", ",", "und", "blieb", "nur", "Eh\u00b7re", "heil", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "$,", "KON", "VVFIN", "ADV", "NN", "ADJD", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Duldet man sein menschlich Teil!", "tokens": ["Dul\u00b7det", "man", "sein", "menschlich", "Teil", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PPOSAT", "ADJD", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.4": {"line.1": {"text": "Morgen als des Friedens Pfand", "tokens": ["Mor\u00b7gen", "als", "des", "Frie\u00b7dens", "Pfand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "KOKOM", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Send ich Euch nach meinem Engelland.", "tokens": ["Send", "ich", "Euch", "nach", "mei\u00b7nem", "En\u00b7gel\u00b7land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Zeit ist m\u00e4chtig! Jede Fessel f\u00e4llt!", "tokens": ["Zeit", "ist", "m\u00e4ch\u00b7tig", "!", "Je\u00b7de", "Fes\u00b7sel", "f\u00e4llt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADJD", "$.", "PIAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Nur die Erde schlie\u00dft und h\u00e4lt.\u00ab", "tokens": ["Nur", "die", "Er\u00b7de", "schlie\u00dft", "und", "h\u00e4lt", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "KON", "VVFIN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "K\u00f6nig Hans, aus seinem Traum", "tokens": ["K\u00f6\u00b7nig", "Hans", ",", "aus", "sei\u00b7nem", "Traum"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NE", "$,", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Blickt er auf und sieht des Zeltes Raum,", "tokens": ["Blickt", "er", "auf", "und", "sieht", "des", "Zel\u00b7tes", "Raum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "KON", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Und in geisterbleichem Angesicht", "tokens": ["Und", "in", "geis\u00b7ter\u00b7blei\u00b7chem", "An\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Zweier schwarzer Augen Licht.", "tokens": ["Zwei\u00b7er", "schwar\u00b7zer", "Au\u00b7gen", "Licht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "ADJA", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Er beschaut das edle Haupt,", "tokens": ["Er", "be\u00b7schaut", "das", "ed\u00b7le", "Haupt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Das ein unsichtbarer Kranz umlaubt,", "tokens": ["Das", "ein", "un\u00b7sicht\u00b7ba\u00b7rer", "Kranz", "um\u00b7laubt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "--+---+-+", "measure": "anapaest.init"}, "line.3": {"text": "\u00c4rgert sich und murmelt: \u00bbWorte sind's.", "tokens": ["\u00c4r\u00b7gert", "sich", "und", "mur\u00b7melt", ":", "\u00bb", "Wor\u00b7te", "sind'", "s."], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "abbreviation"], "pos": ["VVFIN", "PRF", "KON", "VVFIN", "$.", "$(", "FM.la", "FM.la", "FM.la"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Deine Augen spotten, Prinz!", "tokens": ["Dei\u00b7ne", "Au\u00b7gen", "spot\u00b7ten", ",", "Prinz", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Heuchle! Streichle meinen Schmerz!", "tokens": ["Heuch\u00b7le", "!", "Streich\u00b7le", "mei\u00b7nen", "Schmerz", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Leis im Panzer jubelt dir das Herz.", "tokens": ["Leis", "im", "Pan\u00b7zer", "ju\u00b7belt", "dir", "das", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Horch! Es triumphiert!\u00ab Der Sieger spricht:", "tokens": ["Horch", "!", "Es", "tri\u00b7um\u00b7phiert", "!", "\u00ab", "Der", "Sie\u00b7ger", "spricht", ":"], "token_info": ["word", "punct", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PPER", "VVFIN", "$.", "$(", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "\u00bbk\u00f6nig, nein. Es jubelt nicht.", "tokens": ["\u00bb", "k\u00f6\u00b7nig", ",", "nein", ".", "Es", "ju\u00b7belt", "nicht", "."], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "$,", "PTKANT", "$.", "PPER", "VVFIN", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Ich bin eine kurze Kraft,", "tokens": ["Ich", "bin", "ei\u00b7ne", "kur\u00b7ze", "Kraft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "----+-+", "measure": "unknown.measure.di"}, "line.2": {"text": "Heut geharnischt, morgen weggerafft!", "tokens": ["Heut", "ge\u00b7har\u00b7nischt", ",", "mor\u00b7gen", "weg\u00b7ge\u00b7rafft", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVPP", "$,", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Fr\u00fche Stunde lost' ich wie Achill,", "tokens": ["Fr\u00fc\u00b7he", "Stun\u00b7de", "lost'", "ich", "wie", "A\u00b7chill", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "PPER", "KOKOM", "NE", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Meinem Lose halt ich still.\u00ab", "