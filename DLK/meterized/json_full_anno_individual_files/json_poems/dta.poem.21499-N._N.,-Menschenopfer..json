{"dta.poem.21499": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Menschenopfer.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Man sagt, die Jugend selbst sei Gl\u00fcck.", "tokens": ["Man", "sagt", ",", "die", "Ju\u00b7gend", "selbst", "sei", "Gl\u00fcck", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "ART", "NN", "ADV", "VAFIN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich hab\u2019 es nicht erfahren.", "tokens": ["Ich", "hab'", "es", "nicht", "er\u00b7fah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mir waren niemals hold gesinnt", "tokens": ["Mir", "wa\u00b7ren", "nie\u00b7mals", "hold", "ge\u00b7sinnt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die dachnistenden Laren.", "tokens": ["Die", "dach\u00b7nis\u00b7ten\u00b7den", "La\u00b7ren", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+---+-", "measure": "dactylic.init"}}, "stanza.2": {"line.1": {"text": "Mir fehlte, was die Jugend braucht,", "tokens": ["Mir", "fehl\u00b7te", ",", "was", "die", "Ju\u00b7gend", "braucht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des Frohsinns Wohlbehagen;", "tokens": ["Des", "Froh\u00b7sinns", "Wohl\u00b7be\u00b7ha\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Des Kummers bleiche Wange schon", "tokens": ["Des", "Kum\u00b7mers", "blei\u00b7che", "Wan\u00b7ge", "schon"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als Kind ich mu\u00dfte tragen.", "tokens": ["Als", "Kind", "ich", "mu\u00df\u00b7te", "tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Die Rebe, die kein St\u00e4blein hat,", "tokens": ["Die", "Re\u00b7be", ",", "die", "kein", "St\u00e4\u00b7blein", "hat", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PIAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mu\u00df bald zu Grunde gehen;", "tokens": ["Mu\u00df", "bald", "zu", "Grun\u00b7de", "ge\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich war die Rebe, ward zerw\u00fchlt", "tokens": ["Ich", "war", "die", "Re\u00b7be", ",", "ward", "zer\u00b7w\u00fchlt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "VAFIN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In wilden Sturmes Wehen.", "tokens": ["In", "wil\u00b7den", "Stur\u00b7mes", "We\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Nach dir, nach dir, mein Jesu Christ,", "tokens": ["Nach", "dir", ",", "nach", "dir", ",", "mein", "Je\u00b7su", "Christ", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "APPR", "PPER", "$,", "PPOSAT", "NE", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich jugendlich mich sehnte;", "tokens": ["Ich", "ju\u00b7gend\u00b7lich", "mich", "sehn\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das grause Schicksal mich und dich", "tokens": ["Das", "grau\u00b7se", "Schick\u00b7sal", "mich", "und", "dich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PPER", "KON", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Frevelnd und frech verh\u00f6hnte.", "tokens": ["Fre\u00b7velnd", "und", "frech", "ver\u00b7h\u00f6hn\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "ADJD", "VVFIN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.5": {"line.1": {"text": "Der P\u00f6belha\u00df, der P\u00f6belwahn", "tokens": ["Der", "P\u00f6\u00b7bel\u00b7ha\u00df", ",", "der", "P\u00f6\u00b7bel\u00b7wahn"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hat dich an\u2019s Kreuz geschlagen;", "tokens": ["Hat", "dich", "an's", "Kreuz", "ge\u00b7schla\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das Schicksal thut das Gleiche noch", "tokens": ["Das", "Schick\u00b7sal", "thut", "das", "Glei\u00b7che", "noch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit uns an allen Tagen.", "tokens": ["Mit", "uns", "an", "al\u00b7len", "Ta\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Das alte blut\u2019ge Opfer du", "tokens": ["Das", "al\u00b7te", "blut'\u00b7ge", "Op\u00b7fer", "du"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Unblutig hast erneuert:", "tokens": ["Un\u00b7blu\u00b7tig", "hast", "er\u00b7neu\u00b7ert", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das Schicksal opfert blutig fort \u2014", "tokens": ["Das", "Schick\u00b7sal", "op\u00b7fert", "blu\u00b7tig", "fort"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Kein Gott, kein Gott ihm steuert!", "tokens": ["Kein", "Gott", ",", "kein", "Gott", "ihm", "steu\u00b7ert", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}