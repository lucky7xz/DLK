{"dta.poem.10471": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "XxVIII.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Der bleiche, herbstliche Halbmond", "tokens": ["Der", "blei\u00b7che", ",", "herbst\u00b7li\u00b7che", "Halb\u00b7mond"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Lugt aus den Wolken heraus;", "tokens": ["Lugt", "aus", "den", "Wol\u00b7ken", "he\u00b7raus", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Ganz einsam liegt auf dem Kirchhof'", "tokens": ["Ganz", "ein\u00b7sam", "liegt", "auf", "dem", "Kirch\u00b7hof'"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Das stille Pfarrerhaus.", "tokens": ["Das", "stil\u00b7le", "Pfar\u00b7rer\u00b7haus", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die Mutter liest in der Bibel,", "tokens": ["Die", "Mut\u00b7ter", "liest", "in", "der", "Bi\u00b7bel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Der Sohn, der starret in's Licht,", "tokens": ["Der", "Sohn", ",", "der", "star\u00b7ret", "in's", "Licht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Schlaftrunken dehnt sich die \u00e4lt're,", "tokens": ["Schlaf\u00b7trun\u00b7ken", "dehnt", "sich", "die", "\u00e4lt'\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PRF", "ART", "ADJA", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Die j\u00fcngere Tochter spricht:", "tokens": ["Die", "j\u00fcn\u00b7ge\u00b7re", "Toch\u00b7ter", "spricht", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Ach Gott! wie Einem die Tage", "tokens": ["Ach", "Gott", "!", "wie", "Ei\u00b7nem", "die", "Ta\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "NN", "$.", "PWAV", "PIS", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Langweilig hier vergeh'n;", "tokens": ["Lang\u00b7wei\u00b7lig", "hier", "ver\u00b7geh'n", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nur wenn sie Einen begraben,", "tokens": ["Nur", "wenn", "sie", "Ei\u00b7nen", "be\u00b7gra\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "ART", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Bekommen wir etwas zu sehn.", "tokens": ["Be\u00b7kom\u00b7men", "wir", "et\u00b7was", "zu", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIS", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Die Mutter spricht zwischen dem Lesen:", "tokens": ["Die", "Mut\u00b7ter", "spricht", "zwi\u00b7schen", "dem", "Le\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Du irrst, es starben nur Vier,", "tokens": ["Du", "irrst", ",", "es", "star\u00b7ben", "nur", "Vier", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "ADV", "CARD", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Seit man deinen Vater begraben,", "tokens": ["Seit", "man", "dei\u00b7nen", "Va\u00b7ter", "be\u00b7gra\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Dort an der Kirchhofsth\u00fcr'.", "tokens": ["Dort", "an", "der", "Kirch\u00b7hofst\u00b7h\u00fcr'", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Die \u00e4lt're Tochter g\u00e4hnet:", "tokens": ["Die", "\u00e4lt'\u00b7re", "Toch\u00b7ter", "g\u00e4h\u00b7net", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ich will nicht verhungern bei Euch,", "tokens": ["Ich", "will", "nicht", "ver\u00b7hun\u00b7gern", "bei", "Euch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "VVINF", "APPR", "PPER", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Ich gehe morgen zum Grafen,", "tokens": ["Ich", "ge\u00b7he", "mor\u00b7gen", "zum", "Gra\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und der ist verliebt und reich.", "tokens": ["Und", "der", "ist", "ver\u00b7liebt", "und", "reich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "VAFIN", "VVPP", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Der Sohn bricht aus in Lachen:", "tokens": ["Der", "Sohn", "bricht", "aus", "in", "La\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Drei J\u00e4ger zechen im Stern,", "tokens": ["Drei", "J\u00e4\u00b7ger", "ze\u00b7chen", "im", "Stern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Die machen Gold und lehren", "tokens": ["Die", "ma\u00b7chen", "Gold", "und", "leh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "KON", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mir das Geheimni\u00df gern.", "tokens": ["Mir", "das", "Ge\u00b7heim\u00b7ni\u00df", "gern", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "ADV", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.7": {"line.1": {"text": "Die Mutter wirft ihm die Bibel", "tokens": ["Die", "Mut\u00b7ter", "wirft", "ihm", "die", "Bi\u00b7bel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "In's mag're Gesicht hinein:", "tokens": ["In's", "mag'\u00b7re", "Ge\u00b7sicht", "hin\u00b7ein", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "So willst du, Gottverfluchter,", "tokens": ["So", "willst", "du", ",", "Gott\u00b7ver\u00b7fluch\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "$,", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein Stra\u00dfenr\u00e4uber seyn!", "tokens": ["Ein", "Stra\u00b7\u00dfen\u00b7r\u00e4u\u00b7ber", "seyn", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Sie h\u00f6ren pochen an's Fenster,", "tokens": ["Sie", "h\u00f6\u00b7ren", "po\u00b7chen", "an's", "Fens\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und sehn eine winkende Hand;", "tokens": ["Und", "sehn", "ei\u00b7ne", "win\u00b7ken\u00b7de", "Hand", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Der todte Vater steht drau\u00dfen", "tokens": ["Der", "tod\u00b7te", "Va\u00b7ter", "steht", "drau\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Im schwarzen Pred'gergewand.", "tokens": ["Im", "schwar\u00b7zen", "Pre\u00b7d'\u00b7ger\u00b7ge\u00b7wand", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}