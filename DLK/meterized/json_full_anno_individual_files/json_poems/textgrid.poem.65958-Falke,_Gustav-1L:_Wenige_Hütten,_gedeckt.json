{"textgrid.poem.65958": {"metadata": {"author": {"name": "Falke, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wenige H\u00fctten, gedeckt", "genre": "verse", "period": "N.A.", "pub_year": 1884, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenige H\u00fctten, gedeckt", "tokens": ["We\u00b7ni\u00b7ge", "H\u00fct\u00b7ten", ",", "ge\u00b7deckt"], "token_info": ["word", "word", "punct", "word"], "pos": ["PIAT", "NN", "$,", "VVPP"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.2": {"text": "Mit \u00fcberragenden Schindeln.", "tokens": ["Mit", "\u00fc\u00b7berr\u00b7a\u00b7gen\u00b7den", "Schin\u00b7deln", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Manche versteckt,", "tokens": ["Man\u00b7che", "ver\u00b7steckt", ","], "token_info": ["word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Wie's Kind in den Windeln,", "tokens": ["Wie's", "Kind", "in", "den", "Win\u00b7deln", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Hinter Apfelbaumgezweig", "tokens": ["Hin\u00b7ter", "Ap\u00b7fel\u00b7baum\u00b7ge\u00b7zweig"], "token_info": ["word", "word"], "pos": ["APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Und gegen den Steig", "tokens": ["Und", "ge\u00b7gen", "den", "Steig"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Von hohen Dornen eingeheckt.", "tokens": ["Von", "ho\u00b7hen", "Dor\u00b7nen", "ein\u00b7ge\u00b7heckt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Vorm Haus,", "tokens": ["Vorm", "Haus", ","], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.2": {"text": "Kraus", "tokens": ["Kraus"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Zwischen Kraut und Nesseln,", "tokens": ["Zwi\u00b7schen", "Kraut", "und", "Nes\u00b7seln", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Nelken und Georginen;", "tokens": ["Nel\u00b7ken", "und", "Ge\u00b7or\u00b7gi\u00b7nen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.5": {"text": "Hinter den Fenstern und Gardinen", "tokens": ["Hin\u00b7ter", "den", "Fens\u00b7tern", "und", "Gar\u00b7di\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "KON", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Geranien, Goldlack und wieder Nelken,", "tokens": ["Ge\u00b7ra\u00b7ni\u00b7en", ",", "Gold\u00b7lack", "und", "wie\u00b7der", "Nel\u00b7ken", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "KON", "ADV", "NN", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.7": {"text": "In Scherbenfesseln", "tokens": ["In", "Scher\u00b7ben\u00b7fes\u00b7seln"], "token_info": ["word", "word"], "pos": ["APPR", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "Bestimmt zu welken.", "tokens": ["Be\u00b7stimmt", "zu", "wel\u00b7ken", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "PTKZU", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Fischerger\u00e4t, Netze und Schn\u00fcre", "tokens": ["Fi\u00b7scher\u00b7ge\u00b7r\u00e4t", ",", "Net\u00b7ze", "und", "Schn\u00fc\u00b7re"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "NN", "KON", "NN"], "meter": "+---+--+-", "measure": "trochaic.tri.relaxed"}, "line.2": {"text": "Vor jeder Th\u00fcre;", "tokens": ["Vor", "je\u00b7der", "Th\u00fc\u00b7re", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Hin und wieder ein frommer Spruch,", "tokens": ["Hin", "und", "wie\u00b7der", "ein", "from\u00b7mer", "Spruch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Und \u00fcberall Fischgeruch.", "tokens": ["Und", "\u00fc\u00b7be\u00b7rall", "Fischge\u00b7ruch", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADV", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Im Sonnenbrande", "tokens": ["Im", "Son\u00b7nen\u00b7bran\u00b7de"], "token_info": ["word", "word"], "pos": ["APPRART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Spielende Kinder im Sande,", "tokens": ["Spie\u00b7len\u00b7de", "Kin\u00b7der", "im", "San\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPRART", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Schmutzig und putzig,", "tokens": ["Schmut\u00b7zig", "und", "put\u00b7zig", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Halb scheu und stutzig,", "tokens": ["Halb", "scheu", "und", "stut\u00b7zig", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Halb dreist,", "tokens": ["Halb", "dreist", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.6": {"text": "Und barfu\u00df zumeist.", "tokens": ["Und", "bar\u00b7fu\u00df", "zu\u00b7meist", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.5": {"line.1": {"text": "Auf niederm Sitz", "tokens": ["Auf", "nie\u00b7derm", "Sitz"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Der Schwelle hingeduckt", "tokens": ["Der", "Schwel\u00b7le", "hin\u00b7ge\u00b7duckt"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ein altes M\u00fctterchen hockt.", "tokens": ["Ein", "al\u00b7tes", "M\u00fct\u00b7ter\u00b7chen", "hockt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Kartoffel sch\u00e4lend guckt", "tokens": ["Kar\u00b7tof\u00b7fel", "sch\u00e4\u00b7lend", "guckt"], "token_info": ["word", "word", "word"], "pos": ["NN", "ADJD", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sie her und lockt", "tokens": ["Sie", "her", "und", "lockt"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "PTKVZ", "KON", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Mit zitterndem Stimmchen aus zahnlosem Mund", "tokens": ["Mit", "zit\u00b7tern\u00b7dem", "Stimm\u00b7chen", "aus", "zahn\u00b7lo\u00b7sem", "Mund"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "APPR", "ADJA", "NN"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Den klaffenden Hund:", "tokens": ["Den", "klaf\u00b7fen\u00b7den", "Hund", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "Komm Spitz!", "tokens": ["Komm", "Spitz", "!"], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "NE", "$."], "meter": "-+", "measure": "iambic.single"}}, "stanza.6": {"line.1": {"text": "Eine G\u00e4nseherde schnattert vorbei.", "tokens": ["Ei\u00b7ne", "G\u00e4n\u00b7se\u00b7her\u00b7de", "schnat\u00b7tert", "vor\u00b7bei", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Ein M\u00e4dchen vollbusig und drall,", "tokens": ["Ein", "M\u00e4d\u00b7chen", "voll\u00b7bu\u00b7sig", "und", "drall", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "KON", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bringt eine Ziege zu Stall,", "tokens": ["Bringt", "ei\u00b7ne", "Zie\u00b7ge", "zu", "Stall", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Oder auf die Wiese.", "tokens": ["O\u00b7der", "auf", "die", "Wie\u00b7se", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "\u00bbwas macht der Schatz, Liese?\u00ab", "tokens": ["\u00bb", "was", "macht", "der", "Schatz", ",", "Lie\u00b7se", "?", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["$(", "PWS", "VVFIN", "ART", "NN", "$,", "NN", "$.", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Wie versch\u00e4mt sie thut. Ei,", "tokens": ["Wie", "ver\u00b7sch\u00e4mt", "sie", "thut", ".", "Ei", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "VVFIN", "$.", "NN", "$,"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.7": {"text": "Und sich umsieht und lacht.", "tokens": ["Und", "sich", "um\u00b7sieht", "und", "lacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Nimm dich in acht!", "tokens": ["Nimm", "dich", "in", "acht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "CARD", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.7": {"line.1": {"text": "Vorm Wirtshaus Entengeschwatz", "tokens": ["Vorm", "Wirts\u00b7haus", "En\u00b7ten\u00b7ge\u00b7schwatz"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "NN", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Auf dem grasbewachsenen Platz", "tokens": ["Auf", "dem", "gras\u00b7be\u00b7wach\u00b7se\u00b7nen", "Platz"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Und daneben", "tokens": ["Und", "da\u00b7ne\u00b7ben"], "token_info": ["word", "word"], "pos": ["KON", "PAV"], "meter": "-+--", "measure": "dactylic.init"}, "line.4": {"text": "Auf dem \u00fcbelriechenden Teich,", "tokens": ["Auf", "dem", "\u00fc\u00b7bel\u00b7rie\u00b7chen\u00b7den", "Teich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Soeben", "tokens": ["Soe\u00b7ben"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.6": {"text": "Kr\u00e4hen zwei H\u00e4hne zugleich,", "tokens": ["Kr\u00e4\u00b7hen", "zwei", "H\u00e4h\u00b7ne", "zu\u00b7gleich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "CARD", "NN", "ADV", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.7": {"text": "Und die St\u00f6rchin vom Scheundach herab", "tokens": ["Und", "die", "St\u00f6r\u00b7chin", "vom", "Scheun\u00b7dach", "her\u00b7ab"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPRART", "NN", "ADV"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.8": {"text": "Klappert: klappklappklapp!", "tokens": ["Klap\u00b7pert", ":", "klapp\u00b7klapp\u00b7klapp", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$.", "ITJ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.9": {"text": "\u2013 Klapp!", "tokens": ["\u2013", "Klapp", "!"], "token_info": ["punct", "word", "punct"], "pos": ["$(", "NE", "$."], "meter": "+", "measure": "single.up"}}, "stanza.8": {"line.1": {"text": "Schwalben schie\u00dfen wie Pfeile", "tokens": ["Schwal\u00b7ben", "schie\u00b7\u00dfen", "wie", "Pfei\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "KOKOM", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Kreuz und quer \u00fcber den Weg,", "tokens": ["Kreuz", "und", "quer", "\u00fc\u00b7ber", "den", "Weg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "+-++--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Haben immer Eile,", "tokens": ["Ha\u00b7ben", "im\u00b7mer", "Ei\u00b7le", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Sind immer reg,", "tokens": ["Sind", "im\u00b7mer", "reg", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Zierlich und schlank,", "tokens": ["Zier\u00b7lich", "und", "schlank", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "KON", "VVFIN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Blitz und blank.", "tokens": ["Blitz", "und", "blank", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "PTKVZ", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.9": {"line.1": {"text": "Aus dem Schulhaus,", "tokens": ["Aus", "dem", "Schul\u00b7haus", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Neu aus roten Ziegeln erbaut,", "tokens": ["Neu", "aus", "ro\u00b7ten", "Zie\u00b7geln", "er\u00b7baut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Schallt's hell heraus:", "tokens": ["Schallt's", "hell", "he\u00b7raus", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJD", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "\u00bbwei\u00dft du, wie viel Sternlein stehn \u2013\u00ab", "tokens": ["\u00bb", "wei\u00dft", "du", ",", "wie", "viel", "Stern\u00b7lein", "stehn", "\u2013", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "PPER", "$,", "PWAV", "PIAT", "NN", "VVINF", "$(", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Der alte Lehrer singt f\u00fcr zehn", "tokens": ["Der", "al\u00b7te", "Leh\u00b7rer", "singt", "f\u00fcr", "zehn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "CARD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und fiedelt dazu.", "tokens": ["Und", "fie\u00b7delt", "da\u00b7zu", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PAV", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Hartn\u00e4ckig dazwischen br\u00fcllt eine Kuh", "tokens": ["Hart\u00b7n\u00e4\u00b7ckig", "da\u00b7zwi\u00b7schen", "br\u00fcllt", "ei\u00b7ne", "Kuh"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "PAV", "VVFIN", "ART", "NN"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Von naher Wiese, immer gleich kl\u00e4glich.", "tokens": ["Von", "na\u00b7her", "Wie\u00b7se", ",", "im\u00b7mer", "gleich", "kl\u00e4g\u00b7lich", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Es ist unertr\u00e4glich.", "tokens": ["Es", "ist", "un\u00b7er\u00b7tr\u00e4g\u00b7lich", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.10": {"line.1": {"text": "Weiter, beim Kirchhof zum Dorf hinaus,", "tokens": ["Wei\u00b7ter", ",", "beim", "Kirch\u00b7hof", "zum", "Dorf", "hin\u00b7aus", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "APPRART", "NN", "APPRART", "NN", "APZR", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Das letzte Haus sieht wie das erste aus:", "tokens": ["Das", "letz\u00b7te", "Haus", "sieht", "wie", "das", "ers\u00b7te", "aus", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "KOKOM", "ART", "ADJA", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Klein, d\u00fcrf