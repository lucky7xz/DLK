{"textgrid.poem.33747": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Verdammen mu\u00df die Menschheit das Verbrechen.", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Verdammen mu\u00df die Menschheit das Verbrechen.", "tokens": ["Ver\u00b7dam\u00b7men", "mu\u00df", "die", "Menschheit", "das", "Ver\u00b7bre\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wof\u00fcr denn h\u00e4tte sie sich aufgerichtet", "tokens": ["Wo\u00b7f\u00fcr", "denn", "h\u00e4t\u00b7te", "sie", "sich", "auf\u00b7ge\u00b7rich\u00b7tet"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "VAFIN", "PPER", "PRF", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Aus roher Wildheit und die Nacht gelichtet?", "tokens": ["Aus", "ro\u00b7her", "Wild\u00b7heit", "und", "die", "Nacht", "ge\u00b7lich\u00b7tet", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Was darf ihr tief Gef\u00fchl f\u00fcr Recht bestechen?", "tokens": ["Was", "darf", "ihr", "tief", "Ge\u00b7f\u00fchl", "f\u00fcr", "Recht", "be\u00b7ste\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "ADJD", "NN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Oft wagt's selbst nicht das Mitleid f\u00fcrzusprechen,", "tokens": ["Oft", "wagt's", "selbst", "nicht", "das", "Mit\u00b7leid", "f\u00f