{"dta.poem.19205": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "52.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die G\u00f6ttin, die, verh\u00fcllt, ums Antlitz Schleier tr\u00e4gt,", "tokens": ["Die", "G\u00f6t\u00b7tin", ",", "die", ",", "ver\u00b7h\u00fcllt", ",", "ums", "Ant\u00b7litz", "Schlei\u00b7er", "tr\u00e4gt", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "$,", "VVPP", "$,", "APPRART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Braut, nach welcher Lust der freche Freier tr\u00e4gt,", "tokens": ["Die", "Braut", ",", "nach", "wel\u00b7cher", "Lust", "der", "fre\u00b7che", "Frei\u00b7er", "tr\u00e4gt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "PWAT", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Mit Andacht nahen mu\u00dft du der geheimnisreichen,", "tokens": ["Mit", "An\u00b7dacht", "na\u00b7hen", "mu\u00dft", "du", "der", "ge\u00b7heim\u00b7nis\u00b7rei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJA", "VMFIN", "PPER", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn sie den Schl\u00fcssel dir soll zum Geheimnis reichen.", "tokens": ["Wenn", "sie", "den", "Schl\u00fcs\u00b7sel", "dir", "soll", "zum", "Ge\u00b7heim\u00b7nis", "rei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PPER", "VMFIN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Belausche, die im Traum ununterbrochen spricht,", "tokens": ["Be\u00b7lau\u00b7sche", ",", "die", "im", "Traum", "un\u00b7un\u00b7ter\u00b7bro\u00b7chen", "spricht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "APPRART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch unterbrich sie mit Dazwischensprechen nicht.", "tokens": ["Doch", "un\u00b7ter\u00b7brich", "sie", "mit", "Da\u00b7zwi\u00b7schen\u00b7spre\u00b7chen", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PPER", "APPR", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Behorchen magst du sie, nicht peinlich sie verh\u00f6ren;", "tokens": ["Be\u00b7hor\u00b7chen", "magst", "du", "sie", ",", "nicht", "pein\u00b7lich", "sie", "ver\u00b7h\u00f6\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PPER", "$,", "PTKNEG", "ADJD", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gehorchen wird sie nie, nur g\u00fcnstig dich erh\u00f6ren;", "tokens": ["Ge\u00b7hor\u00b7chen", "wird", "sie", "nie", ",", "nur", "g\u00fcns\u00b7tig", "dich", "er\u00b7h\u00f6\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ADV", "$,", "ADV", "ADJD", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Erh\u00f6ren g\u00fcnstig den, der br\u00fcnstig sie ersucht,", "tokens": ["Er\u00b7h\u00f6\u00b7ren", "g\u00fcns\u00b7tig", "den", ",", "der", "br\u00fcns\u00b7tig", "sie", "er\u00b7sucht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ART", "$,", "PRELS", "ADJD", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den aber \u00e4ffen, der zudringlich sie versucht,", "tokens": ["Den", "a\u00b7ber", "\u00e4f\u00b7fen", ",", "der", "zu\u00b7dring\u00b7lich", "sie", "ver\u00b7sucht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVINF", "$,", "PRELS", "ADJD", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Den, der mit Gaukelwerk und Taschenspiel beschw\u00f6ren", "tokens": ["Den", ",", "der", "mit", "Gau\u00b7kel\u00b7werk", "und", "Ta\u00b7schen\u00b7spiel", "be\u00b7schw\u00f6\u00b7ren"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "PRELS", "APPR", "NN", "KON", "NN", "VVINF"], "meter": "---+-+-+-+-+-", "measure": "unknown.measure.penta"}, "line.2": {"text": "Sie will, mit Gaukelwerk und Taschenspiel beth\u00f6ren.", "tokens": ["Sie", "will", ",", "mit", "Gau\u00b7kel\u00b7werk", "und", "Ta\u00b7schen\u00b7spiel", "be\u00b7th\u00f6\u00b7ren", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "APPR", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}