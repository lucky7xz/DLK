{"dta.poem.20809": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "An den geneigten  \n Leser.", "genre": "Lyrik; Prosa; Drama", "period": "N.A.", "pub_year": "1679", "urn": "urn:nbn:de:kobv:b4-20289-1", "language": ["de:0.99"], "booktitle": "Hofmann von Hofmannswaldau, Christian: Deutsche Ubersetzungen und Gedichte. Breslau, 1679."}, "poem": {"stanza.1": {"line.1": {"text": "O aller Schlangen Pracht/ kom\u0303 doch was zuverweilen/", "tokens": ["O", "al\u00b7ler", "Schlan\u00b7gen", "Pracht", "/", "kom\u0303", "doch", "was", "zu\u00b7ver\u00b7wei\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PIAT", "NN", "NN", "$(", "VVIMP", "ADV", "PIS", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Halt an dein tschischend eilen/", "tokens": ["Halt", "an", "dein", "tschi\u00b7schend", "ei\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "APPR", "PPOSAT", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Verbleib doch was allhier/", "tokens": ["Ver\u00b7bleib", "doch", "was", "all\u00b7hier", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "PWS", "ADV", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "So thustu einen Dienst der Liebsten und auch mir.", "tokens": ["So", "thu\u00b7stu", "ei\u00b7nen", "Dienst", "der", "Liebs\u00b7ten", "und", "auch", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "KON", "ADV", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Kom\u0303/ Schwestrichen/ kom\u0303 bald/ der k\u00fchle zugen\u00fcssen.", "tokens": ["Kom\u0303", "/", "Schwest\u00b7ri\u00b7chen", "/", "kom\u0303", "bald", "/", "der", "k\u00fch\u00b7le", "zu\u00b7ge\u00b7n\u00fcs\u00b7sen", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "$(", "NN", "$(", "VVIMP", "ADV", "$(", "ART", "ADJA", "VVPP", "$."], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Sie ist allzeit beflissen/", "tokens": ["Sie", "ist", "all\u00b7zeit", "be\u00b7flis\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "VVPP", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Durch K\u00fcnstler abgericht/", "tokens": ["Durch", "K\u00fcnst\u00b7ler", "ab\u00b7ge\u00b7richt", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Zu mahlen/ was jhr nur kom\u0303t vor das Angesicht.", "tokens": ["Zu", "mah\u00b7len", "/", "was", "jhr", "nur", "kom\u0303t", "vor", "das", "An\u00b7ge\u00b7sicht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$(", "PWS", "PPER", "ADV", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+--+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "So kan nach freyer Arth/", "tokens": ["So", "kan", "nach", "frey\u00b7er", "Ar\u00b7th", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "APPR", "ADJA", "NN", "$("], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.10": {"text": "Nach deines Balges prangen/", "tokens": ["Nach", "dei\u00b7nes", "Bal\u00b7ges", "pran\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Ich dieses bald erlangen", "tokens": ["Ich", "die\u00b7ses", "bald", "er\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "PDS", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Da\u00df sie mit jhrer Hand", "tokens": ["Da\u00df", "sie", "mit", "jhrer", "Hand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.13": {"text": "Mir zubereiten wird ein sch\u00f6nes G\u00fcrtel-Band.", "tokens": ["Mir", "zu\u00b7be\u00b7rei\u00b7ten", "wird", "ein", "sch\u00f6\u00b7nes", "G\u00fcr\u00b7tel\u00b7Band", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "So m\u00fcssen/ glatte Schlang/ auch alle deines gleichen", "tokens": ["So", "m\u00fcs\u00b7sen", "/", "glat\u00b7te", "Schlang", "/", "auch", "al\u00b7le", "dei\u00b7nes", "glei\u00b7chen"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "$(", "ADJA", "NN", "$(", "ADV", "PIS", "PPOSAT", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Auff ewig vor dir weichen.", "tokens": ["Auff", "e\u00b7wig", "vor", "dir", "wei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.16": {"text": "Es wird die scharffe Zeit", "tokens": ["Es", "wird", "die", "scharf\u00b7fe", "Zeit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.17": {"text": "Dir nicht beschwerlich seyn durch jhre Hefftigkeit.", "tokens": ["Dir", "nicht", "be\u00b7schwer\u00b7lich", "seyn", "durch", "jhre", "Heff\u00b7tig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "ADJD", "VAINF", "APPR", "PPOSAT", "NN", "$."], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}}}}}