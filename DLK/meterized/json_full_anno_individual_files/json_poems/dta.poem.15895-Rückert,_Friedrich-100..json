{"dta.poem.15895": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "100.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Sch\u00e4mst du dich nicht, so breit dich auf der Welt zu machen,", "tokens": ["Sch\u00e4mst", "du", "dich", "nicht", ",", "so", "breit", "dich", "auf", "der", "Welt", "zu", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "PTKNEG", "$,", "ADV", "ADJD", "PRF", "APPR", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit solcher Wichtigkeit zu treiben kleine Sachen?", "tokens": ["Mit", "sol\u00b7cher", "Wich\u00b7tig\u00b7keit", "zu", "trei\u00b7ben", "klei\u00b7ne", "Sa\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "PTKZU", "VVINF", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ein jegliches Gef\u00fchl in einen Vers zu fassen,", "tokens": ["Ein", "jeg\u00b7li\u00b7ches", "Ge\u00b7f\u00fchl", "in", "ei\u00b7nen", "Vers", "zu", "fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Um von des Markts Gew\u00fchl bewundern es zu lassen?", "tokens": ["Um", "von", "des", "Markts", "Ge\u00b7w\u00fchl", "be\u00b7wun\u00b7dern", "es", "zu", "las\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "APPR", "ART", "NN", "NN", "VVFIN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wielange willst du auf der Welt nichts be\u00dfres thun? \u2014", "tokens": ["Wie\u00b7lan\u00b7ge", "willst", "du", "auf", "der", "Welt", "nichts", "be\u00df\u00b7res", "thun", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "VMFIN", "PPER", "APPR", "ART", "NN", "PIS", "ADJA", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Solang es gibt auf ihr nichts besseres als nun.", "tokens": ["So\u00b7lang", "es", "gibt", "auf", "ihr", "nichts", "bes\u00b7se\u00b7res", "als", "nun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "APPR", "PPER", "PIS", "PIS", "KOKOM", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}