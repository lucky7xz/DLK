{"textgrid.poem.58207": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "245. Was h\u00e4lt mich auf?", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du klagst, es fall' dir schwer; du fragst: \u00bbWas h\u00e4lt mich auf?\u00ab", "tokens": ["Du", "klagst", ",", "es", "fall'", "dir", "schwer", ";", "du", "fragst", ":", "\u00bb", "Was", "h\u00e4lt", "mich", "auf", "?", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "PPER", "ADJD", "$.", "PPER", "VVFIN", "$.", "$(", "PWS", "VVFIN", "PPER", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vernunft und Eigenwill', die hindern deinen Lauf;", "tokens": ["Ver\u00b7nunft", "und", "Ei\u00b7gen\u00b7will'", ",", "die", "hin\u00b7dern", "dei\u00b7nen", "Lauf", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "PRELS", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ei, \u00fcberleg nicht viel, glaub blindlings ohne Sehen", "tokens": ["Ei", ",", "\u00fc\u00b7ber\u00b7leg", "nicht", "viel", ",", "glaub", "blind\u00b7lings", "oh\u00b7ne", "Se\u00b7hen"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "ADV", "PTKNEG", "ADV", "$,", "VVFIN", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und bleib stets willenlos, so wirst du leichter gehen!", "tokens": ["Und", "bleib", "stets", "wil\u00b7len\u00b7los", ",", "so", "wirst", "du", "leich\u00b7ter", "ge\u00b7h