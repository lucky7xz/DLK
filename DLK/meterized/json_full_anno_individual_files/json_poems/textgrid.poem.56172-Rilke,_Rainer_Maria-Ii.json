{"textgrid.poem.56172": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "Ii", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du hast mir, Sommer, der du pl\u00f6tzlich bist,", "tokens": ["Du", "hast", "mir", ",", "Som\u00b7mer", ",", "der", "du", "pl\u00f6tz\u00b7lich", "bist", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "$,", "NN", "$,", "PRELS", "PPER", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "zum j\u00e4hen Baum den Samen aufgezogen.", "tokens": ["zum", "j\u00e4\u00b7hen", "Baum", "den", "Sa\u00b7men", "auf\u00b7ge\u00b7zo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "(innen Ger\u00e4umige, f\u00fchl in dir den Bogen", "tokens": ["(", "in\u00b7nen", "Ge\u00b7r\u00e4u\u00b7mi\u00b7ge", ",", "f\u00fchl", "in", "dir", "den", "Bo\u00b7gen"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["$(", "ADV", "NN", "$,", "VVFIN", "APPR", "PPER", "ART", "NN"], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.4": {"text": "der Nacht, in der er m\u00fcndig ist.)", "tokens": ["der", "Nacht", ",", "in", "der", "er", "m\u00fcn\u00b7dig", "ist", ".", ")"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "$,", "APPR", "PRELS", "PPER", "ADJD", "VAFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Nun hob er sich und w\u00e4chst zum Firmament,", "tokens": ["Nun", "hob", "er", "sich", "und", "w\u00e4chst", "zum", "Fir\u00b7ma\u00b7ment", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "KON", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "ein Spiegelbild das neben B\u00e4umen steht.", "tokens": ["ein", "Spie\u00b7gel\u00b7bild", "das", "ne\u00b7ben", "B\u00e4u\u00b7men", "steht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+---+-+", "measure": "zehnsilber"}, "line.7": {"text": "O st\u00fcrz ihn, da\u00df er, umgedreht", "tokens": ["O", "st\u00fcrz", "ihn", ",", "da\u00df", "er", ",", "um\u00b7ge\u00b7dreht"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["NE", "VVFIN", "PPER", "$,", "KOUS", "PPER", "$,", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "in deinen Scho\u00df, den Gegen-Himmel kennt,", "tokens": ["in", "dei\u00b7nen", "Scho\u00df", ",", "den", "Ge\u00b7gen\u00b7Him\u00b7mel", "kennt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "in den er wirklich b\u00e4umt und ragt.", "tokens": ["in", "den", "er", "wirk\u00b7lich", "b\u00e4umt", "und", "ragt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ADJD", "VVPP", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Gewagte Landschaft, wie sie Seherinnen", "tokens": ["Ge\u00b7wag\u00b7te", "Land\u00b7schaft", ",", "wie", "sie", "Se\u00b7he\u00b7rin\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "PWAV", "PPER", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic