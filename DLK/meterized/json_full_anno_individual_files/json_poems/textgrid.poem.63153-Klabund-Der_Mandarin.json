{"textgrid.poem.63153": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "Der Mandarin", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das starre Licht des sonnenhaften Thrones", "tokens": ["Das", "star\u00b7re", "Licht", "des", "son\u00b7nen\u00b7haf\u00b7ten", "Thro\u00b7nes"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "F\u00e4llt auf der Majest\u00e4t gefurchte Mienen.", "tokens": ["F\u00e4llt", "auf", "der", "Ma\u00b7jes\u00b7t\u00e4t", "ge\u00b7furch\u00b7te", "Mie\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Um die Gestalt des hohen Himmelssohnes", "tokens": ["Um", "die", "Ge\u00b7stalt", "des", "ho\u00b7hen", "Him\u00b7mels\u00b7soh\u00b7nes"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Stehn in Ergebenheit die Mandarinen.", "tokens": ["Stehn", "in", "Er\u00b7ge\u00b7ben\u00b7heit", "die", "Man\u00b7da\u00b7ri\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "ART", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}, "stanza.2": {"line.1": {"text": "Er blickt, dieweil er leitet Licht und Land,", "tokens": ["Er", "blickt", ",", "die\u00b7weil", "er", "lei\u00b7tet", "Licht", "und", "Land", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Durchs offne Fenster in den Bl\u00fctenreigen.", "tokens": ["Durchs", "off\u00b7ne", "Fens\u00b7ter", "in", "den", "Bl\u00fc\u00b7ten\u00b7rei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein Blumenantlitz ist ihm zugewandt.", "tokens": ["Ein", "Blu\u00b7men\u00b7ant\u00b7litz", "ist", "ihm", "zu\u00b7ge\u00b7wandt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ein F\u00e4cher winkt. Der Kaiser hebt die Hand", "tokens": ["Ein", "F\u00e4\u00b7cher", "winkt", ".", "Der", "Kai\u00b7ser", "hebt", "die", "Hand"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$.", "ART", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und schreitet zwischen K\u00f6pfen, die sich neigen.", "tokens": ["Und", "schrei\u00b7tet", "zwi\u00b7schen", "K\u00f6p\u00b7fen", ",", "die", "sich", "nei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "$,", "PRELS", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Am Neujahrstag erbat ich Audienz.", "tokens": ["Am", "Neu\u00b7jahr\u00b7stag", "er\u00b7bat", "ich", "Au\u00b7di\u00b7enz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der Kaiser war wie immer mir gewogen.", "tokens": ["Der", "Kai\u00b7ser", "war", "wie", "im\u00b7mer", "mir", "ge\u00b7wo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "KOKOM", "ADV", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Er gab mir Urlaub. Urlaub bis zum Lenz.", "tokens": ["Er", "gab", "mir", "Ur\u00b7laub", ".", "Ur\u00b7laub", "bis", "zum", "Lenz", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "NN", "$.", "NN", "APPR", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Zu Weib und Kindern bin ich heimgezogen.", "tokens": ["Zu", "Weib", "und", "Kin\u00b7dern", "bin", "ich", "heim\u00b7ge\u00b7zo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VAFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Im Westen geht die rote Sonne unter.", "tokens": ["Im", "Wes\u00b7ten", "geht", "die", "ro\u00b7te", "Son\u00b7ne", "un\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "ADJA", "NN", "APPR", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Spatzen l\u00e4rmen irgendwo am Tor.", "tokens": ["Die", "Spat\u00b7zen", "l\u00e4r\u00b7men", "ir\u00b7gend\u00b7wo", "am", "Tor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ich bin am Ziel. Aus Str\u00e4uchern l\u00e4chelt bunter", "tokens": ["Ich", "bin", "am", "Ziel", ".", "Aus", "Str\u00e4u\u00b7chern", "l\u00e4\u00b7chelt", "bun\u00b7ter"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPRART", "NN", "$.", "APPR", "NN", "VVFIN", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Bewimpelt wie ein Schiff mein Haus hervor.", "tokens": ["Be\u00b7wim\u00b7pelt", "wie", "ein", "Schiff", "mein", "Haus", "her\u00b7vor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOKOM", "ART", "NN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Mein Weib! Mein Kind! Da bin ich endlich wieder!", "tokens": ["Mein", "Weib", "!", "Mein", "Kind", "!", "Da", "bin", "ich", "end\u00b7lich", "wie\u00b7der", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "PPOSAT", "NN", "$.", "ADV", "VAFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ihr findet Worte nicht und Tr\u00e4nen nur.", "tokens": ["Ihr", "fin\u00b7det", "Wor\u00b7te", "nicht", "und", "Tr\u00e4\u00b7nen", "nur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "PTKNEG", "KON", "NN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der B\u00fcrgerkrieg zerreisst des Landes Glieder,", "tokens": ["Der", "B\u00fcr\u00b7ger\u00b7krieg", "zer\u00b7reisst", "des", "Lan\u00b7des", "Glie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und Galgen stehn statt B\u00e4ume auf der Flur.", "tokens": ["Und", "Gal\u00b7gen", "stehn", "statt", "B\u00e4u\u00b7me", "auf", "der", "Flur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "APPR", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Wir wrackes Boot, am Ufer angekettet,", "tokens": ["Wir", "wra\u00b7ckes", "Boot", ",", "am", "U\u00b7fer", "an\u00b7ge\u00b7ket\u00b7tet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Heimat liegt weit draussen auf dem Meer.", "tokens": ["Die", "Hei\u00b7mat", "liegt", "weit", "draus\u00b7sen", "auf", "dem", "Meer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wie schmerzlich klingen, weibisch und verfettet,", "tokens": ["Wie", "schmerz\u00b7lich", "klin\u00b7gen", ",", "wei\u00b7bisch", "und", "ver\u00b7fet\u00b7tet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVINF", "$,", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der W\u00e4scherinnen Rufe zu uns her ...", "tokens": ["Der", "W\u00e4\u00b7sche\u00b7rin\u00b7nen", "Ru\u00b7fe", "zu", "uns", "her", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPR", "PRF", "APZR", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Wo ist der Wein? Er hat genug gegoren.", "tokens": ["Wo", "ist", "der", "Wein", "?", "Er", "hat", "ge\u00b7nug", "ge\u00b7go\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "$.", "PPER", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein Duft weht durch die d\u00fcrren Baumalleen.", "tokens": ["Ein", "Duft", "weht", "durch", "die", "d\u00fcr\u00b7ren", "Bau\u00b7mal\u00b7le\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.8": {"line.1": {"text": "Die R\u00e4uber haben mir den Zopf geschoren.", "tokens": ["Die", "R\u00e4u\u00b7ber", "ha\u00b7ben", "mir", "den", "Zopf", "ge\u00b7scho\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ihr Kindelein, ich hab den Weg verloren,", "tokens": ["Ihr", "Kin\u00b7de\u00b7lein", ",", "ich", "hab", "den", "Weg", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPER", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Es ist zu sp\u00e4t, im Dunkeln heimzugehn ...", "tokens": ["Es", "ist", "zu", "sp\u00e4t", ",", "im", "Dun\u00b7keln", "heim\u00b7zu\u00b7gehn", "..."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKA", "ADJD", "$,", "APPRART", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Ich musste blutend tausend Meilen rennen,", "tokens": ["Ich", "muss\u00b7te", "blu\u00b7tend", "tau\u00b7send", "Mei\u00b7len", "ren\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADJD", "CARD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "An tausend Galgen sah ich mich verwehn.", "tokens": ["An", "tau\u00b7send", "Gal\u00b7gen", "sah", "ich", "mich", "ver\u00b7wehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "VVFIN", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Es wird schon Nacht. Komm, lass die Lampe brennen", "tokens": ["Es", "wird", "schon", "Nacht", ".", "Komm", ",", "lass", "die", "Lam\u00b7pe", "bren\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "NN", "$.", "VVFIN", "$,", "VVFIN", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und lass uns schweigend in die Augen sehn ...", "tokens": ["Und", "lass", "uns", "schwei\u00b7gend", "in", "die", "Au\u00b7gen", "sehn", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Du bist der tiefste Brunnen, draus zu sch\u00f6pfen", "tokens": ["Du", "bist", "der", "tiefs\u00b7te", "Brun\u00b7nen", ",", "draus", "zu", "sch\u00f6p\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,", "PAV", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Jahrtausende nicht m\u00fcde werden k\u00f6nnen.", "tokens": ["Jahr\u00b7tau\u00b7sen\u00b7de", "nicht", "m\u00fc\u00b7de", "wer\u00b7den", "k\u00f6n\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "ADJD", "VAINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und wenn sie jeden Morgen neu beg\u00f6nnen,", "tokens": ["Und", "wenn", "sie", "je\u00b7den", "Mor\u00b7gen", "neu", "be\u00b7g\u00f6n\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PIAT", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nur immer reicher str\u00f6mt es ihren T\u00f6pfen.", "tokens": ["Nur", "im\u00b7mer", "rei\u00b7cher", "str\u00f6mt", "es", "ih\u00b7ren", "T\u00f6p\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.11": {"line.1": {"text": "Um deinetwillen lassen sie sich k\u00f6pfen,", "tokens": ["Um", "dei\u00b7net\u00b7wil\u00b7len", "las\u00b7sen", "sie", "sich", "k\u00f6p\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADV", "VVFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "O Sohn des Himmels, dass ihr Herzblut r\u00f6nne", "tokens": ["O", "Sohn", "des", "Him\u00b7mels", ",", "dass", "ihr", "Herz\u00b7blut", "r\u00f6n\u00b7ne"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NN", "ART", "NN", "$,", "KOUS", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und eine Tr\u00e4ne deines Augs gew\u00f6nne.", "tokens": ["Und", "ei\u00b7ne", "Tr\u00e4\u00b7ne", "dei\u00b7nes", "Augs", "ge\u00b7w\u00f6n\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wer st\u00fcrb nicht selig unter deinen Z\u00f6pfen", "tokens": ["Wer", "st\u00fcrb", "nicht", "se\u00b7lig", "un\u00b7ter", "dei\u00b7nen", "Z\u00f6p\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PTKNEG", "ADJD", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.12": {"line.1": {"text": "Am h\u00f6chsten Turm von Peking aufgeh\u00e4ngt?", "tokens": ["Am", "h\u00f6chs\u00b7ten", "Turm", "von", "Pe\u00b7king", "auf\u00b7ge\u00b7h\u00e4ngt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "APPR", "NE", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Er legt die Haarschnur um den Hals sich stumm,", "tokens": ["Er", "legt", "die", "Haar\u00b7schnur", "um", "den", "Hals", "sich", "stumm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "PRF", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "In der er zart nun wie ein T\u00e4nzer schwenkt.", "tokens": ["In", "der", "er", "zart", "nun", "wie", "ein", "T\u00e4n\u00b7zer", "schwenkt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ADJD", "ADV", "KOKOM", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.13": {"line.1": {"text": "Er greift, als spiele er Harmonium.", "tokens": ["Er", "greift", ",", "als", "spie\u00b7le", "er", "Har\u00b7mo\u00b7ni\u00b7um", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOKOM", "VVFIN", "PPER", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Kaum hat der Tod den kahlen Kopf gesenkt,", "tokens": ["Kaum", "hat", "der", "Tod", "den", "kah\u00b7len", "Kopf", "ge\u00b7senkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Legt schon ein andrer sich die Haarschnur um.", "tokens": ["Legt", "schon", "ein", "an\u00b7drer", "sich", "die", "Haar\u00b7schnur", "um", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "PRF", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.14": {"line.1": {"text": "Das starre Licht des sonnenhaften Thrones", "tokens": ["Das", "star\u00b7re", "Licht", "des", "son\u00b7nen\u00b7haf\u00b7ten", "Thro\u00b7nes"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "F\u00e4llt auf der Majest\u00e4t gefurchte Mienen.", "tokens": ["F\u00e4llt", "auf", "der", "Ma\u00b7jes\u00b7t\u00e4t", "ge\u00b7furch\u00b7te", "Mie\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Um die Gestalt des hohen Himmelssohnes", "tokens": ["Um", "die", "Ge\u00b7stalt", "des", "ho\u00b7hen", "Him\u00b7mels\u00b7soh\u00b7nes"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Stehn in Ergebenheit die Mandarinen.", "tokens": ["Stehn", "in", "Er\u00b7ge\u00b7ben\u00b7heit", "die", "Man\u00b7da\u00b7ri\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "ART", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}, "stanza.15": {"line.1": {"text": "Er blickt, dieweil er leitet Licht und Land,", "tokens": ["Er", "blickt", ",", "die\u00b7weil", "er", "lei\u00b7tet", "Licht", "und", "Land", ","],