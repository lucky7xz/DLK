": {"line.1": {"text": "Und dann \u2013 nicht wahr, seit alter Zeit", "tokens": ["Und", "dann", "\u2013", "nicht", "wahr", ",", "seit", "al\u00b7ter", "Zeit"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "$(", "PTKNEG", "ADJD", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ist es der Brauch gewesen,", "tokens": ["Ist", "es", "der", "Brauch", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "VAPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df man aus Pfiemenkraut und Heid'", "tokens": ["Da\u00df", "man", "aus", "Pfie\u00b7men\u00b7kraut", "und", "Heid'"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gebunden hat den Besen?", "tokens": ["Ge\u00b7bun\u00b7den", "hat", "den", "Be\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Den Besen, der die Gassen kehrt,", "tokens": ["Den", "Be\u00b7sen", ",", "der", "die", "Gas\u00b7sen", "kehrt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der wie ein Wetter niederf\u00e4hrt,", "tokens": ["Der", "wie", "ein", "Wet\u00b7ter", "nie\u00b7der\u00b7f\u00e4hrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "KOKOM", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wo Staub und Wust sich br\u00fcsten!", "tokens": ["Wo", "Staub", "und", "Wust", "sich", "br\u00fcs\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "So sei dir denn auch noch vertraut,", "tokens": ["So", "sei", "dir", "denn", "auch", "noch", "ver\u00b7traut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was junge Sagen k\u00fcnden:", "tokens": ["Was", "jun\u00b7ge", "Sa\u00b7gen", "k\u00fcn\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Bald wird aus niederm Heidekraut", "tokens": ["Bald", "wird", "aus", "nie\u00b7derm", "Hei\u00b7de\u00b7kraut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sich selbst ein Besen binden,", "tokens": ["Sich", "selbst", "ein", "Be\u00b7sen", "bin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ein ries'ger, der der Niedertracht", "tokens": ["Ein", "ries'\u00b7ger", ",", "der", "der", "Nie\u00b7der\u00b7tracht"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "$,", "PRELS", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und Sklaverei ein Ende macht", "tokens": ["Und", "Skla\u00b7ve\u00b7rei", "ein", "En\u00b7de", "macht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "In Deutschland und auf Erden!", "tokens": ["In", "Deutschland", "und", "auf", "Er\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "APPR", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.16": {"line.1": {"text": "Dann wird auch uns zur Wiederkehr", "tokens": ["Dann", "wird", "auch", "uns", "zur", "Wie\u00b7der\u00b7kehr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Freiheit Glocke l\u00e4uten;", "tokens": ["Der", "Frei\u00b7heit", "Glo\u00b7cke", "l\u00e4u\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Dann wird uns keine Scherbe mehr", "tokens": ["Dann", "wird", "uns", "kei\u00b7ne", "Scher\u00b7be", "mehr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PIAT", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Heimat und Herd bedeuten;", "tokens": ["Hei\u00b7mat", "und", "Herd", "be\u00b7deu\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVINF", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.5": {"text": "Dann \u2013 doch mir schl\u00e4gt das Herz wie toll!", "tokens": ["Dann", "\u2013", "doch", "mir", "schl\u00e4gt", "das", "Herz", "wie", "toll", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "KON", "PPER", "VVFIN", "ART", "NN", "KOKOM", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Rasch, gie\u00df mir einen Tummler voll,", "tokens": ["Rasch", ",", "gie\u00df", "mir", "ei\u00b7nen", "Tumm\u00b7ler", "voll", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "VVFIN", "PPER", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Da\u00df ich dich leben lasse!", "tokens": ["Da\u00df", "ich", "dich", "le\u00b7ben", "las\u00b7se", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}