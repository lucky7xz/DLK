{"textgrid.poem.52258": {"metadata": {"author": {"name": "Freiligrath, Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Meiner Frau zum Geburtstage", "genre": "verse", "period": "N.A.", "pub_year": 1843, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Heide, die bei uns zuland'", "tokens": ["Die", "Hei\u00b7de", ",", "die", "bei", "uns", "zu\u00b7land'"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NE", "$,", "PRELS", "APPR", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Allw\u00e4rts ihr Gr\u00fcn vergeudet;", "tokens": ["All\u00b7w\u00e4rts", "ihr", "Gr\u00fcn", "ver\u00b7geu\u00b7det", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVPP", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Die Berg und Schlucht und Felsenwand", "tokens": ["Die", "Berg", "und", "Schlucht", "und", "Fel\u00b7sen\u00b7wand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit starren B\u00fcscheln kleidet;", "tokens": ["Mit", "star\u00b7ren", "B\u00fc\u00b7scheln", "klei\u00b7det", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die hoch und tief sich blicken l\u00e4\u00dft,", "tokens": ["Die", "hoch", "und", "tief", "sich", "bli\u00b7cken", "l\u00e4\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "KON", "ADJD", "PRF", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die bring' ich dir zu deinem Fest", "tokens": ["Die", "bring'", "ich", "dir", "zu", "dei\u00b7nem", "Fest"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPER", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "In schlichter irdner Scherbe.", "tokens": ["In", "schlich\u00b7ter", "ird\u00b7ner", "Scher\u00b7be", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wo du und ich geboren sind,", "tokens": ["Wo", "du", "und", "ich", "ge\u00b7bo\u00b7ren", "sind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "KON", "PPER", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da rauscht sie allerorten;", "tokens": ["Da", "rauscht", "sie", "al\u00b7le\u00b7ror\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sie sch\u00fcttelt sich im Morgenwind", "tokens": ["Sie", "sch\u00fct\u00b7telt", "sich", "im", "Mor\u00b7gen\u00b7wind"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vor deiner Wartburg Pforten;", "tokens": ["Vor", "dei\u00b7ner", "Wart\u00b7burg", "Pfor\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Sie spiegelt sich in Ilm und Saal'", "tokens": ["Sie", "spie\u00b7gelt", "sich", "in", "Ilm", "und", "Saal'"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "NE", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und in der Unstrut goldnes Tal", "tokens": ["Und", "in", "der", "Un\u00b7strut", "gold\u00b7nes", "Tal"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Herschaut sie vom Kyffh\u00e4user.", "tokens": ["Her\u00b7schaut", "sie", "vom", "Kyff\u00b7h\u00e4u\u00b7ser", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und auch bei mir mit hellem Schein", "tokens": ["Und", "auch", "bei", "mir", "mit", "hel\u00b7lem", "Schein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Schm\u00fcckt sie die Bergeshalde;", "tokens": ["Schm\u00fcckt", "sie", "die", "Ber\u00b7ges\u00b7hal\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sie wallt um meinen Externstein", "tokens": ["Sie", "wallt", "um", "mei\u00b7nen", "Ex\u00b7tern\u00b7stein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und rings im Lipp'schen Walde;", "tokens": ["Und", "rings", "im", "Lipp'\u00b7schen", "Wal\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Da summen Bienen um sie her,", "tokens": ["Da", "sum\u00b7men", "Bie\u00b7nen", "um", "sie", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und durch ihr rotes Bl\u00fctenmeer", "tokens": ["Und", "durch", "ihr", "ro\u00b7tes", "Bl\u00fc\u00b7ten\u00b7meer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ausschlagend jagt der Senner.", "tokens": ["Aus\u00b7schla\u00b7gend", "jagt", "der", "Sen\u00b7ner", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Der alte Rhein, der Traubenkoch,", "tokens": ["Der", "al\u00b7te", "Rhein", ",", "der", "Trau\u00b7ben\u00b7koch", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NE", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "K\u00f6nnt' ihrer wohl entbehren;", "tokens": ["K\u00f6nnt'", "ih\u00b7rer", "wohl", "ent\u00b7beh\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Doch ward auch ihm die Heide noch", "tokens": ["Doch", "ward", "auch", "ihm", "die", "Hei\u00b7de", "noch"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "ADV", "PPER", "ART", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zu seinen andern Ehren.", "tokens": ["Zu", "sei\u00b7nen", "an\u00b7dern", "Eh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wie oft an Forst- und Gr\u00fcndelbach", "tokens": ["Wie", "oft", "an", "For\u00b7st", "und", "Gr\u00fcn\u00b7del\u00b7bach"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "APPR", "TRUNC", "KON", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Unter der Birke wehndem Dach", "tokens": ["Un\u00b7ter", "der", "Bir\u00b7ke", "wehn\u00b7dem", "Dach"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Winkt' und ihr schwellend Kissen!", "tokens": ["Winkt'", "und", "ihr", "schwel\u00b7lend", "Kis\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "PPER", "ADJD", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.5": {"line.1": {"text": "Da bebt sie sp\u00e4t, da bebt sie fr\u00fch,", "tokens": ["Da", "bebt", "sie", "sp\u00e4t", ",", "da", "bebt", "sie", "fr\u00fch", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "$,", "ADV", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da flammt sie durchs Geh\u00f6lze;", "tokens": ["Da", "flammt", "sie", "durchs", "Ge\u00b7h\u00f6l\u00b7ze", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da kr\u00f6nt die siebte M\u00fchle sie", "tokens": ["Da", "kr\u00f6nt", "die", "sieb\u00b7te", "M\u00fch\u00b7le", "sie"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und auch die Silberschmelze;", "tokens": ["Und", "auch", "die", "Sil\u00b7ber\u00b7schmel\u00b7ze", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Da kr\u00f6nt sie Brunn und Felsenschlucht \u2013", "tokens": ["Da", "kr\u00f6nt", "sie", "Brunn", "und", "Fel\u00b7sen\u00b7schlucht", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "O, m\u00f6ge dieser Scherbenhucht", "tokens": ["O", ",", "m\u00f6\u00b7ge", "die\u00b7ser", "Scher\u00b7ben\u00b7hucht"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "VMFIN", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "An alles das dich mahnen!", "tokens": ["An", "al\u00b7les", "das", "dich", "mah\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "ART", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Und dann \u2013 nicht wahr, seit alter Zeit", "tokens": ["Und", "dann", "\u2013", "nicht", "wahr", ",", "seit", "al\u00b7ter", "Zeit"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "$(", "PTKNEG", "ADJD", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ist es der Brauch gewesen,", "tokens": ["Ist", "es", "der", "Brauch", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "VAPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df man aus Pfiemenkraut und Heid'", "tokens": ["Da\u00df", "man", "aus", "Pfie\u00b7men\u00b7kraut", "und", "Heid'"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gebunden hat den Besen?", "tokens": ["Ge\u00b7bun\u00b7den", "hat", "den", "Be\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Den Besen, der die Gassen kehrt,", "tokens": ["Den", "Be\u00b7sen", ",", "der", "die", "Gas\u00b7sen", "kehrt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der wie ein Wetter niederf\u00e4hrt,", "tokens": ["Der", "wie", "ein", "Wet\u00b7ter", "nie\u00b7der\u00b7f\u00e4hrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "KOKOM", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wo Staub und Wust sich br\u00fcsten!", "tokens": ["Wo", "Staub", "und", "Wust", "sich", "br\u00fcs\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "So sei dir denn auch noch vertraut,", "tokens": ["So", "sei", "dir", "denn", "auch", "noch", "ver\u00b7traut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was junge Sagen k\u00fcnden:", "tokens": ["Was", "jun\u00b7ge", "Sa\u00b7gen", "k\u00fcn\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Bald wird aus niederm Heidekraut", "tokens": ["Bald", "wird", "aus", "nie\u00b7derm", "Hei\u00b7de\u00b7kraut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sich selbst ein Besen binden,", "tokens": ["Sich", "selbst", "ein", "Be\u00b7sen", "bin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ein ries'ger, der der Niedertracht", "tokens": ["Ein", "ries'\u00b7ger", ",", "der", "der", "Nie\u00b7der\u00b7tracht"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "$,", "PRELS", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und Sklaverei ein Ende macht", "tokens": ["Und", "Skla\u00b7ve\u00b7rei", "ein", "En\u00b7de", "macht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "In Deutschland und auf Erden!", "tokens": ["In", "Deutschland", "und", "auf", "Er\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "APPR", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.8": {"line.1": {"text": "Dann wird auch uns zur Wiederkehr", "tokens": ["Dann", "wird", "auch", "uns", "zur", "Wie\u00b7der\u00b7kehr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Freiheit Glocke l\u00e4uten;", "tokens": ["Der", "Frei\u00b7heit", "Glo\u00b7cke", "l\u00e4u\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Dann wird uns keine Scherbe mehr", "tokens": ["Dann", "wird", "uns", "kei\u00b7ne", "Scher\u00b7be", "mehr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PIAT", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Heimat und Herd bedeuten;", "tokens": ["Hei\u00b7mat", "und", "Herd", "be\u00b7deu\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVINF", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.5": {"text": "Dann \u2013 doch mir schl\u00e4gt das Herz wie toll!", "tokens": ["Dann", "\u2013", "doch", "mir", "schl\u00e4gt", "das", "Herz", "wie", "toll", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "KON", "PPER", "VVFIN", "ART", "NN", "KOKOM", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Rasch, gie\u00df mir einen Tummler voll,", "tokens": ["Rasch", ",", "gie\u00df", "mir", "ei\u00b7nen", "Tumm\u00b7ler", "voll", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "VVFIN", "PPER", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Da\u00df ich dich leben lasse!", "tokens": ["Da\u00df", "ich", "dich", "le\u00b7ben", "las\u00b7se", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Die Heide, die bei uns zuland'", "tokens": ["Die", "Hei\u00b7de", ",", "die", "bei", "uns", "zu\u00b7land'"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NE", "$,", "PRELS", "APPR", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Allw\u00e4rts ihr Gr\u00fcn vergeudet;", "tokens": ["All\u00b7w\u00e4rts", "ihr", "Gr\u00fcn", "ver\u00b7geu\u00b7det", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVPP", "$."], "meter": "+--+-+-", "measure": "iambic.tri.in