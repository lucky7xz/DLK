{"dta.poem.13483": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Ii .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Ich geh' durch die dunkeln Gassen", "tokens": ["Ich", "geh'", "durch", "die", "dun\u00b7keln", "Gas\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und wandre von Haus zu Haus,", "tokens": ["Und", "wand\u00b7re", "von", "Haus", "zu", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "APPR", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ich kann mich noch immer nicht fassen,", "tokens": ["Ich", "kann", "mich", "noch", "im\u00b7mer", "nicht", "fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Sieht alles so tr\u00fcbe aus.", "tokens": ["Sieht", "al\u00b7les", "so", "tr\u00fc\u00b7be", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "VVFIN", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Da gehen viel M\u00e4nner und Frauen,", "tokens": ["Da", "ge\u00b7hen", "viel", "M\u00e4n\u00b7ner", "und", "Frau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "KON", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Die alle so lustig sehn,", "tokens": ["Die", "al\u00b7le", "so", "lus\u00b7tig", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Die fahren und lachen und bauen,", "tokens": ["Die", "fah\u00b7ren", "und", "la\u00b7chen", "und", "bau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "KON", "VVINF", "KON", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Da\u00df mir die Sinne vergehn.", "tokens": ["Da\u00df", "mir", "die", "Sin\u00b7ne", "ver\u00b7gehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "Oft wenn ich bl\u00e4uliche Streifen", "tokens": ["Oft", "wenn", "ich", "bl\u00e4u\u00b7li\u00b7che", "Strei\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "PPER", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Seh' \u00fcber die D\u00e4cher fliehn,", "tokens": ["Seh'", "\u00fc\u00b7ber", "die", "D\u00e4\u00b7cher", "fliehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Sonnenschein drau\u00dfen schweifen,", "tokens": ["Son\u00b7nen\u00b7schein", "drau\u00b7\u00dfen", "schwei\u00b7fen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVINF", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Wolken am Himmel ziehn:", "tokens": ["Wol\u00b7ken", "am", "Him\u00b7mel", "ziehn", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "VVINF", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.4": {"line.1": {"text": "Da treten mitten im Scherze", "tokens": ["Da", "tre\u00b7ten", "mit\u00b7ten", "im", "Scher\u00b7ze"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "APPRART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Die Thr\u00e4nen ins Auge mir,", "tokens": ["Die", "Thr\u00e4\u00b7nen", "ins", "Au\u00b7ge", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "PPER", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Denn die mich lieben von Herzen", "tokens": ["Denn", "die", "mich", "lie\u00b7ben", "von", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "PPER", "VVFIN", "APPR", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Sind alle so weit von hier.", "tokens": ["Sind", "al\u00b7le", "so", "weit", "von", "hier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "ADV", "ADJD", "APPR", "ADV", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}