{"dta.poem.23580": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Licht und Schatten.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1840", "urn": "urn:nbn:de:kobv:b4-200905192626", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Freilich, Luthers Zeiten hatten", "tokens": ["Frei\u00b7lich", ",", "Lu\u00b7thers", "Zei\u00b7ten", "hat\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ADV", "$,", "ADJA", "NN", "VAFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schatten mehr, viel mehr als Licht,", "tokens": ["Schat\u00b7ten", "mehr", ",", "viel", "mehr", "als", "Licht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "ADV", "ADV", "KOUS", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Und man lie\u00df der Welt den Schatten,", "tokens": ["Und", "man", "lie\u00df", "der", "Welt", "den", "Schat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Doch das Licht verbot man nicht.", "tokens": ["Doch", "das", "Licht", "ver\u00b7bot", "man", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PIS", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Zwar noch heut' ist frei der Schatten,", "tokens": ["Zwar", "noch", "heut'", "ist", "frei", "der", "Schat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "VAFIN", "ADJD", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Aber nicht des Lichtes Schein;", "tokens": ["A\u00b7ber", "nicht", "des", "Lich\u00b7tes", "Schein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Licht will man uns wohl verstatten,", "tokens": ["Licht", "will", "man", "uns", "wohl", "ver\u00b7stat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PIS", "PPER", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Doch zum Schattenspiel allein.", "tokens": ["Doch", "zum", "Schat\u00b7ten\u00b7spiel", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Jene finstern Zeiten kannten", "tokens": ["Je\u00b7ne", "fins\u00b7tern", "Zei\u00b7ten", "kann\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["PDAT", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Keine \u2014 \u2014 sche Censur:", "tokens": ["Kei\u00b7ne", "sche", "Cen\u00b7sur", ":"], "token_info": ["word", "punct", "punct", "word", "word", "punct"], "pos": ["PIAT", "$(", "$(", "ADJA", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Und ihr hellen Protestanten", "tokens": ["Und", "ihr", "hel\u00b7len", "Pro\u00b7tes\u00b7tan\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "R\u00fchmt euch geistiger Cultur?!", "tokens": ["R\u00fchmt", "euch", "geis\u00b7ti\u00b7ger", "Cul\u00b7tur", "?!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Lasst doch jedem seinen Schatten,", "tokens": ["Lasst", "doch", "je\u00b7dem", "sei\u00b7nen", "Schat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PIAT", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und sein Licht verwehrt ihm nicht;", "tokens": ["Und", "sein", "Licht", "ver\u00b7wehrt", "ihm", "nicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Lasst doch uns auch, was wir hatten,", "tokens": ["Lasst", "doch", "uns", "auch", ",", "was", "wir", "hat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPER", "ADV", "$,", "PRELS", "PPER", "VAFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Unsern Schatten, unser Licht!", "tokens": ["Un\u00b7sern", "Schat\u00b7ten", ",", "un\u00b7ser", "Licht", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Lasst doch uns in unsern Tagen", "tokens": ["Lasst", "doch", "uns", "in", "un\u00b7sern", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "PPER", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ihn den F\u00fcrsten finstrer Nacht", "tokens": ["Ihn", "den", "F\u00fcrs\u00b7ten", "finst\u00b7rer", "Nacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mit dem Dintenfa\u00df verjagen,", "tokens": ["Mit", "dem", "Din\u00b7ten\u00b7fa\u00df", "ver\u00b7ja\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie es Luther hat gemacht!", "tokens": ["Wie", "es", "Lu\u00b7ther", "hat", "ge\u00b7macht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "NE", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}