{"textgrid.poem.42441": {"metadata": {"author": {"name": "Liliencron, Detlev von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Eine gro\u00dfe Stadt mu\u00dft' ich durchgehn,", "genre": "verse", "period": "N.A.", "pub_year": 1876, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Eine gro\u00dfe Stadt mu\u00dft' ich durchgehn,", "tokens": ["Ei\u00b7ne", "gro\u00b7\u00dfe", "Stadt", "mu\u00dft'", "ich", "durch\u00b7gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Die seit Jahren ich nicht gesehn.", "tokens": ["Die", "seit", "Jah\u00b7ren", "ich", "nicht", "ge\u00b7sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "PPER", "PTKNEG", "VVPP", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Und in dieser auf meinen Wanderungen", "tokens": ["Und", "in", "die\u00b7ser", "auf", "mei\u00b7nen", "Wan\u00b7de\u00b7run\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PDAT", "APPR", "PPOSAT", "NN"], "meter": "-++--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Bin ich in einen Vorort gedrungen,", "tokens": ["Bin", "ich", "in", "ei\u00b7nen", "Vor\u00b7ort", "ge\u00b7drun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Wo in Armut hinfristen viel tausend Leute,", "tokens": ["Wo", "in", "Ar\u00b7mut", "hin\u00b7fris\u00b7ten", "viel", "tau\u00b7send", "Leu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "NN", "VVFIN", "ADV", "CARD", "NN", "$,"], "meter": "--+--+--+-+-", "measure": "anapaest.tri.plus"}, "line.6": {"text": "Und dort wie fr\u00fcher fand ich's heute.", "tokens": ["Und", "dort", "wie", "fr\u00fc\u00b7her", "fand", "ich's", "heu\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "KOKOM", "ADJD", "VVFIN", "PIS", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und mitten hier auf meiner Runde", "tokens": ["Und", "mit\u00b7ten", "hier", "auf", "mei\u00b7ner", "Run\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Vernahm ich vom n\u00e4chsten Turm die Stunde.", "tokens": ["Ver\u00b7nahm", "ich", "vom", "n\u00e4chs\u00b7ten", "Turm", "die", "Stun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Und wunderbar, wie der reicht\u00f6nende Klang", "tokens": ["Und", "wun\u00b7der\u00b7bar", ",", "wie", "der", "reich\u00b7t\u00f6\u00b7nen\u00b7de", "Klang"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "$,", "PWAV", "ART", "ADJA", "NN"], "meter": "-+---+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Mir pl\u00f6tzlich erinnernd die Brust durchdrang:", "tokens": ["Mir", "pl\u00f6tz\u00b7lich", "e\u00b7rin\u00b7nernd", "die", "Brust", "durch\u00b7drang", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVPP", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Vor mir stand eine Sommernacht,", "tokens": ["Vor", "mir", "stand", "ei\u00b7ne", "Som\u00b7mer\u00b7nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Die einst in diesem Revier ich durchwacht,", "tokens": ["Die", "einst", "in", "die\u00b7sem", "Re\u00b7vier", "ich", "durch\u00b7wacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "PDAT", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wo mir am Herzen ein M\u00e4del lag,", "tokens": ["Wo", "mir", "am", "Her\u00b7zen", "ein", "M\u00e4\u00b7del", "lag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPRART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Wo ich h\u00f6rte den sch\u00f6nen Glockenschlag,", "tokens": ["Wo", "ich", "h\u00f6r\u00b7te", "den", "sch\u00f6\u00b7nen", "Glo\u00b7cken\u00b7schlag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.7": {"text": "Ein Viertel, Halb, drei Viertel, Ganz,", "tokens": ["Ein", "Vier\u00b7tel", ",", "Halb", ",", "drei", "Vier\u00b7tel", ",", "Ganz", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "$,", "CARD", "NN", "$,", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Hoch \u00fcber der Menschen Mummenschanz.", "tokens": ["Hoch", "\u00fc\u00b7ber", "der", "Men\u00b7schen", "Mum\u00b7men\u00b7schanz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Im vierten Stock einer Mietskaserne,", "tokens": ["Im", "vier\u00b7ten", "Stock", "ei\u00b7ner", "Miets\u00b7ka\u00b7ser\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wo unten eine schlechte Taverne", "tokens": ["Wo", "un\u00b7ten", "ei\u00b7ne", "schlech\u00b7te", "Ta\u00b7ver\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Gesindel aufsog, wo die Unruhe wohnte,", "tokens": ["Ge\u00b7sin\u00b7del", "auf\u00b7sog", ",", "wo", "die", "Un\u00b7ru\u00b7he", "wohn\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Wo kein Engel die Tugend belohnte,", "tokens": ["Wo", "kein", "En\u00b7gel", "die", "Tu\u00b7gend", "be\u00b7lohn\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.5": {"text": "Da hab' ich einmal eine kurze Nacht", "tokens": ["Da", "hab'", "ich", "ein\u00b7mal", "ei\u00b7ne", "kur\u00b7ze", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "In Liebes\u00fcbersch\u00fcttung zugebracht.", "tokens": ["In", "Lie\u00b7be\u00b7s\u00fc\u00b7ber\u00b7sch\u00fct\u00b7tung", "zu\u00b7ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Sie schlief, und hat mich in Traumeswonnen", "tokens": ["Sie", "schlief", ",", "und", "hat", "mich", "in", "Trau\u00b7mes\u00b7won\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KON", "VAFIN", "PPER", "APPR", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Mit ihren wei\u00dfen Armen umsponnen,", "tokens": ["Mit", "ih\u00b7ren", "wei\u00b7\u00dfen", "Ar\u00b7men", "um\u00b7spon\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Hat oft mich im Halbschlaf fest an sich gedr\u00fcckt,", "tokens": ["Hat", "oft", "mich", "im", "Halb\u00b7schlaf", "fest", "an", "sich", "ge\u00b7dr\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPER", "APPRART", "NN", "PTKVZ", "APPR", "PRF", "VVPP", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "Das hat mich so grenzenlos entz\u00fcckt.", "tokens": ["Das", "hat", "mich", "so", "gren\u00b7zen\u00b7los", "ent\u00b7z\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ADV", "ADJD", "VVPP", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Sanft strich ich ihr braunes welliges Haar,", "tokens": ["Sanft", "strich", "ich", "ihr", "brau\u00b7nes", "wel\u00b7li\u00b7ges", "Haar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PPOSAT", "ADJA", "ADJA", "NN", "$,"], "meter": "+---+-+--+", "measure": "iambic.tetra.chol"}, "line.12": {"text": "Das schimmernd vom Monde beschienen war.", "tokens": ["Das", "schim\u00b7mernd", "vom", "Mon\u00b7de", "be\u00b7schie\u00b7nen", "war", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "APPRART", "NE", "VVPP", "VAFIN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.13": {"text": "Bis in's sp\u00e4te Morgenrot", "tokens": ["Bis", "in's", "sp\u00e4\u00b7te", "Mor\u00b7gen\u00b7rot"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.14": {"text": "L\u00e4rmt drau\u00dfen das Leben, schreit noch die Not.", "tokens": ["L\u00e4rmt", "drau\u00b7\u00dfen", "das", "Le\u00b7ben", ",", "schreit", "noch", "die", "Not", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$,", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.15": {"text": "Und Zank und Zorn, Geschrei, Gel\u00e4chter,", "tokens": ["Und", "Zank", "und", "Zorn", ",", "Ge\u00b7schrei", ",", "Ge\u00b7l\u00e4ch\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Einmal Dazwischenkommen der W\u00e4chter.", "tokens": ["Ein\u00b7mal", "Da\u00b7zwi\u00b7schen\u00b7kom\u00b7men", "der", "W\u00e4ch\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "ART", "NE", "$."], "meter": "-----+--+-", "measure": "iambic.di.relaxed"}, "line.17": {"text": "Von einem Tanzsaal her w\u00fcstes Gestampf,", "tokens": ["Von", "ei\u00b7nem", "Tanz\u00b7saal", "her", "w\u00fcs\u00b7tes", "Ge\u00b7stampf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APZR", "ADJA", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.18": {"text": "Aus der H\u00f6lle stieg auf ein greulicher Dampf", "tokens": ["Aus", "der", "H\u00f6l\u00b7le", "stieg", "auf", "ein", "greu\u00b7li\u00b7cher", "Dampf"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "--+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.19": {"text": "Aus Bierbudiken und Schnapsspelunken. \u2013", "tokens": ["Aus", "Bier\u00b7bu\u00b7di\u00b7ken", "und", "Schnapss\u00b7pe\u00b7lun\u00b7ken", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$.", "$("], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.20": {"text": "In diesem Dunst schien die Vorstadt ertrunken.", "tokens": ["In", "die\u00b7sem", "Dunst", "schien", "die", "Vor\u00b7stadt", "er\u00b7trun\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.21": {"text": "Klarweg \u00fcber die S\u00fcnde hindrang", "tokens": ["Klar\u00b7weg", "\u00fc\u00b7ber", "die", "S\u00fcn\u00b7de", "hin\u00b7drang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "VVFIN"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.22": {"text": "Der reine, der hehre Glockenklang.", "tokens": ["Der", "rei\u00b7ne", ",", "der", "heh\u00b7re", "Glo\u00b7cken\u00b7klang", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Endlich, nach jeder Weltstadt Weise,", "tokens": ["End\u00b7lich", ",", "nach", "je\u00b7der", "Welt\u00b7stadt", "Wei\u00b7se", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "APPR", "PIAT", "NN", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Ward' um die dritte Stund' es leise.", "tokens": ["Ward'", "um", "die", "drit\u00b7te", "Stund'", "es", "lei\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "ADJA", "NN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und herrlich durch die Stille drang", "tokens": ["Und", "herr\u00b7lich", "durch", "die", "Stil\u00b7le", "drang"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Immer wieder der sch\u00f6ne Glockenklang,", "tokens": ["Im\u00b7mer", "wie\u00b7der", "der", "sch\u00f6\u00b7ne", "Glo\u00b7cken\u00b7klang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.5": {"text": "Ein Viertel, Halb, drei Viertel, Ganz,", "tokens": ["Ein", "Vier\u00b7tel", ",", "Halb", ",", "drei", "Vier\u00b7tel", ",", "Ganz", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "$,", "CARD", "NN", "$,", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Hoch \u00fcber der Menschen Mummenschanz.", "tokens": ["Hoch", "\u00fc\u00b7ber", "der", "Men\u00b7schen", "Mum\u00b7men\u00b7schanz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Da \u00f6ffnet das M\u00e4del die Augen dem Tag,", "tokens": ["Da", "\u00f6ff\u00b7net", "das", "M\u00e4\u00b7del", "die", "Au\u00b7gen", "dem", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.8": {"text": "Und ich h\u00f6rte nicht mehr den Glockenschlag.", "tokens": ["Und", "ich", "h\u00f6r\u00b7te", "nicht", "mehr", "den", "Glo\u00b7cken\u00b7schlag", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PTKNEG", "ADV", "ART", "NN", "$."], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}}, "stanza.5": {"line.1": {"text": "Eine gro\u00dfe Stadt mu\u00dft' ich durchgehn,", "tokens": ["Ei\u00b7ne", "gro\u00b7\u00dfe", "Stadt", "mu\u00dft'", "ich", "durch\u00b7gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Die seit Jahren ich nicht gesehn.", "tokens": ["Die", "seit", "Jah\u00b7ren", "ich", "nicht", "ge\u00b7sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "PPER", "PTKNEG", "VVPP", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Und in dieser auf meinen Wanderungen", "tokens": ["Und", "in", "die\u00b7ser", "auf", "mei\u00b7nen", "Wan\u00b7de\u00b7run\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PDAT", "APPR", "PPOSAT", "NN"], "meter": "-++--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Bin ich in einen Vorort gedrungen,", "tokens": ["Bin", "ich", "in", "ei\u00b7nen", "Vor\u00b7ort", "ge\u00b7drun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Wo in Armut hinfristen viel tau