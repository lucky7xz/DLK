{"dta.poem.6863": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von dem Sitz der Sinnlichkeiten.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Beredet man sich selbst, da\u00df, da die Augen,", "tokens": ["Be\u00b7re\u00b7det", "man", "sich", "selbst", ",", "da\u00df", ",", "da", "die", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PRF", "ADV", "$,", "KOUS", "$,", "KOUS", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In solchem grossen Raum, solch\u2019 eine Menge Z\u00fcge,", "tokens": ["In", "sol\u00b7chem", "gros\u00b7sen", "Raum", ",", "solch'", "ei\u00b7ne", "Men\u00b7ge", "Z\u00fc\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "$,", "PIAT", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Dadurch sie ausgedruckt zu sehen taugen,", "tokens": ["Da\u00b7durch", "sie", "aus\u00b7ge\u00b7druckt", "zu", "se\u00b7hen", "tau\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "VVPP", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sich unsere Vernunfft betr\u00fcge;", "tokens": ["Sich", "un\u00b7se\u00b7re", "Ver\u00b7nunfft", "be\u00b7tr\u00fc\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Stimmt sie mit dieser Meinung ein:", "tokens": ["Stimmt", "sie", "mit", "die\u00b7ser", "Mei\u00b7nung", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PDAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df alle Ding\u2019 in uns beschlossen seyn.", "tokens": ["Da\u00df", "al\u00b7le", "Ding'", "in", "uns", "be\u00b7schlos\u00b7sen", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "APPR", "PPER", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}