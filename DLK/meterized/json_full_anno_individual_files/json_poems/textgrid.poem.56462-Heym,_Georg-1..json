{"textgrid.poem.56462": {"metadata": {"author": {"name": "Heym, Georg", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1899, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im Haar ein Nest von jungen Wasserratten,", "tokens": ["Im", "Haar", "ein", "Nest", "von", "jun\u00b7gen", "Was\u00b7ser\u00b7rat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und die beringten H\u00e4nde auf der Flut", "tokens": ["Und", "die", "be\u00b7ring\u00b7ten", "H\u00e4n\u00b7de", "auf", "der", "Flut"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wie Flossen, also treibt sie durch den Schatten", "tokens": ["Wie", "Flos\u00b7sen", ",", "al\u00b7so", "treibt", "sie", "durch", "den", "Schat\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "$,", "ADV", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Des gro\u00dfen Urwalds, der im Wasser ruht.", "tokens": ["Des", "gro\u00b7\u00dfen", "Ur\u00b7walds", ",", "der", "im", "Was\u00b7ser", "ruht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Die letzte Sonne, die im Dunkel irrt,", "tokens": ["Die", "letz\u00b7te", "Son\u00b7ne", ",", "die", "im", "Dun\u00b7kel", "irrt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word"