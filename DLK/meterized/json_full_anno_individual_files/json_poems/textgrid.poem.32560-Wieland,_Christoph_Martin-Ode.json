token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "$,", "APPR", "ART", "PPOSAT", "NN", "PRF", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "O wie sanft erquickt er meine Brust!", "tokens": ["O", "wie", "sanft", "er\u00b7quickt", "er", "mei\u00b7ne", "Brust", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "ADJD", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "O wie schwimmt Sie in \u00e4therscher Lust!", "tokens": ["O", "wie", "schwimmt", "Sie", "in", "\u00e4t\u00b7her\u00b7scher", "Lust", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Die mir reichlich Schmerz und Leid bezahlet.", "tokens": ["Die", "mir", "reich\u00b7lich", "Schmerz", "und", "Leid", "be\u00b7zah\u00b7let", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "NN", "KON", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.11": {"line.1": {"text": "Dich, Sophie, Dich gab der Himmel mir", "tokens": ["Dich", ",", "So\u00b7phie", ",", "Dich", "gab", "der", "Him\u00b7mel", "mir"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "$,", "NE", "$,", "PPER", "VVFIN", "ART", "NN", "PPER"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Mich der Tugend liebreich zuzuf\u00fchren;", "tokens": ["Mich", "der", "Tu\u00b7gend", "lieb\u00b7reich", "zu\u00b7zu\u00b7f\u00fch\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "ADJD", "VVIZU", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Ja, ich war bereit mich zu verlieren,", "tokens": ["Ja", ",", "ich", "war", "be\u00b7reit", "mich", "zu", "ver\u00b7lie\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VAFIN", "ADJD", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Gott! Du s\u00e4hest es, und gabst sie mir!", "tokens": ["Gott", "!", "Du", "s\u00e4\u00b7hest", "es", ",", "und", "gabst", "sie", "mir", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPER", "VVFIN", "PPER", "$,", "KON", "VVFIN", "PPER", "PPER", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Jetzo dring ich ", "tokens": ["Jet\u00b7zo", "dring", "ich"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Denn ihr redlich Herz verl\u00e4\u00dft mich nie;", "tokens": ["Denn", "ihr", "red\u00b7lich", "Herz", "ver\u00b7l\u00e4\u00dft", "mich", "nie", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADJD", "NN", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Gott und Weisheit Tugend und Sophie", "tokens": ["Gott", "und", "Weis\u00b7heit", "Tu\u00b7gend", "und", "So\u00b7phie"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "NN", "KON", "NE"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Sind bei mir, welch Unfall kann mich schrecken!", "tokens": ["Sind", "bei", "mir", ",", "welch", "Un\u00b7fall", "kann", "mich", "schre\u00b7cken", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPER", "$,", "PWAT", "NN", "VMFIN", "PRF", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.12": {"line.1": {"text": "O Mein Engel, wenn wird einst ein Tag", "tokens": ["O", "Mein", "En\u00b7gel", ",", "wenn", "wird", "einst", "ein", "Tag"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "PPOSAT", "NN", "$,", "KOUS", "VAFIN", "ADV", "ART", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Mich, Dir, ", "tokens": ["Mich", ",", "Dir", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["PPER", "$,", "PPER", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Und mein Gl\u00fcck auf solche Felsen bauen,", "tokens": ["Und", "mein", "Gl\u00fcck", "auf", "sol\u00b7che", "Fel\u00b7sen", "bau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "APPR", "PIAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Die kein Orkan nicht zertr\u00fcmmern mag?", "tokens": ["Die", "kein", "Or\u00b7kan", "nicht", "zer\u00b7tr\u00fcm\u00b7mern", "mag", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Denn bin ich begl\u00fcckt der Not entgangen,", "tokens": ["Denn", "bin", "ich", "be\u00b7gl\u00fcckt", "der", "Not", "ent\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "VVFIN", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Die des Weisen Auge oft benetzt;", "tokens": ["Die", "des", "Wei\u00b7sen", "Au\u00b7ge", "oft", "be\u00b7netzt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "NN", "ADV", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Denn wird nie ein Dunst von mir gesch\u00e4tzt,", "tokens": ["Denn", "wird", "nie", "ein", "Dunst", "von", "mir", "ge\u00b7sch\u00e4tzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ART", "NN", "APPR", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Denn die Tugend selbst h\u00e4lt mich in Dir umfangen!", "tokens": ["Denn", "die", "Tu\u00b7gend", "selbst", "h\u00e4lt", "mich", "in", "Dir", "um\u00b7fan\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "VVFIN", "PPER", "APPR", "PPER", "VVINF", "$."], "meter": "--+-++--+-+-", "measure": "iambic.penta.relaxed"}}}}}