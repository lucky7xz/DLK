{"textgrid.poem.32560": {"metadata": {"author": {"name": "Wieland, Christoph Martin", "birth": "N.A.", "death": "N.A."}, "title": "Ode", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Tugend! o wie reizend sch\u00f6n bist du!", "tokens": ["Tu\u00b7gend", "!", "o", "wie", "rei\u00b7zend", "sch\u00f6n", "bist", "du", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "FM", "KOKOM", "ADJD", "ADJD", "VAFIN", "PPER", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Himmelskind! ach kennten dich die Seelen", "tokens": ["Him\u00b7mel\u00b7skind", "!", "ach", "kenn\u00b7ten", "dich", "die", "See\u00b7len"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$.", "ADV", "VVFIN", "PPER", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Die vor dich ein gl\u00e4nzend Nichts sich w\u00e4hlen", "tokens": ["Die", "vor", "dich", "ein", "gl\u00e4n\u00b7zend", "Nichts", "sich", "w\u00e4h\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "PRF", "ART", "ADJD", "PIS", "PRF", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und erkaufen Schmerz um Seelen-Ruh!", "tokens": ["Und", "er\u00b7kau\u00b7fen", "Schmerz", "um", "See\u00b7len\u00b7Ruh", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "O wie w\u00fcrden sie die Stimme hassen,", "tokens": ["O", "wie", "w\u00fcr\u00b7den", "sie", "die", "Stim\u00b7me", "has\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "VAFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Die sie jetzt zu s\u00fc\u00dfem Elend ruft;", "tokens": ["Die", "sie", "jetzt", "zu", "s\u00fc\u00b7\u00dfem", "E\u00b7lend", "ruft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "O, wie fl\u00f6hen sie aus Circens Zaubergruft,", "tokens": ["O", ",", "wie", "fl\u00f6\u00b7hen", "sie", "aus", "Cir\u00b7cens", "Zau\u00b7ber\u00b7gruft", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "VVFIN", "PPER", "APPR", "NE", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.8": {"text": "Zu dir auf die K\u00f6nigliche Stra\u00dfen!", "tokens": ["Zu", "dir", "auf", "die", "K\u00f6\u00b7nig\u00b7li\u00b7che", "Stra\u00b7\u00dfen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Ach da\u00df doch ein schimmernd Nichts uns blendt!", "tokens": ["Ach", "da\u00df", "doch", "ein", "schim\u00b7mernd", "Nichts", "uns", "blendt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "KOUS", "ADV", "ART", "ADJD", "PIS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Da\u00df der Weise selbst, der Freund der Wahrheit,", "tokens": ["Da\u00df", "der", "Wei\u00b7se", "selbst", ",", "der", "Freund", "der", "Wahr\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "$,", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Oft, mit einem Geist voll heitrer Klarheit,", "tokens": ["Oft", ",", "mit", "ei\u00b7nem", "Geist", "voll", "hei\u00b7trer", "Klar\u00b7heit", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "APPR", "ART", "NN", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Wie bezaubert sich zum Scheingut wendt!", "tokens": ["Wie", "be\u00b7zau\u00b7bert", "sich", "zum", "Schein\u00b7gut", "wendt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PRF", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "O wie gl\u00fchen jetzt die ernsten Wangen", "tokens": ["O", "wie", "gl\u00fc\u00b7hen", "jetzt", "die", "erns\u00b7ten", "Wan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "KOKOM", "VVFIN", "ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Da zu sp\u00e4t ihn die Erfahrung lehrt", "tokens": ["Da", "zu", "sp\u00e4t", "ihn", "die", "Er\u00b7fah\u00b7rung", "lehrt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PTKA", "ADJD", "PPER", "ART", "NN", "VVFIN"], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.7": {"text": "Da\u00df Sein Arm, indem er dich begehrt,", "tokens": ["Da\u00df", "Sein", "Arm", ",", "in\u00b7dem", "er", "dich", "be\u00b7gehrt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "$,", "KOUS", "PPER", "PRF", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Wie Ixion, einen Dunst umfangen!", "tokens": ["Wie", "I\u00b7xion", ",", "ei\u00b7nen", "Dunst", "um\u00b7fan\u00b7gen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "$,", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Englische Sophie, mein Herz, mein Licht", "tokens": ["Eng\u00b7li\u00b7sche", "So\u00b7phie", ",", "mein", "Herz", ",", "mein", "Licht"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NE", "NE", "$,", "PPOSAT", "NN", "$,", "PPOSAT", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Du bist selbst, ja Du bist selbst die Tugend,", "tokens": ["Du", "bist", "selbst", ",", "ja", "Du", "bist", "selbst", "die", "Tu\u00b7gend", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "$,", "ADV", "PPER", "VAFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Aus der Anmut aufgebl\u00fchter Jugend,", "tokens": ["Aus", "der", "An\u00b7mut", "auf\u00b7ge\u00b7bl\u00fch\u00b7ter", "Ju\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Reizt sie selbst in Dir ein klug Gesicht.", "tokens": ["Reizt", "sie", "selbst", "in", "Dir", "ein", "klug", "Ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "PPER", "ART", "ADJD", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "O wie strahlt aus Deinen sch\u00f6nen Blicken,", "tokens": ["O", "wie", "strahlt", "aus", "Dei\u00b7nen", "sch\u00f6\u00b7nen", "Bli\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Wo mit weisem Ernst sich Anmut paart,", "tokens": ["Wo", "mit", "wei\u00b7sem", "Ernst", "sich", "An\u00b7mut", "paart", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ADJA", "NN", "PRF", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Eine Seele von Seraphscher Art,", "tokens": ["Ei\u00b7ne", "See\u00b7le", "von", "Se\u00b7raph\u00b7scher", "Art", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.8": {"text": "F\u00e4hig mehr als Weise zu entz\u00fccken!", "tokens": ["F\u00e4\u00b7hig", "mehr", "als", "Wei\u00b7se", "zu", "ent\u00b7z\u00fc\u00b7cken", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "KOUS", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Doch Dein Mund, Dein liebenswerter Mund,", "tokens": ["Doch", "Dein", "Mund", ",", "Dein", "lie\u00b7bens\u00b7wer\u00b7ter", "Mund", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Nicht nur sch\u00f6n, wenn ihn die K\u00fcsse schlie\u00dfen,", "tokens": ["Nicht", "nur", "sch\u00f6n", ",", "wenn", "ihn", "die", "K\u00fcs\u00b7se", "schlie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ADJD", "$,", "KOUS", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Auch wenn kluge Worte von Ihm flie\u00dfen,", "tokens": ["Auch", "wenn", "klu\u00b7ge", "Wor\u00b7te", "von", "Ihm", "flie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "ADJA", "NN", "APPR", "PPER", "VVINF", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Macht noch mehr als Deine Augen kund.", "tokens": ["Macht", "noch", "mehr", "als", "Dei\u00b7ne", "Au\u00b7gen", "kund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PIAT", "KOKOM", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Und Dein Brief, in dem Dein Herz sich malet", "tokens": ["Und", "Dein", "Brief", ",", "in", "dem", "Dein", "Herz", "sich", "ma\u00b7let"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "$,", "APPR", "ART", "PPOSAT", "NN", "PRF", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "O wie sanft erquickt er meine Brust!", "tokens": ["O", "wie", "sanft", "er\u00b7quickt", "er", "mei\u00b7ne", "Brust", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "ADJD", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "O wie schwimmt Sie in \u00e4therscher Lust!", "tokens": ["O", "wie", "schwimmt", "Sie", "in", "\u00e4t\u00b7her\u00b7scher", "Lust", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Die mir reichlich Schmerz und Leid bezahlet.", "tokens": ["Die", "mir", "reich\u00b7lich", "Schmerz", "und", "Leid", "be\u00b7zah\u00b7let", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "NN", "KON", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Dich, Sophie, Dich gab der Himmel mir", "tokens": ["Dich", ",", "So\u00b7phie", ",", "Dich", "gab", "der", "Him\u00b7mel", "mir"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "$,", "NE", "$,", "PPER", "VVFIN", "ART", "NN", "PPER"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Mich der Tugend liebreich zuzuf\u00fchren;", "tokens": ["Mich", "der", "Tu\u00b7gend", "lieb\u00b7reich", "zu\u00b7zu\u00b7f\u00fch\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "ADJD", "VVIZU", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Ja, ich war bereit mich zu verlieren,", "tokens": ["Ja", ",", "ich", "war", "be\u00b7reit", "mich", "zu", "ver\u00b7lie\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VAFIN", "ADJD", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Gott! Du s\u00e4hest es, und gabst sie mir!", "tokens": ["Gott", "!", "Du", "s\u00e4\u00b7hest", "es", ",", "und", "gabst", "sie", "mir", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPER", "VVFIN", "PPER", "$,", "KON", "VVFIN", "PPER", "PPER", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Jetzo dring ich ", "tokens": ["Jet\u00b7zo", "dring", "ich"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Denn ihr redlich Herz verl\u00e4\u00dft mich nie;", "tokens": ["Denn", "ihr", "red\u00b7lich", "Herz", "ver\u00b7l\u00e4\u00dft", "mich", "nie", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADJD", "NN", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Gott und Weisheit Tugend und Sophie", "tokens": ["Gott", "und", "Weis\u00b7heit", "Tu\u00b7gend", "und", "So\u00b7phie"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "NN", "KON", "NE"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Sind bei mir, welch Unfall kann mich schrecken!", "tokens": ["Sind", "bei", "mir", ",", "welch", "Un\u00b7fall", "kann", "mich", "schre\u00b7cken", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPER", "$,", "PWAT", "NN", "VMFIN", "PRF", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "O Mein Engel, wenn wird einst ein Tag", "tokens": ["O", "Mein", "En\u00b7gel", ",", "wenn", "wird", "einst", "ein", "Tag"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "PPOSAT", "NN", "$,", "KOUS", "VAFIN", "ADV", "ART", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Mich, Dir, ", "tokens": ["Mich", ",", "Dir", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["PPER", "$,", "PPER", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Und mein Gl\u00fcck auf solche Felsen bauen,", "tokens": ["Und", "mein", "Gl\u00fcck", "auf", "sol\u00b7che", "Fel\u00b7sen", "bau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "APPR", "PIAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Die kein Orkan nicht zertr\u00fcmmern mag?", "tokens": ["Die", "kein", "Or\u00b7kan", "nicht", "zer\u00b7tr\u00fcm\u00b7mern", "mag", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Denn bin ich begl\u00fcckt der Not entgangen,", "tokens": ["Denn", "bin", "ich", "be\u00b7gl\u00fcckt", "der", "Not", "ent\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "VVFIN", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Die des Weisen Auge oft benetzt;", "tokens": ["Die", "des", "Wei\u00b7sen", "Au\u00b7ge", "oft", "be\u00b7netzt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "NN", "ADV", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Denn wird nie ein Dunst von mir gesch\u00e4tzt,", "tokens": ["Denn", "wird", "nie", "ein", "Dunst", "von", "mir", "ge\u00b7sch\u00e4tzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ART", "NN", "APPR", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Denn die Tugend selbst h\u00e4lt mich in Dir umfangen!", "tokens": ["Denn", "die", "Tu\u00b7gend", "selbst", "h\u00e4lt", "mich", "in", "Dir", "um\u00b7fan\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "VVFIN", "PPER", "APPR", "PPER", "VVINF", "$."], "meter": "--+-++--+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.7": {"line.1": {"text": "Tugend! o wie reizend sch\u00f6n bist du!", "tokens": ["Tu\u00b7gend", "!", "o", "wie", "rei\u00b7zend", "sch\u00f6n", "bist", "du", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "FM", "KOKOM", "ADJD", "ADJD", "VAFIN", "PPER", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text"