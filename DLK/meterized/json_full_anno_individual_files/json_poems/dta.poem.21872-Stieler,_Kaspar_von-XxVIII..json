{"dta.poem.21872": {"metadata": {"author": {"name": "Stieler, Kaspar von", "birth": "N.A.", "death": "N.A."}, "title": "XxVIII.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1660", "urn": "urn:nbn:de:kobv:b4-20586-2", "language": ["de:0.99"], "booktitle": "Filidor der Dorfferer [i. e. Stieler, Kaspar von]: Die Geharnschte Venus. Hamburg, 1660."}, "poem": {"stanza.1": {"line.1": {"text": "Was mir in der N\u00e4he war/ kunnt die N\u00e4he", "tokens": ["Was", "mir", "in", "der", "N\u00e4\u00b7he", "war", "/", "kunnt", "die", "N\u00e4\u00b7he"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "PPER", "APPR", "ART", "NN", "VAFIN", "$(", "VVFIN", "ART", "NN"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "bi\u00df ich in die Ferne kahm/ da wurd\u2019 erst das", "tokens": ["bi\u00df", "ich", "in", "die", "Fer\u00b7ne", "kahm", "/", "da", "wurd'", "erst", "das"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "$(", "ADV", "VAFIN", "ADV", "ART"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "Nahe kunnt das Nahe nicht meinem Herzen", "tokens": ["Na\u00b7he", "kunnt", "das", "Na\u00b7he", "nicht", "mei\u00b7nem", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN", "PTKNEG", "PPOSAT", "NN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "O du s\u00fcsses Ferne du/ ewig werd\u2019 ich dein", "tokens": ["O", "du", "s\u00fcs\u00b7ses", "Fer\u00b7ne", "du", "/", "e\u00b7wig", "werd'", "ich", "dein"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NE", "ADJA", "NN", "PPER", "$(", "ADJD", "VAFIN", "PPER", "PPOSAT"], "meter": "+-+-+--+-+-+", "measure": "trochaic.hexa.relaxed"}}}}}