{"textgrid.poem.41163": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Geister aus noch tiefrer Nacht", "tokens": ["Geis\u00b7ter", "aus", "noch", "tief\u00b7rer", "Nacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ADV", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Hat das Tintenfa\u00df gebracht,", "tokens": ["Hat", "das", "Tin\u00b7ten\u00b7fa\u00df", "ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Als den Satz ich umger\u00fchrt.", "tokens": ["Als", "den", "Satz", "ich", "um\u00b7ge\u00b7r\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Niemals h\u00e4tt' ich den ber\u00fchrt,", "tokens": ["Nie\u00b7mals", "h\u00e4tt'", "ich", "den", "be\u00b7r\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "H\u00e4tt' ich eher schon erfahren,", "tokens": ["H\u00e4tt'", "ich", "e\u00b7her", "schon", "er\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wie so gro\u00df sind die Gefahren,", "tokens": ["Wie", "so", "gro\u00df", "sind", "die", "Ge\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADJD", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Wenn man mit dem Tintensatze,", "tokens": ["Wenn", "man", "mit", "dem", "Tin\u00b7ten\u00b7sat\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Vorab nachts, klecksographiert;", "tokens": ["Vor\u00b7ab", "nachts", ",", "kleck\u00b7so\u00b7gra\u00b7phiert", ";"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADV", "$,", "VVFIN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.9": {"text": "Dann erscheint oft eine Katze,", "tokens": ["Dann", "er\u00b7scheint", "oft", "ei\u00b7ne", "Kat\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Schneidend eine Teufelsfratze,", "tokens": ["Schnei\u00b7dend", "ei\u00b7ne", "Teu\u00b7fels\u00b7frat\u00b7ze", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Satan ist's der uns vexiert.", "tokens": ["Sa\u00b7tan", "ist's", "der", "uns", "ve\u00b7xiert", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "Oft den Kleksographen prellen", "tokens": ["Oft", "den", "Klek\u00b7so\u00b7gra\u00b7phen", "prel\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Schwarze Geister durchs Verstellen,", "tokens": ["Schwar\u00b7ze", "Geis\u00b7ter", "durchs", "Ver\u00b7stel\u00b7len", ","], "token_info": ["word", "word", "word"