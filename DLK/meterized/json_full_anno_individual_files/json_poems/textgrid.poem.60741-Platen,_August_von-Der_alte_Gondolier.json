{"textgrid.poem.60741": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "Der alte Gondolier", "genre": "verse", "period": "N.A.", "pub_year": 1815, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es sonnt sich auf den Stufen", "tokens": ["Es", "sonnt", "sich", "auf", "den", "Stu\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der seebesp\u00fclten Schwelle", "tokens": ["Der", "see\u00b7be\u00b7sp\u00fcl\u00b7ten", "Schwel\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ein Greis am Rand der Welle,", "tokens": ["Ein", "Greis", "am", "Rand", "der", "Wel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In wei\u00dfer Locken Zier:", "tokens": ["In", "wei\u00b7\u00dfer", "Lo\u00b7cken", "Zier", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und gerne steht dem Fremdling,", "tokens": ["Und", "ger\u00b7ne", "steht", "dem", "Fremd\u00b7ling", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der m\u00fc\u00dfig wandelt, Rede", "tokens": ["Der", "m\u00fc\u00b7\u00dfig", "wan\u00b7delt", ",", "Re\u00b7de"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["ART", "ADJD", "VVFIN", "$,", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Auf seiner Fragen jede", "tokens": ["Auf", "sei\u00b7ner", "Fra\u00b7gen", "je\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "PIAT"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Der alte Gondolier.", "tokens": ["Der", "al\u00b7te", "Gon\u00b7do\u00b7lier", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Er spricht: Ich habe r\u00fcstig", "tokens": ["Er", "spricht", ":", "Ich", "ha\u00b7be", "r\u00fcs\u00b7tig"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "PPER", "VAFIN", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Lagun und Meer befahren;", "tokens": ["La\u00b7gun", "und", "Meer", "be\u00b7fah\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Doch hab ich nun seit Jahren", "tokens": ["Doch", "hab", "ich", "nun", "seit", "Jah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "ADV", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Kein Ruder eingetaucht:", "tokens": ["Kein", "Ru\u00b7der", "ein\u00b7ge\u00b7taucht", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Es hangt die morsche Gondel", "tokens": ["Es", "hangt", "die", "mor\u00b7sche", "Gon\u00b7del"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "An Stricken in der Halle,", "tokens": ["An", "Stri\u00b7cken", "in", "der", "Hal\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Wo Alles im Verfalle,", "tokens": ["Wo", "Al\u00b7les", "im", "Ver\u00b7fal\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Wo Alles ungebraucht.", "tokens": ["Wo", "Al\u00b7les", "un\u00b7ge\u00b7braucht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Es ist der Herr des Hauses", "tokens": ["Es", "ist", "der", "Herr", "des", "Hau\u00b7ses"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Nach fernen Himmelstrichen", "tokens": ["Nach", "fer\u00b7nen", "Him\u00b7mel\u00b7stri\u00b7chen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Seit langer Zeit entwichen,", "tokens": ["Seit", "lan\u00b7ger", "Zeit", "ent\u00b7wi\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "F\u00fcr unsre Bitten taub;", "tokens": ["F\u00fcr", "uns\u00b7re", "Bit\u00b7ten", "taub", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der Gute zog von hinnen", "tokens": ["Der", "Gu\u00b7te", "zog", "von", "hin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Am Tag, als Bonaparte", "tokens": ["Am", "Tag", ",", "als", "Bo\u00b7na\u00b7par\u00b7te"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPRART", "NN", "$,", "KOUS", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Der Republik Standarte", "tokens": ["Der", "Re\u00b7pub\u00b7lik", "Stand\u00b7ar\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Lie\u00df werfen in den Staub.", "tokens": ["Lie\u00df", "wer\u00b7fen", "in", "den", "Staub", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Er stand in besten Jahren,", "tokens": ["Er", "stand", "in", "bes\u00b7ten", "Jah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Als er von uns geschieden;", "tokens": ["Als", "er", "von", "uns", "ge\u00b7schie\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Doch, lebt er noch hienieden,", "tokens": ["Doch", ",", "lebt", "er", "noch", "hien\u00b7ie\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VVFIN", "PPER", "ADV", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So ist's ein greiser Mann.", "tokens": ["So", "ist's", "ein", "grei\u00b7ser", "Mann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Er sprach: Und soll ich dienen,", "tokens": ["Er", "sprach", ":", "Und", "soll", "ich", "die\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "KON", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "So sei's in fremden L\u00e4ndern:", "tokens": ["So", "sei's", "in", "frem\u00b7den", "L\u00e4n\u00b7dern", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Hier soll mit Ordensb\u00e4ndern", "tokens": ["Hier", "soll", "mit", "Or\u00b7dens\u00b7b\u00e4n\u00b7dern"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Mich schm\u00fccken kein Tyrann!", "tokens": ["Mich", "schm\u00fc\u00b7cken", "kein", "Ty\u00b7rann", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Wir blieben, ach, und schauten,", "tokens": ["Wir", "blie\u00b7ben", ",", "ach", ",", "und", "schau\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ITJ", "$,", "KON", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wie Kirchenraub und Schande", "tokens": ["Wie", "Kir\u00b7chen\u00b7raub", "und", "Schan\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Beging die schn\u00f6de Bande", "tokens": ["Be\u00b7ging", "die", "schn\u00f6\u00b7de", "Ban\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nach schnellgebrochnem Eid!", "tokens": ["Nach", "schnell\u00b7ge\u00b7broch\u00b7nem", "Eid", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wir sahn, wie jene Wilden", "tokens": ["Wir", "sahn", ",", "wie", "je\u00b7ne", "Wil\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "PDAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Den Bucentaur zerschlugen,", "tokens": ["Den", "Bu\u00b7cen\u00b7taur", "zer\u00b7schlu\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und unsre Seelen trugen", "tokens": ["Und", "uns\u00b7re", "See\u00b7len", "tru\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ein unerh\u00f6rtes Leid!", "tokens": ["Ein", "un\u00b7er\u00b7h\u00f6r\u00b7tes", "Leid", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Wir sahn den Markusl\u00f6wen", "tokens": ["Wir", "sahn", "den", "Mar\u00b7kus\u00b7l\u00f6\u00b7wen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zum fernen Strand entf\u00fchren,", "tokens": ["Zum", "fer\u00b7nen", "Strand", "ent\u00b7f\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wir sahn, wie man mit Schw\u00fcren", "tokens": ["Wir", "sahn", ",", "wie", "man", "mit", "Schw\u00fc\u00b7ren"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "PIS", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und mit Besiegten scherzt!", "tokens": ["Und", "mit", "Be\u00b7sieg\u00b7ten", "scherzt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wir sahn zerst\u00f6rt von Frevlern,", "tokens": ["Wir", "sahn", "zer\u00b7st\u00f6rt", "von", "Frev\u00b7lern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "VVPP", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Was w\u00fcrdig schien der Dauer,", "tokens": ["Was", "w\u00fcr\u00b7dig", "schien", "der", "Dau\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Wir sahn an Tor und Mauer", "tokens": ["Wir", "sahn", "an", "Tor", "und", "Mau\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NE", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die Wappen ausgemerzt.", "tokens": ["Die", "Wap\u00b7pen", "aus\u00b7ge\u00b7merzt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Doch leb ich und betrachte", "tokens": ["Doch", "leb", "ich", "und", "be\u00b7trach\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "KON", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die teure Stadt noch immer,", "tokens": ["Die", "teu\u00b7re", "Stadt", "noch", "im\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "ADV", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Erquick im Morgenschimmer", "tokens": ["Er\u00b7quick", "im", "Mor\u00b7gen\u00b7schim\u00b7mer"], "token_info": ["word", "word", "word"], "pos": ["NN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Glieder schwach und alt.", "tokens": ["Die", "Glie\u00b7der", "schwach", "und", "alt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Von meines Herrn Palaste", "tokens": ["Von", "mei\u00b7nes", "Herrn", "Pa\u00b7las\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Vermocht ich nicht zu weichen,", "tokens": ["Ver\u00b7mocht", "ich", "nicht", "zu", "wei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Auch l\u00e4\u00dft er gern mir reichen", "tokens": ["Auch", "l\u00e4\u00dft", "er", "gern", "mir", "rei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Den kleinen Unterhalt.", "tokens": ["Den", "klei\u00b7nen", "Un\u00b7ter\u00b7halt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Da denk ich meiner Jugend,", "tokens": ["Da", "denk", "ich", "mei\u00b7ner", "Ju\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und wie ich als Matrose", "tokens": ["Und", "wie", "ich", "als", "Mat\u00b7ro\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PPER", "KOUS", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Gefolgt der Windesrose", "tokens": ["Ge\u00b7folgt", "der", "Win\u00b7des\u00b7ro\u00b7se"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Bei Sturm und Sonnenstrahl;", "tokens": ["Bei", "Sturm", "und", "Son\u00b7nen\u00b7strahl", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und wie blockierte Tunis", "tokens": ["Und", "wie", "blo\u00b7ckier\u00b7te", "Tu\u00b7nis"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PWAV", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und jene T\u00fcrkenrotte,", "tokens": ["Und", "je\u00b7ne", "T\u00fcr\u00b7ken\u00b7rot\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Mit seiner sch\u00f6nen Flotte,", "tokens": ["Mit", "sei\u00b7ner", "sch\u00f6\u00b7nen", "Flot\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Venedigs Admiral.", "tokens": ["Ve\u00b7ne\u00b7digs", "Ad\u00b7mi\u00b7ral", "."], "token_info": ["word", "word", "punct"], "pos": ["NE", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.9": {"line.1": {"text": "O holder Tag, als Emos", "tokens": ["O", "hol\u00b7der", "Tag", ",", "als", "E\u00b7mos"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NE", "ADJA", "NN", "$,", "KOUS", "NE"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Heimzug die Fluten teilte,", "tokens": ["Heim\u00b7zug", "die", "Flu\u00b7ten", "teil\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVFIN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Und ihm entgegen eilte", "tokens": ["Und", "ihm", "ent\u00b7ge\u00b7gen", "eil\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPER", "PTKVZ", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Doge Paul Renier!", "tokens": ["Der", "Do\u00b7ge", "Paul", "Re\u00b7nier", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Gedenk ich jener Zeiten,", "tokens": ["Ge\u00b7denk", "ich", "je\u00b7ner", "Zei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PDAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wird meine Seele milder:", "tokens": ["Wird", "mei\u00b7ne", "See\u00b7le", "mil\u00b7der", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Es fliegen jene Bilder", "tokens": ["Es", "flie\u00b7gen", "je\u00b7ne", "Bil\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PDAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Wie Engel um mich her!", "tokens": ["Wie", "En\u00b7gel", "um", "mich", "her", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Es sonnt sich auf den Stufen", "tokens": ["Es", "sonnt", "sich", "auf", "den", "Stu\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der seebesp\u00fclten Schwelle", "tokens": ["Der", "see\u00b7be\u00b7sp\u00fcl\u00b7ten", "Schwel\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ein Greis am Rand der Welle,", "tokens": ["Ein", "Greis", "am", "Rand", "der", "Wel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In wei\u00dfer Locken Zier:", "tokens": ["In", "wei\u00b7\u00dfer", "Lo\u00b7cken", "Zier", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und gerne steht dem Fremdling,", "tokens": ["Und", "ger\u00b7ne", "steht", "dem", "Fremd\u00b7ling", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der m\u00fc\u00dfig wandelt, Rede", "tokens": ["Der", "m\u00fc\u00b7\u00dfig", "wan\u00b7delt", ",", "Re\u00b7de"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["ART", "ADJD", "VVFIN", "$,", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Auf seiner Fragen jede", "tokens": ["Auf", "sei\u00b7ner", "Fra\u00b7gen", "je\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "PIAT"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Der alte Gondolier.", "tokens": ["Der", "al\u00b7te", "Gon\u00b7do\u00b7lier", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Er spricht: Ich habe r\u00fcstig", "tokens": ["Er", "spricht", ":", "Ich", "ha\u00b7be", "r\u00fcs\u00b7tig"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "PPER", "VAFIN", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Lagun und Meer befahren;", "tokens": ["La\u00b7gun", "und", "Meer", "be\u00b7fah\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Doch hab ich nun seit Jahren", "tokens": ["Doch", "hab", "ich", "nun", "seit", "Jah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "ADV", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Kein Ruder eingetaucht:", "tokens": ["Kein", "Ru\u00b7der", "ein\u00b7ge\u00b7taucht", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Es hangt die morsche Gondel", "tokens": ["Es", "hangt", "die", "mor\u00b7sche", "Gon\u00b7del"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "An Stricken in der Halle,", "tokens": ["An", "Stri\u00b7cken", "in", "der", "Hal\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Wo Alles im Verfalle,", "tokens": ["Wo", "Al\u00b7les", "im", "Ver\u00b7fal\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Wo Alles ungebraucht.", "tokens": ["Wo", "Al\u00b7les", "un\u00b7ge\u00b7braucht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Es ist der Herr des Hauses", "tokens": ["Es", "ist", "der", "Herr", "des", "Hau\u00b7ses"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Nach fernen Himmelstrichen", "tokens": ["Nach", "fer\u00b7nen", "Him\u00b7mel\u00b7stri\u00b7chen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Seit langer Zeit entwichen,", "tokens": ["Seit", "lan\u00b7ger", "Zeit", "ent\u00b7wi\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "F\u00fcr unsre Bitten taub;", "tokens": ["F\u00fcr", "uns\u00b7re", "Bit\u00b7ten", "taub", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der Gute zog von hinnen", "tokens": ["Der", "Gu\u00b7te", "zog", "von", "hin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Am Tag, als Bonaparte", "tokens": ["Am", "Tag", ",", "als", "Bo\u00b7na\u00b7par\u00b7te"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPRART", "NN", "$,", "KOUS", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Der Republik Standarte", "tokens": ["Der", "Re\u00b7pub\u00b7lik", "Stand\u00b7ar\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Lie\u00df werfen in den Staub.", "tokens": ["Lie\u00df", "wer\u00b7fen", "in", "den", "Staub", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Er stand in besten Jahren,", "tokens": ["Er", "stand", "in", "bes\u00b7ten", "Jah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Als er von uns geschieden;", "tokens": ["Als", "er", "von", "uns", "ge\u00b7schie\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Doch, lebt er noch hienieden,", "tokens": ["Doch", ",", "lebt", "er", "noch", "hien\u00b7ie\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VVFIN", "PPER", "ADV", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So ist's ein greiser Mann.", "tokens": ["So", "ist's", "ein", "grei\u00b7ser", "Mann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Er sprach: Und soll ich dienen,", "tokens": ["Er", "sprach", ":", "Und", "soll", "ich", "die\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "KON", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "So sei's in fremden L\u00e4ndern:", "tokens": ["So", "sei's", "in", "frem\u00b7den", "L\u00e4n\u00b7dern", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Hier soll mit Ordensb\u00e4ndern", "tokens": ["Hier", "soll", "mit", "Or\u00b7dens\u00b7b\u00e4n\u00b7dern"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Mich schm\u00fccken kein Tyrann!", "tokens": ["Mich", "schm\u00fc\u00b7cken", "kein", "Ty\u00b7rann", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Wir blieben, ach, und schauten,", "tokens": ["Wir", "blie\u00b7ben", ",", "ach", ",", "und", "schau\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ITJ", "$,", "KON", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wie Kirchenraub und Schande", "tokens": ["Wie", "Kir\u00b7chen\u00b7raub", "und", "Schan\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Beging die schn\u00f6de Bande", "tokens": ["Be\u00b7ging", "die", "schn\u00f6\u00b7de", "Ban\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nach schnellgebrochnem Eid!", "tokens": ["Nach", "schnell\u00b7ge\u00b7broch\u00b7nem", "Eid", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wir sahn, wie jene Wilden", "tokens": ["Wir", "sahn", ",", "wie", "je\u00b7ne", "Wil\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "PDAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Den Bucentaur zerschlugen,", "tokens": ["Den", "Bu\u00b7cen\u00b7taur", "zer\u00b7schlu\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und unsre Seelen trugen", "tokens": ["Und", "uns\u00b7re", "See\u00b7len", "tru\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ein unerh\u00f6rtes Leid!", "tokens": ["Ein", "un\u00b7er\u00b7h\u00f6r\u00b7tes", "Leid", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "Wir sahn den Markusl\u00f6wen", "tokens": ["Wir", "sahn", "den", "Mar\u00b7kus\u00b7l\u00f6\u00b7wen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zum fernen Strand entf\u00fchren,", "tokens": ["Zum", "fer\u00b7nen", "Strand", "ent\u00b7f\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wir sahn, wie man mit Schw\u00fcren", "tokens": ["Wir", "sahn", ",", "wie", "man", "mit", "Schw\u00fc\u00b7ren"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "PIS", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und mit Besiegten scherzt!", "tokens": ["Und", "mit", "Be\u00b7sieg\u00b7ten", "scherzt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wir sahn zerst\u00f6rt von Frevlern,", "tokens": ["Wir", "sahn", "zer\u00b7st\u00f6rt", "von", "Frev\u00b7lern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "VVPP", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Was w\u00fcrdig schien der Dauer,", "tokens": ["Was", "w\u00fcr\u00b7dig", "schien", "der", "Dau\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Wir sahn an Tor und Mauer", "tokens": ["Wir", "sahn", "an", "Tor", "und", "Mau\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NE", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die Wappen ausgemerzt.", "tokens": ["Die", "Wap\u00b7pen", "aus\u00b7ge\u00b7merzt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.16": {"line.1": {"text": "Doch leb ich und betrachte", "tokens": ["Doch", "leb", "ich", "und", "be\u00b7trach\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "KON", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die teure Stadt noch immer,", "tokens": ["Die", "teu\u00b7re", "Stadt", "noch", "im\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "ADV", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Erquick im Morgenschimmer", "tokens": ["Er\u00b7quick", "im", "Mor\u00b7gen\u00b7schim\u00b7mer"], "token_info": ["word", "word", "word"], "pos": ["NN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Glieder schwach und alt.", "tokens": ["Die", "Glie\u00b7der", "schwach", "und", "alt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Von meines Herrn Palaste", "tokens": ["Von", "mei\u0