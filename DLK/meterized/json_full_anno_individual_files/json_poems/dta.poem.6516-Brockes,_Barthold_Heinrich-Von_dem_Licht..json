{"dta.poem.6516": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von dem Licht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die C\u00f6rper, so das Licht in unser Auge bringen,", "tokens": ["Die", "C\u00f6r\u00b7per", ",", "so", "das", "Licht", "in", "un\u00b7ser", "Au\u00b7ge", "brin\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "ART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sind \u00fcberall im gantzen Himmels-Creise,", "tokens": ["Sind", "\u00fc\u00b7be\u00b7rall", "im", "gant\u00b7zen", "Him\u00b7mels\u00b7Crei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Um sich darinn auf-und von allen Seiten", "tokens": ["Um", "sich", "da\u00b7rinn", "auf\u00b7\u00b7und", "von", "al\u00b7len", "Sei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "PRF", "PAV", "ADJD", "APPR", "PIAT", "NN"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Zu regen und sich auszubreiten.", "tokens": ["Zu", "re\u00b7gen", "und", "sich", "aus\u00b7zu\u00b7brei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "KON", "PRF", "VVIZU", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die Sonne hebt kaum an, dem Schein nach, ihre Reise,", "tokens": ["Die", "Son\u00b7ne", "hebt", "kaum", "an", ",", "dem", "Schein", "nach", ",", "ih\u00b7re", "Rei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "PTKVZ", "$,", "ART", "NN", "PTKVZ", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So sieht man sie, bewegt, sich allenthalben schwingen.", "tokens": ["So", "sieht", "man", "sie", ",", "be\u00b7wegt", ",", "sich", "al\u00b7len\u00b7thal\u00b7ben", "schwin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PPER", "$,", "VVFIN", "$,", "PRF", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Sie sind an jedem Ort\u2019, auf gleiche Weise", "tokens": ["Sie", "sind", "an", "je\u00b7dem", "Ort'", ",", "auf", "glei\u00b7che", "Wei\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "PIAT", "NN", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Getrieben und gedr\u00fcckt.", "tokens": ["Ge\u00b7trie\u00b7ben", "und", "ge\u00b7dr\u00fcckt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "KON", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "In einem Augenblick, im kleinsten Theil der Zeit,", "tokens": ["In", "ei\u00b7nem", "Au\u00b7gen\u00b7blick", ",", "im", "kleins\u00b7ten", "Theil", "der", "Zeit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPRART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Gl\u00e4ntzt alles wunderbar geschm\u00fcckt,", "tokens": ["Gl\u00e4ntzt", "al\u00b7les", "wun\u00b7der\u00b7bar", "ge\u00b7schm\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "In einem reinen Glantz in klarer Heiterkeit.", "tokens": ["In", "ei\u00b7nem", "rei\u00b7nen", "Glantz", "in", "kla\u00b7rer", "Hei\u00b7ter\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}