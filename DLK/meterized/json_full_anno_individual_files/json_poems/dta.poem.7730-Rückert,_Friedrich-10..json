{"dta.poem.7730": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "10.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Den heil'gen Weda willst du lesen mit Ersprie\u00dfen?", "tokens": ["Den", "heil'\u00b7gen", "We\u00b7da", "willst", "du", "le\u00b7sen", "mit", "Er\u00b7sprie\u00b7\u00dfen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "PPER", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So jeder St\u00f6rung mu\u00dft den Zugang du verschlie\u00dfen:", "tokens": ["So", "je\u00b7der", "St\u00f6\u00b7rung", "mu\u00dft", "den", "Zu\u00b7gang", "du", "ver\u00b7schlie\u00b7\u00dfen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "VMFIN", "ART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "An einem reinen Ort sollst du den Sitz aufschlagen,", "tokens": ["An", "ei\u00b7nem", "rei\u00b7nen", "Ort", "sollst", "du", "den", "Sitz", "auf\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VMFIN", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wo fromme Blumen bl\u00fchn und stille B\u00e4ume ragen;", "tokens": ["Wo", "from\u00b7me", "Blu\u00b7men", "bl\u00fchn", "und", "stil\u00b7le", "B\u00e4u\u00b7me", "ra\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "VVINF", "KON", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wo klare Wasser gehn, doch die nicht wallend brausen,", "tokens": ["Wo", "kla\u00b7re", "Was\u00b7ser", "gehn", ",", "doch", "die", "nicht", "wal\u00b7lend", "brau\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "VVINF", "$,", "ADV", "ART", "PTKNEG", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wo frische L\u00fcfte wehn, doch die nicht st\u00fcrmend sausen.", "tokens": ["Wo", "fri\u00b7sche", "L\u00fcf\u00b7te", "wehn", ",", "doch", "die", "nicht", "st\u00fcr\u00b7mend", "sau\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "VVINF", "$,", "ADV", "ART", "PTKNEG", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Kein greller Vogelschall, kein thierisches Gest\u00f6hne,", "tokens": ["Kein", "grel\u00b7ler", "Vo\u00b7gel\u00b7schall", ",", "kein", "thie\u00b7ri\u00b7sches", "Ge\u00b7st\u00f6h\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$,", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Kein lauter Widerhall, kein menschliches Get\u00f6ne;", "tokens": ["Kein", "lau\u00b7ter", "Wi\u00b7der\u00b7hall", ",", "kein", "menschli\u00b7ches", "Ge\u00b7t\u00f6\u00b7ne", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "PIAT", "NN", "$,", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.5": {"line.1": {"text": "Solang du lesest, sei die Luft im Gleichgewicht;", "tokens": ["So\u00b7lang", "du", "le\u00b7sest", ",", "sei", "die", "Luft", "im", "Gleich\u00b7ge\u00b7wicht", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVFIN", "$,", "VAFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "H\u00f6r' auf zu lesen gleich, sobald der Donner spricht,", "tokens": ["H\u00f6r'", "auf", "zu", "le\u00b7sen", "gleich", ",", "so\u00b7bald", "der", "Don\u00b7ner", "spricht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PTKZU", "VVINF", "ADV", "$,", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Sobald der Regen rauscht, sobald der Sturm sich regt,", "tokens": ["So\u00b7bald", "der", "Re\u00b7gen", "rauscht", ",", "so\u00b7bald", "der", "Sturm", "sich", "regt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "$,", "KOUS", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sobald das Licht, bei dem du wachst, der Wind bewegt.", "tokens": ["So\u00b7bald", "das", "Licht", ",", "bei", "dem", "du", "wachst", ",", "der", "Wind", "be\u00b7wegt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,", "APPR", "PRELS", "PPER", "VVFIN", "$,", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Nur wo des Fl\u00e4mmchens unbewegte Spitze brennt,", "tokens": ["Nur", "wo", "des", "Fl\u00e4mm\u00b7chens", "un\u00b7be\u00b7weg\u00b7te", "Spit\u00b7ze", "brennt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWAV", "ART", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da ist der Andacht, der Vertiefung Element.", "tokens": ["Da", "ist", "der", "An\u00b7dacht", ",", "der", "Ver\u00b7tie\u00b7fung", "E\u00b7le\u00b7ment", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "$,", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Vom feuchten Dochte kehrt der Lichtblick sich nach oben;", "tokens": ["Vom", "feuch\u00b7ten", "Doch\u00b7te", "kehrt", "der", "Licht\u00b7blick", "sich", "nach", "o\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "ART", "NN", "PRF", "APPR", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So f\u00fchlt sich das Gem\u00fcth dem Irdischen enthoben.", "tokens": ["So", "f\u00fchlt", "sich", "das", "Ge\u00b7m\u00fcth", "dem", "Ir\u00b7di\u00b7schen", "ent\u00b7ho\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Doch wo Natur f\u00fcrs Ohr laut Gottes Lob anstimmt,", "tokens": ["Doch", "wo", "Na\u00b7tur", "f\u00fcrs", "Ohr", "laut", "Got\u00b7tes", "Lob", "an\u00b7stimmt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "NN", "APPRART", "NN", "APPR", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da schweigt der Geist der Schrift, den nur der Geist vernimmt.", "tokens": ["Da", "schweigt", "der", "Geist", "der", "Schrift", ",", "den", "nur", "der", "Geist", "ver\u00b7nimmt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "$,", "PRELS", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}