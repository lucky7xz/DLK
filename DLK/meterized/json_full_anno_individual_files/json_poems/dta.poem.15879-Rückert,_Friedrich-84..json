{"dta.poem.15879": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "84.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Str\u00f6me liefen all gerades Wegs ins Meer,", "tokens": ["Die", "Str\u00f6\u00b7me", "lie\u00b7fen", "all", "ge\u00b7ra\u00b7des", "Wegs", "ins", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIAT", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn sich die Berge nicht vorstreckten \u00fcberqueer.", "tokens": ["Wenn", "sich", "die", "Ber\u00b7ge", "nicht", "vor\u00b7streck\u00b7ten", "\u00fc\u00b7berq\u00b7ue\u00b7er", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "PTKNEG", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Den Bergen m\u00fc\u00dfen sie anschmiegend sich bequemen,", "tokens": ["Den", "Ber\u00b7gen", "m\u00fc\u00b7\u00dfen", "sie", "an\u00b7schmie\u00b7gend", "sich", "be\u00b7que\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "ADJD", "PRF", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und ihren Lauf zum Meer durch manchen Umweg nehmen.", "tokens": ["Und", "ih\u00b7ren", "Lauf", "zum", "Meer", "durch", "man\u00b7chen", "Um\u00b7weg", "neh\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "APPRART", "NN", "APPR", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Die Berge halten sie am Ende doch nicht auf,", "tokens": ["Die", "Ber\u00b7ge", "hal\u00b7ten", "sie", "am", "En\u00b7de", "doch", "nicht", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPRART", "NN", "ADV", "PTKNEG", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und reicher wird dadurch ihr sch\u00f6ngewundner Lauf.", "tokens": ["Und", "rei\u00b7cher", "wird", "da\u00b7durch", "ihr", "sch\u00f6n\u00b7ge\u00b7wund\u00b7ner", "Lauf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "PAV", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Dein Leben ist ein Strom: o la\u00df dichs nicht verdrie\u00dfen,", "tokens": ["Dein", "Le\u00b7ben", "ist", "ein", "Strom", ":", "o", "la\u00df", "dichs", "nicht", "ver\u00b7drie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "NN", "$.", "FM", "FM", "FM", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Durch manchen Berg gehemmt, dem Meere zuzuflie\u00dfen.", "tokens": ["Durch", "man\u00b7chen", "Berg", "ge\u00b7hemmt", ",", "dem", "Mee\u00b7re", "zu\u00b7zu\u00b7flie\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVPP", "$,", "ART", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}