{"textgrid.poem.61443": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "O du, vor dem die St\u00fcrme schweigen", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O du, vor dem die St\u00fcrme schweigen,", "tokens": ["O", "du", ",", "vor", "dem", "die", "St\u00fcr\u00b7me", "schwei\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "APPR", "ART", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vor dem das Meer versinkt in Ruh',", "tokens": ["Vor", "dem", "das", "Meer", "ver\u00b7sinkt", "in", "Ruh'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dies wilde Herz nimm hin zu eigen", "tokens": ["Dies", "wil\u00b7de", "Herz", "nimm", "hin", "zu", "ei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "ADJA", "NN", "VVIMP", "ADV", "PTKA", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und f\u00fchr' es deinem Frieden zu:", "tokens": ["Und", "f\u00fchr'", "es", "dei\u00b7nem", "Frie\u00b7den", "zu", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dies Herz, das, ewig umgetrieben,", "tokens": ["Dies", "Herz", ",", "das", ",", "e\u00b7wig", "um\u00b7ge\u00b7trie\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "NN", "$,", "PDS", "$,", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Entlodert allzu rasch entfacht", "tokens": ["Ent\u00b7lo\u00b7dert", "all\u00b7zu", "rasch", "ent\u00b7facht"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PTKA", "ADJD", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und, ach, mit seinem irren Lieben", "tokens": ["Und", ",", "ach", ",", "mit", "sei\u00b7nem", "ir\u00b7ren", "Lie\u00b7ben"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "$,", "ITJ", "$,", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Sich selbst und andre elend macht.", "tokens": ["Sich", "selbst", "und", "and\u00b7re", "e\u00b7lend", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "KON", "PIS", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Entrei\u00df es, Herr, dem Sturm der Sinne,", "tokens": ["Ent\u00b7rei\u00df", "es", ",", "Herr", ",", "dem", "Sturm", "der", "Sin\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "NN", "$,", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der W\u00fcnsche treulos schwankem Spiel;", "tokens": ["Der", "W\u00fcn\u00b7sche", "treu\u00b7los", "schwan\u00b7kem", "Spiel", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dem dunkeln Drange seiner Minne,", "tokens": ["Dem", "dun\u00b7keln", "Dran\u00b7ge", "sei\u00b7ner", "Min\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "