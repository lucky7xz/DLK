{"textgrid.poem.32709": {"metadata": {"author": {"name": "Miller, Johann Martin", "birth": "N.A.", "death": "N.A."}, "title": "Baurenlied", "genre": "verse", "period": "N.A.", "pub_year": 1772, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie bin ich sonst so sorgenfrei", "tokens": ["Wie", "bin", "ich", "sonst", "so", "sor\u00b7gen\u00b7frei"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "ADV", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Durchs Leben hingeschlendert;", "tokens": ["Durchs", "Le\u00b7ben", "hin\u00b7ge\u00b7schlen\u00b7dert", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Nun f\u00fchl' ich seit dem ersten Mai", "tokens": ["Nun", "f\u00fchl'", "ich", "seit", "dem", "ers\u00b7ten", "Mai"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mich ganz und gar ver\u00e4ndert.", "tokens": ["Mich", "ganz", "und", "gar", "ver\u00b7\u00e4n\u00b7dert", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "KON", "ADV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Dies, liebes R\u00f6schen, k\u00f6mmt von dir;", "tokens": ["Dies", ",", "lie\u00b7bes", "R\u00f6\u00b7schen", ",", "k\u00f6mmt", "von", "dir", ";"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "ADJA", "NN", "$,", "VVFIN", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich kann dir's nicht verhehlen;", "tokens": ["Ich", "kann", "dir's", "nicht", "ver\u00b7heh\u00b7len", ";"], "to