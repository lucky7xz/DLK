{"textgrid.poem.36518": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "17. Das deutsche Pferd und das brittische", "genre": "verse", "period": "N.A.", "pub_year": 1761, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein deutsches Pferd, ein Friese, stand", "tokens": ["Ein", "deut\u00b7sches", "Pferd", ",", "ein", "Frie\u00b7se", ",", "stand"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "$,", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nicht weit von einem Engell\u00e4nder,", "tokens": ["Nicht", "weit", "von", "ei\u00b7nem", "En\u00b7gel\u00b7l\u00e4n\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Verglich sich, fand", "tokens": ["Ver\u00b7glich", "sich", ",", "fand"], "token_info": ["word", "word", "punct", "word"], "pos": ["ADJD", "PRF", "$,", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Den fremden Herrn behender.", "tokens": ["Den", "frem\u00b7den", "Herrn", "be\u00b7hen\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Willst du gestutzt sein? fragt der Herr, dem er geh\u00f6rt.", "tokens": ["Willst", "du", "ge\u00b7stutzt", "sein", "?", "fragt", "der", "Herr", ",", "dem", "er", "ge\u00b7h\u00f6rt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVPP", "VAINF", "$.", "VVFIN", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ja, spricht der Friese, ja! ich werde", "tokens": ["Ja", ",", "spricht", "der", "Frie\u00b7se", ",", "ja", "!", "ich", "wer\u00b7de"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PTKANT", "$,", "VVFIN", "ART", "NN", "$,", "ADV", "$.", "PPER", "VAFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Dann laufen, wie der Vogel fliegt, die Erde", "tokens": ["Dann", "lau\u00b7fen", ",", "wie", "der", "Vo\u00b7gel", "fliegt", ",", "die", "Er\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVINF", "$,", "PWAV", "ART", "NE", "VVFIN", "$,", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Fliegt unter mir!", "tokens": ["Fliegt", "un\u00b7ter", "mir", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Hast aber keinen Schweif, der dir", "tokens": ["Hast", "a\u00b7ber", "kei\u00b7nen", "Schweif", ",", "der", "dir"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["VAFIN", "ADV", "PIAT", "NN", "$,", "PRELS", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Fliegen wehrt!", "tokens": ["Die", "Flie\u00b7gen", "wehrt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Den Fliegen will ich wohl entlaufen!", "tokens": ["Den", "Flie\u00b7gen", "will", "ich", "wohl", "ent\u00b7lau\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Sein Wunsch wird endlich ihm gew\u00e4hrt,", "tokens": ["Sein", "Wunsch", "wird", "end\u00b7lich", "ihm", "ge\u00b7w\u00e4hrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er wird gestutzt, er d\u00fcnkt sich, unter einem Haufen", "tokens": ["Er", "wird", "ge\u00b7stutzt", ",", "er", "d\u00fcnkt", "sich", ",", "un\u00b7ter", "ei\u00b7nem", "Hau\u00b7fen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "VVPP", "$,", "PPER", "VVFIN", "PRF", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Von Pferden, nun das sch\u00f6nste Pferd!", "tokens": ["Von", "Pfer\u00b7den", ",", "nun", "das", "sch\u00f6ns\u00b7te", "Pferd", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Den Engell\u00e4nder her! ruft nun der Herr,", "tokens": ["Den", "En\u00b7gel\u00b7l\u00e4n\u00b7der", "her", "!", "ruft", "nun", "der", "Herr", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$.", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Er soll einmal die Probe laufen.", "tokens": ["Er", "soll", "ein\u00b7mal", "die", "Pro\u00b7be", "lau\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Er l\u00e4uft, er st\u00fcrzt, er kann nicht mehr;", "tokens": ["Er", "l\u00e4uft", ",", "er", "st\u00fcrzt", ",", "er", "kann", "nicht", "mehr", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "$,", "PPER", "VMFIN", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Stechfliegen qu\u00e4len ihn; der Herr will ihn verkaufen.", "tokens": ["Stech\u00b7flie\u00b7gen", "qu\u00e4\u00b7len", "ihn", ";", "der", "Herr", "will", "ihn", "ver\u00b7kau\u00b7fen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$.", "ART", "NN", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Man bietet nichts f\u00fcr ihn; der Abgestutzte kann", "tokens": ["Man", "bie\u00b7tet", "nichts", "f\u00fcr", "ihn", ";", "der", "Ab\u00b7ge\u00b7stutz\u00b7te", "kann"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PIS", "VVFIN", "PIS", "APPR", "PPER", "$.", "ART", "NN", "VMFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der b\u00f6sen Fliegen sich nicht wehren,", "tokens": ["Der", "b\u00f6\u00b7sen", "Flie\u00b7gen", "sich", "nicht", "weh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PRF", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Nicht seinen Herrn ern\u00e4hren,", "tokens": ["Nicht", "sei\u00b7nen", "Herrn", "er\u00b7n\u00e4h\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Man sieht ihm seinen Kummer an.", "tokens": ["Man", "sieht", "ihm", "sei\u00b7nen", "Kum\u00b7mer", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Ach, spricht das Pferd, das seine Thorheit sieht:", "tokens": ["Ach", ",", "spricht", "das", "Pferd", ",", "das", "sei\u00b7ne", "Thor\u00b7heit", "sieht", ":"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VVFIN", "ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ich Narr! es giebt der Narrn in Friesland und in Sachsen!", "tokens": ["Ich", "Narr", "!", "es", "giebt", "der", "Narrn", "in", "Fries\u00b7land", "und", "in", "Sach\u00b7sen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NE", "$.", "PPER", "VVFIN", "ART", "NN", "APPR", "NE", "KON", "APPR", "NE", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mein Schweif, der sch\u00f6ne Schweif! ich la\u00df ihn wieder wachsen.", "tokens": ["Mein", "Schweif", ",", "der", "sch\u00f6\u00b7ne", "Schweif", "!", "ich", "la\u00df", "ihn", "wie\u00b7der", "wach\u00b7sen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ART", "ADJA", "NN", "$.", "PPER", "VVFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die garstge Fliege zieht", "tokens": ["Die", "garst\u00b7ge", "Flie\u00b7ge", "zieht"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Mir alle Kraft noch aus den Knochen!", "tokens": ["Mir", "al\u00b7le", "Kraft", "noch", "aus", "den", "Kno\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "NN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Hans! ruft der Stallknecht, dumm gesprochen!", "tokens": ["Hans", "!", "ruft", "der", "Stall\u00b7knecht", ",", "dumm", "ge\u00b7spro\u00b7chen", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$.", "VVFIN", "ART", "NN", "$,", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Denn sieh, das Glied,", "tokens": ["Denn", "sieh", ",", "das", "Glied", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVIMP", "$,", "ART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "An dem er hing, ist dir gel\u00e4hmt.", "tokens": ["An", "dem", "er", "hing", ",", "ist", "dir", "ge\u00b7l\u00e4hmt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "VVFIN", "$,", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Am besten ist, da\u00df man zur Arbeit sich bequemt,", "tokens": ["Am", "bes\u00b7ten", "ist", ",", "da\u00df", "man", "zur", "Ar\u00b7beit", "sich", "be\u00b7quemt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "VAFIN", "$,", "KOUS", "PIS", "APPRART", "NN", "PRF", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wenn man dazu berufen ist", "tokens": ["Wenn", "man", "da\u00b7zu", "be\u00b7ru\u00b7fen", "ist"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "PAV", "VVPP", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Durch starke Glieder.", "tokens": ["Durch", "star\u00b7ke", "Glie\u00b7der", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Der Schweif, der sch\u00f6ne Schweif, w\u00e4chst dir gewi\u00df nicht wieder!", "tokens": ["Der", "Schweif", ",", "der", "sch\u00f6\u00b7ne", "Schweif", ",", "w\u00e4chst", "dir", "ge\u00b7wi\u00df", "nicht", "wie\u00b7der", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "NN", "$,", "VVFIN", "PPER", "ADV", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Da\u00df du so dumm gewesen bist!", "tokens": ["Da\u00df", "du", "so", "dumm", "ge\u00b7we\u00b7sen", "bist", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "VAPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Und w\u00e4chst er auch, du kannst ihn doch nicht brauchen.", "tokens": ["Und", "w\u00e4chst", "er", "auch", ",", "du", "kannst", "ihn", "doch", "nicht", "brau\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "$,", "PPER", "VMFIN", "PPER", "ADV", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Ha! du geh\u00f6rest zu den Gauchen,", "tokens": ["Ha", "!", "du", "ge\u00b7h\u00f6\u00b7rest", "zu", "den", "Gau\u00b7chen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.11": {"text": "Die ihren Deutschen gern verstecken", "tokens": ["Die", "ih\u00b7ren", "Deut\u00b7schen", "gern", "ver\u00b7ste\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPOSAT", "NN", "ADV", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "In einen Frackrock \u2013 o die Gecken!", "tokens": ["In", "ei\u00b7nen", "Frack\u00b7rock", "\u2013", "o", "die", "Ge\u00b7cken", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$(", "FM", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Ein deutsches Pferd, ein Friese, stand", "tokens": ["Ein", "deut\u00b7sches", "Pferd", ",", "ein", "Frie\u00b7se", ",", "stand"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "$,", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nicht weit von einem Engell\u00e4nder,", "tokens": ["Nicht", "weit", "von", "ei\u00b7nem", "En\u00b7gel\u00b7l\u00e4n\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Verglich sich, fand", "tokens": ["Ver\u00b7glich", "sich", ",", "fand"], "token_info": ["word", "word", "punct", "word"], "pos": ["ADJD", "PRF", "$,", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Den fremden Herrn behender.", "tokens": ["Den", "frem\u00b7den", "Herrn", "be\u00b7hen\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Willst du gestutzt sein? fragt der Herr, dem er geh\u00f6rt.", "tokens": ["Willst", "du", "ge\u00b7stutzt", "sein", "?", "fragt", "der", "Herr", ",", "dem", "er", "ge\u00b7h\u00f6rt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVPP", "VAINF", "$.", "VVFIN", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ja, spricht der Friese, ja! ich werde", "tokens": ["Ja", ",", "spricht", "der", "Frie\u00b7se", ",", "ja", "!", "ich", "wer\u00b7de"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PTKANT", "$,", "VVFIN", "ART", "NN", "$,", "ADV", "$.", "PPER", "VAFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Dann laufen, wie der Vogel fliegt, die Erde", "tokens": ["Dann", "lau\u00b7fen", ",", "wie", "der", "Vo\u00b7gel", "fliegt", ",", "die", "Er\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVINF", "$,", "PWAV", "ART", "NE", "VVFIN", "$,", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Fliegt unter mir!", "tokens": ["Fliegt", "un\u00b7ter", "mir", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.12": {"line.1": {"text": "Hast aber keinen Schweif, der dir", "tokens": ["Hast", "a\u00b7ber", "kei\u00b7nen", "Schweif", ",", "der", "dir"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["VAFIN", "ADV", "PIAT", "NN", "$,", "PRELS", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Fliegen wehrt!", "tokens": ["Die", "Flie\u00b7gen", "wehrt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.13": {"line.1": {"text": "Den Fliegen will ich wohl entlaufen!", "tokens": ["Den", "Flie\u00b7gen", "will", "ich", "wohl", "ent\u00b7lau\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.14": {"line.1": {"text": "Sein Wunsch wird endlich ihm gew\u00e4hrt,", "tokens": ["Sein", "Wunsch", "wird", "end\u00b7lich", "ihm", "ge\u00b7w\u00e4hrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er wird gestutzt, er d\u00fcnkt sich, unter einem Haufen", "tokens": ["Er", "wird", "ge\u00b7stutzt", ",", "er", "d\u00fcnkt", "sich", ",", "un\u00b7ter", "ei\u00b7nem", "Hau\u00b7fen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "VVPP", "$,", "PPER", "VVFIN", "PRF", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Von Pferden, nun das sch\u00f6nste Pferd!", "tokens": ["Von", "Pfer\u00b7den", ",", "nun", "das", "sch\u00f6ns\u00b7te", "Pferd", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.15": {"line.1": {"text": "Den Engell\u00e4nder her! ruft nun der Herr,", "tokens": ["Den", "En\u00b7gel\u00b7l\u00e4n\u00b7der", "her", "!", "ruft", "nun", "der", "Herr", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$.", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Er soll einmal die Probe laufen.", "tokens": ["Er", "soll", "ein\u00b7mal", "die", "Pro\u00b7be", "lau\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Er l\u00e4uft, er st\u00fcrzt, er kann nicht mehr;", "tokens": ["Er", "l\u00e4uft", ",", "er", "st\u00fcrzt", ",", "er", "kann", "nicht", "mehr", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "$,", "PPER", "VMFIN", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Stechfliegen qu\u00e4len ihn; der Herr will ihn verkaufen.", "tokens": ["Stech\u00b7flie\u00b7gen", "qu\u00e4\u00b7len", "ihn", ";", "der", "Herr", "will", "ihn", "ver\u00b7kau\u00b7fen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$.", "ART", "NN", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.16": {"line.1": {"text": "Man bietet nichts f\u00fcr ihn; der Abgestutzte kann", "tokens": ["Man", "bie\u00b7tet", "nichts", "f\u00fcr", "ihn", ";", "der", "Ab\u00b7ge\u00b7stutz\u00b7te", "kann"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PIS", "VVFIN", "PIS", "APPR", "PPER", "$.", "ART", "NN", "VMFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der b\u00f6sen Fliegen sich nicht wehren,", "tokens": ["Der", "b\u00f6\u00b7sen", "Flie\u00b7gen", "sich", "nicht", "weh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PRF", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Nicht seinen Herrn ern\u00e4hren,", "tokens": ["Nicht", "sei\u00b7nen", "Herrn", "er\u00b7n\u00e4h\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Man sieht ihm seinen Kummer an.", "tokens": ["Man", "sieht", "ihm", "sei\u00b7nen", "Kum\u00b7mer", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.17": {"line.1": {"text": "Ach, spricht das Pferd, das seine Thorheit sieht:", "tokens": ["Ach", ",", "spricht", "das", "Pferd", ",", "das", "sei\u00b7ne", "Thor\u00b7heit", "sieht", ":"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VVFIN", "ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ich Narr! es giebt der Narrn in Friesland und in Sachsen!", "tokens": ["Ich", "Narr", "!", "es", "giebt", "der", "Narrn", "in", "Fries\u00b7land", "und", "in", "Sach\u00b7sen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NE", "$.", "PPER", "VVFIN", "ART", "NN", "APPR", "NE", "KON", "APPR", "NE", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mein Schweif, der sch\u00f6ne Schweif! ich la\u00df ihn wieder wachsen.", "tokens": ["Mein", "Schweif", ",", "der", "sch\u00f6\u00b7ne", "Schweif", "!", "ich", "la\u00df", "ihn", "wie\u00b7der", "wach\u00b7sen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ART", "ADJA", "NN", "$.", "PPER", "VVFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die garstge Fliege zieht", "tokens": ["Die", "garst\u00b7ge", "Flie\u00b7ge", "zieht"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Mir alle Kraft noch aus den Knochen!", "tokens": ["Mir", "al\u00b7le", "Kraft", "noch", "aus", "den", "Kno\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "NN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.18": {"line.1": {"text": "Hans! ruft der Stallknecht, dumm gesprochen!", "tokens": ["Hans", "!", "ruft", "der", "Stall\u00b7knecht", ",", "dumm", "ge\u00b7spro\u00b7chen", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$.", "VVFIN", "ART", "NN", "$,", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Denn sieh, das Glied,", "tokens": ["Denn", "sieh", ",", "das", "Glied", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVIMP", "$,", "ART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "An dem er hing, ist dir gel\u00e4hmt.", "tokens": ["An", "dem", "er", "hing", ",", "ist", "dir", "ge\u00b7l\u00e4hmt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "VVFIN", "$,", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Am besten ist, da\u00df man zur Arbeit sich bequemt,", "tokens": ["Am", "bes\u00b7ten", "ist", ",", "da\u00df", "man", "zur", "Ar\u00b7beit", "sich", "be\u00b7quemt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "VAFIN", "$,", "KOUS", "PIS", "APPRART", "NN", "PRF", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wenn man dazu berufen ist", "tokens": ["Wenn", "man", "da\u00b7zu", "be\u00b7ru\u00b7fen", "ist"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "PAV", "VVPP", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Durch starke Glieder.", "tokens": ["Durch", "star\u00b7ke", "Glie\u00b7der", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Der Schweif, der sch\u00f6ne Schweif, w\u00e4chst dir gewi\u00df nicht wieder!", "tokens": ["Der", "Schweif", ",", "der", "sch\u00f6\u00b7ne", "Schweif", ",", "w\u00e4chst", "dir", "ge\u00b7wi\u00df", "nicht", "wie\u00b7der", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "NN", "$,", "VVFIN", "PPER", "ADV", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Da\u00df du so dumm gewesen bist!", "tokens": ["Da\u00df", "du", "so", "dumm", "ge\u00b7we\u00b7sen", "bist", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "VAPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Und w\u00e4chst er auch, du kannst ihn doch nicht brauchen.", "tokens": ["Und", "w\u00e4chst", "er", "auch", ",", "du", "kannst", "ihn", "doch", "nicht", "brau\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "$,", "PPER", "VMFIN", "PPER", "ADV", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Ha! du geh\u00f6rest zu den Gauchen,", "tokens": ["Ha", "!", "du", "ge\u00b7h\u00f6\u00b7rest", "zu", "den", "Gau\u00b7chen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.11": {"text": "Die ihren Deutschen gern verstecken", "toke