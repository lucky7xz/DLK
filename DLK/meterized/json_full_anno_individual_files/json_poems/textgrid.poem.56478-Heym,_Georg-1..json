{"textgrid.poem.56478": {"metadata": {"author": {"name": "Heym, Georg", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1899, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du ruhst im Dunkel trauriger Askesen", "tokens": ["Du", "ruhst", "im", "Dun\u00b7kel", "trau\u00b7ri\u00b7ger", "As\u00b7ke\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In deinem wei\u00dfen Tuch, ein Eremit,", "tokens": ["In", "dei\u00b7nem", "wei\u00b7\u00dfen", "Tuch", ",", "ein", "E\u00b7re\u00b7mit", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und deine Locken, die in Nacht verwesen,", "tokens": ["Und", "dei\u00b7ne", "Lo\u00b7cken", ",", "die", "in", "Nacht", "ver\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "PRELS", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Bedecken tief dein eingesunknes Lid.", "tokens": ["Be\u00b7de\u00b7cken", "tief", "dein", "ein\u00b7ge\u00b7sunk\u00b7nes", "Lid", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Auf deinen Lippen gruben sich die Male", "tokens": ["Auf", "dei\u00b7nen", "Lip\u00b7pen", "gru\u00b7ben", "sich", "die", "Ma\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PRF", "ART", "NN"], "meter": "-+-+-+-