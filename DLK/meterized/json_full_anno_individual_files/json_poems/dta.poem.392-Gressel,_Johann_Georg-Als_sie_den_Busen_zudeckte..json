{"dta.poem.392": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Als sie den Busen zudeckte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Mein Kind du deckst den Schnee der zarten Liebes-Ballen/", "tokens": ["Mein", "Kind", "du", "deckst", "den", "Schnee", "der", "zar\u00b7ten", "Lie\u00b7bes\u00b7Bal\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und siehst nicht da\u00df mein Geist die zarte Gegend k\u00fc\u00dft/", "tokens": ["Und", "siehst", "nicht", "da\u00df", "mein", "Geist", "die", "zar\u00b7te", "Ge\u00b7gend", "k\u00fc\u00dft", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "KOUS", "PPOSAT", "NN", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mein sage/ solt\u2019 es dir auch allzuwohl gefallen?", "tokens": ["Mein", "sa\u00b7ge", "/", "solt'", "es", "dir", "auch", "all\u00b7zu\u00b7wohl", "ge\u00b7fal\u00b7len", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VVFIN", "$(", "VMFIN", "PPER", "PPER", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wenn man den Augen raubt/ was ihr Vergn\u00fcgen ist.", "tokens": ["Wenn", "man", "den", "Au\u00b7gen", "raubt", "/", "was", "ihr", "Ver\u00b7gn\u00fc\u00b7gen", "ist", "."],