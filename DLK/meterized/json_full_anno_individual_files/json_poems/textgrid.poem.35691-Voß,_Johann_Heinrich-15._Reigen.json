{"textgrid.poem.35691": {"metadata": {"author": {"name": "Vo\u00df, Johann Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "15. Reigen", "genre": "verse", "period": "N.A.", "pub_year": 1776, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sagt mir an, was schmunzelt ihr?", "tokens": ["Sagt", "mir", "an", ",", "was", "schmun\u00b7zelt", "ihr", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "$,", "PWS", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Schiebt ihr's auf das Kirmesbier,", "tokens": ["Schiebt", "ih\u00b7r's", "auf", "das", "Kir\u00b7mes\u00b7bier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df ich so vor Freuden kr\u00e4he,", "tokens": ["Da\u00df", "ich", "so", "vor", "Freu\u00b7den", "kr\u00e4\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und auf einem Bein mich drehe?", "tokens": ["Und", "auf", "ei\u00b7nem", "Bein", "mich", "dre\u00b7he", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Schurken um und um!", "tokens": ["Schur\u00b7ken", "um", "und", "um", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "KON", "KOUI", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "K\u00f6mmt die schmucke Binderin", "tokens": ["K\u00f6mmt", "die", "schmu\u00b7cke", "Bin\u00b7de\u00b7rin"], "token_info": ["word", "word",