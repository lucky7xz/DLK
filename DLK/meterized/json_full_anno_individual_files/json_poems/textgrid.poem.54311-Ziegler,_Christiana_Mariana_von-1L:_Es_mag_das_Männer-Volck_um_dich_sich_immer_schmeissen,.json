{"textgrid.poem.54311": {"metadata": {"author": {"name": "Ziegler, Christiana Mariana von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es mag das M\u00e4nner-Volck um dich sich immer schmeissen,", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es mag das M\u00e4nner-Volck um dich sich immer schmeissen,", "tokens": ["Es", "mag", "das", "M\u00e4n\u00b7ner\u00b7Volck", "um", "dich", "sich", "im\u00b7mer", "schmeis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "APPR", "PPER", "PRF", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich werde warlich mir die Krause nicht zerreissen.", "tokens": ["Ich", "wer\u00b7de", "war\u00b7lich", "mir", "die", "Krau\u00b7se", "nicht", "zer\u00b7reis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PPER", "ART", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Du schmeichelst dir zu viel und bist vor Ehrgeitz blind,", "tokens": ["Du", "schmei\u00b7chelst", "dir", "zu", "viel", "und", "bist", "vor", "Ehr\u00b7geitz", "blind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "PIAT", "KON", "VAFIN", "APPR", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ob man gleich hier und dar viel Unvollkommnes find.", "tokens": ["Ob", "man", "gleich", "hier", "und", "dar", "viel", "Un\u00b7voll\u00b7komm\u00b7nes", "find", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", 