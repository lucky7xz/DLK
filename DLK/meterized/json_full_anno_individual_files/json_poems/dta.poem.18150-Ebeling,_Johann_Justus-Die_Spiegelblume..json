{"dta.poem.18150": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "Die Spiegelblume.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198805", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Grosser GOtt! zu deinem Ruhme,", "tokens": ["Gros\u00b7ser", "Gott", "!", "zu", "dei\u00b7nem", "Ruh\u00b7me", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Treibt mich an die Spiegelblume,", "tokens": ["Treibt", "mich", "an", "die", "Spie\u00b7gel\u00b7blu\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Die in g\u00fcldnen Schimmer bl\u00fcht,", "tokens": ["Die", "in", "g\u00fcld\u00b7nen", "Schim\u00b7mer", "bl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Da mein Auge, mein Gem\u00fcthe,", "tokens": ["Da", "mein", "Au\u00b7ge", ",", "mein", "Ge\u00b7m\u00fc\u00b7the", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "In dem Spiegeln deiner G\u00fcte,", "tokens": ["In", "dem", "Spie\u00b7geln", "dei\u00b7ner", "G\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Deiner Gottheit Wunder sieht.", "tokens": ["Dei\u00b7ner", "Got\u00b7theit", "Wun\u00b7der", "sieht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Du hast diese Blum gebildet,", "tokens": ["Du", "hast", "die\u00b7se", "Blum", "ge\u00b7bil\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PDAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Jhre Bl\u00e4tter \u00fcberg\u00fcldet", "tokens": ["Ih\u00b7re", "Bl\u00e4t\u00b7ter", "\u00fc\u00b7berg\u00b7\u00fcl\u00b7det"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Durch den Finger deiner Macht:", "tokens": ["Durch", "den", "Fin\u00b7ger", "dei\u00b7ner", "Macht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Grosser Sch\u00f6pfer! sei gepriesen,", "tokens": ["Gros\u00b7ser", "Sch\u00f6p\u00b7fer", "!", "sei", "ge\u00b7prie\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Da\u00df du Felder, G\u00e4rten, Wiesen,", "tokens": ["Da\u00df", "du", "Fel\u00b7der", ",", "G\u00e4r\u00b7ten", ",", "Wie\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "So geschm\u00fckt herf\u00fcr gebracht.", "tokens": ["So", "ge\u00b7schm\u00fckt", "her\u00b7f\u00fcr", "ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "La\u00df mich ferner bei den Gaben,", "tokens": ["La\u00df", "mich", "fer\u00b7ner", "bei", "den", "Ga\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die die \u00e4usren Sinne laben,", "tokens": ["Die", "die", "\u00e4us\u00b7ren", "Sin\u00b7ne", "la\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Auf dich als dem Geber sehn:", "tokens": ["Auf", "dich", "als", "dem", "Ge\u00b7ber", "sehn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "KOUS", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "La\u00df mich wenn die Leibes Augen,", "tokens": ["La\u00df", "mich", "wenn", "die", "Lei\u00b7bes", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "KOUS", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Anmuth aus dem Blumen saugen,", "tokens": ["An\u00b7muth", "aus", "dem", "Blu\u00b7men", "sau\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Deinen Ruhm im Geist erh\u00f6hn.", "tokens": ["Dei\u00b7nen", "Ruhm", "im", "Geist", "er\u00b7h\u00f6hn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}