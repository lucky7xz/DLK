{"textgrid.poem.57566": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "1. Ich", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ob ich denn keine lieb gehabt,", "tokens": ["Ob", "ich", "denn", "kei\u00b7ne", "lieb", "ge\u00b7habt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PIAT", "ADJD", "VAPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Fragst du, verlornes Kind \u2013", "tokens": ["Fragst", "du", ",", "ver\u00b7lor\u00b7nes", "Kind", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ADJA", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ihr Auge war blau, rotgoldig ihr Haar,", "tokens": ["Ihr", "Au\u00b7ge", "war", "blau", ",", "rot\u00b7gol\u00b7dig", "ihr", "Haar", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$,", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wie deine Z\u00f6pfe sind.", "tokens": ["Wie", "dei\u00b7ne", "Z\u00f6p\u00b7fe", "sind", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ihre Hand war wei\u00df wie deine Hand,", "tokens": ["Ih\u00b7re", "Hand", "war", "wei\u00df", "wie", "dei\u00b7ne", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "KOKOM", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Rein war sie an Seele und Leib \u2013", "tokens": ["Rein", "war", "sie", "an", "See\u00b7le", "und", "Leib", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "APPR", "NN", "KON", "NN", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Doch, M\u00e4del, du kennst ja mein sch\u00f6nstes Gedicht:", "tokens": ["Doch", ",", "M\u00e4\u00b7del", ",", "du", "kennst", "ja", "mein", "sch\u00f6ns\u00b7tes", "Ge\u00b7dicht", ":"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "NN", "$,", "PPER", "VVFIN", "ADV", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Sie ist eines anderen Weib.", "tokens": ["Sie", "ist", "ei\u00b7nes", "an\u00b7de\u00b7ren", "Weib", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "--+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "Und noch eine andere teuer mir war,", "tokens": ["Und", "noch", "ei\u00b7ne", "an\u00b7de\u00b7re", "teu\u00b7er", "mir", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "ADJA", "NN", "PPER", "VAFIN", "$,"], "meter": "--+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sie hing wie ein Hund an mir,", "tokens": ["Sie", "hing", "wie", "ein", "Hund", "an", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "ART", "NN", "APPR", "PPER", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ich wurde es satt und trat mit dem Fu\u00df", "tokens": ["Ich", "wur\u00b7de", "es", "satt", "und", "trat", "mit", "dem", "Fu\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "ADJD", "KON", "V