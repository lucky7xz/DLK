{"dta.poem.2981": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "27.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Leicht w\u00e4re christliche Religion zu gr\u00fcnden", "tokens": ["Leicht", "w\u00e4\u00b7re", "christ\u00b7li\u00b7che", "Re\u00b7li\u00b7gi\u00b7on", "zu", "gr\u00fcn\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "ADJA", "NN", "PTKZU", "VVINF"], "meter": "+--+--+--+-+-", "measure": "dactylic.tri.plus"}, "line.2": {"text": "Im Lande, wo sich frei darf jeder Gott verk\u00fcnden;", "tokens": ["Im", "Lan\u00b7de", ",", "wo", "sich", "frei", "darf", "je\u00b7der", "Gott", "ver\u00b7k\u00fcn\u00b7den", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PWAV", "PRF", "ADJD", "VMFIN", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wo alle Herzen stehn und alle Tempel offen", "tokens": ["Wo", "al\u00b7le", "Her\u00b7zen", "stehn", "und", "al\u00b7le", "Tem\u00b7pel", "of\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PIAT", "NN", "VVINF", "KON", "PIAT", "NN", "ADJD"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "F\u00fcr jedes Gottgebild aus Erd- und Himmelstoffen.", "tokens": ["F\u00fcr", "je\u00b7des", "Gott\u00b7ge\u00b7bild", "aus", "Erd", "und", "Him\u00b7mel\u00b7stof\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "APPR", "TRUNC", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Leicht w\u00e4re Christenthum in Indien auch zu stiften,", "tokens": ["Leicht", "w\u00e4\u00b7re", "Chris\u00b7ten\u00b7thum", "in", "In\u00b7di\u00b7en", "auch", "zu", "stif\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "NN", "APPR", "NE", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Wenn keine Christen nur es k\u00e4men zu vergiften.", "tokens": ["Wenn", "kei\u00b7ne", "Chris\u00b7ten", "nur", "es", "k\u00e4\u00b7men", "zu", "ver\u00b7gif\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "ADV", "PPER", "VVINF", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Aus Glaubensbotenmund was wir mit Lust vernahmen", "tokens": ["Aus", "Glau\u00b7bens\u00b7bo\u00b7ten\u00b7mund", "was", "wir", "mit", "Lust", "ver\u00b7nah\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "PWS", "PPER", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ward uns verleidet als die Glaubensbr\u00fcder kamen;", "tokens": ["Ward", "uns", "ver\u00b7lei\u00b7det", "als", "die", "Glau\u00b7bens\u00b7br\u00fc\u00b7der", "ka\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVFIN", "KOKOM", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Beschmutzt mit jedem Schmutz, unschuldig keiner Schuldung,", "tokens": ["Be\u00b7schmutzt", "mit", "je\u00b7dem", "Schmutz", ",", "un\u00b7schul\u00b7dig", "kei\u00b7ner", "Schul\u00b7dung", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "PIAT", "NN", "$,", "ADJD", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Eigen dem Eigennutz, ohne Geduld und Duldung.", "tokens": ["Ei\u00b7gen", "dem", "Ei\u00b7gen\u00b7nutz", ",", "oh\u00b7ne", "Ge\u00b7duld", "und", "Dul\u00b7dung", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "KOUI", "NN", "KON", "NN", "$."], "meter": "+--+--+--+-+-", "measure": "dactylic.tri.plus"}}, "stanza.6": {"line.1": {"text": "Belehr uns besser doch, bevor wir uns bekehren,", "tokens": ["Be\u00b7lehr", "uns", "bes\u00b7ser", "doch", ",", "be\u00b7vor", "wir", "uns", "be\u00b7keh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADJD", "ADV", "$,", "KOUS", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df besser, als wir sind, euch machten eure Lehren!", "tokens": ["Da\u00df", "bes\u00b7ser", ",", "als", "wir", "sind", ",", "euch", "mach\u00b7ten", "eu\u00b7re", "Leh\u00b7ren", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "$,", "KOUS", "PPER", "VAFIN", "$,", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Und gebt dem Heidenthum bei uns noch ein'ge Fristen,", "tokens": ["Und", "gebt", "dem", "Hei\u00b7den\u00b7thum", "bei", "uns", "noch", "ein'\u00b7ge", "Fris\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "PPER", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bis ihr bei euch bekehrt zum Christenthum die Christen.", "tokens": ["Bis", "ihr", "bei", "euch", "be\u00b7kehrt", "zum", "Chris\u00b7ten\u00b7thum", "die", "Chris\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "APPR", "PPER", "VVFIN", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}