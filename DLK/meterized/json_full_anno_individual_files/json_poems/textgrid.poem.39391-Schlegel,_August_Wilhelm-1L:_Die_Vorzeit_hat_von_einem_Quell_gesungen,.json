{"textgrid.poem.39391": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Vorzeit hat von einem Quell gesungen,", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Vorzeit hat von einem Quell gesungen,", "tokens": ["Die", "Vor\u00b7zeit", "hat", "von", "ei\u00b7nem", "Quell", "ge\u00b7sun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des Zauberkraft die Jugend brachte wieder.", "tokens": ["Des", "Zau\u00b7ber\u00b7kraft", "die", "Ju\u00b7gend", "brach\u00b7te", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der matte Greis, ganz von der Zeit bezwungen,", "tokens": ["Der", "mat\u00b7te", "Greis", ",", "ganz", "von", "der", "Zeit", "be\u00b7zwun\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADV", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Er tauchte kaum in dieses Bad die Glieder,", "tokens": ["Er", "tauch\u00b7te", "kaum", "in", "die\u00b7ses", "Bad", "die", "Glie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PDAT", "NE", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "So war zum Herzen frisches Blut gedrungen,", "tokens": ["So", "war", "zum", "Her\u00b7zen", "fri\u00b7sches", "Blut", "ge\u00b7drun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPRART", "NN", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "So regte Lebenslust ihr neu Gefieder.", "tokens": ["So", "reg\u00b7te", "Le\u00b7bens\u00b7lust", "ihr", "neu", "Ge\u00b7fie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "PPER", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Selbst Tithon f\u00e4nd' in solchen Wunderfluten", "tokens": ["Selbst", "Ti\u00b7thon", "f\u00e4nd'", "in", "sol\u00b7chen", "Wun\u00b7der\u00b7flu\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "NN", "VVFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Sein blondes Haupthaar, und der Liebe Gluten.", "tokens": ["Sein", "blon\u00b7des", "Haupt\u00b7haar", ",", "und", "der", "Lie\u00b7be", "Glu\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "KON", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Die Sage, nicht aus eitlem Wahn ersonnen,", "tokens": ["Die", "Sa\u00b7ge", ",", "nicht", "aus", "eit\u00b7lem", "Wahn", "er\u00b7son\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PTKNEG", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Kann heut wie vormals wahrhaft sich bew\u00e4hren.", "tokens": ["Kann", "heut", "wie", "vor\u00b7mals", "wahr\u00b7haft", "sich", "be\u00b7w\u00e4h\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "KOKOM", "ADV", "ADV", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Poesie ist jener Lebensbronnen.", "tokens": ["Die", "Poe\u00b7sie", "ist", "je\u00b7ner", "Le\u00b7bens\u00b7bron\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PDAT", "NN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Sie wei\u00df die Welt im Spiegel zu verkl\u00e4ren,", "tokens": ["Sie", "wei\u00df", "die", "Welt", "im", "Spie\u00b7gel", "zu", "ver\u00b7kl\u00e4\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Hervorzurufen l\u00e4ngst entschwundne Wonnen,", "tokens": ["Her\u00b7vor\u00b7zu\u00b7ru\u00b7fen", "l\u00e4ngst", "ent\u00b7schwund\u00b7ne", "Won\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Den s\u00fc\u00dfen Glauben jeder Brust zu n\u00e4hren;", "tokens": ["Den", "s\u00fc\u00b7\u00dfen", "Glau\u00b7ben", "je\u00b7der", "Brust", "zu", "n\u00e4h\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PIAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und wer sich labt an ihren G\u00f6ttergaben,", "tokens": ["Und", "wer", "sich", "labt", "an", "ih\u00b7ren", "G\u00f6t\u00b7ter\u00b7ga\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PRF", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Wird im Gem\u00fcth die ew'ge Jugend haben.", "tokens": ["Wird", "im", "Ge\u00b7m\u00fcth", "die", "ew'\u00b7ge", "Ju\u00b7gend", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPRART", "NN", "ART", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Dein denk' ich hier, Verk\u00fcndiger des Sch\u00f6nen!", "tokens": ["Dein", "denk'", "ich", "hier", ",", "Ver\u00b7k\u00fcn\u00b7di\u00b7ger", "des", "Sch\u00f6\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VVFIN", "PPER", "ADV", "$,", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Musen Bot' an das Jahrhundert! Goethe!", "tokens": ["Der", "Mu\u00b7sen", "Bot'", "an", "das", "Jahr\u00b7hun\u00b7dert", "!", "Goe\u00b7the", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "NN", "APPR", "ART", "NN", "$.", "NE", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Du lehrtest Harmonie in allen T\u00f6nen,", "tokens": ["Du", "lehr\u00b7test", "Har\u00b7mo\u00b7nie", "in", "al\u00b7len", "T\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Harfe, der Posaun' und sanften Fl\u00f6te.", "tokens": ["Der", "Har\u00b7fe", ",", "der", "Po\u00b7saun'", "und", "sanf\u00b7ten", "Fl\u00f6\u00b7te", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Wo giebt es Lorbeern, die dein Haupt nicht kr\u00f6nen?", "tokens": ["Wo", "giebt", "es", "Lor\u00b7beern", ",", "die", "dein", "Haupt", "nicht", "kr\u00f6\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "NN", "$,", "PRELS", "PPOSAT", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Du kamest im Geleit der Morgenr\u00f6the:", "tokens": ["Du", "ka\u00b7mest", "im", "Ge\u00b7leit", "der", "Mor\u00b7gen\u00b7r\u00f6\u00b7the", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Sei Tithon denn, stets geistig neu geboren,", "tokens": ["Sei", "Ti\u00b7thon", "denn", ",", "stets", "geis\u00b7tig", "neu", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "ADV", "$,", "ADV", "ADJD", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Geliebt und nie betrauert von Auroren!", "tokens": ["Ge\u00b7liebt", "und", "nie", "be\u00b7trau\u00b7ert", "von", "Au\u00b7ro\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "ADV", "VVPP", "APPR", "NE", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Gleich jenem Baum, dem Liebling der Pomone,", "tokens": ["Gleich", "je\u00b7nem", "Baum", ",", "dem", "Lieb\u00b7ling", "der", "Po\u00b7mo\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDAT", "NN", "$,", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Nektar-Aepfel tr\u00e4gt mit goldnen Schalen,", "tokens": ["Der", "Nek\u00b7ta\u00b7rAe\u00b7pfel", "tr\u00e4gt", "mit", "gold\u00b7nen", "Scha\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dem wei\u00dfe Bl\u00fcthen aus der dunkeln Krone", "tokens": ["Dem", "wei\u00b7\u00dfe", "Bl\u00fc\u00b7then", "aus", "der", "dun\u00b7keln", "Kro\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zugleich mit Fr\u00fcchten jedes Alters strahlen,", "tokens": ["Zu\u00b7gleich", "mit", "Fr\u00fcch\u00b7ten", "je\u00b7des", "Al\u00b7ters", "strah\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ausathmend Balsamduft der sonn'gen Zone,", "tokens": ["Au\u00b7sath\u00b7mend", "Bal\u00b7sam\u00b7duft", "der", "sonn'\u00b7gen", "Zo\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.6": {"text": "In der gl\u00fccksel'gen Inseln stillen Thalen:", "tokens": ["In", "der", "gl\u00fcck\u00b7sel'\u00b7gen", "In\u00b7seln", "stil\u00b7len", "Tha\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "So ward, ein Spr\u00f6\u00dfling aus den Hesperiden,", "tokens": ["So", "ward", ",", "ein", "Spr\u00f6\u00df\u00b7ling", "aus", "den", "Hes\u00b7pe\u00b7ri\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "$,", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Der Dichter unserm Vaterland beschieden.", "tokens": ["Der", "Dich\u00b7ter", "un\u00b7serm", "Va\u00b7ter\u00b7land", "be\u00b7schie\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Er \u00fcberw\u00f6lbt es mit den schatt'gen Aesten", "tokens": ["Er", "\u00fc\u00b7berw\u00b7\u00f6lbt", "es", "mit", "den", "schatt'\u00b7gen", "A\u00b7es\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+----", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Weit von den Alpen zu des Belts Gestaden.", "tokens": ["Weit", "von", "den", "Al\u00b7pen", "zu", "des", "Belts", "Ge\u00b7sta\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "APPR", "ART", "NN", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Wie wir am Rhein, ist manche Schaar von G\u00e4sten", "tokens": ["Wie", "wir", "am", "Rhein", ",", "ist", "man\u00b7che", "Schaar", "von", "G\u00e4s\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPRART", "NE", "$,", "VAFIN", "PIAT", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zu gleicher Feier, nah und fern, geladen;", "tokens": ["Zu", "glei\u00b7cher", "Fei\u00b7er", ",", "nah", "und", "fern", ",", "ge\u00b7la\u00b7den", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ADJD", "KON", "ADJD", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Viel Stimmen sch