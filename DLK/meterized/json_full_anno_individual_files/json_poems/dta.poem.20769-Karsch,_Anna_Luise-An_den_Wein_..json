{"dta.poem.20769": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "An den Wein .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1792", "urn": "urn:nbn:de:kobv:b4-200905193016", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Loben wollt ich die Begier,", "tokens": ["Lo\u00b7ben", "wollt", "ich", "die", "Be\u00b7gier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wein zu trinken halbe N\u00e4chte,", "tokens": ["Wein", "zu", "trin\u00b7ken", "hal\u00b7be", "N\u00e4ch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VVINF", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn mein Milon nur mit mir", "tokens": ["Wenn", "mein", "Mi\u00b7lon", "nur", "mit", "mir"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "APPR", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Manchen Abend zechte;", "tokens": ["Man\u00b7chen", "A\u00b7bend", "zech\u00b7te", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Aber nun trinkt er den Wein", "tokens": ["A\u00b7ber", "nun", "trinkt", "er", "den", "Wein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ART", "NN"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.6": {"text": "Mit den M\u00e4nnern ganz allein.", "tokens": ["Mit", "den", "M\u00e4n\u00b7nern", "ganz", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Ha ihr M\u00e4nner, ha ihr Zecher,", "tokens": ["Ha", "ihr", "M\u00e4n\u00b7ner", ",", "ha", "ihr", "Ze\u00b7cher", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "$,", "NE", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Amor jag euch von dem Becher", "tokens": ["A\u00b7mor", "jag", "euch", "von", "dem", "Be\u00b7cher"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Durch die Pfeile, die im K\u00f6cher", "tokens": ["Durch", "die", "Pfei\u00b7le", ",", "die", "im", "K\u00f6\u00b7cher"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Aufgesammlet sind zur Pein", "tokens": ["Auf\u00b7ge\u00b7samm\u00b7let", "sind", "zur", "Pein"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VAFIN", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Aller Herzenbrecher.", "tokens": ["Al\u00b7ler", "Her\u00b7zen\u00b7bre\u00b7cher", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}