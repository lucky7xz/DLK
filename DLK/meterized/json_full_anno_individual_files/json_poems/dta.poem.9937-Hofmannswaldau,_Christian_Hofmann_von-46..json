{"dta.poem.9937": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "46.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich bin gleiche den monarchen/ wenn auf meinem mist ich bin/", "tokens": ["Ich", "bin", "glei\u00b7che", "den", "mon\u00b7ar\u00b7chen", "/", "wenn", "auf", "mei\u00b7nem", "mist", "ich", "bin", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJA", "ART", "NN", "$(", "KOUS", "APPR", "PPOSAT", "VVFIN", "PPER", "VAFIN", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Keinen ich zu meinem throne la\u00df ohn streiten kommen hin.", "tokens": ["Kei\u00b7nen", "ich", "zu", "mei\u00b7nem", "thro\u00b7ne", "la\u00df", "ohn", "strei\u00b7ten", "kom\u00b7men", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "ADJA", "NN", "APPR", "ADJA", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Ich bin eines volckes vater/ so stets federn tragen mag.", "tokens": ["Ich", "bin", "ei\u00b7nes", "vol\u00b7ckes", "va\u00b7ter", "/", "so", "stets", "fe\u00b7dern", "tra\u00b7gen", "mag."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "abbreviation"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$(", "ADV", "ADV", "VVINF", "VVFIN", "NE"], "meter": "-+--+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Ich bin ein prophet des wetters/ ruffe/ wen\u0303 man sp\u00fcrt den tag.", "tokens": ["Ich", "bin", "ein", "pro\u00b7phet", "des", "wet\u00b7ters", "/", "ruf\u00b7fe", "/", "we\u00f1", "man", "sp\u00fcrt", "den", "tag", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "VVFIN", "ART", "NN", "$(", "VVFIN", "$(", "KOUS", "PIS", "VVFIN", "ART", "NN", "$."], "meter": "-+--+-+-+-+-+-+", "measure": "iambic.septa.relaxed"}}}}}