{"dta.poem.7357": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Das Eselein.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-20090519994", "language": ["de:0.99"], "booktitle": "Droste-H\u00fclshoff, Annette von: Gedichte. Stuttgart u. a., 1844."}, "poem": {"stanza.1": {"line.1": {"text": "Auf einem Wiesengrund gieng einmal", "tokens": ["Auf", "ei\u00b7nem", "Wie\u00b7sen\u00b7grund", "gieng", "ein\u00b7mal"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "ADV"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ein muntres R\u00f6\u00dflein weiden,", "tokens": ["Ein", "mun\u00b7tres", "R\u00f6\u00df\u00b7lein", "wei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ein Schimmelchen war's, doch etwas fahl,", "tokens": ["Ein", "Schim\u00b7mel\u00b7chen", "wa\u00b7r's", ",", "doch", "et\u00b7was", "fahl", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$,", "ADV", "ADV", "ADJD", "$,"], "meter": "-+---+-+-+", "measure": "dactylic.init"}, "line.4": {"text": "Sein Aeu\u00dferes nenn' ich bescheiden,", "tokens": ["Sein", "A\u00b7e\u00b7u\u00b7\u00dfe\u00b7res", "nenn'", "ich", "be\u00b7schei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Das schlechtste und auch das beste nicht,", "tokens": ["Das", "schlechts\u00b7te", "und", "auch", "das", "bes\u00b7te", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "KON", "ADV", "ART", "ADJA", "PTKNEG", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Wir wollen nicht dr\u00fcber zanken,", "tokens": ["Wir", "wol\u00b7len", "nicht", "dr\u00fc\u00b7ber", "zan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "PAV", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Doch hatt' es ein klares Augenlicht", "tokens": ["Doch", "hatt'", "es", "ein", "kla\u00b7res", "Au\u00b7gen\u00b7licht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Und starke geschmeidige Flanken.", "tokens": ["Und", "star\u00b7ke", "ge\u00b7schmei\u00b7di\u00b7ge", "Flan\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.2": {"line.1": {"text": "In selbem Grunde schritt oft und viel", "tokens": ["In", "sel\u00b7bem", "Grun\u00b7de", "schritt", "oft", "und", "viel"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ADV", "KON", "ADV"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Ein edler J\u00fcngling spazieren,", "tokens": ["Ein", "ed\u00b7ler", "J\u00fcng\u00b7ling", "spa\u00b7zie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Hinter jedem Ohre ein Federkiel,", "tokens": ["Hin\u00b7ter", "je\u00b7dem", "Oh\u00b7re", "ein", "Fe\u00b7der\u00b7kiel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ART", "NN", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Das th\u00e4t ihn wunderbar zieren!", "tokens": ["Das", "th\u00e4t", "ihn", "wun\u00b7der\u00b7bar", "zie\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Am R\u00fccken ein G\u00e4nsefl\u00fcgelpaar,", "tokens": ["Am", "R\u00fc\u00b7cken", "ein", "G\u00e4n\u00b7se\u00b7fl\u00fc\u00b7gel\u00b7paar", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Die th\u00e4ten rauschen und wedeln,", "tokens": ["Die", "th\u00e4\u00b7ten", "rau\u00b7schen", "und", "we\u00b7deln", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Und wi\u00dft, seine g\u00f6ttliche Gabe war,", "tokens": ["Und", "wi\u00dft", ",", "sei\u00b7ne", "g\u00f6tt\u00b7li\u00b7che", "Ga\u00b7be", "war", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PPOSAT", "ADJA", "NN", "VAFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.8": {"text": "Die schlechte Natur zu veredeln.", "tokens": ["Die", "schlech\u00b7te", "Na\u00b7tur", "zu", "ver\u00b7e\u00b7deln", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.3": {"line.1": {"text": "Den Tropfen der seiner Stirne entrann,", "tokens": ["Den", "Trop\u00b7fen", "der", "sei\u00b7ner", "Stir\u00b7ne", "ent\u00b7rann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Den soll wie Perle man fassen,", "tokens": ["Den", "soll", "wie", "Per\u00b7le", "man", "fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "KOKOM", "NN", "PIS", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ach, ohne ihn h\u00e4tte die Sonne man", "tokens": ["Ach", ",", "oh\u00b7ne", "ihn", "h\u00e4t\u00b7te", "die", "Son\u00b7ne", "man"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "KOUI", "PPER", "VAFIN", "ART", "NN", "PIS"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "So simpelhin scheinen lassen,", "tokens": ["So", "sim\u00b7pel\u00b7hin", "schei\u00b7nen", "las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVINF", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Und ohne ihn w\u00e4re der Wiesengrund", "tokens": ["Und", "oh\u00b7ne", "ihn", "w\u00e4\u00b7re", "der", "Wie\u00b7sen\u00b7grund"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPER", "VAFIN", "ART", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Ein n\u00fcchterner Anger geblieben,", "tokens": ["Ein", "n\u00fcch\u00b7ter\u00b7ner", "An\u00b7ger", "ge\u00b7blie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.7": {"text": "Ein Quellchen blank, ein H\u00fcgelchen rund,", "tokens": ["Ein", "Quell\u00b7chen", "blank", ",", "ein", "H\u00fc\u00b7gel\u00b7chen", "rund", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.8": {"text": "Und eine Handvoll Maslieben!", "tokens": ["Und", "ei\u00b7ne", "Hand\u00b7voll", "Mas\u00b7lie\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "$."], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}}, "stanza.4": {"line.1": {"text": "Er aber fing in Spiegel den Stral,", "tokens": ["Er", "a\u00b7ber", "fing", "in", "Spie\u00b7gel", "den", "Stral", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "APPR", "NN", "ART", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Und lie\u00df ihn zucken wie Flammen,", "tokens": ["Und", "lie\u00df", "ihn", "zu\u00b7cken", "wie", "Flam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "VVINF", "KOKOM", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Die ruppigen Gr\u00e4ser strich er zumal", "tokens": ["Die", "rup\u00b7pi\u00b7gen", "Gr\u00e4\u00b7ser", "strich", "er", "zu\u00b7mal"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "ADV"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und flocht sie sauber zusammen,", "tokens": ["Und", "flocht", "sie", "sau\u00b7ber", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "PTKVZ", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "An Steinen schleppt er sich krank und matt,", "tokens": ["An", "Stei\u00b7nen", "schleppt", "er", "sich", "krank", "und", "matt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "PRF", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "F\u00fcr ein Ruinchen am H\u00fcgel,", "tokens": ["F\u00fcr", "ein", "Ru\u00b7in\u00b7chen", "am", "H\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+---+-", "measure": "unknown.measure.tri"}, "line.7": {"text": "Dem Hasen k\u00e4mmt' er die Wolle glatt", "tokens": ["Dem", "Ha\u00b7sen", "k\u00e4mmt'", "er", "die", "Wol\u00b7le", "glatt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Und frisirt' den M\u00fccken die Fl\u00fcgel.", "tokens": ["Und", "fri\u00b7sirt'", "den", "M\u00fc\u00b7cken", "die", "Fl\u00fc\u00b7gel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.5": {"line.1": {"text": "So hat er mit saurem Schwei\u00df und M\u00fch'", "tokens": ["So", "hat", "er", "mit", "sau\u00b7rem", "Schwei\u00df", "und", "M\u00fch'"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "ADJA", "NN", "KON", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Das ganz Gemeine verbessert,", "tokens": ["Das", "ganz", "Ge\u00b7mei\u00b7ne", "ver\u00b7bes\u00b7sert", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "NN", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und klareres Wasser fand man nie,", "tokens": ["Und", "kla\u00b7re\u00b7res", "Was\u00b7ser", "fand", "man", "nie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "PIS", "ADV", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Als wo er schaufelt' und w\u00e4ssert',", "tokens": ["Als", "wo", "er", "schau\u00b7felt'", "und", "w\u00e4s\u00b7sert'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PWAV", "PPER", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Und wie's nun aller Edlen Manier,", "tokens": ["Und", "wie's", "nun", "al\u00b7ler", "Ed\u00b7len", "Ma\u00b7nier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Sich mild und nobel zu zeigen,", "tokens": ["Sich", "mild", "und", "no\u00b7bel", "zu", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "KON", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "So, seys Gestein, Mensch, oder Thier,", "tokens": ["So", ",", "seys", "Ge\u00b7stein", ",", "Mensch", ",", "o\u00b7der", "Thier", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "$,", "ADJA", "NN", "$,", "NN", "$,", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Er gab ihm von seinem Eigen.", "tokens": ["Er", "gab", "ihm", "von", "sei\u00b7nem", "Ei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Einst sa\u00df er mit seinem Werkger\u00e4th,", "tokens": ["Einst", "sa\u00df", "er", "mit", "sei\u00b7nem", "Werk\u00b7ge\u00b7r\u00e4\u00b7th", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Mit Scheere, Pinsel und Flasche,", "tokens": ["Mit", "Schee\u00b7re", ",", "Pin\u00b7sel", "und", "Fla\u00b7sche", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "In der eine schw\u00e4rzliche Lymphe steht,", "tokens": ["In", "der", "ei\u00b7ne", "schw\u00e4rz\u00b7li\u00b7che", "Lym\u00b7phe", "steht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Mit Spiegel, Feder und Tasche;", "tokens": ["Mit", "Spie\u00b7gel", ",", "Fe\u00b7der", "und", "Ta\u00b7sche", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Er sa\u00df und lauschte wie in der N\u00e4h", "tokens": ["Er", "sa\u00df", "und", "lauschte", "wie", "in", "der", "N\u00e4h"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "KOKOM", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mein Schimmelchen galoppiret;", "tokens": ["Mein", "Schim\u00b7mel\u00b7chen", "ga\u00b7lop\u00b7pi\u00b7ret", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.7": {"text": "Auf dem Finger pfiff er: \u201ePst, Pferdchen, he!\u201c", "tokens": ["Auf", "dem", "Fin\u00b7ger", "pfiff", "er", ":", "\u201e", "Pst", ",", "Pferd\u00b7chen", ",", "he", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "punct", "word", "punct", "word", "punct", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "$.", "$(", "NN", "$,", "NN", "$,", "FM", "$.", "$("], "meter": "+-+-+-++-+", "measure": "unknown.measure.hexa"}, "line.8": {"text": "Und wacker kam es trottiret.", "tokens": ["Und", "wa\u00b7cker", "kam", "es", "trot\u00b7ti\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Dann sprach der Edle: \u201edu w\u00e4rst schon gut,", "tokens": ["Dann", "sprach", "der", "Ed\u00b7le", ":", "\u201e", "du", "w\u00e4rst", "schon", "gut", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "$.", "$(", "PPER", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "'ne passable Rozinante,", "tokens": ["'ne", "pas\u00b7sab\u00b7le", "Ro\u00b7zi\u00b7nan\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "N\u00e4hm ich dich ernstlich in meine Hut,", "tokens": ["N\u00e4hm", "ich", "dich", "ernst\u00b7lich", "in", "mei\u00b7ne", "Hut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PRF", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Da\u00df ich den Koller dir bannte;", "tokens": ["Da\u00df", "ich", "den", "Kol\u00b7ler", "dir", "bann\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Ein leiser Traber \u2014 ein schmuckes Thier \u2014", "tokens": ["Ein", "lei\u00b7ser", "Tra\u00b7ber", "