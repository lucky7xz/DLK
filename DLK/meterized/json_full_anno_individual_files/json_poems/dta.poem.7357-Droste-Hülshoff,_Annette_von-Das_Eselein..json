unct"], "pos": ["VVFIN", "PPER", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Hinauf, hinunter die Wurzel entlang,", "tokens": ["Hin\u00b7auf", ",", "hin\u00b7un\u00b7ter", "die", "Wur\u00b7zel", "ent\u00b7lang", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Von der Kuppe bis an den R\u00fccken.", "tokens": ["Von", "der", "Kup\u00b7pe", "bis", "an", "den", "R\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "APPR", "ART", "NN", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}}, "stanza.12": {"line.1": {"text": "Dann spricht er freudig: \u201emein schmuckes Thier,", "tokens": ["Dann", "spricht", "er", "freu\u00b7dig", ":", "\u201e", "mein", "schmu\u00b7ckes", "Thier", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "$.", "$(", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Mein Zelter edel wie Keiner!\u201c", "tokens": ["Mein", "Zel\u00b7ter", "e\u00b7del", "wie", "Kei\u00b7ner", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "KOKOM", "PIS", "$.", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und eilends langt er den Spiegel herf\u00fcr:", "tokens": ["Und", "ei\u00b7lends", "langt", "er", "den", "Spie\u00b7gel", "her\u00b7f\u00fcr", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "\u201enun sieh, und freue dich deiner!", "tokens": ["\u201e", "nun", "sieh", ",", "und", "freu\u00b7e", "dich", "dei\u00b7ner", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "$,", "KON", "VVFIN", "PPER", "PPOSAT", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Nun bist ein Parader\u00f6\u00dflein, ba\u00df", "tokens": ["Nun", "bist", "ein", "Pa\u00b7ra\u00b7de\u00b7r\u00f6\u00df\u00b7lein", ",", "ba\u00df"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "$,", "ADV"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Wie Eines von M\u00fcnster bis Wesel.\u201c", "tokens": ["Wie", "Ei\u00b7nes", "von", "M\u00fcns\u00b7ter", "bis", "We\u00b7sel", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "PIS", "APPR", "NE", "APPR", "NN", "$.", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.7": {"text": "Der Schimmel blinzt, und schaut in's Glas, \u2014", "tokens": ["Der", "Schim\u00b7mel", "blinzt", ",", "und", "schaut", "in's", "Glas", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "KON", "VVFIN", "APPRART", "NN", "$,", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "O Himmel, da war er ein Esel!", "tokens": ["O", "Him\u00b7mel", ",", "da", "war", "er", "ein", "E\u00b7sel", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "ADV", "VAFIN", "PPER", "ART", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}}}}