{"textgrid.poem.54978": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Vorklage", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie nimmt ein leidenschaftlich Stammeln", "tokens": ["Wie", "nimmt", "ein", "lei\u00b7den\u00b7schaft\u00b7lich", "Stam\u00b7meln"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "ART", "ADJD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Geschrieben sich so seltsam aus!", "tokens": ["Ge\u00b7schrie\u00b7ben", "sich", "so", "selt\u00b7sam", "aus", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "ADV", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nun soll ich gar von Haus zu Haus", "tokens": ["Nun", "soll", "ich", "gar", "von", "Haus", "zu", "Haus"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "APPR", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die losen Bl\u00e4tter alle sammeln.", "tokens": ["Die", "lo\u00b7sen", "Bl\u00e4t\u00b7ter", "al\u00b7le", "sam\u00b7meln", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Was eine lange, weite Strecke", "tokens": ["Was", "ei\u00b7ne", "lan\u00b7ge", ",", "wei\u00b7te", "Stre\u00b7cke"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PWS", "ART", "ADV", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Im Leben voneinander stand,", "tokens": ["Im", "Le\u00b7ben", "von\u00b7ein\u00b7an\u00b7der", "stand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das kommt nun unter ", "tokens": ["Das", "kommt", "nun", "un\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ADV", "APPR"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Dem guten Leser in die Hand.", "tokens": ["Dem", "gu\u00b7ten", "Le\u00b7ser", "in", "die", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Doch sch\u00e4me dich nicht der Gebrechen,", "tokens": ["Doch", "sch\u00e4\u00b7me", "dich", "nicht", "der", "Ge\u00b7bre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Vollende schnell das kleine Buch;", "tokens": ["Voll\u00b7en\u00b7de", "schnell", "das", "klei\u00b7ne", "Buch", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Die Welt ist voller Widerspruch,", "tokens": ["Die", "Welt", "ist", "vol\u00b7ler", "Wi\u00b7der\u00b7spruch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und sollte sich's nicht widersprechen?", "tokens": ["Und", "soll\u00b7te", "sich's", "nicht", "wi\u00b7der\u00b7spre\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PIS", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"tex