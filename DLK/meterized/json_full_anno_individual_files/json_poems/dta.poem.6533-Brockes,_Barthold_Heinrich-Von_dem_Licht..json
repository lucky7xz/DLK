{"dta.poem.6533": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von dem Licht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ist solche Wolcke nun nur blo\u00df ein d\u00fcnner Dufft;", "tokens": ["Ist", "sol\u00b7che", "Wol\u00b7cke", "nun", "nur", "blo\u00df", "ein", "d\u00fcn\u00b7ner", "Dufft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "ADV", "ADV", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So mildert sich daran das Strahlen-reiche Feuer.", "tokens": ["So", "mil\u00b7dert", "sich", "da\u00b7ran", "das", "Strah\u00b7len\u00b7rei\u00b7che", "Feu\u00b7er", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "PAV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Es schw\u00e4cht sich, doch entweichts nicht g\u00e4ntzlich aus der Lufft.", "tokens": ["Es", "schw\u00e4cht", "sich", ",", "doch", "ent\u00b7weichts", "nicht", "g\u00e4ntz\u00b7lich", "aus", "der", "Lufft", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "$,", "ADV", "ADV", "PTKNEG", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dann dienet ihr der d\u00fcnne Dufft zum Schleyer.", "tokens": ["Dann", "die\u00b7net", "ihr", "der", "d\u00fcn\u00b7ne", "Dufft", "zum", "Schle\u00b7yer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Uns kommt die Sonne dann nicht anders vor,", "tokens": ["Uns", "kommt", "die", "Son\u00b7ne", "dann", "nicht", "an\u00b7ders", "vor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "PTKNEG", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Als s\u00e4h\u2019 man sie durch einen Flor,", "tokens": ["Als", "s\u00e4h'", "man", "sie", "durch", "ei\u00b7nen", "Flor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PIS", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und man vermag sodann, was sonst nicht kan geschehen,", "tokens": ["Und", "man", "ver\u00b7mag", "so\u00b7dann", ",", "was", "sonst", "nicht", "kan", "ge\u00b7sche\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "ADV", "$,", "PRELS", "ADV", "PTKNEG", "VMFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Sie ungeblendet anzusehen.", "tokens": ["Sie", "un\u00b7ge\u00b7blen\u00b7det", "an\u00b7zu\u00b7se\u00b7hen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVIZU", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}