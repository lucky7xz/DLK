{"dta.poem.1358": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Wunsch aus dem 10. und 119. Psalm.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Meine Seele liegt im Staube/ da\u00df mein Hertz f\u00fcr Gram", "tokens": ["Mei\u00b7ne", "See\u00b7le", "liegt", "im", "Stau\u00b7be", "/", "da\u00df", "mein", "Hertz", "f\u00fcr", "Gram"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "APPRART", "NN", "$(", "KOUS", "PPOSAT", "NN", "APPR", "NN"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.2": {"text": "verschmacht/", "tokens": ["ver\u00b7schmacht", "/"], "token_info": ["word", "punct"], "pos": ["VVPP", "$("], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Heb mein Haubt empor/ und st\u00e4rcke mich durch deines Trostes", "tokens": ["Heb", "mein", "Haubt", "em\u00b7por", "/", "und", "st\u00e4r\u00b7cke", "mich", "durch", "dei\u00b7nes", "Tros\u00b7tes"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "PPOSAT", "NN", "PTKVZ", "$(", "KON", "VVFIN", "PRF", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.4": {"text": "Macht.", "tokens": ["Macht", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Ja du h\u00f6rst/ HERR/ das Verlangen der betr\u00fcbten Seelen an.", "tokens": ["Ja", "du", "h\u00f6rst", "/", "HeRR", "/", "das", "Ver\u00b7lan\u00b7gen", "der", "be\u00b7tr\u00fcb\u00b7ten", "See\u00b7len", "an", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PPER", "VVFIN", "$(", "NN", "$(", "ART", "NN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "--++--+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.6": {"text": "Mein Hertz wei\u00df/ da\u00df deine G\u00fctte helffen will und helffen", "tokens": ["Mein", "Hertz", "wei\u00df", "/", "da\u00df", "dei\u00b7ne", "G\u00fct\u00b7te", "helf\u00b7fen", "will", "und", "helf\u00b7fen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "$(", "KOUS", "PPOSAT", "NN", "VVINF", "VMFIN", "KON", "VVINF"], "meter": "+++-+-+-+-+-+-", "measure": "unknown.measure.octa.plus"}, "line.7": {"text": "kan.", "tokens": ["kan", "."], "token_info": ["word", "punct"], "pos": ["VMFIN", "$."], "meter": "-", "measure": "single.down"}, "line.8": {"text": "Diesen Trost Immanuel/ will ich fest zu Sinne fassen/", "tokens": ["Die\u00b7sen", "Trost", "Im\u00b7ma\u00b7nu\u00b7el", "/", "will", "ich", "fest", "zu", "Sin\u00b7ne", "fas\u00b7sen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "NE", "$(", "VMFIN", "PPER", "ADJD", "APPR", "NN", "VVINF", "$("], "meter": "+-+--+-+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.9": {"text": "Zeit und Ort/ wenn/ wie es gutt/ deinem Willen \u00fcberlassen.", "tokens": ["Zeit", "und", "Ort", "/", "wenn", "/", "wie", "es", "gutt", "/", "dei\u00b7nem", "Wil\u00b7len", "\u00fc\u00b7ber\u00b7las\u00b7sen", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$(", "KOUS", "$(", "PWAV", "PPER", "ADJD", "$(", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+--+-+-+-+-+-", "measure": "trochaic.septa.relaxed"}}}}}