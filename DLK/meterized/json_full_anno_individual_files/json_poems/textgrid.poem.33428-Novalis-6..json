{"textgrid.poem.33428": {"metadata": {"author": {"name": "Novalis", "birth": "N.A.", "death": "N.A."}, "title": "6.", "genre": "verse", "period": "N.A.", "pub_year": 1787, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hinunter in der Erde Schoo\u00df,", "tokens": ["Hin\u00b7un\u00b7ter", "in", "der", "Er\u00b7de", "Schoo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Weg aus des Lichtes Reichen,", "tokens": ["Weg", "aus", "des", "Lich\u00b7tes", "Rei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Der Schmerzen Wuth und wilder Sto\u00df", "tokens": ["Der", "Schmer\u00b7zen", "Wuth", "und", "wil\u00b7der", "Sto\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ist froher Abfahrt Zeichen.", "tokens": ["Ist", "fro\u00b7her", "Ab\u00b7fahrt", "Zei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wir kommen in dem engen Kahn", "tokens": ["Wir", "kom\u00b7men", "in", "dem", "en\u00b7gen", "Kahn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Geschwind am Himmelsufer an.", "tokens": ["Ge\u00b7schwind", "am", "Him\u00b7mel\u00b7su\u00b7fer", "an", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Gelobt sey uns die ewge Nacht,", "tokens": ["Ge\u00b7lobt", "sey", "uns", "die", "ew\u00b7ge", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gelobt der ewge Schlummer.", "tokens": ["Ge\u00b7lobt", "der", "ew\u00b7ge", "Schlum\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wohl hat der Tag uns warm gemacht,", "tokens": ["Wohl", "hat", "der", "Tag", "uns", "warm", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und welk der lange Kummer.", "tokens": ["Und", "welk", "der", "lan\u00b7ge", "Kum\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die Lust der Fremde ging uns aus,", "tokens": ["Die", "Lust", "der", "Frem\u00b7de", "ging", "uns", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Zum Vater wollen wir nach Haus.", "tokens": ["Zum", "Va\u00b7ter", "wol\u00b7len", "wir", "nach", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VMFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Was sollen wir auf dieser Welt", "tokens": ["Was", "sol\u00b7len", "wir", "auf", "die\u00b7ser", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VMFIN", "PPER", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit unsrer Lieb' und Treue.", "tokens": ["Mit", "uns\u00b7rer", "Lieb'", "und", "Treu\u00b7e", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das Alte wird hintangestellt,", "tokens": ["Das", "Al\u00b7te", "wird", "hin\u00b7tan\u00b7ge\u00b7stellt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Was soll uns dann das Neue.", "tokens": ["Was", "soll", "uns", "dann", "das", "Neu\u00b7e", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "ADV", "ART", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "O! einsam steht und tiefbetr\u00fcbt,", "tokens": ["O", "!", "ein\u00b7sam", "steht", "und", "tief\u00b7be\u00b7tr\u00fcbt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "ADJD", "VVFIN", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wer hei\u00df und fromm die Vorzeit liebt.", "tokens": ["Wer", "hei\u00df", "und", "fromm", "die", "Vor\u00b7zeit", "liebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "KON", "ADJD", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Die Vorzeit wo die Sinne licht", "tokens": ["Die", "Vor\u00b7zeit", "wo", "die", "Sin\u00b7ne", "licht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PWAV", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In hohen Flammen brannten,", "tokens": ["In", "ho\u00b7hen", "Flam\u00b7men", "brann\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Des Vaters Hand und Angesicht", "tokens": ["Des", "Va\u00b7ters", "Hand", "und", "An\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Menschen noch erkannten.", "tokens": ["Die", "Men\u00b7schen", "noch", "er\u00b7kann\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und hohen Sinns, einf\u00e4ltiglich", "tokens": ["Und", "ho\u00b7hen", "Sinns", ",", "ein\u00b7f\u00e4l\u00b7tig\u00b7lich"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["KON", "ADJA", "NN", "$,", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Noch mancher seinem Urbild glich.", "tokens": ["Noch", "man\u00b7cher", "sei\u00b7nem", "Ur\u00b7bild", "glich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Die Vorzeit, wo noch bl\u00fcthenreich", "tokens": ["Die", "Vor\u00b7zeit", ",", "wo", "noch", "bl\u00fc\u00b7then\u00b7reich"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PWAV", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Uralte St\u00e4mme prangten,", "tokens": ["Ur\u00b7al\u00b7te", "St\u00e4m\u00b7me", "prang\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und Kinder f\u00fcr das Himmelreich", "tokens": ["Und", "Kin\u00b7der", "f\u00fcr", "das", "Him\u00b7mel\u00b7reich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "nach Quaal und Tod verlangten.", "tokens": ["nach", "Qua\u00b7al", "und", "Tod", "ver\u00b7lang\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Und wenn auch Lust und Leben sprach,", "tokens": ["Und", "wenn", "auch", "Lust", "und", "Le\u00b7ben", "sprach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADV", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Doch manches Herz f\u00fcr Liebe brach.", "tokens": ["Doch", "man\u00b7ches", "Herz", "f\u00fcr", "Lie\u00b7be", "brach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Die Vorzeit, wo in Jugendglut", "tokens": ["Die", "Vor\u00b7zeit", ",", "wo", "in", "Ju\u00b7gend\u00b7glut"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PWAV", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gott selbst sich kundgegeben", "tokens": ["Gott", "selbst", "sich", "kund\u00b7ge\u00b7ge\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ADV", "PRF", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und fr\u00fchem Tod in Liebesmuth", "tokens": ["Und", "fr\u00fc\u00b7hem", "Tod", "in", "Lie\u00b7bes\u00b7muth"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Geweiht sein s\u00fc\u00dfes Leben.", "tokens": ["Ge\u00b7weiht", "sein", "s\u00fc\u00b7\u00dfes", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und Angst und Schmerz nicht von sich trieb,", "tokens": ["Und", "Angst", "und", "Schmerz", "nicht", "von", "sich", "trieb", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "PTKNEG", "APPR", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Damit er uns nur theuer blieb.", "tokens": ["Da\u00b7mit", "er", "uns", "nur", "theu\u00b7er", "blieb", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Mit banger Sehnsucht sehn wir sie", "tokens": ["Mit", "ban\u00b7ger", "Sehn\u00b7sucht", "sehn", "wir", "sie"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In dunkle Nacht geh\u00fcllet,", "tokens": ["In", "dunk\u00b7le", "Nacht", "ge\u00b7h\u00fcl\u00b7let", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "In dieser Zeitlichkeit wird nie", "tokens": ["In", "die\u00b7ser", "Zeit\u00b7lich\u00b7keit", "wird", "nie"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "VAFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der hei\u00dfe Durst gestillet.", "tokens": ["Der", "hei\u00b7\u00dfe", "Durst", "ge\u00b7stil\u00b7let", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wir m\u00fcssen nach der Heymath gehn,", "tokens": ["Wir", "m\u00fcs\u00b7sen", "nach", "der", "Hey\u00b7math", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Um diese heilge Zeit zu sehn.", "tokens": ["Um", "die\u00b7se", "heil\u00b7ge", "Zeit", "zu", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PDAT", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Was h\u00e4lt noch unsre R\u00fcckkehr auf,", "tokens": ["Was", "h\u00e4lt", "noch", "uns\u00b7re", "R\u00fcck\u00b7kehr", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Liebsten ruhn schon lange.", "tokens": ["Die", "Liebs\u00b7ten", "ruhn", "schon", "lan\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ihr Grab schlie\u00dft unsern Lebenslauf,", "tokens": ["Ihr", "Grab", "schlie\u00dft", "un\u00b7sern", "Le\u00b7bens\u00b7lauf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nun wird uns weh und bange.", "tokens": ["Nun", "wird", "uns", "weh", "und", "ban\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PTKVZ", "KON", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Zu suchen haben wir nichts mehr \u2013", "tokens": ["Zu", "su\u00b7chen", "ha\u00b7ben", "wir", "nichts", "mehr", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "VAFIN", "PPER", "PIS", "ADV", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das Herz ist satt \u2013 die Welt ist leer.", "tokens": ["Das", "Herz", "ist", "satt", "\u2013", "die", "Welt", "ist", "leer", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$(", "ART", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Unendlich und geheimni?voll", "tokens": ["Un\u00b7end\u00b7lich", "und", "ge\u00b7heim\u00b7ni", "?", "voll"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["ADJD", "KON", "NE", "$.", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Durchstr\u00f6mt uns s\u00fc\u00dfer Schauer \u2013", "tokens": ["Durch\u00b7str\u00f6mt", "uns", "s\u00fc\u00b7\u00dfer", "Schau\u00b7er", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJA", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mir d\u00e4ucht, aus tiefen Fernen scholl", "tokens": ["Mir", "d\u00e4ucht", ",", "aus", "tie\u00b7fen", "Fer\u00b7nen", "scholl"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "APPR", "ADJA", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Echo unsrer Trauer.", "tokens": ["Ein", "E\u00b7cho", "uns\u00b7rer", "Trau\u00b7er", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die Lieben sehnen sich wohl auch", "tokens": ["Die", "Lie\u00b7ben", "seh\u00b7nen", "sich", "wohl", "auch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "VVFIN", "PRF", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und sandten uns der Sehnsucht Hauch.", "tokens": ["Und", "sand\u00b7ten", "uns", "der", "Sehn\u00b7sucht", "Hauch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Hinunter zu der s\u00fc\u00dfen Braut,", "tokens": ["Hin\u00b7un\u00b7ter", "zu", "der", "s\u00fc\u00b7\u00dfen", "Braut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zu Jesus, dem Geliebten \u2013", "tokens": ["Zu", "Je\u00b7sus", ",", "dem", "Ge\u00b7lieb\u00b7ten", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "ART", "NN", "$("], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Getrost, die Abendd\u00e4mmrung graut", "tokens": ["Ge\u00b7trost", ",", "die", "A\u00b7bend\u00b7d\u00e4mm\u00b7rung", "graut"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den Liebenden, Betr\u00fcbten.", "tokens": ["Den", "Lie\u00b7ben\u00b7den", ",", "Be\u00b7tr\u00fcb\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ein Traum bricht unsre Banden los", "tokens": ["Ein"