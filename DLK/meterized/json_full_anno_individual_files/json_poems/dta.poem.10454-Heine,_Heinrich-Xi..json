{"dta.poem.10454": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Xi.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Der Sturm spielt auf zum Tanze,", "tokens": ["Der", "Sturm", "spielt", "auf", "zum", "Tan\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Er pfeift und saust und br\u00fcllt;", "tokens": ["Er", "pfeift", "und", "saust", "und", "br\u00fcllt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Heisa, wie springt das Schifflein!", "tokens": ["Hei\u00b7sa", ",", "wie", "springt", "das", "Schif\u00b7flein", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "VVFIN", "ART", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Die Nacht ist lustig und wild.", "tokens": ["Die", "Nacht", "ist", "lus\u00b7tig", "und", "wild", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Ein lebendes Wassergebirge", "tokens": ["Ein", "le\u00b7ben\u00b7des", "Was\u00b7ser\u00b7ge\u00b7bir\u00b7ge"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Bildet die tosende See;", "tokens": ["Bil\u00b7det", "die", "to\u00b7sen\u00b7de", "See", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Hier g\u00e4hnt ein schwarzer Abgrund,", "tokens": ["Hier", "g\u00e4hnt", "ein", "schwar\u00b7zer", "Ab\u00b7grund", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dort th\u00fcrmt es sich weit in die H\u00f6h'.", "tokens": ["Dort", "th\u00fcrmt", "es", "sich", "weit", "in", "die", "H\u00f6h'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.3": {"line.1": {"text": "Ein Fluchen, Erbrechen und Beten,", "tokens": ["Ein", "Flu\u00b7chen", ",", "Er\u00b7bre\u00b7chen", "und", "Be\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Schallt aus der Kaj\u00fcte heraus;", "tokens": ["Schallt", "aus", "der", "Ka\u00b7j\u00fc\u00b7te", "he\u00b7raus", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Ich halte mich fest am Mastbaum,", "tokens": ["Ich", "hal\u00b7te", "mich", "fest", "am", "Mast\u00b7baum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "APPRART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und w\u00fcnsche: w\u00e4r ich zu Haus.", "tokens": ["Und", "w\u00fcn\u00b7sche", ":", "w\u00e4r", "ich", "zu", "Haus", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "VAFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}}}}