{"textgrid.poem.57879": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Ein Wunder", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun wird der Bahnhof nach dem Raschplatz", "tokens": ["Nun", "wird", "der", "Bahn\u00b7hof", "nach", "dem", "Raschplatz"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ge\u00f6ffnet; ja, es ist gegl\u00fcckt,", "tokens": ["Ge\u00b7\u00f6ff\u00b7net", ";", "ja", ",", "es", "ist", "ge\u00b7gl\u00fcckt", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$.", "PTKANT", "$,", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nach jahrelangen Petitionen", "tokens": ["Nach", "jah\u00b7re\u00b7lan\u00b7gen", "Pe\u00b7ti\u00b7ti\u00b7o\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Hat es die Oststadt durchgedr\u00fcckt.", "tokens": ["Hat", "es", "die", "Ost\u00b7stadt", "durch\u00b7ge\u00b7dr\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Doch schwere Arbeit hat's gekostet,", "tokens": ["Doch", "schwe\u00b7re", "Ar\u00b7beit", "hat's", "ge\u00b7kos\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Viel Reden, Tinte und Papier,", "tokens": ["Viel", "Re\u00b7den", ",", "Tin\u00b7te",