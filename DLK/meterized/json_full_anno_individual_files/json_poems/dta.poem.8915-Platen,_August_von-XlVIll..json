{"dta.poem.8915": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "XlVIll.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Mit edler Stirn, mit Augen voll von Treue,", "tokens": ["Mit", "ed\u00b7ler", "Stirn", ",", "mit", "Au\u00b7gen", "voll", "von", "Treu\u00b7e", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "APPR", "NN", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "An Jahren jung und reizend wie das Neue,", "tokens": ["An", "Jah\u00b7ren", "jung", "und", "rei\u00b7zend", "wie", "das", "Neu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "KON", "ADJD", "KOKOM", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So fand ich dich, so fand ich meine Sorgen.", "tokens": ["So", "fand", "ich", "dich", ",", "so", "fand", "ich", "mei\u00b7ne", "Sor\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "$,", "ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "O w\u00e4r' ich schon an deiner Brust geborgen,", "tokens": ["O", "w\u00e4r'", "ich", "schon", "an", "dei\u00b7ner", "Brust", "ge\u00b7bor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wo ich mich sammle, wenn ich mich zerstreue!", "tokens": ["Wo", "ich", "mich", "samm\u00b7le", ",", "wenn", "ich", "mich", "zer\u00b7streu\u00b7e", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "VVFIN", "$,", "KOUS", "PPER", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "O w\u00e4re schon bezwungen diese Scheue,", "tokens": ["O", "w\u00e4\u00b7re", "schon", "be\u00b7zwun\u00b7gen", "die\u00b7se", "Scheu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ADV", "VVPP", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die unsern Bund vertagt von heut auf morgen!", "tokens": ["Die", "un\u00b7sern", "Bund", "ver\u00b7tagt", "von", "heut", "auf", "mor\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVPP", "APPR", "ADV", "APPR", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Was fliehst du mich? Vermagst du mich zu hassen?", "tokens": ["Was", "fliehst", "du", "mich", "?", "Ver\u00b7magst", "du", "mich", "zu", "has\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PRF", "$.", "VVFIN", "PPER", "PRF", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Was qu\u00e4lst du so durch deiner Huld Verschweigung", "tokens": ["Was", "qu\u00e4lst", "du", "so", "durch", "dei\u00b7ner", "Huld", "Ver\u00b7schwei\u00b7gung"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Den Liebevollen, der sich f\u00fchlt verlassen?", "tokens": ["Den", "Lie\u00b7be\u00b7vol\u00b7len", ",", "der", "sich", "f\u00fchlt", "ver\u00b7las\u00b7sen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PRF", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Beym ersten Zeichen deiner k\u00fcnft'gen Neigung", "tokens": ["Beym", "ers\u00b7ten", "Zei\u00b7chen", "dei\u00b7ner", "k\u00fcnft'\u00b7gen", "Nei\u00b7gung"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wird eine bange Wonne mich erfassen,", "tokens": ["Wird", "ei\u00b7ne", "ban\u00b7ge", "Won\u00b7ne", "mich", "er\u00b7fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie einen F\u00fcrsten bey der Thronbesteigung.", "tokens": ["Wie", "ei\u00b7nen", "F\u00fcrs\u00b7ten", "bey", "der", "Thron\u00b7be\u00b7stei\u00b7gung", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}