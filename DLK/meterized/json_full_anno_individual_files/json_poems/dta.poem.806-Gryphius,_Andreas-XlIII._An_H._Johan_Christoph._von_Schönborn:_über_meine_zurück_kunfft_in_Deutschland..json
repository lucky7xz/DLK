{"dta.poem.806": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "XlIII.  \n An H. Johan Christoph. von Sch\u00f6nborn: \u00fcber  \n meine zur\u00fcck kunfft in Deutschland.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Der/ den jhr offt gew\u00fcndscht/ der der Euch offt begehret ", "tokens": ["Der", "/", "den", "jhr", "offt", "ge\u00b7w\u00fcnd\u00b7scht", "/", "der", "der", "Euch", "offt", "be\u00b7ge\u00b7hret"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "$(", "ART", "PPER", "ADV", "VVPP", "$(", "ART", "ART", "PPER", "ADV", "VVFIN"], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Der vnter frembder schar den ne\u00fcndt\u1ebd Fr\u00fchling zehlt", "tokens": ["Der", "vn\u00b7ter", "fremb\u00b7der", "schar", "den", "ne\u00b7\u00fcndt\u1ebd", "Fr\u00fch\u00b7ling", "zehlt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Den seiner Gr\u00e4ntzen Angst/ abwesend auch gequ\u00e4lt/", "tokens": ["Den", "sei\u00b7ner", "Gr\u00e4nt\u00b7zen", "Angst", "/", "ab\u00b7we\u00b7send", "auch", "ge\u00b7qu\u00e4lt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "NN", "$(", "ADJD", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der den der Feinde Neydt/ der Freunde Todt beschweret:", "tokens": ["Der", "den", "der", "Fein\u00b7de", "Neydt", "/", "der", "Freun\u00b7de", "Todt", "be\u00b7schwe\u00b7ret", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ART", "NN", "NE", "$(", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Der/ nun der Erden Haupt/ jhm sein Gesicht gewehret/", "tokens": ["Der", "/", "nun", "der", "Er\u00b7den", "Haupt", "/", "jhm", "sein", "Ge\u00b7sicht", "ge\u00b7weh\u00b7ret", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$(", "ADV", "ART", "NN", "NN", "$(", "PPER", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nun nach der R\u00f6mischen pracht kaum was zu schawen", "tokens": ["Nun", "nach", "der", "R\u00f6\u00b7mi\u00b7schen", "pracht", "kaum", "was", "zu", "scha\u00b7wen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "ADV", "PIS", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "fehlt/ ", "tokens": ["fehlt", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Nun sich jhm ", "tokens": ["Nun", "sich", "jhm"], "token_info": ["word", "word", "word"], "pos": ["ADV", "PRF", "PPER"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Hat seinen weg zu r\u00fcck\u2019 ins Vaterland gekehret.", "tokens": ["Hat", "sei\u00b7nen", "weg", "zu", "r\u00fcck", "ins", "Va\u00b7ter\u00b7land", "ge\u00b7keh\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "ADV", "APPR", "NE", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ins Vaterland! ach nein/ er misset die bekandten/", "tokens": ["Ins", "Va\u00b7ter\u00b7land", "!", "ach", "nein", "/", "er", "mis\u00b7set", "die", "be\u00b7kand\u00b7ten", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$.", "XY", "PTKANT", "$(", "PPER", "VVFIN", "ART", "ADJA", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Er findet kaum die grufft so vieler anverwandten", "tokens": ["Er", "fin\u00b7det", "kaum", "die", "grufft", "so", "vie\u00b7ler", "an\u00b7ver\u00b7wand\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "ADV", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Weil auch die Erden selbst durch glutt vnd fluth ver-", "tokens": ["Weil", "auch", "die", "Er\u00b7den", "selbst", "durch", "glutt", "vnd", "fluth", "ver"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "NN", "ADV", "APPR", "ADJD", "KON", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "geht.", "tokens": ["geht", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-", "measure": "single.down"}}, "stanza.4": {"line.1": {"text": "Was wil Er denn bey Euch? was hat Er vor genommen?", "tokens": ["Was", "wil", "Er", "denn", "bey", "Euch", "?", "was", "hat", "Er", "vor", "ge\u00b7nom\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "ADV", "APPR", "PPER", "$.", "PWS", "VAFIN", "PPER", "APPR", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Er sucht den Geist/ der noch/ nun alles vmb-ist kommen:", "tokens": ["Er", "sucht", "den", "Geist", "/", "der", "noch", "/", "nun", "al\u00b7les", "vm\u00b7bist", "kom\u00b7men", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$(", "ART", "ADV", "$(", "ADV", "PIS", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Auf fester Trewe grund auffrichtig vor jhn steht.", "tokens": ["Auf", "fes\u00b7ter", "Tre\u00b7we", "grund", "auf\u00b7frich\u00b7tig", "vor", "jhn", "steht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ADJD", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}}}}}