", "die", "Welt", "so", "s\u00fcs\u00b7se", "singt", ",", "drauff", "traut", "und", "si\u00b7cher", "hofft", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "$,", "PRELS", "ART", "NN", "ADV", "ADJA", "VVFIN", "$,", "PAV", "VVFIN", "KON", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.5": {"text": "Der hat nur Schaum, der nimt nur Schleim; es ist nur Leckerey;", "tokens": ["Der", "hat", "nur", "Schaum", ",", "der", "nimt", "nur", "Schleim", ";", "es", "ist", "nur", "Le\u00b7cke\u00b7rey", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "NN", "$,", "PRELS", "VVFIN", "ADV", "NN", "$.", "PPER", "VAFIN", "ADV", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.6": {"text": "Der Schmack ist gut; doch weist sichs klar, die Krafft ist nicht dabey.", "tokens": ["Der", "Schmack", "ist", "gut", ";", "doch", "weist", "sichs", "klar", ",", "die", "Krafft", "ist", "nicht", "da\u00b7bey", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$.", "ADV", "VVFIN", "PIS", "ADJD", "$,", "ART", "NN", "VAFIN", "PTKNEG", "PAV", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}, "stanza.2": {"line.1": {"text": "Der Zucker ist ietzt so gemein; Fisch, Vogel, Thier und Frucht", "tokens": ["Der", "Zu\u00b7cker", "ist", "ietzt", "so", "ge\u00b7mein", ";", "Fisch", ",", "Vo\u00b7gel", ",", "Thier", "und", "Frucht"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADV", "ADJD", "$.", "NN", "$,", "NE", "$,", "NN", "KON", "NN"], "meter