tokens": ["Der", "ist", "ein", "Schwei\u00b7zer", "und", "ein", "Christ", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Wer seiner V\u00e4ter Tugend ehrt,", "tokens": ["Wer", "sei\u00b7ner", "V\u00e4\u00b7ter", "Tu\u00b7gend", "ehrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie aus\u00fcbt und sie andre lehrt,", "tokens": ["Sie", "aus\u00b7\u00fcbt", "und", "sie", "and\u00b7re", "lehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "PPER", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das Gute sch\u00fctzt, dem B\u00f6sen wehrt,", "tokens": ["Das", "Gu\u00b7te", "sch\u00fctzt", ",", "dem", "B\u00f6\u00b7sen", "wehrt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Des Schmeichlers Stimme niemals h\u00f6rt,", "tokens": ["Des", "Schmeich\u00b7lers", "Stim\u00b7me", "nie\u00b7mals", "h\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und Treu h\u00e4lt, wenn er auch nicht schw\u00f6rt,", "tokens": ["Und", "Treu", "h\u00e4lt", ",", "wenn", "er", "auch", "nicht", "schw\u00f6rt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "$,", "KOUS", "PPER", "ADV", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der ist des Helden Namens werth!", "tokens": ["Der", "ist", "des", "Hel\u00b7den", "Na\u00b7mens", "werth", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Wen Vieler Gl\u00fcck und Sicherheit", "tokens": ["Wen", "Vie\u00b7ler", "Gl\u00fcck", "und", "Si\u00b7cher\u00b7heit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "PIAT", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mehr als sein eigen Gl\u00fcck erfreut:", "tokens": ["Mehr", "als", "sein", "ei\u00b7gen", "Gl\u00fcck", "er\u00b7freut", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "KOKOM", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wen keine sch\u00f6ne That gereut,", "tokens": ["Wen", "kei\u00b7ne", "sch\u00f6\u00b7ne", "That", "ge\u00b7reut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIAT", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wer fr\u00fche den Tyrannen dr\u00e4ut,", "tokens": ["Wer", "fr\u00fc\u00b7he", "den", "Ty\u00b7ran\u00b7nen", "dr\u00e4ut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dem Laster gleich die Knechtschaft scheut;", "tokens": ["Dem", "Las\u00b7ter", "gleich", "die", "Knecht\u00b7schaft", "scheut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der, der hat Schweizerredlichkeit!", "tokens": ["Der", ",", "der", "hat", "Schwei\u00b7zer\u00b7red\u00b7lich\u00b7keit", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "$,", "PRELS", "VAFIN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Wer immer, wo er stehn soll, steht", "tokens": ["Wer", "im\u00b7mer", ",", "wo", "er", "stehn", "soll", ",", "steht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["PWS", "ADV", "$,", "PWAV", "PPER", "VVINF", "VMFIN", "$,", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sich niemals \u00fcber Andre bl\u00e4ht,", "tokens": ["Sich", "nie\u00b7mals", "\u00fc\u00b7ber", "And\u00b7re", "bl\u00e4ht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "APPR", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Den graden Weg in allem geht,", "tokens": ["Den", "gra\u00b7den", "Weg", "in", "al\u00b7lem", "geht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gold, Wollust, \u00dcppigkeit verschm\u00e4ht,", "tokens": ["Gold", ",", "Wol\u00b7lust", ",", "\u00dcp\u00b7pig\u00b7keit", "ver\u00b7schm\u00e4ht", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Selbst erndtet, was er selber s\u00e4t;", "tokens": ["Selbst", "ernd\u00b7tet", ",", "was", "er", "sel\u00b7ber", "s\u00e4t", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "$,", "PWS", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ist \u00fcber K\u00f6nige erh\u00f6ht!", "token