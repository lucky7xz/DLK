{"textgrid.poem.39322": {"metadata": {"author": {"name": "Lavater, Johann Kaspar", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wer, Schweizer, wer hat Schweizerblut?", "genre": "verse", "period": "N.A.", "pub_year": 1770, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer, Schweizer, wer hat Schweizerblut?", "tokens": ["Wer", ",", "Schwei\u00b7zer", ",", "wer", "hat", "Schwei\u00b7zer\u00b7blut", "?"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "$,", "NN", "$,", "PWS", "VAFIN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der, der mit Ernst und frohem Muth", "tokens": ["Der", ",", "der", "mit", "Ernst", "und", "fro\u00b7hem", "Muth"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "$,", "PRELS", "APPR", "NE", "KON", "ADJA", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Dem Vaterlande Gutes thut,", "tokens": ["Dem", "Va\u00b7ter\u00b7lan\u00b7de", "Gu\u00b7tes", "thut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In seinem Schoo\u00dfe friedlich ruht;", "tokens": ["In", "sei\u00b7nem", "Schoo\u00b7\u00dfe", "fried\u00b7lich", "ruht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Nicht f\u00fcrchtet seiner Feinde Wut;", "tokens": ["Nicht", "f\u00fcrch\u00b7tet", "sei\u00b7ner", "Fein\u00b7de", "Wut", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "In dem flie\u00dft reines Schweizerblut.", "tokens"