{"dta.poem.2115": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die sch\u00f6ne Welt.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Warum willt du denn, lieber Mensch, dem Gott, der die-", "tokens": ["Wa\u00b7rum", "willt", "du", "denn", ",", "lie\u00b7ber", "Mensch", ",", "dem", "Gott", ",", "der", "die"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "VMFIN", "PPER", "ADV", "$,", "ADV", "NN", "$,", "ART", "NN", "$,", "ART", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "se sch\u00f6ne Welt", "tokens": ["se", "sch\u00f6\u00b7ne", "Welt"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Nur dir zu Gut so herrlich schm\u00fcckt, und dem nur deine", "tokens": ["Nur", "dir", "zu", "Gut", "so", "herr\u00b7lich", "schm\u00fcckt", ",", "und", "dem", "nur", "dei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "PTKA", "ADJD", "ADV", "ADJD", "VVFIN", "$,", "KON", "ART", "ADV", "PPOSAT"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Lust gef\u00e4llt,", "tokens": ["Lust", "ge\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Zur Ehr\u2019 und Lust, dich nicht vergn\u00fcgen? Warum soll", "tokens": ["Zur", "Ehr'", "und", "Lust", ",", "dich", "nicht", "ver\u00b7gn\u00fc\u00b7gen", "?", "Wa\u00b7rum", "soll"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["APPRART", "NN", "KON", "NN", "$,", "PPER", "PTKNEG", "VVINF", "$.", "PWAV", "VMFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "seiner Gottheit Schein,", "tokens": ["sei\u00b7ner", "Got\u00b7theit", "Schein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Der aus den sch\u00f6nen Werken stralet, dir, wie dem Vieh,", "tokens": ["Der", "aus", "den", "sch\u00f6\u00b7nen", "Wer\u00b7ken", "stra\u00b7let", ",", "dir", ",", "wie", "dem", "Vieh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "ADJA", "NN", "VVFIN", "$,", "PPER", "$,", "PWAV", "ART", "NN", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "verborgen seyn?", "tokens": ["ver\u00b7bor\u00b7gen", "seyn", "?"], "token_info": ["word", "word", "punct"], "pos": ["VVPP", "VAINF", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Er wollte Weisheit, Lieb und Macht durch deine Sinnen", "tokens": ["Er", "woll\u00b7te", "Weis\u00b7heit", ",", "Lieb", "und", "Macht", "durch", "dei\u00b7ne", "Sin\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "NN", "$,", "NN", "KON", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "dir entdecken:", "tokens": ["dir", "ent\u00b7de\u00b7cken", ":"], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.11": {"text": "Du willt zu seinem Ruhm nicht sehn, nicht h\u00f6ren, f\u00fch-", "tokens": ["Du", "willt", "zu", "sei\u00b7nem", "Ruhm", "nicht", "sehn", ",", "nicht", "h\u00f6\u00b7ren", ",", "f\u00fch"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PPER", "VMFIN", "APPR", "PPOSAT", "NN", "PTKNEG", "VVINF", "$,", "PTKNEG", "VVINF", "$,", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "len, riechen, schmecken.", "tokens": ["len", ",", "rie\u00b7chen", ",", "schme\u00b7cken", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["VVINF", "$,", "VVFIN", "$,", "VVFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}}}}