{"dta.poem.13183": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Z ehndes  B uch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wir beyde traten auch in diese Reihen ein,", "tokens": ["Wir", "bey\u00b7de", "tra\u00b7ten", "auch", "in", "die\u00b7se", "Rei\u00b7hen", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "VVFIN", "ADV", "APPR", "PDAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich w\u00fcnschte, da\u00df ich k\u00f6nnt\u2019 an allen Orten seyn;", "tokens": ["Ich", "w\u00fcnschte", ",", "da\u00df", "ich", "k\u00f6nnt'", "an", "al\u00b7len", "Or\u00b7ten", "seyn", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "VMFIN", "APPR", "PIAT", "NN", "VAINF", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Wir suchten an dem Hof uns nahe zu bequemen,", "tokens": ["Wir", "such\u00b7ten", "an", "dem", "Hof", "uns", "na\u00b7he", "zu", "be\u00b7que\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "PPER", "ADJD", "APPR", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Um, was geschehen sollt, umst\u00e4ndlich zu vernehmen.", "tokens": ["Um", ",", "was", "ge\u00b7sche\u00b7hen", "sollt", ",", "um\u00b7st\u00e4nd\u00b7lich", "zu", "ver\u00b7neh\u00b7men", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUI", "$,", "PWS", "VVINF", "VMFIN", "$,", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}