{"textgrid.poem.45935": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Lureley", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Singet leise, leise, leise,", "tokens": ["Sin\u00b7get", "lei\u00b7se", ",", "lei\u00b7se", ",", "lei\u00b7se", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VVFIN", "ADJD", "$,", "ADJD", "$,", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Singt ein fl\u00fcsternd Wiegenlied,", "tokens": ["Singt", "ein", "fl\u00fcs\u00b7ternd", "Wie\u00b7gen\u00b7lied", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJD", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Von dem Monde lernt die Weise,", "tokens": ["Von", "dem", "Mon\u00b7de", "lernt", "die", "Wei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NE", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Der so still am Himmel zieht.", "tokens": ["Der", "so", "still", "am", "Him\u00b7mel", "zieht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Denn es schlummern in dem Rheine", "tokens": ["Denn", "es", "schlum\u00b7mern", "in", "dem", "Rhei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ART", "NE"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Jetzt die lieben Kindlein klein,", "tokens": ["Jetzt", "die", "lie\u00b7ben", "Kin\u00b7dlein", "klein", ","], "token_info": ["word", "word", "w