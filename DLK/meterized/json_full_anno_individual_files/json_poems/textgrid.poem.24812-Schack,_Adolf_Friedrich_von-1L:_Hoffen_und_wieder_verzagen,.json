{"textgrid.poem.24812": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Hoffen und wieder verzagen,", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hoffen und wieder verzagen,", "tokens": ["Hof\u00b7fen", "und", "wie\u00b7der", "ver\u00b7za\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADV", "VVINF", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Harrend lauschen vor ihrem Balkon,", "tokens": ["Har\u00b7rend", "lau\u00b7schen", "vor", "ih\u00b7rem", "Bal\u00b7kon", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVINF", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Ob nicht, vom Winde getragen,", "tokens": ["Ob", "nicht", ",", "vom", "Win\u00b7de", "ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "$,", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Zu mir dringe von ihr ein Ton,", "tokens": ["Zu", "mir", "drin\u00b7ge", "von", "ihr", "ein", "Ton", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "APPR", "PPOSAT", "ART", "NN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Also reihen seit Monden schon", "tokens": ["Al\u00b7so", "rei\u00b7hen", "seit", "Mon\u00b7den", "schon"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "NN", "ADV"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.6": {"text": "Tage sich mir zu Tagen.", "tokens": ["Ta\u00b7ge", "sich", "mir", "zu", "Ta\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "PPER", "APPR", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.2": {"line.1": {"text": "Sp\u00e4t, wenn stumm und stummer", "tokens": ["Sp\u00e4t", ",", "wenn", "stumm", "und", "stum\u00b7mer"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "$,", "KOUS", "ADJD", "KON", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Nacht sich lagert im \u00f6den Revier,", "tokens": ["Nacht", "sich", "la\u00b7gert", "im", "\u00f6\u00b7den", "Re\u00b7vier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+--+---", "measure": "trochaic.tri.relaxed"}, "line.3": {"text": "Senken zu kurzem Schlummer", "tokens": ["Sen\u00b7ken", "zu", "kur\u00b7zem", "Schlum\u00b7mer"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ADJA", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Sich erm\u00fcdet die Wimpern mir;", "tokens": ["Sich", "er\u00b7m\u00fc\u00b7det", "die", "Wim\u00b7pern", "mir", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "ART", "NN", "PPER", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.5": {"text": "Wieder empor aus Tr\u00e4umen von ihr", "tokens": ["Wie\u00b7der", "em\u00b7por", "aus", "Tr\u00e4u\u00b7men", "von", "ihr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PTKVZ", "APPR", "NN", "APPR", "PPOSAT"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Fahr' ich zu neuem Kummer.", "tokens": ["Fahr'", "ich", "zu", "neu\u00b7em", "Kum\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Aber, o Himmel, ich flehe:", "tokens": ["A\u00b7ber", ",", "o", "Him\u00b7mel", ",", "ich", "fle\u00b7he", ":"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "$,", "FM", "NN", "$,", "PPER", "VVFIN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Raube mir nicht mein einziges Gut,", "tokens": ["Rau\u00b7be", "mir", "nicht", "mein", "ein\u00b7zi\u00b7ges", "Gut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PTKNEG", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+---+-+-+", "measure": "dactylic.init"}, "line.3": {"text": "Dies begl\u00fcckende Wehe,", "tokens": ["Dies", "be\u00b7gl\u00fc\u00b7cken\u00b7de", "We\u00b7he", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "ADJA", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Das ich gen\u00e4hrt mit des Herzens Blut!", "tokens": ["Das", "ich", "ge\u00b7n\u00e4hrt", "mit", "des", "Her\u00b7zens", "Blut", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "VVPP", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Hoch und h\u00f6her la\u00df lodern die Glut,", "tokens": ["Hoch", "und", "h\u00f6\u00b7her", "la\u00df", "lo\u00b7dern", "die", "Glut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "Drin ich selig vergehe!", "tokens": ["Drin", "ich", "se\u00b7lig", "ver\u00b7ge\u00b7he", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ADJD", "VVFIN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.4": {"line.1": {"text": "Hoffen und wieder verzagen,", "tokens": ["Hof\u00b7fen", "und", "wie\u00b7de