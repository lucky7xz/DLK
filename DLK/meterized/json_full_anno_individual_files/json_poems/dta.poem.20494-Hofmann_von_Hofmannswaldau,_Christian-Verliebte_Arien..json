{"dta.poem.20494": {"metadata": {"author": {"name": "Hofmann von Hofmannswaldau, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Verliebte Arien.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1695", "urn": "urn:nbn:de:kobv:b4-200905197751", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Flavie schaut meine thr\u00e4nen", "tokens": ["Fla\u00b7vie", "schaut", "mei\u00b7ne", "thr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPOSAT", "NN"], "meter": "+-++-+-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Nur als wasser-perlen an/", "tokens": ["Nur", "als", "was\u00b7ser\u00b7per\u00b7len", "an", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "NN", "PTKVZ", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und mein seuffzer-reiches sehnen/", "tokens": ["Und", "mein", "seuff\u00b7zer\u00b7rei\u00b7ches", "seh\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "So ich doch nicht lassen kan/", "tokens": ["So", "ich", "doch", "nicht", "las\u00b7sen", "kan", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ADV", "PTKNEG", "VVINF", "VMFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Zeigt mir/ da\u00df betr\u00fcgerey", "tokens": ["Zeigt", "mir", "/", "da\u00df", "be\u00b7tr\u00fc\u00b7ge\u00b7rey"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "$(", "KOUS", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Meiner treu belohnung sey.", "tokens": ["Mei\u00b7ner", "treu", "be\u00b7loh\u00b7nung", "sey", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Doch kan ich mich nicht erwehren/", "tokens": ["Doch", "kan", "ich", "mich", "nicht", "er\u00b7weh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PRF", "PTKNEG", "VVINF", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Zu verehren/ was mich ha\u00dft/", "tokens": ["Zu", "ver\u00b7eh\u00b7ren", "/", "was", "mich", "ha\u00dft", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$(", "PWS", "PPER", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und mich schmertzlich zu verzehren/", "tokens": ["Und", "mich", "schmertz\u00b7lich", "zu", "ver\u00b7zeh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADJD", "PTKZU", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Unter einer schweren last;", "tokens": ["Un\u00b7ter", "ei\u00b7ner", "schwe\u00b7ren", "last", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Denn die liebe liebet pein/", "tokens": ["Denn", "die", "lie\u00b7be", "lie\u00b7bet", "pein", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "VVFIN", "NE", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Und heist galle zucker seyn.", "tokens": ["Und", "heist", "gal\u00b7le", "zu\u00b7cker", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ADJD", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Mir gefallen noch die narben/", "tokens": ["Mir", "ge\u00b7fal\u00b7len", "noch", "die", "nar\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVPP", "ADV", "ART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die mir hat das joch gedr\u00fcckt/", "tokens": ["Die", "mir", "hat", "das", "joch", "ge\u00b7dr\u00fcckt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VAFIN", "ART", "NN", "VVPP", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und durch tausend falsche farben", "tokens": ["Und", "durch", "tau\u00b7send", "fal\u00b7sche", "far\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "CARD", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wird mein treuer sinn ber\u00fcckt.", "tokens": ["Wird", "mein", "treu\u00b7er", "sinn", "be\u00b7r\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Meiner f\u00e4ssel heller klang/", "tokens": ["Mei\u00b7ner", "f\u00e4s\u00b7sel", "hel\u00b7ler", "klang", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Ist mein bester trost-gesang.", "tokens": ["Ist", "mein", "bes\u00b7ter", "trost\u00b7ge\u00b7sang", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Ich mu\u00df itzt zur\u00fccke dienen/", "tokens": ["Ich", "mu\u00df", "itzt", "zu\u00b7r\u00fc\u00b7cke", "die\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "VVFIN", "VVINF", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und der sonnen edles licht/", "tokens": ["Und", "der", "son\u00b7nen", "ed\u00b7les", "licht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "So mir vormahls hat geschienen/", "tokens": ["So", "mir", "vor\u00b7mahls", "hat", "ge\u00b7schie\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ADV", "VAFIN", "VVPP", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Kennt itzund mein auge nicht/", "tokens": ["Kennt", "it\u00b7zund", "mein", "au\u00b7ge", "nicht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "PTKNEG", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Die mich ihren engel hie\u00df/", "tokens": ["Die", "mich", "ih\u00b7ren", "en\u00b7gel", "hie\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPOSAT", "NN", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "St\u00f6st mich aus dem paradie\u00df.", "tokens": ["St\u00f6st", "mich", "aus", "dem", "pa\u00b7ra\u00b7die\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Flavie lacht meiner schmertzen/", "tokens": ["Fla\u00b7vie", "lacht", "mei\u00b7ner", "schmert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPOSAT", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Meine qvaal ist ihre lust;", "tokens": ["Mei\u00b7ne", "qvaal", "ist", "ih\u00b7re", "lust", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und das joch in meinem hertzen", "tokens": ["Und", "das", "joch", "in", "mei\u00b7nem", "hert\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ist ein kleinod ihrer brust.", "tokens": ["Ist", "ein", "klei\u00b7nod", "ih\u00b7rer", "brust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Mein verderben ist das ziel/", "tokens": ["Mein", "ver\u00b7der\u00b7ben", "ist", "das", "ziel", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VVFIN", "VAFIN", "ART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Und mein\u2019 angst ihr possen-spiel.", "tokens": ["Und", "mein'", "angst", "ihr", "pos\u00b7sen\u00b7spiel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Flavie zu deinen f\u00fcssen", "tokens": ["Fla\u00b7vie", "zu", "dei\u00b7nen", "f\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Leg\u2019 ich meine freyheit hin/", "tokens": ["Leg'", "ich", "mei\u00b7ne", "frey\u00b7heit", "hin", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPOSAT", "NN", "PTKVZ", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und bin itzt in furcht beflissen", "tokens": ["Und", "bin", "itzt", "in", "furcht", "be\u00b7flis\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "ADV", "APPR", "NN", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Dir zu opffern geist und sinn;", "tokens": ["Dir", "zu", "opf\u00b7fern", "geist", "und", "sinn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "VVPP", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Doch was mir das hertze bricht/", "tokens": ["Doch", "was", "mir", "das", "hert\u00b7ze", "bricht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "ART", "NN", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Kennt itzund mein opffer nicht.", "tokens": ["Kennt", "it\u00b7zund", "mein", "opf\u00b7fer", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Die mir vormahls hat geschworen/", "tokens": ["Die", "mir", "vor\u00b7mahls", "hat", "ge\u00b7schwo\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VAFIN", "VVPP", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der werd\u2019 ich itzt unbekandt/", "tokens": ["Der", "werd'", "ich", "itzt", "un\u00b7be\u00b7kandt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ADV", "ADJD", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ich singe tauben ohren/", "tokens": ["Und", "ich", "sin\u00b7ge", "tau\u00b7ben", "oh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Man verschleust mir hertz und hand/", "tokens": ["Man", "ver\u00b7schleust", "mir", "hertz", "und", "hand", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "NN", "KON", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Was mein irrthum hat gethan/", "tokens": ["Was", "mein", "irr\u00b7thum", "hat", "ge\u00b7than", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "VAFIN", "VVPP", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Ziert itzt ihre sieges-fahn.", "tokens": ["Ziert", "itzt", "ih\u00b7re", "sie\u00b7ges\u00b7fahn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Flavie ist das erbarmen", "tokens": ["Fla\u00b7vie", "ist", "das", "er\u00b7bar\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VAFIN", "PDS", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Nicht aus deiner brust verjagt/", "tokens": ["Nicht", "aus", "dei\u00b7ner", "brust", "ver\u00b7jagt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "PPOSAT", "NN", "VVPP", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ach so reiche dem die armen/", "tokens": ["Ach", "so", "rei\u00b7che", "dem", "die", "ar\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "VVFIN", "ART", "ART", "ADJA", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Den sonst keine schuld verklagt/", "tokens": ["Den", "sonst", "kei\u00b7ne", "schuld", "ver\u00b7klagt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PIAT", "ADJD", "VVPP", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ausser da\u00df er in der welt", "tokens": ["Aus\u00b7ser", "da\u00df", "er", "in", "der", "welt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "KOUS", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Dich allzeit vor g\u00f6ttlich h\u00e4lt.", "tokens": ["Dich", "all\u00b7zeit", "vor", "g\u00f6tt\u00b7lich", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}