{"textgrid.poem.31694": {"metadata": {"author": {"name": "Weerth, Georg", "birth": "N.A.", "death": "N.A."}, "title": "16.", "genre": "verse", "period": "N.A.", "pub_year": 1839, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Maria war von lichtem Scheine,", "tokens": ["Ma\u00b7ria", "war", "von", "lich\u00b7tem", "Schei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sie hatte ein lieb Gesicht.", "tokens": ["Sie", "hat\u00b7te", "ein", "lieb", "Ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJD", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Das wu\u00dften die Burschen am ganzen Rheine \u2013", "tokens": ["Das", "wu\u00df\u00b7ten", "die", "Bur\u00b7schen", "am", "gan\u00b7zen", "Rhei\u00b7ne", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "APPRART", "ADJA", "NE", "$("], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Maria wu\u00dfte es nicht.", "tokens": ["Ma\u00b7ria", "wu\u00df\u00b7te", "es", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Sie setzte sich unter die alte Linde,", "tokens": ["Sie", "setz\u00b7te", "sich", "un\u00b7ter", "die", "al\u00b7te", "Lin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "ADJA", "NE", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Sie wand einen vollen frischen Kranz,", "tokens": ["Sie", "wand", "ei\u00b7nen", "vol\u00b7len", "fri\u00b7schen", "Kranz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Aus Rosen machte sie ein Gewinde", "tokens": ["Aus", "Ro\u00b7sen", "mach\u00b7te", "sie", "ein", "Ge\u00b7win\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Und sprach: \u00bbNun denk ich an meinen Franz.", "tokens": ["Und", "sprach", ":", "\u00bb", "Nun", "denk", "ich", "an", "mei\u00b7nen", "Franz."], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["KON", "VVFIN", "$.", "$(", "ADV", "VVFIN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Nun denk ich an die stille Stunde,", "tokens": ["Nun", "denk", "ich", "an", "die", "stil\u00b7le", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wo zuerst er aus dem Walde trat,", "tokens": ["Wo", "zu\u00b7erst", "er", "aus", "dem", "Wal\u00b7de", "trat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Wie er mich dr\u00fcben im Wiesengrunde", "tokens": ["Wie", "er", "mich", "dr\u00fc\u00b7ben", "im", "Wie\u00b7sen\u00b7grun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "PRF", "ADV", "APPRART", "NN"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.4": {"text": "Um meine bla\u00dfblaue Schleife bat;", "tokens": ["Um", "mei\u00b7ne", "bla\u00df\u00b7blau\u00b7e", "Schlei\u00b7fe", "bat", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Wie er mich in die Dorfesschenke", "tokens": ["Wie", "er", "mich", "in", "die", "Dor\u00b7fes\u00b7schen\u00b7ke"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "PRF", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "An seinem Arm gef\u00fchrt zum Tanz,", "tokens": ["An", "sei\u00b7nem", "Arm", "ge\u00b7f\u00fchrt", "zum", "Tanz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und wie wir getanzt \u2013 ach Gott, ich denke,", "tokens": ["Und", "wie", "wir", "ge\u00b7tanzt", "\u2013", "ach", "Gott", ",", "ich", "den\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "VVPP", "$(", "XY", "NN", "$,", "PPER", "VVFIN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Ich denke nur immer an meinen Franz.", "tokens": ["Ich", "den\u00b7ke", "nur", "im\u00b7mer", "an", "mei\u00b7nen", "Franz."], "token_info": ["word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-++--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "An meinen Franz! Wie im schmucken Kleide", "tokens": ["An", "mei\u00b7nen", "Franz", "!", "Wie", "im", "schmu\u00b7cken", "Klei\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$.", "PWAV", "APPRART", "ADJA", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Als Soldat er vor die T\u00fcr gesprengt,", "tokens": ["Als", "Sol\u00b7dat", "er", "vor", "die", "T\u00fcr", "ge\u00b7sprengt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPER", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Wie er gek\u00fc\u00dft meine Lippen beide", "tokens": ["Wie", "er", "ge\u00b7k\u00fc\u00dft", "mei\u00b7ne", "Lip\u00b7pen", "bei\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "VVPP", "PPOSAT", "NN", "PIS"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und von Gold mir diesen Ring geschenkt;", "tokens": ["Und", "von", "Gold", "mir", "die\u00b7sen", "Ring", "ge\u00b7schenkt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "PPER", "PDAT", "NN", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Und wie er am Ro\u00df mich emporgehoben,", "tokens": ["Und", "wie", "er", "am", "Ro\u00df", "mich", "em\u00b7por\u00b7ge\u00b7ho\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "APPRART", "NN", "PPER", "VVPP", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Das Auge voll Tr\u00e4nen ganz,", "tokens": ["Das", "Au\u00b7ge", "voll", "Tr\u00e4\u00b7nen", "ganz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "NN", "ADV", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Wie die Waffen geklirrt und die Reiter stoben", "tokens": ["Wie", "die", "Waf\u00b7fen", "ge\u00b7klirrt", "und", "die", "Rei\u00b7ter", "sto\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "VVPP", "KON", "ART", "NN", "VVINF"], "meter": "--+--+--+-+-", "measure": "anapaest.tri.plus"}, "line.8": {"text": "Hinweg \u2013 und hinweg mein Franz!", "tokens": ["Hin\u00b7weg", "\u2013", "und", "hin\u00b7weg", "mein", "Franz", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "KON", "ADV", "PPOSAT", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Und wie ich die langen Wintern\u00e4chte", "tokens": ["Und", "wie", "ich", "die", "lan\u00b7gen", "Win\u00b7ter\u00b7n\u00e4ch\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PPER", "ART", "ADJA", "NN"], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "In Kummer verlebt und immer gedacht:", "tokens": ["In", "Kum\u00b7mer", "ver\u00b7lebt", "und", "im\u00b7mer", "ge\u00b7dacht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "KON", "ADV", "VVPP", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Wo er weilen m\u00f6cht, ob er mein ged\u00e4chte \u2013", "tokens": ["Wo", "er", "wei\u00b7len", "m\u00f6cht", ",", "ob", "er", "mein", "ge\u00b7d\u00e4ch\u00b7te", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVINF", "VMFIN", "$,", "KOUS", "PPER", "PPOSAT", "ADJA", "$("], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "Bis zum Rhein man die blutige Locke gebracht.", "tokens": ["Bis", "zum", "Rhein", "man", "die", "blu\u00b7ti\u00b7ge", "Lo\u00b7cke", "ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPRART", "NE", "PIS", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.5": {"text": "Bis alle kamen \u2013 nur nimmer der eine!\u00ab \u2013", "tokens": ["Bis", "al\u00b7le", "ka\u00b7men", "\u2013", "nur", "nim\u00b7mer", "der", "ei\u00b7ne", "!", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["APPR", "PIS", "VVFIN", "$(", "ADV", "ADV", "ART", "ART", "$.", "$(", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Da ward sie still, ihr entsank der Kranz.", "tokens": ["Da", "ward", "sie", "still", ",", "ihr", "ent\u00b7sank", "der", "Kranz", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PTKVZ", "$,", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Aufrauschte die Erle im nahen Haine,", "tokens": ["Auf\u00b7rauschte", "die", "Er\u00b7le", "im", "na\u00b7hen", "Hai\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.8": {"text": "Und sie weinte um ihren toten Franz.", "tokens": ["Und", "sie", "wein\u00b7te", "um", "ih\u00b7ren", "to\u00b7ten", "Franz."], "token_info": ["word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["KON", "PPER", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}}, "stanza.5": {"line.1": {"text": "Maria war von lichtem Scheine,", "tokens": ["Ma\u00b7ria", "war", "von", "lich\u00b7tem", "Schei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sie hatte ein lieb Gesicht.", "tokens": ["Sie