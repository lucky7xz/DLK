], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "VVPP", "PPOSAT", "NN", "PIS"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und von Gold mir diesen Ring geschenkt;", "tokens": ["Und", "von", "Gold", "mir", "die\u00b7sen", "Ring", "ge\u00b7schenkt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "PPER", "PDAT", "NN", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Und wie er am Ro\u00df mich emporgehoben,", "tokens": ["Und", "wie", "er", "am", "Ro\u00df", "mich", "em\u00b7por\u00b7ge\u00b7ho\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "APPRART", "NN", "PPER", "VVPP", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Das Auge voll Tr\u00e4nen ganz,", "tokens": ["Das", "Au\u00b7ge", "voll", "Tr\u00e4\u00b7nen", "ganz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "NN", "ADV", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Wie die Waffen geklirrt und die Reiter stoben", "tokens": ["Wie", "die", "Waf\u00b7fen", "ge\u00b7klirrt", "und", "die", "Rei\u00b7ter", "sto\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "VVPP", "KON", "ART", "NN", "VVINF"], "meter": "--+--+--+-+-", "measure": "anapaest.tri.plus"}, "line.8": {"text": "Hinweg \u2013 und hinweg mein Franz!", "tokens": ["Hin\u00b7weg", "\u2013", "und", "hin\u00b7weg", "mein", "Franz", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "KON", "ADV", "PPOSAT", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.8": {"line.1": {"text": "Und wie ich die langen Wintern\u00e4chte", "tokens": ["Und", "wie", "ich", "die", "lan\u00b7gen", "Win\u00b7ter\u00b7n\u00e4ch\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PPER", "ART", "ADJA", "NN"], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "In Kummer verlebt und immer gedacht:", "tokens": ["In", "Kum\u00b7mer", "ver\u00b7lebt", "und", "im\u00b7mer", "ge\u00b7dacht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "KON", "ADV", "VVPP", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Wo er weilen m\u00f6cht, ob er mein ged\u00e4chte \u2013", "tokens": ["Wo", "er", "wei\u00b7len", "m\u00f6cht", ",", "ob", "er", "mein", "ge\u00b7d\u00e4ch\u00b7te", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVINF", "VMFIN", "$,", "KOUS", "PPER", "PPOSAT", "ADJA", "$("], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "Bis zum Rhein man die blutige Locke gebracht.", "tokens": ["Bis", "zum", "Rhein", "man", "die", "blu\u00b7ti\u00b7ge", "Lo\u00b7cke", "ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPRART", "NE", "PIS", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.5": {"text": "Bis alle kamen \u2013 nur nimmer der eine!\u00ab \u2013", "tokens": ["Bis", "al\u00b7le", "ka\u00b7men", "\u2013", "nur", "nim\u00b7mer", "der", "ei\u00b7ne", "!", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["APPR", "PIS", "VVFIN", "$(", "ADV", "ADV", "ART", "ART", "$.", "$(", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Da ward sie still, ihr entsank der Kranz.", "tokens": ["Da", "ward", "sie", "still", ",", "ihr", "ent\u00b7sank", "der", "Kranz", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PTKVZ", "$,", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Aufrauschte die Erle im nahen Haine,", "tokens": ["Auf\u00b7rauschte", "die", "Er\u00b7le", "im", "na\u00b7hen", "Hai\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.8": {"text": "Und sie weinte um ihren toten Franz.", "tokens": ["Und", "sie", "wein\u00b7te", "um", "ih\u00b7ren", "to\u00b7ten", "Franz."], "token_info": ["word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["KON", "PPER", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}}}}}