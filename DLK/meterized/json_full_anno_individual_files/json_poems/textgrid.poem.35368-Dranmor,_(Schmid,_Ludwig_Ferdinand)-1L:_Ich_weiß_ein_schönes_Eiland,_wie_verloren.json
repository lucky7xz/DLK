{"textgrid.poem.35368": {"metadata": {"author": {"name": "Dranmor, (Schmid, Ludwig Ferdinand)", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich wei\u00df ein sch\u00f6nes Eiland, wie verloren", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich wei\u00df ein sch\u00f6nes Eiland, wie verloren", "tokens": ["Ich", "wei\u00df", "ein", "sch\u00f6\u00b7nes", "Ei\u00b7land", ",", "wie", "ver\u00b7lo\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,", "PWAV", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Im stillen Ocean, ein waldbedecktes,", "tokens": ["Im", "stil\u00b7len", "O\u00b7cean", ",", "ein", "wald\u00b7be\u00b7deck\u00b7tes", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "In milden Sonnenstrahlen hingestrecktes,", "tokens": ["In", "mil\u00b7den", "Son\u00b7nen\u00b7strah\u00b7len", "hin\u00b7ge\u00b7streck\u00b7tes", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wie ein Asyl, f\u00fcr Dichter auserkoren,", "tokens": ["Wie", "ein", "A\u00b7syl", ",", "f\u00fcr", "Dich\u00b7ter", "au\u00b7ser\u00b7ko\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "APPR", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ein Eden, von der Trope Glut durchhaucht,", "tokens": ["Ein", "E\u00b7den", ",", "von", "der", "Tro\u00b7pe", "Glut", "durch\u00b7haucht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Ein Eiland, wie ein Strau\u00df von wilden Rosen", "tokens": ["Ein", "Ei\u00b7land", ",", "wie", "ein", "Strau\u00df", "von", "wil\u00b7den", "Ro\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PWAV", "ART", "NE", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "F\u00fcr die Betr\u00fcbten, f\u00fcr die Heimatlosen", "tokens": ["F\u00fcr", "die", "Be\u00b7tr\u00fcb\u00b7ten", ",", "f\u00fcr", "die", "Hei\u00b7mat\u00b7lo\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Aus tr\u00e4umerischen Fluten aufgetaucht.", "tokens": ["Aus", "tr\u00e4u\u00b7me\u00b7ri\u00b7schen", "Flu\u00b7ten", "auf\u00b7ge\u00b7taucht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Es ragt empor, der Schiffer Augenweide,", "tokens": ["Es", "ragt", "em\u00b7por", ",", "der", "Schif\u00b7fer", "Au\u00b7gen\u00b7wei\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Mit Halden, Silberb\u00e4chen, k\u00fchlen Schluchten.", "tokens": ["Mit", "Hal\u00b7den", ",", "Sil\u00b7ber\u00b7b\u00e4\u00b7chen", ",", "k\u00fch\u00b7len", "Schluch\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Es streift mit seinem dunkelgr\u00fcnen Kleide", "tokens": ["Es", "streift", "mit", "sei\u00b7nem", "dun\u00b7kel\u00b7gr\u00fc\u00b7nen", "Klei\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Bis an den Spiegel seiner Felsenbuchten.", "tokens": ["Bis", "an", "den", "Spie\u00b7gel", "sei\u00b7ner", "Fel\u00b7sen\u00b7buch\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Lianen werfen ihre Bl\u00fctenschnur", "tokens": ["Li\u00b7a\u00b7nen", "wer\u00b7fen", "ih\u00b7re", "Bl\u00fc\u00b7ten\u00b7schnur"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Von Baum zu Baum; durch buntes Strauchwerk fliegen", "tokens": ["Von", "Baum", "zu", "Baum", ";", "durch", "bun\u00b7tes", "Strauch\u00b7werk", "flie\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NE", "APPR", "NE", "$.", "APPR", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Zwitschernde V\u00f6gel. Stolze Forsten schmiegen", "tokens": ["Zwit\u00b7schern\u00b7de", "V\u00f6\u00b7gel", ".", "Stol\u00b7ze", "Fors\u00b7ten", "schmie\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$.", "ADJA", "NN", "VVINF"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.16": {"text": "Sich an des Himmels blendendes Azur.", "tokens": ["Sich", "an", "des", "Him\u00b7mels", "blen\u00b7den\u00b7des", "A\u00b7zur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.17": {"text": "Dort gl\u00e4nzt der Morgentau wie Diamanten", "tokens": ["Dort", "gl\u00e4nzt", "der", "Mor\u00b7gen\u00b7tau", "wie", "Di\u00b7a\u00b7man\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "KOKOM", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Auf satten Gr\u00e4sern; Antilopen schauen", "tokens": ["Auf", "sat\u00b7ten", "Gr\u00e4\u00b7sern", ";", "An\u00b7ti\u00b7lo\u00b7pen", "schau\u00b7en"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$.", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Furchtlos hernieder von der Berge Kanten,", "tokens": ["Furcht\u00b7los", "her\u00b7nie\u00b7der", "von", "der", "Ber\u00b7ge", "Kan\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "APPR", "ART", "NN", "VMFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Und Palmen rauschen auf bebl\u00fcmten Auen.", "tokens": ["Und", "Pal\u00b7men", "rau\u00b7schen", "auf", "be\u00b7bl\u00fcm\u00b7ten", "Au\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Ostindienfahrer kommen, reich beladen,", "tokens": ["Ost\u00b7in\u00b7dien\u00b7fah\u00b7rer", "kom\u00b7men", ",", "reich", "be\u00b7la\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVINF", "$,", "ADJD", "VVPP", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.22": {"text": "Und Wallfischf\u00e4nger, l\u00e4rmende Gesellen.", "tokens": ["Und", "Wall\u00b7fischf\u00e4n\u00b7ger", ",", "l\u00e4r\u00b7men\u00b7de", "Ge\u00b7sel\u00b7len", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+--+---+-", "measure": "iambic.tri.relaxed"}, "line.23": {"text": "Sie kennen meiner Insel Ankerstellen,", "tokens": ["Sie", "ken\u00b7nen", "mei\u00b7ner", "In\u00b7sel", "An\u00b7ker\u00b7stel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "Doch nur als rastbed\u00fcrftige Nomaden.", "tokens": ["Doch", "nur", "als", "rast\u00b7be\u00b7d\u00fcrf\u00b7ti\u00b7ge", "No\u00b7ma\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "KOUS", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.25": {"text": "Mich zieht es hin, in meinen liebsten Tr\u00e4umen,", "tokens": ["Mich", "zieht", "es", "hin", ",", "in", "mei\u00b7nen", "liebs\u00b7ten", "Tr\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$,", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.26": {"text": "Zu jenen Th\u00e4lern mit den Kokosb\u00e4umen;", "tokens": ["Zu", "je\u00b7nen", "Th\u00e4\u00b7lern", "mit", "den", "Ko\u00b7kos\u00b7b\u00e4u\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.27": {"text": "Ich m\u00f6chte dort, auf freier Erde weilend,", "tokens": ["Ich", "m\u00f6ch\u00b7te", "dort", ",", "auf", "frei\u00b7er", "Er\u00b7de", "wei\u00b7lend", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "$,", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.28": {"text": "Mein Paradies mit freien M\u00e4nnern teilend,", "tokens": ["Mein", "Pa\u00b7ra\u00b7dies", "mit", "frei\u00b7en", "M\u00e4n\u00b7nern", "tei\u00b7lend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.29": {"text": "Ein Pflanzer unter Pflanzern, meine Saat", "tokens": ["Ein", "Pflan\u00b7zer", "un\u00b7ter", "Pflan\u00b7zern", ",", "mei\u00b7ne", "Saat"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "APPR", "NN", "$,", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.30": {"text": "Ausstreuen, einer gro\u00dfen Zukunft Keime,", "tokens": ["Aus\u00b7streu\u00b7en", ",", "ei\u00b7ner", "gro\u00b7\u00dfen", "Zu\u00b7kunft", "Kei\u00b7me", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.31": {"text": "Und ohne Wortgeklingel, ohne Reime", "tokens": ["Und", "oh\u00b7ne", "Wort\u00b7ge\u00b7klin\u00b7gel", ",", "oh\u00b7ne", "Rei\u00b7me"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "APPR", "NN", "$,", "KOUI", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.32": {"text": "Den Tod erwarten als ein Mann der That.", "tokens": ["Den", "Tod", "er\u00b7war\u00b7ten", "als", "ein", "Mann", "der", "That", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "KOKOM", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.33": {"text": "Es ist zu sp\u00e4t! Die Jugend ist entwichen.", "tokens": ["Es", "ist", "zu", "sp\u00e4t", "!", "Die", "Ju\u00b7gend", "ist", "ent\u00b7wi\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKA", "ADJD", "$.", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.34": {"text": "Statt neuer Freuden sind Erinnerungen", "tokens": ["Statt", "neu\u00b7er", "Freu\u00b7den", "sind", "E\u00b7rin\u00b7ne\u00b7run\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADJA", "NN", "VAFIN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.35": {"text": "Aus guter alter Zeit herbeigesprungen,", "tokens": ["Aus", "gu\u00b7ter", "al\u00b7ter", "Zeit", "her\u00b7bei\u00b7ge\u00b7sprun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.36": {"text": "Und Selbstbetrachtung kam herangeschlichen.", "tokens": ["Und", "Selbst\u00b7be\u00b7trach\u00b7tung", "kam", "her\u00b7an\u00b7ge\u00b7schli\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "VVIZU", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.37": {"text": "Es sei! \u2013 Ich will an Tr\u00e4umen mich erfreun,", "tokens": ["Es", "sei", "!", "\u2013", "Ich", "will", "an", "Tr\u00e4u\u00b7men", "mich", "er\u00b7freun", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$.", "$(", "PPER", "VMFIN", "APPR", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.38": {"text": "Die meine tiefe Trauer \u00fcberragen.", "tokens": ["Die", "mei\u00b7ne", "tie\u00b7fe", "Trau\u00b7er", "\u00fc\u00b7berr\u00b7a\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.39": {"text": "Ich habe hei\u00df gestrebt \u2013 ich mu\u00df entsagen,", "tokens": ["Ich", "ha\u00b7be", "hei\u00df", "ge\u00b7strebt", "\u2013", "ich", "mu\u00df", "ent\u00b7sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "VVPP", "$(", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.40": {"text": "Ich mu\u00df auf meine Wunden Asche streun.", "tokens": ["Ich", "mu\u00df", "auf", "mei\u00b7ne", "Wun\u00b7den", "A\u00b7sche", "streun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PPOSAT", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ich wei\u00df ein sch\u00f6nes Eiland, wie verloren", "tokens": ["Ich", "wei\u00df", "ein", "sch\u00f6\u00b7nes", "Ei\u00b7land", ",", "wie", "ver\u00b7lo\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,", "PWAV", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Im stillen Ocean, ein waldbedecktes,", "tokens": ["Im", "stil\u00b7len", "O\u00b7cean", ",", "ein", "wald\u00b7be\u00b7deck\u00b7tes", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "In milden Sonnenstrahlen hingestrecktes,", "tokens": ["In", "mil\u00b7den", "Son\u00b7nen\u00b7strah\u00b7len", "hin\u00b7ge\u00b7streck\u00b7tes", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wie ein Asyl,