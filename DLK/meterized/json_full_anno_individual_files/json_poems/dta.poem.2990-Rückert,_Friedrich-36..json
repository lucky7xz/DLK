{"dta.poem.2990": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "36.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Entweder ist mein Blick nur gegen euern stumpf,", "tokens": ["Ent\u00b7we\u00b7der", "ist", "mein", "Blick", "nur", "ge\u00b7gen", "eu\u00b7ern", "stumpf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPOSAT", "NN", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Oder auch euer Ohr ist gegen meines dumpf.", "tokens": ["O\u00b7der", "auch", "eu\u00b7er", "Ohr", "ist", "ge\u00b7gen", "mei\u00b7nes", "dumpf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPOSAT", "NN", "VAFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ich seh' ein Ganzes, wo ihr sehet manchen Bruch,", "tokens": ["Ich", "seh'", "ein", "Gan\u00b7zes", ",", "wo", "ihr", "se\u00b7het", "man\u00b7chen", "Bruch", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PWAV", "PPER", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und h\u00f6r' Einklingendes, wo ihr h\u00f6rt Widerspruch.", "tokens": ["Und", "h\u00f6r'", "Ein\u00b7klin\u00b7gen\u00b7des", ",", "wo", "ihr", "h\u00f6rt", "Wi\u00b7der\u00b7spruch", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "$,", "PWAV", "PPER", "VVFIN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wenn mein die T\u00e4uschung ist, so g\u00f6nn' ich euch die Wahrheit;", "tokens": ["Wenn", "mein", "die", "T\u00e4u\u00b7schung", "ist", ",", "so", "g\u00f6nn'", "ich", "euch", "die", "Wahr\u00b7heit", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ART", "NN", "VAFIN", "$,", "ADV", "VVFIN", "PPER", "PRF", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ist aber mein das Licht, so kommt in meine Klarheit!", "tokens": ["Ist", "a\u00b7ber", "mein", "das", "Licht", ",", "so", "kommt", "in", "mei\u00b7ne", "Klar\u00b7heit", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPOSAT", "ART", "NN", "$,", "ADV", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}