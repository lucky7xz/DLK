{"dta.poem.10151": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Noch andere Herbst-Gedancken.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20086-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es k\u00fchlete nunmehr der Herbst die Tage schon,", "tokens": ["Es", "k\u00fch\u00b7le\u00b7te", "nun\u00b7mehr", "der", "Herbst", "die", "Ta\u00b7ge", "schon", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als ich, zur Abend-Zeit, im Garten hin und her", "tokens": ["Als", "ich", ",", "zur", "A\u00b7ben\u00b7dZeit", ",", "im", "Gar\u00b7ten", "hin", "und", "her"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "$,", "APPRART", "NN", "$,", "APPRART", "NN", "PTKVZ", "KON", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Jm hellen Mond-Schein gieng; da ich von ungefehr", "tokens": ["Jm", "hel\u00b7len", "Mon\u00b7dSchein", "gieng", ";", "da", "ich", "von", "un\u00b7ge\u00b7fehr"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "$.", "KOUS", "PPER", "APPR", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Den Glantz der herrlichsten Jllumination,", "tokens": ["Den", "Glantz", "der", "herr\u00b7lichs\u00b7ten", "Jllu\u00b7mi\u00b7na\u00b7ti\u00b7on", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Die ie ein Mensch gesehn, erblickt.", "tokens": ["Die", "ie", "ein", "Mensch", "ge\u00b7sehn", ",", "er\u00b7blickt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "VVPP", "$,", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich sah zuerst von meinem Zimmer,", "tokens": ["Ich", "sah", "zu\u00b7erst", "von", "mei\u00b7nem", "Zim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Fenster angestrahlt und herrlich ausgeschm\u00fcckt.", "tokens": ["Die", "Fens\u00b7ter", "an\u00b7ge\u00b7strahlt", "und", "herr\u00b7lich", "aus\u00b7ge\u00b7schm\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "KON", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der mehr als Silber-weisse Schimmer", "tokens": ["Der", "mehr", "als", "Sil\u00b7ber\u00b7weis\u00b7se", "Schim\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PIS", "KOKOM", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Vom vollen Mond, der funckelnd r\u00fcckw\u00e4rts fiel,", "tokens": ["Vom", "vol\u00b7len", "Mond", ",", "der", "fun\u00b7ckelnd", "r\u00fcck\u00b7w\u00e4rts", "fiel", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "PRELS", "ADJD", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "War meiner frohen Augen Ziel.", "tokens": ["War", "mei\u00b7ner", "fro\u00b7hen", "Au\u00b7gen", "Ziel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Absonderlich, da bey so heiterm Wetter,", "tokens": ["Ab\u00b7son\u00b7der\u00b7lich", ",", "da", "bey", "so", "hei\u00b7term", "Wet\u00b7ter", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "KOUS", "APPR", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Durch einen Wein-Stock, dessen Bl\u00e4tter", "tokens": ["Durch", "ei\u00b7nen", "Wein\u00b7Stock", ",", "des\u00b7sen", "Bl\u00e4t\u00b7ter"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "PRELAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Die Scheiben fast bedecket hatten,", "tokens": ["Die", "Schei\u00b7ben", "fast", "be\u00b7de\u00b7cket", "hat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "VAFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Das Licht noch eins so hell, durch untermischte Schatten,", "tokens": ["Das", "Licht", "noch", "eins", "so", "hell", ",", "durch", "un\u00b7ter\u00b7mischte", "Schat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PIS", "ADV", "ADJD", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "Gl\u00e4ntzt, blitzt\u2019 und funckelte. Wie ich nun also stand,", "tokens": ["Gl\u00e4ntzt", ",", "blitzt'", "und", "fun\u00b7ckel\u00b7te", ".", "Wie", "ich", "nun", "al\u00b7so", "stand", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "KON", "VVFIN", "$.", "PWAV", "PPER", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Und diesen reinen Glantz recht Wunder-w\u00fcrdig fand;", "tokens": ["Und", "die\u00b7sen", "rei\u00b7nen", "Glantz", "recht", "Wun\u00b7der\u00b7w\u00fcr\u00b7dig", "fand", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "ADJA", "NN", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Setzt\u2019 iemand ungefehr ein brennend Licht", "tokens": ["Setzt'", "ie\u00b7mand", "un\u00b7ge\u00b7fehr", "ein", "bren\u00b7nend", "Licht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "ADJD", "ART", "ADJD", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Jm Zimmer vor das Fenster nieder.", "tokens": ["Jm", "Zim\u00b7mer", "vor", "das", "Fens\u00b7ter", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "O welch ein Wunder-Glantz traff meiner Augen Lieder,", "tokens": ["O", "welch", "ein", "Wun\u00b7der\u00b7Glantz", "traff", "mei\u00b7ner", "Au\u00b7gen", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PIAT", "ART", "NN", "VVFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Und fiel nicht nur in mein Gesicht,", "tokens": ["Und", "fiel", "nicht", "nur", "in", "mein", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Fiel in die Seele selbst hinein!", "tokens": ["Fiel", "in", "die", "See\u00b7le", "selbst", "hin\u00b7ein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Des hellen Lichts fast g\u00fcldner Schein", "tokens": ["Des", "hel\u00b7len", "Lichts", "fast", "g\u00fcld\u00b7ner", "Schein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Drang in das bunte Laub der Neben,", "tokens": ["Drang", "in", "das", "bun\u00b7te", "Laub", "der", "Ne\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Das, durch den k\u00fchlen Herbst, und fenchte Wittrung, eben", "tokens": ["Das", ",", "durch", "den", "k\u00fch\u00b7len", "Herbst", ",", "und", "fench\u00b7te", "Witt\u00b7rung", ",", "e\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["PDS", "$,", "APPR", "ART", "ADJA", "NN", "$,", "KON", "ADJA", "NN", "$,", "ADV"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sein bis daher so lieblich Gr\u00fcn,", "tokens": ["Sein", "bis", "da\u00b7her", "so", "lieb\u00b7lich", "Gr\u00fcn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADV", "PAV", "ADV", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So gelb als Gold, so roth, als ein Rubin,", "tokens": ["So", "gelb", "als", "Gold", ",", "so", "roth", ",", "als", "ein", "Ru\u00b7bin", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "NN", "$,", "ADV", "ADJD", "$,", "KOUS", "ART", "NE", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Gemahlet und gef\u00e4rbt. Es ward dadurch zugleich", "tokens": ["Ge\u00b7mah\u00b7let", "und", "ge\u00b7f\u00e4rbt", ".", "Es", "ward", "da\u00b7durch", "zu\u00b7gleich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "KON", "VVPP", "$.", "PPER", "VAFIN", "PAV", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "So sch\u00f6n der Farben Schmuck gebrochen und gemildert,", "tokens": ["So", "sch\u00f6n", "der", "Far\u00b7ben", "Schmuck", "ge\u00b7bro\u00b7chen", "und", "ge\u00b7mil\u00b7dert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "NN", "VVPP", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Da\u00df ", "tokens": ["Da\u00df"], "token_info": ["word"], "pos": ["KOUS"], "meter": "+", "measure": "single.up"}, "line.9": {"text": "Noch seine Farben mischt. Ein Licht-Glantz, welcher strahl", "tokens": ["Noch", "sei\u00b7ne", "Far\u00b7ben", "mischt", ".", "Ein", "Licht\u00b7Glantz", ",", "wel\u00b7cher", "strahl"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADV", "PPOSAT", "NN", "VVPP", "$.", "ART", "NN", "$,", "PWAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Durch Laubwerck, so auf Tafft gemahlt,", "tokens": ["Durch", "Laub\u00b7werck", ",", "so", "auf", "Tafft", "ge\u00b7mahlt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "ADV", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Sieht angenehm, sieht nied-und lieblich aus:", "tokens": ["Sieht", "an\u00b7ge\u00b7nehm", ",", "sieht", "nie\u00b7d\u00b7und", "lieb\u00b7lich", "aus", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "$,", "VVFIN", "ADJD", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "Allein, wie gro\u00df ist doch der Unterscheid", "tokens": ["Al\u00b7lein", ",", "wie", "gro\u00df", "ist", "doch", "der", "Un\u00b7ter\u00b7scheid"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "PWAV", "ADJD", "VAFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Von der Copie zum Urbild! irdisch nur", "tokens": ["Von", "der", "Co\u00b7pie", "zum", "Ur\u00b7bild", "!", "ir\u00b7disch", "nur"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "NN", "APPRART", "NN", "$.", "ADJD", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Sind Farben, so die Kunst gebrauchet; die Natur", "tokens": ["Sind", "Far\u00b7ben", ",", "so", "die", "Kunst", "ge\u00b7brau\u00b7chet", ";", "die", "Na\u00b7tur"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VAFIN", "NN", "$,", "ADV", "ART", "NN", "VVPP", "$.", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Zeigt hier in einem Safft, der wol gel\u00e4utert, rein,", "tokens": ["Zeigt", "hier", "in", "ei\u00b7nem", "Safft", ",", "der", "wol", "ge\u00b7l\u00e4u\u00b7tert", ",", "rein", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ART", "NN", "$,", "PRELS", "ADV", "VVPP", "$,", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Des Lichtes selbst gef\u00e4rbten Schein.", "tokens": ["Des", "Lich\u00b7tes", "selbst", "ge\u00b7f\u00e4rb\u00b7ten", "Schein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Wie herrlich flammt in einer rothen Glut", "tokens": ["Wie", "herr\u00b7lich", "flammt", "in", "ei\u00b7ner", "ro\u00b7then", "Glut"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Manch, selbst das reinste Schnecken-Blut", "tokens": ["Manch", ",", "selbst", "das", "reins\u00b7te", "Schne\u00b7cken\u00b7Blut"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PIAT", "$,", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Besiegend, und an Glantz weit \u00fcbertreffend Blat!", "tokens": ["Be\u00b7sie\u00b7gend", ",", "und", "an", "Glantz", "weit", "\u00fc\u00b7bert\u00b7ref\u00b7fend", "Blat", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "KON", "APPR", "NN", "ADJD", "VVPP", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Das viele bunte Nachbarn hat,", "tokens": ["Das", "vie\u00b7le", "bun\u00b7te", "Nach\u00b7barn", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "ADJA", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die auch, nicht minder sch\u00f6n,", "tokens": ["Die", "auch", ",", "nicht", "min\u00b7der", "sch\u00f6n", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "$,", "PTKNEG", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "In gleichsam g\u00fcldnen Flammen stehn.", "tokens": ["In", "gleich\u00b7sam", "g\u00fcld\u00b7nen", "Flam\u00b7men", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Durch diese bunte Glut, und Feuer-reiche Pracht", "tokens": ["Durch", "die\u00b7se", "bun\u00b7te", "Glut", ",", "und", "Feu\u00b7er\u00b7rei\u00b7che", "Pracht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$,", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ward alsobald in meinem Hertzen", "tokens": ["Ward", "al\u00b7so\u00b7bald", "in", "mei\u00b7nem", "Hert\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Ein Freuden-Feuer angefacht.", "tokens": ["Ein", "Freu\u00b7den\u00b7Feu\u00b7er", "an\u00b7ge\u00b7facht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Es brennen reiner Andacht Kertzen", "tokens": ["Es", "bren\u00b7nen", "rei\u00b7ner", "An\u00b7dacht", "Kert\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVINF", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zu dessen Ruhm, der Laub und Licht,", "tokens": ["Zu", "des\u00b7sen", "Ruhm", ",", "der", "Laub", "und", "Licht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "$,", "ART", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und, \u00fcber alles, mein Gesicht", "tokens": ["Und", ",", "\u00fc\u00b7ber", "al\u00b7les", ",", "mein", "Ge\u00b7sicht"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KON", "$,", "APPR", "PIS", "$,", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So wunderbar formirt, und mir gegeben.", "tokens": ["So", "wun\u00b7der\u00b7bar", "for\u00b7mirt", ",", "und", "mir", "ge\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "$,", "KON", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ach m\u00f6gt\u2019 ich doch, zu Seiner Ehr,", "tokens": ["Ach", "m\u00f6gt'", "ich", "doch", ",", "zu", "Sei\u00b7ner", "Ehr", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "ADV", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "In seiner Creaturen Heer,", "tokens": ["In", "sei\u00b7ner", "Crea\u00b7tu\u00b7ren", "Heer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Sein unausdr\u00fccklich Lob ie mehr und mehr,", "tokens": ["Sein", "un\u00b7aus\u00b7dr\u00fcck\u00b7lich", "Lob", "ie", "mehr", "und", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "ADV", "ADV", "KON", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "In der Betrachtungs-Lust, zu mehren, mich bestreben!", "tokens": ["In", "der", "Be\u00b7trach\u00b7tungs\u00b7Lust", ",", "zu", "meh\u00b7ren", ",", "mich", "be\u00b7stre\u00b7ben", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PTKZU", "VVINF", "$,", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}