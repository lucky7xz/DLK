{"dta.poem.14501": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Herbst .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Durch die Felder sieht man fahren", "tokens": ["Durch", "die", "Fel\u00b7der", "sieht", "man", "fah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "PIS", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Eine wundersch\u00f6ne Frau,", "tokens": ["Ei\u00b7ne", "wun\u00b7der\u00b7sch\u00f6\u00b7ne", "Frau", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und von ihren langen Haaren", "tokens": ["Und", "von", "ih\u00b7ren", "lan\u00b7gen", "Haa\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gold'ne F\u00e4den auf der Au", "tokens": ["Gold'\u00b7ne", "F\u00e4\u00b7den", "auf", "der", "Au"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Spinnet sie und singt im Gehen:", "tokens": ["Spin\u00b7net", "sie", "und", "singt", "im", "Ge\u00b7hen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KON", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Eya, meine Bl\u00fcmelein,", "tokens": ["Ey\u00b7a", ",", "mei\u00b7ne", "Bl\u00fc\u00b7me\u00b7lein", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Nicht nach andern immer sehen,", "tokens": ["Nicht", "nach", "an\u00b7dern", "im\u00b7mer", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "PIS", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Eya, schlafet, schlafet ein.", "tokens": ["Ey\u00b7a", ",", "schla\u00b7fet", ",", "schla\u00b7fet", "ein", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "$,", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}