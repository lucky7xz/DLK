{"dta.poem.651": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "Gegensatz.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Doch/ nichts ist das so scharff/ als eine zunge sey!", "tokens": ["Doch", "/", "nichts", "ist", "das", "so", "scharff", "/", "als", "ei\u00b7ne", "zun\u00b7ge", "sey", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$(", "PIS", "VAFIN", "PDS", "ADV", "ADJD", "$(", "KOUS", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nichts das so tief vns arme st\u00fcrtzen k\u00f6nne.", "tokens": ["Nichts", "das", "so", "tief", "vns", "ar\u00b7me", "st\u00fcrt\u00b7zen", "k\u00f6n\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ART", "ADV", "ADJD", "PPER", "ADJA", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "O da\u00df der Himmel stum\u0303 zu werden g\u00f6nne! ", "tokens": ["O", "da\u00df", "der", "Him\u00b7mel", "stum\u0303", "zu", "wer\u00b7den", "g\u00f6n\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOUS", "ART", "NN", "ADJD", "PTKZU", "VAINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dem/ der mit worten frech; mit reden/ viel zu frey:", "tokens": ["Dem", "/", "der", "mit", "wor\u00b7ten", "frech", ";", "mit", "re\u00b7den", "/", "viel", "zu", "frey", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "$(", "ART", "APPR", "NN", "ADJD", "$.", "APPR", "VVINF", "$(", "ADV", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der st\u00e4dte grau\u00df/ das leichen volle feldt/", "tokens": ["Der", "st\u00e4d\u00b7te", "grau\u00df", "/", "das", "lei\u00b7chen", "vol\u00b7le", "feldt", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "ART", "ADJA", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Der schiffe brandt/ das Meer durch blutt verstellt.", "tokens": ["Der", "schif\u00b7fe", "brandt", "/", "das", "Meer", "durch", "blutt", "ver\u00b7stellt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "$(", "ART", "NN", "APPR", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Die Schwartze Zauberkunst/ ", "tokens": ["Die", "Schwart\u00b7ze", "Zau\u00b7ber\u00b7kunst", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Der eiteln Lehre dunst/", "tokens": ["Der", "ei\u00b7teln", "Leh\u00b7re", "dunst", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Die macht durch gifft/ den Parcen vorzukommen:", "tokens": ["Die", "macht", "durch", "gifft", "/", "den", "Par\u00b7cen", "vor\u00b7zu\u00b7kom\u00b7men", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "NN", "$(", "ART", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Der V\u00f6lcker grimmer ha\u00df/ der vngehewre Krieg.", "tokens": ["Der", "V\u00f6l\u00b7cker", "grim\u00b7mer", "ha\u00df", "/", "der", "vn\u00b7ge\u00b7hew\u00b7re", "Krieg", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$(", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Der zanck der Kirch\u2019 vnd Seelen eingenommen/", "tokens": ["Der", "zanck", "der", "Kirch'", "vnd", "See\u00b7len", "ein\u00b7ge\u00b7nom\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "KON", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Der Tugend vntergang/ der grimmen Laster sieg/ ", "tokens": ["Der", "Tu\u00b7gend", "vn\u00b7ter\u00b7gang", "/", "der", "grim\u00b7men", "Las\u00b7ter", "sieg", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$(", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Ist durch der zungen macht gebohren:", "tokens": ["Ist", "durch", "der", "zun\u00b7gen", "macht", "ge\u00b7boh\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "VVFIN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Durch welche Lieb vnd trew verlohren. ", "tokens": ["Durch", "wel\u00b7che", "Lieb", "vnd", "trew", "ver\u00b7loh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "NN", "KON", "ADJD", "VVPP", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.15": {"text": "Wie manchen hat die Zung\u2019 in seine grufft gedrungen!", "tokens": ["Wie", "man\u00b7chen", "hat", "die", "Zung'", "in", "sei\u00b7ne", "grufft", "ge\u00b7drun\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VAFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "De\u00df Menschen Todt beruht auff jedes ", "tokens": ["De\u00df", "Men\u00b7schen", "Todt", "be\u00b7ruht", "auff", "je\u00b7des"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VVFIN", "APPR", "PIAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}