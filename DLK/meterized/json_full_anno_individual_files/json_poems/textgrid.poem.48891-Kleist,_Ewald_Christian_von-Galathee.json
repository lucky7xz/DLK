{"textgrid.poem.48891": {"metadata": {"author": {"name": "Kleist, Ewald Christian von", "birth": "N.A.", "death": "N.A."}, "title": "Galathee", "genre": "verse", "period": "N.A.", "pub_year": 1755, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Begl\u00fcckter Schmerz, der in den Hain mich f\u00fchrte!", "tokens": ["Be\u00b7gl\u00fcck\u00b7ter", "Schmerz", ",", "der", "in", "den", "Hain", "mich", "f\u00fchr\u00b7te", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PRELS", "APPR", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dort schl\u00e4ft im Klee", "tokens": ["Dort", "schl\u00e4ft", "im", "Klee"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPRART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Die Ursach meiner Pein, die sch\u00f6ne Galathee.", "tokens": ["Die", "Ur\u00b7sach", "mei\u00b7ner", "Pein", ",", "die", "sch\u00f6\u00b7ne", "Ga\u00b7la\u00b7thee", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "O w\u00e4r ich doch der Klee,", "tokens": ["O", "w\u00e4r", "ich", "doch", "der", "Klee", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Da\u00df mich ihr Leib ber\u00fchrte!", "tokens": ["Da\u00df", "mich", "ihr", "Leib", "be\u00b7r\u00fchr\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Weh sanft o Luft! da\u00df sich die Bl\u00e4tter nicht bewegen \u2013 \u2013 \u2013 \u2013", "tokens": ["Weh", "sanft", "o", "Luft", "!", "da\u00df", "sich", "die", "Bl\u00e4t\u00b7ter", "nicht", "be\u00b7we\u00b7gen", "\u2013", "\u2013", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct", "punct"], "pos": ["NN", "ADJD", "FM", "NN", "$.", "KOUS", "PRF", "ART", "NN", "PTKNEG", "VVINF", "$(", "$(", "$(", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Doch sie erwachet schon und fliehet \u2013 Folg ich ihr?", "tokens": ["Doch", "sie", "er\u00b7wa\u00b7chet", "schon", "und", "flie\u00b7het", "\u2013", "Folg", "ich", "ihr", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "KON", "VVFIN", "$(", "NN", "PPER", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "O nein, sie z\u00fcrnt und sie entfliehet mir!", "tokens": ["O", "nein", ",", "sie", "z\u00fcrnt", "und", "sie", "ent\u00b7flie\u00b7het", "mir", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKANT", "$,", "PPER", "VVFIN", "KON", "PPER", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Ich will, o welch ein Gl\u00fcck! Da wo sie lag, mich legen,", "tokens": ["Ich", "will", ",", "o", "welch", "ein", "Gl\u00fcck", "!", "Da", "wo", "sie", "lag", ",", "mich", "le\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "FM", "PWAT", "ART", "NN", "$.", "ADV", "PWAV", "PPER", "VVFIN", "$,", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "lin