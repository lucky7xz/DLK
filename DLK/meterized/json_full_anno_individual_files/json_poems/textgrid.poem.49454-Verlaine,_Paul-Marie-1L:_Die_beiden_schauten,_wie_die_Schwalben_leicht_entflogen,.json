{"textgrid.poem.49454": {"metadata": {"author": {"name": "Verlaine, Paul-Marie", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die beiden schauten, wie die Schwalben leicht entflogen,", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die beiden schauten, wie die Schwalben leicht entflogen,", "tokens": ["Die", "bei\u00b7den", "schau\u00b7ten", ",", "wie", "die", "Schwal\u00b7ben", "leicht", "ent\u00b7flo\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "ADJA", "$,", "PWAV", "ART", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die eine rosig blond, bleich und mit schwarzem Haare", "tokens": ["Die", "ei\u00b7ne", "ro\u00b7sig", "blond", ",", "bleich", "und", "mit", "schwar\u00b7zem", "Haa\u00b7re"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ART", "ADJD", "ADJD", "$,", "ADJD", "KON", "APPR", "ADJA", "NN"], "meter": "-+-+-++--+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Die andre, und das matte Nachtkleid floss dem Paare", "tokens": ["Die", "and\u00b7re", ",", "und", "das", "mat\u00b7te", "Nacht\u00b7kleid", "floss", "dem", "Paa\u00b7re"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PIS", "$,", "KON", "ART", "ADJA", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sanft nieder, wolkengleich, in weichen, \u00fcpp'gen Wogen.", "tokens": ["Sanft", "nie\u00b7der", ",", "wol\u00b7ken\u00b7gleich", ",", "in", "wei\u00b7chen", ",", "\u00fcpp'\u00b7gen", "Wo\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "punct"], 