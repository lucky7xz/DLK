{"dta.poem.3295": {"metadata": {"author": {"name": "Uz, Johann Peter", "birth": "N.A.", "death": "N.A."}, "title": "Tempe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1755", "urn": "urn:nbn:de:kobv:b4-20614-6", "language": ["de:0.99"], "booktitle": "Uz, Johann Peter: Lyrische und andere Gedichte. 2. Aufl. Ansbach, 1755."}, "poem": {"stanza.1": {"line.1": {"text": "Durch welch geheimen Zwang", "tokens": ["Durch", "welch", "ge\u00b7hei\u00b7men", "Zwang"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PWAT", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Erwacht mein schlafender Gesang?", "tokens": ["Er\u00b7wacht", "mein", "schla\u00b7fen\u00b7der", "Ge\u00b7sang", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich f\u00fchle wiederum die Herrschaft weiser", "tokens": ["Ich", "f\u00fch\u00b7le", "wie\u00b7de\u00b7rum", "die", "Herr\u00b7schaft", "wei\u00b7ser"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Musen.", "tokens": ["Mu\u00b7sen", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Wie st\u00fcrmet nicht in meinem Busen", "tokens": ["Wie", "st\u00fcr\u00b7met", "nicht", "in", "mei\u00b7nem", "Bu\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PTKNEG", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die ungest\u00fcme Glut,", "tokens": ["Die", "un\u00b7ge\u00b7st\u00fc\u00b7me", "Glut", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und reisst mich hin in trunkner Wuth!", "tokens": ["Und", "reisst", "mich", "hin", "in", "trunk\u00b7ner", "Wuth", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "T\u00e4uscht mich der s\u00fcsse Wahn?", "tokens": ["T\u00e4uscht", "mich", "der", "s\u00fcs\u00b7se", "Wahn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Welch Thal der Freuden lockt mich an", "tokens": ["Welch", "Thal", "der", "Freu\u00b7den", "lockt", "mich", "an"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "ART", "NN", "VVFIN", "PPER", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit frischbethautem Gr\u00fcn, mit ambrareichen L\u00fcften?", "tokens": ["Mit", "frischbe\u00b7thau\u00b7tem", "Gr\u00fcn", ",", "mit", "am\u00b7bra\u00b7rei\u00b7chen", "L\u00fcf\u00b7ten", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Wie plaudert in der Berge Kl\u00fcften", "tokens": ["Wie", "plau\u00b7dert", "in", "der", "Ber\u00b7ge", "Kl\u00fcf\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Der wache Wiederhall!", "tokens": ["Der", "wa\u00b7che", "Wie\u00b7der\u00b7hall", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Die V\u00f6gel singen \u00fcberall!", "tokens": ["Die", "V\u00f6\u00b7gel", "sin\u00b7gen", "\u00fc\u00b7be\u00b7rall", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Durch k\u00fchle B\u00fcsche rauscht", "tokens": ["Durch", "k\u00fch\u00b7le", "B\u00fc\u00b7sche", "rauscht"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Ein Zephyr, der um Floren lauscht:", "tokens": ["Ein", "Ze\u00b7phyr", ",", "der", "um", "Flo\u00b7ren", "lauscht", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es murmelt mancher Bach; es wandelt unter B\u00e4umen", "tokens": ["Es", "mur\u00b7melt", "man\u00b7cher", "Bach", ";", "es", "wan\u00b7delt", "un\u00b7ter", "B\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$.", "PPER", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der holde Schlaf mit holdern Tr\u00e4umen.", "tokens": ["Der", "hol\u00b7de", "Schlaf", "mit", "hol\u00b7dern", "Tr\u00e4u\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Entz\u00fcckendes Revier!", "tokens": ["Ent\u00b7z\u00fc\u00b7cken\u00b7des", "Re\u00b7vier", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Dich, himmlisch Tempe, seh ich hier!", "tokens": ["Dich", ",", "himm\u00b7lisch", "Tem\u00b7pe", ",", "seh", "ich", "hier", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ADJD", "NN", "$,", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Hier, wo der Pelion,", "tokens": ["Hier", ",", "wo", "der", "Pe\u00b7li\u00b7on", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Wo der Olymp, der G\u00f6tter Thron,", "tokens": ["Wo", "der", "O\u00b7lymp", ",", "der", "G\u00f6t\u00b7ter", "Thron", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sich in die Wolken th\u00fcrmt aus heerdenvollen Matten:", "tokens": ["Sich", "in", "die", "Wol\u00b7ken", "th\u00fcrmt", "aus", "heer\u00b7den\u00b7vol\u00b7len", "Mat\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "In dieser gr\u00fcner Lorbeern Schatten", "tokens": ["In", "die\u00b7ser", "gr\u00fc\u00b7ner", "Lor\u00b7beern", "Schat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Gl\u00e4ntzt, als ein glatter See,", "tokens": ["Gl\u00e4ntzt", ",", "als", "ein", "glat\u00b7ter", "See", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Der Peneus durch bebl\u00fchmten Klee.", "tokens": ["Der", "Pe\u00b7neus", "durch", "be\u00b7bl\u00fchm\u00b7ten", "Klee", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Die Gegend ist so sch\u00f6n,", "tokens": ["Die", "Ge\u00b7gend", "ist", "so", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Da\u00df hier die Musen sich ergehn.", "tokens": ["Da\u00df", "hier", "die", "Mu\u00b7sen", "sich", "er\u00b7gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Thalien seh ich dort bedornte Rosen pfl\u00fccken:", "tokens": ["Tha\u00b7li\u00b7en", "seh", "ich", "dort", "be\u00b7dorn\u00b7te", "Ro\u00b7sen", "pfl\u00fc\u00b7cken", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADV", "ADJA", "NN", "VVINF", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.4": {"text": "Die Schalkheit spricht aus ihren Blicken;", "tokens": ["Die", "Schalk\u00b7heit", "spricht", "aus", "ih\u00b7ren", "Bli\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und ihren Mund beseelt", "tokens": ["Und", "ih\u00b7ren", "Mund", "be\u00b7seelt"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Ein L\u00e4cheln, das die Thoren qu\u00e4lt.", "tokens": ["Ein", "L\u00e4\u00b7cheln", ",", "das", "die", "Tho\u00b7ren", "qu\u00e4lt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Wer scherzt an ihrer Hand?", "tokens": ["Wer", "scherzt", "an", "ih\u00b7rer", "Hand", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Ists Clio, deren leicht Gewand", "tokens": ["Ists", "Clio", ",", "de\u00b7ren", "leicht", "Ge\u00b7wand"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NE", "$,", "PRELAT", "ADJD", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Nachl\u00e4ssig flatternd wallt und nicht mit Golde prahlet?", "tokens": ["Nach\u00b7l\u00e4s\u00b7sig", "flat\u00b7ternd", "wallt", "und", "nicht", "mit", "Gol\u00b7de", "prah\u00b7let", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "VVFIN", "KON", "PTKNEG", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Fontaine, der verewigt strahlet,", "tokens": ["Fon\u00b7tai\u00b7ne", ",", "der", "ve\u00b7re\u00b7wigt", "strah\u00b7let", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "VVPP", "VVFIN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Sang einst an ihrer Brust", "tokens": ["Sang", "einst", "an", "ih\u00b7rer", "Brust"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Von Hymens Qual und Amors Lust.", "tokens": ["Von", "Hy\u00b7mens", "Qual", "und", "A\u00b7mors", "Lust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "KON", "NE", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Du aber irrst allein,", "tokens": ["Du", "a\u00b7ber", "irrst", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "O Uranie! durch Thal und Hayn!", "tokens": ["O", "U\u00b7ra\u00b7nie", "!", "durch", "Thal", "und", "Hayn", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dein heilig Saitenspiel schl\u00e4ft unter stillem Laube:", "tokens": ["Dein", "hei\u00b7lig", "Sai\u00b7ten\u00b7spiel", "schl\u00e4ft", "un\u00b7ter", "stil\u00b7lem", "Lau\u00b7be", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Bis von verschm\u00e4htem niedern Staube", "tokens": ["Bis", "von", "ver\u00b7schm\u00e4h\u00b7tem", "nie\u00b7dern", "Stau\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "APPR", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Sich dein entbundner Geist", "tokens": ["Sich", "dein", "ent\u00b7bund\u00b7ner", "Geist"], "token_info": ["word", "word", "word", "word"], "pos": ["PRF", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Zum Himmel, seinem Ursprung, reisst.", "tokens": ["Zum", "Him\u00b7mel", ",", "sei\u00b7nem", "Ur\u00b7sprung", ",", "reisst", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PPOSAT", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Den Sternen schwingest du", "tokens": ["Den", "Ster\u00b7nen", "schwin\u00b7gest", "du"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Dein brausendes Gefieder zu,", "tokens": ["Dein", "brau\u00b7sen\u00b7des", "Ge\u00b7fie\u00b7der", "zu", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Durch unsre gr\u00f6bre Luft, die Werkstatt rother Blitze;", "tokens": ["Durch", "uns\u00b7re", "gr\u00f6b\u00b7re", "Luft", ",", "die", "Werk\u00b7statt", "ro\u00b7ther", "Blit\u00b7ze", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und wo, wann Gott von seinem Sitze", "tokens": ["Und", "wo", ",", "wann", "Gott", "von", "sei\u00b7nem", "Sit\u00b7ze"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "$,", "PWAV", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die Welt im Wetter schilt,", "tokens": ["Die", "Welt", "im", "Wet\u00b7ter", "schilt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Sein ausgesandter Donner br\u00fcllt.", "tokens": ["Sein", "aus\u00b7ge\u00b7sand\u00b7ter", "Don\u00b7ner", "br\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Du dringst Auroren nach", "tokens": ["Du", "dringst", "Au\u00b7ro\u00b7ren", "nach"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NE", "APPR"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "In ihr bepurpert Schlafgemach;", "tokens": ["In", "ihr", "be\u00b7pur\u00b7pert", "Schlaf\u00b7ge\u00b7mach", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und siehst aus blauer H\u00f6h die Erde silbern gl\u00e4nzen.", "tokens": ["Und", "siehst", "aus", "blau\u00b7er", "H\u00f6h", "die", "Er\u00b7de", "sil\u00b7bern", "gl\u00e4n\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "ART", "NN", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Bald reisst aus unsers Titans Gr\u00e4nzen", "tokens": ["Bald", "reisst", "aus", "un\u00b7sers", "Ti\u00b7tans", "Gr\u00e4n\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Dich dein entflammter Sinn", "tokens": ["Dich", "dein", "ent\u00b7flamm\u00b7ter", "Sinn"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "In andrer Sonnen Herrschaft hin.", "tokens": ["In", "an\u00b7drer", "Son\u00b7nen", "Herr\u00b7schaft", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Die Erde scheint wie Nichts", "tokens": ["Die", "Er\u00b7de", "scheint", "wie", "Nichts"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN",