{"dta.poem.814": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "I.  Auff den Sontag des Sanftm\u00fctigen K\u00f6-  \n nigs. Oder den  I.  der Zukunft Christi  \n Matth. 21.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Kom K\u00f6nig/ kom den offt dein Zion hat be- \ngehret! ", "tokens": ["Kom", "K\u00f6\u00b7nig", "/", "kom", "den", "offt", "dein", "Zi\u00b7on", "hat", "be", "geh\u00b7ret", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "$(", "VVFIN", "ART", "ADV", "PPOSAT", "NN", "VAFIN", "TRUNC", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Kom Davids Kind vnd HERR! Gott/", "tokens": ["Kom", "Da\u00b7vids", "Kind", "vnd", "HeRR", "!", "Gott", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "NE", "NN", "KON", "NN", "$.", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Helffer in der Noth", "tokens": ["Helf\u00b7fer", "in", "der", "Noth"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "APPR", "ART", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Vnd Zarter Menschen Sohn! Rei\u00df aus", "tokens": ["Vnd", "Zar\u00b7ter", "Men\u00b7schen", "Sohn", "!", "Rei\u00df", "aus"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "NN", "NN", "NN", "$.", "NN", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "dem S\u00fcnden koth", "tokens": ["dem", "S\u00fcn\u00b7den", "koth"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Die Seelen/ die Gesetz vnd S\u00fcnden last beschweret.", "tokens": ["Die", "See\u00b7len", "/", "die", "Ge\u00b7setz", "vnd", "S\u00fcn\u00b7den", "last", "be\u00b7schwe\u00b7ret", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "NN", "KON", "NN", "VVFIN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Erfrische was die glutt der H\u00f6llen hart verheeret!", "tokens": ["Er\u00b7fri\u00b7sche", "was", "die", "glutt", "der", "H\u00f6l\u00b7len", "hart", "ver\u00b7hee\u00b7ret", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PWS", "ART", "ADJD", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "O Leichter Lebens Taw/ erquicke was der Todt", "tokens": ["O", "Leich\u00b7ter", "Le\u00b7bens", "Taw", "/", "er\u00b7qui\u00b7cke", "was", "der", "Todt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "ADJA", "NN", "NN", "$(", "VVFIN", "PWS", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mit hartten F\u00fcssen tritt! kom s\u00fcsses Himmel brodt", "tokens": ["Mit", "hart\u00b7ten", "F\u00fcs\u00b7sen", "tritt", "!", "kom", "s\u00fcs\u00b7ses", "Him\u00b7mel", "brodt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$.", "APPRART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Vnd labe die/ die durst vnd hunger gantz verzehret.", "tokens": ["Vnd", "la\u00b7be", "die", "/", "die", "durst", "vnd", "hun\u00b7ger", "gantz", "ver\u00b7zeh\u00b7ret", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "$(", "ART", "NN", "KON", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Kom vnverf\u00e4lschte Lust/ wenn vns der Teufel schreckt.", "tokens": ["Kom", "vn\u00b7ver\u00b7f\u00e4lschte", "Lust", "/", "wenn", "vns", "der", "Teu\u00b7fel", "schreckt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$(", "KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Kom Licht! vnd scheine dem/ den Nacht vnd grawen deckt/", "tokens": ["Kom", "Licht", "!", "vnd", "schei\u00b7ne", "dem", "/", "den", "Nacht", "vnd", "gra\u00b7wen", "deckt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$.", "KON", "VVFIN", "ART", "$(", "ART", "NN", "KON", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Kom Friede! Kom zu den/ die Angst vnd Pein bekriegen.", "tokens": ["Kom", "Frie\u00b7de", "!", "Kom", "zu", "den", "/", "die", "Angst", "vnd", "Pein", "be\u00b7krie\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "$.", "VVFIN", "APPR", "ART", "$(", "ART", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "O Held vnd Helffer kom/ den aller V\u00f6lcker schar", "tokens": ["O", "Held", "vnd", "Helf\u00b7fer", "kom", "/", "den", "al\u00b7ler", "V\u00f6l\u00b7cker", "schar"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NN", "KON", "NN", "VVFIN", "$(", "ART", "PIAT", "NN", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zum Haupt v\u00f1 F\u00fcrsten w\u00fcndsch\u2019t/ v\u00f1 zeig vns offenbahr.", "tokens": ["Zum", "Haupt", "v\u00f1", "F\u00fcrs\u00b7ten", "w\u00fcnd\u00b7sch't", "/", "v\u00f1", "zeig", "vns", "of\u00b7fen\u00b7bahr", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "NN", "VVFIN", "$(", "NE", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Da\u00df wer dir widersteht mit spott m\u00fc\u00df vnten liegen.", "tokens": ["Da\u00df", "wer", "dir", "wi\u00b7der\u00b7steht", "mit", "spott", "m\u00fc\u00df", "vn\u00b7ten", "lie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PWS", "PPER", "VVFIN", "APPR", "NN", "VMFIN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}