{"dta.poem.14659": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Die Brautfahrt .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Monde steigen und sich neigen,", "tokens": ["Mon\u00b7de", "stei\u00b7gen", "und", "sich", "nei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVINF", "KON", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lieblich weht schon fremde Luft,", "tokens": ["Lieb\u00b7lich", "weht", "schon", "frem\u00b7de", "Luft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ADV", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da seh'n sie ein Eiland steigen", "tokens": ["Da", "seh'n", "sie", "ein", "Ei\u00b7land", "stei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Feenhaft aus blauem Duft,", "tokens": ["Fe\u00b7en\u00b7haft", "aus", "blau\u00b7em", "Duft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wie ein farb'ger Blumenstreifen \u2014", "tokens": ["Wie", "ein", "fa\u00b7rb'\u00b7ger", "Blu\u00b7men\u00b7strei\u00b7fen"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Meerw\u00e4rts fremde V\u00f6gel schweifen.", "tokens": ["Meer\u00b7w\u00e4rts", "frem\u00b7de", "V\u00f6\u00b7gel", "schwei\u00b7fen", "."], "token_info":