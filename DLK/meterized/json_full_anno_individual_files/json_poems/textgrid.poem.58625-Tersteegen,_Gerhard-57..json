{"textgrid.poem.58625": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "57.", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Untugend, gro\u00df und klein, Gott und die Seele scheidet,", "tokens": ["Un\u00b7tu\u00b7gend", ",", "gro\u00df", "und", "klein", ",", "Gott", "und", "die", "See\u00b7le", "schei\u00b7det", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "KON", "ADJD", "$,", "NN", "KON", "ART", "NN", "VVFIN", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Gleichwie die Finsternis verdeckt das Sonnenlicht;", "tokens": ["Gleich\u00b7wie", "die", "Fins\u00b7ter\u00b7nis", "ver\u00b7deckt", "das", "Son\u00b7nen\u00b7licht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wer nichts Unlauteres in seinem Herzen leidet,", "tokens": ["Wer", "nichts", "Un\u00b7lau\u00b7te\u00b7res", "in", "sei\u00b7nem", "Her\u00b7zen", "lei\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "ADJA", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der schauet endlich klar des Herren Angesicht.", "tokens": ["Der", "schau\u00b7et", "end\u00b7lich", "klar", "des", "Her\u00b7ren", "An\u00b7ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ADJD", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Schau nicht die Sterne an, Gott ist dir nicht so ferne,", "tokens": ["Schau", "nicht", "die", "Ster\u00b7ne", "an", ",", "Gott", "ist", "dir", "nicht", "so", "fer\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "ART", "NN", "PTKVZ", "$,", "NN", "VAFIN", "PPER", "PTKNEG", "ADV", "ADJA", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.6": {"text": "W\u00e4r' nur die S\u00fcnde weg, so k\u00f6nntest du ihn sehn;", "tokens": ["W\u00e4r'", "nur", "die", "S\u00fcn\u00b7de", "weg", ",", "so", "k\u00f6nn\u00b7test", "du", "ihn", "sehn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "PTKVZ", "$,", "ADV", "VMFIN", "PPER", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Gott ist dem Geiste nah und will sich zeigen gerne,", "tokens": ["Gott", "ist", "dem", "Geis\u00b7te", "nah", "und", "will", "sich", "zei\u00b7gen", "ger\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "ADJD", "KON", "VMFIN", "PRF", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ach, kehre da hinein, so kann es bald geschehn!", "tokens": ["Ach", ",", "keh\u00b7re", "da", "hin\u00b7ein", ",", "so", "kann", "es", "bald", "ge\u00b7schehn", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VVFIN", "ADV", "PTKVZ", "$,", "ADV", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Untugend, gro\u00df und klein, Gott und die Seele scheidet,", "tokens": ["Un\u00b7tu\u00b7gend", ",", "gro\u00df", "und