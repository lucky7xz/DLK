{"dta.poem.21535": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Ii.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Und wieder sah ich Opferd\u00fcfte wallen,", "tokens": ["Und", "wie\u00b7der", "sah", "ich", "Op\u00b7fer\u00b7d\u00fcf\u00b7te", "wal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den Weihrauch hoch gen Himmel zieh\u2019n", "tokens": ["Den", "Weih\u00b7rauch", "hoch", "gen", "Him\u00b7mel", "zieh'n"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "APPR", "NN", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In duft\u2019gen Wolken.", "tokens": ["In", "duft'\u00b7gen", "Wol\u00b7ken", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "\u2014 Es naht ein Zug \u2014", "tokens": ["Es", "naht", "ein", "Zug"], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "ART", "NN", "$("], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Vermummte Gestalten \u2014", "tokens": ["Ver\u00b7mumm\u00b7te", "Ge\u00b7stal\u00b7ten"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Teufelsfratzen grinsen mich an \u2014", "tokens": ["Teu\u00b7fels\u00b7frat\u00b7zen", "grin\u00b7sen", "mich", "an"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PTKVZ", "$("], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.7": {"text": "\u2014 \u201eGott der Liebe, Gott der Liebe!", "tokens": ["\u201e", "Gott", "der", "Lie\u00b7be", ",", "Gott", "der", "Lie\u00b7be", "!"], "token_info": ["punct", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "$(", "NN", "ART", "NN", "$,", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "\u2014 Und einen Scheiterhaufen sah ich hoch gerichtet,", "tokens": ["Und", "ei\u00b7nen", "Schei\u00b7ter\u00b7hau\u00b7fen", "sah", "ich", "hoch", "ge\u00b7rich\u00b7tet", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "ART", "NN", "VVFIN", "PPER", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.9": {"text": "Ein Kreuz darauf \u2014", "tokens": ["Ein", "Kreuz", "da\u00b7rauf"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PAV", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "Und Flammen sah ich gierig lecken", "tokens": ["Und", "Flam\u00b7men", "sah", "ich", "gie\u00b7rig", "le\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "VVFIN", "PPER", "ADJD", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Von unten hoch,", "tokens": ["Von", "un\u00b7ten", "hoch", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJD", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.12": {"text": "Und oben an dem Kreuze stand", "tokens": ["Und", "o\u00b7ben", "an", "dem", "Kreu\u00b7ze", "stand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Eine wei\u00dfe Gestalt,", "tokens": ["Ei\u00b7ne", "wei\u00b7\u00dfe", "Ge\u00b7stalt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.14": {"text": "Und in das Gewand,", "tokens": ["Und", "in", "das", "Ge\u00b7wand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.15": {"text": "Da hatten sie eingewirkt", "tokens": ["Da", "hat\u00b7ten", "sie", "ein\u00b7ge\u00b7wirkt"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "VVPP"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.16": {"text": "Rothe Zeichen \u2014", "tokens": ["Ro\u00b7the", "Zei\u00b7chen"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$("], "meter": "+-+-", "measure": "trochaic.di"}, "line.17": {"text": "Es war ein Weib,", "tokens": ["Es", "war", "ein", "Weib", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.18": {"text": "An dem wei\u00dfen Kleide", "tokens": ["An", "dem", "wei\u00b7\u00dfen", "Klei\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.19": {"text": "Troff es wie Blut \u2014", "tokens": ["Troff", "es", "wie", "Blut"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KOKOM", "NN", "$("], "meter": "+--+", "measure": "iambic.di.chol"}, "line.20": {"text": "Das waren blutige Male", "tokens": ["Das", "wa\u00b7ren", "blu\u00b7ti\u00b7ge", "Ma\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.21": {"text": "Der Taufe \u2014", "tokens": ["Der", "Tau\u00b7fe"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$("], "meter": "-+-", "measure": "amphibrach.single"}, "line.22": {"text": "Ihr tauft mit Blut,", "tokens": ["Ihr", "tauft", "mit", "Blut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.23": {"text": "Ihr treuen J\u00fcnger eures Herrn? \u2014", "tokens": ["Ihr", "treu\u00b7en", "J\u00fcn\u00b7ger", "eu\u00b7res", "Herrn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Wie sch\u00f6n das Weib ist,", "tokens": ["Wie", "sch\u00f6n", "das", "Weib", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.25": {"text": "Wie ihrer Glieder duft\u2019ge Weichheit", "tokens": ["Wie", "ih\u00b7rer", "Glie\u00b7der", "duft'\u00b7ge", "Weich\u00b7heit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Hervordr\u00e4ngt aus den festen Stricken,", "tokens": ["Her\u00b7vor\u00b7dr\u00e4ngt", "aus", "den", "fes\u00b7ten", "Stri\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.27": {"text": "Mit denen sie an\u2019s Kreuz geschn\u00fcrt.", "tokens": ["Mit", "de\u00b7nen", "sie", "an's", "Kreuz", "ge\u00b7schn\u00fcrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Die dunklen Augen blicken", "tokens": ["Die", "dunk\u00b7len", "Au\u00b7gen", "bli\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.29": {"text": "Zum reinen, wolkenlosen Himmel,", "tokens": ["Zum", "rei\u00b7nen", ",", "wol\u00b7ken\u00b7lo\u00b7sen", "Him\u00b7mel", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "$,", "VMFIN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.30": {"text": "Und Gottes gn\u00e4dige Sonne", "tokens": ["Und", "Got\u00b7tes", "gn\u00e4\u00b7di\u00b7ge", "Son\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NN", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.31": {"text": "Ihr in dem weichen Gelock,", "tokens": ["Ihr", "in", "dem", "wei\u00b7chen", "Ge\u00b7lock", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.32": {"text": "Das auf die wei\u00dfen Schultern niederwallt,", "tokens": ["Das", "auf", "die", "wei\u00b7\u00dfen", "Schul\u00b7tern", "nie\u00b7der\u00b7wallt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.33": {"text": "Goldene Strahlen spinnt.", "tokens": ["Gol\u00b7de\u00b7ne", "Strah\u00b7len", "spinnt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.34": {"text": "\u2014 Die Flammen prasseln", "tokens": ["Die", "Flam\u00b7men", "pras\u00b7seln"], "token_info": ["punct", "word", "word", "word"], "pos": ["$(", "ART", "NN", "VVINF"], "meter": "+-+-", "measure": "trochaic.di"}, "line.35": {"text": "Und z\u00fcngeln roth sich h\u00f6her \u2014", "tokens": ["Und", "z\u00fcn\u00b7geln", "roth", "sich", "h\u00f6\u00b7her"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "PRF", "ADJD", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.36": {"text": "Da bohrt sich ihr Blick", "tokens": ["Da", "bohrt", "sich", "ihr", "Blick"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "PPOSAT", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.37": {"text": "Mir in das Herz,", "tokens": ["Mir", "in", "das", "Herz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "$,"], "meter": "++-+", "measure": "iambic.di"}, "line.38": {"text": "Thr\u00e4nenlos \u2014 seelenlos \u2014", "tokens": ["Thr\u00e4\u00b7nen\u00b7los", "see\u00b7len\u00b7los"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$(", "ADJD", "$("], "meter": "+-+", "measure": "trochaic.di"}, "line.39": {"text": "Dunkel wie Nacht \u2014", "tokens": ["Dun\u00b7kel", "wie", "Nacht"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "NN", "$("], "meter": "+--+", "measure": "iambic.di.chol"}, "line.40": {"text": "Als ob sie nicht empf\u00e4nde. \u2014", "tokens": ["Als", "ob", "sie", "nicht", "em\u00b7pf\u00e4n\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "KOUS", "PPER", "PTKNEG", "VVFIN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.41": {"text": "\u201egott der Liebe, Gott der Liebe!", "tokens": ["\u201e", "gott", "der", "Lie\u00b7be", ",", "Gott", "der", "Lie\u00b7be", "!"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "NN", "ART", "NN", "$,", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.42": {"text": "Te deum laudamus!&#8220; &#8212;                 ", "tokens": ["Te", "deum", "lau\u00b7da\u00b7mus", "!", "&#8220;", "&#8212;"], "token_info": ["word", "word", "word", "punct", "XML_entity", "XML_entity"], "pos": ["FM.la", "FM.la", "FM.la", "$.", "$(", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.43": {"text": "Der weite Platz ist dicht gedr\u00e4ngt", "tokens": ["Der", "wei\u00b7te", "Platz", "ist", "dicht", "ge\u00b7dr\u00e4ngt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADJD", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.44": {"text": "Vom Volk, das liebt ja Schaugepr\u00e4nge \u2014", "tokens": ["Vom", "Volk", ",", "das", "liebt", "ja", "Schau\u00b7ge\u00b7pr\u00e4n\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PDS", "VVFIN", "ADV", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.45": {"text": "Was bist du Mensch f\u00fcr ein Gew\u00fcrm,", "tokens": ["Was", "bist", "du", "Mensch", "f\u00fcr", "ein", "Ge\u00b7w\u00fcrm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.46": {"text": "Da\u00df du die reinste Gabe,", "tokens": ["Da\u00df", "du", "die", "reins\u00b7te", "Ga\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.47": {"text": "Die dir je geboten,", "tokens": ["Die", "dir", "je", "ge\u00b7bo\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.48": {"text": "Besudelst.", "tokens": ["Be\u00b7su\u00b7delst", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.49": {"text": "Gibt man dir den Himmel,", "tokens": ["Gibt", "man", "dir", "den", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.50": {"text": "Gibt man dir das Gl\u00fcck \u2014", "tokens": ["Gibt", "man", "dir", "das", "Gl\u00fcck"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PPER", "ART", "NN", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.51": {"text": "Du zerrst es nieder", "tokens": ["Du", "zerrst", "es", "nie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ"], "meter": "-+-+-", "measure": "iambic.di"}, "line.52": {"text": "In deiner Laster Unverstand;", "tokens": ["In", "dei\u00b7ner", "Las\u00b7ter", "Un\u00b7ver\u00b7stand", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.53": {"text": "So wie ein Thier, das nichts genie\u00dft,", "tokens": ["So", "wie", "ein", "Thier", ",", "das", "nichts", "ge\u00b7nie\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ART", "NN", "$,", "PRELS", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.54": {"text": "Was nicht mit eig\u2019nem Safte erst zersetzt. \u2014 \u2014", "tokens": ["Was", "nicht", "mit", "eig'\u00b7nem", "Saf\u00b7te", "erst", "zer\u00b7setzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PWS", "PTKNEG", "APPR", "ADJA", "NN", "ADV", "VVPP", "$.", "$(", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.55": {"text": "\u201egott der Liebe, Gott der Liebe!", "tokens": ["\u201e", "gott", "der", "Lie\u00b7be", ",", "Gott", "der", "Lie\u00b7be", "!"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "NN", "ART", "NN", "$,", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.56": {"text": "Te deum laudamus!&#8220;                 ", "tokens": ["Te", "deum", "lau\u00b7da\u00b7mus", "!", "&#8220;"], "token_info": ["word", "word", "word", "punct", "XML_entity"], "pos": ["FM.la", "FM.la", "FM.la", "$.", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.57": {"text": "Und lauter wird der heilige Gesang", "tokens": ["Und", "lau\u00b7ter", "wird", "der", "hei\u00b7li\u00b7ge", "Ge\u00b7sang"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.58": {"text": "Und dichter wirbelten die Weihrauchwolken", "tokens": ["Und", "dich\u00b7ter", "wir\u00b7bel\u00b7ten", "die", "Weih\u00b7rauch\u00b7wol\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.59": {"text": "Und h\u00f6her rannte die Flamme", "tokens": ["Und", "h\u00f6\u00b7her", "rann\u00b7te", "die", "Flam\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.60": {"text": "Blutroth \u2014", "tokens": ["Blut\u00b7roth"], "token_info": ["word", "punct"], "pos": ["NN", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.61": {"text": "Ein letzter Blick \u2014", "tokens": ["Ein", "letz\u00b7ter", "Blick"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.62": {"text": "Opfersang \u2014 Weihrauchduft \u2014", "tokens": ["Op\u00b7fer\u00b7sang", "Weih\u00b7rauch\u00b7duft"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$(", "NN", "$("], "meter": "+-+", "measure": "trochaic.di"}, "line.63": {"text": "\u201egott der Liebe, Gott der Liebe!", "tokens": ["\u201e", "gott", "der", "Lie\u00b7be", ",", "Gott", "der", "Lie\u00b7be", "!"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "NN", "ART", "NN", "$,", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.64": {"text": "Te deum laudamus!&#8220; &#8212;               ", "tokens": ["Te", "deum", "lau\u00b7da\u00b7mus", "!", "&#8220;", "&#8212;"], "token_info": ["word", "word", "word", "punct", "XML_entity", "XML_entity"], "pos": ["FM.la", "FM.la", "FM.la", "$.", "$(", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}