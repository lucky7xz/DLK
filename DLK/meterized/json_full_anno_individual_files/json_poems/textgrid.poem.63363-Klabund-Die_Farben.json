{"textgrid.poem.63363": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "Die Farben", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich habe, Jahr, dein Sinnbild bald erbeutet:", "tokens": ["Ich", "ha\u00b7be", ",", "Jahr", ",", "dein", "Sinn\u00b7bild", "bald", "er\u00b7beu\u00b7tet", ":"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "NN", "$,", "PPOSAT", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Du Coeur bist Fr\u00fchlingsblut \u2013 und Bl\u00fctenfarbe.", "tokens": ["Du", "Co\u00b7eur", "bist", "Fr\u00fch\u00b7lings\u00b7blut", "\u2013", "und", "Bl\u00fc\u00b7ten\u00b7far\u00b7be", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "NN", "VAFIN", "NN", "$(", "KON", "NN", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Du Caro bindest Sonnenschein zur Garbe,", "tokens": ["Du", "Ca\u00b7ro", "bin\u00b7dest", "Son\u00b7nen\u00b7schein", "zur", "Gar\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NE", "VVFIN", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Du Pik bist Glocke, die zum Herbste l\u00e4utet.", "tokens": ["Du", "Pik", "bist", "Glo\u00b7cke", ",", "die", "zum", "Herbs\u00b7te", "l\u00e4u\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "VAFIN", "NN", "$,", "PRELS", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wenn Hund und Mensch sich dann im Winter h\u00e4utet,", "tokens": ["Wenn", "Hund"