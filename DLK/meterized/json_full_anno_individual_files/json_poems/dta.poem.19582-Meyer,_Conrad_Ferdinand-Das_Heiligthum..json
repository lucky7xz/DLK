{"dta.poem.19582": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Das Heiligthum.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-200905193933", "language": ["de:0.99"], "booktitle": "Meyer, Conrad Ferdinand: Gedichte. Leipzig, 1882."}, "poem": {"stanza.1": {"line.1": {"text": "Waldnacht. Urm\u00e4cht'ge Eichen, unter die", "tokens": ["Wald\u00b7nacht", ".", "Ur\u00b7m\u00e4cht'\u00b7ge", "Ei\u00b7chen", ",", "un\u00b7ter", "die"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NN", "$.", "ADJA", "NN", "$,", "APPR", "ART"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Des Blitzes greller Strahl geleuchtet nie!", "tokens": ["Des", "Blit\u00b7zes", "grel\u00b7ler", "Strahl", "ge\u00b7leuch\u00b7tet", "nie", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVPP", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "D\u00e4mmernde W\u00f6lbung, Ast in Ast verwebt,", "tokens": ["D\u00e4m\u00b7mern\u00b7de", "W\u00f6l\u00b7bung", ",", "Ast", "in", "Ast", "ver\u00b7webt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.4": {"text": "Von keines Vogels Lustgeschrei belebt!", "tokens": ["Von", "kei\u00b7nes", "Vo\u00b7gels", "Lust\u00b7ge\u00b7schrei", "be\u00b7lebt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ein br\u00fctend Schweigen, nie vom Sturm gest\u00f6rt,", "tokens": ["Ein", "br\u00fc\u00b7tend", "Schwei\u00b7gen", ",", "nie", "vom", "Sturm", "ge\u00b7st\u00f6rt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "$,", "ADV", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Ein heilig Dunkel, das dem Gott geh\u00f6rt,", "tokens": ["Ein", "hei\u00b7lig", "Dun\u00b7kel", ",", "das", "dem", "Gott", "ge\u00b7h\u00f6rt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Darin, umblinkt von Sch\u00e4del und Gebein,", "tokens": ["Da\u00b7rin", ",", "um\u00b7blinkt", "von", "Sch\u00e4\u00b7del", "und", "Ge\u00b7bein", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "VVFIN", "APPR", "NE", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Sich ungewi\u00df erhebt ein Opferstein ...", "tokens": ["Sich", "un\u00b7ge\u00b7wi\u00df", "er\u00b7hebt", "ein", "Op\u00b7fer\u00b7stein", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Es rauscht. Es raschelt. Schritte durch den Wald!", "tokens": ["Es", "rauscht", ".", "Es", "ra\u00b7schelt", ".", "Schrit\u00b7te", "durch", "den", "Wald", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PPER", "VVFIN", "$.", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Das kurze r\u00f6mische Commando schallt.", "tokens": ["Das", "kur\u00b7ze", "r\u00f6\u00b7mi\u00b7sche", "Com\u00b7man\u00b7do", "schallt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Geleucht von Helmen! Eine reis'ge Schaar!", "tokens": ["Ge\u00b7leucht", "von", "Hel\u00b7men", "!", "Ei\u00b7ne", "reis'\u00b7ge", "Schaar", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$.", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Vorauf ein Gallier und ein Legionar:", "tokens": ["Vor\u00b7auf", "ein", "Gal\u00b7lier", "und", "ein", "Le\u00b7gi\u00b7o\u00b7nar", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.13": {"text": "\u201edie St\u00e4mme k\u00f6nnen dienen. Beil in Schwung!", "tokens": ["\u201e", "die", "St\u00e4m\u00b7me", "k\u00f6n\u00b7nen", "die\u00b7nen", ".", "Beil", "in", "Schwung", "!"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VMFIN", "VVINF", "$.", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "C\u00e4sar braucht Widder zur Belagerung!\u201c ", "tokens": ["C\u00e4\u00b7sar", "braucht", "Wid\u00b7der", "zur", "Be\u00b7la\u00b7ge\u00b7rung", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "VVFIN", "NN", "APPRART", "NN", "$.", "$("], "meter": "+--+-+-+--", "measure": "iambic.tetra.invert"}, "line.15": {"text": "Erbleichend spricht der Gallier ein Gebet,", "tokens": ["Er\u00b7blei\u00b7chend", "spricht", "der", "Gal\u00b7lier", "ein", "Ge\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.16": {"text": "Den R\u00f6mer auch ergreift die Majest\u00e4t", "tokens": ["Den", "R\u00f6\u00b7mer", "auch", "er\u00b7greift", "die", "Ma\u00b7jes\u00b7t\u00e4t"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Des Orts, doch hebt gehorchend er die Axt \u2014", "tokens": ["Des", "Orts", ",", "doch", "hebt", "ge\u00b7hor\u00b7chend", "er", "die", "Axt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "VVFIN", "VVPP", "PPER", "ART", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Der Gallier fl\u00fcstert: \u201eWei\u00dft du was du wagst?", "tokens": ["Der", "Gal\u00b7lier", "fl\u00fcs\u00b7tert", ":", "\u201e", "Wei\u00dft", "du", "was", "du", "wagst", "?"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "$(", "VVFIN", "PPER", "PWS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Die St\u00e4mme \u2014 diese Riesen \u2014 sind gefeit,", "tokens": ["Die", "St\u00e4m\u00b7me", "die\u00b7se", "Rie\u00b7sen", "sind", "ge\u00b7feit", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PDAT", "NN", "$(", "VAFIN", "VVPP", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.20": {"text": "Hier wohnt ein m\u00e4cht'ger Gott seit alter Zeit,", "tokens": ["Hier", "wohnt", "ein", "m\u00e4cht'\u00b7ger", "Gott", "seit", "al\u00b7ter", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "In dessen N\u00e4he nur der Priester tritt,", "tokens": ["In", "des\u00b7sen", "N\u00e4\u00b7he", "nur", "der", "Pries\u00b7ter", "tritt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Ein todtenblasses Opfer schleppt er mit.", "tokens": ["Ein", "tod\u00b7ten\u00b7blas\u00b7ses", "Op\u00b7fer", "schleppt", "er", "mit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "Versehrtest nur ein Blatt du freventlich,", "tokens": ["Ver\u00b7sehr\u00b7test", "nur", "ein", "Blatt", "du", "fre\u00b7vent\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.24": {"text": "Stracks kehrte sich die Waffe wider dich!\u201c ...", "tokens": ["Stracks", "kehr\u00b7te", "sich", "die", "Waf\u00b7fe", "wi\u00b7der", "dich", "!", "\u201c", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["NE", "VVFIN", "PRF", "ART", "NN", "APPR", "PPER", "$.", "$(", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "Die heil'gen Eichen drohen Baum an Baum,", "tokens": ["Die", "heil'\u00b7gen", "Ei\u00b7chen", "dro\u00b7hen", "Baum", "an", "Baum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "NE", "APPR", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.26": {"text": "Die R\u00f6mer lauschen bang und athmen kaum,", "tokens": ["Die", "R\u00f6\u00b7mer", "lau\u00b7schen", "bang", "und", "ath\u00b7men", "kaum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "ADJD", "KON", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.27": {"text": "Schwer, schwerer wird der Hand des Beiles Wucht", "tokens": ["Schwer", ",", "schwe\u00b7rer", "wird", "der", "Hand", "des", "Bei\u00b7les", "Wucht"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "$,", "ADJD", "VAFIN", "ART", "NN", "ART", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.28": {"text": "Und ihr entsinkt's. Sie st\u00fcrzen auf die Flucht.", "tokens": ["Und", "ihr", "ent\u00b7sinkt'", "s.", "Sie", "st\u00fcr\u00b7zen", "auf", "die", "Flucht", "."], "token_info": ["word", "word", "word", "abbreviation", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "VVIMP", "PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.29": {"text": "\u201esteht!\u201c Und sie stehn. Denn es ist C\u00e4sar's Ruf,", "tokens": ["\u201e", "steht", "!", "\u201c", "Und", "sie", "stehn", ".", "Denn", "es", "ist", "C\u00e4\u00b7sa\u00b7r's", "Ruf", ","], "token_info": ["punct", "word", "punct", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "$.", "$(", "KON", "PPER", "VVINF", "$.", "KON", "PPER", "VAFIN", "NE", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.30": {"text": "Der sie durch strenge Zucht zu M\u00e4nnern schuf!", "tokens": ["Der", "sie", "durch", "stren\u00b7ge", "Zucht", "zu", "M\u00e4n\u00b7nern", "schuf", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "ADJA", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.31": {"text": "Er ist bei seiner Schaar. Er deutet hin", "tokens": ["Er", "ist", "bei", "sei\u00b7ner", "Schaar", ".", "Er", "deu\u00b7tet", "hin"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "PPOSAT", "NN", "$.", "PPER", "VVFIN", "PTKVZ"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.32": {"text": "Auf eine Eiche. Sie umschlingen ihn,", "tokens": ["Auf", "ei\u00b7ne", "Ei\u00b7che", ".", "Sie", "um\u00b7schlin\u00b7gen", "ihn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$.", "PPER", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.33": {"text": "Sie decken ihn wie im Gedr\u00e4ng der Schlacht,", "tokens": ["Sie", "de\u00b7cken", "ihn", "wie", "im", "Ge\u00b7dr\u00e4ng", "der", "Schlacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "KOKOM", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.34": {"text": "Sie flehn. Er ringt. Er hat sich losgemacht,", "tokens": ["Sie", "flehn", ".", "Er", "ringt", ".", "Er", "hat", "sich", "los\u00b7ge\u00b7macht", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "$.", "PPER", "VVFIN", "$.", "PPER", "VAFIN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.35": {"text": "Er schreitet vor. Sie folgen. Er ergreift", "tokens": ["Er", "schrei\u00b7tet", "vor", ".", "Sie", "fol\u00b7gen", ".", "Er", "er\u00b7greift"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ", "$.", "PPER", "VVINF", "$.", "PPER", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.36": {"text": "Ein Beil, hebt's, f\u00fchrt den Schlag, der saust und pfeift ...", "tokens": ["Ein", "Beil", ",", "hebt's", ",", "f\u00fchrt", "den", "Schlag", ",", "der", "saust", "und", "pfeift", "..."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "$,", "VVFIN", "ART", "NN", "$,", "PRELS", "ADJD", "KON", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.37": {"text": "Sank er verwundet von dem frevlen Beil?", "tokens": ["Sank", "er", "ver\u00b7wun\u00b7det", "von", "dem", "frev\u00b7len", "Beil", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVPP", "APPR", "ART", "ADJA", "NN", "$."], "meter": "++-+-+-+-+", "measure": "iambic.penta.spondeus"}, "line.38": {"text": "Er l\u00e4chelt: \u201eSchauet, Kinder, ich bin heil!\u201c", "tokens": ["Er", "l\u00e4\u00b7chelt", ":", "\u201e", "Schau\u00b7et", ",", "Kin\u00b7der", ",", "ich", "bin", "heil", "!", "\u201c"], "token_info": ["word", "word", "punct", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "$.", "$(", "VVFIN", "$,", "NN", "$,", "PPER", "VAFIN", "ADJD", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.39": {"text": "Erstaunen! Jubel! Hohngel\u00e4chter! Spott!", "tokens": ["Er\u00b7stau\u00b7nen", "!", "Ju\u00b7bel", "!", "Hohn\u00b7ge\u00b7l\u00e4ch\u00b7ter", "!", "Spott", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$.", "NN", "$.", "NN", "$.", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.40": {"text": "Soldatenwitz: \u201eVerendet hat der Gott!\u201c", "tokens": ["Sol\u00b7da\u00b7ten\u00b7witz", ":", "\u201e", "Ver\u00b7en\u00b7det", "hat", "der", "Gott", "!", "\u201c"], "token_info": ["word", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "$.", "$(", "VVPP", "VAFIN", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.41": {"text": "Die Rinde fliegt! Des Stammes St\u00e4rke kracht!", "tokens": ["Die", "Rin\u00b7de", "fliegt", "!", "Des", "Stam\u00b7mes", "St\u00e4r\u00b7ke", "kracht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.42": {"text": "Von Laub zu dunklerm Laube flieht die Nacht.", "tokens": ["Von", "Laub", "zu", "dunk\u00b7lerm", "Lau\u00b7be", "flieht", "die", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ADJA", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.43": {"text": "Die Beile thun ihr Werk. Die W\u00f6lbung bricht \u2014", "tokens": ["Die", "Bei\u00b7le", "thun", "ihr", "Werk", ".", "Die", "W\u00f6l\u00b7bung", "bricht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "PPOSAT", "NN", "$.", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.44": {"text": "Auf Riesentr\u00fcmmer f\u00e4llt das wei\u00dfe Licht.", "tokens": ["Auf", "Rie\u00b7sen\u00b7tr\u00fcm\u00b7mer", "f\u00e4llt", "das", "wei\u00b7\u00dfe", "Licht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}