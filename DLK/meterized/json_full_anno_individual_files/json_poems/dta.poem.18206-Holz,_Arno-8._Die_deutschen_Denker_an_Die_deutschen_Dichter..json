{"dta.poem.18206": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "8.  \n  Die deutschen Denker  \n an  \n  Die deutschen Dichter.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1886", "urn": "urn:nbn:de:kobv:b4-200905192665", "language": ["de:0.99"], "booktitle": "Holz, Arno: Das Buch der Zeit. Lieder eines Modernen. Z\u00fcrich, 1886."}, "poem": {"stanza.1": {"line.1": {"text": "Wohl reiht ihr Reim an Reime", "tokens": ["Wohl", "reiht", "ihr", "Reim", "an", "Rei\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und f\u00fcgt zum Wort das Wort,", "tokens": ["Und", "f\u00fcgt", "zum", "Wort", "das", "Wort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch eurer Saaten Keime", "tokens": ["Doch", "eu\u00b7rer", "Saa\u00b7ten", "Kei\u00b7me"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Verdorrt, noch eh die Sichel", "tokens": ["Ver\u00b7dorrt", ",", "noch", "eh", "die", "Si\u00b7chel"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVPP", "$,", "ADV", "KOUS", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der Zeit sie j\u00e4h durchkracht", "tokens": ["Der", "Zeit", "sie", "j\u00e4h", "durch\u00b7kracht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PPER", "ADJD", "VVPP"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Und so dem deutschen Michel", "tokens": ["Und", "so", "dem", "deut\u00b7schen", "Mi\u00b7chel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Die Arbeit leichter macht.", "tokens": ["Die", "Ar\u00b7beit", "leich\u00b7ter", "macht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Denn ach, euch ging verloren", "tokens": ["Denn", "ach", ",", "euch", "ging", "ver\u00b7lo\u00b7ren"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "XY", "$,", "PPER", "VVFIN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Dinge Gang und Grund,", "tokens": ["Der", "Din\u00b7ge", "Gang", "und", "Grund", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ihr h\u00f6rt mit tauben Ohren", "tokens": ["Ihr", "h\u00f6rt", "mit", "tau\u00b7ben", "Oh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und sprecht mit stummem Mund.", "tokens": ["Und", "sprecht", "mit", "stum\u00b7mem", "Mund", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Doch wehe eurer Scheitel", "tokens": ["Doch", "we\u00b7he", "eu\u00b7rer", "Schei\u00b7tel"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJD", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Am Tage des Gerichts,", "tokens": ["Am", "Ta\u00b7ge", "des", "Ge\u00b7richts", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Denn was ihr singt ist eitel,", "tokens": ["Denn", "was", "ihr", "singt", "ist", "ei\u00b7tel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VVFIN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und was ihr sagt ist nichts!", "tokens": ["Und", "was", "ihr", "sagt", "ist", "nichts", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VVFIN", "VAFIN", "PIS", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und doch, ging je vor Zeiten", "tokens": ["Und", "doch", ",", "ging", "je", "vor", "Zei\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADV", "$,", "VVFIN", "ADV", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der S\u00e4nger mit dem Sieg,", "tokens": ["Der", "S\u00e4n\u00b7ger", "mit", "dem", "Sieg", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dann gilt es ", "tokens": ["Dann", "gilt", "es"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "In einem heilgen Krieg.", "tokens": ["In", "ei\u00b7nem", "heil\u00b7gen", "Krieg", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Denn nicht um Hof und Heerde", "tokens": ["Denn", "nicht", "um", "Hof", "und", "Heer\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PTKNEG", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Schl\u00e4gt unser Herz und schwillt:", "tokens": ["Schl\u00e4gt", "un\u00b7ser", "Herz", "und", "schwillt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "KON", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Heut ist's die ganze Erde,", "tokens": ["Heut", "ist's", "die", "gan\u00b7ze", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Der unser Sterben gilt!", "tokens": ["Der", "un\u00b7ser", "Ster\u00b7ben", "gilt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Seit Urbeginn schon g\u00e4hrte", "tokens": ["Seit", "Ur\u00b7be\u00b7ginn", "schon", "g\u00e4hr\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "ADV", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Es tief im Schoo\u00df der Zeit", "tokens": ["Es", "tief", "im", "Schoo\u00df", "der", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADJD", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und jede Stunde n\u00e4hrte", "tokens": ["Und", "je\u00b7de", "Stun\u00b7de", "n\u00e4hr\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Den grausen Widerstreit,", "tokens": ["Den", "grau\u00b7sen", "Wi\u00b7der\u00b7streit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Doch heute erst entrauchte", "tokens": ["Doch", "heu\u00b7te", "erst", "ent\u00b7rauch\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die Lohe ihrem Schacht", "tokens": ["Die", "Lo\u00b7he", "ih\u00b7rem", "Schacht"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und blutig \u00fcberhauchte", "tokens": ["Und", "blu\u00b7tig", "\u00fc\u00b7ber\u00b7hauch\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Sie das Gew\u00f6lk der Nacht.", "tokens": ["Sie", "das", "Ge\u00b7w\u00f6lk", "der", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Und weh, das Gl\u00fcck zerschellte,", "tokens": ["Und", "weh", ",", "das", "Gl\u00fcck", "zer\u00b7schell\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PTKVZ", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Was ganz war, brach entzwei,", "tokens": ["Was", "ganz", "war", ",", "brach", "ent\u00b7zwei", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "ADV", "VAFIN", "$,", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und durch die Lande gellte", "tokens": ["Und", "durch", "die", "Lan\u00b7de", "gell\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein einzig lauter Schrei.", "tokens": ["Ein", "ein\u00b7zig", "lau\u00b7ter", "Schrei", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Mit Mehlthau \u00fcbernetzte", "tokens": ["Mit", "Mehlt\u00b7hau", "\u00fc\u00b7ber\u00b7netz\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Das Feld sich weit und breit", "tokens": ["Das", "Feld", "sich", "weit", "und", "breit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PRF", "ADJD", "KON", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und es begann der letzte,", "tokens": ["Und", "es", "be\u00b7gann", "der", "letz\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Der B\u00fcrgerkrieg der Zeit.", "tokens": ["Der", "B\u00fcr\u00b7ger\u00b7krieg", "der", "Zeit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Nun rast er durch die Auen", "tokens": ["Nun", "rast", "er", "durch", "die", "Au\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und spielt sein wildes Spiel", "tokens": ["Und", "spielt", "sein", "wil\u00b7des", "Spiel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und uns durchrinnt ein Grauen,", "tokens": ["Und", "uns", "durch\u00b7rinnt", "ein", "Grau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Bedenken wir sein Ziel.", "tokens": ["Be\u00b7den\u00b7ken", "wir", "sein", "Ziel", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die Tafel der Gesetze", "tokens": ["Die", "Ta\u00b7fel", "der", "Ge\u00b7set\u00b7ze"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Zerbarst wie spr\u00f6des Glas,", "tokens": ["Zer\u00b7barst", "wie", "spr\u00f6\u00b7des", "Glas", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Die Tugend ward zur Metze,", "tokens": ["Die", "Tu\u00b7gend", "ward", "zur", "Met\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die Liebe ward zum Ha\u00df.", "tokens": ["Die", "Lie\u00b7be", "ward", "zum", "Ha\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Die Wahrheit liegt im Staube,", "tokens": ["Die", "Wahr\u00b7heit", "liegt", "im", "Stau\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Hoffnung sitzt und weint,", "tokens": ["Die", "Hoff\u00b7nung", "sitzt", "und", "weint", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Gestorben ist der Glaube", "tokens": ["Ge\u00b7stor\u00b7ben", "ist", "der", "Glau\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und ach, das Herz versteint!", "tokens": ["Und", "ach", ",", "das", "Herz", "ver\u00b7steint", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$,", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Des Wahnsinns Schlangen zischen", "tokens": ["Des", "Wahn\u00b7sinns", "Schlan\u00b7gen", "zi\u00b7schen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und Alp th\u00fcrmt sich auf Alp", "tokens": ["Und", "Alp", "th\u00fcrmt", "sich", "auf", "Alp"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "VVFIN", "PRF", "APPR", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und w\u00fcst erschallt dazwischen", "tokens": ["Und", "w\u00fcst", "er\u00b7schallt", "da\u00b7zwi\u00b7schen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "VVFIN", "PAV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Der Tanz ums goldne Kalb.", "tokens": ["Der", "Tanz", "ums", "gold\u00b7ne", "Kalb", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Doch nahn schon Gottes Boten", "tokens": ["Doch", "nahn", "schon", "Got\u00b7tes", "Bo\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "ADV", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und ihre Stimme spricht:", "tokens": ["Und", "ih\u00b7re", "Stim\u00b7me", "spricht", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u201elebendig sind die Todten", "tokens": ["\u201e", "le\u00b7ben\u00b7dig", "sind", "die", "Tod\u00b7ten"], "token_info": ["punct", "word", "word", "word", "word"], "pos": ["$(", "ADJD", "VAFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und nahe das Gericht!\u201c", "tokens": ["Und", "na\u00b7he", "das", "Ge\u00b7richt", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der Erdball wankt und zittert,", "tokens": ["Der", "Erd\u00b7ball", "wankt", "und", "zit\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Des Himmels Wolken drohn", "tokens": ["Des", "Him\u00b7mels", "Wol\u00b7ken", "drohn"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und durch die Lande wittert", "tokens": ["Und", "durch", "die", "Lan\u00b7de", "wit\u00b7tert"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Der Hauch des Todes schon.", "tokens": ["Der", "Hauch", "des", "To\u00b7des", "schon", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Ihr aber, die zu W\u00e4chtern", "tokens": ["Ihr", "a\u00b7ber", ",", "die", "zu", "W\u00e4ch\u00b7tern"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "ADV", "$,", "PRELS", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Des Heiligthums bestellt,", "tokens": ["Des", "Hei\u00b7lig\u00b7thums", "be\u00b7stellt", ","], "token_info": ["word", "word", "word", "pu