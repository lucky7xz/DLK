{"textgrid.poem.52019": {"metadata": {"author": {"name": "Czepko von Reigersfeld, Daniel", "birth": "N.A.", "death": "N.A."}, "title": "44.", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als Cynthia ihr Thier im goldnen Kettlein hielt,", "tokens": ["Als", "Cyn\u00b7thia", "ihr", "Thier", "im", "gold\u00b7nen", "Ket\u00b7tlein", "hielt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "PPOSAT", "NN", "APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Das unser Deutsche nennt vom Horn und von der Eichen,", "tokens": ["Das", "un\u00b7ser", "Deut\u00b7sche", "nennt", "vom", "Horn", "und", "von", "der", "Ei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "NN", "VVFIN", "APPRART", "NN", "KON", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Nahm es ihr Buhler ihr, und wolt es freundlich streichen,", "tokens": ["Nahm", "es", "ihr", "Buh\u00b7ler", "ihr", ",", "und", "wolt", "es", "freund\u00b7lich", "strei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "PPER", "$,", "KON", "VMFIN", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und nennt es sch\u00f6n und werth von ihrer Huld erf\u00fcllt.", "tokens": ["Und", "nennt", "es", "sch\u00f6n", "und", "werth", "von", "ih\u00b7rer", "Huld", "er\u00b7f\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Als er sich b\u00fcckt und k\u00fcst aus Demuth ihre Schuh,", "tokens": ["Als", "er", "sich", "b\u00fcckt", "und", "k\u00fcst", "aus", "De\u00b7muth", "ih\u00b7re", "Schuh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVFIN", "KON", "VVFIN", "APPR", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "F\u00e4hrt ihm der K\u00e4ffer auff, wie fest er zugeschlossen,", "tokens": ["F\u00e4hrt", "ihm", "der", "K\u00e4f\u00b7fer", "auff", ",", "wie", "fest", "er", "zu\u00b7ge\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKVZ", "$,", "PWAV", "ADJD", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Drein springt der kleine Narr, und reist ihm diesen Possen,", "tokens": ["Drein", "springt", "der", "klei\u00b7ne", "Narr", ",", "und", "reist", "ihm", "die\u00b7sen", "Pos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,", "KON", "VVFIN", "PPER", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Was sch\u00e4mstu dich [?] dis Thier spricht seinem Bruder zu.", "tokens": ["Was", "sch\u00e4ms\u00b7tu", "dich", "?", "dis", "Thier", "spricht", "sei\u00b7nem", "Bru\u00b7der", "zu", "."], "token_info": ["word", "word", "word", "punct", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$(", "$.", "$(", "PDS", "NN", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$."], "met