{"dta.poem.12233": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Sr .  Excellenz  \n des Herrn Geheimerath von G\u00f6the .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1806", "urn": "urn:nbn:de:kobv:b4-20090519157", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201eich stund auf an eim Morgen,", "tokens": ["\u201e", "ich", "stund", "auf", "an", "eim", "Mor\u00b7gen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "APPR", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "\u201eund wollt gen M\u00fcnchen gehn,", "tokens": ["\u201e", "und", "wollt", "gen", "M\u00fcn\u00b7chen", "gehn", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "VMFIN", "APPR", "NE", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u201eund war in gro\u00dfen Sorgen,", "tokens": ["\u201e", "und", "war", "in", "gro\u00b7\u00dfen", "Sor\u00b7gen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "VAFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "\u201each Gott w\u00e4r ich davon,", "tokens": ["\u201e", "ach", "Gott", "w\u00e4r", "ich", "da\u00b7von", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "XY", "NN", "VAFIN", "PPER", "PAV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "\u201emein Wirth, dem war ich schuldig viel,", "tokens": ["\u201e", "mein", "Wirth", ",", "dem", "war", "ich", "schul\u00b7dig", "viel", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "$,", "PDS", "VAFIN", "PPER", "ADJD", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "\u201eich wollt ihn gern bezahlen,", "tokens": ["\u201e", "ich", "wollt", "ihn", "gern", "be\u00b7zah\u00b7len", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "\u201edoch auf ein ander Ziel.", "tokens": ["\u201e", "doch", "auf", "ein", "an\u00b7der", "Ziel", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "\u201eherr Gast ich hab vernommen,", "tokens": ["\u201e", "herr", "Gast", "ich", "hab", "ver\u00b7nom\u00b7men", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJA", "NN", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "\u201edu w\u00f6llest von hinnen schier,", "tokens": ["\u201e", "du", "w\u00f6l\u00b7lest", "von", "hin\u00b7nen", "schier", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "APPR", "ADV", "ADJD", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "\u201eich la\u00df dich nicht weg kommen,", "tokens": ["\u201e", "ich", "la\u00df", "dich", "nicht", "weg", "kom\u00b7men", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVIMP", "PPER", "PTKNEG", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "\u201edie Zehrung zahl vor mir,", "tokens": ["\u201e", "die", "Zeh\u00b7rung", "zahl", "vor", "mir", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "NN", "APPR", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "\u201eoder setz mir den Mantel ein,", "tokens": ["\u201e", "o\u00b7der", "setz", "mir", "den", "Man\u00b7tel", "ein", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "VVIMP", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.6": {"text": "\u201edemnach will ich gern warten,", "tokens": ["\u201e", "dem\u00b7nach", "will", "ich", "gern", "war\u00b7ten", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PAV", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "\u201eauf die Bezahlung dein.", "tokens": ["\u201e", "auf", "die", "Be\u00b7zah\u00b7lung", "dein", "."], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "ART", "NN", "PPOSAT", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.3": {"line.1": {"text": "\u201edie Red ging mir zu Herzen,", "tokens": ["\u201e", "die", "Red", "ging", "mir", "zu", "Her\u00b7zen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "\u201ebetr\u00fcbt ward mir mein Muth,", "tokens": ["\u201e", "be\u00b7tr\u00fcbt", "ward", "mir", "mein", "Muth", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVPP", "VAFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u201eich dacht, da hilft kein Scherzen,", "tokens": ["\u201e", "ich", "dacht", ",", "da", "hilft", "kein", "Scher\u00b7zen", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "$,", "ADV", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "\u201esollt ich mein Mantel gut", "tokens": ["\u201e", "sollt", "ich", "mein", "Man\u00b7tel", "gut"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "VMFIN", "PPER", "PPOSAT", "NN", "ADJD"], "meter": "---+-+", "measure": "unknown.measure.di"}, "line.5": {"text": "\u201ezu Augsburg lassen auf der Gant", "tokens": ["\u201e", "zu", "Augs\u00b7burg", "las\u00b7sen", "auf", "der", "Gant"], "token_info": ["punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "APPR", "NE", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "\u201eund blos von hinnen ziehen,", "tokens": ["\u201e", "und", "blos", "von", "hin\u00b7nen", "zie\u00b7hen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "ADV", "APPR", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "\u201eist allen Singern ein Schand.", "tokens": ["\u201e", "ist", "al\u00b7len", "Sin\u00b7gern", "ein", "Schand", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "PIAT", "NN", "ART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.4": {"line.1": {"text": "\u201each Wirth nun hab Gedulte", "tokens": ["\u201e", "ach", "Wirth", "nun", "hab", "Ge\u00b7dul\u00b7te"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "XY", "NN", "ADV", "VAFIN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "\u201emit mir ein kleine Zeit,", "tokens": ["\u201e", "mit", "mir", "ein", "klei\u00b7ne", "Zeit", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u201ees ist nicht gros die Schulde,", "tokens": ["\u201e", "es", "ist", "nicht", "gros", "die", "Schul\u00b7de", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "PTKNEG", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "\u201evielleicht sich bald begeit,", "tokens": ["\u201e", "viel\u00b7leicht", "sich", "bald", "be\u00b7geit", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "PRF", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "\u201eda\u00df ich dich zahl mit baarem Geld,", "tokens": ["\u201e", "da\u00df", "ich", "dich", "zahl", "mit", "baa\u00b7rem", "Geld", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PPER", "PRF", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "\u201edrum lasse mich von hinnen,", "tokens": ["\u201e", "drum", "las\u00b7se", "mich", "von", "hin\u00b7nen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PAV", "VVFIN", "PRF", "APPR", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "\u201eich zieh nicht aus der Welt.", "tokens": ["\u201e", "ich", "zieh", "nicht", "aus", "der", "Welt", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PTKNEG", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "\u201eo Gast! das geschieht mit nichten,", "tokens": ["\u201e", "o", "Gast", "!", "das", "ge\u00b7schieht", "mit", "nich\u00b7ten", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM", "NN", "$.", "PDS", "VVFIN", "APPR", "PIS", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "\u201eda\u00df ich dir borg die\u00dfmal,", "tokens": ["\u201e", "da\u00df", "ich", "dir", "borg", "die\u00df\u00b7mal", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PPER", "PPER", "VVFIN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u201edich hilft kein Ausred-Dichten,", "tokens": ["\u201e", "dich", "hilft", "kein", "Aus\u00b7re\u00b7dDich\u00b7ten", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "\u201etag Nacht wollst du seyn voll,", "tokens": ["\u201e", "tag", "Nacht", "wollst", "du", "seyn", "voll", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "NN", "NN", "VMFIN", "PPER", "PPOSAT", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "\u201eich trug dir auf den besten Wein,", "tokens": ["\u201e", "ich", "trug", "dir", "auf", "den", "bes\u00b7ten", "Wein", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "\u201edrum mach dich nur nicht m\u00fc\u00dfig,", "tokens": ["\u201e", "drum", "mach", "dich", "nur", "nicht", "m\u00fc\u00b7\u00dfig", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PAV", "VVFIN", "PPER", "ADV", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "\u201eich will bezahlet seyn.", "tokens": ["\u201e", "ich", "will", "be\u00b7zah\u00b7let", "seyn", "."], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VMFIN", "VVPP", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "\u201eder Wirth, der sah ganz krumme,", "tokens": ["\u201e", "der", "Wirth", ",", "der", "sah", "ganz", "krum\u00b7me", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "$,", "PRELS", "VVFIN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "\u201ewas ich sang oder sagt,", "tokens": ["\u201e", "was", "ich", "sang", "o\u00b7der", "sagt", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "PPER", "ADJD", "KON", "VVFIN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "\u201eso gab er nichts darumme,", "tokens": ["\u201e", "so", "gab", "er", "nichts", "da\u00b7rum\u00b7me", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPER", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "\u201eerst macht er mich verzagt,", "tokens": ["\u201e", "erst", "macht", "er", "mich", "ver\u00b7zagt", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPER", "PRF", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "\u201ekein Geld wu\u00dft ich in solcher Noth,", "tokens": ["\u201e", "kein", "Geld", "wu\u00dft", "ich", "in", "sol\u00b7cher", "Noth", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PIAT", "NN", "VVFIN", "PPER", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "\u201ewo nicht der fromm Herr Fuker", "tokens": ["\u201e", "wo", "nicht", "der", "fromm", "Herr", "Fu\u00b7ker"], "token_info": ["punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "PWAV", "PTKNEG", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "\u201emir hilft mit seinem Rath.", "tokens": ["\u201e", "mir", "hilft", "mit", "sei\u00b7nem", "Rath", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "\u201eherr Fuker la\u00dft Euch erbarmen", "tokens": ["\u201e", "herr", "Fu\u00b7ker", "la\u00dft", "Euch", "er\u00b7bar\u00b7men"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "ADJA", "NN", "VVFIN", "PPER", "VVINF"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "\u201emein Klag und gro\u00dfe Pein", "tokens": ["\u201e", "mein", "Klag", "und", "gro\u00b7\u00dfe", "Pein"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "PPOSAT", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u201eund kommt zu H\u00fclf mir Armen,", "tokens": ["\u201e", "und", "kommt", "zu", "H\u00fclf", "mir", "Ar\u00b7men", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "VVFIN", "APPR", "NN", "PPER", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "\u201ees will bezahlet seyn", "tokens": ["\u201e", "es", "will", "be\u00b7zah\u00b7let", "seyn"], "token_info": ["punct", "word", "word", "word", "word"], "pos": ["$(", "PPER", "VMFIN", "VVPP", "VAINF"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "\u201emein Wirth von mir auf diesen Tag,", "tokens": ["\u201e", "mein", "Wirth", "von", "mir", "auf", "die\u00b7sen", "Tag", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "APPR", "PPER", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "\u201emein Mantel thut ihm gefallen,", "tokens": ["\u201e", "mein", "Man\u00b7tel", "thut", "ihm", "ge\u00b7fal\u00b7len", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "VVF