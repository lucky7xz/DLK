{"textgrid.poem.54394": {"metadata": {"author": {"name": "Ziegler, Christiana Mariana von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ach Freundinn! du bist todt! der Schmerz hemmt mir die Zunge,", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach Freundinn! du bist todt! der Schmerz hemmt mir die Zunge,", "tokens": ["Ach", "Freun\u00b7dinn", "!", "du", "bist", "todt", "!", "der", "Schmerz", "hemmt", "mir", "die", "Zun\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "$.", "PPER", "VAFIN", "ADJD", "$.", "ART", "NN", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-++--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "O Ungl\u00fcck! das mir schon mein Allerliebstes raubt.", "tokens": ["O", "Un\u00b7gl\u00fcck", "!", "das", "mir", "schon", "mein", "Al\u00b7ler\u00b7liebs\u00b7tes", "raubt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "PRELS", "PPER", "ADV", "PPOSAT", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So bald die Trauerpost in meine Ohren drunge", "tokens": ["So", "bald", "die", "Trau\u00b7er\u00b7post", "in", "mei\u00b7ne", "Oh\u00b7ren", "drun\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So sagte mirs mein Herz, sonst h\u00e4tt ich nicht geglaubt.", "tokens": ["So", "sag\u00b7te", "mirs", "mein", "Herz", ",", "sonst", "h\u00e4tt", "ich", "nicht", "ge\u00b7glaubt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "PPOSAT", "NN", "$,", "ADV", "VAFIN", "PPER", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Mein Kummer ist gerecht, gerecht sind meine Klagen;", "tokens": ["Mein", "Kum\u00b7mer", "ist", "ge\u00b7recht", ",", "ge\u00b7recht", "sind", "mei\u00b7ne", "Kla\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$,", "ADJD", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ich bin den Tauben gleich, ich suche Wald und Thal,", "tokens": ["Ich", "bin", "den", "Tau\u00b7ben", "gleich", ",", "ich", "su\u00b7che", "Wald", "und", "Thal", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADV", "$,", "PPER", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Dich meiner Augen Lust, mein alles auszufragen,", "tokens": ["Dich", "mei\u00b7ner", "Au\u00b7gen", "Lust", ",", "mein", "al\u00b7les", "aus\u00b7zu\u00b7fra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "NN", "$,", "PPOSAT", "PIS", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Doch alles ist umsonst; drum mehrt sich meine Qual.", "tokens": ["Doch", "al\u00b7les", "ist", "um\u00b7sonst", ";", "drum", "mehrt", "sich", "mei\u00b7ne", "Qual", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "ADV", "$.", "PAV", "VVFIN", "PRF", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Dein Geist begleitet mich, ich sehe dich im Schatten,", "tokens": ["Dein", "Geist", "be\u00b7glei\u00b7tet", "mich", ",", "ich", "se\u00b7he", "dich", "im", "Schat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "$,", "PPER", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Ich fahre oftmals auf, und denk ich fasse dich,", "tokens": ["Ich", "fah\u00b7re", "oft\u00b7mals", "auf", ",", "und", "denk", "ich", "fas\u00b7se", "dich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PTKVZ", "$,", "KON", "VVFIN", "PPER", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Allein so ists ein Schein mit dem ich mich wil gatten;", "tokens": ["Al\u00b7lein", "so", "ists", "ein", "Schein", "mit", "dem", "ich", "mich", "wil", "gat\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "ART", "NN", "APPR", "PRELS", "PPER", "PRF", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "So mehret sich die Pein, so fri\u00dft der Kummer mich!", "tokens": ["So", "meh\u00b7ret", "sich", "die", "Pein", ",", "so", "fri\u00dft", "der", "Kum\u00b7mer", "mich", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "$,", "ADV", "VVFIN", "ART", "NN", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "O deckten mich doch auch schon k\u00fchler Sand und Steine!", "tokens": ["O", "deck\u00b7ten", "mich", "doch", "auch", "schon", "k\u00fch\u00b7ler", "Sand", "und", "Stei\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADV", "ADV", "ADV", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Denn ich bin in der That nun meines Lebens satt.", "tokens": ["Denn", "ich", "bin", "in", "der", "That", "nun", "mei\u00b7nes", "Le\u00b7bens", "satt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "APPR", "ART", "NN", "ADV", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "So oft ich den Verlust und deinen Werth beweine,", "tokens": ["So", "oft", "ich", "den", "Ver\u00b7lust", "und", "dei\u00b7nen", "Werth", "be\u00b7wei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "ART", "NN", "KON", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "So oft werd ich auch schwach und von dem Seufzen matt.", "tokens": ["So", "oft", "werd", "ich", "auch", "schwach", "und", "von", "dem", "Seuf\u00b7zen", "matt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "ADV", "ADJD", "KON", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Kaum da\u00df ich mich erholt, k\u00fc\u00df ich die kalten Wangen;", "tokens": ["Kaum", "da\u00df", "ich", "mich", "er\u00b7holt", ",", "k\u00fc\u00df", "ich", "die", "kal\u00b7ten", "Wan\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "PRF", "VVPP", "$,", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Doch Schicksal, k\u00fcrze nur mein Leben durch den Tod:", "tokens": ["Doch", "Schick\u00b7sal", ",", "k\u00fcr\u00b7ze", "nur", "mein", "Le\u00b7ben", "durch", "den", "Tod", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "VVFIN", "ADV", "PPOSAT", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Der soll mir s\u00fcsser seyn als was ich kann erlangen;", "tokens": ["Der", "soll", "mir", "s\u00fcs\u00b7ser", "seyn", "als", "was", "ich", "kann", "er\u00b7lan\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "ADJD", "VAINF", "KOKOM", "PWS", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Denn jeder Blick vermehrt hinf\u00fchro meine Noth.", "tokens": ["Denn", "je\u00b7der", "Blick", "ver\u00b7mehrt", "hin\u00b7f\u00fch\u00b7ro", "mei\u00b7ne", "Noth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "ADJD", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Indessen schwer ich dir, ich bleibe stets die deine,", "tokens": ["In\u00b7des\u00b7sen", "schwer", "ich", "dir", ",", "ich", "blei\u00b7be", "stets", "die", "dei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "PPER", "$,", "PPER", "VVFIN", "ADV", "ART", "PPOSAT", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Man schlie\u00dft mein treues Herz mit in dein Grab hinein.", "tokens": ["Man", "schlie\u00dft", "mein", "treu\u00b7es", "Herz", "mit", "in", "dein", "Grab", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPOSAT", "ADJA", "NN", "APPR", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Ja du bleibst noch erbla\u00dft mein Liebstes und die meine", "tokens": ["Ja", "du", "bleibst", "noch", "er\u00b7bla\u00dft", "mein", "Liebs\u00b7tes", "und", "die", "mei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "PPER", "VVFIN", "ADV", "VVFIN", "PPOSAT", "NN", "KON", "ART", "PPOSAT"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Ich w\u00fcnsche sonsten nichts, als nur um dich zu seyn,", "tokens": ["Ich", "w\u00fcn\u00b7sche", "sons\u00b7ten", "nichts", ",", "als", "nur", "um", "dich", "zu", "seyn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PIS", "$,", "KOUS", "ADV", "APPR", "PPER", "PTKZU", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Doch der vergebne Wunsch verdoppelt mein Empfinden;", "tokens": ["Doch", "der", "ver\u00b7geb\u00b7ne", "Wunsch", "ver\u00b7dop\u00b7pelt", "mein", "Emp\u00b7fin\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Mein Leiden wird vermehret, die Marter wird stets neu;", "tokens": ["Mein", "Lei\u00b7den", "wird", "ver\u00b7meh\u00b7ret", ",", "die", "Mar\u00b7ter", "wird", "stets", "neu", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVFIN", "$,", "ART", "NN", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.27": {"text": "Der Schmerz nimmt immer zu, und bey dem H\u00e4nde winden", "tokens": ["Der", "Schmerz", "nimmt", "im\u00b7mer", "zu", ",", "und", "bey", "dem", "H\u00e4n\u00b7de", "win\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "PTKVZ", "$,", "KON", "APPR", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "L\u00e4\u00dft er die bange Brust nicht eine Stunde frey.", "tokens": ["L\u00e4\u00dft", "er", "die", "ban\u00b7ge", "Brust", "nicht", "ei\u00b7ne", "Stun\u00b7de", "frey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "PTKNEG", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Dein Ansehn, dein Verstand, dein Herz, dein ganzes Wesen", "tokens": ["Dein", "An\u00b7sehn", ",", "dein", "Ver\u00b7stand", ",", "dein", "Herz", ",", "dein", "gan\u00b7zes", "We\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Dein aufger\u00e4umter Kopf, und deine Trefflichkeit,", "tokens": ["Dein", "auf\u00b7ge\u00b7r\u00e4um\u00b7ter", "Kopf", ",", "und", "dei\u00b7ne", "Treff\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Dies alles soll nunmehr verschwinden und verwesen.", "tokens": ["Dies", "al\u00b7les", "soll", "nun\u00b7mehr", "ver\u00b7schwin\u00b7den", "und", "ver\u00b7we\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "VMFIN", "ADV", "VVPP", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Mein Ungl\u00fcck ist zu gro\u00df, es mindert keine Zeit!", "tokens": ["Mein", "Un\u00b7gl\u00fcck", "ist", "zu", "gro\u00df", ",", "es", "min\u00b7dert", "kei\u00b7ne", "Zeit", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PTKA", "ADJD", "$,", "PPER", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Der Tugend Ebenbild warst du in allen Sitten", "tokens": ["Der", "Tu\u00b7gend", "E\u00b7ben\u00b7bild", "warst", "du", "in", "al\u00b7len", "Sit\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VAFIN", "PPER", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Der Unschuld Meisterst\u00fcck, ein Wunder der Natur", "tokens": ["Der", "Un\u00b7schuld", "Meis\u00b7ter\u00b7st\u00fcck", ",", "ein", "Wun\u00b7der", "der", "Na\u00b7tur"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "$,", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Du warst bey jedermann beliebt und wohl gelitten.", "tokens": ["Du", "warst", "bey", "je\u00b7der\u00b7mann", "be\u00b7liebt", "und", "wohl", "ge\u00b7lit\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PIS", "ADJD", "KON", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Ach! meine Freundinn war die sch\u00f6nste Creatur!", "tokens": ["Ach", "!", "mei\u00b7ne", "Freun\u00b7dinn", "war", "die", "sch\u00f6ns\u00b7te", "Crea\u00b7tur", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PPOSAT", "NN", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.37": {"text": "Die treue Freundlichkeit, die unverf\u00e4lschten Triebe", "tokens": ["Die", "treu\u00b7e", "Freund\u00b7lich\u00b7keit", ",", "die", "un\u00b7ver\u00b7f\u00e4lschten", "Trie\u00b7be"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.38": {"text": "Besiegten meine Brust, und raubten mir das Herz,", "tokens": ["Be\u00b7sieg\u00b7ten", "mei\u00b7ne", "Brust", ",", "und", "raub\u00b7ten", "mir", "das", "Herz", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "KON", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Da\u00df ich dich noch im Tod mehr als mich selber liebe,", "tokens": ["Da\u00df", "ich", "dich", "noch", "im", "Tod", "mehr", "als", "mich", "sel\u00b7ber", "lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "APPRART", "NN", "ADV", "KOUS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Doch nicht zu meiner Lust, zu meinem gr\u00f6sten Schmerz.", "tokens": ["Doch", "nicht", "zu", "mei\u00b7ner", "Lust", ",", "zu", "mei\u00b7nem", "gr\u00f6s\u00b7ten", "Schmerz", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "APPR", "PPOSAT", "NN", "$,", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Ich n\u00e4hre selbigen bis ich dabey vergehe,", "tokens": ["Ich", "n\u00e4h\u00b7re", "sel\u00b7bi\u00b7gen", "bis", "ich", "da\u00b7bey", "ver\u00b7ge\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "KOUS", "PPER", "PAV", "VVFIN", "$,"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.42": {"text": "Ich sehne mich nach nichts als deiner Todtengruft,", "tokens": ["Ich", "seh\u00b7ne", "mich", "nach", "nichts", "als", "dei\u00b7ner", "Tod\u00b7ten\u00b7gruft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "PIS", "KOKOM", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Du theiltest Freud und Lust, du theiltest Wohl und Wehe;", "tokens": ["Du", "theil\u00b7test", "Freud", "und", "Lust", ",", "du", "theil\u00b7test", "Wohl", "und", "We\u00b7he", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "$,", "PPER", "VVFIN", "ADV", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Was Wunder wenn mein Geist dich noch zur\u00fccke ruft?", "tokens": ["Was", "Wun\u00b7der", "wenn", "mein", "Geist", "dich", "noch", "zu\u00b7r\u00fc\u00b7cke", "ruft", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "KOUS", "PPOSAT", "NN", "PPER", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "O k\u00f6nnte meine Hand dir doch ein Denkmal bauen!", "tokens": ["O", "k\u00f6nn\u00b7te", "mei\u00b7ne", "Hand", "dir", "doch", "ein", "Denk\u00b7mal", "bau\u00b7en", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PPOSAT", "NN", "PPER", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "Doch alles ist umsonst was ich bereits gethan.", "tokens": ["Doch", "al\u00b7les", "ist", "um\u00b7sonst", "was", "ich", "be\u00b7reits", "ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "ADV", "PWS", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "So ruhe sanft und wohl, du Muster kluger Frauen;", "tokens": ["So", "ru\u00b7he", "sanft", "und", "wohl", ",", "du", "Mus\u00b7ter", "klu\u00b7ger", "Frau\u00b7en", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "ADJD", "KON", "ADV", "$,", "PPER", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.48": {"text": "Ich kenne deinen Werth den nichts ersetzen kann.", "tokens": ["Ich", "ken\u00b7ne", "dei\u00b7nen", "Werth", "den", "nichts", "er\u00b7set\u00b7zen", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "ART", "PIS", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.49": {"text": "Wie gerne wollt ich mich an deiner Seite schmiegen,", "tokens": ["Wie", "ger\u00b7ne", "wollt", "ich", "mich", "an", "dei\u00b7ner", "Sei\u00b7te", "schmie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VMFIN", "PPER", "PRF", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.50": {"text": "Allein des Himmels Schlu\u00df verg\u00f6nnt mir nicht die Lust.", "tokens": ["Al\u00b7lein", "des", "Him\u00b7mels", "Schlu\u00df", "ver\u00b7g\u00f6nnt", "mir", "nicht", "die", "Lust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "NN", "VVFIN", "PPER", "PTKNEG", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.51": {"text": "Und kann ich nicht mit dir in einem Sarge liegen,", "tokens": ["Und", "kann", "ich", "nicht", "mit", "dir", "in", "ei\u00b7nem", "Sar\u00b7ge", "lie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PTKNEG", "APPR", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.52": {"text": "So stirbt dein Name doch niemals in meiner Brust.", "tokens": ["So", "stirbt", "dein", "Na\u00b7me", "doch", "nie\u00b7mals", "in", "mei\u00b7ner", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "ADV", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Ach Freundinn! du bist todt! der Schmerz hemmt mir die Zunge,", "tokens": ["Ach", "Freun\u00b7dinn", "!", "du", "bist", "todt", "!", "der", "Schmerz", "hemmt", "mir", "die", "Zun\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "$.", "PPER", "VAFIN", "ADJD", "$.", "ART", "NN", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-++--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "O Ungl\u00fcck! das mir schon mein Allerliebstes raubt.", "tokens": ["O", "Un\u00b7gl\u00fcck", "!", "das", "mir", "schon", "mein", "Al\u00b7ler\u00b7liebs\u00b7tes", "raubt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "PRELS", "PPER", "ADV", "PPOSAT", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So bald die Trauerpost in meine Ohren drunge", "tokens": ["So", "bald", "die", "Trau\u00b7er\u00b7post", "in", "mei\u00b7ne", "Oh\u00b7ren", "drun\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So sagte mirs mein Herz, sonst h\u00e4tt ich nicht geglaubt.", "tokens": ["So", "sag\u00b7te", "mirs", "mein", "Herz", ",", "sonst", "h\u00e4tt", "ich", "nicht", "ge\u00b7glaubt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "PPOSAT", "NN", "$,", "ADV", "VAFIN", "PPER", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Mein Kummer ist gerecht, gerecht sind meine Klagen;", "tokens": ["Mein", "Kum\u00b7mer", "ist", "ge\u00b7recht", ",", "ge\u00b7recht", "sind", "mei\u00b7ne", "Kla\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$,", "ADJD", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ich bin den Tauben gleich, ich suche Wald und Thal,", "tokens": ["Ich", "bin", "den", "Tau\u00b7ben", "gleich", ",", "ich", "su\u00b7che", "Wald", "und", "Thal", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADV", "$,", "PPER", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Dich meiner Augen Lust, mein alles auszufragen,", "tokens": ["Dich", "mei\u00b7ner", "Au\u00b7gen", "Lust", ",", "mein", "al\u00b7les", "aus\u00b7zu\u00b7fra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "NN", "$,", "PPOSAT", "PIS", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Doch alles ist umsonst; drum mehrt sich meine Qual.", "tokens": ["Doch", "al\u00b7les", "ist", "um\u00b7sonst", ";", "drum", "mehrt", "sich", "mei\u00b7ne", "Qual", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "ADV", "$.", "PAV", "VVFIN", "PRF", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Dein Geist begleitet mich, ich sehe dich im Schatten,", "tokens": ["Dein", "Geist", "be\u00b7glei\u00b7tet", "mich", ",", "ich", "se\u00b7he", "dich", "im", "Schat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "$,", "PPER", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Ich fahre oftmals auf, und denk ich fasse dich,", "tokens": ["Ich", "fah\u00b7re", "oft\u00b7mals", "auf", ",", "und", "denk", "ich", "fas\u00b7se", "dich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PTKVZ", "$,", "KON", "VVFIN", "PPER", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Allein so ists ein Schein mit dem ich mich wil gatten;", "tokens": ["Al\u00b7lein", "so", "ists", "ein", "Schein", "mit", "dem", "ich", "mich", "wil", "gat\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "ART", "NN", "APPR", "PRELS", "PPER", "PRF", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "So mehret sich die Pein, so fri\u00dft der Kummer mich!", "tokens": ["So", "meh\u00b7ret", "sich", "die", "Pein", ",", "so", "fri\u00dft", "der", "Kum\u00b7mer", "mich", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "$,", "ADV", "VVFIN", "ART", "NN", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "O deckten mich doch auch schon k\u00fchler Sand und Steine!", "tokens": ["O", "deck\u00b7ten", "mich", "doch", "auch", "schon", "k\u00fch\u00b7ler", "Sand", "und", "Stei\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADV", "ADV", "ADV", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Denn ich bin in der That nun meines Lebens satt.", "tokens": ["Denn", "ich", "bin", "in", "der", "That", "nun", "mei\u00b7nes", "Le\u00b7bens", "satt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "APPR", "ART", "NN", "ADV", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "So oft ich den Verlust und deinen Werth beweine,", "tokens": ["So", "oft", "ich", "den", "Ver\u00b7lust", "und", "dei\u00b7nen", "Werth", "be\u00b7wei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "ART", "NN", "KON", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "So oft werd ich auch schwach und von dem Seufzen matt.", "tokens": ["So", "oft", "werd", "ich", "auch", "schwach", "und", "von", "dem", "Seuf\u00b7zen", "matt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "ADV", "ADJD", "KON", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Kaum da\u00df ich mich erholt, k\u00fc\u00df ich die kalten Wangen;", "tokens": ["Kaum", "da\u00df", "ich", "mich", "er\u00b7holt", ",", "k\u00fc\u00df", "ich", "die", "kal\u00b7ten", "Wan\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "PRF", "VVPP", "$,", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Doch Schicksal, k\u00fcrze nur mein Leben durch den Tod:", "tokens": ["Doch", "Schick\u00b7sal", ",", "k\u00fcr\u00b7ze", "nur", "mein", "Le\u00b7ben", "durch", "den", "Tod", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "VVFIN", "ADV", "PPOSAT", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Der soll mir s\u00fcsser seyn als was ich kann erlangen;", "tokens": ["Der", "soll", "mir", "s\u00fcs\u00b7ser", "seyn", "als", "was", "ich", "kann", "er\u00b7lan\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "ADJD", "VAINF", "KOKOM", "PWS", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Denn jeder Blick vermehrt hinf\u00fchro meine Noth.", "tokens": ["Denn", "je\u00b7der", "Blick", "ver\u00b7mehrt", "hin\u00b7f\u00fch\u00b7ro", "mei\u00b7ne", "Noth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "ADJD", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Indessen schwer ich dir, ich bleibe stets die deine,", "tokens": ["In\u00b7des\u00b7sen", "schwer", "ich", "dir", ",", "ich", "blei\u00b7be", "stets", "die", "dei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "PPER", "$,", "PPER", "VVFIN", "ADV", "ART", "PPOSAT", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Man schlie\u00dft mein treues Herz mit in dein Grab hinein.", "tokens": ["Man", "schlie\u00dft", "mein", "treu\u00b7es", "Herz", "mit", "in", "dein", "Grab", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPOSAT", "ADJA", "NN", "APPR", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Ja du bleibst noch erbla\u00dft mein Liebstes und die meine", "tokens": ["Ja", "du", "bleibst", "noch", "er\u00b7bla\u00dft", "mein", "Liebs\u00b7tes", "und", "die", "mei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "PPER", "VVFIN", "ADV", "VVFIN", "PPOSAT", "NN", "KON", "ART", "PPOSAT"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Ich w\u00fcnsche sonsten nichts, als nur um dich zu seyn,", "tokens": ["Ich", "w\u00fcn\u00b7sche", "sons\u00b7ten", "nichts", ",", "als", "nur", "um", "dich", "zu", "seyn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PIS", "$,", "KOUS", "ADV", "APPR", "PPER", "PTKZU", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Doch der vergebne Wunsch verdoppelt mein Empfinden;", "tokens": ["Doch", "der", "ver\u00b7geb\u00b7ne", "Wunsch", "ver\u00b7dop\u00b7pelt", "mein", "Emp\u00b7fin\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Mein Leiden wird vermehret, die Marter wird stets neu;", "tokens": ["Mein", "Lei\u00b7den", "wird", "ver\u00b7meh\u00b7ret", ",", "die", "Mar\u00b7ter", "wird", "stets", "neu", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVFIN", "$,", "ART", "NN", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.27": {"text": "Der Schmerz nimmt immer zu, und bey dem H\u00e4nde winden", "tokens": ["Der", "Schmerz", "nimmt", "im\u00b7mer", "zu", ",", "und", "bey", "dem", "H\u00e4n\u00b7de", "win\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "PTKVZ", "$,", "KON", "APPR", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "L\u00e4\u00dft er die bange Brust nicht eine Stunde frey.", "tokens": ["L\u00e4\u00dft", "er", "die", "ban\u00b7ge", "Brust", "nicht", "ei\u00b7ne", "Stun\u00b7de", "frey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "PTKNEG", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Dein Ansehn, dein Verstand, dein Herz, dein ganzes Wesen", "tokens": ["Dein", "An\u00b7sehn", ",", "dein", "Ver\u00b7stand", ",", "dein", "Herz", ",", "dein", "gan\u00b7zes", "We\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Dein aufger\u00e4umter Kopf, und deine Trefflichkeit,", "tokens": ["Dein", "auf\u00b7ge\u00b7r\u00e4um\u00b7ter", "Kopf", ",", "und", "dei\u00b7ne", "Treff\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Dies alles soll nunmehr verschwinden und verwesen.", "tokens": ["Dies", "al\u00b7les", "soll", "nun\u00b7mehr", "ver\u00b7schwin\u00b7den", "und", "ver\u00b7we\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "VMFIN", "ADV", "VVPP", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Mein Ungl\u00fcck ist zu gro\u00df, es mindert keine Zeit!", "tokens": ["Mein", "Un\u00b7gl\u00fcck", "ist", "zu", "gro\u00df", ",", "es", "min\u00b7dert", "kei\u00b7ne", "Zeit", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PTKA", "ADJD", "$,", "PPER", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Der Tugend Ebenbild warst du in allen Sitten", "tokens": ["Der", "Tu\u00b7gend", "E\u00b7ben\u00b7bild", "warst", "du", "in", "al\u00b7len", "Sit\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VAFIN", "PPER", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Der Unschuld Meisterst\u00fcck, ein Wunder der Natur", "tokens": ["Der", "Un\u00b7schuld", "Meis\u00b7ter\u00b7st\u00fcck", ",", "ein", "Wun\u00b7der", "der", "Na\u00b7tur"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "$,", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Du warst bey jedermann beliebt und wohl gelitten.", "tokens": ["Du", "warst", "bey", "je\u00b7der\u00b7mann", "be\u00b7liebt", "und", "wohl", "ge\u00b7lit\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PIS", "ADJD", "KON", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Ach! meine Freundinn war die sch\u00f6nste Creatur!", "tokens": ["Ach", "!", "mei\u00b7ne", "Freun\u00b7dinn", "war", "die", "sch\u00f6ns\u00b7te", "Crea\u00b7tur", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PPOSAT", "NN", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.37": {"text": "Die treue Freundlichkeit, die unverf\u00e4lschten Triebe", "tokens": ["Die", "treu\u00b7e", "Freund\u00b7lich\u00b7keit", ",", "die", "un\u00b7ver\u00b7f\u00e4lschten", "Trie\u00b7be"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.38": {"text": "Besiegten meine Brust, und raubten mir das Herz,", "tokens": ["Be\u00b7sieg\u00b7ten", "mei\u00b7ne", "Brust", ",", "und", "raub\u00b7ten", "mir", "das", "Herz", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "KON", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Da\u00df ich dich noch im Tod mehr als mich selber liebe,", "tokens": ["Da\u00df", "ich", "dich", "noch", "im", "Tod", "mehr", "als", "mich", "sel\u00b7ber", "lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "APPRART", "NN", "ADV", "KOUS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Doch nicht zu meiner Lust, zu meinem gr\u00f6sten Schmerz.", "tokens": ["Doch", "nicht", "zu", "mei\u00b7ner", "Lust", ",", "zu", "mei\u00b7nem", "gr\u00f6s\u00b7ten", "Schmerz", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "APPR", "PPOSAT", "NN", "$,", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Ich n\u00e4hre selbigen bis ich dabey vergehe,", "tokens": ["Ich", "n\u00e4h\u00b7re", "sel\u00b7bi\u00b7gen", "bis", "ich", "da\u00b7bey", "ver\u00b7ge\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "KOUS", "PPER", "PAV", "VVFIN", "$,"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.42": {"text": "Ich sehne mich nach 