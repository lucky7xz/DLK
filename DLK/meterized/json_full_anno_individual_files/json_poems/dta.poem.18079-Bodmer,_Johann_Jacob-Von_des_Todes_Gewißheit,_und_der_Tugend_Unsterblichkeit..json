{"dta.poem.18079": {"metadata": {"author": {"name": "Bodmer, Johann Jacob", "birth": "N.A.", "death": "N.A."}, "title": "Von des Todes Gewi\u00dfheit, und der  \n Tugend Unsterblichkeit.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-200905198432", "language": ["de:0.71", "nl:0.28"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Man findet nichts vollkommen in der Welt,", "tokens": ["Man", "fin\u00b7det", "nichts", "voll\u00b7kom\u00b7men", "in", "der", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PIS", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wir Menschen sind mit Sorgen, Pein und Plagen", "tokens": ["Wir", "Men\u00b7schen", "sind", "mit", "Sor\u00b7gen", ",", "Pein", "und", "Pla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "NN", "VAFIN", "APPR", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "All Ort und Zeit, in St\u00e4dten, auf dem Feld,", "tokens": ["All", "Ort", "und", "Zeit", ",", "in", "St\u00e4d\u00b7ten", ",", "auf", "dem", "Feld", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "NN", "$,", "APPR", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Vom Himmel, Lufft, Meer, und uns selbst geschlagen:", "tokens": ["Vom", "Him\u00b7mel", ",", "Lufft", ",", "Meer", ",", "und", "uns", "selbst", "ge\u00b7schla\u00b7gen", ":"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "NN", "$,", "NN", "$,", "KON", "PPER", "ADV", "VVPP", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}}, "stanza.2": {"line.1": {"text": "Ja auch der G\u00f6tter Macht", "tokens": ["Ja", "auch", "der", "G\u00f6t\u00b7ter", "Macht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKANT", "ADV", "ART", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Hat ihr Wohnung vollkommen", "tokens": ["Hat", "ihr", "Woh\u00b7nung", "voll\u00b7kom\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "PPOSAT", "NN", "ADJD"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Und selig nicht gemacht.", "tokens": ["Und", "se\u00b7lig", "nicht", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Wer hat nicht wahrgenommen,", "tokens": ["Wer", "hat", "nicht", "wahr\u00b7ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wie Sonn und Mond gemein", "tokens": ["Wie", "Sonn", "und", "Mond", "ge\u00b7mein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "KON", "NN", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Verfinstern ihren Schein?", "tokens": ["Ver\u00b7fins\u00b7tern", "ih\u00b7ren", "Schein", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und wie des Himmels Zeichen", "tokens": ["Und", "wie", "des", "Him\u00b7mels", "Zei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Offt mangelhafft verbleichen?", "tokens": ["Offt", "man\u00b7gel\u00b7hafft", "ver\u00b7blei\u00b7chen", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Mit wie viel Angst, Gefahren, M\u00fch und Noth", "tokens": ["Mit", "wie", "viel", "Angst", ",", "Ge\u00b7fah\u00b7ren", ",", "M\u00fch", "und", "Noth"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["APPR", "KOKOM", "PIAT", "NN", "$,", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sind ohn Abla\u00df wir Menschen umgegeben?", "tokens": ["Sind", "ohn", "Ab\u00b7la\u00df", "wir", "Men\u00b7schen", "um\u00b7ge\u00b7ge\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "NN", "PPER", "NN", "VVPP", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Diese mit List man \u00fcbergibt dem Tod,", "tokens": ["Die\u00b7se", "mit", "List", "man", "\u00fc\u00b7berg\u00b7ibt", "dem", "Tod", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "NE", "PIS", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Jener hertzhafft verkrieget selbst sein Leben.", "tokens": ["Je\u00b7ner", "hertz\u00b7hafft", "ver\u00b7krie\u00b7get", "selbst", "sein", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VVFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Dieser aus viel Verdru\u00df", "tokens": ["Die\u00b7ser", "aus", "viel", "Ver\u00b7dru\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "APPR", "PIAT", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "Und Trauren will verderben,", "tokens": ["Und", "Trau\u00b7ren", "will", "ver\u00b7der\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VMFIN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Jener erb\u00e4rmlich mu\u00df", "tokens": ["Je\u00b7ner", "er\u00b7b\u00e4rm\u00b7lich", "mu\u00df"], "token_info": ["word", "word", "word"], "pos": ["PDAT", "ADJD", "VMFIN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "In der Gef\u00e4ngni\u00df sterben.", "tokens": ["In", "der", "Ge\u00b7f\u00e4ng\u00b7ni\u00df", "ster\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Etlich d\u00fcrstig nach Gut", "tokens": ["Et\u00b7lich", "d\u00fcrs\u00b7tig", "nach", "Gut"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADJD", "APPR", "NN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "Fliehen vor der Armuth,", "tokens": ["Flie\u00b7hen", "vor", "der", "Ar\u00b7muth", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Und ihren Geitz versincken,", "tokens": ["Und", "ih\u00b7ren", "Geitz", "ver\u00b7sin\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Wann sie im Meer ertrincken.", "tokens": ["Wann", "sie", "im", "Meer", "er\u00b7trin\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Diese mit Wasser, Gifft, Schwerdt oder Strick", "tokens": ["Die\u00b7se", "mit", "Was\u00b7ser", ",", "Gifft", ",", "Schwerdt", "o\u00b7der", "Strick"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PDS", "APPR", "NN", "$,", "NN", "$,", "NE", "KON", "NN"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.2": {"text": "Selbst \u00fcber sich ein schrecklich Urtheil sprechen,", "tokens": ["Selbst", "\u00fc\u00b7ber", "sich", "ein", "schreck\u00b7lich", "Ur\u00b7theil", "spre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PRF", "ART", "ADJD", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und rettend sich von zu schwerem Ungl\u00fcck", "tokens": ["Und", "ret\u00b7tend", "sich", "von", "zu", "schwe\u00b7rem", "Un\u00b7gl\u00fcck"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "PRF", "APPR", "APPR", "ADJA", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Zweiffeln sie nicht sich wider sich zu r\u00e4chen.", "tokens": ["Zweif\u00b7feln", "sie", "nicht", "sich", "wi\u00b7der", "sich", "zu", "r\u00e4\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PTKNEG", "PRF", "APPR", "PRF", "PTKZU", "VVINF", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}, "stanza.6": {"line.1": {"text": "Jene kommen mit Zwang", "tokens": ["Je\u00b7ne", "kom\u00b7men", "mit", "Zwang"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "APPR", "NN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "In dieses Lebens Leiden,", "tokens": ["In", "die\u00b7ses", "Le\u00b7bens", "Lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Finden gleich den Ausgang,", "tokens": ["Fin\u00b7den", "gleich", "den", "Aus\u00b7gang", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Und andre M\u00fch vermeiden,", "tokens": ["Und", "and\u00b7re", "M\u00fch", "ver\u00b7mei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Oder sich in ihr Grab,", "tokens": ["O\u00b7der", "sich", "in", "ihr", "Grab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Eh sie einige Gab", "tokens": ["Eh", "sie", "ei\u00b7ni\u00b7ge", "Gab"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PIAT", "NN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Des Tags selig geniessen,", "tokens": ["Des", "Tags", "se\u00b7lig", "ge\u00b7nies\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "In Mutter-Leib beschliessen.", "tokens": ["In", "Mut\u00b7ter\u00b7Leib", "be\u00b7schlies\u00b7sen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Der Tod gewi\u00df klopffet mit einem Bein", "tokens": ["Der", "Tod", "ge\u00b7wi\u00df", "klopf\u00b7fet", "mit", "ei\u00b7nem", "Bein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "An grosser Herrn Wolcken-tragende Schl\u00f6sser,", "tokens": ["An", "gros\u00b7ser", "Herrn", "Wol\u00b7cken\u00b7tra\u00b7gen\u00b7de", "Schl\u00f6s\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und armer Leut liegende H\u00fcttelein,", "tokens": ["Und", "ar\u00b7mer", "Leut", "lie\u00b7gen\u00b7de", "H\u00fct\u00b7tel\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+---+-+", "measure": "zehnsilber"}, "line.4": {"text": "Und ist f\u00fcr beyd weder b\u00f6ser noch besser.", "tokens": ["Und", "ist", "f\u00fcr", "beyd", "we\u00b7der", "b\u00f6\u00b7ser", "noch", "bes\u00b7ser", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "PIS", "KON", "ADJD", "ADV", "ADJD", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}}, "stanza.8": {"line.1": {"text": "Den Leib ein Tod allein", "tokens": ["Den", "Leib", "ein", "Tod", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Mit unheilbaren Plagen,", "tokens": ["Mit", "un\u00b7heil\u00b7ba\u00b7ren", "Pla\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Unentfliehlicher Pein,", "tokens": ["Un\u00b7ent\u00b7flieh\u00b7li\u00b7cher", "Pein", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Und undienstlichen Klagen,", "tokens": ["Und", "un\u00b7di\u00b7enst\u00b7li\u00b7chen", "Kla\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Aengstiget Tag und Nacht,", "tokens": ["A\u00b7engs\u00b7ti\u00b7get", "Tag", "und", "Nacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Und die Seel wird gebracht", "tokens": ["Und", "die", "Seel", "wird", "ge\u00b7bracht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VAFIN", "VVPP"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.7": {"text": "Vor Minos, der kein Flehen", "tokens": ["Vor", "Mi\u00b7nos", ",", "der", "kein", "Fle\u00b7hen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "PRELS", "PIAT", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.8": {"text": "Mehr pfleget anzusehen.", "tokens": ["Mehr", "pfle\u00b7get", "an\u00b7zu\u00b7se\u00b7hen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "VVIZU", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Der Weg ist breit in das finstere Hau\u00df,", "tokens": ["Der", "Weg", "ist", "breit", "in", "das", "fins\u00b7te\u00b7re", "Hau\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Offen die Th\u00fcr, da\u00df man hinein stets gehet,", "tokens": ["Of\u00b7fen", "die", "Th\u00fcr", ",", "da\u00df", "man", "hin\u00b7ein", "stets", "ge\u00b7het", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "KOUS", "PIS", "ADV", "ADV", "VVFIN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Aber wiedrum zu entrinnen daraus,", "tokens": ["A\u00b7ber", "wie\u00b7drum", "zu", "ent\u00b7rin\u00b7nen", "da\u00b7raus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PTKZU", "VVFIN", "PAV", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.4": {"text": "Hierauf das Werck, hierauf die M\u00fch bestehet.", "tokens": ["Hier\u00b7auf", "das", "Werck", ",", "hier\u00b7auf", "die", "M\u00fch", "be\u00b7ste\u00b7het", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "$,", "PAV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Der Tugend Weg ist schmahl,", "tokens": ["Der", "Tu\u00b7gend", "Weg", "ist", "schmahl", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Mit Dornen wohl verschlossen,", "tokens": ["Mit", "Dor\u00b7nen", "wohl", "ver\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Gering ist die Anzahl,", "tokens": ["Ge\u00b7ring", "ist", "die", "An\u00b7zahl", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Deren die unverdrossen", "tokens": ["De\u00b7ren", "die", "un\u00b7ver\u00b7dros\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["PDS", "ART", "ADJA"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.5": {"text": "Und durch der G\u00f6tter Gunst,", "tokens": ["Und", "durch", "der", "G\u00f6t\u00b7ter", "Gunst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Und der Tugend Einbrunst,", "tokens": ["Und", "der", "Tu\u00b7gend", "Ein\u00b7brunst", ","], "token_info": ["word", "word",