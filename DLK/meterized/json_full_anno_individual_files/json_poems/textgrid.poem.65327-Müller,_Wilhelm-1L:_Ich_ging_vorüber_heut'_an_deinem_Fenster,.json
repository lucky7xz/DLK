{"textgrid.poem.65327": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich ging vor\u00fcber heut' an deinem Fenster,", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich ging vor\u00fcber heut' an deinem Fenster,", "tokens": ["Ich", "ging", "vor\u00b7\u00fc\u00b7ber", "heut'", "an", "dei\u00b7nem", "Fens\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und zankte mit dem dichten gr\u00fcnen Ginster,", "tokens": ["Und", "zank\u00b7te", "mit", "dem", "dich\u00b7ten", "gr\u00fc\u00b7nen", "Gins\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der dich vor meinen Blicken ganz versteckte.", "tokens": ["Der", "dich", "vor", "mei\u00b7nen", "Bli\u00b7cken", "ganz", "ver\u00b7steck\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPR", "PPOSAT", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Da sah ich, wie aus dem Gestr\u00e4uch geschwinde", "tokens": ["Da", "sah", "ich", ",", "wie", "aus", "dem", "Ge\u00b7str\u00e4uch", "ge\u00b7schwin\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PWAV", "APPR", "ART", "NN", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Heraus sich streckten deine wei\u00dfen H\u00e4nde,", "tokens": ["He\u00b7raus", "sich", "streck\u00b7t