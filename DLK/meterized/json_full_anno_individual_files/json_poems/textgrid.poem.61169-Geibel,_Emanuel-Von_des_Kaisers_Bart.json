{"textgrid.poem.61169": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Von des Kaisers Bart", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im Schank zur goldnen Traube,", "tokens": ["Im", "Schank", "zur", "gold\u00b7nen", "Trau\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da sa\u00dfen im Monat Mai", "tokens": ["Da", "sa\u00b7\u00dfen", "im", "Mo\u00b7nat", "Mai"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "In bl\u00fchender Rosenlaube", "tokens": ["In", "bl\u00fc\u00b7hen\u00b7der", "Ro\u00b7sen\u00b7lau\u00b7be"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Guter Gesellen drei.", "tokens": ["Gu\u00b7ter", "Ge\u00b7sel\u00b7len", "drei", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "CARD", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.2": {"line.1": {"text": "Ein frischer Bursch war jeder,", "tokens": ["Ein", "fri\u00b7scher", "Bursch", "war", "je\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PIS", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der erst' am Gurt das Horn,", "tokens": ["Der", "er\u00b7st'", "am", "Gurt", "das", "Horn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Der zweit' am Hut die Feder,", "tokens": ["Der", "zweit'", "am", "Hut", "die", "Fe\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der dritte mit Koller und Sporn.", "tokens": ["Der", "drit\u00b7te", "mit", "Kol\u00b7ler", "und", "Sporn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "APPR", "NN", "KON", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.3": {"line.1": {"text": "Es trug in funkelnden Kannen", "tokens": ["Es", "trug", "in", "fun\u00b7keln\u00b7den", "Kan\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Der Wirt den Wein auf den Tisch;", "tokens": ["Der", "Wirt", "den", "Wein", "auf", "den", "Tisch", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Lustige Reden sie spannen", "tokens": ["Lus\u00b7ti\u00b7ge", "Re\u00b7den", "sie", "span\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "PPER", "VVINF"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Und sangen und tranken frisch.", "tokens": ["Und", "san\u00b7gen", "und", "tran\u00b7ken", "frisch", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "ADJD", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Da war auch einer drunter,", "tokens": ["Da", "war", "auch", "ei\u00b7ner", "drun\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "PIS", "PAV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der gr\u00fcne J\u00e4gersmann,", "tokens": ["Der", "gr\u00fc\u00b7ne", "J\u00e4\u00b7gers\u00b7mann", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Vom Kaiser Rotbart munter", "tokens": ["Vom", "Kai\u00b7ser", "Rot\u00b7bart", "mun\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "NN", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zu sprechen hub er an:", "tokens": ["Zu", "spre\u00b7chen", "hub", "er", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "\u00bbich habe den Herrn gesehen", "tokens": ["\u00bb", "ich", "ha\u00b7be", "den", "Herrn", "ge\u00b7se\u00b7hen"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "PPER", "VAFIN", "ART", "NN", "VVPP"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Am Rebengestade des Rheins,", "tokens": ["Am", "Re\u00b7ben\u00b7ge\u00b7sta\u00b7de", "des", "Rheins", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NE", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Zur Messe wollt' er gehen", "tokens": ["Zur", "Mes\u00b7se", "wollt'", "er", "ge\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wohl in den Dom nach Mainz.", "tokens": ["Wohl", "in", "den", "Dom", "nach", "Mainz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "APPR", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Das war ein Bild, der Alte,", "tokens": ["Das", "war", "ein", "Bild", ",", "der", "Al\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "F\u00fcrwahr von Kaiserart!", "tokens": ["F\u00fcr\u00b7wahr", "von", "Kai\u00b7ser\u00b7art", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Bis auf die Brust ihm wallte", "tokens": ["Bis", "auf", "die", "Brust", "ihm", "wall\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "APPR", "ART", "NN", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der lange braune Bart.\u00ab", "tokens": ["Der", "lan\u00b7ge", "brau\u00b7ne", "Bart", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Ins Wort fiel ihm der zweite,", "tokens": ["Ins", "Wort", "fiel", "ihm", "der", "zwei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ART", "ADJA", "$,"], "meter": "-+---+-", "measure": "dactylic.init"}, "line.2": {"text": "Der mit dem Federhut:", "tokens": ["Der", "mit", "dem", "Fe\u00b7der\u00b7hut", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "\u00bbei Bursch, bist du gescheite?", "tokens": ["\u00bb", "ei", "Bursch", ",", "bist", "du", "ge\u00b7schei\u00b7te", "?"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "NE", "NE", "$,", "VAFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dein M\u00e4rlein ist nicht gut.", "tokens": ["Dein", "M\u00e4r\u00b7lein", "ist", "nicht", "gut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Auch ich hab' ihn gesehen", "tokens": ["Auch", "ich", "hab'", "ihn", "ge\u00b7se\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "VAFIN", "PPER", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Auf seiner Burg im Harz,", "tokens": ["Auf", "sei\u00b7ner", "Burg", "im", "Harz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Am S\u00f6ller t\u00e4t er stehen,", "tokens": ["Am", "S\u00f6l\u00b7ler", "t\u00e4t", "er", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sein Bart, sein Bart war schwarz.\u00ab", "tokens": ["Sein", "Bart", ",", "sein", "Bart", "war", "schwarz", ".", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "VAFIN", "ADJD", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Da fuhr vom Sitz der dritte,", "tokens": ["Da", "fuhr", "vom", "Sitz", "der", "drit\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "ART", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Mann mit Koller und Sporn,", "tokens": ["Der", "Mann", "mit", "Kol\u00b7ler", "und", "Sporn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Und in der Z\u00e4nker Mitte", "tokens": ["Und", "in", "der", "Z\u00e4n\u00b7ker", "Mit\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Rief er in hellem Zorn:", "tokens": ["Rief", "er", "in", "hel\u00b7lem", "Zorn", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "\u00bbso geht mir doch zur H\u00f6llen,", "tokens": ["\u00bb", "so", "geht", "mir", "doch", "zur", "H\u00f6l\u00b7len", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ihr L\u00fcgner! Gl\u00fcck zur Reis'! -", "tokens": ["Ihr", "L\u00fcg\u00b7ner", "!", "Gl\u00fcck", "zur", "Reis'", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["PPOSAT", "NN", "$.", "NN", "APPRART", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich sah den Kaiser zu K\u00f6llen,", "tokens": ["Ich", "sah", "den", "Kai\u00b7ser", "zu", "K\u00f6l\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Sein Bart war wei\u00df, war wei\u00df.\u00ab", "tokens": ["Sein", "Bart", "war", "wei\u00df", ",", "war", "wei\u00df", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVFIN", "$,", "VAFIN", "VVFIN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Das gab ein grimmes Zanken", "tokens": ["Das", "gab", "ein", "grim\u00b7mes", "Zan\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Um Wei\u00df und Schwarz und Braun,", "tokens": ["Um", "Wei\u00df", "und", "Schwarz", "und", "Braun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "KON", "NE", "KON", "NE", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es sprangen die Klingen, die blanken,", "tokens": ["Es", "spran\u00b7gen", "die", "Klin\u00b7gen", ",", "die", "blan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PRELS", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Und wurde scharf gehaun.", "tokens": ["Und", "wur\u00b7de", "scharf", "ge\u00b7haun", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADJD", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Versch\u00fcttet aus den Kannen", "tokens": ["Ver\u00b7sch\u00fct\u00b7tet", "aus", "den", "Kan\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Flo\u00df der vieledle Wein,", "tokens": ["Flo\u00df", "der", "vie\u00b7led\u00b7le", "Wein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,"], "meter": "+----+", "measure": "dactylic.init"}, "line.3": {"text": "Blutige Tropfen rannen", "tokens": ["Blu\u00b7ti\u00b7ge", "Trop\u00b7fen", "ran\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Aus leichten Wunden drein.", "tokens": ["Aus", "leich\u00b7ten", "Wun\u00b7den", "drein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Und als es kam zum Wandern,", "tokens": ["Und", "als", "es", "kam", "zum", "Wan\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ging jeder in zornigem Mut,", "tokens": ["Ging", "je\u00b7der", "in", "zor\u00b7ni\u00b7gem", "Mut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Sah keiner nach dem andern,", "tokens": ["Sah", "kei\u00b7ner", "nach", "dem", "an\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPR", "ART", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und waren sich j\u00fcngst so gut.", "tokens": ["Und", "wa\u00b7ren", "sich", "j\u00fcngst", "so", "gut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PRF", "ADV", "ADV", "ADJD", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.14": {"line.1": {"text": "Ihr Br\u00fcder, lernt das eine", "tokens": ["Ihr", "Br\u00fc\u00b7der", ",", "lernt", "das", "ei\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "ART", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Aus dieser schlimmen Fahrt:", "tokens": ["Aus", "die\u00b7ser", "schlim\u00b7men", "Fahrt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Zankt, wenn ihr sitzt beim Weine,", "tokens": ["Zankt", ",", "wenn", "ihr", "sitzt", "beim", "Wei\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nicht um des Kaisers Bart!", "tokens": ["Nicht", "um", "des", "Kai\u00b7sers", "Bart", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "Im Schank zur goldnen Traube,", "tokens": ["Im", "Schank", "zur", "gold\u00b7nen", "Trau\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da sa\u00dfen im Monat Mai", "tokens": ["Da", "sa\u00b7\u00dfen", "im", "Mo\u00b7nat", "Mai"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "In bl\u00fchender Rosenlaube", "tokens": ["In", "bl\u00fc\u00b7hen\u00b7der", "Ro\u00b7sen\u00b7lau\u00b7be"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Guter Gesellen drei.", "tokens": ["Gu\u00b7ter", "Ge\u00b7sel\u00b7len", "drei", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "CARD", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.16": {"line.1": {"text": "Ein frischer Bursch war jeder,", "tokens": ["Ein", "fri\u00b7scher", "Bursch", "war", "je\u00b7der", ","], "token_info": ["word", "wo