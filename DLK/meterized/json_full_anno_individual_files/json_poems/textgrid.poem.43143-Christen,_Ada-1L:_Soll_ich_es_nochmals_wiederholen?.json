{"textgrid.poem.43143": {"metadata": {"author": {"name": "Christen, Ada", "birth": "N.A.", "death": "N.A."}, "title": "1L: Soll ich es nochmals wiederholen?", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Soll ich es nochmals wiederholen?", "tokens": ["Soll", "ich", "es", "noch\u00b7mals", "wie\u00b7der\u00b7ho\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr habt mich ja so oft gefragt,", "tokens": ["Ihr", "habt", "mich", "ja", "so", "oft", "ge\u00b7fragt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und tausend Mal hab' ich auf Ehre", "tokens": ["Und", "tau\u00b7send", "Mal", "hab'", "ich", "auf", "Eh\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "CARD", "NN", "VAFIN", "PPER", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die volle Wahrheit Euch gesagt. \u2013", "tokens": ["Die", "vol\u00b7le", "Wahr\u00b7heit", "Euch", "ge\u00b7sagt", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "VVPP", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ja, ich bewund're Eure Tugend,", "tokens": ["Ja", ",", "ich", "be\u00b7wun\u00b7d'\u00b7re", "Eu\u00b7re", "Tu\u00b7gend", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Und ich bewund're Eure Kin