{"dta.poem.7293": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Sommer.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-20090519994", "language": ["de:0.99"], "booktitle": "Droste-H\u00fclshoff, Annette von: Gedichte. Stuttgart u. a., 1844."}, "poem": {"stanza.1": {"line.1": {"text": "Du gute Linde, sch\u00fcttle dich!", "tokens": ["Du", "gu\u00b7te", "Lin\u00b7de", ",", "sch\u00fctt\u00b7le", "dich", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NE", "$,", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein wenig Luft, ein schwacher West!", "tokens": ["Ein", "we\u00b7nig", "Luft", ",", "ein", "schwa\u00b7cher", "West", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Wo nicht, dann schlie\u00dfe dein Gezweig", "tokens": ["Wo", "nicht", ",", "dann", "schlie\u00b7\u00dfe", "dein", "Ge\u00b7zweig"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "PTKNEG", "$,", "ADV", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So recht, da\u00df Blatt an Blatt sich pre\u00dft.", "tokens": ["So", "recht", ",", "da\u00df", "Blatt", "an", "Blatt", "sich", "pre\u00dft", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "KOUS", "NN", "APPR", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Kein Vogel zirpt, es bellt kein Hund;", "tokens": ["Kein", "Vo\u00b7gel", "zirpt", ",", "es", "bellt", "kein", "Hund", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "$,", "PPER", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Allein die bunte Fliegenbrut", "tokens": ["Al\u00b7lein", "die", "bun\u00b7te", "Flie\u00b7gen\u00b7brut"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Summt auf und nieder \u00fcber'n Rain", "tokens": ["Summt", "auf", "und", "nie\u00b7der", "\u00fc\u00b7ber'n", "Rain"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PTKVZ", "KON", "PTKVZ", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und l\u00e4\u00dft sich r\u00f6sten in der Glut.", "tokens": ["Und", "l\u00e4\u00dft", "sich", "r\u00f6s\u00b7ten", "in", "der", "Glut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Sogar der B\u00e4ume dunkles Laub", "tokens": ["So\u00b7gar", "der", "B\u00e4u\u00b7me", "dunk\u00b7les", "Laub"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Erscheint verdickt und athmet Staub.", "tokens": ["Er\u00b7scheint", "ver\u00b7dickt", "und", "ath\u00b7met", "Staub", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "VVPP", "KON", "VVFIN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich liege hier wie ausgedorrt", "tokens": ["Ich", "lie\u00b7ge", "hier", "wie", "aus\u00b7ge\u00b7dorrt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "KOKOM", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und scheuche kaum die M\u00fccken fort.", "tokens": ["Und", "scheu\u00b7che", "kaum", "die", "M\u00fc\u00b7cken", "fort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "O S\u00e4ntis, S\u00e4ntis! l\u00e4g' ich doch", "tokens": ["O", "S\u00e4n\u00b7tis", ",", "S\u00e4n\u00b7tis", "!", "l\u00e4g'", "ich", "doch"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NE", "NE", "$,", "NE", "$.", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dort, \u2014 grad' an deinem Felsenjoch,", "tokens": ["Dort", ",", "grad'", "an", "dei\u00b7nem", "Fel\u00b7sen\u00b7joch", ","], "token_info": ["word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "$(", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Wo sich die kalten, wei\u00dfen Decken", "tokens": ["Wo", "sich", "die", "kal\u00b7ten", ",", "wei\u00b7\u00dfen", "De\u00b7cken"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "PRF", "ART", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So frisch und saftig dr\u00fcben strecken,", "tokens": ["So", "frisch", "und", "saf\u00b7tig", "dr\u00fc\u00b7ben", "stre\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Viel tausend blanker Tropfen Spiel;", "tokens": ["Viel", "tau\u00b7send", "blan\u00b7ker", "Trop\u00b7fen", "Spiel", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Gl\u00fccksel'ger S\u00e4ntis, dir ist k\u00fchl!", "tokens": ["Gl\u00fcck\u00b7sel'\u00b7ger", "S\u00e4n\u00b7tis", ",", "dir", "ist", "k\u00fchl", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PPER", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}