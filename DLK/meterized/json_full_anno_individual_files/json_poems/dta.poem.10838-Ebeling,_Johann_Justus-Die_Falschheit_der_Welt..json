{"dta.poem.10838": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "Die  \n Falschheit der Welt.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198774", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Da\u00df in denen s\u00fcssen K\u00fcssen,", "tokens": ["Da\u00df", "in", "de\u00b7nen", "s\u00fcs\u00b7sen", "K\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PRELS", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Heimlich Gifft und Galle fliessen;", "tokens": ["Heim\u00b7lich", "Gifft", "und", "Gal\u00b7le", "flies\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "KON", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ist die Mode b\u00f6ser Welt.", "tokens": ["Ist", "die", "Mo\u00b7de", "b\u00f6\u00b7ser", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Man sucht nur durch freundlich stel-", "tokens": ["Man", "sucht", "nur", "durch", "freund\u00b7lich", "stel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "ADV", "APPR", "ADJD", "TRUNC"], "meter": "+---+-+", "measure": "dactylic.init"}, "line.5": {"text": "Andre f\u00e4lschlich zu beschnellen;", "tokens": ["And\u00b7re", "f\u00e4lschlich", "zu", "be\u00b7schnel\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "ADJD", "PTKZU", "VVINF", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.6": {"text": "Und wenn man sie hat gef\u00e4llt;", "tokens": ["Und", "wenn", "man", "sie", "hat", "ge\u00b7f\u00e4llt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PIS", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Durch ein solch verf\u00fcrisch Gleissen,", "tokens": ["Durch", "ein", "solch", "ver\u00b7f\u00fc\u00b7risch", "Gleis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PIAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Mu\u00df die That noch Klugheit heissen.", "tokens": ["Mu\u00df", "die", "That", "noch", "Klug\u00b7heit", "heis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "ADV", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "O! verdammte Brut der H\u00f6llen,", "tokens": ["O", "!", "ver\u00b7damm\u00b7te", "Brut", "der", "H\u00f6l\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kunst sich listig zu verstellen!", "tokens": ["Kunst", "sich", "lis\u00b7tig", "zu", "ver\u00b7stel\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "ADJD", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Nach dem du erfunden bist,", "tokens": ["Nach", "dem", "du", "er\u00b7fun\u00b7den", "bist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "VVPP", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ist die echte Treu verlohren,", "tokens": ["Ist", "die", "ech\u00b7te", "Treu", "ver\u00b7loh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und das Ungl\u00fckskind gebohren,", "tokens": ["Und", "das", "Un\u00b7gl\u00fcks\u00b7kind", "ge\u00b7boh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Der Betrug, die Argelist;", "tokens": ["Der", "Be\u00b7trug", ",", "die", "Ar\u00b7ge\u00b7list", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Nunmehr heist den N\u00e4chsten lieben,", "tokens": ["Nun\u00b7mehr", "heist", "den", "N\u00e4chs\u00b7ten", "lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Leider! nur ihn zu betr\u00fcben.", "tokens": ["Lei\u00b7der", "!", "nur", "ihn", "zu", "be\u00b7tr\u00fc\u00b7ben", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "ADV", "PPER", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Menschen! wollen Schaffe heissen,", "tokens": ["Men\u00b7schen", "!", "wol\u00b7len", "Schaf\u00b7fe", "heis\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VMFIN", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und als W\u00f6lffe doch zerreissen,", "tokens": ["Und", "als", "W\u00f6lf\u00b7fe", "doch", "zer\u00b7reis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "NN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Den der sich zu sehr vertraut;", "tokens": ["Den", "der", "sich", "zu", "sehr", "ver\u00b7traut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRELS", "PRF", "APPR", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "O! betr\u00fcgrisch falsche Wolle", "tokens": ["O", "!", "be\u00b7tr\u00fcg\u00b7risch", "fal\u00b7sche", "Wol\u00b7le"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$.", "ADJD", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Der Verdammten Heuchler-Rolle,", "tokens": ["Der", "Ver\u00b7damm\u00b7ten", "Heuch\u00b7ler\u00b7Rol\u00b7le", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Da man solche Klauen schaut,", "tokens": ["Da", "man", "sol\u00b7che", "Klau\u00b7en", "schaut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PIAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Die sich in dem Pelz verh\u00fcllen,", "tokens": ["Die", "sich", "in", "dem", "Pelz", "ver\u00b7h\u00fcl\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Damit sie den Rachen f\u00fcllen.", "tokens": ["Da\u00b7mit", "sie", "den", "Ra\u00b7chen", "f\u00fcl\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Die wie Crocodille weinen,", "tokens": ["Die", "wie", "Cro\u00b7co\u00b7dil\u00b7le", "wei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "KOKOM", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und wie die Sirenen scheinen,", "tokens": ["Und", "wie", "die", "Si\u00b7re\u00b7nen", "schei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Sind fast keine Menschen mehr;", "tokens": ["Sind", "fast", "kei\u00b7ne", "Men\u00b7schen", "mehr", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PIAT", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Denn die s\u00fcssen Augenblikke,", "tokens": ["Denn", "die", "s\u00fcs\u00b7sen", "Au\u00b7gen\u00b7blik\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Die voll innrer Wuth und T\u00fckke,", "tokens": ["Die", "voll", "inn\u00b7rer", "Wuth", "und", "T\u00fck\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "KON", "NN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.6": {"text": "Stammen von dem Teufel her;", "tokens": ["Stam\u00b7men", "von", "dem", "Teu\u00b7fel", "her", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Man giebt ihnen ihren Nahmen,", "tokens": ["Man", "giebt", "ih\u00b7nen", "ih\u00b7ren", "Nah\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "++--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.8": {"text": "Wenn sie heissen Schlangen-Saamen.", "tokens": ["Wenn", "sie", "heis\u00b7sen", "Schlan\u00b7gen\u00b7Saa\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}