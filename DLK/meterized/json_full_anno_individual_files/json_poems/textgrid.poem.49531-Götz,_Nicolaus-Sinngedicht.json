{"textgrid.poem.49531": {"metadata": {"author": {"name": "G\u00f6tz, Nicolaus", "birth": "N.A.", "death": "N.A."}, "title": "Sinngedicht", "genre": "verse", "period": "N.A.", "pub_year": 1751, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Damen scheinen hier den edlen Nachtviolen", "tokens": ["Die", "Da\u00b7men", "schei\u00b7nen", "hier", "den", "ed\u00b7len", "Nacht\u00b7vi\u00b7o\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vollkommen gleich zu seyn.", "tokens": ["Voll\u00b7kom\u00b7men", "gleich", "zu", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PTKZU", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dann Nachts verbreiten sie, am Mondschein, unverholen,", "tokens": ["Dann", "Nachts", "ver\u00b7brei\u00b7ten", "sie", ",", "am", "Mond\u00b7schein", ",", "un\u00b7ver\u00b7ho\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "$,", "APPRART", "NN", "$,", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "In junger Buhler Arm, der Sch\u00f6nheit vollen Schein;", "tokens": ["In", "jun\u00b7ger", "Buh\u00b7ler", "Arm", ",", "der", "Sch\u00f6n\u00b7heit", "vol\u00b7len", "Schein", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$,", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Des Morgens ziehen sie, verstohlen,", "tokens": ["Des", "Mor\u00b7gens", "zie\u00b7hen", "sie", ",", "ver\u00b7stoh\u00b7len", ","], "token_info":