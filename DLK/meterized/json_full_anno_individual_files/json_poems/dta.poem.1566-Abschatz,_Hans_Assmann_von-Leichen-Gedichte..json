{"dta.poem.1566": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Leichen-Gedichte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Hier liegt ein Rittersmann/ den Schlesien gebahr/", "tokens": ["Hier", "liegt", "ein", "Rit\u00b7ters\u00b7mann", "/", "den", "Schle\u00b7si\u00b7en", "ge\u00b7bahr", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$(", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den Sachsen aufferzog und mit zu Felde f\u00fchrte/", "tokens": ["Den", "Sach\u00b7sen", "auf\u00b7fer\u00b7zog", "und", "mit", "zu", "Fel\u00b7de", "f\u00fchr\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "APPR", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Den Tapfferkeit/ Verstand und Fr\u00f6mmigkeit bezierte/", "tokens": ["Den", "Tapf\u00b7fer\u00b7keit", "/", "Ver\u00b7stand", "und", "Fr\u00f6m\u00b7mig\u00b7keit", "be\u00b7zier\u00b7te", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "NN", "KON", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der sich am Rheinstrom gab zu kennen was er war/", "tokens": ["Der", "sich", "am", "Rhein\u00b7strom", "gab", "zu", "ken\u00b7nen", "was", "er", "war", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPRART", "NN", "VVFIN", "PTKZU", "VVINF", "PWS", "PPER", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der Wien befreyen halff von Mechmets frecher Schaar/", "tokens": ["Der", "Wi\u00b7en", "be\u00b7fre\u00b7yen", "halff", "von", "Mech\u00b7mets", "fre\u00b7cher", "Schaar", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VVFIN", "APPR", "NE", "ADJA", "NN", "$("], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Der/ was ein Deutscher kan/ zu See und Land probirte/", "tokens": ["Der", "/", "was", "ein", "Deut\u00b7scher", "kan", "/", "zu", "See", "und", "Land", "pro\u00b7bir\u00b7te", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$(", "PWS", "ART", "NN", "VMFIN", "$(", "APPR", "NN", "KON", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "De\u00df k\u00fchnen Helden-Mutt Moreens Krone sp\u00fcrte/", "tokens": ["De\u00df", "k\u00fch\u00b7nen", "Hel\u00b7den\u00b7Mutt", "Mo\u00b7re\u00b7ens", "Kro\u00b7ne", "sp\u00fcr\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NE", "NE", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Der Cefala besch\u00fczt in st\u00fcrmender Gefahr.", "tokens": ["Der", "Ce\u00b7fa\u00b7la", "be\u00b7sch\u00fczt", "in", "st\u00fcr\u00b7men\u00b7der", "Ge\u00b7fahr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}