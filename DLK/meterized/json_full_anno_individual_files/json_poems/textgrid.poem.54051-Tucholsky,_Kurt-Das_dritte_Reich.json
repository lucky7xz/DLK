{"textgrid.poem.54051": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Das dritte Reich", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es braucht ein hohes Ideal", "tokens": ["Es", "braucht", "ein", "ho\u00b7hes", "I\u00b7deal"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "der nationale Mann,", "tokens": ["der", "na\u00b7ti\u00b7o\u00b7na\u00b7le", "Mann", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "daran er morgens allemal", "tokens": ["da\u00b7ran", "er", "mor\u00b7gens", "al\u00b7le\u00b7mal"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "PPER", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "ein wenig turnen kann.", "tokens": ["ein", "we\u00b7nig", "tur\u00b7nen", "kann", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Da hat denn deutsche Manneskraft", "tokens": ["Da", "hat", "denn", "deut\u00b7sche", "Man\u00b7nes\u00b7kraft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "in segensreichen Stunden", "tokens": ["in", "se\u00b7gens\u00b7rei\u00b7chen", "Stun\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "als neueste Errungenschaft", "tokens": ["als", "neu\u00b7es\u00b7te", "Er\u00b7run\u00b7gen\u00b7schaft"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "ein Ideal erfunden:", "tokens": ["ein", "I\u00b7deal", "er\u00b7fun\u00b7den", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "Es soll nicht sein das erste Reich,", "tokens": ["Es", "soll", "nicht", "sein", "das", "ers\u00b7te", "Reich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "VAINF", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "es soll nicht sein das zweite Reich . . .", "tokens": ["es", "soll", "nicht", "sein", "das", "zwei\u00b7te", "Reich", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "VAINF", "ART", "ADJA", "NN", "$.", "$.", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Das dritte Reich?", "tokens": ["Das", "drit\u00b7te", "Reich", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Bitte sehr! Bitte gleich!", "tokens": ["Bit\u00b7te", "sehr", "!", "Bit\u00b7te", "gleich", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "$.", "NN", "ADV", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Wir d\u00fcrfen nicht mehr massisch sein \u2013", "tokens": ["Wir", "d\u00fcr\u00b7fen", "nicht", "mehr", "mas\u00b7sisch", "sein", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "ADV", "ADJD", "VAINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "wir m\u00fcssen durchaus rassisch sein \u2013", "tokens": ["wir", "m\u00fcs\u00b7sen", "durc\u00b7haus", "ras\u00b7sisch", "sein", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADJD", "VAINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "und freideutsch, jungdeutsch, heimatwolkig", "tokens": ["und", "frei\u00b7deutsch", ",", "jung\u00b7deutsch", ",", "hei\u00b7mat\u00b7wol\u00b7kig"], "token_info": ["word", "word", "punct", "word", "punct", "word"], "pos": ["KON", "ADJD", "$,", "ADJD", "$,", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "und b\u00fcndisch, v\u00f6lkisch, volkisch, volkig . . .", "tokens": ["und", "b\u00fcn\u00b7disch", ",", "v\u00f6l\u00b7ki\u00b7sch", ",", "vol\u00b7kisch", ",", "vol\u00b7kig", ".", ".", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "punct", "punct"], "pos": ["KON", "ADJD", "$,", "ADJD", "$,", "ADJD", "$,", "ADJD", "$.", "$.", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "und \u00fcberhaupt.", "tokens": ["und", "\u00fc\u00b7ber\u00b7haupt", "."], "token_info": ["word", "word", "punct"], "pos": ["KON", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Wers glaubt,", "tokens": ["Wers", "glaubt", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "VVFIN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.9": {"text": "wird selig. Wer es nicht glaubt, ist", "tokens": ["wird", "se\u00b7lig", ".", "Wer", "es", "nicht", "glaubt", ",", "ist"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["VAFIN", "ADJD", "$.", "PWS", "PPER", "PTKNEG", "VVFIN", "$,", "VAFIN"], "meter": "+--+-+++", "measure": "iambic.penta.invert"}, "line.10": {"text": "ein ganz verkommener Paz- und Bolschewist.", "tokens": ["ein", "ganz", "ver\u00b7kom\u00b7me\u00b7ner", "Pa\u00b7z", "und", "Bol\u00b7sche\u00b7wist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "TRUNC", "KON", "NN", "$."], "meter": "-+-+---+-+-+", "measure": "unknown.measure.penta"}}, "stanza.3": {"line.1": {"text": "Das dritte Reich?", "tokens": ["Das", "drit\u00b7te", "Reich", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Bitte sehr! Bitte gleich!", "tokens": ["Bit\u00b7te", "sehr", "!", "Bit\u00b7te", "gleich", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "$.", "NN", "ADV", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.4": {"line.1": {"text": "Im dritten Reich ist alles eitel Gl\u00fcck.", "tokens": ["Im", "drit\u00b7ten", "Reich", "ist", "al\u00b7les", "ei\u00b7tel", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VAFIN", "PIS", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wir holen unsre Br\u00fcder uns zur\u00fcck:", "tokens": ["Wir", "ho\u00b7len", "uns\u00b7re", "Br\u00fc\u00b7der", "uns", "zu\u00b7r\u00fcck", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "die Sudetendeutschen und die Saardeutschen", "tokens": ["die", "Su\u00b7de\u00b7ten\u00b7deut\u00b7schen", "und", "die", "Saar\u00b7deut\u00b7schen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NN"], "meter": "-+--+-+-+--", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "und die Eupendeutschen und die D\u00e4nendeutschen . . .", "tokens": ["und", "die", "Eu\u00b7pen\u00b7deut\u00b7schen", "und", "die", "D\u00e4\u00b7nen\u00b7deut\u00b7schen", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["KON", "ART", "NN", "KON", "ART", "NN", "$.", "$.", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.5": {"text": "Trutz dieser Welt! Wir pfeifen auf den Frieden.", "tokens": ["Trutz", "die\u00b7ser", "Welt", "!", "Wir", "pfei\u00b7fen", "auf", "den", "Frie\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PDAT", "NN", "$.", "PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wir brauchen Krieg. Sonst sind wir nichts hienieden.", "tokens": ["Wir", "brau\u00b7chen", "Krieg", ".", "Sonst", "sind", "wir", "nichts", "hien\u00b7ie\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$.", "NN", "VAFIN", "PPER", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Im dritten Reich haben wir gewonnenes Spiel.", "tokens": ["Im", "drit\u00b7ten", "Reich", "ha\u00b7ben", "wir", "ge\u00b7won\u00b7ne\u00b7nes", "Spiel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VAFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+----+--+", "measure": "iambic.tetra.chol"}, "line.8": {"text": "Da sind wir unter uns.", "tokens": ["Da", "sind", "wir", "un\u00b7ter", "uns", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Und unter uns, da ist nicht viel.", "tokens": ["Und", "un\u00b7ter", "uns", ",", "da", "ist", "nicht", "viel", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "$,", "ADV", "VAFIN", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Da herrscht der Bakel und der S\u00e4bel und der Stock \u2013", "tokens": ["Da", "herrscht", "der", "Ba\u00b7kel", "und", "der", "S\u00e4\u00b7bel", "und", "der", "Stock", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "KON", "ART", "NN", "KON", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "da gl\u00e4nzt der Orden an dem bunten Rock,", "tokens": ["da", "gl\u00e4nzt", "der", "Or\u00b7den", "an", "dem", "bun\u00b7ten", "Rock", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "da wird das Rad der Zeit zur\u00fcckgedreht \u2013", "tokens": ["da", "wird", "das", "Rad", "der", "Zeit", "zu\u00b7r\u00fcck\u00b7ge\u00b7dreht", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "wir rufen \u00bbVaterland!\u00ab, wenns gar nicht weiter geht . . .", "tokens": ["wir", "ru\u00b7fen", "\u00bb", "Va\u00b7ter\u00b7land", "!", "\u00ab", ",", "wenns", "gar", "nicht", "wei\u00b7ter", "geht", ".", ".", "."], "token_info": ["word", "word", "punct", "word", "punct", "punct", "punct", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PPER", "VVFIN", "$(", "NN", "$.", "$(", "$,", "KOUS", "ADV", "PTKNEG", "ADV", "VVFIN", "$.", "$.", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Da sind wir alle reich und gleich", "tokens": ["Da", "sind", "wir", "al\u00b7le", "reich", "und", "gleich"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PIAT", "ADJD", "KON", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "im dritten Reich.", "tokens": ["im", "drit\u00b7ten", "Reich", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.16": {"text": "Und wendisch und kaschubisch reine Arier.", "tokens": ["Und", "wen\u00b7disch", "und", "ka\u00b7schu\u00b7bisch", "rei\u00b7ne", "A\u00b7rier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KON", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.5": {"line.1": {"text": "Ja, richtig . . . Und die Proletarier!", "tokens": ["Ja", ",", "rich\u00b7tig", ".", ".", ".", "Und", "die", "Pro\u00b7le\u00b7ta\u00b7rier", "!"], "token_info": ["word", "punct", "word", "punct", "punct", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADJD", "$.", "$.", "$.", "KON", "ART", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "F\u00fcr die sind wir die Original-Befreier!", "tokens": ["F\u00fcr", "die", "sind", "wir", "die", "O\u00b7ri\u00b7gi\u00b7na\u00b7lBe\u00b7frei\u00b7er", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "VAFIN", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "Die danken Gott in jeder Morgenfeier \u2013", "tokens": ["Die", "dan\u00b7ken", "Gott", "in", "je\u00b7der", "Mor\u00b7gen\u00b7fei\u00b7er", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und merken gleich:", "tokens": ["Und", "mer\u00b7ken", "gleich", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Sie sind genau so arme Luder wie vorher,", "tokens": ["Sie", "sind", "ge\u00b7nau", "so", "ar\u00b7me", "Lu\u00b7der", "wie", "vor\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "ADV", "ADJA", "NN", "KOKOM", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "genau solch schuftendes und graues Heer,", "tokens": ["ge\u00b7nau", "solch", "schuf\u00b7ten\u00b7des", "und", "grau\u00b7es", "Heer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PIAT", "ADJA", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "genau so arme Schelme ohne Halm und Haber \u2013", "tokens": ["ge\u00b7nau", "so", "ar\u00b7me", "Schel\u00b7me", "oh\u00b7ne", "Halm", "und", "Ha\u00b7ber", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "ADJA", "NN", "APPR", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Aber:", "tokens": ["A\u00b7ber", ":"], "token_info": ["word", "punct"], "pos": ["KON", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.9": {"text": "im dritten Reich.", "tokens": ["im", "drit\u00b7ten", "Reich", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Und das sind wir.", "tokens": ["Und", "das", "sind", "wir", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Ein Blick in die Statistik:", "tokens": ["Ein", "Blick", "in", "die", "Sta\u00b7tis\u00b7tik", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wir fabrizieren viel. Am meisten nationale Mistik.", "tokens": ["Wir", "fab\u00b7ri\u00b7zie\u00b7ren", "viel", ".", "Am", "meis\u00b7ten", "na\u00b7ti\u00b7o\u00b7na\u00b7le", "Mis\u00b7tik", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$.", "PTKA", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+--+--+", "measure": "iambic.septa.relaxed"}}, "stanza.7": {"line.1": {"text": "Es braucht ein hohes Ideal", "tokens": ["Es", "braucht", "ein", "ho\u00b7hes", "I\u00b7deal"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "der nationale Mann,", "tokens": ["der", "na\u00b7ti\u00b7o\u00b7na\u00b7le", "Mann", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "daran er morgens allemal", "tokens": ["da\u00b7ran", "er", "mor\u00b7gens", "al\u00b7le\u00b7mal"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "PPER", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "ein wenig turnen kann.", "tokens": ["ein", "we\u00b7nig", "tur\u00b7nen", "kann", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Da hat denn deutsche Manneskraft", "tokens": ["Da", "hat", "denn", "deut\u00b7sche", "Man\u00b7nes\u00b7kraft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "in segensreichen Stunden", "tokens": ["in", "se\u00b7gens\u00b7r