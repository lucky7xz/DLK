{"dta.poem.5390": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Das durch die C\u00f6rper verherrlichte  \n Licht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Man siehet jetzt die Welt im Licht-Meer gleichsam", "tokens": ["Man", "sie\u00b7het", "jetzt", "die", "Welt", "im", "Licht\u00b7Meer", "gleich\u00b7sam"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "ADV", "ART", "NN", "APPRART", "NN", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "schwimmen,", "tokens": ["schwim\u00b7men", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Und in gef\u00e4rbter Glut der Sonnen herrlich glimmen;", "tokens": ["Und", "in", "ge\u00b7f\u00e4rb\u00b7ter", "Glut", "der", "Son\u00b7nen", "herr\u00b7lich", "glim\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "ART", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Jhr bunter Glantz, ihr sch\u00f6n gef\u00e4rbter Schein", "tokens": ["Ihr", "bun\u00b7ter", "Glantz", ",", "ihr", "sch\u00f6n", "ge\u00b7f\u00e4rb\u00b7ter", "Schein"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PPER", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Scheint himmlisch fast, nichts irdisches, zu seyn.", "tokens": ["Scheint", "himm\u00b7lisch", "fast", ",", "nichts", "ir\u00b7di\u00b7sches", ",", "zu", "seyn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ADV", "$,", "PIS", "ADJA", "$,", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Es fiel, indem ich j\u00fcngst auf einer H\u00f6he stand,", "tokens": ["Es", "fiel", ",", "in\u00b7dem", "ich", "j\u00fcngst", "auf", "ei\u00b7ner", "H\u00f6\u00b7he", "stand", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und tausendfache Lust am bunten Glantz empfand,", "tokens": ["Und", "tau\u00b7send\u00b7fa\u00b7che", "Lust", "am", "bun\u00b7ten", "Glantz", "emp\u00b7fand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Den mir die Sonne wies, mir folgends ein:", "tokens": ["Den", "mir", "die", "Son\u00b7ne", "wies", ",", "mir", "fol\u00b7gends", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "VVFIN", "$,", "PPER", "PIS", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Die Sonne scheint, nebst tausend sch\u00f6nen Dingen,", "tokens": ["Die", "Son\u00b7ne", "scheint", ",", "nebst", "tau\u00b7send", "sch\u00f6\u00b7nen", "Din\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "APPR", "CARD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Der Pflantzen Pracht darum hervor zu bringen,", "tokens": ["Der", "Pflant\u00b7zen", "Pracht", "da\u00b7rum", "her\u00b7vor", "zu", "brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PAV", "PTKVZ", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Damit ihr eignes, uns sonst gar nicht sichtbar, Licht,", "tokens": ["Da\u00b7mit", "ihr", "eig\u00b7nes", ",", "uns", "sonst", "gar", "nicht", "sicht\u00b7bar", ",", "Licht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PAV", "PPOSAT", "ADJA", "$,", "PPER", "ADV", "ADV", "PTKNEG", "ADJD", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Wenn es sich nicht an C\u00f6rpern bricht,", "tokens": ["Wenn", "es", "sich", "nicht", "an", "C\u00f6r\u00b7pern", "bricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "PTKNEG", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Durch diese sch\u00f6ne Zucht der Erden,", "tokens": ["Durch", "die\u00b7se", "sch\u00f6\u00b7ne", "Zucht", "der", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Jm Wiederschlag uns m\u00f6ge fichtbar werden.", "tokens": ["Jm", "Wie\u00b7der\u00b7schlag", "uns", "m\u00f6\u00b7ge", "ficht\u00b7bar", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "VMFIN", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Denn dadurch, da\u00df sich an der C\u00f6rper Fl\u00e4chen,", "tokens": ["Denn", "da\u00b7durch", ",", "da\u00df", "sich", "an", "der", "C\u00f6r\u00b7per", "Fl\u00e4\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "$,", "KOUS", "PRF", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Jm Wiederschlag, die strahlen brechen,", "tokens": ["Jm", "Wie\u00b7der\u00b7schlag", ",", "die", "strah\u00b7len", "bre\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PRELS", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Vermag nur an des Lichts sonst unbekannten Sch\u00e4tzen,", "tokens": ["Ver\u00b7mag", "nur", "an", "des", "Lichts", "sonst", "un\u00b7be\u00b7kann\u00b7ten", "Sch\u00e4t\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ART", "NN", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Sich unser Geist, durchs Auge, zu ergetzen.", "tokens": ["Sich", "un\u00b7ser", "Geist", ",", "durchs", "Au\u00b7ge", ",", "zu", "er\u00b7get\u00b7zen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PRF", "PPOSAT", "NN", "$,", "APPRART", "NN", "$,", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Es scheint es lie\u00df die Sonne darum blos", "tokens": ["Es", "scheint", "es", "lie\u00df", "die", "Son\u00b7ne", "da\u00b7rum", "blos"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "VVFIN", "ART", "NN", "PAV", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Die Pflantzen aus dem duncklem Scho\u00df", "tokens": ["Die", "Pflant\u00b7zen", "aus", "dem", "dunck\u00b7lem", "Scho\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Der Erde steigen,", "tokens": ["Der", "Er\u00b7de", "stei\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.22": {"text": "Um ihrer Strahlen Pracht in ihnen uns zu zeigen.", "tokens": ["Um", "ih\u00b7rer", "Strah\u00b7len", "Pracht", "in", "ih\u00b7nen", "uns", "zu", "zei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "NN", "APPR", "PPER", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Weil sonst, wie bunt, wie hell, wie wundersch\u00f6n", "tokens": ["Weil", "sonst", ",", "wie", "bunt", ",", "wie", "hell", ",", "wie", "wun\u00b7der\u00b7sch\u00f6n"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "ADV", "$,", "PWAV", "ADJD", "$,", "PWAV", "ADJD", "$,", "PWAV", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "Der Strahlen Eigenschaft, nicht, ohne sie, zu sehn.", "tokens": ["Der", "Strah\u00b7len", "Ei\u00b7gen\u00b7schaft", ",", "nicht", ",", "oh\u00b7ne", "sie", ",", "zu", "sehn", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "PTKNEG", "$,", "KOUI", "PPER", "$,", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Da wie bekannt, die Farben anders nichts", "tokens": ["Da", "wie", "be\u00b7kannt", ",", "die", "Far\u00b7ben", "an\u00b7ders", "nichts"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "ADJD", "$,", "ART", "NN", "ADV", "PIS"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.26": {"text": "Als Eigenschaft- und Mildrungen des Lichts,", "tokens": ["Als", "Ei\u00b7gen\u00b7schaft", "und", "Mil\u00b7drun\u00b7gen", "des", "Lichts", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "TRUNC", "KON", "NN", "ART", "NN", "$,"], "meter": "-+-+-++--+", "measure": "iambic.penta.chol"}, "line.27": {"text": "So wie sie an den Bau verschiedner C\u00f6rper fallen,", "tokens": ["So", "wie", "sie", "an", "den", "Bau", "ver\u00b7schied\u00b7ner", "C\u00f6r\u00b7per", "fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "PPER", "APPR", "ART", "NN", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Nachdem die Strahlen an- und wiedr abw\u00e4rts prallen.", "tokens": ["Nach\u00b7dem", "die", "Strah\u00b7len", "an", "und", "wiedr", "ab\u00b7w\u00e4rts", "pral\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "TRUNC", "KON", "ADV", "ADV", "VVINF", "$."], "meter": "+--+-+--+-+-", "measure": "iambic.penta.invert"}}}}}