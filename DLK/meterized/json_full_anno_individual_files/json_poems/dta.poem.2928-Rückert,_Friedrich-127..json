{"dta.poem.2928": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "127.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nur auf die Lebensfahrt nicht viel Gep\u00e4ck-Geschleppe!", "tokens": ["Nur", "auf", "die", "Le\u00b7bens\u00b7fahrt", "nicht", "viel", "Ge\u00b7p\u00e4ck\u00b7Ge\u00b7schlep\u00b7pe", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "PTKNEG", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Denn \u00fcber manchen Berg geht sie und manche Steppe.", "tokens": ["Denn", "\u00fc\u00b7ber", "man\u00b7chen", "Berg", "geht", "sie", "und", "man\u00b7che", "Step\u00b7pe", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PIAT", "NN", "VVFIN", "PPER", "KON", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Nicht von Spielwaaren sei ein Wagen mitgefahren,", "tokens": ["Nicht", "von", "Spiel\u00b7waa\u00b7ren", "sei", "ein", "Wa\u00b7gen", "mit\u00b7ge\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "NN", "VAFIN", "ART", "NN", "VVINF", "$,"], "meter": "---+-+-+-+-+-", "measure": "unknown.measure.penta"}, "line.2": {"text": "Genug zu schaffen macht ein Wagen Essenswaaren.", "tokens": ["Ge\u00b7nug", "zu", "schaf\u00b7fen", "macht", "ein", "Wa\u00b7gen", "Es\u00b7sens\u00b7waa\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKZU", "VVINF", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Auch von Andenken sei nicht mitgef\u00fchrt ein Kasten,", "tokens": ["Auch", "von", "An\u00b7den\u00b7ken", "sei", "nicht", "mit\u00b7ge\u00b7f\u00fchrt", "ein", "Kas\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VAFIN", "PTKNEG", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Bilder gn\u00fcgen dir, die schon im Hirne rasten,", "tokens": ["Die", "Bil\u00b7der", "gn\u00fc\u00b7gen", "dir", ",", "die", "schon", "im", "Hir\u00b7ne", "ras\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,", "PRELS", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Und im Schreibt\u00e4felchen besonders eingeschrieben", "tokens": ["Und", "im", "Schreib\u00b7t\u00e4\u00b7fel\u00b7chen", "be\u00b7son\u00b7ders", "ein\u00b7ge\u00b7schrie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "ADV", "VVINF"], "meter": "--+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ein Abschiedsliebesgru\u00df nur von besonders lieben.", "tokens": ["Ein", "Ab\u00b7schieds\u00b7lie\u00b7bes\u00b7gru\u00df", "nur", "von", "be\u00b7son\u00b7ders", "lie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}