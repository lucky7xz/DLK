{"textgrid.poem.40351": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "Laufbahn", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als ich jung war, hab ich verwogen", "tokens": ["Als", "ich", "jung", "war", ",", "hab", "ich", "ver\u00b7wo\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "VAFIN", "$,", "VAFIN", "PPER", "VVINF"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "alle Z\u00e4une im Feld \u00fcberflogen.", "tokens": ["al\u00b7le", "Z\u00e4u\u00b7ne", "im", "Feld", "\u00fc\u00b7berf\u00b7lo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "+-+--++-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Nun ich \u00e4lter bin, will ich verwegen", "tokens": ["Nun", "ich", "\u00e4l\u00b7ter", "bin", ",", "will", "ich", "ver\u00b7we\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "PPER", "ADJD", "VAFIN", "$,", "VMFIN", "PPER", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "selber neue Felder einhegen.", "tokens": ["sel\u00b7ber", "neu\u00b7e", "Fel\u00b7der", "ein\u00b7he\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VVIZU", "$."], "meter": "+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.5": {"text": "Und kommen, wenn ich alt bin, die Jungen", "tokens": ["Und", "kom\u00b7men", ",", "wenn", "ich", "alt", "bin", ",", "die", "Jun\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVINF", "$,", "KOUS", "PPER", "ADJD", "VAFIN", "$,", "ART", "NN