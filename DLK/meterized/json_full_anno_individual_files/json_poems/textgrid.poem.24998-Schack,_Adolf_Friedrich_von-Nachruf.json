{"textgrid.poem.24998": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Nachruf", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "L\u00e4ssest du allein mich so,", "tokens": ["L\u00e4s\u00b7sest", "du", "al\u00b7lein", "mich", "so", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PPER", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Der ich manchen Abend froh", "tokens": ["Der", "ich", "man\u00b7chen", "A\u00b7bend", "froh"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "PIAT", "NN", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hier mit dir gesessen?", "tokens": ["Hier", "mit", "dir", "ge\u00b7ses\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Deiner l\u00e4ngst zum Zwiegespr\u00e4ch", "tokens": ["Dei\u00b7ner", "l\u00e4ngst", "zum", "Zwie\u00b7ge\u00b7spr\u00e4ch"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADV", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Harr' ich; und hierher den Weg", "tokens": ["Harr'", "ich", ";", "und", "hier\u00b7her", "den", "Weg"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "$.", "KON", "PAV", "ART", "NN"], "meter": "+---+-+", "measure": "dactylic.init"}, "line.6": {"text": "Hast du nun vergessen?", "tokens": ["Hast", "du", "nun", "ver\u00b7ges\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Unten rauscht wie sonst der Rhein;", "tokens": ["Un\u00b7ten", "rauscht", "wie", "sonst", "der", "Rhein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KOKOM", "ADV", "ART", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "In dem Glase blinkt der Wein,", "tokens": ["In", "dem", "Gla\u00b7se", "blinkt", "der", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df mein Karl ihn trinke;", "tokens": ["Da\u00df", "mein", "Karl", "ihn", "trin\u00b7ke", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NE", "PPER", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Und ich lausch' und lausche bang,", "tokens": ["Und", "ich", "lausch'", "und", "lau\u00b7sche", "bang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADJD", "KON", "ADJA", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ob ich h\u00f6re seinen Gang,", "tokens": ["Ob", "ich", "h\u00f6\u00b7re", "sei\u00b7nen", "Gang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Ob sich regt die Klinke.", "tokens": ["Ob", "sich", "regt", "die", "Klin\u00b7ke", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "O die Zeit, wie froh sie war,", "tokens": ["O", "die", "Zeit", ",", "wie", "froh", "sie", "war", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$,", "PWAV", "ADJD", "PPER", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Als so wie ein Bl\u00fctenpaar,", "tokens": ["Als", "so", "wie", "ein", "Bl\u00fc\u00b7ten\u00b7paar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "KOKOM", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hier des Lebens s\u00fc\u00dfem Mai,", "tokens": ["Hier", "des", "Le\u00b7bens", "s\u00fc\u00b7\u00dfem", "Mai", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Knospend, duftend unsre zwei", "tokens": ["Knos\u00b7pend", ",", "duf\u00b7tend", "uns\u00b7re", "zwei"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ADJD", "$,", "ADJD", "PPOSAT", "CARD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Seelen sich erschlossen.", "tokens": ["See\u00b7len", "sich", "er\u00b7schlos\u00b7sen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PRF", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Hier im sch\u00f6nen Seelenrausch", "tokens": ["Hier", "im", "sch\u00f6\u00b7nen", "See\u00b7len\u00b7rausch"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Bei der Reden Wechseltausch", "tokens": ["Bei", "der", "Re\u00b7den", "Wech\u00b7sel\u00b7tausch"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ihn zum Freund gewann ich;", "tokens": ["Ihn", "zum", "Freund", "ge\u00b7wann", "ich", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "VVFIN", "PPER", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Jedes Wort, das ihm entquoll,", "tokens": ["Je\u00b7des", "Wort", ",", "das", "ihm", "ent\u00b7quoll", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PRELS", "PPER", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Schien mir tiefer Weisheit voll,", "tokens": ["Schien", "mir", "tie\u00b7fer", "Weis\u00b7heit", "voll", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJA", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Lang dar\u00fcber sann ich.", "tokens": ["Lang", "da\u00b7r\u00fc\u00b7ber", "sann", "ich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PAV", "VVFIN", "PPER", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Eh mit erstem Schein der Tag", "tokens": ["Eh", "mit", "ers\u00b7tem", "Schein", "der", "Tag"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ADJA", "NN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Durch das Rebengitter brach,", "tokens": ["Durch", "das", "Re\u00b7ben\u00b7git\u00b7ter", "brach", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Kam er, mich zu wecken,", "tokens": ["Kam", "er", ",", "mich", "zu", "we\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "$,", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Und bei Lerchen-Morgensang", "tokens": ["Und", "bei", "Ler\u00b7chen\u00b7Mor\u00b7gens\u00b7ang"], "token_info": ["word", "word", "word"], "pos": ["KON", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Schritten wir den Rhein entlang", "tokens": ["Schrit\u00b7ten", "wir", "den", "Rhein", "ent\u00b7lang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NE", "APPO"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Durch die Wei\u00dfdornhecken,", "tokens": ["Durch", "die", "Wei\u00df\u00b7dorn\u00b7he\u00b7cken", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "Sahen \u00fcber Wiesengr\u00fcn", "tokens": ["Sa\u00b7hen", "\u00fc\u00b7ber", "Wie\u00b7sen\u00b7gr\u00fcn"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Fernhin alte Burgen gl\u00fchn", "tokens": ["Fern\u00b7hin", "al\u00b7te", "Bur\u00b7gen", "gl\u00fchn"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Auf den Felsenspitzen,", "tokens": ["Auf", "den", "Fel\u00b7sen\u00b7spit\u00b7zen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Und die Th\u00e4ler, feucht von Tau,", "tokens": ["Und", "die", "Th\u00e4\u00b7ler", ",", "feucht", "von", "Tau", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "ADJD", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Nach und nach durchs D\u00e4mmergrau", "tokens": ["Nach", "und", "nach", "durchs", "D\u00e4m\u00b7mer\u00b7grau"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "KON", "APPR", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Hell im Fr\u00fchlicht blitzen.", "tokens": ["Hell", "im", "Fr\u00fch\u00b7licht", "blit\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.7": {"line.1": {"text": "Dann, wenn in des Lernens Drang", "tokens": ["Dann", ",", "wenn", "in", "des", "Ler\u00b7nens", "Drang"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "KOUS", "APPR", "ART", "NN", "NN"], "meter": "+---+-+", "measure": "dactylic.init"}, "line.2": {"text": "Einer mit dem andern rang", "tokens": ["Ei\u00b7ner", "mit", "dem", "an\u00b7dern", "rang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Um den Sieg im Wissen,", "tokens": ["Um", "den", "Sieg", "im", "Wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Stets von ihm mir, ob ich hei\u00df", "tokens": ["Stets", "von", "ihm", "mir", ",", "ob", "ich", "hei\u00df"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "APPR", "PPER", "PPER", "$,", "KOUS", "PPER", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Auch geworben um den Preis,", "tokens": ["Auch", "ge\u00b7wor\u00b7ben", "um", "den", "Preis", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Sah ich ihn entrissen.", "tokens": ["Sah", "ich", "ihn", "ent\u00b7ris\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.8": {"line.1": {"text": "Ihm mit Staunen blickt' ich nach;", "tokens": ["Ihm", "mit", "Stau\u00b7nen", "blickt'", "ich", "nach", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Doch, wenn mir die Kraft gebrach,", "tokens": ["Doch", ",", "wenn", "mir", "die", "Kraft", "ge\u00b7brach", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Um ihm nachzuringen,", "tokens": ["Um", "ihm", "nach\u00b7zu\u00b7rin\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "VVIZU", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Dacht' ich bang: Genug! genug!", "tokens": ["Dacht'", "ich", "bang", ":", "Ge\u00b7nug", "!", "ge\u00b7nug", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "$.", "ADV", "$.", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Brechen m\u00fcssen bei dem Flug", "tokens": ["Bre\u00b7chen", "m\u00fcs\u00b7sen", "bei", "dem", "Flug"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VMFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Endlich seine Schwingen.", "tokens": ["End\u00b7lich", "sei\u00b7ne", "Schwin\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.9": {"line.1": {"text": "Und es kam, wie ich gedacht;", "tokens": ["Und", "es", "kam", ",", "wie", "ich", "ge\u00b7dacht", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "PWAV", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Um sein fr\u00fches Grab bei Nacht", "tokens": ["Um", "sein", "fr\u00fc\u00b7hes", "Grab", "bei", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "PPOSAT", "ADJA", "NN", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Flattert die Phal\u00e4ne;", "tokens": ["Flat\u00b7tert", "die", "Pha\u00b7l\u00e4\u00b7ne", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Wo so oft er bei mir sa\u00df,", "tokens": ["Wo", "so", "oft", "er", "bei", "mir", "sa\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADV", "PPER", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Bleib' ich einsam, und ins Glas", "tokens": ["Bleib'", "ich", "ein\u00b7sam", ",", "und", "ins", "Glas"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADJD", "$,", "KON", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Rieselt eine Thr\u00e4ne.", "tokens": ["Rie\u00b7selt", "ei\u00b7ne", "Thr\u00e4\u00b7ne", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.10": {"line.1": {"text": "L\u00e4ssest du allein mich so,", "tokens": ["L\u00e4s\u00b7sest", "du", "al\u00b7lein", "mich", "so", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PPER", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Der ich manchen Abend froh", "tokens": ["Der", "ich", "man\u00b7chen", "A\u00b7bend", "froh"], "token_info": ["word", "w