{"textgrid.poem.56217": {"metadata": {"author": {"name": "Chamisso, Adelbert von", "birth": "N.A.", "death": "N.A."}, "title": "18", "genre": "verse", "period": "N.A.", "pub_year": 1809, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du liebst mich wohl, ich zweifle nicht daran,", "tokens": ["Du", "liebst", "mich", "wohl", ",", "ich", "zweif\u00b7le", "nicht", "da\u00b7ran", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$,", "PPER", "VVFIN", "PTKNEG", "PAV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und lebte nicht, wenn mir ein Zweifel bliebe;", "tokens": ["Und", "leb\u00b7te", "nicht", ",", "wenn", "mir", "ein", "Zwei\u00b7fel", "blie\u00b7be", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch liebst du mich, du lieber b\u00f6ser Mann,", "tokens": ["Doch", "liebst", "du", "mich", ",", "du", "lie\u00b7ber", "b\u00f6\u00b7ser", "Mann", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PRF", "$,", "PPER", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Nicht so, wie ich dich liebe.", "tokens": ["Nicht", "so", ",", "wie", "ich", "dich", "lie\u00b7be", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "$,", "PWAV", "PPER", "PRF", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Geteilten Herzens, halb, und halb wohl kaum,", "tokens": ["Ge\u00b7teil\u00b7ten", "Her\u00b7zens", ",", "halb", ",", "und", "halb", "wohl", "kaum", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJD", "$,", "KON", "ADJD", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wann eben Zeit und Ort es also geben;", "tokens": ["Wann", "e\u00b7ben", "Zeit", "und", "Ort", "es", "al\u00b7so", "ge\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "NN", "KON", "NN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Du aber bist mein Wachen und mein Traum,", "tokens": ["Du", "a\u00b7ber", "bist", "mein", "Wa\u00b7chen", "und", "mein", "Traum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mein ganzes Sein, mein Leben.", "tokens": ["Mein", "gan\u00b7zes", "Sein", ",", "mein", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Du kennst nicht deiner s\u00fc\u00dfen Stimme Macht,", "tokens": ["Du", "kennst", "nicht", "dei\u00b7ner", "s\u00fc\u00b7\u00dfen", "Stim\u00b7me", "Macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "PPOSAT", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wenn du dich liebefl\u00fcsternd zu mir neigest;", "tokens": ["Wenn", "du", "dich", "lie\u00b7be\u00b7fl\u00fcs\u00b7ternd", "zu", "mir", "nei\u00b7gest", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADJD", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein armes Wort, das schon mich selig macht,", "tokens": ["Ein", "ar\u00b7mes", "Wort", ",", "das", "schon", "mich", "se\u00b7lig", "macht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ADV", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Du sprichst es nicht, du schweigest.", "tokens": ["Du", "sprichst", "es", "nicht", ",", "du", "schwei\u00b7gest", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "$,", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Noch winde dich aus meinem Arm nicht fort,", "tokens": ["Noch", "win\u00b7de", "dich", "aus", "mei\u00b7nem", "Arm", "nicht", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "PTKNEG", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "La\u00df lesen mich aus deinen lieben Augen,", "tokens": ["La\u00df", "le\u00b7sen", "mich", "aus", "dei\u00b7nen", "lie\u00b7ben", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "VVFIN", "PRF", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und von dem kargen Lippenpaar das Wort,", "tokens": ["Und", "von", "dem", "kar\u00b7gen", "Lip\u00b7pen\u00b7paar", "das", "Wort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Das ungesprochne, saugen.", 