{"textgrid.poem.41266": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "An Melint", "genre": "verse", "period": "N.A.", "pub_year": 1731, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du willst, ich soll jetzt mit Cecil,", "tokens": ["Du", "willst", ",", "ich", "soll", "jetzt", "mit", "Ce\u00b7cil", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "PPER", "VMFIN", "ADV", "APPR", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dem feinen Mann, Bekanntschaft machen.", "tokens": ["Dem", "fei\u00b7nen", "Mann", ",", "Be\u00b7kannt\u00b7schaft", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Du r\u00fchmest ihn: er spricht nicht viel,", "tokens": ["Du", "r\u00fch\u00b7mest", "ihn", ":", "er", "spricht", "nicht", "viel", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$.", "PPER", "VVFIN", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "H\u00e4lt Ordnung in den kleinsten Sachen,", "tokens": ["H\u00e4lt", "Ord\u00b7nung", "in", "den", "kleins\u00b7ten", "Sa\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Liebt H\u00e4uslichkeit, und flieht das Spiel.", "tokens": ["Liebt", "H\u00e4us\u00b7lich\u00b7keit", ",", "und", "flieht", "das", "Spiel", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "$,", "KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Er sagt recht h\u00f6flich, was er meint:", "tokens": ["Er", "sagt", "recht", "h\u00f6f\u00b7lich", ",", "was", "er", "meint", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Er wird nicht, durch den Umgang, k\u00fchner.", "tokens": ["Er", "wird", "nicht", ",", "durch", "den", "Um\u00b7gang", ",", "k\u00fch\u00b7ner", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "$,", "APPR", "ART", "NN", "$,", "ADJA", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Wie sehr ist er dem Weine feind! ...", "tokens": ["Wie", "sehr", "ist", "er", "dem", "Wei\u00b7ne", "feind", "!", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "ADV", "VAFIN", "PPER", "ART", "NN", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Melint, so lob' ich einen Diener,", "tokens": ["Me\u00b7lint", ",", "so", "lob'", "ich", "ei\u00b7nen", "Die\u00b7ner", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "So lob' ich niemals einen Freund.", "tokens": ["So", "lob'", "ich", "nie\u00b7mals", "ei\u00b7nen", "Freund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Du willst, ich soll jetzt mit Cecil,", "tokens": ["Du", "willst", ",", "ich", "soll", "jetzt", "mit", "Ce\u00b