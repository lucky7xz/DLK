{"textgrid.poem.55100": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Amyntas", "genre": "verse", "period": "N.A.", "pub_year": 1797, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nikias, trefflicher Mann, du Arzt des Leibs und der Seele!", "tokens": ["Ni\u00b7ki\u00b7as", ",", "treff\u00b7li\u00b7cher", "Mann", ",", "du", "Arzt", "des", "Leibs", "und", "der", "See\u00b7le", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJA", "NN", "$,", "PPER", "NN", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Krank, ich bin es f\u00fcrwahr; aber dein Mittel ist hart.", "tokens": ["Krank", ",", "ich", "bin", "es", "f\u00fcr\u00b7wahr", ";", "a\u00b7ber", "dein", "Mit\u00b7tel", "ist", "hart", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VAFIN", "PPER", "ADV", "$.", "ADV", "PPOSAT", "NN", "VAFIN", "ADJD", "$."], "meter": "+-+--+---+--+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Ach, mir schwanden die Kr\u00e4fte dahin, dem Rate zu folgen;", "tokens": ["Ach", ",", "mir", "schwan\u00b7den", "die", "Kr\u00e4f\u00b7te", "da\u00b7hin", ",", "dem", "Ra\u00b7te", "zu", "fol\u00b7gen", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "VVFIN", "ART", "NN", "PAV", "$,", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Ja, und es scheinet der Freund schon mir ein Gegner zu sein.", "tokens": ["Ja", ",", "und", "es", "schei\u00b7net", "der", "Freund", "schon", "mir", "ein", "Geg\u00b7ner", "zu", "sein", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "KON", "PPER", "VVFIN", "ART", "NN", "ADV", "PPER", "ART", "NN", "PTKZU", "VAINF", "$."], "meter": "-+-+--+-+-+--+", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Widerlegen kann ich dich nicht; ich sage mir alles,", "tokens": ["Wi\u00b7der\u00b7le\u00b7gen", "kann", "ich", "dich", "nicht", ";", "ich", "sa\u00b7ge", "mir", "al\u00b7les", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PRF", "PTKNEG", "$.", "PPER", "VVFIN", "PPER", "PIS", "$,"], "meter": "+-+-+-+--+--+-", "measure": "hexameter"}, "line.6": {"text": "Sage das h\u00e4rtere Wort, das du verschweigest, mir auch.", "tokens": ["Sa\u00b7ge", "das", "h\u00e4r\u00b7te\u00b7re", "Wort", ",", "das", "du", "ver\u00b7schwei\u00b7gest", ",", "mir", "auch", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,", "PPER", "ADV", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.7": {"text": "Aber, ach! das Wasser entst\u00fcrzt der Steile des Felsens", "tokens": ["A\u00b7ber", ",", "ach", "!", "das", "Was\u00b7ser", "ent\u00b7st\u00fcrzt", "der", "Stei\u00b7le", "des", "Fel\u00b7sens"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "$,", "ITJ", "$.", "ART", "NN", "VVFIN", "ART", "NN", "ART", "NN"], "meter": "+-+-+--+-+--+-", "measure": "hexameter"}, "line.8": {"text": "Rasch, und die Welle des Bachs halten Ges\u00e4nge nicht auf.", "tokens": ["Rasch", ",", "und", "die", "Wel\u00b7le", "des", "Bachs", "hal\u00b7ten", "Ge\u00b7s\u00e4n\u00b7ge", "nicht", "auf", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "KON", "ART", "NN", "ART", "NN", "ADJA", "NN", "PTKNEG", "PTKVZ", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.9": {"text": "Rast nicht unaufhaltsam der Sturm? und w\u00e4lzet die Sonne", "tokens": ["Rast", "nicht", "un\u00b7auf\u00b7halt\u00b7sam", "der", "Sturm", "?", "und", "w\u00e4l\u00b7zet", "die", "Son\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PTKNEG", "ADJD", "ART", "NN", "$.", "KON", "VVFIN", "ART", "NN"], "meter": "+-+-+--+-+--+-", "measure": "hexameter"}, "line.10": {"text": "Sich, von dem Gipfel des Tags, nicht in die Wellen hinab?", "tokens": ["Sich", ",", "von", "dem", "Gip\u00b7fel", "des", "Tags", ",", "nicht", "in", "die", "Wel\u00b7len", "hin\u00b7ab", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "$,", "APPR", "ART", "NN", "ART", "NN", "$,", "PTKNEG", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.11": {"text": "Und so spricht mir rings die Natur: \u00bbAuch du bist, Amyntas,", "tokens": ["Und", "so", "spricht", "mir", "rings", "die", "Na\u00b7tur", ":", "\u00bb", "Auch", "du", "bist", ",", "A\u00b7myn\u00b7tas", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "$.", "$(", "ADV", "PPER", "VAFIN", "$,", "NE", "$,"], "meter": "--+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "Unter das strenge Gesetz ehrner Gewalten gebeugt.\u00ab", "tokens": ["Un\u00b7ter", "das", "stren\u00b7ge", "Ge\u00b7setz", "ehr\u00b7ner", "Ge\u00b7wal\u00b7ten", "ge\u00b7beugt", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ADJA", "NN", "VVPP", "$.", "$("], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}, "line.13": {"text": "Runzle die Stirne nicht tiefer, mein Freund, und h\u00f6re gef\u00e4llig,", "tokens": ["Runz\u00b7le", "die", "Stir\u00b7ne", "nicht", "tie\u00b7fer", ",", "mein", "Freund", ",", "und", "h\u00f6\u00b7re", "ge\u00b7f\u00e4l\u00b7lig", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKNEG", "ADJD", "$,", "PPOSAT", "NN", "$,", "KON", "VVFIN", "ADJD", "$,"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.14": {"text": "Was mich gestern ein Baum dort an dem Bache gelehrt.", "tokens": ["Was", "mich", "ge\u00b7stern", "ein", "Baum", "dort", "an", "dem", "Ba\u00b7che", "ge\u00b7lehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "ART", "NN", "ADV", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.15": {"text": "Wenig \u00c4pfel tr\u00e4gt er mir nur, der sonst so beladne;", "tokens": ["We\u00b7nig", "\u00c4p\u00b7fel", "tr\u00e4gt", "er", "mir", "nur", ",", "der", "sonst", "so", "be\u00b7lad\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "PPER", "ADV", "$,", "PRELS", "ADV", "ADV", "ADJA", "$."], "meter": "+-+-+--+-+--+-", "measure": "hexameter"}, "line.16": {"text": "Sieh, der Efeu ist schuld, der ihn gewaltig umgibt.", "tokens": ["Sieh", ",", "der", "E\u00b7feu", "ist", "schuld", ",", "der", "ihn", "ge\u00b7wal\u00b7tig", "um\u00b7gibt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "NN", "VAFIN", "ADJD", "$,", "PRELS", "PPER", "ADJD", "VVFIN", "$."], "meter": "+--+-+-+-+--+", "measure": "iambic.hexa.invert"}, "line.17": {"text": "Und ich fa\u00dfte das Messer, das krummgebogene, scharfe,", "tokens": ["Und", "ich", "fa\u00df\u00b7te", "das", "Mes\u00b7ser", ",", "das", "krumm\u00b7ge\u00b7bo\u00b7ge\u00b7ne", ",", "schar\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "$,", "ART", "ADJA", "$,", "VVFIN", "$,"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.18": {"text": "Trennte schneidend und ri\u00df Ranke nach Ranken herab;", "tokens": ["Trenn\u00b7te", "schnei\u00b7dend", "und", "ri\u00df", "Ran\u00b7ke", "nach", "Ran\u00b7ken", "her\u00b7ab", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "KON", "VVFIN", "NN", "APPR", "NN", "ADV", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.19": {"text": "Aber ich schauderte gleich, als tief erseufzend und kl\u00e4glich", "tokens": ["A\u00b7ber", "ich", "schau\u00b7der\u00b7te", "gleich", ",", "als", "tief", "er\u00b7seuf\u00b7zend", "und", "kl\u00e4g\u00b7lich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ADV", "$,", "KOUS", "ADJD", "VVPP", "KON", "ADJD"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.20": {"text": "Aus den Wipfeln zu mir lispelnde Klage sich go\u00df:", "tokens": ["Aus", "den", "Wip\u00b7feln", "zu", "mir", "lis\u00b7peln\u00b7de", "Kla\u00b7ge", "sich", "go\u00df", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "PPER", "ADJA", "NN", "PRF", "VVFIN", "$."], "meter": "--+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.21": {"text": "\u00bbo verletze mich nicht! den treuen Gartengenossen,", "tokens": ["\u00bb", "o", "ver\u00b7let\u00b7ze", "mich", "nicht", "!", "den", "treu\u00b7en", "Gar\u00b7ten\u00b7ge\u00b7nos\u00b7sen", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "FM", "VVFIN", "PPER", "PTKNEG", "$.", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.22": {"text": "Dem du, als Knabe, so fr\u00fch, manche Gen\u00fcsse verdankt.", "tokens": ["Dem", "du", ",", "als", "Kna\u00b7be", ",", "so", "fr\u00fch", ",", "man\u00b7che", "Ge\u00b7n\u00fcs\u00b7se", "ver\u00b7dankt", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NE", "$,", "KOUS", "NN", "$,", "ADV", "ADJD", "$,", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+--++--+--+", "measure": "iambic.hexa.relaxed"}, "line.23": {"text": "O verletze mich nicht! du rei\u00dfest mit diesem Geflechte,", "tokens": ["O", "ver\u00b7let\u00b7ze", "mich", "nicht", "!", "du", "rei\u00b7\u00dfest", "mit", "die\u00b7sem", "Ge\u00b7flech\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "PTKNEG", "$.", "PPER", "VVFIN", "APPR", "PDAT", "NN", "$,"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.24": {"text": "Das du gewaltig zerst\u00f6rst, grausam das Leben mir aus.", "tokens": ["Das", "du", "ge\u00b7wal\u00b7tig", "zer\u00b7st\u00f6rst", ",", "grau\u00b7sam", "das", "Le\u00b7ben", "mir", "aus", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "VVPP", "$,", "ADJD", "ART", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+---+--+--+", "measure": "iambic.penta.relaxed"}, "line.25": {"text": "Hab ich nicht selbst sie gen\u00e4hrt und sanft sie herauf mir erzogen?", "tokens": ["Hab", "ich", "nicht", "selbst", "sie", "ge\u00b7n\u00e4hrt", "und", "sanft", "sie", "her\u00b7auf", "mir", "er\u00b7zo\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PTKNEG", "ADV", "PPER", "VVPP", "KON", "VVFIN", "PPER", "ADV", "PPER", "VVPP", "$."], "meter": "+-+-+-+-+--+--+-", "measure": "trochaic.septa.relaxed"}, "line.26": {"text": "Ist wie mein eigenes Laub nicht mir das ihre verwandt?", "tokens": ["Ist", "wie", "mein", "ei\u00b7ge\u00b7nes", "Laub", "nicht", "mir", "das", "ih\u00b7re", "ver\u00b7wandt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "KOKOM", "PPOSAT", "ADJA", "NN", "PTKNEG", "PPER", "PDS", "PPOSAT", "VVPP", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.27": {"text": "Soll ich nicht lieben die Pflanze, die, meiner einzig bed\u00fcrftig,", "tokens": ["Soll", "ich", "nicht", "lie\u00b7ben", "die", "Pflan\u00b7ze", ",", "die", ",", "mei\u00b7ner", "ein\u00b7zig", "be\u00b7d\u00fcrf\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "VVFIN", "ART", "NN", "$,", "PRELS", "$,", "PPOSAT", "ADJD", "ADJD", "$,"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.28": {"text": "Still mit begieriger Kraft mir um die Seite sich schlingt?", "tokens": ["Still", "mit", "be\u00b7gie\u00b7ri\u00b7ger", "Kraft", "mir", "um", "die", "Sei\u00b7te", "sich", "schlingt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ADJA", "NN", "PPER", "APPR", "ART", "NN", "PRF", "VVFIN", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.29": {"text": "Tausend Ranken wurzelten an, mit tausend und tausend", "tokens": ["Tau\u00b7send", "Ran\u00b7ken", "wur\u00b7zel\u00b7ten", "an", ",", "mit", "tau\u00b7send", "und", "tau\u00b7send"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["CARD", "NN", "VVFIN", "PTKVZ", "$,", "APPR", "CARD", "KON", "CARD"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.30": {"text": "Fasern senket sie fest mir in das Leben sich ein.", "tokens": ["Fa\u00b7sern", "sen\u00b7ket", "sie", "fest", "mir", "in", "das", "Le\u00b7ben", "sich", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "PRF", "PTKVZ", "$."], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.31": {"text": "Nahrung nimmt sie von mir; was ich bed\u00fcrfte, genie\u00dft sie,", "tokens": ["Nah\u00b7rung", "nimmt", "sie", "von", "mir", ";", "was", "ich", "be\u00b7d\u00fcrf\u00b7te", ",", "ge\u00b7nie\u00dft", "sie", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPR", "PPER", "$.", "PWS", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "$,"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.32": {"text": "Und so saugt sie das Mark, sauget die Seele mir aus.", "tokens": ["Und", "so", "saugt", "sie", "das", "Mark", ",", "sau\u00b7get", "die", "See\u00b7le", "mir", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ART", "NN", "$,", "VVFIN", "ART", "NN", "PPER", "PTKVZ", "$."], "meter": "--+--++--+--+", "measure": "anapaest.di.plus"}, "line.33": {"text": "Nur vergebens n\u00e4hr ich mich noch; die gewaltige Wurzel", "tokens": ["Nur", "ver\u00b7ge\u00b7bens", "n\u00e4hr", "ich", "mich", "noch", ";", "die", "ge\u00b7wal\u00b7ti\u00b7ge", "Wur\u00b7zel"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "PRF", "ADV", "$.", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.34": {"text": "Sendet lebendigen Safts, ach ! nur die H\u00e4lfte hinauf.", "tokens": ["Sen\u00b7det", "le\u00b7ben\u00b7di\u00b7gen", "Safts", ",", "ach", "!", "nur", "die", "H\u00e4lf\u00b7te", "hin\u00b7auf", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "$,", "ITJ", "$.", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.35": {"text": "Denn der gef\u00e4hrliche Gast, der geliebteste, ma\u00dfet behende", "tokens": ["Denn", "der", "ge\u00b7f\u00e4hr\u00b7li\u00b7che", "Gast", ",", "der", "ge\u00b7lieb\u00b7tes\u00b7te", ",", "ma\u00b7\u00dfet", "be\u00b7hen\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "$,", "ART", "ADJA", "$,", "VVFIN", "ADJA"], "meter": "-+-+--+--+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.36": {"text": "Unterweges die Kraft herbstlicher Fr\u00fcchte sich an.", "tokens": ["Un\u00b7ter\u00b7we\u00b7ges", "die", "Kraft", "herbst\u00b7li\u00b7cher", "Fr\u00fcch\u00b7te", "sich", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADJA", "NN", "PRF", "PTKVZ", "$."], "meter": "+-+--++--+--+", "measure": "trochaic.hexa.relaxed"}, "line.37": {"text": "Nichts gelangt zur Krone hinauf, die \u00e4u\u00dfersten Wipfel", "tokens": ["Nichts", "ge\u00b7langt", "zur", "Kro\u00b7ne", "hin\u00b7auf", ",", "die", "\u00e4u\u00b7\u00dfers\u00b7ten", "Wip\u00b7fel"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PIS", "VVPP", "APPRART", "NN", "PTKVZ", "$,", "ART", "ADJA", "NN"], "meter": "+-+-+--+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.38": {"text": "Dorren, es dorret der Ast \u00fcber dem Bache schon hin.", "tokens": ["Dor\u00b7ren", ",", "es", "dor\u00b7ret", "der", "Ast", "\u00fc\u00b7ber", "dem", "Ba\u00b7che", "schon", "hin", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "ADV", "PTKVZ", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.39": {"text": "Ja, die Verr\u00e4terin ist's! sie schmeichelt mir Leben und G\u00fcter,", "tokens": ["Ja", ",", "die", "Ver\u00b7r\u00e4\u00b7te\u00b7rin", "ist's", "!", "sie", "schmei\u00b7chelt", "mir", "Le\u00b7ben", "und", "G\u00fc\u00b7ter", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ART", "NN", "VAFIN", "$.", "PPER", "VVFIN", "PPER", "NN", "KON", "NN", "$,"], "meter": "-+-+-+--+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.40": {"text": "Schmeichelt die strebende Kraft, schmeichelt die Hoffnung mir ab.", "tokens": ["Schmei\u00b7chelt", "die", "stre\u00b7ben\u00b7de", "Kraft", ",", "schmei\u00b7chelt", "die", "Hoff\u00b7nung", "mir", "ab", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,", "VVFIN", "ART", "NN", "PPER", "PTKVZ", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.41": {"text": "Sie nur f\u00fchl ich, nur sie, die umschlingende, freue der Fesseln,", "tokens": ["Sie", "nur", "f\u00fchl", "ich", ",", "nur", "sie", ",", "die", "um\u00b7schlin\u00b7gen\u00b7de", ",", "freu\u00b7e", "der", "Fes\u00b7seln", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PPER", "$,", "ADV", "PPER", "$,", "ART", "ADJA", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "--+-+-+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.42": {"text": "Freue des t\u00f6tenden Schmucks, fremder Umlaubung mich nur.\u00ab", "tokens": ["Freu\u00b7e", "des", "t\u00f6\u00b7ten\u00b7den", "Schmucks", ",", "frem\u00b7der", "Um\u00b7lau\u00b7bung", "mich", "nur", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,", "ADJA", "NN", "PPER", "ADV", "$.", "$("], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.43": {"text": "Halte das Messer zur\u00fcck! o Nikias, schone den Armen,", "tokens": ["Hal\u00b7te", "das", "Mes\u00b7ser", "zu\u00b7r\u00fcck", "!", "o", "Ni\u00b7ki\u00b7as", ",", "scho\u00b7ne", "den", "Ar\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "PTKVZ", "$.", "FM", "NE", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.44": {"text": "Der sich in liebender Lust, willig gezwungen, verzehrt!", "tokens": ["Der", "sich", "in", "lie\u00b7ben\u00b7der", "Lust", ",", "wil\u00b7lig", "ge\u00b7zwun\u00b7gen", ",", "ver\u00b7zehrt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "PRF", "APPR", "ADJA", "NN", "$,", "ADJD", "VVPP", "$,", "VVPP", "$."], "meter": "-+-+--++--+--+", "measure": "iambic.hexa.relaxed"}, "line.45": {"text": "S\u00fc\u00df ist jede Verschwendung; o la\u00df mich der sch\u00f6nsten genie\u00dfen!", "tokens": ["S\u00fc\u00df", "ist", "je\u00b7de", "Ver\u00b7schwen\u00b7dung", ";", "o", "la\u00df", "mich", "der", "sch\u00f6ns\u00b7ten", "ge\u00b7nie\u00b7\u00dfen", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PIAT", "NN", "$.", "FM", "VVIMP", "PPER", "ART", "ADJA", "VVINF", "$."], "meter": "+-+--+-+-+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.46": {"text": "Wer sich der Liebe vertraut, h\u00e4lt er sein Leben zu Rat?", "tokens": ["Wer", "sich", "der", "Lie\u00b7be", "ver\u00b7traut", ",", "h\u00e4lt", "er", "sein", "Le\u00b7ben", "zu", "Rat", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "ART", "NN", "ADJD", "$,", "VVFIN", "PPER", "PPOSAT", "NN", "APPR", "NN", "$."], "meter": "-+-+--+---+--+", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Nikias, trefflicher Mann, du Arzt des Leibs und der Seele!", "tokens": ["Ni\u00b7ki\u00b7as", ",", "treff\u00b7li\u00b7cher", "Mann", ",", "du", "Arzt", "des", "Leibs", "und", "der", "See\u00b7le", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJA", "NN", "$,", "PPER", "NN", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Krank, ich bin es f\u00fcrwahr; aber dein Mittel ist hart.", "tokens": ["Krank", ",", "ich", "bin", "es", "f\u00fcr\u00b7wahr", ";", "a\u00b7ber", "dein", "Mit\u00b7tel", "ist", "hart", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VAFIN", "PPER", "ADV", "$.", "ADV", "PPOSAT", "NN", "VAFIN", "ADJD", "$."], "meter": "+-+--+---+--+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Ach, mir schwanden die Kr\u00e4fte dahin, dem Rate zu folgen;", "tokens": ["Ach", ",", "mir", "schwan\u00b7den", "die", "Kr\u00e4f\u00b7te", "da\u00b7hin", ",", "dem", "Ra\u00b7te", "zu", "fol\u00b7gen", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "VVFIN", "ART", "NN", "PAV", "$,", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Ja, und es scheinet der Freund schon mir ein Gegner zu sein.", "tokens": ["Ja", ",", "und", "es", "schei\u00b7net", "der", "Freund", "schon", "mir", "ein", "Geg\u00b7ner", "zu", "sein", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "KON", "PPER", "VVFIN", "ART", "NN", "ADV", "PPER", "ART", "NN", "PTKZU", "VAINF", "$."], "meter": "-+-+--+-+-+--+", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Widerlegen kann ich dich nicht; ich sage mir alles,", "tokens": ["Wi\u00b7der\u00b7le\u00b7gen", "kann", "ich", "dich", "nicht", ";", "ich", "sa\u00b7ge", "mir", "al\u00b7les", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PRF", "PTKNEG", "$.", "PPER", "VVFIN", "PPER", "PIS", "$,"], "meter": "+-+-+-+--+--+-", "measure": "hexameter"}, "line.6": {"text": "Sage das h\u00e4rtere Wort, das du verschweigest, mir auch.", "tokens": ["Sa\u00b7ge", "das", "h\u00e4r\u00b7te\u00b7re", "Wort", ",", "das", "du", "ver\u00b7schwei\u00b7gest", ",", "mir", "auch", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,", "PPER", "ADV", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.7": {"text": "Aber, ach! das Wasser entst\u00fcrzt der Steile des Felsens", "tokens": ["A\u00b7ber", ",", "ach", "!", "das", "Was\u00b7ser", "ent\u00b7st\u00fcrzt", "der", "Stei\u00b7le", "des", "Fel\u00b7sens"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "$,", "ITJ", "$.", "ART", "NN", "VVFIN", "ART", "NN", "ART", "NN"], "meter": "+-+-+--+-+--+-", "measure": "hexameter"}, "line.8": {"text": "Rasch, und die Welle des Bachs halten Ges\u00e4nge nicht auf.", "tokens": ["Rasch", ",", "und", "die", "Wel\u00b7le", "des", "Bachs", "hal\u00b7ten", "Ge\u00b7s\u00e4n\u00b7ge", "nicht", "auf", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "KON", "ART", "NN", "ART", "NN", "ADJA", "NN", "PTKNEG", "PTKVZ", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.9": {"text": "Rast nicht unaufhaltsam der Sturm? und w\u00e4lzet die Sonne", "tokens": ["Rast", "nicht", "un\u00b7auf\u00b7halt\u00b7sam", "der", "Sturm", "?", "und", "w\u00e4l\u00b7zet", "die", "Son\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PTKNEG", "ADJD", "ART", "NN", "$.", "KON", "VVFIN", "ART", "NN"], "meter": "+-+-+--+-+--+-", "measure": "hexameter"}, "line.10": {"text": "Sich, von dem Gipfel des Tags, nicht in die Wellen hinab?", "tokens": ["Sich", ",", "von", "dem", "Gip\u00b7fel", "des", "Tags", ",", "nicht", "in", "die", "Wel\u00b7len", "hin\u00b7ab", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "$,", "APPR", "ART", "NN", "ART", "NN", "$,", "PTKNEG", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.11": {"text": "Und so spricht mir rings die Natur: \u00bbAuch du bist, Amyntas,", "tokens": ["Und", "so", "spricht", "mir", "rings", "die", "Na\u00b7tur", ":", "\u00bb", "Auch", "du", "bist", ",", "A\u00b7myn\u00b7tas", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "$.", "$(", "ADV", "PPER", "VAFIN", "$,", "NE", "$,"], "meter": "--+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "Unter das strenge Gesetz ehrner Gewalten gebeugt.\u00ab", "tokens": ["Un\u00b7ter", "das", "stren\u00b7ge", "Ge\u00b7setz", "ehr\u00b7ner", "Ge\u00b7wal\u00b7ten", "ge\u00b7beugt", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ADJA", "NN", "VVPP", "$.", "$("], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}, "line.13": {"text": "Runzle die Stirne nicht tiefer, mein Freund, und h\u00f6re gef\u00e4llig,", "tokens": ["Runz\u00b7le", "die", "Stir\u00b7ne", "nicht", "tie\u00b7fer", ",", "mein", "Freund", ",", "und", "h\u00f6\u00b7re", "ge\u00b7f\u00e4l\u00b7lig", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKNEG", "ADJD", "$,", "PPOSAT", "NN", "$,", "KON", "VVFIN", "ADJD", "$,"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.14": {"text": "Was mich gestern ein Baum dort an dem Bache gelehrt.", "tokens": ["Was", "mich", "ge\u00b7stern", "ein", "Baum", "dort", "an", "dem", "Ba\u00b7che", "ge\u00b7lehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "ART", "NN", "ADV", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.15": {"text": "Wenig \u00c4pfel tr\u00e4gt er mir nur, der sonst so beladne;", "tokens": ["We\u00b7nig", "\u00c4p\u00b7fel", "tr\u00e4gt", "er", "mir", "nur", ",", "der", "sonst", "so", "be\u00b7lad\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "PPER", "ADV", "$,", "PRELS", "ADV", "ADV", "ADJA", "$."], "meter": "+-+-+--+-+--+-", "measure": "hexameter"}, "line.16": {"text": "Sieh, der Efeu ist schuld, der ihn gewaltig umgibt.", "tokens": ["Sieh", ",", "der", "E\u00b7feu", "ist", "schuld", ",", "der", "ihn", "ge\u00b7wal\u00b7tig", "um\u00b7gibt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "NN", "VAFIN", "ADJD", "$,", "PRELS", "PPER", "ADJD", "VVFIN", "$."], "meter": "+--+-+-+-+--+", "measure": "iambic.hexa.invert"}, "line.17": {"text": "Und ich fa\u00dfte das Messer, das krummgebogene, scharfe,", "tokens": ["Und", "ich", "fa\u00df\u00b7te", "das", "Mes\u00b7ser", ",", "das", "krumm\u00b7ge\u00b7bo\u00b7ge\u00b7ne", ",", "schar\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "$,", "ART", "ADJA", "$,", "VVFIN", "$,"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.18": {"text": "Trennte schneidend und ri\u00df Ranke nach Ranken herab;", "tokens": ["Trenn\u00b7te", "schnei\u00b7dend", "und", "ri\u00df", "Ran\u00b7ke", "nach", "Ran\u00b7ken", "her\u00b7ab", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "KON", "VVFIN", "NN", "APPR", "NN", "ADV", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.19": {"text": "Aber ich schauderte gleich, als tief erseufzend und kl\u00e4glich", "tokens": ["A\u00b7ber", "ich", "schau\u00b7der\u00b7te", "gleich", ",", "als", "tief", "er\u00b7seuf\u00b7zend", "und", "kl\u00e4g\u00b7lich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ADV", "$,", "KOUS", "ADJD", "VVPP", "KON", "ADJD"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.20": {"text": "Aus den Wipfeln zu mir lispelnde Klage sich go\u00df:", "tokens": ["Aus", "den", "Wip\u00b7feln", "zu", "mir", "lis\u00b7peln\u00b7de", "Kla\u00b7ge", "sich", "go\u00df", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "PPER", "ADJA", "NN", "PRF", "VVFIN", "$."], "meter": "--+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.21": {"text": "\u00bbo verletze mich nicht! den treuen Gartengenossen,", "tokens": ["\u00bb", "o", "ver\u00b7let\u00b7ze", "mich", "nicht", "!", "den", "treu\u00b7en", "Gar\u00b7ten\u00b7ge\u00b7nos\u00b7sen", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "FM", "VVFIN", "PPER", "PTKNEG", "$.", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.22": {"text": "Dem du, als Knabe, so fr\u00fch, manche Gen\u00fcsse verdankt.", "tokens": ["Dem", "du", ",", "als", "Kna\u00b7be", ",", "so", "fr\u00fch", ",", "man\u00b7che", "Ge\u00b7n\u00fcs\u00b7se", "ver\u00b7dankt", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NE", "$,", "KOUS", "NN", "$,", "ADV", "ADJD", "$,", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+--++--+--+", "measure": "iambic.hexa.relaxed"}, "line.23": {"text": "O verletze mich nicht! du rei\u00dfest mit diesem Geflechte,", "tokens": ["O", "ver\u00b7let\u00b7ze", "mich", "nicht", "!", "du", "rei\u00b7\u00dfest", "mit", "die\u00b7sem", "Ge\u00b7flech\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "PTKNEG", "$.", "PPER", "VVFIN", "APPR", "PDAT", "NN", "$,"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.24": {"text": "Das du gewaltig zerst\u00f6rst, grausam das Leben mir aus.", "tokens": ["Das", "du", "ge\u00b7wal\u00b7tig", "zer\u00b7st\u00f6rst", ",", "grau\u00b7sam", "das", "Le\u00b7ben", "mir", "aus", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "VVPP", "$,", "ADJD", "ART", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+---+--+--+", "measure": "iambic.penta.relaxed"}, "line.25": {"text": "Hab ich nicht selbst sie gen\u00e4hrt und sanft sie herauf mir erzogen?", "tokens": ["Hab", "ich", "nicht", "selbst", "sie", "ge\u00b7n\u00e4hrt", "und", "sanft", "sie", "her\u00b7auf", "mir", "er\u00b7zo\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PTKNEG", "ADV", "PPER", "VVPP", "KON", "VVFIN", "PPER", "ADV", "PPER", "VVPP", "$."], "meter": "+-+-+-+-+--+--+-", "measure": "trochaic.septa.relaxed"}, "line.26": {"text": "Ist wie mein eigenes Laub nicht mir das ihre verwandt?", "tokens": ["Ist", "wie", "mein", "ei\u00b7ge\u00b7nes", "Laub", "nicht", "mir", "das", "ih\u00b7re", "ver\u00b7wandt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "KOKOM", "PPOSAT", "ADJA", "NN", "PTKNEG", "PPER", "PDS", "PPOSAT", "VVPP", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.27": {"text": "Soll ich nicht lieben die Pflanze, die, meiner einzig bed\u00fcrftig,", "tokens": ["Soll", "ich", "nicht", "lie\u00b7ben", "die", "Pflan\u00b7ze", ",", "die", ",", "mei\u00b7ner", "ein\u00b7zig", "be\u00b7d\u00fcrf\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "VVFIN", "ART", "NN", "$,", "PRELS", "$,", "PPOSAT", "ADJD", "ADJD", "$,"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.28": {"text": "Still mit begieriger Kraft mir um die Seite sich schlingt?", "tokens": ["Still", "mit", "be\u00b7gie\u00b7ri\u00b7ger", "Kraft", "mir", "um", "die", "Sei\u00b7te", "sich", "schlingt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ADJA", "NN", "PPER", "APPR", "ART", "NN", "PRF", "VVFIN", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.29": {"text": "Tausend Ranken wurzelten an, mit tausend und tausend", "tokens": ["Tau\u00b7send", "Ran\u00b7ken", "wur\u00b7zel\u00b7ten", "an", ",", "mit", "tau\u00b7send", "und", "tau\u00b7send"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["CARD", "NN", "VVFIN", "PTKVZ", "$,", "APPR", "CARD", "KON", "CARD"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.30": {"text": "Fasern senket sie fest mir in das Leben sich ein.", "tokens": ["Fa\u00b7sern", "sen\u00b7ket", "sie", "fest", "mir", "in", "das", "Le\u00b7ben", "sich", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "PRF", "PTKVZ", "$."], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.31": {"text": "Nahrung nimmt sie von mir; was ich bed\u00fcrfte, genie\u00dft sie,", "tokens": ["Nah\u00b7rung", "nimmt", "sie", "von", "mir", ";", "was", "ich", "be\u00b7d\u00fcrf\u00b7te", ",", "ge\u00b7nie\u00dft", "sie", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPR", "PPER", "$.", "PWS", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "$,"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.32": {"text": "Und so saugt sie das Mark, sauget die Seele mir aus.", "tokens": ["Und", "so", "saugt", "sie", "das", "Mark", ",", "sau\u00b7get", "die", "See\u00b7le", "mir", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ART", "NN", "$,", "VVFIN", "ART", "NN", "PPER", "PTKVZ", "$."], "meter": "--+--++--+--+", "measure": "anapaest.di.plus"}, "line.33": {"text": "Nur vergebens n\u00e4hr ich mich noch; die gewaltige Wurzel", "tokens": ["Nur", "ver\u00b7ge\u00b7bens", "n\u00e4hr", "ich", "mich", "noch", ";", "die", "ge\u00b7wal\u00b7ti\u00b7ge", "Wur\u00b7zel"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "PRF", "ADV", "$.", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.34": {"text": "Sendet lebendigen Safts, ach ! nur die H\u00e4lfte hinauf.", "tokens": ["Sen\u00b7det", "le\u00b7ben\u00b7di\u00b7gen", "Safts", ",", "ach", "!", "nur", "die", "H\u00e4lf\u00b7te", "hin\u00b7auf", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "$,", "ITJ", "$.", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.35": {"text": "Denn der gef\u00e4hrliche Gast, der geliebteste, ma\u00dfet behende", "tokens": ["Denn", "der", "ge\u00b7f\u00e4hr\u00b7li\u00b7che", "Gast", ",", "der", "ge\u00b7lieb\u00b7tes\u00b7te", ",", "ma\u00b7\u00dfet", "be\u00b7hen\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "$,", "ART", "ADJA", "$,", "VVFIN", "ADJA"], "meter": "-+-+--+--+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.36": {"text": "Unterweges die Kraft herbstlicher Fr\u00fcchte sich an.", "tokens": ["Un\u00b7ter\u00b7we\u00b7ges", "die", "Kraft", "herbst\u00b7li\u00b7cher", "Fr\u00fcch\u00b7te", "sich", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADJA", "NN", "PRF", "PTKVZ", "$."], "meter": "+-+--++--+--+", "measure": "trochaic.hexa.relaxed"}, "line.37": {"text": "Nichts gelangt zur Krone hinauf, die \u00e4u\u00dfersten Wipfel", "tokens": ["Nichts", "ge\u00b7langt", "zur", "Kro\u00b7ne", "hin\u00b7auf", ",", "die", "\u00e4u\u00b7\u00dfers\u00b7ten", "Wip\u00b7fel"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PIS", "VVPP", "APPRART", "NN", "PTKVZ", "$,", "ART", "ADJA", "NN"], "meter": "+-+-+--+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.38": {"text": "Dorren, es dorret der Ast \u00fcber dem Bache schon hin.", "tokens": ["Dor\u00b7ren", ",", "es", "dor\u00b7ret", "der", "Ast", "\u00fc\u00b7ber", "dem", "Ba\u00b7che", "schon", "hin", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "ADV", "PTKVZ", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.39": {"text": "Ja, die Verr\u00e4terin ist's! sie schmeichelt mir Leben und G\u00fcter,", "tokens": ["Ja", ",", "die", "Ver\u00b7r\u00e4\u00b7te\u00b7rin", "ist's", "!", "sie", "schmei\u00b7chelt", "mir", "Le\u00b7ben", "und", "G\u00fc\u00b7ter", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ART", "NN", "VAFIN", "$.", "PPER", "VVFIN", "PPER", "NN", "KON", "NN", "$,"], "meter": "-+-+-+--+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.40": {"text": "Schmeichelt die strebende Kraft, schmeichelt die Hoffnung mir ab.", "tokens": ["Schmei\u00b7chelt", "die", "stre\u00b7ben\u00b7de", "Kraft", ",", "schmei\u00b7chelt", "die", "Hoff\u00b7nung", "mir", "ab", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,", "VVFIN", "ART", "NN", "PPER", "PTKVZ", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.41": {"text": "Sie nur f\u00fchl ich, nur sie, die umschlingende, freue der Fesseln,", "tokens": ["Sie", "nur", "f\u00fchl", "ich", ",", "nur", "sie", ",", "die", "um\u00b7schlin\u00b7gen\u00b7de", ",", "freu\u00b7e", "der", "Fes\u00b7seln", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PPER", "$,", "ADV", "PPER", "$,", "ART", "ADJA", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "--+-+-+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.42": {"text": "Freue des t\u00f6tenden Schmucks, fremder Umlaubung mich nur.\u00ab", "tokens": ["Freu\u00b7e", "des", "t\u00f6\u00b7ten\u00b7den", "Schmucks", ",", "frem\u00b7der", "Um\u00b7lau\u00b7bung", "mich", "nur", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,", "ADJA", "NN", "PPER", "ADV", "$.", "$("], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.43": {"text": "Halte das Messer zur\u00fcck! o Nikias, schone den Armen,", "tokens": ["Hal\u00b7te", "das", "Mes\u00b7ser", "zu\u00b7r\u00fcck", "!", "o", "Ni\u00b7ki\u00b7as", ",", "scho\u00b7ne", "den", "Ar\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "PTKVZ", "$.", "FM", "NE", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.44": {"text": "Der sich in liebender Lust, willig gezwungen, verzehrt!", "tokens": ["Der", "sich", "in", "lie\u00b7ben\u00b7der", "Lust", ",", "wil\u00b7lig", "ge\u00b7zwun\u00b7gen", ",", "ver\u00b7zehrt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "PRF", "APPR", "ADJA", "NN", "$,", "ADJD", "VVPP", "$,", "VVPP", "$."], "meter": "-+-+--++--+--+", "measure": "iambic.hexa.relaxed"}, "line.45": {"text": "S\u00fc\u00df ist jede Verschwendung; o la\u00df mich der sch\u00f6nsten genie\u00dfen!", "tokens": ["S\u00fc\u00df", "ist", "je\u00b7de", "Ver\u00b7schwen\u00b7dung", ";", "o", "la\u00df", "mich", "der", "sch\u00f6ns\u00b7ten", "ge\u00b7nie\u00b7\u00dfen", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PIAT", "NN", "$.", "FM", "VVIMP", "PPER", "ART", "ADJA", "VVINF", "$."], "meter": "+-+--+-+-+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.46": {"text": "Wer sich der Liebe vertraut, h\u00e4lt er sein Leben zu Rat?", "tokens": ["Wer", "sich", "der", "Lie\u00b7be", "ver\u00b7traut", ",", "h\u00e4lt", "er", "sein", "Le\u00b7ben", "zu", "Rat", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "ART", "NN", "ADJD", "$,", "VVFIN", "PPER", "PPOSAT", "NN", "APPR", "NN", "$."], "meter": "-+-+--+---+--+", "measure": "iambic.penta.relaxed"}}}}}