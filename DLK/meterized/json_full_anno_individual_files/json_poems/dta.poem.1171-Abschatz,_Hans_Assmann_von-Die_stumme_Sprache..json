{"dta.poem.1171": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Die stumme Sprache.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn ich nicht reden darff/ nimm meine Seufftzer hin;", "tokens": ["Wenn", "ich", "nicht", "re\u00b7den", "darff", "/", "nimm", "mei\u00b7ne", "Seufft\u00b7zer", "hin", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "VVINF", "VMFIN", "$(", "VVIMP", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sie werden dir in ihrer Sprache sagen:", "tokens": ["Sie", "wer\u00b7den", "dir", "in", "ih\u00b7rer", "Spra\u00b7che", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wenn Gl\u00fcck und Himmel h\u00e4tten meinen Sinn/", "tokens": ["Wenn", "Gl\u00fcck", "und", "Him\u00b7mel", "h\u00e4t\u00b7ten", "mei\u00b7nen", "Sinn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "VAFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ich wolte dir mehr Opffer tragen.", "tokens": ["Ich", "wol\u00b7te", "dir", "mehr", "Opf\u00b7fer", "tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}