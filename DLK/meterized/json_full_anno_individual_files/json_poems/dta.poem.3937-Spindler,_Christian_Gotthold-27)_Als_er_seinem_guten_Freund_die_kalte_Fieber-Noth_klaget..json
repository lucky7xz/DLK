{"dta.poem.3937": {"metadata": {"author": {"name": "Spindler, Christian Gotthold", "birth": "N.A.", "death": "N.A."}, "title": "27) Als er seinem guten Freund die kalte  \n Fieber-Noth klaget.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-20581-9", "language": ["de:0.99"], "booktitle": "Spindler, Christian Gotthold: Unschuldige Jugend-Fr\u00fcchte. Leipzig, 1745."}, "poem": {"stanza.1": {"line.1": {"text": "Strenger Himmel! hat dein Wesen", "tokens": ["Stren\u00b7ger", "Him\u00b7mel", "!", "hat", "dein", "We\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$.", "VAFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mir nichts anders auserlesen", "tokens": ["Mir", "nichts", "an\u00b7ders", "au\u00b7ser\u00b7le\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "PIS", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Als die kalte Fieber-Last?", "tokens": ["Als", "die", "kal\u00b7te", "Fie\u00b7ber\u00b7Last", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Die den schwachen Leib umfa\u00dft.", "tokens": ["Die", "den", "schwa\u00b7chen", "Leib", "um\u00b7fa\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Es versch\u00e4umen alle S\u00e4ffte,", "tokens": ["Es", "ver\u00b7sch\u00e4u\u00b7men", "al\u00b7le", "S\u00e4ff\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Aller Muth, und alle Kr\u00e4ffte,", "tokens": ["Al\u00b7ler", "Muth", ",", "und", "al\u00b7le", "Kr\u00e4ff\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "KON", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und mein sonst so muntrer Sinn", "tokens": ["Und", "mein", "sonst", "so", "mun\u00b7trer", "Sinn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADV", "ADV", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Ist bey diesem Zustand hin.", "tokens": ["Ist", "bey", "die\u00b7sem", "Zu\u00b7stand", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PDAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Frost und Hitze sind die Plagen,", "tokens": ["Frost", "und", "Hit\u00b7ze", "sind", "die", "Pla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die den armen C\u00f6rper nagen,", "tokens": ["Die", "den", "ar\u00b7men", "C\u00f6r\u00b7per", "na\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Diese wechseln immer ab;", "tokens": ["Die\u00b7se", "wech\u00b7seln", "im\u00b7mer", "ab", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Als die Stuffe zu dem Grab.", "tokens": ["Als", "die", "Stuf\u00b7fe", "zu", "dem", "Grab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Strenges Schicksal, dein Begehren", "tokens": ["Stren\u00b7ges", "Schick\u00b7sal", ",", "dein", "Be\u00b7geh\u00b7ren"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$,", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Fordert ja ein bang Verzehren,", "tokens": ["For\u00b7dert", "ja", "ein", "bang", "Ver\u00b7zeh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Da mich dieses Fieber qu\u00e4lt/", "tokens": ["Da", "mich", "die\u00b7ses", "Fie\u00b7ber", "qu\u00e4lt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PDAT", "NN", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Das den C\u00f6rper fast entseelt.", "tokens": ["Das", "den", "C\u00f6r\u00b7per", "fast", "ent\u00b7seelt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Packe dich einmahl, o Fieber!", "tokens": ["Pa\u00b7cke", "dich", "ein\u00b7mahl", ",", "o", "Fie\u00b7ber", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "FM", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bruder! w\u00e4r es nur vor\u00fcber,", "tokens": ["Bru\u00b7der", "!", "w\u00e4r", "es", "nur", "vor\u00b7\u00fc\u00b7ber", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VAFIN", "PPER", "ADV", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Mir ist w\u00fcrcklich angst und bang,", "tokens": ["Mir", "ist", "w\u00fcrck\u00b7lich", "angst", "und", "bang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "VVPP", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Denn es dauret allzulang.", "tokens": ["Denn", "es", "dau\u00b7ret", "all\u00b7zu\u00b7lang", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Hilff mir doch in n\u00e4chsten Tagen", "tokens": ["Hilff", "mir", "doch", "in", "n\u00e4chs\u00b7ten", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Meines Fiebers Last ertragen,", "tokens": ["Mei\u00b7nes", "Fie\u00b7bers", "Last", "er\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Sprich doch balde bey mir ein,", "tokens": ["Sprich", "doch", "bal\u00b7de", "bey", "mir", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "ADV", "APPR", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Vielleicht wird es besser seyn.", "tokens": ["Viel\u00b7leicht", "wird", "es", "bes\u00b7ser", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "VAINF", "$."], "meter": "++--+-+", "measure": "trochaic.tetra.relaxed"}}}}}