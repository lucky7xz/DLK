{"dta.poem.8465": {"metadata": {"author": {"name": "H\u00f6lty, Ludwig Christoph Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Trinklied .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1783", "urn": "urn:nbn:de:kobv:b4-200905192568", "language": ["de:0.99"], "booktitle": "H\u00f6lty, Ludwig Christoph Heinrich: Gedichte. Hamburg, 1783."}, "poem": {"stanza.1": {"line.1": {"text": "Ein Leben, wie im Paradies,", "tokens": ["Ein", "Le\u00b7ben", ",", "wie", "im", "Pa\u00b7ra\u00b7dies", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gew\u00e4hrt uns Vater Rhein.", "tokens": ["Ge\u00b7w\u00e4hrt", "uns", "Va\u00b7ter", "Rhein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "PPER", "NN", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich geb' es zu, ein Kuss ist s\u00fcss;", "tokens": ["Ich", "geb'", "es", "zu", ",", "ein", "Kuss", "ist", "s\u00fcss", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$,", "ART", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Doch s\u00fcsser ist der Wein.", "tokens": ["Doch", "s\u00fcs\u00b7ser", "ist", "der", "Wein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ich bin so fr\u00f6hlich, wie ein Reh,", "tokens": ["Ich", "bin", "so", "fr\u00f6h\u00b7lich", ",", "wie", "ein", "Reh", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$,", "PWAV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das um die Quelle tanzt,", "tokens": ["Das", "um", "die", "Quel\u00b7le", "tanzt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wenn ich den lieben Schenktisch seh,", "tokens": ["Wenn", "ich", "den", "lie\u00b7ben", "Schenk\u00b7tisch", "seh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und Gl\u00e4ser drauf gepflanzt.", "tokens": ["Und", "Gl\u00e4\u00b7ser", "drauf", "ge\u00b7pflanzt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PAV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Was k\u00fcmmert mich die ganze Welt,", "tokens": ["Was", "k\u00fcm\u00b7mert", "mich", "die", "gan\u00b7ze", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wenns liebe Gl\u00e4slein winkt,", "tokens": ["Wenns", "lie\u00b7be", "Gl\u00e4s\u00b7lein", "winkt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und Traubensaft, der mir gef\u00e4llt,", "tokens": ["Und", "Trau\u00b7ben\u00b7saft", ",", "der", "mir", "ge\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "An meiner Lippe blinkt ?", "tokens": ["An", "mei\u00b7ner", "Lip\u00b7pe", "blinkt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Dann trink' ich, wie ein G\u00f6tterkind,", "tokens": ["Dann", "trink'", "ich", ",", "wie", "ein", "G\u00f6t\u00b7ter\u00b7kind", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PWAV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die volle Flasche leer,", "tokens": ["Die", "vol\u00b7le", "Fla\u00b7sche", "leer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Dass Glut mir durch die Adern rinnt,", "tokens": ["Dass", "Glut", "mir", "durch", "die", "A\u00b7dern", "rinnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und tauml', und fodre mehr.", "tokens": ["Und", "tauml'", ",", "und", "fod\u00b7re", "mehr", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KON", "VVFIN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Die Erde w\u00e4r' ein Jammerthal,", "tokens": ["Die", "Er\u00b7de", "w\u00e4r'", "ein", "Jam\u00b7mer\u00b7thal", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Voll Grillenfang und Gicht,", "tokens": ["Voll", "Gril\u00b7len\u00b7fang", "und", "Gicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "W\u00fcchs' uns zur Lindrung unsrer Qual", "tokens": ["W\u00fcchs'", "uns", "zur", "Lind\u00b7rung", "uns\u00b7rer", "Qual"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "APPRART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der edle Rheinwein nicht.", "tokens": ["Der", "ed\u00b7le", "Rhein\u00b7wein", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der hebt den Bettler auf den Thron,", "tokens": ["Der", "hebt", "den", "Bett\u00b7ler", "auf", "den", "Thron", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Schafft Erd' und Himmel um,", "tokens": ["Schafft", "Erd'", "und", "Him\u00b7mel", "um", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "KON", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und zaubert jeden Erdensohn", "tokens": ["Und", "zau\u00b7bert", "je\u00b7den", "Er\u00b7den\u00b7sohn"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Stracks in Elisium.", "tokens": ["Stracks", "in", "E\u00b7li\u00b7si\u00b7um", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NE", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.4": {"line.1": {"text": "Er ist die wahre Panacee,", "tokens": ["Er", "ist", "die", "wah\u00b7re", "Pa\u00b7na\u00b7cee", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Verj\u00fcngt des Alten Blut,", "tokens": ["Ver\u00b7j\u00fcngt", "des", "Al\u00b7ten", "Blut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Verscheuchet Hirn- und Magenweh,", "tokens": ["Ver\u00b7scheu\u00b7chet", "Hirn", "und", "Ma\u00b7gen\u00b7weh", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "TRUNC", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und was er weiter thut.", "tokens": ["Und", "was", "er", "wei\u00b7ter", "thut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Drum lebe das gelobte Land,", "tokens": ["Drum", "le\u00b7be", "das", "ge\u00b7lob\u00b7te", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das uns den Wein erzog!", "tokens": ["Das", "uns", "den", "Wein", "er\u00b7zog", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Der Winzer, der ihn pflanzt' und band,", "tokens": ["Der", "Win\u00b7zer", ",", "der", "ihn", "pflanzt'", "und", "band", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der Winzer lebe hoch!", "tokens": ["Der", "Win\u00b7zer", "le\u00b7be", "hoch", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Und jeder sch\u00f6nen Winzerin,", "tokens": ["Und", "je\u00b7der", "sch\u00f6\u00b7nen", "Win\u00b7ze\u00b7rin", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die uns die Trauben las,", "tokens": ["Die", "uns", "die", "Trau\u00b7ben", "las", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Weih' ich als meiner K\u00f6nigin", "tokens": ["Weih'", "ich", "als", "mei\u00b7ner", "K\u00f6\u00b7ni\u00b7gin"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "KOUS", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein volles Deckelglas!", "tokens": ["Ein", "vol\u00b7les", "De\u00b7ckel\u00b7glas", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Es lebe jeder deutsche Mann,", "tokens": ["Es", "le\u00b7be", "je\u00b7der", "deut\u00b7sche", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der seinen Rheinwein trinkt,", "tokens": ["Der", "sei\u00b7nen", "Rhein\u00b7wein", "trinkt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "So lang' ers Kelchglas halten kann,", "tokens": ["So", "lang'", "ers", "Kelchg\u00b7las", "hal\u00b7ten", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJA", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und dann zu Boden sinkt!", "tokens": ["Und", "dann", "zu", "Bo\u00b7den", "sinkt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}