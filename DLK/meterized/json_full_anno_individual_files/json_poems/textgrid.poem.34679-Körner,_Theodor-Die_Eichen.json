ochaic.penta"}}, "stanza.3": {"line.1": {"text": "Und ihr ", "tokens": ["Und", "ihr"], "token_info": ["word", "word"], "pos": ["KON", "PPER"], "meter": "+-", "measure": "trochaic.single"}, "line.2": {"text": "Gr\u00fcnt ihr frisch und k\u00fchn mit starkem Mut.", "tokens": ["Gr\u00fcnt", "ihr", "frisch", "und", "k\u00fchn", "mit", "star\u00b7kem", "Mut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "KON", "ADJD", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Wohl kein Pilger wird vor\u00fcberwallen,", "tokens": ["Wohl", "kein", "Pil\u00b7ger", "wird", "vor\u00b7\u00fc\u00b7berw\u00b7al\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "VAFIN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Der in euerm Schatten nicht geruht.", "tokens": ["Der", "in", "eu\u00b7erm", "Schat\u00b7ten", "nicht", "ge\u00b7ruht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPOSAT", "NN", "PTKNEG", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Und wenn herbstlich eure Bl\u00e4tter fallen:", "tokens": ["Und", "wenn", "herbst\u00b7lich", "eu\u00b7re", "Bl\u00e4t\u00b7ter", "fal\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADV", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Tot auch sind sie euch ein k\u00f6stlich Gut,", "tokens": ["Tot", "auch", "sind", "sie", "euch", "ein", "k\u00f6st\u00b7lich", "Gut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VAFIN", "PPER", "PPER", "ART", "ADJD", "ADJD", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Denn verwesend werden eure Kinder", "tokens": ["Denn", "ver\u00b7we\u00b7send", "wer\u00b7den", "eu\u00b7re", "Kin\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVPP", "VAFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Eurer n\u00e4chsten Fr\u00fchlingspracht Begr\u00fcnder.", "tokens": ["Eu\u00b7rer", "n\u00e4chs\u00b7ten", "Fr\u00fch\u00b7lings\u00b7pracht", "Be\u00b7gr\u00fcn\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Sch\u00f6nes Bild von alter deutscher Treue,", "tokens": ["Sch\u00f6\u00b7nes", "Bild", "von", "al\u00b7ter", "deut\u00b7scher", "Treu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wie sie bess're Zeiten angeschaut,", "tokens": ["Wie", "sie", "bess'\u00b7re", "Zei\u00b7ten", "an\u00b7ge\u00b7schaut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Wo in freudig k\u00fchner Todesweihe", "tokens": ["Wo", "in", "freu\u00b7dig", "k\u00fch\u00b7ner", "To\u00b7des\u00b7wei\u00b7he"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ADJD", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "B\u00fcrger ihre Staaten festgebaut!", "tokens": ["B\u00fcr\u00b7ger", "ih\u00b7re", "Staa\u00b7ten", "fest\u00b7ge\u00b7baut", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Ach was hilft's, da\u00df