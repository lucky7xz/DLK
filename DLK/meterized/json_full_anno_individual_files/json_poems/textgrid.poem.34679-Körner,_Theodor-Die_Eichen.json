{"textgrid.poem.34679": {"metadata": {"author": {"name": "K\u00f6rner, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Die Eichen", "genre": "verse", "period": "N.A.", "pub_year": 1811, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Abend wird's, des Tages Stimmen schweigen,", "tokens": ["A\u00b7bend", "wird's", ",", "des", "Ta\u00b7ges", "Stim\u00b7men", "schwei\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "$,", "ART", "NN", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "R\u00f6ter strahlt der Sonne letztes Gl\u00fch'n;", "tokens": ["R\u00f6\u00b7ter", "strahlt", "der", "Son\u00b7ne", "letz\u00b7tes", "Gl\u00fch'n", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Und hier sitz' ich unter euren Zweigen,", "tokens": ["Und", "hier", "sitz'", "ich", "un\u00b7ter", "eu\u00b7ren", "Zwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und das Herz ist mir so voll, so k\u00fchn!", "tokens": ["Und", "das", "Herz", "ist", "mir", "so", "voll", ",", "so", "k\u00fchn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "PPER", "ADV", "ADJD", "$,", "ADV", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Alter Zeiten alte treue Zeugen,", "tokens": ["Al\u00b7ter", "Zei\u00b7ten", "al\u00b7te", "treu\u00b7e", "Zeu\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Schm\u00fcckt euch doch des Lebens frisches Gr\u00fcn,", "tokens": ["Schm\u00fcckt", "euch", "doch", "des", "Le\u00b7bens", "fri\u00b7sches", "Gr\u00fcn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Und der Vorwelt kr\u00e4ftige Gestalten", "tokens": ["Und", "der", "Vor\u00b7welt", "kr\u00e4f\u00b7ti\u00b7ge", "Ge\u00b7stal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Sind uns noch in eurer Pracht enthalten.", "tokens": ["Sind", "uns", "noch", "in", "eu\u00b7rer", "Pracht", "ent\u00b7hal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Viel des Edlen hat die Zeit zertr\u00fcmmert,", "tokens": ["Viel", "des", "Ed\u00b7len", "hat", "die", "Zeit", "zer\u00b7tr\u00fcm\u00b7mert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Viel des Sch\u00f6nen starb den fr\u00fchen Tod,", "tokens": ["Viel", "des", "Sch\u00f6\u00b7nen", "starb", "den", "fr\u00fc\u00b7hen", "Tod", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Durch die reichen Bl\u00e4tterkr\u00e4nze schimmert", "tokens": ["Durch", "die", "