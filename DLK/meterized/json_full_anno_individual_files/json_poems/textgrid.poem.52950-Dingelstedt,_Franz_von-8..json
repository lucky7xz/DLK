{"textgrid.poem.52950": {"metadata": {"author": {"name": "Dingelstedt, Franz von", "birth": "N.A.", "death": "N.A."}, "title": "8.", "genre": "verse", "period": "N.A.", "pub_year": 1847, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mir tr\u00e4umte letzte Nacht: Wir beide sa\u00dfen", "tokens": ["Mir", "tr\u00e4um\u00b7te", "letz\u00b7te", "Nacht", ":", "Wir", "bei\u00b7de", "sa\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "$.", "PPER", "PIS", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Hier unter Deines Vaters Hochzeitslinde,", "tokens": ["Hier", "un\u00b7ter", "Dei\u00b7nes", "Va\u00b7ters", "Hoch\u00b7zeits\u00b7lin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So wie wir, Hand in Hand, zu sitzen pflegen.", "tokens": ["So", "wie", "wir", ",", "Hand", "in", "Hand", ",", "zu", "sit\u00b7zen", "pfle\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "PPER", "$,", "NN", "APPR", "NN", "$,", "PTKZU", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zu Deinen F\u00fc\u00dfen spielte auf dem Rasen", "tokens": ["Zu", "Dei\u00b7nen", "F\u00fc\u00b7\u00dfen", "spiel\u00b7te", "auf", "dem", "Ra\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ein Lamm mit einem blondgelockten Kinde,", "tokens": ["Ein", "Lamm", "mit", "ei\u00b7nem", "blond\u00b7ge\u00b7lock\u00b7ten", "Kin\u00b7de", ","], "token_info": [