{"dta.poem.15658": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "91.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Das alte Sprichwort sagt: Nichts unterm Sonnenstral", "tokens": ["Das", "al\u00b7te", "Sprich\u00b7wort", "sagt", ":", "Nichts", "un\u00b7term", "Son\u00b7nen\u00b7stral"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$.", "PIS", "APPRART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Kommt Neues, das nicht dagewesen schon einmal.", "tokens": ["Kommt", "Neu\u00b7es", ",", "das", "nicht", "da\u00b7ge\u00b7we\u00b7sen", "schon", "ein\u00b7mal", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "NN", "$,", "PRELS", "PTKNEG", "PAV", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Umkehren l\u00e4\u00dft sichs auch: Nichts Altes kehret wieder,", "tokens": ["Um\u00b7keh\u00b7ren", "l\u00e4\u00dft", "sichs", "auch", ":", "Nichts", "Al\u00b7tes", "keh\u00b7ret", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PIS", "ADV", "$.", "PIS", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und immer neues Licht scheint von der Sonne nieder.", "tokens": ["Und", "im\u00b7mer", "neu\u00b7es", "Licht", "scheint", "von", "der", "Son\u00b7ne", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Auf anderm Punkt im Raum sind wir an jedem Tag,", "tokens": ["Auf", "an\u00b7derm", "Punkt", "im", "Raum", "sind", "wir", "an", "je\u00b7dem", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "VVFIN", "APPRART", "NN", "VAFIN", "PPER", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil nie in ihrem Lauf die Erde rasten mag.", "tokens": ["Weil", "nie", "in", "ih\u00b7rem", "Lauf", "die", "Er\u00b7de", "ras\u00b7ten", "mag."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["KOUS", "ADV", "APPR", "PPOSAT", "NN", "ART", "NN", "VVFIN", "NE"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Sie kommt auf gleicher Bahn nicht \u00fcbers Jahr zur\u00fcck;", "tokens": ["Sie", "kommt", "auf", "glei\u00b7cher", "Bahn", "nicht", "\u00fc\u00b7bers", "Jahr", "zu\u00b7r\u00fcck", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "PTKNEG", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Denn weiter lief die Sonn' inzwischen auch ein St\u00fcck.", "tokens": ["Denn", "wei\u00b7ter", "lief", "die", "Sonn'", "in\u00b7zwi\u00b7schen", "auch", "ein", "St\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "ADV", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Die Sonn' auch kehret nie im Schwung um ihre Sonne;", "tokens": ["Die", "Sonn'", "auch", "keh\u00b7ret", "nie", "im", "Schwung", "um", "ih\u00b7re", "Son\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "ADV", "APPRART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Denn ihre Sonn' auch kreist inde\u00df um h\u00f6h're Wonne.", "tokens": ["Denn", "ih\u00b7re", "Sonn'", "auch", "kreist", "in\u00b7de\u00df", "um", "h\u00f6h'\u00b7re", "Won\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADV", "VVFIN", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Der Welten h\u00f6chste Wonn' ist solche Liebestreue,", "tokens": ["Der", "Wel\u00b7ten", "h\u00f6chs\u00b7te", "Wonn'", "ist", "sol\u00b7che", "Lie\u00b7be\u00b7streu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VAFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zum Ewigen der Trieb, der ewig alt und neue.", "tokens": ["Zum", "E\u00b7wi\u00b7gen", "der", "Trieb", ",", "der", "e\u00b7wig", "alt", "und", "neu\u00b7e", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "ART", "NN", "$,", "PRELS", "ADJD", "ADJD", "KON", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}