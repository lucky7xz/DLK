{"dta.poem.21504": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Pythagoras.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Gebreitet liegt auf Berg und Auen", "tokens": ["Ge\u00b7brei\u00b7tet", "liegt", "auf", "Berg", "und", "Au\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVPP", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das schattende Gewand der Nacht,", "tokens": ["Das", "schat\u00b7ten\u00b7de", "Ge\u00b7wand", "der", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Auf alle Augen niederthauen", "tokens": ["Auf", "al\u00b7le", "Au\u00b7gen", "nie\u00b7dert\u00b7hau\u00b7en"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Des Traumes Bilder, s\u00fc\u00df und sacht;", "tokens": ["Des", "Trau\u00b7mes", "Bil\u00b7der", ",", "s\u00fc\u00df", "und", "sacht", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADJD", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Nur mich allein will\u2019s nicht umschlingen,", "tokens": ["Nur", "mich", "al\u00b7lein", "will's", "nicht", "um\u00b7schlin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ADV", "VMFIN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Dies selige Sinken in das Nichts:", "tokens": ["Dies", "se\u00b7li\u00b7ge", "Sin\u00b7ken", "in", "das", "Nichts", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Ich will erkennen, will erringen,", "tokens": ["Ich", "will", "er\u00b7ken\u00b7nen", ",", "will", "er\u00b7rin\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "$,", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Erringen einen Strahl des Lichts.", "tokens": ["Er\u00b7rin\u00b7gen", "ei\u00b7nen", "Strahl", "des", "Lichts", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Durchforscht umsonst hab\u2019 ich die Rollen,", "tokens": ["Durch\u00b7forscht", "um\u00b7sonst", "hab'", "ich", "die", "Rol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VAFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die uns der V\u00e4ter Weisheit schrieb,", "tokens": ["Die", "uns", "der", "V\u00e4\u00b7ter", "Weis\u00b7heit", "schrieb", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Umsonst gesucht im Lieben, Grollen", "tokens": ["Um\u00b7sonst", "ge\u00b7sucht", "im", "Lie\u00b7ben", ",", "Grol\u00b7len"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "VVPP", "APPRART", "ADJA", "$,", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Des Menschenherzens tiefsten Trieb,", "tokens": ["Des", "Men\u00b7schen\u00b7her\u00b7zens", "tiefs\u00b7ten", "Trieb", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Umsonst Natur und ihrem Sprossen", "tokens": ["Um\u00b7sonst", "Na\u00b7tur", "und", "ih\u00b7rem", "Spros\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "NN", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Bin ich gefolgt mit Stab und Ma\u00df, \u2014", "tokens": ["Bin", "ich", "ge\u00b7folgt", "mit", "Stab", "und", "Ma\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "APPR", "NN", "KON", "NN", "$,", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die Th\u00fcr zum R\u00e4thsel blieb verschlossen,", "tokens": ["Die", "Th\u00fcr", "zum", "R\u00e4th\u00b7sel", "blieb", "ver\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und wirre Schrift war, was ich las.", "tokens": ["Und", "wir\u00b7re", "Schrift", "war", ",", "was", "ich", "las", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VAFIN", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und was ich jung mit kecken Sinnen,", "tokens": ["Und", "was", "ich", "jung", "mit", "ke\u00b7cken", "Sin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit meinem Herzen, stolz und hei\u00df,", "tokens": ["Mit", "mei\u00b7nem", "Her\u00b7zen", ",", "stolz", "und", "hei\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Im Fluge dachte zu gewinnen,", "tokens": ["Im", "Flu\u00b7ge", "dach\u00b7te", "zu", "ge\u00b7win\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ich fand\u2019s nicht und mein Haar ist wei\u00df,", "tokens": ["Ich", "fan\u00b7d's", "nicht", "und", "mein", "Haar", "ist", "wei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "KON", "PPOSAT", "NN", "VAFIN", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Nicht lang\u2019 mehr wird der Faden w\u00e4hren,", "tokens": ["Nicht", "lang'", "mehr", "wird", "der", "Fa\u00b7den", "w\u00e4h\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ADV", "VAFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Den hastig mir die Moira webt, \u2014", "tokens": ["Den", "has\u00b7tig", "mir", "die", "Moi\u00b7ra", "webt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJD", "PPER", "ART", "NN", "VVFIN", "$,", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Nun lausch\u2019 ich \u00e4ngstlich nach den Sph\u00e4ren,", "tokens": ["Nun", "lausch'", "ich", "\u00e4ngst\u00b7lich", "nach", "den", "Sph\u00e4\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Doch ach, kein Ton, der niederschwebt.", "tokens": ["Doch", "ach", ",", "kein", "Ton", ",", "der", "nie\u00b7der\u00b7schwebt", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "XY", "$,", "PIAT", "NN", "$,", "PRELS", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Und doch, es mu\u00df! Ich darf nicht irren!", "tokens": ["Und", "doch", ",", "es", "mu\u00df", "!", "Ich", "darf", "nicht", "ir\u00b7ren", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "PPER", "VMFIN", "$.", "PPER", "VMFIN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dies Treiben, dieses Lebens Schwall,", "tokens": ["Dies", "Trei\u00b7ben", ",", "die\u00b7ses", "Le\u00b7bens", "Schwall", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "$,", "PDAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der wilde Streit, die b\u00f6sen Wirren,", "tokens": ["Der", "wil\u00b7de", "Streit", ",", "die", "b\u00f6\u00b7sen", "Wir\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Des Scheines Truggespenster all\u2019,", "tokens": ["Des", "Schei\u00b7nes", "Trug\u00b7ge\u00b7spens\u00b7ter", "all'", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PIS", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dies tolle Lachen, bitt\u2019re Weinen,", "tokens": ["Dies", "tol\u00b7le", "La\u00b7chen", ",", "bitt'\u00b7re", "Wei\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Dies Gl\u00fcck, das falsch die Loose theilt:", "tokens": ["Dies", "Gl\u00fcck", ",", "das", "falsch", "die", "Loo\u00b7se", "theilt", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "$,", "PRELS", "ADJD", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Es mu\u00df zu einem Klang sich einen", "tokens": ["Es", "mu\u00df", "zu", "ei\u00b7nem", "Klang", "sich", "ei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "ART", "NN", "PRF", "ART"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Dort oben, wo mein Sehnen weilt.", "tokens": ["Dort", "o\u00b7ben", ",", "wo", "mein", "Seh\u00b7nen", "weilt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "PWAV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Zu einem Klange, voll und pr\u00e4chtig,", "tokens": ["Zu", "ei\u00b7nem", "Klan\u00b7ge", ",", "voll", "und", "pr\u00e4ch\u00b7tig", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der hell den Himmelsraum durchdringt,", "tokens": ["Der", "hell", "den", "Him\u00b7mels\u00b7raum", "durch\u00b7dringt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und alles Ungef\u00fcge m\u00e4chtig", "tokens": ["Und", "al\u00b7les", "Un\u00b7ge\u00b7f\u00fc\u00b7ge", "m\u00e4ch\u00b7tig"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In seinen hohen Zauber zwingt,", "tokens": ["In", "sei\u00b7nen", "ho\u00b7hen", "Zau\u00b7ber", "zwingt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Zu einem Klang, der Alles k\u00fcndet,", "tokens": ["Zu", "ei\u00b7nem", "Klang", ",", "der", "Al\u00b7les", "k\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Was hier der m\u00fcde Geist verlor,", "tokens": ["Was", "hier", "der", "m\u00fc\u00b7de", "Geist", "ver\u00b7lor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "D\u2019rin Rauh und Lieblich sich verb\u00fcndet,", "tokens": ["D'\u00b7rin", "Rauh", "und", "Lieb\u00b7lich", "sich", "ver\u00b7b\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ADJD", "PRF", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Zu f\u00fcllen das entz\u00fcckte Ohr.", "tokens": ["Zu", "f\u00fcl\u00b7len", "das", "ent\u00b7z\u00fcck\u00b7te", "Ohr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Dort oben! Seit mir die Gedanken", "tokens": ["Dort", "o\u00b7ben", "!", "Seit", "mir", "die", "Ge\u00b7dan\u00b7ken"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "$.", "APPR", "PPER", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zum ersten Mal im Hirn gereift,", "tokens": ["Zum", "ers\u00b7ten", "Mal", "im", "Hirn", "ge\u00b7reift", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Lie\u00df ich hinan die Hoffnung ranken", "tokens": ["Lie\u00df", "ich", "hi\u00b7nan", "die", "Hoff\u00b7nung", "ran\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zum Sternenchor, der oben schweift;", "tokens": ["Zum", "Ster\u00b7nen\u00b7chor", ",", "der", "o\u00b7ben", "schweift", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PRELS", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Von oben sollt\u2019 es niedert\u00f6nen,", "tokens": ["Von", "o\u00b7ben", "sollt'", "es", "nie\u00b7der\u00b7t\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Mein unbefriedigt Herz durchgl\u00fch\u2019n,", "tokens": ["Mein", "un\u00b7be\u00b7frie\u00b7digt", "Herz", "durch\u00b7gl\u00fch'n", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und mir im Strahl des ewig Sch\u00f6nen", "tokens": ["Und", "mir", "im", "Strahl", "des", "e\u00b7wig", "Sch\u00f6\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "APPRART", "NN", "ART", "ADJD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Der Erde Leben neu erbl\u00fch\u2019n.", "tokens": ["Der", "Er\u00b7de", "Le\u00b7ben", "neu", "er\u00b7bl\u00fch'", "n."], "token_info": ["word", "word", "word", "word", "word", "abbreviation"], "pos": ["ART", "NN", "NN", "ADJD", "VVFIN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Was ich geliebt, ich hab\u2019s vergessen,", "tokens": ["Was", "ich", "ge\u00b7liebt", ",", "ich", "hab's", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "$,", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Was ich begehrt, ich lie\u00df es lang\u2019,", "tokens": ["Was", "ich", "be\u00b7gehrt", ",", "ich", "lie\u00df", "es", "lang'", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "$,", "PPER", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nur Sehnsucht f\u00fcllt mich unermessen", "tokens": ["Nur", "Sehn\u00b7sucht", "f\u00fcllt", "mich", "un\u00b7er\u00b7mes\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "NN", "VVFIN", "PPER", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nach diesem einen hohen Klang,", "tokens": ["Nach", "die\u00b7sem", "ei\u00b7nen", "ho\u00b7hen", "Klang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Vor\u00fcber lass\u2019 ich alles rauschen,", "tokens": ["Vor\u00b7\u00fc\u00b7ber", "lass'", "ich", "al\u00b7les", "rau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ein Wunsch allein, der in mir wohnt &#8212;               ", "tokens": ["Ein", "Wunsch", "al\u00b7lein", ",", "der", "in", "mir", "wohnt", "&#8212;"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "XML_entity"], "pos": ["ART", "NN", "ADV", "$,", "PRELS", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "O, einmal h\u00f6ren, einmal lauschen,", "tokens": ["O", ",", "ein\u00b7mal", "h\u00f6\u00b7ren", ",", "ein\u00b7mal", "lau\u00b7schen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "VVINF", "$,", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und all mein Streben w\u00e4r\u2019 gelohnt!", "tokens": ["Und", "all", "mein", "Stre\u00b7ben", "w\u00e4r'", "ge\u00b7lohnt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "PPOSAT", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Umsonst, umsonst. Die Sph\u00e4ren schweigen,", "tokens": ["Um\u00b7sonst", ",", "um\u00b7sonst", ".", "Die", "Sph\u00e4\u00b7ren", "schwei\u00b7gen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "$.", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mein Aug\u2019 wird matt, mein Ohr wird stumpf,", "tokens": ["Mein", "Aug'", "wird", "matt", ",", "mein", "Ohr", "wird", "stumpf", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$,", "PPOSAT", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Fremd schau\u2019 ich auf der Erde Reigen,", "tokens": ["Fremd", "schau'", "ich", "auf", "der", "Er\u00b7de", "Rei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der sinnlos mich umdr\u00e4ngt und dumpf.", "tokens": ["Der", "sinn\u00b7los", "mich", "um\u00b7dr\u00e4ngt", "und", "dumpf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PPER", "VVFIN", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wie leer die Stunden hin sich dehnen!", "tokens": ["Wie", "leer", "die", "Stun\u00b7den", "hin", "sich", "deh\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "ADV", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Du b\u00f6se, Moira, meine Last;", "tokens": ["Du", "b\u00f6\u00b7se", ",", "Moi\u00b7ra", ",", "mei\u00b7ne", "Last", ";"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "NE", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Von meinem Denken, meinem Sehnen", "tokens": ["Von", "mei\u00b7nem", "Den\u00b7ken", ",", "mei\u00b7nem", "Seh\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Gieb in der Urne s\u00fc\u00dfe Rast!", "tokens": ["Gieb", "in", "der", "Ur\u00b7ne", "s\u00fc\u00b7\u00dfe", "Rast", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}