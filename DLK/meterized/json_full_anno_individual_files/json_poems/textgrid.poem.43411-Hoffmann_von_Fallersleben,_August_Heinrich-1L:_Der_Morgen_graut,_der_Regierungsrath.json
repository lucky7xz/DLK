{"textgrid.poem.43411": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der Morgen graut, der Regierungsrath", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Morgen graut, der Regierungsrath", "tokens": ["Der", "Mor\u00b7gen", "graut", ",", "der", "Re\u00b7gie\u00b7rungs\u00b7rath"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sitzt schon bei seinen Gesch\u00e4ften,", "tokens": ["Sitzt", "schon", "bei", "sei\u00b7nen", "Ge\u00b7sch\u00e4f\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ist ausger\u00fcstet f\u00fcr Kirch' und Staat", "tokens": ["Ist", "aus\u00b7ge\u00b7r\u00fcs\u00b7tet", "f\u00fcr", "Kirch'", "und", "Staat"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "VVPP", "APPR", "NN", "KON", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Mit frisch erneueten Kr\u00e4ften.", "tokens": ["Mit", "frisch", "er\u00b7neu\u00b7e\u00b7ten", "Kr\u00e4f\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Er denkt mit Freuden an seine Pflicht", "tokens": ["Er", "denkt", "mit", "Freu\u00b7den", "an", "sei\u00b7ne", "Pflicht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Und schrei