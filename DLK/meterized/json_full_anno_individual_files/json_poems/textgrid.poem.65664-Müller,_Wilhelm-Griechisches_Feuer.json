{"textgrid.poem.65664": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Griechisches Feuer", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "K\u00f6nnt' ich meine Feder doch jetzt in ", "tokens": ["K\u00f6nnt'", "ich", "mei\u00b7ne", "Fe\u00b7der", "doch", "jetzt", "in"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PPOSAT", "NN", "ADV", "ADV", "APPR"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Das kein Wasser l\u00f6schen kann, das im ", "tokens": ["Das", "kein", "Was\u00b7ser", "l\u00f6\u00b7schen", "kann", ",", "das", "im"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "PIAT", "NN", "VVINF", "VMFIN", "$,", "PRELS", "APPRART"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "O und k\u00f6nnt' ich mit dem Kiel eure starren Busen spalten,", "tokens": ["O", "und", "k\u00f6nnt'", "ich", "mit", "dem", "Kiel", "eu\u00b7re", "star\u00b7ren", "Bu\u00b7sen", "spal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "VMFIN", "PPER", "APPR", "ART", "NE", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Und ein solches Feuer spein tief in eurer Herzen Falten,", "tokens": ["Und", "ein", "sol\u00b7ches", "Feu\u00b7er", "spein", "tief", "in", "eu\u00b7rer", "Her\u00b7zen", "Fal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PIAT", "NN", "ADV", "ADJD", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "--+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Drinnen ihre Nester baun schillernde Cham\u00e4leone,", "tokens": ["Drin\u00b7nen", "ih\u00b7re", "Nes\u00b7ter", "baun", "schil\u00b7lern\u00b7de", "Cha\u00b7m\u00e4\u00b7le\u00b7o\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVFIN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+--+-+-", "measure": "trochaic.septa.relaxed"}, "line.6": {"text": "Und der Ottern bunt Gez\u00fccht spielt mit Christi Dornenkrone.", "tokens": ["Und", "der", "Ot\u00b7tern", "bunt", "Ge\u00b7z\u00fccht", "spielt", "mit", "Chris\u00b7ti", "Dor\u00b7nen\u00b7kro\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJD", "NN", "VVFIN", "APPR", "NE", "NE", "$."], "meter": "--+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "M\u00f6cht' er \u00f6ffnen, da\u00df ihr Dunst ungew\u00fcrzt stieg' in die L\u00fcfte.", "tokens": ["M\u00f6cht'", "er", "\u00f6ff\u00b7nen", ",", "da\u00df", "ihr", "Dunst", "un\u00b7ge\u00b7w\u00fcrzt", "stieg'", "in", "die", "L\u00fcf\u00b7te", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVINF", "$,", "KOUS", "PPER", "ADV", "ADJD", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-++-+-+-+-", "measure": "unknown.measure.octa.plus"}, "line.8": {"text": "Alle Brut der Schlangennester, da\u00df die Welt sie kriechen s\u00e4he.", "tokens": ["Al\u00b7le", "Brut", "der", "Schlan\u00b7gen\u00b7nes\u00b7ter", ",", "da\u00df", "die", "Welt", "sie", "krie\u00b7chen", "s\u00e4\u00b7he", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ART", "NN", "$,", "KOUS", "ART", "NN", "PPER", "VVINF", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.9": {"text": "Eurer blanken Kreuze ja 