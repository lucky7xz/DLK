{"textgrid.poem.39430": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sieh diese heil'ge Waldkapell!", "genre": "verse", "period": "N.A.", "pub_year": 1807, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sieh diese heil'ge Waldkapell!", "tokens": ["Sieh", "die\u00b7se", "heil'\u00b7ge", "Wald\u00b7ka\u00b7pell", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie ist geweiht an selber Stell,", "tokens": ["Sie", "ist", "ge\u00b7weiht", "an", "sel\u00b7ber", "Stell", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wo Ge\u00dflers Hochmuth Tell erscho\u00df,", "tokens": ["Wo", "Ge\u00df\u00b7lers", "Hoch\u00b7muth", "Tell", "er\u00b7scho\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "NE", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und edle Schweizer Freiheit spro\u00df.", "tokens": ["Und", "ed\u00b7le", "Schwei\u00b7zer", "Frei\u00b7heit", "spro\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Hubertus habe Dank und Lohn,", "tokens": ["Hu\u00b7ber\u00b7tus", "ha\u00b7be", "Dank", "und", "Lohn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "NN", "KON", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Des wackern Waidwerks Schutzpatron!", "tokens": ["Des", "wa\u00b7ckern", "Waid\u00b7werks", "Schutz\u00b7pat\u00b7ron", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Tell klomm, ein rascher J\u00e4gersmann,", "tokens": ["Tell", "klomm", ",", "ein", "ra\u00b7scher", "J\u00e4\u00b7gers\u00b7mann", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Schl\u00fcft' hinab und Alpen an.", "tokens": ["Die", "Schl\u00fcft'", "hin\u00b7ab", "und", "Al\u00b7pen", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Den Steinbock hat er oft gef\u00e4llt,", "tokens": ["Den", "Stein\u00b7bock", "hat", "er", "oft", "ge\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Gems' in Wolken nachgestellt.", "tokens": ["Der", "Gems'", "in", "Wol\u00b7ken", "nach\u00b7ge\u00b7stellt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er scheute nicht den Wolf und B\u00e4r,", "tokens": ["Er", "scheu\u00b7te", "nicht", "den", "Wolf", "und", "B\u00e4r", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ART", "NE", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit seiner guten Armbrust Wehr.", "tokens": ["Mit", "sei\u00b7ner", "gu\u00b7ten", "Arm\u00b7brust", "Wehr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Da rief ihn Gott zu h\u00f6herm Werk,", "tokens": ["Da", "rief", "ihn", "Gott", "zu", "h\u00f6\u00b7herm", "Werk", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und gab ihm Muth und Heldenst\u00e4rk':", "tokens": ["Und", "gab", "ihm", "Muth", "und", "Hel\u00b7den\u00b7st\u00e4rk'", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Vollbringen sollt' er das Gericht,", "tokens": ["Voll\u00b7brin\u00b7gen", "sollt'", "er", "das", "Ge\u00b7richt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das Ge\u00dflern Todes schuldig spricht.", "tokens": ["Das", "Ge\u00df\u00b7lern", "To\u00b7des", "schul\u00b7dig", "spricht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Hier in dem Hohlweg kam zu Ro\u00df", "tokens": ["Hier", "in", "dem", "Hohl\u00b7weg", "kam", "zu", "Ro\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Landvogt mit der Knechte Tro\u00df.", "tokens": ["Der", "Land\u00b7vogt", "mit", "der", "Knech\u00b7te", "Tro\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Tell lauschet still, und zielt so wohl,", "tokens": ["Tell", "lau\u00b7schet", "still", ",", "und", "zielt", "so", "wohl", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PTKVZ", "$,", "KON", "VVFIN", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df ihn sein Volk noch preisen soll.", "tokens": ["Da\u00df", "ihn", "sein", "Volk", "noch", "prei\u00b7sen", "soll", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Die Senne schnellt, es saust der Pfeil,", "tokens": ["Die", "Sen\u00b7ne", "schnellt", ",", "es", "saust", "der", "Pfeil", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des Himmels Blitzen gleich an Eil;", "tokens": ["Des", "Him\u00b7mels", "Blit\u00b7zen", "gleich", "an", "Eil", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es spaltet recht der scharfe Bolz", "tokens": ["Es", "spal\u00b7tet", "recht", "der", "schar\u00b7fe", "Bolz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Des Ge\u00dflers Herz so frech und stolz.", "tokens": ["Des", "Ge\u00df\u00b7lers", "Herz", "so", "frech", "und", "stolz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Gepriesen sei der wackre Sch\u00fctz,", "tokens": ["Ge\u00b7prie\u00b7sen", "sei", "der", "wack\u00b7re", "Sch\u00fctz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er ist f\u00fcr manches Raubthier n\u00fctz;", "tokens": ["Er", "ist", "f\u00fcr", "man\u00b7ches", "Raubt\u00b7hier", "n\u00fctz", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PIAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sein Aug' ist hell, sein Sinn ist frei,", "tokens": ["Sein", "Aug'", "ist", "hell", ",", "sein", "Sinn", "ist", "frei", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$,", "PPOSAT", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Feind aller Schmach und Dr\u00e4ngerei.", "tokens": ["Feind", "al\u00b7ler", "Schmach", "und", "Dr\u00e4n\u00b7ge\u00b7rei", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "KON", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.8": {"line.1": {"text": "Sein bestes Ziel ist ein Tyrann,", "tokens": ["Sein", "bes\u00b7tes", "Ziel", "ist", "ein", "Ty\u00b7rann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In aller Menschen Acht und Bann.", "tokens": ["In", "al\u00b7ler", "Men\u00b7schen", "Acht", "und", "Bann", "."], "token_info": ["word", "word", "word"