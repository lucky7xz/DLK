{"dta.poem.9825": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Ein vornehmes frauenzimmer in der  \n Schlesien nimt solches von den m\u00e4nnern  \n f\u00fcr bekant an:", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.71", "sv:0.28"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "So must ihr m\u00e4nner doch bekennen/", "tokens": ["So", "must", "ihr", "m\u00e4n\u00b7ner", "doch", "be\u00b7ken\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPOSAT", "ADJA", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df Adam that/ was Eva wolt.", "tokens": ["Da\u00df", "A\u00b7dam", "that", "/", "was", "E\u00b7va", "wolt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVFIN", "$(", "PWS", "NN", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Damit ihr ihm nachfolgen solt/", "tokens": ["Da\u00b7mit", "ihr", "ihm", "nach\u00b7fol\u00b7gen", "solt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Lie\u00df er euch seine erben nennen.", "tokens": ["Lie\u00df", "er", "euch", "sei\u00b7ne", "er\u00b7ben", "nen\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "PPOSAT", "VVINF", "VVINF", "$."], "meter": "-+-+-+---", "measure": "unknown.measure.tri"}}}}}