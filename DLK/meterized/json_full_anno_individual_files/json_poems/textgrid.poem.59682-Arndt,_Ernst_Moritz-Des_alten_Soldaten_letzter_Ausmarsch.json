{"textgrid.poem.59682": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "Des alten Soldaten letzter Ausmarsch", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Marsch! Was klingen die Trompeten?", "tokens": ["Marsch", "!", "Was", "klin\u00b7gen", "die", "Trom\u00b7pe\u00b7ten", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PWS", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Marsch! Klingt das nicht Totenmarsch?", "tokens": ["Marsch", "!", "Klingt", "das", "nicht", "To\u00b7ten\u00b7marsch", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "VVFIN", "PDS", "PTKNEG", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Helles Blasen nicht und Fl\u00f6ten", "tokens": ["Hel\u00b7les", "Bla\u00b7sen", "nicht", "und", "Fl\u00f6\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "NN", "PTKNEG", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ernst und still, nicht wild und barsch?", "tokens": ["Ernst", "und", "still", ",", "nicht", "wild", "und", "barsch", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ADJD", "$,", "PTKNEG", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Marsch! Es mu\u00df gewandert werden!", "tokens": ["Marsch", "!", "Es", "mu\u00df", "ge\u00b7wan\u00b7dert", "wer\u00b7den", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PPER", "VMFIN", "VVPP", "VAINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Nicht zu Tanz und Kriegesspiel,", "tokens": ["Nicht", "zu", "Tanz", "und", "Krie\u00b7ges\u00b7spiel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Nein, der letzte Marsch auf Erden", "tokens": ["Nein", ",", "der", "letz\u00b7te", "Marsch", "auf", "Er\u00b7den"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "ART", "ADJA", "NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und der n\u00e4chste Marsch zum Ziel.", "tokens": ["Und", "der", "n\u00e4chs\u00b7te", "Marsch", "zum", "Ziel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}, "stanza.2": {"line.1": {"text": "Marsch! Zum Abzug wird geblasen,", "tokens": ["Marsch", "!", "Zum", "Ab\u00b7zug", "wird", "ge\u00b7bla\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "APPRART", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und des Lebens hast du satt;", "tokens": ["Und", "des", "Le\u00b7bens", "hast", "du", "satt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "PPER", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nimm das letzte Gr\u00fcn vom Rasen,", "tokens": ["Nimm", "das", "letz\u00b7te", "Gr\u00fcn", "vom", "Ra\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nimm vom Baum das letzte Blatt,", "tokens": ["Nimm", "vom", "Baum", "das", "letz\u00b7te", "Blatt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Nimm vom Strauch die letzte Rose;", "tokens": ["Nimm", "vom", "Strauch", "die", "letz\u00b7te", "Ro\u00b7se", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Denn es mu\u00df geschieden sein:", "tokens": ["Denn", "es", "mu\u00df", "ge\u00b7schie\u00b7den", "sein", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "VVPP", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "All vergriffen sind die Lose,", "tokens": ["All", "ver\u00b7grif\u00b7fen", "sind", "die", "Lo\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Keines steht f\u00fcr dich noch ein.", "tokens": ["Kei\u00b7nes", "steht", "f\u00fcr", "dich", "noch", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "PPER", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Sei's! Trompeten und Posaunen,", "tokens": ["Sei's", "!", "Trom\u00b7pe\u00b7ten", "und", "Po\u00b7sau\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$.", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schallt, und donnre, Paukenschlag!", "tokens": ["Schallt", ",", "und", "donn\u00b7re", ",", "Pau\u00b7ken\u00b7schlag", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "KON", "VVFIN", "$,", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Donnre Schrecken und Erstaunen!", "tokens": ["Donn\u00b7re", "Schre\u00b7cken", "und", "Er\u00b7stau\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mir entbebt kein Weh noch Ach!", "tokens": ["Mir", "ent\u00b7bebt", "kein", "Weh", "noch", "Ach", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "ADV", "ITJ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und ich will es fr\u00f6hlich sagen:", "tokens": ["Und", "ich", "will", "es", "fr\u00f6h\u00b7lich", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PPER", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ja, des Lebens hab' ich satt,", "tokens": ["Ja", ",", "des", "Le\u00b7bens", "hab'", "ich", "satt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ART", "NN", "VAFIN", "PPER", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Falle still und ohne Klagen", "tokens": ["Fal\u00b7le", "still", "und", "oh\u00b7ne", "Kla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PTKVZ", "KON", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wie vom Baum ein welkes Blatt.", "tokens": ["Wie", "vom", "Baum", "ein", "wel\u00b7kes", "Blatt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Denn ich bin Soldat gewesen,", "tokens": ["Denn", "ich", "bin", "Sol\u00b7dat", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "NN", "VAPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und in manchem hei\u00dfen Strau\u00df", "tokens": ["Und", "in", "man\u00b7chem", "hei\u00b7\u00dfen", "Strau\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PIAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bliesen Kugeln auserlesen", "tokens": ["Blie\u00b7sen", "Ku\u00b7geln", "au\u00b7ser\u00b7le\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mir fast Licht und Atem aus,", "tokens": ["Mir", "fast", "Licht", "und", "A\u00b7tem", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "NN", "KON", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wilde Scharen aller Farben", "tokens": ["Wil\u00b7de", "Scha\u00b7ren", "al\u00b7ler", "Far\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Drangen st\u00fcrmend auf mich ein,", "tokens": ["Dran\u00b7gen", "st\u00fcr\u00b7mend", "auf", "mich", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADJD", "APPR", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Schrammen, Striemen, Wunden, Narben", "tokens": ["Schram\u00b7men", ",", "Strie\u00b7men", ",", "Wun\u00b7den", ",", "Nar\u00b7ben"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "M\u00fcssen des mir Zeugen sein.", "tokens": ["M\u00fcs\u00b7sen", "des", "mir", "Zeu\u00b7gen", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "PPER", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Nicht auf weichen, seidnen Sitzen", "tokens": ["Nicht", "auf", "wei\u00b7chen", ",", "seid\u00b7nen", "Sit\u00b7zen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PTKNEG", "APPR", "VVINF", "$,", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wiegte mich das Leben durch,", "tokens": ["Wieg\u00b7te", "mich", "das", "Le\u00b7ben", "durch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Scharf mit Donnerschlag und Blitzen", "tokens": ["Scharf", "mit", "Don\u00b7ner\u00b7schlag", "und", "Blit\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Traf's mich aus der Himmelsburg:", "tokens": ["Traf's", "mich", "aus", "der", "Him\u00b7mels\u00b7burg", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PRF", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Denn wo gute K\u00e4mpfer standen,", "tokens": ["Denn", "wo", "gu\u00b7te", "K\u00e4mp\u00b7fer", "stan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Bot ich mich dem Sch\u00fctzen voll,", "tokens": ["Bot", "ich", "mich", "dem", "Sch\u00fct\u00b7zen", "voll", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und der Sch\u00fctz hat wohl verstanden,", "tokens": ["Und", "der", "Sch\u00fctz", "hat", "wohl", "ver\u00b7stan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wie ins Herz man treffen soll.", "tokens": ["Wie", "ins", "Herz", "man", "tref\u00b7fen", "soll", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPRART", "NN", "PIS", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "\u00bbwelcher Sch\u00fctz? O welche Fabeln?", "tokens": ["\u00bb", "wel\u00b7cher", "Sch\u00fctz", "?", "O", "wel\u00b7che", "Fa\u00b7beln", "?"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PWAT", "NN", "$.", "NE", "PWAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wohin tr\u00e4umt der irre Greis?", "tokens": ["Wo\u00b7hin", "tr\u00e4umt", "der", "ir\u00b7re", "Greis", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Spielt in Bildern und Parabeln", "tokens": ["Spielt", "in", "Bil\u00b7dern", "und", "Pa\u00b7ra\u00b7beln"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Aus, wovon er selbst nicht wei\u00df?\u00ab", "tokens": ["Aus", ",", "wo\u00b7von", "er", "selbst", "nicht", "wei\u00df", "?", "\u00ab"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "$,", "PWAV", "PPER", "ADV", "PTKNEG", "VVFIN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Schweigt! Hier m\u00fc\u00dft ihr alle lallen,", "tokens": ["Schweigt", "!", "Hier", "m\u00fc\u00dft", "ihr", "al\u00b7le", "lal\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "ADV", "VMFIN", "PPER", "PIS", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Kinder, kind'sche Tr\u00e4umer sein,", "tokens": ["Kin\u00b7der", ",", "kin\u00b7d'\u00b7sche", "Tr\u00e4u\u00b7mer", "sein", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "VAINF", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Beten, knien, niederfallen", "tokens": ["Be\u00b7ten", ",", "kni\u00b7en", ",", "nie\u00b7der\u00b7fal\u00b7len"], "token_info": ["word", "punct", "word", "punct", "word"], "pos": ["NN", "$,", "VVFIN", "$,", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Vor des Sch\u00fctzen Blitzesschein.", "tokens": ["Vor", "des", "Sch\u00fct\u00b7zen", "Blit\u00b7zes\u00b7schein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Marsch! O Freudenmarsch! Und munter", "tokens": ["Marsch", "!", "O", "Freu\u00b7den\u00b7marsch", "!", "Und", "mun\u00b7ter"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NE", "$.", "NE", "NN", "$.", "KON", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Spielt mir auf zum letzten Gang!", "tokens": ["Spielt", "mir", "auf", "zum", "letz\u00b7ten", "Gang", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Klingt mir fr\u00f6hlich noch hinunter", "tokens": ["Klingt", "mir", "fr\u00f6h\u00b7lich", "noch", "hin\u00b7un\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADJD", "ADV", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In das stille Grab den Klang!", "tokens": ["In", "das", "stil\u00b7le", "Grab", "den", "Klang", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Kameraden, bald hienieder", "tokens": ["Ka\u00b7me\u00b7ra\u00b7den", ",", "bald", "hien\u00b7ie\u00b7der"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$,", "ADV", "PAV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Folgt ihr mir zu gleichem Ziel \u2013", "tokens": ["Folgt", "ihr", "mir", "zu", "glei\u00b7chem", "Ziel", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Doch getrost! Wir k\u00e4mpfen wieder", "tokens": ["Doch", "ge\u00b7trost", "!", "Wir", "k\u00e4mp\u00b7fen", "wie\u00b7der"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVPP", "$.", "PPER", "VVFIN", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Droben be\u00dfres Kriegesspiel.", "tokens": ["Dro\u00b7ben", "be\u00df\u00b7res", "Krie\u00b7ges\u00b7spiel", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Marsch! Was klingen die Trompeten?", "tokens": ["Marsch", "!", "Was", "klin\u00b7gen", "die", "Trom\u00b7pe\u00b7ten", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": [