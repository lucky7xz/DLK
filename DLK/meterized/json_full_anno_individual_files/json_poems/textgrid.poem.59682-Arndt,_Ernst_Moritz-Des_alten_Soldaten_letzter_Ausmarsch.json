, "pos": ["PPER", "VVFIN", "PIAT", "NN", "ADV", "ITJ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und ich will es fr\u00f6hlich sagen:", "tokens": ["Und", "ich", "will", "es", "fr\u00f6h\u00b7lich", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PPER", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ja, des Lebens hab' ich satt,", "tokens": ["Ja", ",", "des", "Le\u00b7bens", "hab'", "ich", "satt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ART", "NN", "VAFIN", "PPER", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Falle still und ohne Klagen", "tokens": ["Fal\u00b7le", "still", "und", "oh\u00b7ne", "Kla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PTKVZ", "KON", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wie vom Baum ein welkes Blatt.", "tokens": ["Wie", "vom", "Baum", "ein", "wel\u00b7kes", "Blatt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Denn ich bin Soldat gewesen,", "tokens": ["Denn", "ich", "bin", "Sol\u00b7dat", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "NN", "VAPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und in manchem hei\u00dfen Strau\u00df", "tokens": ["Und", "in", "man\u00b7chem", "hei\u00b7\u00dfen", "Strau\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PIAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bliesen Kugeln auserlese