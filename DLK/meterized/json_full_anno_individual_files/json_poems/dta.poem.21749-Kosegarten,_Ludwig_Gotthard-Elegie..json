, "token_info": ["word", "punct"], "pos": ["NE", "$,"], "meter": "+", "measure": "single.up"}, "line.80": {"text": "W\u00fcrden, wenn thaute die Nacht, zu ambrosischem", "tokens": ["W\u00fcr\u00b7den", ",", "wenn", "thau\u00b7te", "die", "Nacht", ",", "zu", "am\u00b7bro\u00b7si\u00b7schem"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VAFIN", "$,", "KOUS", "VVFIN", "ART", "NN", "$,", "APPR", "NN"], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}, "line.81": {"text": "Schlummer uns lagern,", "tokens": ["Schlum\u00b7mer", "uns", "la\u00b7gern", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVFIN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.82": {"text": "Meine Rechte dich g\u00fcrtend, die deinige mich!", "tokens": ["Mei\u00b7ne", "Rech\u00b7te", "dich", "g\u00fcr\u00b7tend", ",", "die", "dei\u00b7ni\u00b7ge", "mich", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "VVPP", "$,", "PRELS", "PPOSAT", "PPER", "$."], "meter": "+-+--+--+--+", "measure": "trochaic.penta.relaxed"}, "line.83": {"text": "W\u00fcrden jede Sekunde mit K\u00fcssen befl\u00fcgeln, und", "tokens": ["W\u00fcr\u00b7den", "je\u00b7de", "Se\u00b7kun\u00b7de", "mit", "K\u00fcs\u00b7sen", "be\u00b7fl\u00fc\u00b7geln", ",", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["VAFIN", "PIAT", "NN", "APPR", "NN", "VVINF", "$,", "KON"], "meter": "+-+--+--+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.84": {"text": "jede", "tokens": ["je\u00b7de"], "token_info": ["word"], "pos": ["PIAT"], "meter": "+-", "measure": "trochaic.single"}, "line.85": {"text": "Fesseln mit reinem Genuss, w\u00fcrden in Einem", "tokens": ["Fes\u00b7seln", "mit", "rei\u00b7nem", "Ge\u00b7nuss", ",", "w\u00fcr\u00b7den", "in", "Ei\u00b7nem"], "token_info": ["word", "word", "word", "word", "