{"dta.poem.21749": {"metadata": {"author": {"name": "Kosegarten, Ludwig Gotthard", "birth": "N.A.", "death": "N.A."}, "title": "Elegie.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1802", "urn": "urn:nbn:de:kobv:b4-200905193310", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Welche fremde Gef&#252;hle durchschaudern mich!", "tokens": ["Wel\u00b7che", "frem\u00b7de", "Gef", "&#252;", "hle", "durch\u00b7schau\u00b7dern", "mich", "!"], "token_info": ["word", "word", "word", "XML_entity", "word", "word", "word", "punct"], "pos": ["PWAT", "ADJA", "NN", "$(", "VVFIN", "VVFIN", "PPER", "$."], "meter": "+-+-++-+-+", "measure": "unknown.measure.hexa"}, "line.2": {"text": "Welche Verwirrung", "tokens": ["Wel\u00b7che", "Ver\u00b7wir\u00b7rung"], "token_info": ["word", "word"], "pos": ["PWAT", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "W\u00f6lket die Sinne! Mich fasst wechselnd Ent-", "tokens": ["W\u00f6l\u00b7ket", "die", "Sin\u00b7ne", "!", "Mich", "fasst", "wech\u00b7selnd", "Ent"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "$.", "PPER", "VVFIN", "ADJD", "TRUNC"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.4": {"text": "Itzt hebt seliges Ahnen empor zu den Sternen die", "tokens": ["Itzt", "hebt", "se\u00b7li\u00b7ges", "Ah\u00b7nen", "em\u00b7por", "zu", "den", "Ster\u00b7nen", "die"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADJA", "NN", "PTKVZ", "APPR", "ART", "NN", "ART"], "meter": "-+-+-+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Seele;", "tokens": ["See\u00b7le", ";"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.6": {"text": "T\u00f6dtliches Zagen sodann senkt in den Hades", "tokens": ["T\u00f6dt\u00b7li\u00b7ches", "Za\u00b7gen", "so\u00b7dann", "senkt", "in", "den", "Ha\u00b7des"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "ADV", "VVFIN", "APPR", "ART", "NN"], "meter": "+--+-+-+--+-", "measure": "iambic.penta.invert"}, "line.7": {"text": "den Geist.", "tokens": ["den", "Geist", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.8": {"text": "M\u00e4chte, die mich befehden, ihr feindlichen fremden", "tokens": ["M\u00e4ch\u00b7te", ",", "die", "mich", "be\u00b7feh\u00b7den", ",", "ihr", "feind\u00b7li\u00b7chen", "frem\u00b7den"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "PRELS", "PPER", "VVPP", "$,", "PPOSAT", "ADJA", "ADJA"], "meter": "+--+-+--+--+-", "measure": "iambic.penta.invert"}, "line.9": {"text": "Gewalten,", "tokens": ["Ge\u00b7wal\u00b7ten", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.10": {"text": "Sagt, was verbrach ich? was ists, dass ihr mich", "tokens": ["Sagt", ",", "was", "ver\u00b7brach", "ich", "?", "was", "ists", ",", "dass", "ihr", "mich"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$,", "PWS", "VVFIN", "PPER", "$.", "PWS", "VAFIN", "$,", "KOUS", "PPER", "PRF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "r\u00e4chend verfolgt;", "tokens": ["r\u00e4\u00b7chend", "ver\u00b7folgt", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVPP", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.12": {"text": "Dass ihr mit dieser Erynnis mich straft, die Frie-", "tokens": ["Dass", "ihr", "mit", "die\u00b7ser", "E\u00b7ryn\u00b7nis", "mich", "straft", ",", "die", "Frie"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PDAT", "NN", "PPER", "VVFIN", "$,", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "B\u00f6sslich mir raubet, mit Gift wechselnd und", "tokens": ["B\u00f6ss\u00b7lich", "mir", "rau\u00b7bet", ",", "mit", "Gift", "wech\u00b7selnd", "und"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "PPER", "VVFIN", "$,", "APPR", "NN", "ADJD", "KON"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.14": {"text": "Nektar mich tr\u00e4nkt!", "tokens": ["Nek\u00b7tar", "mich", "tr\u00e4nkt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.15": {"text": "Lass, lass ab von mir, gef\u00fcrchtete Liebe! Nicht", "tokens": ["Lass", ",", "lass", "ab", "von", "mir", ",", "ge\u00b7f\u00fcrch\u00b7te\u00b7te", "Lie\u00b7be", "!", "Nicht"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["NE", "$,", "VVFIN", "PTKVZ", "APPR", "PPER", "$,", "ADJA", "NN", "$.", "PTKNEG"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.16": {"text": "mag ich", "tokens": ["mag", "ich"], "token_info": ["word", "word"], "pos": ["VMFIN", "PPER"], "meter": "+-", "measure": "trochaic.single"}, "line.17": {"text": "Kosten des Kelches, der einst selbst den Alci-", "tokens": ["Kos\u00b7ten", "des", "Kel\u00b7ches", ",", "der", "einst", "selbst", "den", "Al\u00b7ci"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "$,", "PRELS", "ADV", "ADV", "ART", "TRUNC"], "meter": "+--+-+-+--+", "measure": "iambic.penta.invert"}, "line.18": {"text": "Lass, lass ab von mir. Von deinem Athem be-", "tokens": ["Lass", ",", "lass", "ab", "von", "mir", ".", "Von", "dei\u00b7nem", "A\u00b7them", "be"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "VVFIN", "PTKVZ", "APPR", "PPER", "$.", "APPR", "PPOSAT", "NN", "TRUNC"], "meter": "+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.19": {"text": "Taumel' ich, schwindel' ich schier. Schone,", "tokens": ["Tau\u00b7mel'", "ich", ",", "schwin\u00b7del'", "ich", "schier", ".", "Scho\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "VVFIN", "PPER", "ADJD", "$.", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Gef\u00e4hrliche, mein!", "tokens": ["Ge\u00b7f\u00e4hr\u00b7li\u00b7che", ",", "mein", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.21": {"text": "Nein du willst nicht schonen. Mit jeglichem sie-", "tokens": ["Nein", "du", "willst", "nicht", "scho\u00b7nen", ".", "Mit", "jeg\u00b7li\u00b7chem", "sie"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKANT", "PPER", "VMFIN", "PTKNEG", "VVINF", "$.", "APPR", "PIAT", "TRUNC"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.22": {"text": "Jeder gewinnenden Huld hast du die Feindinn", "tokens": ["Je\u00b7der", "ge\u00b7win\u00b7nen\u00b7den", "Huld", "hast", "du", "die", "Fein\u00b7dinn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "ADJA", "NN", "VAFIN", "PPER", "ART", "NN"], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.23": {"text": "geschm\u00fcckt,", "tokens": ["ge\u00b7schm\u00fcckt", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.24": {"text": "Diese zu freundliche Feindinn \u2014 dich, meine Ida!", "tokens": ["Die\u00b7se", "zu", "freund\u00b7li\u00b7che", "Fein\u00b7dinn", "dich", ",", "mei\u00b7ne", "I\u00b7da", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "PTKZU", "ADJA", "NN", "$(", "PPER", "$,", "PPOSAT", "NE", "$."], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}, "line.25": {"text": "Verg\u00f6nne,", "tokens": ["Ver\u00b7g\u00f6n\u00b7ne", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.26": {"text": "Dass die Seinige dich gr\u00fcsset das liebende", "tokens": ["Dass", "die", "Sei\u00b7ni\u00b7ge", "dich", "gr\u00fcs\u00b7set", "das", "lie\u00b7ben\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "PPOSS", "PPER", "VVFIN", "ART", "ADJA"], "meter": "+--+--+--+--", "measure": "dactylic.tetra"}, "line.27": {"text": "Herz.", "tokens": ["Herz", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}, "line.28": {"text": "Freundliche Ida, du bist so hold, wie die Schim-", "tokens": ["Freund\u00b7li\u00b7che", "I\u00b7da", ",", "du", "bist", "so", "hold", ",", "wie", "die", "Schim"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "NE", "$,", "PPER", "VAFIN", "ADV", "ADJD", "$,", "PWAV", "ART", "TRUNC"], "meter": "+--+--+-+--+", "measure": "dactylic.di.plus"}, "line.29": {"text": "Wenn der erwachende Tag r\u00f6thet das d\u00e4mmern-", "tokens": ["Wenn", "der", "er\u00b7wa\u00b7chen\u00b7de", "Tag", "r\u00f6\u00b7thet", "das", "d\u00e4m\u00b7mern"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVFIN", "ART", "TRUNC"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.30": {"text": "Bl\u00e4ue des Himmels umrieselt dein schimmerrollen-", "tokens": ["Bl\u00e4u\u00b7e", "des", "Him\u00b7mels", "um\u00b7rie\u00b7selt", "dein", "schim\u00b7mer\u00b7rol\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "VVFIN", "PPOSAT", "TRUNC"], "meter": "+--+--+--+-+-", "measure": "dactylic.tri.plus"}, "line.31": {"text": "R\u00f6the des Aufgangs verkl\u00e4rt, Huldinn, dein", "tokens": ["R\u00f6\u00b7the", "des", "Auf\u00b7gangs", "ver\u00b7kl\u00e4rt", ",", "Hul\u00b7dinn", ",", "dein"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["NN", "ART", "NN", "VVPP", "$,", "NN", "$,", "PPOSAT"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.32": {"text": "bl\u00fchend Gesicht.", "tokens": ["bl\u00fc\u00b7hend", "Ge\u00b7sicht", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.33": {"text": "Dunklere Tinten versch\u00f6nern den Mund, und lich-", "tokens": ["Dunk\u00b7le\u00b7re", "Tin\u00b7ten", "ver\u00b7sch\u00f6\u00b7nern", "den", "Mund", ",", "und", "lich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "ART", "NN", "$,", "KON", "TRUNC"], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}, "line.34": {"text": "Schwelgrisch umwallt dich die Fluth goldenen", "tokens": ["Schwel\u00b7grisch", "um\u00b7wallt", "dich", "die", "Fluth", "gol\u00b7de\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "ART", "NN", "ADJA"], "meter": "+--+--++--", "measure": "dactylic.di.plus"}, "line.35": {"text": "ringelnden Haars.", "tokens": ["rin\u00b7geln\u00b7den", "Haars", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.36": {"text": "Abgewogen aufs strengste ward dieser Glieder Ver-", "tokens": ["Ab\u00b7ge\u00b7wo\u00b7gen", "aufs", "strengs\u00b7te", "ward", "die\u00b7ser", "Glie\u00b7der", "Ver"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPRART", "ADJA", "VAFIN", "PDAT", "NN", "TRUNC"], "meter": "+-+--+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.37": {"text": "Dieser Formen Kontur ward von Apelles be-", "tokens": ["Die\u00b7ser", "For\u00b7men", "Kon\u00b7tur", "ward", "von", "A\u00b7pel\u00b7les", "be"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDAT", "NN", "NN", "VAFIN", "APPR", "NE", "TRUNC"], "meter": "+-+--+-+-+--", "measure": "trochaic.penta.relaxed"}, "line.38": {"text": "Doch wer redet es aus, was diese Formen veredelt,", "tokens": ["Doch", "wer", "re\u00b7det", "es", "aus", ",", "was", "die\u00b7se", "For\u00b7men", "ver\u00b7e\u00b7delt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VVFIN", "PPER", "PTKVZ", "$,", "PWS", "PDAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.39": {"text": "Diese Z\u00fcge verkl\u00e4rt, dieses Gebilde beseelt!", "tokens": ["Die\u00b7se", "Z\u00fc\u00b7ge", "ver\u00b7kl\u00e4rt", ",", "die\u00b7ses", "Ge\u00b7bil\u00b7de", "be\u00b7seelt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VVPP", "$,", "PDAT", "NN", "VVPP", "$."], "meter": "+-+--+---+--+", "measure": "trochaic.penta.relaxed"}, "line.40": {"text": "Wer die r\u00e4hrende Huld, die herzgewinnende Milde,", "tokens": ["Wer", "die", "r\u00e4h\u00b7ren\u00b7de", "Huld", ",", "die", "herz\u00b7ge\u00b7win\u00b7nen\u00b7de", "Mil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.41": {"text": "Wer die Ruh im Blick, wer in den Augen den", "tokens": ["Wer", "die", "Ruh", "im", "Blick", ",", "wer", "in", "den", "Au\u00b7gen", "den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "ART", "NN", "APPRART", "NN", "$,", "PWS", "APPR", "ART", "NN", "ART"], "meter": "--+-+-+-+--", "measure": "anapaest.init"}, "line.42": {"text": "Sinn!", "tokens": ["Sinn", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}, "line.43": {"text": "Wer die Einfalt und Demuth, die Zucht und Zart-", "tokens": ["Wer", "die", "Ein\u00b7falt", "und", "De\u00b7muth", ",", "die", "Zucht", "und", "Zar\u00b7t"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "ART", "NN", "KON", "NN", "$,", "ART", "NN", "KON", "TRUNC"], "meter": "--++--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.44": {"text": "Welche dir jegliches Herz, erstes der M\u00e4dchen,", "tokens": ["Wel\u00b7che", "dir", "jeg\u00b7li\u00b7ches", "Herz", ",", "ers\u00b7tes", "der", "M\u00e4d\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADJA", "NN", "$,", "ADJA", "ART", "NN", "$,"], "meter": "+--+--++--+-", "measure": "dactylic.di.plus"}, "line.45": {"text": "gewinnt.", "tokens": ["ge\u00b7winnt", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.46": {"text": "Also fand ich dich, Ida. So siegtest du, wenig es", "tokens": ["Al\u00b7so", "fand", "ich", "dich", ",", "I\u00b7da", ".", "So", "sieg\u00b7test", "du", ",", "we\u00b7nig", "es"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "$,", "NE", "$.", "ADV", "VVFIN", "PPER", "$,", "KOUS", "PPER"], "meter": "+-+--+--+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.47": {"text": "ahnend,", "tokens": ["ah\u00b7nend", ","], "token_info": ["word", "punct"], "pos": ["ADJD", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.48": {"text": "Wenig es wollend f\u00fcrwahr, \u00fcber des Sicheren", "tokens": ["We\u00b7nig", "es", "wol\u00b7lend", "f\u00fcr\u00b7wahr", ",", "\u00fc\u00b7ber", "des", "Si\u00b7che\u00b7ren"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "PPER", "ADJD", "PTKVZ", "$,", "APPR", "ART", "NN"], "meter": "+--+-+-+--+--", "measure": "iambic.penta.invert"}, "line.49": {"text": "Herz.", "tokens": ["Herz", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}, "line.50": {"text": "Ach zu sicheres Herz, wird dir auch Ihres begegnen?", "tokens": ["Ach", "zu", "si\u00b7che\u00b7res", "Herz", ",", "wird", "dir", "auch", "Ih\u00b7res", "be\u00b7geg\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PTKZU", "ADJA", "NN", "$,", "VAFIN", "PPER", "ADV", "PPOSAT", "VVINF", "$."], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.51": {"text": "Allzuvermessenes, wird Ida nicht stolz dich ver-", "tokens": ["All\u00b7zu\u00b7ver\u00b7mes\u00b7se\u00b7nes", ",", "wird", "I\u00b7da", "nicht", "stolz", "dich", "ver"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "$,", "VAFIN", "NE", "PTKNEG", "ADJD", "PPER", "TRUNC"], "meter": "+--+---+--+--", "measure": "dactylic.di.plus"}, "line.52": {"text": "Wirst du mich lieben, Geliebte? Dein schmach-", "tokens": ["Wirst", "du", "mich", "lie\u00b7ben", ",", "Ge\u00b7lieb\u00b7te", "?", "Dein", "schmach"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["VAFIN", "PPER", "PRF", "VVINF", "$,", "NN", "$.", "PPOSAT", "TRUNC"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.53": {"text": "Dein Err\u00f6then verr\u00e4th, dass du zu lieben ver-", "tokens": ["Dein", "Er\u00b7r\u00f6\u00b7then", "ver\u00b7r\u00e4\u00b7th", ",", "dass", "du", "zu", "lie\u00b7ben", "ver"], "token