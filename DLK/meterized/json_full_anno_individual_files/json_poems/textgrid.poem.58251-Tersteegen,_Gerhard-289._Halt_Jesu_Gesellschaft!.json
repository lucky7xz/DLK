{"textgrid.poem.58251": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "289. Halt Jesu Gesellschaft!", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wo l\u00e4ufst du hin \u2013 zu eiteln Dingen?", "tokens": ["Wo", "l\u00e4ufst", "du", "hin", "\u2013", "zu", "ei\u00b7teln", "Din\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PTKVZ", "$(", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie kannst du's \u00fcbers Herze bringen,", "tokens": ["Wie", "kannst", "du's", "\u00fc\u00b7bers", "Her\u00b7ze", "brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PIS", "ADV", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df du l\u00e4\u00dft deinen Freund allein?", "tokens": ["Da\u00df", "du", "l\u00e4\u00dft", "dei\u00b7nen", "Freund", "al\u00b7lein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Er will so gern mit dir umgehen,", "tokens": ["Er", "will", "so", "gern", "mit", "dir", "um\u00b7ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Sollt'st du ihn denn nicht auch ansehen?", "tokens": ["Sollt'st", "du", "ihn", "denn", "nicht", "auch", "an\u00b7se\u00b7hen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "ADV"