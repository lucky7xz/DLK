{"textgrid.poem.60752": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie flog der Wagen leicht dahin,", "genre": "verse", "period": "N.A.", "pub_year": 1828, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie flog der Wagen leicht dahin,", "tokens": ["Wie", "flog", "der", "Wa\u00b7gen", "leicht", "da\u00b7hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "ADJD", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Seit hinter mir der Apennin,", "tokens": ["Seit", "hin\u00b7ter", "mir", "der", "A\u00b7pen\u00b7nin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "PPER", "ART", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Seit jeder Pfad, auf dem er flog,", "tokens": ["Seit", "je\u00b7der", "Pfad", ",", "auf", "dem", "er", "flog", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "APPR", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ins Arnotal hinunterbog!", "tokens": ["Ins", "Ar\u00b7no\u00b7tal", "hin\u00b7un\u00b7ter\u00b7bog", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Olivenhaine rings herum,", "tokens": ["O\u00b7li\u00b7ven\u00b7hai\u00b7ne", "rings", "he\u00b7rum", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADV", "PTKVZ", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Wo manches sch\u00f6ne Tuskulum,", "tokens": ["Wo", "man\u00b7ches", "sch\u00f6\u00b7ne", "Tus\u00b7ku\u00b7lum", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Umgeben von Zypressen, stand,", "tokens": ["Um\u00b7ge\u00b7ben", "von", "Zyp\u00b7res\u00b7sen", ",", "stand", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "APPR", "NE", "$,", "VVFIN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.8": {"text": "Verhie\u00dfen mir ein mildres Land,", "tokens": ["Ver\u00b7hie\u00b7\u00dfen", "mir", "ein", "mild\u00b7res", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Ein Volk, das immer fr\u00f6hlich singt,", "tokens": ["Ein", "Volk", ",", "das", "im\u00b7mer", "fr\u00f6h\u00b7lich", "singt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Und dessen Sprache s\u00fc\u00dfer klingt.", "tokens": ["Und", "des\u00b7sen", "Spra\u00b7che", "s\u00fc\u00b7\u00dfer", "klingt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRELAT", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Nie la\u00dft mich wiedersehn, o nie", "tokens": ["Nie", "la\u00dft", "mich", "wie\u00b7der\u00b7sehn", ",", "o", "nie"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVIMP", "PPER", "VVFIN", "$,", "FM", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die nebelreiche Lombardie,", "tokens": ["Die", "ne\u00b7bel\u00b7rei\u00b7che", "Lom\u00b7bar\u00b7die", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Wo winterlich der Fl\u00fcsse Qualm", "tokens": ["Wo", "win\u00b7ter\u00b7lich", "der", "Fl\u00fcs\u00b7se", "Qualm"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Umdampft den d\u00fcrren Stoppelhalm,", "tokens": ["Um\u00b7dampft", "den", "d\u00fcr\u00b7ren", "Stop\u00b7pel\u00b7halm", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und \u00fcber ebne Fl\u00e4chen weit", "tokens": ["Und", "\u00fc\u00b7ber", "eb\u00b7ne", "Fl\u00e4\u00b7chen", "weit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sich legt die dicke Feuchtigkeit!", "tokens": ["Sich", "legt", "die", "di\u00b7cke", "Feuch\u00b7tig\u00b7keit", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wie pr\u00e4chtig Mailand auch, wie gro\u00df,", "tokens": ["Wie", "pr\u00e4ch\u00b7tig", "Mai\u00b7land", "auch", ",", "wie", "gro\u00df", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "NN", "ADV", "$,", "PWAV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Es liegt der Finsternis im Scho\u00df,", "tokens": ["Es", "liegt", "der", "Fins\u00b7ter\u00b7nis", "im", "Scho\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Und seiner breiten Stra\u00dfen Glanz,", "tokens": ["Und", "sei\u00b7ner", "brei\u00b7ten", "Stra\u00b7\u00dfen", "Glanz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Was frommt er ihm? Der Scala Tanz,", "tokens": ["Was", "frommt", "er", "ihm", "?", "Der", "Sca\u00b7la", "Tanz", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PPER", "$.", "ART", "NE", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den alten, marmorblanken Dom", "tokens": ["Den", "al\u00b7ten", ",", "mar\u00b7mor\u00b7blan\u00b7ken", "Dom"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Beneiden ihm Florenz und Rom;", "tokens": ["Be\u00b7nei\u00b7den", "ihm", "Flo\u00b7renz", "und", "Rom", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "NN", "KON", "NE", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Doch wo's so finster ist und kalt,", "tokens": ["Doch", "wo's", "so", "fins\u00b7ter", "ist", "und", "kalt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "ADV", "ADJD", "VAFIN", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Welch qu\u00e4lerischer Aufenthalt!", "tokens": ["Welch", "qu\u00e4\u00b7le\u00b7ri\u00b7scher", "Auf\u00b7ent\u00b7halt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wer wollte nicht, um ihn zu fliehn,", "tokens": ["Wer", "woll\u00b7te", "nicht", ",", "um", "ihn", "zu", "fliehn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PTKNEG", "$,", "KOUI", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Hoch \u00fcber die Geb\u00fcrge ziehn,", "tokens": ["Hoch", "\u00fc\u00b7ber", "die", "Ge\u00b7b\u00fcr\u00b7ge", "ziehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Hinab zur sch\u00f6nen Stadt gekehrt,", "tokens": ["Hin\u00b7ab", "zur", "sch\u00f6\u00b7nen", "Stadt", "ge\u00b7kehrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Die einst der Welt so viel gelehrt?", "tokens": ["Die", "einst", "der", "Welt", "so", "viel", "ge\u00b7lehrt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Du bist mir im Dezember Lenz,", "tokens": ["Du", "bist", "mir", "im", "De\u00b7zem\u00b7ber", "Lenz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "APPRART", "NN", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Du milder Himmel von Florenz!", "tokens": ["Du", "mil\u00b7der", "Him\u00b7mel", "von", "Flo\u00b7renz", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "APPR", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Pal\u00e4ste, gr\u00fcne Haine ziert", "tokens": ["Pa\u00b7l\u00e4s\u00b7te", ",", "gr\u00fc\u00b7ne", "Hai\u00b7ne", "ziert"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Arno, welcher nie gefriert,", "tokens": ["Der", "Ar\u00b7no", ",", "wel\u00b7cher", "nie", "ge\u00b7friert", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NE", "$,", "PRELS", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und \u00fcber ihm, so sch\u00f6n und breit,", "tokens": ["Und", "\u00fc\u00b7ber", "ihm", ",", "so", "sch\u00f6n", "und", "breit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "$,", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Br\u00fccke der Dreifaltigkeit.", "tokens": ["Die", "Br\u00fc\u00b7cke", "der", "Drei\u00b7fal\u00b7tig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Wie flog der Wagen leicht dahin,", "tokens": ["Wie", "flog", "der", "Wa\u00b7gen", "leicht", "da\u00b7hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "ADJD", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Seit hinter mir der Apennin,", "tokens": ["Seit", "hin\u00b7ter", "mir", "der", "A\u00b7pen\u00b7nin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "PPER", "ART", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Seit jeder Pfad, auf dem er flog,", "tokens": ["Seit", "je\u00b7der", "Pfad", ",", "auf", "dem", "er", "flog", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "APPR", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ins Arnotal hinunt