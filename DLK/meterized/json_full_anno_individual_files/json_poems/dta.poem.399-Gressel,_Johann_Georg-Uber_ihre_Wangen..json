{"dta.poem.399": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Uber ihre Wangen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Die Rosen-Wangen sind recht unvergleichlich sch\u00f6n/", "tokens": ["Die", "Ro\u00b7sen\u00b7Wan\u00b7gen", "sind", "recht", "un\u00b7ver\u00b7gleich\u00b7lich", "sch\u00f6n", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zinnober kan man da bey feinem Purpur sehn/", "tokens": ["Zin\u00b7no\u00b7ber", "kan", "man", "da", "bey", "fei\u00b7nem", "Pur\u00b7pur", "sehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PIS", "ADV", "APPR", "ADJA", "NN", "VVINF", "$("], "meter": "---+-+-+-+-+", "measure": "unknown.measure.penta"}, "line.3": {"text": "Die Nelcke mag nicht so mit bunten Farben prahlen/", "tokens": ["Die", "Nel\u00b7cke", "mag", "nicht", "so", "mit", "bun\u00b7ten", "Far\u00b7ben", "prah\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PTKNEG", "ADV", "APPR", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ein Scarlach mu\u00df das Feld so Tag als Nacht bemahlen.", "tokens": ["Ein", "Scar\u00b7lach", "mu\u00df", "das", "Feld", "so", "Tag", "als", "Nacht", "be\u00b7mah\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ART", "NN", "ADV", "NN", "KOUS", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Jhr Rosen Indiens h\u00fclt eure Bl\u00e4tter ein", "tokens": ["Ihr", "Ro\u00b7sen", "In\u00b7di\u00b7ens", "h\u00fclt", "eu\u00b7re", "Bl\u00e4t\u00b7ter", "ein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "NE", "VVFIN", "PPOSAT", "NN", "ART"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Eur Wunder wird hier nichts bey diesen Blumen seyn;", "tokens": ["Eur", "Wun\u00b7der", "wird", "hier", "nichts", "bey", "die\u00b7sen", "Blu\u00b7men", "seyn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "PIS", "APPR", "PDAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Der Atlas/ welcher euch am hellen Morgen zieret ", "tokens": ["Der", "At\u00b7las", "/", "wel\u00b7cher", "euch", "am", "hel\u00b7len", "Mor\u00b7gen", "zie\u00b7ret"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NE", "$(", "PRELS", "PPER", "APPRART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Der Scarlach/ welchen ihr des Mittags bey euch f\u00fchret/", "tokens": ["Der", "Scar\u00b7lach", "/", "wel\u00b7chen", "ihr", "des", "Mit\u00b7tags", "bey", "euch", "f\u00fch\u00b7ret", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PWAT", "PPOSAT", "ART", "NN", "APPR", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Die schwartze Mohren Farb\u2019/ so euch des Abends deckt/", "tokens": ["Die", "schwart\u00b7ze", "Moh\u00b7ren", "Fa\u00b7rb'", "/", "so", "euch", "des", "A\u00b7bends", "deckt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$(", "ADV", "PPER", "ART", "NN", "VVFIN", "$("], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "Die sind zu einer Zeit zur Schau hier ausgesteckt.", "tokens": ["Die", "sind", "zu", "ei\u00b7ner", "Zeit", "zur", "Schau", "hier", "aus\u00b7ge\u00b7steckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "ART", "NN", "APPRART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Die sch\u00f6ne Stirn ist weis/ ein Purpur ziert die Wangen/", "tokens": ["Die", "sch\u00f6\u00b7ne", "Stirn", "ist", "weis", "/", "ein", "Pur\u00b7pur", "ziert", "die", "Wan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PTKVZ", "$(", "ART", "NN", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Die Augen/ gleich der Nacht/ mit schwartzer Farben prangen.", "tokens": ["Die", "Au\u00b7gen", "/", "gleich", "der", "Nacht", "/", "mit", "schwart\u00b7zer", "Far\u00b7ben", "pran\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ADV", "ART", "NN", "$(", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}