{"textgrid.poem.60534": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "1L: L\u00e4chelnd lehnt er am Weidenstumpen,", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "L\u00e4chelnd lehnt er am Weidenstumpen,", "tokens": ["L\u00e4\u00b7chelnd", "lehnt", "er", "am", "Wei\u00b7den\u00b7stum\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Zwerghaft, bucklig, uralt, in Lumpen.", "tokens": ["Zwerg\u00b7haft", ",", "buck\u00b7lig", ",", "ur\u00b7alt", ",", "in", "Lum\u00b7pen", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "$,", "ADJD", "$,", "APPR", "NN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Seine abendbesonnte Herde", "tokens": ["Sei\u00b7ne", "a\u00b7bend\u00b7be\u00b7sonn\u00b7te", "Her\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Freut sich br\u00fcllend der \u00fcppigen Erde.", "tokens": ["Freut", "sich", "br\u00fcl\u00b7lend", "der", "\u00fcp\u00b7pi\u00b7gen", "Er\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.5": {"text": "Schauen sonst Tiere mit dunklem Leid", "tokens": ["Schau\u00b7en", "sonst", "Tie\u00b7re", "mit", "dunk\u00b7lem", "Leid"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "NN", "APPR", "ADJA", "NN"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.6": {"text": "Menschengestalt, hier sonder Neid", "tokens": ["Men\u00b7schen\u00b7ge\u00b7stalt", ",", "hier", "son\u00b7der", "Neid"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "ADV", "ADJA", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Blicken die wohlgewachsenen Rinder", "tokens": ["Bli\u00b7cken", "die", "wohl\u00b7ge\u00b7wach\u00b7se\u00b7nen", "Rin\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.8": {"text": "Auf das unsch\u00f6nste der Menschenkinder;", "tokens": ["Auf", "das", "un\u00b7sch\u00f6ns\u00b7te", "der", "Men\u00b7schen\u00b7kin\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ART", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.9": {"text": "Neidlos, auch ohne Furcht und Grauen", "tokens": ["Neid\u00b7los", ",", "auch", "oh\u00b7ne", "Furcht", "und", "Grau\u00b7en"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "ADV", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Mag die Herde den Hirten beschauen;", "tokens": ["Mag", "die", "Her\u00b7de", "den", "Hir\u00b7ten", "be\u00b7schau\u00b7en", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.11": {"text": "Haben auch Rinder Phantasie,", "tokens": ["Ha\u00b7ben", "auch", "Rin\u00b7der", "Phan\u00b7ta\u00b7sie", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "NN", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.12": {"text": "Ist sie doch so gewaltig nie,", "tokens": ["Ist", "sie", "doch", "so", "ge\u00b7wal\u00b7tig"