{"dta.poem.5615": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Vom Geist und C\u00f6rper", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn durch Gesetze nun die unver\u00e4ndert bleiben,", "tokens": ["Wenn", "durch", "Ge\u00b7set\u00b7ze", "nun", "die", "un\u00b7ver\u00b7\u00e4n\u00b7dert", "blei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "NN", "ADV", "ART", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Welt so viel Ver\u00e4nderungen weist,", "tokens": ["Die", "Welt", "so", "viel", "Ver\u00b7\u00e4n\u00b7de\u00b7run\u00b7gen", "weist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Verbirgt uns GOtt dennoch die R\u00e4der, die sie treiben.", "tokens": ["Ver\u00b7birgt", "uns", "Gott", "den\u00b7noch", "die", "R\u00e4\u00b7der", ",", "die", "sie", "trei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "ADV", "ART", "NN", "$,", "PRELS", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die Regeln, Ordnungen und der Zusammenhang,", "tokens": ["Die", "Re\u00b7geln", ",", "Ord\u00b7nun\u00b7gen", "und", "der", "Zu\u00b7sam\u00b7men\u00b7hang", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "KON", "ART", "NN", "$,"], "meter": "-+--+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Sind R\u00e4htsel, worinn unser Geist", "tokens": ["Sind", "R\u00e4ht\u00b7sel", ",", "wo\u00b7rinn", "un\u00b7ser", "Geist"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "NN", "$,", "PWAV", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mit M\u00fche die Bedeutung findet,", "tokens": ["Mit", "M\u00fc\u00b7he", "die", "Be\u00b7deu\u00b7tung", "fin\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Die man nicht sonder Flei\u00df und Arbeits-Last ergr\u00fcndet,", "tokens": ["Die", "man", "nicht", "son\u00b7der", "Flei\u00df", "und", "Ar\u00b7beits\u00b7Last", "er\u00b7gr\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "PTKNEG", "ADJA", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und die der Weisen Schaaren,", "tokens": ["Und", "die", "der", "Wei\u00b7sen", "Schaa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Von je her, schwer zu fassen waren.", "tokens": ["Von", "je", "her", ",", "schwer", "zu", "fas\u00b7sen", "wa\u00b7ren", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "PTKVZ", "$,", "ADJD", "PTKZU", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}