{"dta.poem.5365": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "J rdisches  \n  V ergn\u00fcgen  \n in  \n  GOTT .  \n  F \u00fcnfter  T heil.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "So viel dessen Meng\u2019 erlaubet, zu betrachten, zu erwegen,", "tokens": ["So", "viel", "des\u00b7sen", "Meng'", "er\u00b7lau\u00b7bet", ",", "zu", "be\u00b7trach\u00b7ten", ",", "zu", "er\u00b7we\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PIAT", "PDS", "NE", "VVFIN", "$,", "PTKZU", "VVINF", "$,", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Und, im fr\u00f6lichem Erinnern, ordentlich zu \u00fcberlegen,", "tokens": ["Und", ",", "im", "fr\u00f6\u00b7li\u00b7chem", "E\u00b7rin\u00b7nern", ",", "or\u00b7dent\u00b7lich", "zu", "\u00fc\u00b7berl\u00b7e\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "APPRART", "ADJA", "NN", "$,", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Was uns aus dem Gnaden-Born seiner Liebe zugeflossen,", "tokens": ["Was", "uns", "aus", "dem", "Gna\u00b7den\u00b7Born", "sei\u00b7ner", "Lie\u00b7be", "zu\u00b7ge\u00b7flos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "ART", "NN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+--+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Wie so mannigfachen Seegen wir geniessen, und genossen.", "tokens": ["Wie", "so", "man\u00b7nig\u00b7fa\u00b7chen", "See\u00b7gen", "wir", "ge\u00b7nies\u00b7sen", ",", "und", "ge\u00b7nos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PIAT", "NN", "PPER", "VVPP", "$,", "KON", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.5": {"text": "Sind nun aber, wie im Meer alle Tropfen nicht zu zehlen,", "tokens": ["Sind", "nun", "a\u00b7ber", ",", "wie", "im", "Meer", "al\u00b7le", "Trop\u00b7fen", "nicht", "zu", "zeh\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "$,", "PWAV", "APPRART", "NN", "PIAT", "NN", "PTKNEG", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.6": {"text": "Auch die Wunder sonder Zahl, die der Sch\u00f6pfer uns", "tokens": ["Auch", "die", "Wun\u00b7der", "son\u00b7der", "Zahl", ",", "die", "der", "Sch\u00f6p\u00b7fer", "uns"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "KON", "NN", "$,", "PRELS", "ART", "NN", "PPER"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.7": {"text": "verliehn;", "tokens": ["ver\u00b7liehn", ";"], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "-+", "measure": "iambic.single"}, "line.8": {"text": "Wollen wir doch wenigstens, wie wir schuldig, uns bem\u00fchn,", "tokens": ["Wol\u00b7len", "wir", "doch", "we\u00b7nigs\u00b7tens", ",", "wie", "wir", "schul\u00b7dig", ",", "uns", "be\u00b7m\u00fchn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "$,", "PWAV", "PPER", "ADJD", "$,", "PPER", "VVINF", "$,"], "meter": "+--+-+-+-+-+-+", "measure": "iambic.septa.invert"}, "line.9": {"text": "Einige derselbigen vor den andern zu erwehlen.", "tokens": ["Ei\u00b7ni\u00b7ge", "der\u00b7sel\u00b7bi\u00b7gen", "vor", "den", "an\u00b7dern", "zu", "er\u00b7weh\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "PDS", "APPR", "ART", "ADJA", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.10": {"text": "Und, weil unsre Seele leicht, wenn sie es erwegt, empfindet,", "tokens": ["Und", ",", "weil", "uns\u00b7re", "See\u00b7le", "leicht", ",", "wenn", "sie", "es", "er\u00b7wegt", ",", "emp\u00b7fin\u00b7det", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PPOSAT", "NN", "ADJD", "$,", "KOUS", "PPER", "PPER", "VVFIN", "$,", "VVFIN", "$,"], "meter": "-+--+-+----+-+-", "measure": "iambic.penta.relaxed"}, "line.11": {"text": "Wie sie durch die Sinnen blos mit der Creatur sich bindet;", "tokens": ["Wie", "sie", "durch", "die", "Sin\u00b7nen", "blos", "mit", "der", "Crea\u00b7tur", "sich", "bin\u00b7det", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "ART", "NN", "ADV", "APPR", "ART", "NN", "PRF", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.12": {"text": "Also r\u00fchm\u2019 ich billig, lobe, preis\u2019 und ehre den daf\u00fcr,", "tokens": ["Al\u00b7so", "r\u00fchm'", "ich", "bil\u00b7lig", ",", "lo\u00b7be", ",", "preis'", "und", "eh\u00b7re", "den", "da\u00b7f\u00fcr", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "$,", "VVFIN", "$,", "VVFIN", "KON", "VVFIN", "ART", "PAV", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.13": {"text": "Der, in Sinnen, solche Sch\u00e4tze menschlicher Natur zu g\u00f6nnen", "tokens": ["Der", ",", "in", "Sin\u00b7nen", ",", "sol\u00b7che", "Sch\u00e4t\u00b7ze", "menschli\u00b7cher", "Na\u00b7tur", "zu", "g\u00f6n\u00b7nen"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "$,", "APPR", "NN", "$,", "PIAT", "NN", "ADJA", "NN", "PTKZU", "VVINF"], "meter": "+-+-+-+-+--+-+-", "measure": "trochaic.septa.relaxed"}, "line.14": {"text": "Uns alhier gew\u00fcrdiget. GOtt geh\u00f6ret wahrlich mehr,", "tokens": ["Uns", "al\u00b7hier", "ge\u00b7w\u00fcr\u00b7di\u00b7get", ".", "Gott", "ge\u00b7h\u00f6\u00b7ret", "wahr\u00b7lich", "mehr", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVPP", "$.", "NN", "VVFIN", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.15": {"text": "Als wir, leider! ihm erweisen, f\u00fcr die Sinnen Preis und", "tokens": ["Als", "wir", ",", "lei\u00b7der", "!", "ihm", "er\u00b7wei\u00b7sen", ",", "f\u00fcr", "die", "Sin\u00b7nen", "Preis", "und"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "$,", "ADV", "$.", "PPER", "VVINF", "$,", "APPR", "ART", "NN", "NN", "KON"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.16": {"text": "Ehr.", "tokens": ["Ehr", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}, "line.17": {"text": "Da\u00df ich heute ", "tokens": ["Da\u00df", "ich", "heu\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPER", "ADV"], "meter": "+-+-", "measure": "trochaic.di"}, "line.18": {"text": "k\u00f6nnen,", "tokens": ["k\u00f6n\u00b7nen", ","], "token_info": ["word", "punct"], "pos": ["VMFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.19": {"text": "Da\u00df du dazu ", "tokens": ["Da\u00df", "du", "da\u00b7zu"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPER", "PAV"], "meter": "-+-+", "measure": "iambic.di"}, "line.20": {"text": "Wie geschenckt, so auch erhalten, Sch\u00f6pfer, daf\u00fcr danck", "tokens": ["Wie", "ge\u00b7schenckt", ",", "so", "auch", "er\u00b7hal\u00b7ten", ",", "Sch\u00f6p\u00b7fer", ",", "da\u00b7f\u00fcr", "danck"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PWAV", "VVPP", "$,", "ADV", "ADV", "VVPP", "$,", "NN", "$,", "PAV", "NN"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.21": {"text": "ich dir.", "tokens": ["ich", "dir", "."], "token_info": ["word", "word", "punct"], "pos": ["PPER", "PPER", "$."], "meter": "-+", "measure": "iambic.single"}, "line.22": {"text": "Da\u00df ich mit Getr\u00e4nck und Speisen habe Mund und Ma-", "tokens": ["Da\u00df", "ich", "mit", "Ge\u00b7tr\u00e4nck", "und", "Spei\u00b7sen", "ha\u00b7be", "Mund", "und", "Ma"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "NN", "KON", "NN", "VAFIN", "NN", "KON", "TRUNC"], "meter": "--+-+-+-+-+-+", "measure": "anapaest.init"}, "line.23": {"text": "gen f\u00fcllen,", "tokens": ["gen", "f\u00fcl\u00b7len", ","], "token_info": ["word", "word", "punct"], "pos": ["APPR", "VVINF", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.24": {"text": "Ja mit mannigfacher Anmuth meinen Durst und Hunger", "tokens": ["Ja", "mit", "man\u00b7nig\u00b7fa\u00b7cher", "An\u00b7muth", "mei\u00b7nen", "Durst", "und", "Hun\u00b7ger"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "APPR", "PIAT", "NN", "PPOSAT", "NN", "KON", "NN"], "meter": "+---+-+-+-+-+-", "measure": "dactylic.init"}, "line.25": {"text": "stillen,", "tokens": ["stil\u00b7len", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.26": {"text": "Und mit Lust mich n\u00e4hren k\u00f6nnen, ist warhaftig danckens", "tokens": ["Und", "mit", "Lust", "mich", "n\u00e4h\u00b7ren", "k\u00f6n\u00b7nen", ",", "ist", "war\u00b7haf\u00b7tig", "dan\u00b7ckens"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "PPER", "VVINF", "VMINF", "$,", "VAFIN", "ADJD", "ADV"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.27": {"text": "wehrt,", "tokens": ["wehrt", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+", "measure": "single.up"}, "line.28": {"text": "Da uns GOtt, nicht nur zur Nothdurft, sondern auch mit", "tokens": ["Da", "uns", "Gott", ",", "nicht", "nur", "zur", "Noth\u00b7durft", ",", "son\u00b7dern", "auch", "mit"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "NN", "$,", "PTKNEG", "ADV", "APPRART", "NN", "$,", "KON", "ADV", "APPR"], "meter": "--+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.29": {"text": "Anmuth n\u00e4hrt.", "tokens": ["An\u00b7muth", "n\u00e4hrt", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.30": {"text": "Da\u00df die mir geschenckte Glieder, deren eine solche Menge", "tokens": ["Da\u00df", "die", "mir", "ge\u00b7schenck\u00b7te", "Glie\u00b7der", ",", "de\u00b7ren", "ei\u00b7ne", "sol\u00b7che", "Men\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "PPER", "ADJA", "NN", "$,", "PRELAT", "ART", "PIAT", "NN"], "meter": "----+-+-+-+-+-+-", "measure": "unknown.measure.hexa"}, "line.31": {"text": "Innerlich und \u00e4usserlich, da\u00df der Nerv-und Adern G\u00e4nge", "tokens": ["In\u00b7ner\u00b7lich", "und", "\u00e4us\u00b7ser\u00b7lich", ",", "da\u00df", "der", "Ner\u00b7v\u00b7und", "A\u00b7dern", "G\u00e4n\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "KON", "ADJD", "$,", "KOUS", "ART", "NN", "NN", "NN"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}}}}