{"dta.poem.7641": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "13.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wo du mit der Natur dich f\u00fchlst im Gleichgewicht,", "tokens": ["Wo", "du", "mit", "der", "Na\u00b7tur", "dich", "f\u00fchlst", "im", "Gleich\u00b7ge\u00b7wicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "ART", "NN", "PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zweifelst du an der Welt Vollkommenheit auch nicht.", "tokens": ["Zwei\u00b7felst", "du", "an", "der", "Welt", "Voll\u00b7kom\u00b7men\u00b7heit", "auch", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "NN", "ADV", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wol zweifeln magst du, wo das Gleichgewicht gest\u00f6rt,", "tokens": ["Wol", "zwei\u00b7feln", "magst", "du", ",", "wo", "das", "Gleich\u00b7ge\u00b7wicht", "ge\u00b7st\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "VMFIN", "PPER", "$,", "PWAV", "ART", "NN", "VVPP", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Der Elemente Kampf ist gegen dich emp\u00f6rt.", "tokens": ["Der", "E\u00b7le\u00b7men\u00b7te", "Kampf", "ist", "ge\u00b7gen", "dich", "em\u00b7p\u00f6rt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "Doch mu\u00df der Menschengeist nur seine Waffen n\u00fctzen,", "tokens": ["Doch", "mu\u00df", "der", "Men\u00b7schen\u00b7geist", "nur", "sei\u00b7ne", "Waf\u00b7fen", "n\u00fct\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "NN", "ADV", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Um gegen Himmelstrich und Jahreszeit zu sch\u00fctzen.", "tokens": ["Um", "ge\u00b7gen", "Him\u00b7mel\u00b7strich", "und", "Jah\u00b7res\u00b7zeit", "zu", "sch\u00fct\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "APPR", "NN", "KON", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Und immer ist die Welt vollkommen ausgedacht,", "tokens": ["Und", "im\u00b7mer", "ist", "die", "Welt", "voll\u00b7kom\u00b7men", "aus\u00b7ge\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "ART", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Auch wo der Menschengeist sie erst vollkommen macht;", "tokens": ["Auch", "wo", "der", "Men\u00b7schen\u00b7geist", "sie", "erst", "voll\u00b7kom\u00b7men", "macht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWAV", "ART", "NN", "PPER", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Weil ja der Menschengeist dazu grad' aufgenommen", "tokens": ["Weil", "ja", "der", "Men\u00b7schen\u00b7geist", "da\u00b7zu", "grad'", "auf\u00b7ge\u00b7nom\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "NN", "PAV", "ADV", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ist in den gro\u00dfen Plan, da\u00df dieser sei vollkommen.", "tokens": ["Ist", "in", "den", "gro\u00b7\u00dfen", "Plan", ",", "da\u00df", "die\u00b7ser", "sei", "voll\u00b7kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "ADJA", "NN", "$,", "KOUS", "PDS", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}