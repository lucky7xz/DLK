{"textgrid.poem.37056": {"metadata": {"author": {"name": "H\u00f6lty, Ludwig Christoph Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "An die platonische Liebe", "genre": "verse", "period": "N.A.", "pub_year": 1771, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dir, o Liebe, schallet meine Laute,", "tokens": ["Dir", ",", "o", "Lie\u00b7be", ",", "schal\u00b7let", "mei\u00b7ne", "Lau\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "FM", "NN", "$,", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Die dem Plato, den sie sich erlas,", "tokens": ["Die", "dem", "Pla\u00b7to", ",", "den", "sie", "sich", "er\u00b7las", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NE", "$,", "PRELS", "PPER", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Rosenh\u00fctten am Ili\u00dfus baute,", "tokens": ["Ro\u00b7sen\u00b7h\u00fct\u00b7ten", "am", "I\u00b7li\u00b7\u00dfus", "bau\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NE", "VVFIN", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Mit ihm in den H\u00fctten sa\u00df,", "tokens": ["Mit", "ihm", "in", "den", "H\u00fct\u00b7ten", "sa\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Unter Valchiusens Lorbeerschatten,", "tokens": ["Un\u00b7ter", "Val\u00b7chiu\u00b7sens", "Lor\u00b7beer\u00b7schat\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Taumel \u00fcber dich, Petrarca, go\u00df,", "tokens": ["Tau\u00b7mel", "\u00fc\u00b7ber", "dich", ",", "Pe\u00b7trar\u00b7ca", ",", "go\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "APPR", "PPER", "$,", "NE", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Bald am Bache, bald auf bunter Matten,", "tokens": ["Bald", "am", "Ba\u00b7che", ",", "bald", "auf", "bun\u00b7ter", "Mat\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$,", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "An den Schwanenbusen schlo\u00df.", "tokens": ["An", "den", "Schwa\u00b7nen\u00b7bu\u00b7sen", "schlo\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Die der Seele neue Fl\u00fcgel leihet,", "tokens": ["Die", "der", "See\u00b7le", "neu\u00b7e", "Fl\u00fc\u00b7gel", "lei\u00b7het", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Sie in heilge Phantasien wiegt,", "tokens": ["Sie", "in", "heil\u00b7ge", "Phan\u00b7ta\u00b7si\u00b7en", "wiegt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "--+-+---+", "measure": "anapaest.init"}, "line.3": {"text": "Zum Genu\u00df der G\u00f6tterfreuden weihet,", "tokens": ["Zum", "Ge\u00b7nu\u00df", "der", "G\u00f6t\u00b7ter\u00b7freu\u00b7den", "wei\u00b7het", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und mit ihr gen Himmel fliegt.", "tokens": ["Und", "mit", "ihr", "gen", "Him\u00b7mel", "fliegt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Ihr die goldne Himmelsth\u00fcr entschlie\u00dfet,", "tokens": ["Ihr", "die", "gold\u00b7ne", "Him\u00b7mel\u00b7sth\u00fcr", "ent\u00b7schlie\u00b7\u00dfet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Sie dem Engelchor entgegenf\u00fchrt,", "tokens": ["Sie", "dem", "En\u00b7gel\u00b7chor", "ent\u00b7ge\u00b7gen\u00b7f\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Das sie Schwester, und Gespielin gr\u00fc\u00dfet,", "tokens": ["Das", "sie", "Schwes\u00b7ter", ",", "und", "Ge\u00b7spie\u00b7lin", "gr\u00fc\u00b7\u00dfet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "NN", "$,", "KON", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und mit Palmenkronen ziert.", "tokens": ["Und", "mit", "Pal\u00b7men\u00b7kro\u00b7nen", "ziert", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Sorgen, die sich in die Seele stahlen,", "tokens": ["Sor\u00b7gen", ",", "die", "sich", "in", "die", "See\u00b7le", "stah\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PRF", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Mit der Wurzel aus der Seele rei\u00dft,", "tokens": ["Mit", "der", "Wur\u00b7zel", "aus", "der", "See\u00b7le", "rei\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Tausend \u00fcbervolle Taumelschaalen", "tokens": ["Tau\u00b7send", "\u00fc\u00b7ber\u00b7vol\u00b7le", "Tau\u00b7mel\u00b7schaa\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["CARD", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Durch ihr Innerstes ergeu\u00dft.", "tokens": ["Durch", "ihr", "In\u00b7ners\u00b7tes", "er\u00b7geu\u00dft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Jeden Keim des Lasters niederdr\u00fccket,", "tokens": ["Je\u00b7den", "Keim", "des", "Las\u00b7ters", "nie\u00b7der\u00b7dr\u00fc\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Jeden Keim der Tugenden enth\u00fcllt,", "tokens": ["Je\u00b7den", "Keim", "der", "Tu\u00b7gen\u00b7den", "ent\u00b7h\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Sanftmuth, Unschuld in die Herzen blicket,", "tokens": ["Sanft\u00b7muth", ",", "Un\u00b7schuld", "in", "die", "Her\u00b7zen", "bli\u00b7cket", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und der Gottheit Ebenbild.", "tokens": ["Und", "der", "Got\u00b7theit", "E\u00b7ben\u00b7bild", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}, "stanza.7": {"line.1": {"text": "Wie der J\u00fcngling, voll Begeisterungen,", "tokens": ["Wie", "der", "J\u00fcng\u00b7ling", ",", "voll", "Be\u00b7geis\u00b7te\u00b7run\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "ADJD", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "In dem Feuer deines Taumels gl\u00fcht,", "tokens": ["In", "dem", "Feu\u00b7er", "dei\u00b7nes", "Tau\u00b7mels", "gl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Der, an seines M\u00e4dchens Arm geschlungen,", "tokens": ["Der", ",", "an", "sei\u00b7nes", "M\u00e4d\u00b7chens", "Arm", "ge\u00b7schlun\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "APPR", "PPOSAT", "NN", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Dich auf Purpurwolken sieht!", "tokens": ["Dich", "auf", "Pur\u00b7pur\u00b7wol\u00b7ken", "sieht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Wonnebilder gleiten, auf den Schwingen", "tokens": ["Won\u00b7ne\u00b7bil\u00b7der", "glei\u00b7ten", ",", "auf", "den", "Schwin\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVINF", "$,", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Der Entz\u00fcckung, durch den Hayn dahin,", "tokens": ["Der", "Ent\u00b7z\u00fc\u00b7ckung", ",", "durch", "den", "Hayn", "da\u00b7hin", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "ART", "NN", "PAV", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Engel, die zu goldnen Harfen singen,", "tokens": ["En\u00b7gel", ",", "die", "zu", "gold\u00b7nen", "Har\u00b7fen", "sin\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Kr\u00e4nzen seine Sch\u00e4ferin.", "tokens": ["Kr\u00e4n\u00b7zen", "sei\u00b7ne", "Sch\u00e4\u00b7fe\u00b7rin", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Alles freuet sich des G\u00f6tterkindes,", "tokens": ["Al\u00b7les", "freu\u00b7et", "sich", "des", "G\u00f6t\u00b7ter\u00b7kin\u00b7des", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "S\u00fc\u00dfer lispelt jedes Buchenblat,", "tokens": ["S\u00fc\u00b7\u00dfer", "lis\u00b7pelt", "je\u00b7des", "Bu\u00b7chen\u00b7blat", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Milder weht der Hauch des Abendwindes,", "tokens": ["Mil\u00b7der", "weht", "der", "Hauch", "des", "Ab\u00b7end\u00b7win\u00b7des", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Der ihr Haar gef\u00e4chelt hat.", "tokens": ["Der", "ihr", "Haar", "ge\u00b7f\u00e4\u00b7chelt", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVPP", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "G\u00f6tter tanzen mit ihr durch die Fluren,", "tokens": ["G\u00f6t\u00b7ter", "tan\u00b7zen", "mit", "ihr", "durch", "die", "Flu\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Seelge Geister folgen jedem Schritt,", "tokens": ["Seel\u00b7ge", "Geis\u00b7ter", "fol\u00b7gen", "je\u00b7dem", "Schritt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Neue Blumen wehen in den Spuren,", "tokens": ["Neu\u00b7e", "Blu\u00b7men", "we\u00b7hen", "in", "den", "Spu\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Die das holde M\u00e4dchen tritt.", "tokens": ["Die", "das", "hol\u00b7de", "M\u00e4d\u00b7chen", "tritt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Heller wird die Luft um ihre Wangen,", "tokens": ["Hel\u00b7ler", "wird", "die", "Luft", "um", "ih\u00b7re", "Wan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Von den Engelharfen, welche nie", "tokens": ["Von", "den", "En\u00b7gel\u00b7har\u00b7fen", ",", "wel\u00b7che", "nie"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "ADV"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "S\u00fc\u00dfer in den Lauben Gottes klangen,", "tokens": ["S\u00fc\u00b7\u00dfer", "in", "den", "Lau\u00b7ben", "Got\u00b7tes", "klan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Str\u00f6mt ein Strom von Melodie.", "tokens": ["Str\u00f6mt", "ein", "Strom", "von", "Me\u00b7lo\u00b7die", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Liebe, Mutter holder Schw\u00e4rmereyen,", "tokens": ["Lie\u00b7be", ",", "Mut\u00b7ter", "hol\u00b7der", "Schw\u00e4r\u00b7me\u00b7re\u00b7yen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Die das Herz zu Gott zu heben wei\u00df,", "tokens": ["Die", "das", "Herz", "zu", "Gott", "zu", "he\u00b7ben", "wei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "APPR", "NN", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "L\u00e4chle mir den Wintermond zum Mayen,", "tokens": ["L\u00e4ch\u00b7le", "mir", "den", "Win\u00b7ter\u00b7mond", "zum", "Ma\u00b7yen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Streue Blumen auf das Eis.", "tokens": ["Streu\u00b7e", "Blu\u00b7men", "auf", "das", "Eis", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Welch ein Himmel um mich! Laura wallet", "tokens": ["Welch", "ein", "Him\u00b7mel", "um", "mich", "!", "Lau\u00b7ra", "wal\u00b7let"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PIAT", "ART", "NN", "APPR", "PPER", "$.", "NE", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Durch die wei\u00dfbereifeten Alleen.", "tokens": ["Durch", "die", "wei\u00df\u00b7be\u00b7rei\u00b7fe\u00b7ten", "Al\u00b7leen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Heil mir! Aus den todten Wipfeln schallet", "tokens": ["Heil", "mir", "!", "Aus", "den", "tod\u00b7ten", "Wip\u00b7feln", "schal\u00b7let"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "$.", "APPR", "ART", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "\u00dcberirrdisches Get\u00f6n.", "tokens": ["\u00dc\u00b7be\u00b7rirr\u00b7di\u00b7sches", "Ge\u00b7t\u00f6n", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "V\u00f6gelchen erwachen aus dem Schlafe,", "tokens": ["V\u00f6\u00b7gel\u00b7chen", "er\u00b7wa\u00b7chen", "aus", "dem", "Schla\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Kr\u00e4uter keimen auf, der West erwacht,", "tokens": ["Kr\u00e4u\u00b7ter", "kei\u00b7men", "auf", ",", "der", "West", "er\u00b7wacht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PTKVZ", "$,", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Durch die bunten Kr\u00e4uter h\u00fcpfen Schafe,", "tokens": ["Durch", "die", "bun\u00b7ten", "Kr\u00e4u\u00b7ter", "h\u00fcp\u00b7fen", "Scha\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Alles freut sich, alles lacht.", "tokens": ["Al\u00b7les", "freut", "sich", ",", "al\u00b7les", "lacht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PRF", "$,", "PIS", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.15": {"line.1": {"text": "Sie verl\u00e4\u00dft, mir einen Gru\u00df zu nicken,", "tokens": ["Sie", "ver\u00b7l\u00e4\u00dft", ",", "mir", "ei\u00b7nen", "Gru\u00df", "zu", "ni\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Itzt die Lindeng\u00e4nge, wo sie schlich.", "tokens": ["Itzt", "die", "Lin\u00b7den\u00b7g\u00e4n\u00b7ge", ",", "wo", "sie", "schlich", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "PWAV", "PPER", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Welche Himmelsfreude, welch Entz\u00fccken,", "tokens": ["Wel\u00b7che", "Him\u00b7mels\u00b7freu\u00b7de", ",", "