,", "tokens": ["Zum", "Ge\u00b7nu\u00df", "der", "G\u00f6t\u00b7ter\u00b7freu\u00b7den", "wei\u00b7het", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und mit ihr gen Himmel fliegt.", "tokens": ["Und", "mit", "ihr", "gen", "Him\u00b7mel", "fliegt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.19": {"line.1": {"text": "Ihr die goldne Himmelsth\u00fcr entschlie\u00dfet,", "tokens": ["Ihr", "die", "gold\u00b7ne", "Him\u00b7mel\u00b7sth\u00fcr", "ent\u00b7schlie\u00b7\u00dfet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Sie dem Engelchor entgegenf\u00fchrt,", "tokens": ["Sie", "dem", "En\u00b7gel\u00b7chor", "ent\u00b7ge\u00b7gen\u00b7f\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Das sie Schwester, und Gespielin gr\u00fc\u00dfet,", "tokens": ["Das", "sie", "Schwes\u00b7ter", ",", "und", "Ge\u00b7spie\u00b7lin", "gr\u00fc\u00b7\u00dfet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "NN", "$,", "KON", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und mit Palmenkronen ziert.", "tokens": ["Und", "mit", "Pal\u00b7men\u00b7kro\u00b7nen", "ziert", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.20": {"line.1": {"text": "Sorgen, die sich in die Seele stahlen,", "tokens": ["Sor\u00b7gen", ",", "die", "sich", "in", "die", "See\u00b7le", "stah\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PRF", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Mit der Wurzel aus der Seele rei\u00dft,", "tokens": ["Mit", "der", "Wur\u00b7zel", "aus", "der", "See\u00b7le", "rei\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Tausend \u00fcbervolle Taumelschaalen", "tokens": ["Tau\u00b7send", "\u00fc\u00b7ber\u00b7vol\u00b7le", "Tau\u00b7mel\u00b7schaa\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["CARD", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Durch ihr Innerstes ergeu\u00dft.", "tokens": ["Durch", "ihr", "In\u00b7ners\u00b7tes", "er\u00b7geu\u00dft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.21": {"line.1": {"text": "Jeden Keim des Lasters niederdr\u00fccket,", "tokens": ["Je\u00b7den", "Keim", "des", "Las\u00b7ters", "nie\u00b7der\u00b7dr\u00fc\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Jeden Keim der Tugenden enth\u00fcllt,", "tokens": ["Je\u00b7den", "Keim", "der", "Tu\u00b7gen\u00b7den", "ent\u00b7h\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Sanftmuth, Unschuld in die Herzen blicket,", "tokens"