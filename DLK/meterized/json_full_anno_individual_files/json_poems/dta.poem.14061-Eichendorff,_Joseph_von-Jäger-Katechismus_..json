{"dta.poem.14061": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "J\u00e4ger-Katechismus .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Was tragt ihr H\u00f6rner an der Seite,", "tokens": ["Was", "tragt", "ihr", "H\u00f6r\u00b7ner", "an", "der", "Sei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPOSAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn ihr des Hornes Sinn verga\u00dft,", "tokens": ["Wenn", "ihr", "des", "Hor\u00b7nes", "Sinn", "ver\u00b7ga\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn's euch nicht selbst lockt in die Weite,", "tokens": ["Wenn's", "euch", "nicht", "selbst", "lockt", "in", "die", "Wei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ADV", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Wie ihr vom Berg' fr\u00fchmorgens blast?", "tokens": ["Wie", "ihr", "vom", "Ber\u00b7g'", "fr\u00fch\u00b7mor\u00b7gens", "blast", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPRART", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}}}}