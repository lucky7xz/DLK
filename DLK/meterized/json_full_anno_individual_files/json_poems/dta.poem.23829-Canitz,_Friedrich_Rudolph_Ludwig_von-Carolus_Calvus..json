{"dta.poem.23829": {"metadata": {"author": {"name": "Canitz, Friedrich Rudolph Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "Carolus Calvus.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1700", "urn": "urn:nbn:de:kobv:b4-200905197532", "language": ["de:0.99"], "booktitle": "[Canitz, Friedrich Rudolph Ludwig von]: Neben-Stunden Unterschiedener Gedichte. [Hrsg. v. Joachim Lange]. Berlin, 1700."}, "poem": {"stanza.1": {"line.1": {"text": "Der Him\u0303el l\u00e4\u00dft sich nicht durch langes Unrecht h\u00f6hnen/", "tokens": ["Der", "Him\u0303el", "l\u00e4\u00dft", "sich", "nicht", "durch", "lan\u00b7ges", "Un\u00b7recht", "h\u00f6h\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "PTKNEG", "APPR", "ADJA", "NN", "VVINF", "$("], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Ich trat im K\u00e4yserthum dem \u00e4ltern Bruder vor/", "tokens": ["Ich", "trat", "im", "K\u00e4y\u00b7ser\u00b7thum", "dem", "\u00e4l\u00b7tern", "Bru\u00b7der", "vor", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ART", "ADJA", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und nahm das Welsche Reich/ bi\u00df ich von dessen S\u00f6hnen", "tokens": ["Und", "nahm", "das", "Wel\u00b7sche", "Reich", "/", "bi\u00df", "ich", "von", "des\u00b7sen", "S\u00f6h\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$(", "KOUS", "PPER", "APPR", "PRELAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Geschlagen und gejagt/ durch Gifft den Geist verlohr.", "tokens": ["Ge\u00b7schla\u00b7gen", "und", "ge\u00b7jagt", "/", "durch", "Gifft", "den", "Geist", "ver\u00b7lohr", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVPP", "$(", "APPR", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}