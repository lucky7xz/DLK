{"dta.poem.7551": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "50.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Zieh deine Selbheit aus, und an die G\u00f6ttlichkeit!", "tokens": ["Zieh", "dei\u00b7ne", "Selb\u00b7heit", "aus", ",", "und", "an", "die", "G\u00f6tt\u00b7lich\u00b7keit", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PTKVZ", "$,", "KON", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Selbheit ist so eng, die G\u00f6ttlichkeit so weit.", "tokens": ["Die", "Selb\u00b7heit", "ist", "so", "eng", ",", "die", "G\u00f6tt\u00b7lich\u00b7keit", "so", "weit", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "$,", "ART", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Sei selbst! Er selber will, da\u00df selbst du sollest seyn,", "tokens": ["Sei", "selbst", "!", "Er", "sel\u00b7ber", "will", ",", "da\u00df", "selbst", "du", "sol\u00b7lest", "seyn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "$.", "PPER", "ADV", "VMFIN", "$,", "KOUS", "ADV", "PPER", "VMFIN", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df du erkennest selbst, er sei dein Selbst allein.", "tokens": ["Da\u00df", "du", "er\u00b7ken\u00b7nest", "selbst", ",", "er", "sei", "dein", "Selbst", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ADV", "$,", "PPER", "VAFIN", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Erinnre dich daran! du hast es nur vergessen.", "tokens": ["E\u00b7rinn\u00b7re", "dich", "da\u00b7ran", "!", "du", "hast", "es", "nur", "ver\u00b7ges\u00b7sen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PAV", "$.", "PPER", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "La\u00df dich erinnern! stets erinnert er dich dessen.", "tokens": ["La\u00df", "dich", "e\u00b7rin\u00b7nern", "!", "stets", "e\u00b7rin\u00b7nert", "er", "dich", "des\u00b7sen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VVINF", "$.", "ADV", "VVFIN", "PPER", "PRF", "PDS", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Wenn du ihn h\u00f6ren willst in dir, mu\u00dft du nur schweigen;", "tokens": ["Wenn", "du", "ihn", "h\u00f6\u00b7ren", "willst", "in", "dir", ",", "mu\u00dft", "du", "nur", "schwei\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVINF", "VMFIN", "APPR", "PPER", "$,", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So spricht er laut: Du warst, sollst seyn und bist mein eigen.", "tokens": ["So", "spricht", "er", "laut", ":", "Du", "warst", ",", "sollst", "seyn", "und", "bist", "mein", "ei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "$.", "PPER", "VAFIN", "$,", "VMFIN", "VAINF", "KON", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}