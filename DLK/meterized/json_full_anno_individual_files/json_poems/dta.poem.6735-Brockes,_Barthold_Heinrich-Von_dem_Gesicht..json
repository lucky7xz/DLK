{"dta.poem.6735": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von dem Gesicht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Auf! lasset uns mit Ernst das Auge, das so sch\u00f6n,", "tokens": ["Auf", "!", "las\u00b7set", "uns", "mit", "Ernst", "das", "Au\u00b7ge", ",", "das", "so", "sch\u00f6n", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "$.", "VVFIN", "PPER", "APPR", "NE", "ART", "NN", "$,", "PRELS", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das GOTT so Weisheits-voll gebildet hat, besehn!", "tokens": ["Das", "GoTT", "so", "Weis\u00b7heits\u00b7voll", "ge\u00b7bil\u00b7det", "hat", ",", "be\u00b7sehn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "VVPP", "VAFIN", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Es ist recht wie ein Glas durchsichtig, rein,", "tokens": ["Es", "ist", "recht", "wie", "ein", "Glas", "durch\u00b7sich\u00b7tig", ",", "rein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "KOKOM", "ART", "NN", "ADJD", "$,", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ja eigentlich bereitet zum Gesicht.", "tokens": ["Ja", "ei\u00b7gent\u00b7lich", "be\u00b7rei\u00b7tet", "zum", "Ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADV", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Es h\u00fcllen es die Augen-Lieder ein,", "tokens": ["Es", "h\u00fcl\u00b7len", "es", "die", "Au\u00b7gen\u00b7Lie\u00b7der", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Es dringt darein offt mehr, offt minder Licht.", "tokens": ["Es", "dringt", "da\u00b7rein", "offt", "mehr", ",", "offt", "min\u00b7der", "Licht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PAV", "ADV", "ADV", "$,", "ADV", "ADV", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Sechs Muskeln dienen ihm, wodurch es sich bewegt,", "tokens": ["Sechs", "Mus\u00b7keln", "die\u00b7nen", "ihm", ",", "wo\u00b7durch", "es", "sich", "be\u00b7wegt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "PPER", "$,", "PWAV", "PPER", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Es macht sich platt, verl\u00e4ngt sich, hellt sich, sencket,", "tokens": ["Es", "macht", "sich", "platt", ",", "ver\u00b7l\u00e4ngt", "sich", ",", "hellt", "sich", ",", "sen\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PTKVZ", "$,", "VVFIN", "PRF", "$,", "VVFIN", "PRF", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Und f\u00fcgt sich so, \u00fcm besser zu empfangen", "tokens": ["Und", "f\u00fcgt", "sich", "so", ",", "\u00fcm", "bes\u00b7ser", "zu", "emp\u00b7fan\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "ADV", "$,", "ADV", "ADJD", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Die Z\u00fcge, die bald starck, bald schwach aus C\u00f6rpern gehn,", "tokens": ["Die", "Z\u00fc\u00b7ge", ",", "die", "bald", "starck", ",", "bald", "schwach", "aus", "C\u00f6r\u00b7pern", "gehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "ADJD", "$,", "ADV", "ADJD", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Die wir verlangen", "tokens": ["Die", "wir", "ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["ART", "PPER", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.12": {"text": "Anzusehn.", "tokens": ["An\u00b7zu\u00b7sehn", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-+", "measure": "trochaic.di"}}}}}