{"dta.poem.20556": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "15.  \n  Napoleons Kriegserkl\u00e4rung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1848", "urn": "urn:nbn:de:kobv:b4-25129-1", "language": ["de:0.85", "nl:0.14"], "booktitle": "[N. N.]: Erinnerungen eines freiwilligen reitenden J\u00e4gers aus den Kriegsjahren 1813\u20131815. Berlin, 1848."}, "poem": {"stanza.1": {"line.1": {"text": "Seht dort jenen Kranken,", "tokens": ["Seht", "dort", "je\u00b7nen", "Kran\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PDAT", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Dem die Wangen gl\u00fch\u2019n,", "tokens": ["Dem", "die", "Wan\u00b7gen", "gl\u00fch'n", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Wandeln in Gedanken", "tokens": ["Wan\u00b7deln", "in", "Ge\u00b7dan\u00b7ken"], "token_info": ["word", "word", "word"], "pos": ["NN", "APPR", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Durch die Tuilerien!", "tokens": ["Durch", "die", "Tu\u00b7i\u00b7le\u00b7ri\u00b7en", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.5": {"text": "Wer sah je ihn wanken?", "tokens": ["Wer", "sah", "je", "ihn", "wan\u00b7ken", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "PPER", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "St\u00e4rkstes ward durch ihn.", "tokens": ["St\u00e4rks\u00b7tes", "ward", "durch", "ihn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "APPR", "PPER", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "V\u00f6lker, vor dem Franken", "tokens": ["V\u00f6l\u00b7ker", ",", "vor", "dem", "Fran\u00b7ken"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "APPR", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Lasst den Hut uns ziehn!", "tokens": ["Lasst", "den", "Hut", "uns", "ziehn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPER", "VVINF", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Pl\u00f6tzlich hebt er r\u00fcstig", "tokens": ["Pl\u00f6tz\u00b7lich", "hebt", "er", "r\u00fcs\u00b7tig"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "ADJD"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Sich, wirft fort die Pein.", "tokens": ["Sich", ",", "wirft", "fort", "die", "Pein", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PRF", "$,", "VVFIN", "PTKVZ", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Spricht zu sich, gern w\u00fc\u00dft\u2019 ich,", "tokens": ["Spricht", "zu", "sich", ",", "gern", "w\u00fc\u00dft'", "ich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PRF", "$,", "ADV", "VVFIN", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Wird treu Habsburg sein?", "tokens": ["Wird", "treu", "Habs\u00b7burg", "sein", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "NN", "VAINF", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Fiel\u2019s ab, dann fast m\u00fc\u00dft\u2019 ich", "tokens": ["Fiel's", "ab", ",", "dann", "fast", "m\u00fc\u00dft'", "ich"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "PTKVZ", "$,", "ADV", "ADV", "VMFIN", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Geben auf den Rhein.", "tokens": ["Ge\u00b7ben", "auf", "den", "Rhein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NE", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Kanzlern \u00fcberlist\u2019 ich", "tokens": ["Kanz\u00b7lern", "\u00fc\u00b7ber\u00b7list'", "ich"], "token_info": ["word", "word", "word"], "pos": ["NN", "VVFIN", "PPER"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Schwerer noch, wie ", "tokens": ["Schwe\u00b7rer", "noch", ",", "wie"], "token_info": ["word", "word", "punct", "word"], "pos": ["NN", "ADV", "$,", "PWAV"], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "Berthier reit' geschwinde", "tokens": ["Bert\u00b7hier", "reit'", "ge\u00b7schwin\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["NE", "VVFIN", "ADJA"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Hin zum Marschallsrath!", "tokens": ["Hin", "zum", "Mar\u00b7schalls\u00b7rath", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Wirke, da\u00df dort z\u00fcnde", "tokens": ["Wir\u00b7ke", ",", "da\u00df", "dort", "z\u00fcn\u00b7de"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "KOUS", "ADV", "ADJA"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Hier Mein Kampfmandat!", "tokens": ["Hier", "Mein", "Kampf\u00b7man\u00b7dat", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Preu\u00dfens Feuerschl\u00fcnde", "tokens": ["Preu\u00b7\u00dfens", "Feu\u00b7er\u00b7schl\u00fcn\u00b7de"], "token_info": ["word", "word"], "pos": ["NE", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Wecken Uns zur That.", "tokens": ["We\u00b7cken", "Uns", "zur", "That", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Breit\u2019re Kriegesgr\u00fcnde", "tokens": ["Breit'\u00b7re", "Krie\u00b7ges\u00b7gr\u00fcn\u00b7de"], "token_info": ["word", "word"], "pos": ["NE", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Drucke der Senat.", "tokens": ["Dru\u00b7cke", "der", "Se\u00b7nat", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Rasch in Stadt und Flecken", "tokens": ["Rasch", "in", "Stadt", "und", "Fle\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Werbt Uns Jung und Alt!", "tokens": ["Werbt", "Uns", "Jung", "und", "Alt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "KON", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Forscht in allen Ecken,", "tokens": ["Forscht", "in", "al\u00b7len", "E\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Me\u00dft nicht erst Gestalt!", "tokens": ["Me\u00dft", "nicht", "erst", "Ge\u00b7stalt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ADV", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Die, so sich verstecken,", "tokens": ["Die", ",", "so", "sich", "ver\u00b7ste\u00b7cken", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "$,", "ADV", "PRF", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Nehmt ihr mit Gewalt!", "tokens": ["Nehmt", "ihr", "mit", "Ge\u00b7walt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Deutschland soll erschrecken.", "tokens": ["Deutschland", "soll", "er\u00b7schre\u00b7cken", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "VVINF", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Ich bin Sieger bald!", "tokens": ["Ich", "bin", "Sie\u00b7ger", "bald", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "ADV", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}}}}