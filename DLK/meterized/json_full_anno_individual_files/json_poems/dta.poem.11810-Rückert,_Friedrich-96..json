{"dta.poem.11810": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "96.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ihr habt euch nun einmal verliebt ins H\u00e4\u00dfliche,", "tokens": ["Ihr", "habt", "euch", "nun", "ein\u00b7mal", "ver\u00b7liebt", "ins", "H\u00e4\u00df\u00b7li\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ADV", "VVPP", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.2": {"text": "Und zur Bewunderung braucht ihr das Gr\u00e4\u00dfliche.", "tokens": ["Und", "zur", "Be\u00b7wun\u00b7de\u00b7rung", "braucht", "ihr", "das", "Gr\u00e4\u00df\u00b7li\u00b7che", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "+--+--+--+--", "measure": "dactylic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich aber will mit Gott das Sch\u00f6ne lieb behalten,", "tokens": ["Ich", "a\u00b7ber", "will", "mit", "Gott", "das", "Sch\u00f6\u00b7ne", "lieb", "be\u00b7hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VMFIN", "APPR", "NN", "ART", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und siegreich seinen Glanz auch noch der Welt entfalten.", "tokens": ["Und", "sieg\u00b7reich", "sei\u00b7nen", "Glanz", "auch", "noch", "der", "Welt", "ent\u00b7fal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PPOSAT", "NN", "ADV", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-",