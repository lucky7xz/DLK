{"dta.poem.10196": {"metadata": {"author": {"name": "Liliencron, Detlev von", "birth": "N.A.", "death": "N.A."}, "title": "( Marschall Niel. )", "genre": "Lyrik", "period": "N.A.", "pub_year": "1883", "urn": "urn:nbn:de:kobv:b4-200905197184", "language": ["de:0.99"], "booktitle": "Liliencron, Detlev von: Adjutantenritte und andere Gedichte. Leipzig, [1883]."}, "poem": {"stanza.1": {"line.1": {"text": "Die gro\u00dfe gelbe Rose ruhte schwer", "tokens": ["Die", "gro\u00b7\u00dfe", "gel\u00b7be", "Ro\u00b7se", "ruh\u00b7te", "schwer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN", "VVFIN", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Auf schwarzem Marmorsarg in Marmorhallen.", "tokens": ["Auf", "schwar\u00b7zem", "Mar\u00b7mor\u00b7sarg", "in", "Mar\u00b7mo\u00b7rhal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wess\u2019 Hand sie brach und wer sie trug anher,", "tokens": ["Wess'", "Hand", "sie", "brach", "und", "wer", "sie", "trug", "an\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "NN", "PPER", "VVFIN", "KON", "PWS", "PPER", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Auch wer die Leiche war, ist mir entfallen.", "tokens": ["Auch", "wer", "die", "Lei\u00b7che", "war", ",", "ist", "mir", "ent\u00b7fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PWS", "ART", "NN", "VAFIN", "$,", "VAFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Es schlief der Sarg, von Blatt und Blumen leer,", "tokens": ["Es", "schlief", "der", "Sarg", ",", "von", "Blatt", "und", "Blu\u00b7men", "leer", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "APPR", "NN", "KON", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Im D\u00e4mmer, eine Sphinx, auf L\u00f6wenkrallen.", "tokens": ["Im", "D\u00e4m\u00b7mer", ",", "ei\u00b7ne", "Sphinx", ",", "auf", "L\u00f6\u00b7wen\u00b7kral\u00b7len", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "ART", "NN", "$,", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Der Abendw\u00f6lkchen lichtgeflocktes Heer", "tokens": ["Der", "Ab\u00b7end\u00b7w\u00f6lk\u00b7chen", "licht\u00b7ge\u00b7flock\u00b7tes", "Heer"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Entstieg dem Meere, rot wie Blutkorallen.", "tokens": ["Ent\u00b7stieg", "dem", "Mee\u00b7re", ",", "rot", "wie", "Blut\u00b7ko\u00b7ral\u00b7len", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "ADJD", "KOKOM", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}