{"textgrid.poem.40313": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "1L: Was ist die Freude, das Gl\u00fcck, das Leben", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was ist die Freude, das Gl\u00fcck, das Leben", "tokens": ["Was", "ist", "die", "Freu\u00b7de", ",", "das", "Gl\u00fcck", ",", "das", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PWS", "VAFIN", "ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "ohne den Traum von Hoffnung und von Ruhm!", "tokens": ["oh\u00b7ne", "den", "Traum", "von", "Hoff\u00b7nung", "und", "von", "Ruhm", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Eine Stra\u00dfe, endlos, \u00f6d, uneben:", "tokens": ["Ei\u00b7ne", "Stra\u00b7\u00dfe", ",", "end\u00b7los", ",", "\u00f6d", ",", "un\u00b7e\u00b7ben", ":"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "$,", "ADJD", "$,", "ADJD", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "immer m\u00fcder wird dein Pilgerthum.", "tokens": ["im\u00b7mer", "m\u00fc\u00b7der", "wird", "dein", "Pil\u00b7ger\u00b7thum", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Gieb mir Melodieen \u2013 oh nur eine,", "tokens": ["Gieb", "mir", "Me\u00b7lo\u00b7di\u00b7e\u00b7en", "\u2013", "oh", "nur", "ei\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "NN", "$(", "ADJD", "ADV", "ART", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "wiege das Herz in Tr\u00e4ume, wenn es schreit,", "tokens": ["wie\u00b7ge", "das", "Herz", "in", "Tr\u00e4u\u00b7me", ",", "wenn", "es", "schreit", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "NN", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "und dir wachsen ewige Marmorsteine", "tokens": ["und", "dir", "wach\u00b7sen", "e\u00b7wi\u00b7ge", "Mar\u00b7mor\u00b7stei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ADJA", "NN"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "aus der Asche der Vergessenheit!", "tokens": ["aus", "der", "A\u00b7sche", "der", "Ver\u00b7ges\u00b7sen\u00b7heit", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Hoffnung. Ruhm. Was soll ich mich beklagen:", "tokens": ["Hoff\u00b7nung", ".", "Ruhm", ".", "Was", "soll", "ich", "mich", "be\u00b7kla\u00b7gen", ":"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "NN", "$.", "PWS", "VMFIN", "PPER", "PRF", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "ein Diadem zieht straalend vor mir her!", "tokens": ["ein", "Di\u00b7a\u00b7dem", "zieht", "straa\u00b7lend", "vor", "mir", "her", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "APPR", "PPER", "PTKVZ", "$."], "meter": "+-+--+-+-+