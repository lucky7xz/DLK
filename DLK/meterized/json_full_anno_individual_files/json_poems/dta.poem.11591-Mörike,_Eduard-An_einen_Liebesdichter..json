{"dta.poem.11591": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "An einen Liebesdichter.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905193969", "language": ["de:0.99"], "booktitle": "M\u00f6rike, Eduard: Gedichte. Stuttgart, 1838."}, "poem": {"stanza.1": {"line.1": {"text": "Damit er auch von Liebe singe,", "tokens": ["Da\u00b7mit", "er", "auch", "von", "Lie\u00b7be", "sin\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und hebt ein m\u00e4chtig Klagen an,", "tokens": ["Und", "hebt", "ein", "m\u00e4ch\u00b7tig", "Kla\u00b7gen", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJD", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Ruhm ist gro\u00df, die Pein geringe.", "tokens": ["Der", "Ruhm", "ist", "gro\u00df", ",", "die", "Pein", "ge\u00b7rin\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "ART", "NN", "ADJA", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Nun bist du nicht im selben Fall,", "tokens": ["Nun", "bist", "du", "nicht", "im", "sel\u00b7ben", "Fall", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PTKNEG", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und l\u00e4ssest auch Gesang erschallen,", "tokens": ["Und", "l\u00e4s\u00b7sest", "auch", "Ge\u00b7sang", "er\u00b7schal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Obwohl noch keine Nachtigall,", "tokens": ["Ob\u00b7wohl", "noch", "kei\u00b7ne", "Nach\u00b7ti\u00b7gall", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Doch mehr als jene Nachtigallen.", "tokens": ["Doch", "mehr", "als", "je\u00b7ne", "Nach\u00b7ti\u00b7gal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "KOUS", "PDAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Was ist denn wohl der Unterschied,", "tokens": ["Was", "ist", "denn", "wohl", "der", "Un\u00b7ter\u00b7schied", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Freund, zwischen dir und zwischen jenen?", "tokens": ["Freund", ",", "zwi\u00b7schen", "dir", "und", "zwi\u00b7schen", "je\u00b7nen", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "PPER", "KON", "APPR", "PDAT", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "\u2014 Sie singen froh ein traurig Lied,", "tokens": ["Sie", "sin\u00b7gen", "froh", "ein", "trau\u00b7rig", "Lied", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "ADJD", "ART", "ADJD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und du ein fr\u00f6hlichs unter Thr\u00e4nen.", "tokens": ["Und", "du", "ein", "fr\u00f6h\u00b7lichs", "un\u00b7ter", "Thr\u00e4\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ART", "ADJA", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}