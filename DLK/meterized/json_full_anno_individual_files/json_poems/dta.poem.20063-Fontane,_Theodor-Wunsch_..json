{"dta.poem.20063": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Wunsch .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1851", "urn": "urn:nbn:de:kobv:b4-200905191321", "language": ["de:0.99"], "booktitle": "Fontane, Theodor: Gedichte. Berlin, 1851."}, "poem": {"stanza.1": {"line.1": {"text": "Ich wollte, da\u00df in Sturmesnacht", "tokens": ["Ich", "woll\u00b7te", ",", "da\u00df", "in", "Stur\u00b7mes\u00b7nacht"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VMFIN", "$,", "KOUS", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Mutter mich zur Welt gebracht,", "tokens": ["Die", "Mut\u00b7ter", "mich", "zur", "Welt", "ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df auf das Blitzen rings umher", "tokens": ["Da\u00df", "auf", "das", "Blit\u00b7zen", "rings", "um\u00b7her"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "ADV", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mein erster Blick gefallen w\u00e4r\u2019.", "tokens": ["Mein", "ers\u00b7ter", "Blick", "ge\u00b7fal\u00b7len", "w\u00e4r'", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich wollte, da\u00df sie nackt und blo\u00df", "tokens": ["Ich", "woll\u00b7te", ",", "da\u00df", "sie", "nackt", "und", "blo\u00df"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "$,", "KOUS", "PPER", "ADJD", "KON", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gebettet mich in Laub und Moos,", "tokens": ["Ge\u00b7bet\u00b7tet", "mich", "in", "Laub", "und", "Moos", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "PPER", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df Sturm und Donner um die Wett\u2019", "tokens": ["Da\u00df", "Sturm", "und", "Don\u00b7ner", "um", "die", "Wett'"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "KON", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mein Wiegenlied gesungen h\u00e4tt\u2019.", "tokens": ["Mein", "Wie\u00b7gen\u00b7lied", "ge\u00b7sun\u00b7gen", "h\u00e4tt'", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ich wollte, da\u00df der Hirsch im Tann", "tokens": ["Ich", "woll\u00b7te", ",", "da\u00df", "der", "Hirsch", "im", "Tann"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "$,", "KOUS", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mein Spielgeno\u00df als Knabe dann,", "tokens": ["Mein", "Spiel\u00b7ge\u00b7no\u00df", "als", "Kna\u00b7be", "dann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KOUS", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df, \u00fcber mir, der Sterne Heer", "tokens": ["Da\u00df", ",", "\u00fc\u00b7ber", "mir", ",", "der", "Ster\u00b7ne", "Heer"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "$,", "APPR", "PPER", "$,", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Bibel mein gewesen w\u00e4r\u2019.", "tokens": ["Die", "Bi\u00b7bel", "mein", "ge\u00b7we\u00b7sen", "w\u00e4r'", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "VAPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Das Auge hell, im Arme Mark,", "tokens": ["Das", "Au\u00b7ge", "hell", ",", "im", "Ar\u00b7me", "Mark", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Frisch wie der Quell, wie Eichen stark,", "tokens": ["Frisch", "wie", "der", "Quell", ",", "wie", "Ei\u00b7chen", "stark", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "ART", "NN", "$,", "PWAV", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So w\u00e4r\u2019 ich in das Leben dann", "tokens": ["So", "w\u00e4r'", "ich", "in", "das", "Le\u00b7ben", "dann"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "ART", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Getreten als ein ganzer Mann.", "tokens": ["Ge\u00b7tre\u00b7ten", "als", "ein", "gan\u00b7zer", "Mann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Im Busen lebte mir die Kraft,", "tokens": ["Im", "Bu\u00b7sen", "leb\u00b7te", "mir", "die", "Kraft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Thaten statt der Lieder schafft,", "tokens": ["Die", "Tha\u00b7ten", "statt", "der", "Lie\u00b7der", "schafft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nicht l\u00e4nger s\u00e4\u00df der gute Will\u2019", "tokens": ["Nicht", "l\u00e4n\u00b7ger", "s\u00e4\u00df", "der", "gu\u00b7te", "Will'"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADJD", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Im Winkel drinnen, fromm und still.", "tokens": ["Im", "Win\u00b7kel", "drin\u00b7nen", ",", "fromm", "und", "still", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "O w\u00e4r\u2019 ich stark! nah ist der Streit,", "tokens": ["O", "w\u00e4r'", "ich", "stark", "!", "nah", "ist", "der", "Streit", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "ADJD", "$.", "ADJD", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und ganze M\u00e4nner heischt die Zeit; \u2014", "tokens": ["Und", "gan\u00b7ze", "M\u00e4n\u00b7ner", "heischt", "die", "Zeit", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich wollte, da\u00df in Sturmesnacht", "tokens": ["Ich", "woll\u00b7te", ",", "da\u00df", "in", "Stur\u00b7mes\u00b7nacht"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VMFIN", "$,", "KOUS", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Mutter mich zur Welt gebracht.", "tokens": ["Die", "Mut\u00b7ter", "mich", "zur", "Welt", "ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}