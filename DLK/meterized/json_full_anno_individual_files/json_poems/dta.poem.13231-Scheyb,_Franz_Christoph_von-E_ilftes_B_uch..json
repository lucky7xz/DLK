{"dta.poem.13231": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "E ilftes  B uch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Drauf fieng er selber an, und sprach auf diese Wei\u00df:", "tokens": ["Drauf", "fi\u00b7eng", "er", "sel\u00b7ber", "an", ",", "und", "sprach", "auf", "die\u00b7se", "Wei\u00df", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ADV", "PTKVZ", "$,", "KON", "VVFIN", "APPR", "PDAT", "NN", "$."], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "50\u201dGemahlin! K\u00f6niginn! der L\u00e4nder Trost und Prei\u00df!", "tokens": ["\"", "Ge\u00b7mah\u00b7lin", "!", "K\u00f6\u00b7ni\u00b7ginn", "!", "der", "L\u00e4n\u00b7der", "Trost", "und", "Prei\u00df", "!"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "NN", "$.", "NN", "$.", "ART", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "\u201edie Welt ist \u00fcberzeugt, wie man um deine Rechte", "tokens": ["\u201e", "die", "Welt", "ist", "\u00fc\u00b7berz\u00b7eugt", ",", "wie", "man", "um", "dei\u00b7ne", "Rech\u00b7te"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["$(", "ART", "NN", "VAFIN", "VVPP", "$,", "PWAV", "PIS", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "\u201eschon in das f\u00fcnfte Jahr mit Blut-vergiessen fechte.", "tokens": ["\u201e", "schon", "in", "das", "f\u00fcnf\u00b7te", "Jahr", "mit", "Blut\u00b7ver\u00b7gies\u00b7sen", "fech\u00b7te", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "APPR", "ART", "ADJA", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "\u201ewir sehen auch wie GOtt den Krieges-Stab gef\u00fchrt;", "tokens": ["\u201e", "wir", "se\u00b7hen", "auch", "wie", "Gott", "den", "Krie\u00b7ge\u00b7sStab", "ge\u00b7f\u00fchrt", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "ADV", "KOKOM", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "\u201ewie sich der Feinde Rath in seinem Schlu\u00df geirrt;", "tokens": ["\u201e", "wie", "sich", "der", "Fein\u00b7de", "Rath", "in", "sei\u00b7nem", "Schlu\u00df", "ge\u00b7irrt", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "PRF", "ART", "NN", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "55\u201dUnd wem ists unbekannt, wie wir durch unsre Waffen", "tokens": ["\"", "Und", "wem", "ists", "un\u00b7be\u00b7kannt", ",", "wie", "wir", "durch", "uns\u00b7re", "Waf\u00b7fen"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["$(", "KON", "PWS", "VAFIN", "ADJD", "$,", "PWAV", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "\u201eder Feinde Macht gewu\u00dft die Brustwehr zu verschaffen?", "tokens": ["\u201e", "der", "Fein\u00b7de", "Macht", "ge\u00b7wu\u00dft", "die", "Brust\u00b7wehr", "zu", "ver\u00b7schaf\u00b7fen", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "NN", "VVPP", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "\u201esie stiegen mit Gewalt unvorgesehn\u2019 empor,", "tokens": ["\u201e", "sie", "stie\u00b7gen", "mit", "Ge\u00b7walt", "un\u00b7vor\u00b7ge\u00b7sehn'", "em\u00b7por", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "APPR", "NN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "\u201eund schrieben diesem Thron Ma\u00df und Geseze vor.", "tokens": ["\u201e", "und", "schrie\u00b7ben", "die\u00b7sem", "Thron", "Ma\u00df", "und", "Ge\u00b7se\u00b7ze", "vor", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "VVFIN", "PDAT", "NN", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-++--+-+", "measure": "iambic.hexa.relaxed"}, "line.11": {"text": "\u201ejhr Anzug war ein Strohm, nur nach dem Sieg zu eilen,", "tokens": ["\u201e", "jhr", "An\u00b7zug", "war", "ein", "Strohm", ",", "nur", "nach", "dem", "Sieg", "zu", "ei\u00b7len", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "VAFIN", "ART", "NN", "$,", "ADV", "APPR", "ART", "NN", "PTKZU", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "60\u201dUnd ihrem Willen nach die L\u00e4nder auszutheilen.", "tokens": ["\"", "Und", "ih\u00b7rem", "Wil\u00b7len", "nach", "die", "L\u00e4n\u00b7der", "aus\u00b7zu\u00b7thei\u00b7len", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "PPOSAT", "NN", "APPR", "ART", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "\u201esie sorgten um kein Recht, nur um der Krieger Zahl;", "tokens": ["\u201e", "sie", "sorg\u00b7ten", "um", "kein", "Recht", ",", "nur", "um", "der", "Krie\u00b7ger", "Zahl", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "APPR", "PIAT", "NN", "$,", "ADV", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "\u201edann die Gerechtigkeit kam nicht in ihre Wahl.", "tokens": ["\u201e", "dann", "die", "Ge\u00b7rech\u00b7tig\u00b7keit", "kam", "nicht", "in", "ih\u00b7re", "Wahl", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ART", "NN", "VVFIN", "PTKNEG", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "\u201esie z\u00e4hlten unser Heer, das eigene zu messen,", "tokens": ["\u201e", "sie", "z\u00e4hl\u00b7ten", "un\u00b7ser", "Heer", ",", "das", "ei\u00b7ge\u00b7ne", "zu", "mes\u00b7sen", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PPOSAT", "NN", "$,", "ART", "ADJA", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "\u201eund hatten des Geleits, das GOtt uns gab, vergessen.", "tokens": ["\u201e", "und", "hat\u00b7ten", "des", "Ge\u00b7leits", ",", "das", "Gott", "uns", "gab", ",", "ver\u00b7ges\u00b7sen", "."], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["$(", "KON", "VAFIN", "ART", "NN", "$,", "ART", "NN", "PPER", "VVFIN", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "65\u201dSie rieffen in der Welt nichts als den Frieden aus;", "tokens": ["\"", "Sie", "rief\u00b7fen", "in", "der", "Welt", "nichts", "als", "den", "Frie\u00b7den", "aus", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "APPR", "ART", "NN", "PIS", "KOKOM", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "\u201everbargen den Entschlu\u00df, da\u00df er auf unserm Haus,", "tokens": ["\u201e", "ver\u00b7bar\u00b7gen", "den", "Ent\u00b7schlu\u00df", ",", "da\u00df", "er", "auf", "un\u00b7serm", "Haus", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "ART", "NN", "$,", "KOUS", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "\u201eauf dessen Untergang und Schutt sich sollte gr\u00fcnden,", "tokens": ["\u201e", "auf", "des\u00b7sen", "Un\u00b7ter\u00b7gang", "und", "Schutt", "sich", "soll\u00b7te", "gr\u00fcn\u00b7den", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "PRELAT", "NN", "KON", "NN", "PRF", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "\u201ewann auch die Krieges-Glut das Erd-Rund sollt entz\u00fcnden.", "tokens": ["\u201e", "wann", "auch", "die", "Krie\u00b7ge\u00b7sGlut", "das", "Erd\u00b7Rund", "sollt", "ent\u00b7z\u00fcn\u00b7den", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "ADV", "ART", "NN", "ART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}