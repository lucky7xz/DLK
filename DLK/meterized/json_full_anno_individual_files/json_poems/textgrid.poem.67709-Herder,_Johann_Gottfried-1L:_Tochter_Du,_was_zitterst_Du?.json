{"textgrid.poem.67709": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1L: Tochter Du, was zitterst Du?", "genre": "verse", "period": "N.A.", "pub_year": 1771, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Tochter Du, was zitterst Du?", "tokens": ["Toch\u00b7ter", "Du", ",", "was", "zit\u00b7terst", "Du", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "PWS", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Was sinkest Du?", "tokens": ["Was", "sin\u00b7kest", "Du", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Schweige, leide,", "tokens": ["Schwei\u00b7ge", ",", "lei\u00b7de", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Hoffe, meide,", "tokens": ["Hof\u00b7fe", ",", "mei\u00b7de", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Nicht verzag,", "tokens": ["Nicht", "ver\u00b7zag", ","], "token_info": ["word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Nicht klag!", "tokens": ["Nicht", "klag", "!"], "token_info": ["word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.7": {"text": "Wahrheit kommt alle Tag'.", "tokens": ["Wahr\u00b7heit", "kommt", "al\u00b7le", "Tag'", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PIAT", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Du suchtest ja ein Wunderland,", "tokens": ["Du", "such\u00b7test", "ja", "ein", "Wun\u00b7der\u00b7land", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wo sonder Tand", "tokens": ["Wo", "son\u00b7der", "Tand"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "PIAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Und sonder Schall", "tokens": ["Und", "son\u00b7der", "Schall"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Man ist und hat", "tokens": ["Man", "ist", "und", "hat"], "token_info": ["word", "word", "word", "word"], "pos": ["PIS", "VAFIN", "KON", "VAFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Und Gr\u00f6\u00dfe suchet, That,", "tokens": ["Und", "Gr\u00f6\u00b7\u00dfe", "su\u00b7chet", ",", "That", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "$,", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Und strebt, statt Schein,", "tokens": ["Und", "strebt", ",", "statt", "Schein", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUI", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Zu sein.", "tokens": ["Zu", "sein", "."], "token_info": ["word", "word", "punct"], "pos": ["PTKZU", "VAINF", "$."], "meter": "-+", "measure": "iambic.single"}}, "stanza.3": {"line.1": {"text": "Dies Land ist \u00fcberall f\u00fcr Den, der's in sich hat.", "tokens": ["Dies", "Land", "ist", "\u00fc\u00b7be\u00b7rall", "f\u00fcr", "Den", ",", "der's", "in", "sich", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "VAFIN", "ADV", "APPR", "NE", "$,", "PRELS", "APPR", "PRF", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und hier sieh Deine Statt:", "tokens": ["Und", "hier", "sieh", "Dei\u00b7ne", "Statt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVIMP", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sollst jene Mummereien bekehren,", "tokens": ["Sollst", "je\u00b7ne", "Mum\u00b7me\u00b7rei\u00b7en", "be\u00b7keh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PDAT", "NN", "VVINF", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die Schatten hellen, Wahrheit lehren;", "tokens": ["Die", "Schat\u00b7ten", "hel\u00b7len", ",", "Wahr\u00b7heit", "leh\u00b7ren", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Sollst wandeln hier dies Zauberthal", "tokens": ["Sollst", "wan\u00b7deln", "hier", "dies", "Zau\u00b7bert\u00b7hal"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "PDS", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Voll Mondesschein in Wahrheit \u00fcberall,", "tokens": ["Voll", "Mon\u00b7des\u00b7schein", "in", "Wahr\u00b7heit", "\u00fc\u00b7be\u00b7rall", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "APPR", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "In Sonnenstrahl.", "tokens": ["In", "Son\u00b7nen\u00b7strahl", "."], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Auf, fasse Dich,", "tokens": ["Auf", ",", "fas\u00b7se", "Dich", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["APPR", "$,", "VVFIN", "PPER", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Ermanne Dich!", "tokens": ["Er\u00b7man\u00b7ne", "Dich", "!"], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Es wird Dir schwer sein,", "tokens": ["Es", "wird", "Dir", "schwer", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADJD", "VAINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Mu\u00dft lange sein allein,", "tokens": ["Mu\u00dft", "lan\u00b7ge", "sein", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PPOSAT", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Verkennen Dich lassen in falschem Schein,", "tokens": ["Ver\u00b7ken\u00b7nen", "Dich", "las\u00b7sen", "in", "fal\u00b7schem", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Sehn Deine W\u00fcnsche mi\u00dfgedeihn.", "tokens": ["Sehn", "Dei\u00b7ne", "W\u00fcn\u00b7sche", "mi\u00df\u00b7ge\u00b7deihn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Auf, fasse Dich,", "tokens": ["Auf", ",", "fas\u00b7se", "Dich", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["APPR", "$,", "VVFIN", "PPER", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Muthig f\u00fchl Dich,", "tokens": ["Mut\u00b7hig", "f\u00fchl", "Dich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Du edle Jungfrau, gro\u00df und frei", "tokens": ["Du", "ed\u00b7le", "Jung\u00b7frau", ",", "gro\u00df", "und", "frei"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "ADJA", "NN", "$,", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und fest und treu,", "tokens": ["Und", "fest", "und", "treu", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Den Blick so edel vor sich hin,", "tokens": ["Den", "Blick", "so", "e\u00b7del", "vor", "sich", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "APPR", "PRF", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mit reinem Sinn und stillem Sinn,", "tokens": ["Mit", "rei\u00b7nem", "Sinn", "und", "stil\u00b7lem", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Auf, fasse Dich", "tokens": ["Auf", ",", "fas\u00b7se", "Dich"], "token_info": ["word", "punct", "word", "word"], "pos": ["APPR", "$,", "VVFIN", "PPER"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "M\u00e4nnlich!", "tokens": ["M\u00e4nn\u00b7lich", "!"], "token_info": ["word", "punct"], "pos": ["ADJD", "$."], "meter": "+-", "measure": "trochaic.single"}}, "stanza.6": {"line.1": {"text": "Schweige, leide,", "tokens": ["Schwei\u00b7ge", ",", "lei\u00b7de", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Hoffe, meide,", "tokens": ["Hof\u00b7fe", ",", "mei\u00b7de", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Nicht verzag", "tokens": ["Nicht", "ver\u00b7zag"], "token_info": ["word", "word"], "pos": ["PTKNEG", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "Und suche den Lieben alle Tag'!", "tokens": ["Und", "su\u00b7che", "den", "Lie\u00b7ben", "al\u00b7le", "Tag'", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "PIAT", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Such ihn durch Wald und Thal und H\u00f6hn,", "tokens": ["Such", "ihn", "durch", "Wald", "und", "Thal", "und", "H\u00f6hn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "APPR", "NN", "KON", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und obgleich Flimmer vor Dir gehn,", "tokens": ["Und", "ob\u00b7gleich", "Flim\u00b7mer", "vor", "Dir", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "NN", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Erm\u00fcde nicht, ihn zu empfahn,", "tokens": ["Er\u00b7m\u00fc\u00b7de", "nicht", ",", "ihn", "zu", "em\u00b7pfahn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "$,", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ihm sanft zu folgen auf seiner Bahn!", "tokens": ["Ihm", "sanft", "zu", "fol\u00b7gen", "auf", "sei\u00b7ner", "Bahn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "PTKZU", "VVINF", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Du wirst ihn haben, ihn bestehn", "tokens": ["Du", "wirst", "ihn", "ha\u00b7ben", ",", "ihn", "be\u00b7stehn"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "VAFIN", "$,", "PPER", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Und neuverwandelt mit ihm gehn,", "tokens": ["Und", "neu\u00b7ver\u00b7wan\u00b7delt", "mit", "ihm", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Mit ihm in aller Liebe F\u00fclle,", "tokens": ["Mit", "ihm", "in", "al\u00b7ler", "Lie\u00b7be", "F\u00fcl\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPR", "PIAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Und er Dich lieben stark und stille,", "tokens": ["Und", "er", "Dich", "lie\u00b7ben", "stark", "und", "stil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PPER", "VVFIN", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Und er sich f\u00fchlen neu ins Leben", "tokens": ["Und", "er", "sich", "f\u00fch\u00b7len", "neu", "ins", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "PRF", "VVFIN", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Zur\u00fcckgegeben,", "tokens": ["Zu\u00b7r\u00fcck\u00b7ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "punct"], "pos": ["ADV", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.15": {"text": "Durch Dich, durch Dich zur\u00fcckgegeben,", "tokens": ["Durch", "Dich", ",", "durch", "Dich", "zu\u00b7r\u00fcck\u00b7ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Und Du ihm Sch\u00f6pferin und Braut,", "tokens": ["Und", "Du", "ihm", "Sch\u00f6p\u00b7fe\u00b7rin", "und", "Braut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PPER", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Ihm ewig, ewig sein vertraut.", "tokens": ["Ihm", "e\u00b7wig", ",", "e\u00b7wig", "sein", "ver\u00b7traut", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "$,", "ADJD", "VAINF", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Tochter Du, was zitterst Du?", "tokens": ["Toch\u00b7ter", "Du", ",", "was", "zit\u00b7terst", "Du", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "PWS", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Was sinkest Du?", "tokens": ["Was", "sin\u00b7kest", "Du", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Schweige, leide,", "tokens": ["Schwei\u00b7ge", ",", "lei\u00b7de", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Hoffe, meide,", "tokens": ["Hof\u00b7fe", ",", "mei\u00b7de", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Nicht verzag,", "tokens": ["Nicht", "ver\u00b7zag", ","], "token_info": ["word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Nicht klag!", "tokens": ["Nicht", "klag", "!"], "token_info": ["word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.7": {"text": "Wahrheit kommt alle Tag'.", "tokens": ["Wahr\u00b7heit", "kommt", "al\u00b7le", "Tag'", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PIAT", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.8": {"line.1": {"text": "Du suchtest ja ein Wunderland,", "tokens": ["Du", "such\u00b7test", "ja", "ein", "Wun\u00b7der\u00b7land", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wo sonder Tand", "tokens": ["Wo", "son\u00b7der", "Tand"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "PIAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Und sonder Schall", "tokens": ["Und", "son\u00b7der", "Schall"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Man ist und hat", "tokens": ["Man", "ist", "und", "hat"], "token_info": ["word", "word", "word", "word"], "pos": ["PIS", "VAFIN", "KON", "VAFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Und Gr\u00f6\u00dfe suchet, That,", "tokens": ["Und", "Gr\u00f6\u00b7\u00dfe", "su\u00b7chet", ",", "That", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "$,", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Und strebt, statt Schein,", "tokens": ["Und", "strebt", ",", "statt", "Schein", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUI", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Zu sein.", "tokens": ["Zu", "sein", "."], "token_info": ["word", "word", "punct"], "pos": ["PTKZU", "VAINF", "$."], "meter": "-+", "measure": "ia