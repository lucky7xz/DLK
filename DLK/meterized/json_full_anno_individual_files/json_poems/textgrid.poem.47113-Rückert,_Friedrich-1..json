{"textgrid.poem.47113": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zwischen Lied und Liebe war mein Leben;", "tokens": ["Zwi\u00b7schen", "Lied", "und", "Lie\u00b7be", "war", "mein", "Le\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Aber, schwebend zwischen Lieb' und Liede,", "tokens": ["A\u00b7ber", ",", "schwe\u00b7bend", "zwi\u00b7schen", "Lieb'", "und", "Lie\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ADJD", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Wu\u00dft' ich nie die beiden auszugleichen.", "tokens": ["Wu\u00dft'", "ich", "nie", "die", "bei\u00b7den", "aus\u00b7zu\u00b7glei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "PIAT", "VVIZU", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Oftmal sang ich anders als ich liebte,", "tokens": ["Oft\u00b7mal", "sang", "ich", "an\u00b7ders", "als", "ich", "lieb\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "KOUS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Anders liebt' ich oft als ich gesungen.", "tokens": ["An\u00b7ders", "liebt'", "ich", "oft", "als", "ich", "ge\u00b7sun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "KOUS", "PPER", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Nun ich dich gefunden, ist der Zwiespalt", "tokens": ["Nun", "ich", "dich", "ge\u00b7fun\u00b7den", ",", "ist", "der", "Zwies\u00b7palt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "PPER", "PRF", "VVPP", "$,", "VAFIN", "ART", "NN"], "meter": "+-+-+-+---", "measure": "unknown.measure.tetra"}, "line.7": {"text": "Ausgeglichen, und rein ineinander", "tokens": ["Aus\u00b7ge\u00b7gli\u00b7chen", ",", "und", "rein", "in\u00b7ein\u00b7an\u00b7der"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "KON", "ADJD", "ADV"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.8": {"text": "Aufgegangen sind mir Lied und Liebe.", "tokens": ["Auf\u00b7ge\u00b7gan\u00b7gen", "sind", "mir", "Lied", "und", "Lie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "Dich nur darf ich, wie ich liebe, singen;", "tokens": ["Dich", "nur", "darf", "ich", ",", "wie", "ich", "lie\u00b7be", ",", "sin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "ADV", "VMFIN", "PPER", "$,", "PWAV", "PPER", "VVFIN", "$,", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "Dich nur kann ich, wie ich singe, lieben.", "tokens": ["Dich", "nur", "kann", "ich", ",", "wie", "ich", "sin\u00b7ge", ",", "lie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "ADV", "VMFIN", "PPER", "$,", "PWAV", "PPER", "VVFIN", "$,", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.11": {"text": "Sollt' ich je nach andrem Sang, nach andrer", "tokens": ["Sollt'", "ich", "je", "nach", "an\u00b7drem", "Sang", ",", "nach", "an\u00b7drer"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VMFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "$,", "APPR", "ADJA"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.12": {"text": "Liebe greifen, wieder unstet schwanken,", "tokens": ["Lie\u00b7be", "grei\u00b7fen", ",", "wie\u00b7der", "un\u00b7stet", "schwan\u00b7ken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "$,", "ADV", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.13": {"text": "Da in deinem Herzen so vereinigt", "tokens": ["Da", "in", "dei\u00b7nem", "Her\u00b7zen", "so", "ver\u00b7ei\u00b7nigt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "ADV", "VVPP"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.14": {"text": "Sind die beiden Pole meines Lebens?", "tokens": ["Sind", "die", "bei\u00b7den", "Po\u00b7le", "mei\u00b7nes", "Le\u00b7bens", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "PIAT", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Zwischen Lied und Liebe war mein Leben;", "tokens": ["Zwi\u00b7schen", "Lied", "und", "Lie\u00b7be", "war", "mein", "Le\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Aber, schwebend zwischen Lieb' und Liede,", "tokens": ["A\u00b7ber", ",", "schwe\u00b7bend", "zwi\u00b7schen", "Lieb'", "und", "Lie\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ADJD", "APPR"