{"dta.poem.5182": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "An den Dohmdechant  \n Freyherrn von Spiegel, zum Diesenberg,  \n als er gesagt hatte, da\u00df er schlaflose N\u00e4chte  \n h\u00e4tte, und bey Lichte nicht gut lesen  \n k\u00f6nnte", "genre": "Lyrik", "period": "N.A.", "pub_year": "1764", "urn": "urn:nbn:de:kobv:b4-200905197778", "language": ["de:0.99"], "booktitle": "Karsch, Anna Luise: Auserlesene Gedichte. Berlin, 1764."}, "poem": {"stanza.1": {"line.1": {"text": "Dich flieht der Schlaf? dich sieht die Lampe wachen?", "tokens": ["Dich", "flieht", "der", "Schlaf", "?", "dich", "sieht", "die", "Lam\u00b7pe", "wa\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$.", "PPER", "VVFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Du nimmst ein Buch, um dir die N\u00e4chte kurz zu", "tokens": ["Du", "nimmst", "ein", "Buch", ",", "um", "dir", "die", "N\u00e4ch\u00b7te", "kurz", "zu"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "KOUI", "PPER", "ART", "NN", "ADJD", "PTKZU"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "machen?", "tokens": ["ma\u00b7chen", "?"], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "Das Auge fodert Sonnenschein?", "tokens": ["Das", "Au\u00b7ge", "fo\u00b7dert", "Son\u00b7nen\u00b7schein", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dein Blick ist, wie die N\u00e4chte, tr\u00fcbe?", "tokens": ["Dein", "Blick", "ist", ",", "wie", "die", "N\u00e4ch\u00b7te", ",", "tr\u00fc\u00b7be", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "$,", "PWAV", "ART", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ein M\u00e4dchen sollte da dir vorzulesen seyn,", "tokens": ["Ein", "M\u00e4d\u00b7chen", "soll\u00b7te", "da", "dir", "vor\u00b7zu\u00b7le\u00b7sen", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ADV", "PPER", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ein M\u00e4dchen, sch\u00f6n wie Lenz und Liebe!", "tokens": ["Ein", "M\u00e4d\u00b7chen", ",", "sch\u00f6n", "wie", "Lenz", "und", "Lie\u00b7be", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "KOKOM", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und an Geschmack wie Dichter fein!", "tokens": ["Und", "an", "Ge\u00b7schmack", "wie", "Dich\u00b7ter", "fein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "KOKOM", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Doch wachend f\u00e4llt dir nur die Lust zu jagen ein,", "tokens": ["Doch", "wa\u00b7chend", "f\u00e4llt", "dir", "nur", "die", "Lust", "zu", "ja\u00b7gen", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "ADV", "ART", "NN", "PTKZU", "VVINF", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Du w\u00fcnschest dir im Wald dem Hirschmann nachzusetzen,", "tokens": ["Du", "w\u00fcn\u00b7schest", "dir", "im", "Wald", "dem", "Hirsc\u00b7hmann", "nach\u00b7zu\u00b7set\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPRART", "NN", "ART", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Den Hauer im Morast zu hetzen,", "tokens": ["Den", "Hau\u00b7er", "im", "Mo\u00b7rast", "zu", "het\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Ihn f\u00e4llen, und dann zappeln sehn,", "tokens": ["Ihn", "f\u00e4l\u00b7len", ",", "und", "dann", "zap\u00b7peln", "sehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "$,", "KON", "ADV", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Wie Russen, die gest\u00fcrzt in schwarzem Blute r\u00f6cheln,", "tokens": ["Wie", "Rus\u00b7sen", ",", "die", "ge\u00b7st\u00fcrzt", "in", "schwar\u00b7zem", "Blu\u00b7te", "r\u00f6\u00b7cheln", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "$,", "PRELS", "ADJD", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Und kr\u00fcmmend sich im Staube drehn!", "tokens": ["Und", "kr\u00fcm\u00b7mend", "sich", "im", "Stau\u00b7be", "drehn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PRF", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Sie fallen br\u00fcllend hin; die Preussen aber l\u00e4cheln", "tokens": ["Sie", "fal\u00b7len", "br\u00fcl\u00b7lend", "hin", ";", "die", "Preus\u00b7sen", "a\u00b7ber", "l\u00e4\u00b7cheln"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "PTKVZ", "$.", "ART", "NN", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Des Heldentodes, sterben so,", "tokens": ["Des", "Hel\u00b7den\u00b7to\u00b7des", ",", "ster\u00b7ben", "so", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Als ein gerechter Mann, verarmt, auf wenig Stroh", "tokens": ["Als", "ein", "ge\u00b7rech\u00b7ter", "Mann", ",", "ver\u00b7armt", ",", "auf", "we\u00b7nig", "Stroh"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "$,", "VVPP", "$,", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Im Schlafe l\u00e4cheln liegt; Ihn tr\u00e4umt von Gottes", "tokens": ["Im", "Schla\u00b7fe", "l\u00e4\u00b7cheln", "liegt", ";", "Ihn", "tr\u00e4umt", "von", "Got\u00b7tes"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "VVFIN", "$.", "PPER", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Knechten,", "tokens": ["Knech\u00b7ten", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.20": {"text": "Von Engeln, die ihm Brodt auch in der W\u00fcste", "tokens": ["Von", "En\u00b7geln", ",", "die", "ihm", "Brodt", "auch", "in", "der", "W\u00fcs\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "PRELS", "PPER", "NN", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "br\u00e4chten.", "tokens": ["br\u00e4ch\u00b7ten", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.22": {"text": "So schlief ich ehedem sanft und geruhig ein,", "tokens": ["So", "schlief", "ich", "e\u00b7he\u00b7dem", "sanft", "und", "ge\u00b7ru\u00b7hig", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADJD", "KON", "ADJD", "PTKVZ", "$,"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.23": {"text": "Vier Kinder um mich her und neben mir ein Gatte,", "tokens": ["Vier", "Kin\u00b7der", "um", "mich", "her", "und", "ne\u00b7ben", "mir", "ein", "Gat\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "APPR", "PPER", "PTKVZ", "KON", "APPR", "PPER", "ART", "NN", "$,"], "meter": "-+---+-+---+-", "measure": "dactylic.init"}, "line.24": {"text": "Der keinen Gram um Brodt, und keine Pflichten", "tokens": ["Der", "kei\u00b7nen", "Gram", "um", "Brodt", ",", "und", "kei\u00b7ne", "Pflich\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "PIAT", "NN", "APPR", "NN", "$,", "KON", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.25": {"text": "hatte,", "tokens": ["hat\u00b7te", ","], "token_info": ["word", "punct"], "pos": ["VAFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.26": {"text": "Als, \u00fcber mich ein Herr zu seyn!", "tokens": ["Als", ",", "\u00fc\u00b7ber", "mich", "ein", "Herr", "zu", "seyn", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "APPR", "PRF", "ART", "NN", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Die Sorgen blieben alle mein;", "tokens": ["Die", "Sor\u00b7gen", "blie\u00b7ben", "al\u00b7le", "mein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIAT", "PPOSAT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Mein s\u00fcsser Trost der Schlaf, und Tr\u00e4ume wie", "tokens": ["Mein", "s\u00fcs\u00b7ser", "Trost", "der", "Schlaf", ",", "und", "Tr\u00e4u\u00b7me", "wie"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "ART", "NN", "$,", "KON", "NN", "KOKOM"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.29": {"text": "Propheten", "tokens": ["Pro\u00b7phe\u00b7ten"], "token_info": ["word"], "pos": ["NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.30": {"text": "Verminderten den Druck von t\u00e4glich neuen N\u00f6then;", "tokens": ["Ver\u00b7min\u00b7der\u00b7ten", "den", "Druck", "von", "t\u00e4g\u00b7lich", "neu\u00b7en", "N\u00f6\u00b7then", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ADJD", "ADJA", "NN", "$."], "meter": "-+---+-+-+-+-", "measure": "dactylic.init"}, "line.31": {"text": "Je gr\u00f6sser Kummer nun mit mir zu Bette ging,", "tokens": ["Je", "gr\u00f6s\u00b7ser", "Kum\u00b7mer", "nun", "mit", "mir", "zu", "Bet\u00b7te", "ging", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "ADV", "APPR", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Je lieblicher da\u00df mich der sanfte Schlaf umfing!", "tokens": ["Je", "lieb\u00b7li\u00b7cher", "da\u00df", "mich", "der", "sanf\u00b7te", "Schlaf", "um\u00b7fing", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOUS", "PPER", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Wie gl\u00fccklich war ich da! ich f\u00fchlte halb mein Leiden!", "tokens": ["Wie", "gl\u00fcck\u00b7lich", "war", "ich", "da", "!", "ich", "f\u00fchl\u00b7te", "halb", "mein", "Lei\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PPER", "ADV", "$.", "PPER", "VVFIN", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Der Tag war schwer f\u00fcr mich! die be\u00dfre milde Nacht", "tokens": ["Der", "Tag", "war", "schwer", "f\u00fcr", "mich", "!", "die", "be\u00df\u00b7re", "mil\u00b7de", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADJD", "APPR", "PPER", "$.", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Verliehe dieser Seele Freuden,", "tokens": ["Ver\u00b7lie\u00b7he", "die\u00b7ser", "See\u00b7le", "Freu\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.36": {"text": "Die noch im Schlafe denkt und wacht.", "tokens": ["Die", "noch", "im", "Schla\u00b7fe", "denkt", "und", "wacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPRART", "NN", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.37": {"text": "Ich lag wie unterm Schutz von einer Gottheit", "tokens": ["Ich", "lag", "wie", "un\u00b7term", "Schutz", "von", "ei\u00b7ner", "Got\u00b7theit"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KOKOM", "APPRART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.38": {"text": "H\u00e4nden,", "tokens": ["H\u00e4n\u00b7den", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.39": {"text": "Nicht aufgeweckt, bis sich die Schatten von uns", "tokens": ["Nicht", "auf\u00b7ge\u00b7weckt", ",", "bis", "sich", "die", "Schat\u00b7ten", "von", "uns"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "VVPP", "$,", "KOUS", "PRF", "ART", "NN", "APPR", "PPER"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.40": {"text": "wenden,", "tokens": ["wen\u00b7den", ","], "token_info": ["word", "punct"], "pos": ["VVINF", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.41": {"text": "Bis mir die Sonne schien, da sah ich ihren Gru\u00df", "tokens": ["Bis", "mir", "die", "Son\u00b7ne", "schien", ",", "da", "sah", "ich", "ih\u00b7ren", "Gru\u00df"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "ART", "NN", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Wie Gottes Augen an, die auf mich niederblickten.", "tokens": ["Wie", "Got\u00b7tes", "Au\u00b7gen", "an", ",", "die", "auf", "mich", "nie\u00b7der\u00b7blick\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "NN", "PTKVZ", "$,", "PRELS", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Ich bat ihn nie um Ueberflu\u00df;", "tokens": ["Ich", "bat", "ihn", "nie", "um", "Ue\u00b7berf\u00b7lu\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.44": {"text": "Nicht ungeduldig bat ich, Sorgen die mich dr\u00fcckten", "tokens": ["Nicht", "un\u00b7ge\u00b7dul\u00b7dig", "bat", "ich", ",", "Sor\u00b7gen", "die", "mich", "dr\u00fcck\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADJD", "VVFIN", "PPER", "$,", "NN", "ART", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Von mir zu nehmen, nein, ich blieb", "tokens": ["Von", "mir", "zu", "neh\u00b7men", ",", "nein", ",", "ich", "blieb"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["APPR", "PPER", "PTKZU", "VVINF", "$,", "PTKANT", "$,", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.46": {"text": "Gelassen, bis er meinen Kummer,", "tokens": ["Ge\u00b7las\u00b7sen", ",", "bis", "er", "mei\u00b7nen", "Kum\u00b7mer", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.47": {"text": "Wie einen Nebel von mir trieb.", "tokens": ["Wie", "ei\u00b7nen", "Ne\u00b7bel", "von", "mir", "trieb", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.48": {"text": "Dir w\u00fcnsch ich jenen sanften Schlummer,", "tokens": ["Dir", "w\u00fcnsch", "ich", "je\u00b7nen", "sanf\u00b7ten", "Schlum\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.49": {"text": "Der mir im Elend Wohlthat war;", "tokens": ["Der", "mir", "im", "E\u00b7lend", "Wohlt\u00b7hat", "war", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "NN", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.50": {"text": "So wirst du nie das Buch erw\u00e4hlen,", "tokens": ["So", "wirst", "du", "nie", "das", "Buch", "er\u00b7w\u00e4h\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.51": {"text": "Um Mitternacht wird dir nie deine Ruhe fehlen,", "tokens": ["Um", "Mit\u00b7ter\u00b7nacht", "wird", "dir", "nie", "dei\u00b7ne", "Ru\u00b7he", "feh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "VAFIN", "PPER", "ADV", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.52": {"text": "Zur Sommernacht wird dir die l\u00e4ngste Nacht im", "tokens": ["Zur", "Som\u00b7mer\u00b7nacht", "wird", "dir", "die", "l\u00e4ngs\u00b7te", "Nacht", "im"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VAFIN", "PPER", "ART", "ADJA", "NN", "APPRART"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.53": {"text": "Jahr.", "tokens": ["Jahr", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "-", "measure": "single.down"}}}}}