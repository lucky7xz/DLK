{"textgrid.poem.32995": {"metadata": {"author": {"name": "Flaischlen, C\u00e4sar", "birth": "N.A.", "death": "N.A."}, "title": "Februar", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schon leuchtet die Sonne wieder am Himmel", "tokens": ["Schon", "leuch\u00b7tet", "die", "Son\u00b7ne", "wie\u00b7der", "am", "Him\u00b7mel"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "APPRART", "NN"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "und schmilzt die Schneelast von den D\u00e4chern", "tokens": ["und", "schmilzt", "die", "Schnee\u00b7last", "von", "den", "D\u00e4\u00b7chern"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "und taut das Eis auf an den Fenstern", "tokens": ["und", "taut", "das", "Eis", "auf", "an", "den", "Fens\u00b7tern"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "und lacht ins Zimmer: wie geht's? wie steht's?", "tokens": ["und", "lacht", "ins", "Zim\u00b7mer", ":", "wie", "geht's", "?", "wie", "steht's", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "$.", "PWAV", "VVFIN", "$.", "PWAV", "VVFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Und wenn es auch noch lang nicht Fr\u00fchling,", "tokens": ["Und", "wenn", "es", "auch", "noch", "lang", "nicht", "Fr\u00fch\u00b7ling", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "ADV", "ADJD", "PTKNEG", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "so laut es \u00fcberall tropft und rinnt ...", "tokens": ["so", "laut", "es", "\u00fc\u00b7be\u00b7rall", "tropft", "und", "rinnt", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "ADV", "VVFIN", "KON", "VVFIN", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "du sinnst hinaus \u00fcber deine D\u00e4cher ...", "tokens": ["du", "sinnst", "hin\u00b7aus", "\u00fc\u00b7ber", "dei\u00b7ne", "D\u00e4\u00b7cher", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PRF", "APZR", "APPR", "PPOSAT", "NN", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "du sagst, es sei ein schreckliches Wetter,", "tokens": ["du", "sagst", ",", "es", "sei", "ein", "schreck\u00b7li\u00b7ches", "Wet\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "man werde ganz krank! und bist im stillen", "tokens": ["man", "wer\u00b7de", "ganz", "krank", "!", "und", "bist", "im", "stil\u00b7len"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIS", "VAFIN", "ADV", "ADJD", "$.", "KON", "VAFIN", "APPRART", "ADJA"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "gl\u00fcckselig dr\u00fcber wie ein Kind.", "tokens": ["gl\u00fcck\u00b7se\u00b7lig", "dr\u00fc\u00b7ber", "wie", "ein", "Kind", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PAV", "KOKOM", "ART", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.