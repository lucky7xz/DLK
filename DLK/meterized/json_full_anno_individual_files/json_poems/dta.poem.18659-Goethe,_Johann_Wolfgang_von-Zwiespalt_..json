{"dta.poem.18659": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang von", "birth": "N.A.", "death": "N.A."}, "title": "Zwiespalt .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1819", "urn": "urn:nbn:de:kobv:b4-200905191692", "language": ["de:0.99"], "booktitle": "Goethe, Johann Wolfgang von: West-\u00f6stlicher Divan. Stuttgart, 1819."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn links an Baches Rand", "tokens": ["Wenn", "links", "an", "Ba\u00b7ches", "Rand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Cupido fl\u00f6tet,", "tokens": ["Cu\u00b7pi\u00b7do", "fl\u00f6\u00b7tet", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "VVFIN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Im Felde rechter Hand", "tokens": ["Im", "Fel\u00b7de", "rech\u00b7ter", "Hand"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Mavors drommetet,", "tokens": ["Ma\u00b7vors", "drom\u00b7me\u00b7tet", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "VVFIN", "$,"], "meter": "+-+--", "measure": "unknown.measure.di"}, "line.5": {"text": "Da wird dorthin das Ohr", "tokens": ["Da", "wird", "dor\u00b7thin", "das", "Ohr"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Lieblich gezogen,", "tokens": ["Lieb\u00b7lich", "ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVPP", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Doch um des Liedes Flor", "tokens": ["Doch", "um", "des", "Lie\u00b7des", "Flor"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Durch L\u00e4rm betrogen.", "tokens": ["Durch", "L\u00e4rm", "be\u00b7tro\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.9": {"text": "Nun fl\u00f6tets immer voll", "tokens": ["Nun", "fl\u00f6\u00b7tets", "im\u00b7mer", "voll"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Im Kriegesthunder,", "tokens": ["Im", "Krie\u00b7gest\u00b7hun\u00b7der", ","], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.11": {"text": "Ich werde rasend, toll,", "tokens": ["Ich", "wer\u00b7de", "ra\u00b7send", ",", "toll", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$,", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "Ist das ein Wunder.", "tokens": ["Ist", "das", "ein", "Wun\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "ART", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.13": {"text": "Fort w\u00e4chst der Fl\u00f6tenton", "tokens": ["Fort", "w\u00e4chst", "der", "Fl\u00f6\u00b7ten\u00b7ton"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.14": {"text": "Schall der Posaunen,", "tokens": ["Schall", "der", "Po\u00b7sau\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.15": {"text": "Ich irre, rase schon,", "tokens": ["Ich", "ir\u00b7re", ",", "ra\u00b7se", "schon", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.16": {"text": "Ist das zu staunen!", "tokens": ["Ist", "das", "zu", "stau\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "PTKZU", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}}}}