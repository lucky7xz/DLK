{"dta.poem.7014": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der sch\u00f6nste Thau.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Dies Tr\u00f6pfgen, das so rein, so rund,", "tokens": ["Dies", "Tr\u00f6pf\u00b7gen", ",", "das", "so", "rein", ",", "so", "rund", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "NN", "$,", "PRELS", "ADV", "ADJD", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und, wie ein Tr\u00f6pfchen Thau, auf Bluhmen klar", "tokens": ["Und", ",", "wie", "ein", "Tr\u00f6pf\u00b7chen", "Thau", ",", "auf", "Bluh\u00b7men", "klar"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "$,", "PWAV", "ART", "NN", "NN", "$,", "APPR", "NN", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "und bunt;", "tokens": ["und", "bunt", ";"], "token_info": ["word", "word", "punct"], "pos": ["KON", "ADJD", "$."], "meter": "-+", "measure": "iambic.single"}, "line.4": {"text": "Auch klar und bunt auf bunten Bluhmen lieget,", "tokens": ["Auch", "klar", "und", "bunt", "auf", "bun\u00b7ten", "Bluh\u00b7men", "lie\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ergetzt, erquicket und vergn\u00fcget", "tokens": ["Er\u00b7getzt", ",", "er\u00b7quic\u00b7ket", "und", "ver\u00b7gn\u00fc\u00b7get"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["VVPP", "$,", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Mich recht auf eine neue Weise.", "tokens": ["Mich", "recht", "auf", "ei\u00b7ne", "neu\u00b7e", "Wei\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Ein s\u00fcsses Hoffen schmeichelt mir,", "tokens": ["Ein", "s\u00fcs\u00b7ses", "Hof\u00b7fen", "schmei\u00b7chelt", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Es gl\u00e4ntze dieses Na\u00df, in seiner reinen Zier,", "tokens": ["Es", "gl\u00e4nt\u00b7ze", "die\u00b7ses", "Na\u00df", ",", "in", "sei\u00b7ner", "rei\u00b7nen", "Zier", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PDAT", "NN", "$,", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Dem grossen Sch\u00f6pfer selbst zum Preise,", "tokens": ["Dem", "gros\u00b7sen", "Sch\u00f6p\u00b7fer", "selbst", "zum", "Prei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Und werde diese kleine Fluht,", "tokens": ["Und", "wer\u00b7de", "die\u00b7se", "klei\u00b7ne", "Fluht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "(die aus der Creaturen Pracht,", "tokens": ["(", "die", "aus", "der", "Crea\u00b7tu\u00b7ren", "Pracht", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "APPR", "ART", "NN", "NN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.12": {"text": "Zu Ehren dem, der sie gemacht,", "tokens": ["Zu", "Eh\u00b7ren", "dem", ",", "der", "sie", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Durch eifriger Betrachtung Gluht,", "tokens": ["Durch", "eif\u00b7ri\u00b7ger", "Be\u00b7trach\u00b7tung", "Gluht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Gezogen, erst sich sublimiret,", "tokens": ["Ge\u00b7zo\u00b7gen", ",", "erst", "sich", "sub\u00b7li\u00b7mi\u00b7ret", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "ADV", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Und vom Gehirn den Sitz der Seelen distilliret,)", "tokens": ["Und", "vom", "Ge\u00b7hirn", "den", "Sitz", "der", "See\u00b7len", "dis\u00b7til\u00b7li\u00b7ret", ",", ")"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "APPRART", "NN", "ART", "NN", "ART", "NN", "VVFIN", "$,", "$("], "meter": "---+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.16": {"text": "In ihrem rein und sch\u00f6n gef\u00e4rbten Schein,", "tokens": ["In", "ih\u00b7rem", "rein", "und", "sch\u00f6n", "ge\u00b7f\u00e4rb\u00b7ten", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJD", "KON", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Vielleicht der Gottheit selbst gef\u00e4llig seyn.", "tokens": ["Viel\u00b7leicht", "der", "Got\u00b7theit", "selbst", "ge\u00b7f\u00e4l\u00b7lig", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADV", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Ja, ja, wo etwas auf der Welt geschickt,", "tokens": ["Ja", ",", "ja", ",", "wo", "et\u00b7was", "auf", "der", "Welt", "ge\u00b7schickt", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PTKANT", "$,", "PWAV", "PIS", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Dem Sch\u00f6pfer Freude zu gew\u00e4hren;", "tokens": ["Dem", "Sch\u00f6p\u00b7fer", "Freu\u00b7de", "zu", "ge\u00b7w\u00e4h\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "So ist es solch ein Thau, so sind es Freuden-Z\u00e4hren,", "tokens": ["So", "ist", "es", "solch", "ein", "Thau", ",", "so", "sind", "es", "Freu\u00b7den\u00b7Z\u00e4h\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PIAT", "ART", "NN", "$,", "ADV", "VAFIN", "PPER", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Die Sein Gesch\u00f6pf, zu Seinen Ehren,", "tokens": ["Die", "Sein", "Ge\u00b7sch\u00f6pf", ",", "zu", "Sei\u00b7nen", "Eh\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Voll Lieb, aus Danckbarkeit, uns aus den Augen dr\u00fcckt.", "tokens": ["Voll", "Lieb", ",", "aus", "Dan\u00b7ck\u00b7bar\u00b7keit", ",", "uns", "aus", "den", "Au\u00b7gen", "dr\u00fcckt", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "$,", "APPR", "NN", "$,", "PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}}}}}