{"dta.poem.11413": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Als er keine gegen-liebe hoffen kunte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ach allerliebste thr\u00e4nen!", "tokens": ["Ach", "al\u00b7ler\u00b7liebs\u00b7te", "thr\u00e4\u00b7nen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ITJ", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Jhr boten meiner herben pein!", "tokens": ["Ihr", "bo\u00b7ten", "mei\u00b7ner", "her\u00b7ben", "pein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Kan ein so reines sehnen", "tokens": ["Kan", "ein", "so", "rei\u00b7nes", "seh\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "ART", "ADV", "ADJA", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Durch eure krafft dann nicht gestillet seyn?", "tokens": ["Durch", "eu\u00b7re", "krafft", "dann", "nicht", "ge\u00b7stil\u00b7let", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "PTKNEG", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ist es unm\u00f6glich, dieses hertze,", "tokens": ["Ist", "es", "un\u00b7m\u00f6g\u00b7lich", ",", "die\u00b7ses", "hert\u00b7ze", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "$,", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Das \u00fcber des Myrtillus schmertze", "tokens": ["Das", "\u00fc\u00b7ber", "des", "Myr\u00b7til\u00b7lus", "schmert\u00b7ze"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Sich nicht erbarmen will, noch endlich zu erweichen?", "tokens": ["Sich", "nicht", "er\u00b7bar\u00b7men", "will", ",", "noch", "end\u00b7lich", "zu", "er\u00b7wei\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PRF", "PTKNEG", "VVINF", "VMFIN", "$,", "ADV", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So lescht zum wenigsten durch eure milde fluth,", "tokens": ["So", "lescht", "zum", "we\u00b7nigs\u00b7ten", "durch", "eu\u00b7re", "mil\u00b7de", "fluth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "PIS", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+---+-+-+", "measure": "unknown.measure.penta"}, "line.9": {"text": "O thr\u00e4nen! meine gluth!", "tokens": ["O", "thr\u00e4\u00b7nen", "!", "mei\u00b7ne", "gluth", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVINF", "$.", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Oder wachset so weit an,", "tokens": ["O\u00b7der", "wach\u00b7set", "so", "weit", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADJD", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Damit ich und mein schmertz in euch ertrincken kan!", "tokens": ["Da\u00b7mit", "ich", "und", "mein", "schmertz", "in", "euch", "er\u00b7trin\u00b7cken", "kan", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "KON", "PPOSAT", "NN", "APPR", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}