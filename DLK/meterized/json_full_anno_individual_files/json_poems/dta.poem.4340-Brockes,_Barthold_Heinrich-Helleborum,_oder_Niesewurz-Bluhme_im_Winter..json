{"dta.poem.4340": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Helleborum,   oder Niesewurz-Bluhme  \n im Winter.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "So mein- als ihrem HErrn zum Ruhme,", "tokens": ["So", "mein", "als", "ih\u00b7rem", "Herrn", "zum", "Ruh\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "TRUNC", "KOKOM", "PPOSAT", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Seh ich hier eine neue Bluhme,", "tokens": ["Seh", "ich", "hier", "ei\u00b7ne", "neu\u00b7e", "Bluh\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Da alle Bluhmen weg, vergangen,", "tokens": ["Da", "al\u00b7le", "Bluh\u00b7men", "weg", ",", "ver\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "PTKVZ", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So gar im Schnee und Eise, prangen.", "tokens": ["So", "gar", "im", "Schnee", "und", "Ei\u00b7se", ",", "pran\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADV", "APPRART", "NN", "KON", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "F\u00fcrwahr, es ist Bewundrung wehrt,", "tokens": ["F\u00fcr\u00b7wahr", ",", "es", "ist", "Be\u00b7wund\u00b7rung", "wehrt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VAFIN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da deiner Bl\u00e4tter zart Gewebe", "tokens": ["Da", "dei\u00b7ner", "Bl\u00e4t\u00b7ter", "zart", "Ge\u00b7we\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "ADJD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Auch nicht der strenge Frost versehrt;", "tokens": ["Auch", "nicht", "der", "stren\u00b7ge", "Frost", "ver\u00b7sehrt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Da\u00df man es merkt, und Den erhebe,", "tokens": ["Da\u00df", "man", "es", "merkt", ",", "und", "Den", "er\u00b7he\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "VVFIN", "$,", "KON", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Der uns in dir aufs neue zeiget,", "tokens": ["Der", "uns", "in", "dir", "aufs", "neu\u00b7e", "zei\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "PPER", "APPRART", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Wie Seine Weisheit, Seine Macht,", "tokens": ["Wie", "Sei\u00b7ne", "Weis\u00b7heit", ",", "Sei\u00b7ne", "Macht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Was jemahls unser Geist erdacht,", "tokens": ["Was", "je\u00b7mahls", "un\u00b7ser", "Geist", "er\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "An Gr\u00f6ss\u2019 unendlich \u00fcbersteiget.", "tokens": ["An", "Gr\u00f6ss'", "un\u00b7end\u00b7lich", "\u00fc\u00b7bers\u00b7tei\u00b7get", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Da nicht allein die Bluhmen nur,", "tokens": ["Da", "nicht", "al\u00b7lein", "die", "Bluh\u00b7men", "nur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "ADV", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da fast die Kr\u00e4fte der Natur", "tokens": ["Da", "fast", "die", "Kr\u00e4f\u00b7te", "der", "Na\u00b7tur"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der K\u00e4lte Strenge will vernichten;", "tokens": ["Der", "K\u00e4l\u00b7te", "Stren\u00b7ge", "will", "ver\u00b7nich\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wei\u00df GOtt ein Bl\u00fchmchen zuzurichten,", "tokens": ["Wei\u00df", "Gott", "ein", "Bl\u00fch\u00b7mchen", "zu\u00b7zu\u00b7rich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "ART", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Das, obs gleich schneiet, friert und wehet,", "tokens": ["Das", ",", "obs", "gleich", "schnei\u00b7et", ",", "friert", "und", "we\u00b7het", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "KOUS", "ADV", "VVFIN", "$,", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Doch aller K\u00e4lte widerstehet.", "tokens": ["Doch", "al\u00b7ler", "K\u00e4l\u00b7te", "wi\u00b7der\u00b7ste\u00b7het", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Es stellet mir dein holder Flor,", "tokens": ["Es", "stel\u00b7let", "mir", "dein", "hol\u00b7der", "Flor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bey meiner Lust, ein Lehrbild vor.", "tokens": ["Bey", "mei\u00b7ner", "Lust", ",", "ein", "Lehr\u00b7bild", "vor", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mir f\u00e4llt, bey dir, die Wahrheit bey:", "tokens": ["Mir", "f\u00e4llt", ",", "bey", "dir", ",", "die", "Wahr\u00b7heit", "bey", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "APPR", "PPER", "$,", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df, wenn gleich Ungl\u00fccks-Winde st\u00fcrmen,", "tokens": ["Da\u00df", ",", "wenn", "gleich", "Un\u00b7gl\u00fccks\u00b7Win\u00b7de", "st\u00fcr\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "KOUS", "ADV", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Doch GOtt verm\u00f6gend zu beschirmen,", "tokens": ["Doch", "Gott", "ver\u00b7m\u00f6\u00b7gend", "zu", "be\u00b7schir\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVPP", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und in den Schwachen m\u00e4chtig sey.", "tokens": ["Und", "in", "den", "Schwa\u00b7chen", "m\u00e4ch\u00b7tig", "sey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Nun will ich deinen Bau betrachten,", "tokens": ["Nun", "will", "ich", "dei\u00b7nen", "Bau", "be\u00b7trach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So Farb\u2019 als Form\u2019 an dir beachten,", "tokens": ["So", "Fa\u00b7rb'", "als", "Form'", "an", "dir", "be\u00b7ach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KOUS", "NN", "APPR", "PPER", "VVINF", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Und dich, da du besonders sch\u00f6n,", "tokens": ["Und", "dich", ",", "da", "du", "be\u00b7son\u00b7ders", "sch\u00f6n", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "KOUS", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auch, mit besonderm Flei\u00df, besehn.", "tokens": ["Auch", ",", "mit", "be\u00b7son\u00b7derm", "Flei\u00df", ",", "be\u00b7sehn", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "$,", "APPR", "ADJA", "NN", "$,", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "So lange sie geschlossen stehn,", "tokens": ["So", "lan\u00b7ge", "sie", "ge\u00b7schlos\u00b7sen", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "VVPP", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sind nur drey Bl\u00e4tter dran zu sehn,", "tokens": ["Sind", "nur", "drey", "Bl\u00e4t\u00b7ter", "dran", "zu", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "CARD", "NN", "PAV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In einer Form, fast Tulpen gleich:", "tokens": ["In", "ei\u00b7ner", "Form", ",", "fast", "Tul\u00b7pen", "gleich", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ADV", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Doch ist sie noch an dreyen reich,", "tokens": ["Doch", "ist", "sie", "noch", "an", "drey\u00b7en", "reich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "APPR", "CARD", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So bald sie von einander gehen.", "tokens": ["So", "bald", "sie", "von", "ein\u00b7an\u00b7der", "ge\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "APPR", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Da ich sie, wenn sie offen, bald", "tokens": ["Da", "ich", "sie", ",", "wenn", "sie", "of\u00b7fen", ",", "bald"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["KOUS", "PPER", "PPER", "$,", "KOUS", "PPER", "ADJD", "$,", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Den wilden Rosen an Gestalt,", "tokens": ["Den", "wil\u00b7den", "Ro\u00b7sen", "an", "Ge\u00b7stalt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "In einer angenehmen R\u00fcnde,", "tokens": ["In", "ei\u00b7ner", "an\u00b7ge\u00b7neh\u00b7men", "R\u00fcn\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "In hohlen Bl\u00e4ttern, \u00e4hnlich finde.", "tokens": ["In", "hoh\u00b7len", "Bl\u00e4t\u00b7tern", ",", "\u00e4hn\u00b7lich", "fin\u00b7de", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "In ihrer Mitte sehen wir,", "tokens": ["In", "ih\u00b7rer", "Mit\u00b7te", "se\u00b7hen", "wir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "In ausserordentlicher Zier,", "tokens": ["In", "aus\u00b7ser\u00b7or\u00b7dent\u00b7li\u00b7cher", "Zier", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.12": {"text": "Um neun vereinten rohten Spitzen,", "tokens": ["Um", "neun", "ver\u00b7ein\u00b7ten", "roh\u00b7ten", "Spit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Auf kleinen Silber-weissen Stangen", "tokens": ["Auf", "klei\u00b7nen", "Sil\u00b7ber\u00b7weis\u00b7sen", "Stan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Zu Anfangs g\u00fcldne K\u00f6lbchen prangen,", "tokens": ["Zu", "An\u00b7fangs", "g\u00fcld\u00b7ne", "K\u00f6lb\u00b7chen", "pran\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "So erstlich Trauben formig sitzen.", "tokens": ["So", "erst\u00b7lich", "Trau\u00b7ben", "for\u00b7mig", "sit\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Die aber doch nicht dergestalt", "tokens": ["Die", "a\u00b7ber", "doch", "nicht", "der\u00b7ge\u00b7stalt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ADV", "PTKNEG", "ADV"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.17": {"text": "In ihrer Lage lange bleiben,", "tokens": ["In", "ih\u00b7rer", "La\u00b7ge", "lan\u00b7ge", "blei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Da sie sich von dem Centro bald", "tokens": ["Da", "sie", "sich", "von", "dem", "Cent\u00b7ro", "bald"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "APPR", "ART", "NE", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Zur\u00fccke ziehn, sich abwerts treiben,", "tokens": ["Zu\u00b7r\u00fc\u00b7cke", "ziehn", ",", "sich", "ab\u00b7werts", "trei\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "$,", "PRF", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Und eine neue Form empfangen.", "tokens": ["Und", "ei\u00b7ne", "neu\u00b7e", "Form", "emp\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Wodurch sich denn, zu gleicher Zeit,", "tokens": ["Wo\u00b7durch", "sich", "denn", ",", "zu", "glei\u00b7cher", "Zeit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "ADV", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Die zierliche Vollkommenheit", "tokens": ["Die", "zier\u00b7li\u00b7che", "Voll\u00b7kom\u00b7men\u00b7heit"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Des Mittelpuncts dem Blick entdecket,", "tokens": ["Des", "Mit\u00b7tel\u00b7puncts", "dem", "Blick", "ent\u00b7de\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Der bis dahero ganz verstecket,", "tokens": ["Der", "bis", "da\u00b7he\u00b7ro", "ganz", "ver\u00b7ste\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PAV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "An dem sich die Bewundrung h\u00e4uft.", "tokens": ["An", "dem", "sich", "die", "Be\u00b7wund\u00b7rung", "h\u00e4uft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PRF", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}