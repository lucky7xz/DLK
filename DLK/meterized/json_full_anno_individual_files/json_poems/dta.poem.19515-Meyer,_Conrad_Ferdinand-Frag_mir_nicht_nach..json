{"dta.poem.19515": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Frag mir nicht nach.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-200905193933", "language": ["de:0.99"], "booktitle": "Meyer, Conrad Ferdinand: Gedichte. Leipzig, 1882."}, "poem": {"stanza.1": {"line.1": {"text": "Wo wei\u00df die Landquart durch die Tannen sch\u00e4umt,", "tokens": ["Wo", "wei\u00df", "die", "Land\u00b7quart", "durch", "die", "Tan\u00b7nen", "sch\u00e4umt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Irrt' unbek\u00fcmmert ich um Weg und Zeit,", "tokens": ["Irrt'", "un\u00b7be\u00b7k\u00fcm\u00b7mert", "ich", "um", "Weg", "und", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da stand ein grauer Thurm \u2014 wie hingetr\u00e4umt", "tokens": ["Da", "stand", "ein", "grau\u00b7er", "Thurm", "wie", "hin\u00b7ge\u00b7tr\u00e4umt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$(", "PWAV", "VVPP"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "In ungebrochne Waldeseinsamkeit.", "tokens": ["In", "un\u00b7ge\u00b7broch\u00b7ne", "Wal\u00b7de\u00b7sein\u00b7sam\u00b7keit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ich sah mich um und frug: \u201eWie hei\u00dft das Schlo\u00df?\u201c", "tokens": ["Ich", "sah", "mich", "um", "und", "frug", ":", "\u201e", "Wie", "hei\u00dft", "das", "Schlo\u00df", "?", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "KON", "VVFIN", "$.", "$(", "PWAV", "VVFIN", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Ein bucklig M\u00fctterlein, das Kr\u00e4uter brach;", "tokens": ["Ein", "buck\u00b7lig", "M\u00fct\u00b7ter\u00b7lein", ",", "das", "Kr\u00e4u\u00b7ter", "brach", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "$,", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Da grollte sie, die jedes Wort verdro\u00df:", "tokens": ["Da", "groll\u00b7te", "sie", ",", "die", "je\u00b7des", "Wort", "ver\u00b7dro\u00df", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PRELS", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "\u201efragmirnichtnach.\u201c", "tokens": ["\u201e", "frag\u00b7mir\u00b7nicht\u00b7nach", ".", "\u201c"], "token_info": ["punct", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "$.", "$("], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Ich schritt hinan; im Hof ein Br\u00fcnnlein scholl,", "tokens": ["Ich", "schritt", "hi\u00b7nan", ";", "im", "Hof", "ein", "Br\u00fcnn\u00b7lein", "scholl", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$.", "APPRART", "NN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Durch den verwachsnen Thorweg drang ich ein,", "tokens": ["Durch", "den", "ver\u00b7wachs\u00b7nen", "Thor\u00b7weg", "drang", "ich", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ein d\u00fcnnes k\u00fchles Rieseln \u00fcberquoll", "tokens": ["Ein", "d\u00fcn\u00b7nes", "k\u00fch\u00b7les", "Rie\u00b7seln", "\u00fc\u00b7berq\u00b7uoll"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Auf einer Gruft den schwarz bemoosten Stein.", "tokens": ["Auf", "ei\u00b7ner", "Gruft", "den", "schwarz", "be\u00b7moos\u00b7ten", "Stein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ich beugte mich nach des Verschollnen Spur,", "tokens": ["Ich", "beug\u00b7te", "mich", "nach", "des", "Ver\u00b7scholl\u00b7nen", "Spur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Entziffernd, was des Steines Inschrift sprach,", "tokens": ["Ent\u00b7zif\u00b7fernd", ",", "was", "des", "Stei\u00b7nes", "In\u00b7schrift", "sprach", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "PRELS", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Nicht Zahl, nicht Namen \u2014 ein Begehren nur:", "tokens": ["Nicht", "Zahl", ",", "nicht", "Na\u00b7men", "ein", "Be\u00b7geh\u00b7ren", "nur", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "$,", "PTKNEG", "NN", "$(", "ART", "NN", "ADV", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Frag mir nicht nach!", "tokens": ["Frag", "mir", "nicht", "nach", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKNEG", "PTKVZ", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}}}}