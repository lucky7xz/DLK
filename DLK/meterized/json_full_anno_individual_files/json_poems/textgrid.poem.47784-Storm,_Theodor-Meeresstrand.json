{"textgrid.poem.47784": {"metadata": {"author": {"name": "Storm, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Meeresstrand", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ans Haff nun fliegt die M\u00f6we,", "tokens": ["Ans", "Haff", "nun", "fliegt", "die", "M\u00f6\u00b7we", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+---", "measure": "unknown.measure.di"}, "line.2": {"text": "Und D\u00e4mmrung bricht herein;", "tokens": ["Und", "D\u00e4mm\u00b7rung", "bricht", "her\u00b7ein", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u00dcber die feuchten Watten", "tokens": ["\u00dc\u00b7ber", "die", "feuch\u00b7ten", "Wat\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Spiegelt der Abendschein.", "tokens": ["Spie\u00b7gelt", "der", "A\u00b7bend\u00b7schein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.2": {"line.1": {"text": "Graues Gefl\u00fcgel huschet", "tokens": ["Grau\u00b7es", "Ge\u00b7fl\u00fc\u00b7gel", "hu\u00b7schet"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Neben dem Wasser her;", "tokens": ["Ne\u00b7ben", "dem", "Was\u00b7ser", "her", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Wie Tr\u00e4ume liegen die Inseln", "tokens": ["Wie", "Tr\u0