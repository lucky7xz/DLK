{"textgrid.poem.63796": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "7. Silens Nachtbesuch bei den Liebenden", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sagt, wer l\u00e4dt so sp\u00e4t sich zu Gast? Sie w\u00e4hnten sich sicher,", "tokens": ["Sagt", ",", "wer", "l\u00e4dt", "so", "sp\u00e4t", "sich", "zu", "Gast", "?", "Sie", "w\u00e4hn\u00b7ten", "sich", "si\u00b7cher", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWS", "VVFIN", "ADV", "ADJD", "PRF", "APPR", "NN", "$.", "PPER", "VVFIN", "PRF", "ADJD", "$,"], "meter": "--+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Aber der Alte, der Gott, sp\u00fcrte die Liebenden aus.", "tokens": ["A\u00b7ber", "der", "Al\u00b7te", ",", "der", "Gott", ",", "sp\u00fcr\u00b7te", "die", "Lie\u00b7ben\u00b7den", "aus", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "ART", "NN", "$,", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "+--+---+--+--+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Hier, so ruft den Begleitern er zu, hier will ich ein wenig", "tokens": ["Hier", ",", "so", "ruft", "den", "Be\u00b7glei\u00b7tern", "er", "zu", ",", "hier", "will", "ich", "ein", "we\u00b7nig"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "ADV", "VVFIN", "ART", "NN", "PPER", "PTKVZ", "$,", "ADV", "VMFIN", "PPER", "ART", "PIS"], "meter": "+-+--+-+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Rasten. Der Hausherr war einst mir genauer bekannt.", "tokens": ["Ras\u00b7ten", ".", "Der", "Haus\u00b7herr", "war", "einst", "mir", "ge\u00b7nau\u00b7er", "be\u00b7kannt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ART", "NN", "VAFIN", "ADV", "PPER", "ADJD", "PTKVZ", "$."], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.5": {"text": "Untreu ward er dem Alten; es zwang ihn st\u00e4rkerer Zauber,", "tokens": ["Un\u00b7treu", "ward", "er", "dem", "Al\u00b7ten", ";", "es", "zwang", "ihn", "st\u00e4r\u00b7ke\u00b7rer", "Zau\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "ART", "NN", "$.", "PPER", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.6": {"text": "Und mit Eros im Kampf pfleg' ich den k\u00fcrzern zu ziehn.", "tokens": ["Und", "mit", "E\u00b7ros", "im", "Kampf", "pfleg'", "ich", "den", "k\u00fcr\u00b7zern", "zu", "ziehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NE", "APPRART", "NN", "VVFIN", "PPER", "ART", "VVINF", "PTKZU", "VVINF", "$."], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}, "line.7": {"text": "Doch mir kehrt ein jeder zur\u00fcck; ich harre geduldig,", "tokens": ["Doch", "mir", "kehrt", "ein", "je\u00b7der", "zu\u00b7r\u00fcck", ";", "ich", "har\u00b7re", "ge\u00b7dul\u00b7dig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "PIS", "PTKVZ", "$.", "PPER", "VVFIN", "ADJD", "$,"], "meter": "--+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Bis die lodernde Glut selbst nach Erfrischung verlangt.", "tokens": ["Bis", "die", "lo\u00b7dern\u00b7de", "Glut", "selbst", "nach", "Er\u00b7fri\u00b7schung", "ver\u00b7langt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ADV", "APPR", "NN", "VVPP", "$."], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.9": {"text": "L\u00f6st mir nun die Sandalen, ihr Knaben. Ich mach' es als Hausfreund", "tokens": ["L\u00f6st", "mir", "nun", "die", "San\u00b7da\u00b7len", ",", "ihr", "Kna\u00b7ben", ".", "Ich", "mach'", "es", "als", "Haus\u00b7freund"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "$,", "PPOSAT", "NN", "$.", "PPER", "VVFIN", "PPER", "KOUS", "NN"], "meter": "+-+-+-+-+--+-+++", "measure": "trochaic.octa.plus.relaxed"}, "line.10": {"text": "Gern mir bequem. Doch ihr, trunkene Laffen, entweicht! \u2013", "tokens": ["Gern", "mir", "be\u00b7quem", ".", "Doch", "ihr", ",", "trun\u00b7ke\u00b7ne", "Laf\u00b7fen", ",", "ent\u00b7weicht", "!", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "punct", "punct"], "pos": ["ADV", "PPER", "ADJD", "$.", "KON", "PPER", "$,", "ADJA", "NN", "$,", "VVFIN", "$.", "$("], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.11": {"text": "Ach, wie erschrickt das P\u00e4rchen! Sie h\u00f6ren die taumelnden Stimmen", "tokens": ["Ach", ",", "wie", "er\u00b7schrickt", "das", "P\u00e4r\u00b7chen", "!", "Sie", "h\u00f6\u00b7ren", "die", "tau\u00b7meln\u00b7den", "Stim\u00b7men"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "PWAV", "VVPP", "ART", "NN", "$.", "PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+--+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "Drau\u00df in der Gasse; die Mutwilligen l\u00e4rmen am Tor.", "tokens": ["Drau\u00df", "in", "der", "Gas\u00b7se", ";", "die", "Mut\u00b7wil\u00b7li\u00b7gen", "l\u00e4r\u00b7men", "am", "Tor", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$.", "ART", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.13": {"text": "Seid nur getrost! Ihr seht, kaum h\u00e4lt der Alte sich aufrecht;", "tokens": ["Seid", "nur", "ge\u00b7trost", "!", "Ihr", "seht", ",", "kaum", "h\u00e4lt", "der", "Al\u00b7te", "sich", "auf\u00b7recht", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADV", "VVPP", "$.", "PPER", "VVFIN", "$,", "ADV", "VVFIN", "ART", "NN", "PRF", "ADJD", "$."], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.14": {"text": "Bald entschl\u00e4ft er, und treu h\u00fctet dann Eros das Haus.", "tokens": ["Bald", "ent\u00b7schl\u00e4ft", "er", ",", "und", "treu", "h\u00fc\u00b7tet", "dann", "E\u00b7ros", "das", "Haus", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KON", "ADJD", "VVFIN", "ADV", "NE", "ART", "NN", "$."], "meter": "--+--++-+-+-+", "measure": "anapaest.di.plus"}}, "stanza.2": {"line.1": {"text": "Sagt, wer l\u00e4dt so sp\u00e4t sich zu Gast? Sie w\u00e4hnten sich sicher,", "tokens": ["Sagt", ",", "wer", "l\u00e4dt", "so", "sp\u00e4t", "sich", "zu", "Gast", "?", "Sie", "w\u00e4hn\u00b7ten", "sich", "si\u00b7cher", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWS", "VVFIN", "ADV", "ADJD", "PRF", "APPR", "NN", "$.", "PPER", "VVFIN", "PRF", "ADJD", "$,"], "meter": "--+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Aber der Alte, der Gott, sp\u00fcrte die Liebenden aus.", "tokens": ["A\u00b7ber", "der", "Al\