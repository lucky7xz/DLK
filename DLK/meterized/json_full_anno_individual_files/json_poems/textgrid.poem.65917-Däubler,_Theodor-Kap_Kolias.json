{"textgrid.poem.65917": {"metadata": {"author": {"name": "D\u00e4ubler, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Kap Kolias", "genre": "verse", "period": "N.A.", "pub_year": 1905, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Geweihtes Kap gew\u00e4hrt den Frauen Staunen.", "tokens": ["Ge\u00b7weih\u00b7tes", "Kap", "ge\u00b7w\u00e4hrt", "den", "Frau\u00b7en", "Stau\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Es staut vor hohen Wogen sich der Zug.", "tokens": ["Es", "staut", "vor", "ho\u00b7hen", "Wo\u00b7gen", "sich", "der", "Zug", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "PRF", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ein Pilger ruht durch eigner Brust: Genug!", "tokens": ["Ein", "Pil\u00b7ger", "ruht", "durch", "eig\u00b7ner", "Brust", ":", "Ge\u00b7nug", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$.", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Gesang verkl\u00e4rt der Brandung schweres Raunen,", "tokens": ["Ge\u00b7sang", "ver\u00b7kl\u00e4rt", "der", "Bran\u00b7dung", "schwe\u00b7res", "Rau\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Des Meeres Ernst lebendigt tausend Launen;", "tokens": ["Des", "Mee\u00b7res", "Ernst", "le\u00b7ben\u00b7digt", "tau\u00b7send", "Lau\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "VVFIN", "CARD", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wer sich beherrscht, erkennt es: ohne Trug.", "tokens": ["Wer", "sich", "be\u00b7herrscht", ",", "er\u00b7kennt", "es", ":", "oh\u00b7ne", "Trug", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "PRF", "VVPP", "$,", "VVFIN", "PPER", "$.", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Besteht ein Mensch, der keinen Zwist vertrug?", "tokens": ["Be\u00b7steht", "ein", "Mensch", ",", "der", "kei\u00b7nen", "Zwist", "ver\u00b7trug", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "PRELS", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Gib St\u00fcrmen Kriegers Antwort durch Posaunen!", "tokens": ["Gib", "St\u00fcr\u00b7men", "Krie\u00b7gers", "Ant\u00b7wort", "durch", "Po\u00b7sau\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "NN", "NN", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Hier gilt es kaum, das Meer zu \u00fcbert\u00f6nen.", "tokens": ["Hier", "gilt", "es", "kaum", ",", "das", "Meer", "zu", "\u00fc\u00b7ber\u00b7t\u00f6\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wo \u00fcberwand sich je Gewalt ein Herz?", "tokens": ["Wo", "\u00fc\u00b7ber\u00b7wand", "sich", "je", "Ge\u00b7walt", "ein", "Herz", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "