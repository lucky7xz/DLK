{"textgrid.poem.24951": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Am Kamin", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "St\u00fcrme, Dezember, vor meinem Gemach,", "tokens": ["St\u00fcr\u00b7me", ",", "De\u00b7zem\u00b7ber", ",", "vor", "mei\u00b7nem", "Ge\u00b7mach", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.2": {"text": "H\u00e4nge Zapfen von Eis an das Dach;", "tokens": ["H\u00e4n\u00b7ge", "Zap\u00b7fen", "von", "Eis", "an", "das", "Dach", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPR", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Nichts doch wei\u00df ich vom Froste;", "tokens": ["Nichts", "doch", "wei\u00df", "ich", "vom", "Fros\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Hier am w\u00e4rmenden, trauten Kamin", "tokens": ["Hier", "am", "w\u00e4r\u00b7men\u00b7den", ",", "trau\u00b7ten", "Ka\u00b7min"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "APPRART", "VAFIN", "$,", "ADJA", "NN"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.5": {"text": "Ist mir, als ob des Fr\u00fchlings Gr\u00fcn", "tokens": ["Ist", "mir", ",", "als", "ob", "des", "Fr\u00fch\u00b7lings", "Gr\u00fcn"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "$,", "KOKOM", "KOUS", "ART", "NN", "NN"], "meter": "--+--+-+", "m