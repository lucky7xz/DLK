{"dta.poem.168": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "An seine Augen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Jhr Augen seht die Rosilis nicht an/", "tokens": ["Ihr", "Au\u00b7gen", "seht", "die", "Ro\u00b7si\u00b7lis", "nicht", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NE", "PTKNEG", "PTKVZ", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Jhr/ die ihr mir die gr\u00f6\u00dfte Marter seyd;", "tokens": ["Ihr", "/", "die", "ihr", "mir", "die", "gr\u00f6\u00df\u00b7te", "Mar\u00b7ter", "seyd", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$(", "PRELS", "PPER", "PRF", "ART", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Jhr bringt vor Lust nur herbe Traurigkeit:", "tokens": ["Ihr", "bringt", "vor", "Lust", "nur", "her\u00b7be", "Trau\u00b7rig\u00b7keit", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ach da\u00df eur Blitz mich nicht erretten kan!", "tokens": ["Ach", "da\u00df", "eur", "Blitz", "mich", "nicht", "er\u00b7ret\u00b7ten", "kan", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "KOUS", "PPOSAT", "NN", "PPER", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Durch euch wird mir der meiste Drampff gethan/", "tokens": ["Durch", "euch", "wird", "mir", "der", "meis\u00b7te", "Drampff", "ge\u00b7than", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "PPER", "ART", "ADJA", "NN", "VVPP", "$("], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "Jhr macht aus Ruh mir t\u00e4glich Zanck und Streit/", "tokens": ["Ihr", "macht", "aus", "Ruh", "mir", "t\u00e4g\u00b7lich", "Zanck", "und", "Streit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "PPER", "ADJD", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Wenn ihr euch vor ", "tokens": ["Wenn", "ihr", "euch", "vor"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "APPR"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "So bringt ihr mich auf eine Marter-Bahn.", "tokens": ["So", "bringt", "ihr", "mich", "auf", "ei\u00b7ne", "Mar\u00b7ter\u00b7Bahn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Ich bin nicht Herr von euch ihr falschen Augen/", "tokens": ["Ich", "bin", "nicht", "Herr", "von", "euch", "ihr", "fal\u00b7schen", "Au\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "NN", "APPR", "PPER", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Jhr lasset mich die heissen Funcken saugen/", "tokens": ["Ihr", "las\u00b7set", "mich", "die", "heis\u00b7sen", "Fun\u00b7cken", "sau\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Die ", "tokens": ["Die"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.12": {"text": "Und macht den Schein durch ihren Blitz zu nichte/", "tokens": ["Und", "macht", "den", "Schein", "durch", "ih\u00b7ren", "Blitz", "zu", "nich\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "APPR", "PIS", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Ach! Augen r\u00e4cht die Ungerechtigkeiten/", "tokens": ["Ach", "!", "Au\u00b7gen", "r\u00e4cht", "die", "Un\u00b7ge\u00b7rech\u00b7tig\u00b7kei\u00b7ten", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "NN", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "La\u00df ", "tokens": ["La\u00df"], "token_info": ["word"], "pos": ["VVIMP"], "meter": "+", "measure": "single.up"}}}}}