{"dta.poem.20931": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Ungenand an Agnes.", "genre": "Lyrik; Prosa; Drama", "period": "N.A.", "pub_year": "1679", "urn": "urn:nbn:de:kobv:b4-20289-1", "language": ["de:0.99"], "booktitle": "Hofmann von Hofmannswaldau, Christian: Deutsche Ubersetzungen und Gedichte. Breslau, 1679."}, "poem": {"stanza.1": {"line.1": {"text": "Jst dieses was ich soll von meiner Agnes ha-", "tokens": ["Ist", "die\u00b7ses", "was", "ich", "soll", "von", "mei\u00b7ner", "Ag\u00b7nes", "ha"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PDAT", "PWS", "PPER", "VMFIN", "APPR", "PPOSAT", "NE", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "ben?", "tokens": ["ben", "?"], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "-", "measure": "single.down"}, "line.3": {"text": "Soll Gru\u00df und Abschied denn nah\u2019 aneinan-", "tokens": ["Soll", "Gru\u00df", "und", "Ab\u00b7schied", "denn", "nah'", "an\u00b7ein\u00b7an"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "NN", "KON", "NN", "KON", "ADJD", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "der stehn?", "tokens": ["der", "stehn", "?"], "token_info": ["word", "word", "punct"], "pos": ["ART", "VVINF", "$."], "meter": "-+", "measure": "iambic.single"}, "line.5": {"text": "L\u00e4st meine Taube sich \u00fcmbgeben schwartze Raben?", "tokens": ["L\u00e4st", "mei\u00b7ne", "Tau\u00b7be", "sich", "\u00fcmb\u00b7ge\u00b7ben", "schwart\u00b7ze", "Ra\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PRF", "ADV", "ADJA", "NN", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Mu\u00df meine Sonne denn so schimpflich untergehn?", "tokens": ["Mu\u00df", "mei\u00b7ne", "Son\u00b7ne", "denn", "so", "schimpf\u00b7lich", "un\u00b7ter\u00b7gehn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "KON", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ist kein Erbarmnis mehr in dieser Welt zufinden?", "tokens": ["Ist", "kein", "Er\u00b7barm\u00b7nis", "mehr", "in", "die\u00b7ser", "Welt", "zu\u00b7fin\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "ADV", "APPR", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Kennt Blut den\u0303 Blut nicht mehr? kennt mich mein", "tokens": ["Kennt", "Blut", "de\u00f1", "Blut", "nicht", "mehr", "?", "kennt", "mich", "mein"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "NN", "ART", "NN", "PTKNEG", "ADV", "$.", "VVFIN", "PPER", "PPOSAT"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Vater nicht?", "tokens": ["Va\u00b7ter", "nicht", "?"], "token_info": ["word", "word", "punct"], "pos": ["NN", "PTKNEG", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.10": {"text": "L\u00e4st er mit Kett\u2019 und Band dich meine Seele binden?", "tokens": ["L\u00e4st", "er", "mit", "Kett'", "und", "Band", "dich", "mei\u00b7ne", "See\u00b7le", "bin\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "KON", "NN", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "So bin ich allbereit erb\u00e4rmlich hingericht.", "tokens": ["So", "bin", "ich", "all\u00b7be\u00b7reit", "er\u00b7b\u00e4rm\u00b7lich", "hin\u00b7ge\u00b7richt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Ach w\u00e4r\u2019 ich hingericht! Er l\u00e4st mich in dem Leben/", "tokens": ["Ach", "w\u00e4r'", "ich", "hin\u00b7ge\u00b7richt", "!", "Er", "l\u00e4st", "mich", "in", "dem", "Le\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VAFIN", "PPER", "VVPP", "$.", "PPER", "VVFIN", "PRF", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Damit ich schmecken soll die Galle meiner Noth;", "tokens": ["Da\u00b7mit", "ich", "schme\u00b7cken", "soll", "die", "Gal\u00b7le", "mei\u00b7ner", "Noth", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVINF", "VMFIN", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Er l\u00e4st den Todes Stifft auf meinen Hertzen schweben/", "tokens": ["Er", "l\u00e4st", "den", "To\u00b7des", "Stifft", "auf", "mei\u00b7nen", "Hert\u00b7zen", "schwe\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Denn ohne dich zuseyn/ ist \u00e4rger als der Todt.", "tokens": ["Denn", "oh\u00b7ne", "dich", "zu\u00b7seyn", "/", "ist", "\u00e4r\u00b7ger", "als", "der", "Todt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "ADV", "$(", "VAFIN", "ADJD", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Die Sinnen wancken mir/ die Feder will nicht schrei-", "tokens": ["Die", "Sin\u00b7nen", "wan\u00b7cken", "mir", "/", "die", "Fe\u00b7der", "will", "nicht", "schrei"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "$(", "ART", "NN", "VMFIN", "PTKNEG", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "ben/", "tokens": ["ben", "/"], "token_info": ["word", "punct"], "pos": ["VVPP", "$("], "meter": "-", "measure": "single.down"}, "line.18": {"text": "Das Hertze waltzet sich und will mit Macht zu dir/", "tokens": ["Das", "Hert\u00b7ze", "walt\u00b7zet", "sich", "und", "will", "mit", "Macht", "zu", "dir", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "VVFIN", "PRF", "KON", "VMFIN", "APPR", "NN", "APPR", "PPER", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Es scheut sich ohne dich itzt mehr in mir zubleiben/", "tokens": ["Es", "scheut", "sich", "oh\u00b7ne", "dich", "itzt", "mehr", "in", "mir", "zu\u00b7blei\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "PPER", "ADV", "ADV", "APPR", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Und was nur Marter heist das find\u2019 sich itzt in Mir.", "tokens": ["Und", "was", "nur", "Mar\u00b7ter", "heist", "das", "find'", "sich", "itzt", "in", "Mir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADV", "NN", "VAFIN", "PDS", "VVFIN", "PRF", "ADV", "APPR", "NE", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Es steht die Schuldigkeit mir trotzig im Gesichte/", "tokens": ["Es", "steht", "die", "Schul\u00b7dig\u00b7keit", "mir", "trot\u00b7zig", "im", "Ge\u00b7sich\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PPER", "ADJD", "APPRART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Und spricht mir deutlich zu/ ist di\u00df die heisse Gluth?", "tokens": ["Und", "spricht", "mir", "deut\u00b7lich", "zu", "/", "ist", "di\u00df", "die", "heis\u00b7se", "Gluth", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "PTKZU", "$(", "VAFIN", "PDS", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "L\u00e4st Albrecht seine Braut vergehen im Gerichte?", "tokens": ["L\u00e4st", "Al\u00b7brecht", "sei\u00b7ne", "Braut", "ver\u00b7ge\u00b7hen", "im", "Ge\u00b7rich\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "PPOSAT", "NN", "VVINF", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Ist dieses seine Treu? ist di\u00df sein Helden Muth?", "tokens": ["Ist", "die\u00b7ses", "sei\u00b7ne", "Treu", "?", "ist", "di\u00df", "sein", "Hel\u00b7den", "Muth", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "PPOSAT", "NN", "$.", "VAFIN", "PDS", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Kan seine Liebe denn die Riegel nicht zerbrechen?", "tokens": ["Kan", "sei\u00b7ne", "Lie\u00b7be", "denn", "die", "Rie\u00b7gel", "nicht", "zer\u00b7bre\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "KON", "ART", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Ver\u00fcbt sie di\u00df nicht mehr was in der alten Zeit?", "tokens": ["Ver\u00b7\u00fcbt", "sie", "di\u00df", "nicht", "mehr", "was", "in", "der", "al\u00b7ten", "Zeit", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PDS", "PTKNEG", "ADV", "PWS", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Kan seine Man\u0303heit sich nicht an den Richtern r\u00e4chen?", "tokens": ["Kan", "sei\u00b7ne", "Ma\u00f1heit", "sich", "nicht", "an", "den", "Rich\u00b7tern", "r\u00e4\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "PRF", "PTKNEG", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.28": {"text": "Hat denn ein Augenblick die Kr\u00e4fften abgemayt?", "tokens": ["Hat", "denn", "ein", "Au\u00b7gen\u00b7blick", "die", "Kr\u00e4ff\u00b7ten", "ab\u00b7ge\u00b7mayt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Hergegen mu\u00df ich auch den Schlu\u00df des Himmels h\u00f6-", "tokens": ["Her\u00b7ge\u00b7gen", "mu\u00df", "ich", "auch", "den", "Schlu\u00df", "des", "Him\u00b7mels", "h\u00f6"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VMFIN", "PPER", "ADV", "ART", "NN", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "ren/", "tokens": ["ren", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "-", "measure": "single.down"}, "line.31": {"text": "Der als ein harter Schlag mir in die Ohren f\u00e4llt/", "tokens": ["Der", "als", "ein", "har\u00b7ter", "Schlag", "mir", "in", "die", "Oh\u00b7ren", "f\u00e4llt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "KOUS", "ART", "ADJA", "NN", "PPER", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Du solt/ soviel du kanst/ den alten Vater ehren/", "tokens": ["Du", "solt", "/", "so\u00b7viel", "du", "kanst", "/", "den", "al\u00b7ten", "Va\u00b7ter", "eh\u00b7ren", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$(", "VVFIN", "PPER", "VMFIN", "$(", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Er hat dich neben Gott auf diese Welt gestellt.", "tokens": ["Er", "hat", "dich", "ne\u00b7ben", "Gott", "auf", "die\u00b7se", "Welt", "ge\u00b7stellt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PRF", "APPR", "NN", "APPR", "PDAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Mit Eltern soll man nur mit Demuths Waffen strei-", "tokens": ["Mit", "El\u00b7tern", "soll", "man", "nur", "mit", "De\u00b7muths", "Waf\u00b7fen", "strei"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VMFIN", "PIS", "ADV", "APPR", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "ten/", "tokens": ["ten", "/"], "token_info": ["word", "punct"], "pos": ["FM", "$("], "meter": "-", "measure": "single.down"}, "line.36": {"text": "Jhr Wort\u2019 und Wille soll uns ein Gesetze seyn/", "tokens": ["Ihr", "Wort'", "und", "Wil\u00b7le", "soll", "uns", "ein", "Ge\u00b7set\u00b7ze", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "VMFIN", "PPER", "ART", "NN", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Jhr Seegen kan uns Heil und Wohlfarth zubereiten/", "tokens": ["Ihr", "See\u00b7gen", "kan", "uns", "Heil", "und", "Wohl\u00b7farth", "zu\u00b7be\u00b7rei\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PPER", "NN", "KON", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Und Jhrer Fl\u00fcche Sturm reist alles gutes ein.", "tokens": ["Und", "Ih\u00b7rer", "Fl\u00fc\u00b7che", "Sturm", "reist", "al\u00b7les", "gu\u00b7tes", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NN", "VVFIN", "PIAT", "ADJA", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "So mu\u00df ich zwischen Blut und heissen Flammen lie-", "tokens": ["So", "mu\u00df", "ich", "zwi\u00b7schen", "Blut", "und", "heis\u00b7sen", "Flam\u00b7men", "lie"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "NN", "KON", "ADJA", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "gen/", "tokens": ["gen", "/"], "token_info": ["word", "punct"], "pos": ["APPR", "$("], "meter": "-", "measure": "single.down"}, "line.41": {"text": "Bin schimpflich halb befleckt/ und schmertzlich halb ver-", "tokens": ["Bin", "schimpf\u00b7lich", "halb", "be\u00b7fleckt", "/", "und", "schmertz\u00b7lich", "halb", "ver"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "ADJD", "ADJD", "VVPP", "$(", "KON", "ADJD", "ADJD", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.42": {"text": "brennt/", "tokens": ["brennt", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "-", "measure": "single.down"}, "line.43": {"text": "Und mu\u00df den schwachen Hals f\u00fcr dem Verh\u00e4ngn\u00fc\u00df", "tokens": ["Und", "mu\u00df", "den", "schwa\u00b7chen", "Hals", "f\u00fcr", "dem", "Ver\u00b7h\u00e4ng\u00b7n\u00fc\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "ART", "ADJA", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.44": {"text": "biegen/", "tokens": ["bie\u00b7gen", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.45": {"text": "So diese gantze Welt vor ihren Zaum erkennt.", "tokens": ["So", "die\u00b7se", "gant\u00b7ze", "Welt", "vor", "ih\u00b7ren", "Zaum", "er\u00b7kennt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDAT", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "Ich werde nur erstum\u0303t itzunder warten m\u00fcssen/", "tokens": ["Ich", "wer\u00b7de", "nur", "er\u00b7stum\u0303t", "it\u00b7zun\u00b7der", "war\u00b7ten", "m\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "VVFIN", "ADV", "VVINF", "VMINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "Was \u00fcber dich und mich die Welt beschlossen hat/", "tokens": ["Was", "\u00fc\u00b7ber", "dich", "und", "mich", "die", "Welt", "be\u00b7schlos\