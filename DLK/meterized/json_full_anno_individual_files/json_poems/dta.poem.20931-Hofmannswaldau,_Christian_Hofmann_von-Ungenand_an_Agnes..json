: "alexandrine.iambic.hexa"}, "line.115": {"text": "Und kan mein Hertze nicht mit deinem Hertzen ster-", "tokens": ["Und", "kan", "mein", "Hert\u00b7ze", "nicht", "mit", "dei\u00b7nem", "Hert\u00b7zen", "ster"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPOSAT", "VVFIN", "PTKNEG", "APPR", "PPOSAT", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.116": {"text": "ben/", "tokens": ["ben", "/"], "token_info": ["word", "punct"], "pos": ["VVPP", "$("], "meter": "-", "measure": "single.down"}, "line.117": {"text": "So la\u00df doch meine Hand mit dir zu Grabe gehn.", "tokens": ["So", "la\u00df", "doch", "mei\u00b7ne", "Hand", "mit", "dir", "zu", "Gra\u00b7be", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "ADV", "PPOSAT", "NN", "APPR", "PPER", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}