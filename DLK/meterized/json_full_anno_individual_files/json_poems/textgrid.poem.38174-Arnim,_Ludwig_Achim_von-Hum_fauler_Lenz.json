{"textgrid.poem.38174": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Hum fauler Lenz", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es wollt eine Frau zu Weine gahn. Hum fauler Lenz.", "tokens": ["Es", "wollt", "ei\u00b7ne", "Frau", "zu", "Wei\u00b7ne", "gahn", ".", "Hum", "fau\u00b7ler", "Lenz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "APPR", "NN", "VVFIN", "$.", "NE", "ADJA", "NN", "$."], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und wollt den Mann nicht mit sich han. Ha ha ha.", "tokens": ["Und", "wollt", "den", "Mann", "nicht", "mit", "sich", "han", ".", "Ha", "ha", "ha", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "NN", "PTKNEG", "APPR", "PRF", "VAFIN", "$.", "ITJ", "ITJ", "ITJ", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Du mu\u00dft zu Hause bleiben. Hum usw.", "tokens": ["Du", "mu\u00dft", "zu", "Hau\u00b7se", "blei\u00b7ben", ".", "Hum", "us\u00b7w."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "abbreviation"], "pos": ["PPER", "VMFIN", "APPR", "NN", "VVINF", "$.", "NE", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sollst K\u00fch und K\u00e4lber treiben. Ha usw.", "tokens": ["Sollst", "K\u00fch", "und", "K\u00e4l\u00b7ber", "trei\u00b7ben", ".", "Ha", "us\u00b7w."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "abbreviation"], "pos": ["VMFIN", "NN", "KON", "NN", "VVINF", "$.", "NE", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"t