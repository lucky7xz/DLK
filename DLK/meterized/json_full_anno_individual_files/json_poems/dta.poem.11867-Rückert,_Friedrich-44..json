{"dta.poem.11867": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "44.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "So sprach des L\u00f6wen Muth zu seinem eignen Rachen,", "tokens": ["So", "sprach", "des", "L\u00f6\u00b7wen", "Muth", "zu", "sei\u00b7nem", "eig\u00b7nen", "Ra\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als er in ihrem Nest fand eine Brut von Drachen:", "tokens": ["Als", "er", "in", "ih\u00b7rem", "Nest", "fand", "ei\u00b7ne", "Brut", "von", "Dra\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Fri\u00df du zum Fr\u00fchst\u00fcck sie oder zum Mittagsessen,", "tokens": ["Fri\u00df", "du", "zum", "Fr\u00fch\u00b7st\u00fcck", "sie", "o\u00b7der", "zum", "Mit\u00b7ta\u00b7gses\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "PPER", "KON", "APPRART", "NN", "$,"], "meter": "+--+--+--+-+-", "measure": "dactylic.tri.plus"}, "line.2": {"text": "Eh sie zur Vesper dich oder zur Nachtkost fressen.", "tokens": ["Eh", "sie", "zur", "Ves\u00b7per", "dich", "o\u00b7der", "zur", "Nacht\u00b7kost", "fres\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "PPER", "KON", "APPRART