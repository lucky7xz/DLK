{"dta.poem.6829": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von dem Sitz der Sinnlichkeiten.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Auf solche Weise nun wird nnsuns von allen Sinnen", "tokens": ["Auf", "sol\u00b7che", "Wei\u00b7se", "nun", "wird", "nn\u00b7suns", "von", "al\u00b7len", "Sin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "ADV", "VAFIN", "PPER", "APPR", "PIAT", "NN"], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Die W\u00fcrckung mitgetheilt. Der Leib, durch welchen wir", "tokens": ["Die", "W\u00fcr\u00b7ckung", "mit\u00b7ge\u00b7theilt", ".", "Der", "Leib", ",", "durch", "wel\u00b7chen", "wir"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVPP", "$.", "ART", "NN", "$,", "APPR", "PWAT", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Dies so empfinden, ist ein wol gestimmt Clavier,", "tokens": ["Dies", "so", "emp\u00b7fin\u00b7den", ",", "ist", "ein", "wol", "ge\u00b7stimmt", "Cla\u00b7vier", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "VVFIN", "$,", "VAFIN", "ART", "ADV", "VVPP", "NE", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}