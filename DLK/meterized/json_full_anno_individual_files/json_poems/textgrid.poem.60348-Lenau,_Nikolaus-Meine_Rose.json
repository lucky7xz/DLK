{"textgrid.poem.60348": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Meine Rose", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dem holden Lenzgeschmeide,", "tokens": ["Dem", "hol\u00b7den", "Lenz\u00b7ge\u00b7schmei\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Rose, meiner Freude,", "tokens": ["Der", "Ro\u00b7se", ",", "mei\u00b7ner", "Freu\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die schon gebeugt und blasser", "tokens": ["Die", "schon", "ge\u00b7beugt", "und", "blas\u00b7ser"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "VVPP", "KON", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Vom hei\u00dfen Strahl der Sonnen,", "tokens": ["Vom", "hei\u00b7\u00dfen", "Strahl", "der", "Son\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Reich' ich den Becher Wasser", "tokens": ["Reich'", "ich", "den", "Be\u00b7cher", "Was\u00b7ser"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Aus tiefem Bronnen.", "tokens": ["Aus", "tie\u00b7fem", "Bron\u00b7nen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Du Rose meines Herzens!", "tokens": ["Du", "Ro\u00b7se", "mei\u00b7nes", "Her\u00b7zens", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Vom stillen Strahl des Schmerzens", "tokens": ["Vom", "stil\u00b7len", "Strahl", "des", "Schmer\u00b7zens"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Bist du gebeugt und blasser;", "tokens": ["Bist", "du", "ge\u00b7beugt", "und", "blas\u00b7ser", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "KON", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Ich m\u00f6chte dir zu F\u00fc\u00dfen,", "tokens": ["Ich", "m\u00f6ch\u00b7te", "dir", "zu", "F\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Wie dieser Blume Wasser,", "tokens": ["Wie", "die\u00b7ser", "Blu\u00b7me", "Was\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PDAT", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Still meine Seele gie\u00dfen!", "tokens": ["Still", "mei\u00b7ne", "See\u00b7le", "gie\u00b7\u00dfen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "K\u00f6nnt ich dann auch nicht sehen", "tokens": ["K\u00f6nnt", "ich", "dann", "auch", "nicht", "se\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ADV", "ADV", "PTKNEG", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.14": {"text": "Dich auferstehen.", "tokens": ["Dich", "auf\u00b7er\u00b7ste\u00b7hen", "."], "token_info": ["word",