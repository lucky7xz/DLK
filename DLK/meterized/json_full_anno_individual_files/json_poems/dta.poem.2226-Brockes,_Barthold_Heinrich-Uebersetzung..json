{"dta.poem.2226": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Uebersetzung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Bey der verderbten Phantasey, sagt man, mit Recht,", "tokens": ["Bey", "der", "ver\u00b7derb\u00b7ten", "Phan\u00b7ta\u00b7sey", ",", "sagt", "man", ",", "mit", "Recht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "VVFIN", "PIS", "$,", "APPR", "NN", "$,"], "meter": "+--+--+-+--+", "measure": "dactylic.di.plus"}, "line.2": {"text": "von unsrer Erden:", "tokens": ["von", "uns\u00b7rer", "Er\u00b7den", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Sie sey ein Ort, wo, was man f\u00fcrchtet, oft, selten, was", "tokens": ["Sie", "sey", "ein", "Ort", ",", "wo", ",", "was", "man", "f\u00fcrch\u00b7tet", ",", "oft", ",", "sel\u00b7ten", ",", "was"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PWAV", "$,", "PRELS", "PIS", "VVFIN", "$,", "ADV", "$,", "ADJD", "$,", "PWS"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "man w\u00fcnscht, geschicht.", "tokens": ["man", "w\u00fcnscht", ",", "ge\u00b7schicht", "."], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Wo mancher: ich war einsten gl\u00fccklich! Ein gro\u00df Theil:", "tokens": ["Wo", "man\u00b7cher", ":", "ich", "war", "eins\u00b7ten", "gl\u00fcck\u00b7lich", "!", "Ein", "gro\u00df", "Theil", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "$.", "PPER", "VAFIN", "ADV", "ADJD", "$.", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "gl\u00fccklich werd' ich werden!", "tokens": ["gl\u00fcck\u00b7lich", "werd'", "ich", "wer\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "VAINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Und keiner: ich bin gl\u00fccklich! spricht.", "tokens": ["Und", "kei\u00b7ner", ":", "ich", "bin", "gl\u00fcck\u00b7lich", "!", "spricht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PIS", "$.", "PPER", "VAFIN", "ADJD", "$.", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}