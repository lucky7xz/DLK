{"textgrid.poem.40551": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "Bundeslied", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nicht mit Spie\u00dfen, M\u00f6rsern, Stangen", "tokens": ["Nicht", "mit", "Spie\u00b7\u00dfen", ",", "M\u00f6r\u00b7sern", ",", "Stan\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word"], "pos": ["PTKNEG", "APPR", "NN", "$,", "NN", "$,", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ziehn wir in den heil'gen Streit;", "tokens": ["Ziehn", "wir", "in", "den", "heil'\u00b7gen", "Streit", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mag nach solchen Waffen langen,", "tokens": ["Mag", "nach", "sol\u00b7chen", "Waf\u00b7fen", "lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "PIAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wer nicht bessre h\u00e4lt bereit!", "tokens": ["Wer", "nicht", "bess\u00b7re", "h\u00e4lt", "be\u00b7reit", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PTKNEG", "VVFIN", "VVFIN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Nicht ist in der Burg von Steine", "tokens": ["Nicht", "ist", "in", "der", "Burg", "von", "Stei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "VAFIN", "APPR", "ART", "NN", "APPR", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Uns verschanzt der Heeresbann,", "tokens": ["Uns", "ver\u00b7schanzt", "der", "Hee\u00b7res\u00b7bann", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nein, im Busen drin die seine", "tokens": ["Nein", ",", "im", "Bu\u00b7sen", "drin", "die", "sei\u00b7ne"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "APPRART", "NN", "ADV", "ART", "PPOSAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schirmt wohl auch der einz'le Mann.", "tokens": ["Schirmt", "wohl", "auch", "der", "einz'\u00b7le", "Mann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Dem sorglosen Feind beim Becher", "tokens": ["Dem", "sorg\u00b7lo\u00b7sen", "Feind", "beim", "Be\u00b7cher"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Senden wir nicht Dolch und Gift;", "tokens": ["Sen\u00b7den", "wir", "nicht", "Dolch", "und", "Gift", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sonnenstrahl ist unser R\u00e4cher,", "tokens": ["Son\u00b7nen\u00b7strahl", "ist", "un\u00b7ser", "R\u00e4\u00b7cher", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Weh, wen der ins Herz nicht trifft!", "tokens": ["Weh", ",", "wen", "der", "ins", "Herz", "nicht", "trifft", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWS", "ART", "APPRART", "NN", "PTKNEG", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Nicht ein Streit um Landesmarken", "tokens": ["Nicht", "ein", "Streit", "um", "Lan\u00b7des\u00b7mar\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ART", "NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und um irdisch Gut und Blut,", "tokens": ["Und", "um", "ir\u00b7disch", "Gut", "und", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUI", "ADJD", "ADJD", "KON", "NN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Nein, uns macht zum Kampf erstarken", "tokens": ["Nein", ",", "uns", "macht", "zum", "Kampf", "er\u00b7star\u00b7ken"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "PPER", "VVFIN", "APPRART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ein unsterblich, g\u00f6ttlich Gut!", "tokens": ["Ein", "uns\u00b7terb\u00b7lich", ",", "g\u00f6tt\u00b7lich", "Gut", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJD", "$,", "ADJD", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "In dem dunklen Bauch der Berge", "tokens": ["In", "dem", "dunk\u00b7len", "Bauch", "der", "Ber\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Suchet unser Zeughaus nicht,", "tokens": ["Su\u00b7chet", "un\u00b7ser", "Zeug\u00b7haus", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PTKNEG", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn nicht sind Kobold' und Zwerge", "tokens": ["Denn", "nicht", "sind", "Ko\u00b7bold'", "und", "Zwer\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PTKNEG", "VAFIN", "NN", "KON", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Lehrer uns in Recht und Pflicht.", "tokens": ["Leh\u00b7rer", "uns", "in", "Recht", "und", "Pflicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Klimmt zu h\u00f6chsten Bergesspitzen,", "tokens": ["Klimmt", "zu", "h\u00f6chs\u00b7ten", "Ber\u00b7ges\u00b7spit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dann vor euch im Sonnenstrahl", "tokens": ["Dann", "vor", "euch", "im", "Son\u00b7nen\u00b7strahl"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "PPER", "APPRART", "NN"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Seht ihr golden, silbern blitzen", "tokens": ["Seht", "ihr", "gol\u00b7den", ",", "sil\u00b7bern", "blit\u00b7zen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "ADJD", "$,", "KON", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Unser gro\u00dfes Arsenal.", "tokens": ["Un\u00b7ser", "gro\u00b7\u00dfes", "Ar\u00b7se\u00b7nal", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Lichteswaffen, die kein Meister", "tokens": ["Lich\u00b7tes\u00b7waf\u00b7fen", ",", "die", "kein", "Meis\u00b7ter"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "PRELS", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ird'scher Zunft euch schmieden darf,", "tokens": ["Ird'\u00b7scher", "Zunft", "euch", "schmie\u00b7den", "darf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPER", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und womit der Herr der Geister", "tokens": ["Und", "wo\u00b7mit", "der", "Herr", "der", "Geis\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "NN", "ART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Einst die s\u00fcnd'gen Engel warf;", "tokens": ["Einst", "die", "s\u00fcn\u00b7d'\u00b7gen", "En\u00b7gel", "warf", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.8": {"line.1": {"text": "Bundsgenossen, die entraffen", "tokens": ["Bunds\u00b7ge\u00b7nos\u00b7sen", ",", "die", "en\u00b7traf\u00b7fen"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$,", "PRELS", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Uns kein Kerker mag, kein Schwert!", "tokens": ["Uns", "kein", "Ker\u00b7ker", "mag", ",", "kein", "Schwert", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "PIAT", "NN", "VMFIN", "$,", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Fielen wir, stehn sie in Waffen", "tokens": ["Fie\u00b7len", "wir", ",", "stehn", "sie", "in", "Waf\u00b7fen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "VVFIN", "PPER", "APPR", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Unserm Recht noch, unversehrt.", "tokens": ["Un\u00b7serm", "Recht", "noch", ",", "un\u00b7ver\u00b7sehrt", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "$,", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Unsre Losung, h\u00f6rt sie schallen", "tokens": ["Uns\u00b7re", "Lo\u00b7sung", ",", "h\u00f6rt", "sie", "schal\u00b7len"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Leis und laut im L\u00fcftezug!", "tokens": ["Leis", "und", "laut", "im", "L\u00fcf\u00b7te\u00b7zug", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Vorw\u00e4rts! rauscht der Strom im Wallen,", "tokens": ["Vor\u00b7w\u00e4rts", "!", "rauscht", "der", "Strom", "im", "Wal\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Vorw\u00e4rts! dr\u00f6hnt die Wolk' im Flug.", "tokens": ["Vor\u00b7w\u00e4rts", "!", "dr\u00f6hnt", "die", "Wol\u00b7k'", "im", "Flug", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.10": {"line.1": {"text": "Der Gedanke, der uns b\u00fcndet,", "tokens": ["Der", "Ge\u00b7dan\u00b7ke", ",", "der", "uns", "b\u00fcn\u00b7det", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Siegreich schwebt er ob dem All,", "tokens": ["Sieg\u00b7reich", "schwebt", "er", "ob", "dem", "All", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "KOUS", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dort als Nordens Licht entz\u00fcndet,", "tokens": ["Dort", "als", "Nor\u00b7dens", "Licht", "ent\u00b7z\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "NN", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hier im Bergschacht als Kristall.", "tokens": ["Hier", "im", "Berg\u00b7schacht", "als", "Kris\u00b7tall", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "KOUS", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.11": {"line.1": {"text": "Aus des Vogels Kehle dr\u00e4ngt er", "tokens": ["Aus", "des", "Vo\u00b7gels", "Keh\u00b7le", "dr\u00e4ngt", "er"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN", "VVFIN", "PPER"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sich als Lied im L\u00fcfteraum,", "tokens": ["Sich", "als", "Lied", "im", "L\u00fcf\u00b7te\u00b7raum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "KOUS", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und verwandelt wieder h\u00e4ngt er", "tokens": ["Und", "ver\u00b7wan\u00b7delt", "wie\u00b7der", "h\u00e4ngt", "er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "VVFIN", "PPER"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dort als Bl\u00fcthenreis am Baum.", "tokens": ["Dort", "als", "Bl\u00fc\u00b7then\u00b7reis", "am", "Baum", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Wie ein s\u00fc\u00df Geheimni\u00df spendet", "tokens": ["Wie", "ein", "s\u00fc\u00df", "Ge\u00b7heim\u00b7ni\u00df", "spen\u00b7det"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJD", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Fl\u00fcsternd ihn der Wiesenbach,", "tokens": ["Fl\u00fcs\u00b7ternd", "ihn", "der", "Wie\u00b7sen\u00b7bach", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch als Donnerpredigt sendet", "tokens": ["Doch", "als", "Don\u00b7ner\u00b7pre\u00b7digt", "sen\u00b7det"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "KOUS", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ihn der Katarakt euch nach.", "tokens": ["Ihn", "der", "Ka\u00b7ta\u00b7rakt", "euch", "nach", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Ja der Blitz selbst, nachtentsprungen,", "tokens": ["Ja", "der", "Blitz", "selbst", ",", "nach\u00b7tents\u00b7prun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PTKANT", "ART", "NN", "ADV", "$,", "VVIZU", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn er durch die Wolken bricht,", "tokens": ["Wenn", "er", "durch", "die", "Wol\u00b7ken", "bricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Stottert nach mit trunknen Zungen", "tokens": ["Stot\u00b7tert", "nach", "mit", "trunk\u00b7nen", "Zun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "APPR", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gottes Wort: Es werde Licht!", "tokens": ["Got\u00b7tes", "Wort", ":", "Es", "wer\u00b7de", "Licht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$.", "PPER", "VAFIN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "Nicht mit Spie\u00dfen, M\u00f6rsern, Stangen", "tokens": ["Nicht", "mit", "Spie\u00b7\u00dfen", ",", "M\u00f6r\u00b7sern", ",", "Stan\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word"], "pos": ["PTKNEG", "APPR", "NN", "$,", "NN", "$,", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ziehn wir in den heil'gen Streit;", "tokens": ["Ziehn", "wir", "in", "den", "heil'\u00b7gen", "Streit", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mag nach solchen Waffen langen,", "tokens": ["Mag", "nach", "sol\u00b7chen", "Waf\u00b7fen", "lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "PIAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wer nicht bessre h\u00e4lt bereit!", "tokens": ["Wer", "nicht", "bess\u00b7re", "h\u00e4lt", "be\u00b7reit", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PTKNEG", "VVFIN", "VVFIN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.15": {"line.1": {"text": "Nicht ist in der Burg von Steine", "tokens": ["Nicht", "ist", "in", "der", "Burg", "von", "Stei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "VAFIN", "APPR", "ART", "NN", "APPR", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Uns verschanzt der Heeresbann,", "tokens": ["Uns", "ver\u00b7schanzt", "der", "Hee\u00b7res\u00b7bann", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nein, im Busen drin die seine", "tokens": ["Nein", ",", "im", "Bu\u00b7sen", "drin", "die", "sei\u00b7ne"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "APPRART", "NN", "ADV", "ART", "PPOSAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schirmt wohl auch der einz'le Mann.", "tokens": ["Schirmt", "wohl", "auch", "der", "einz'\u00b7le", "Mann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.16": {"line.1": {"text": "Dem sorglosen Feind beim Becher", "tokens": ["Dem", "sorg\u00b7lo\u00b7sen", "Feind", "beim", "Be\u00b7cher"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Senden wir nicht Dolch und Gift;", "tokens": ["Sen\u00b7den", "wir", "nicht", "Dolch", "und", "Gift", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sonnenstrahl ist unser R\u00e4cher,", "tokens": ["Son\u00b7nen\u00b7strahl", "ist", "un\u00b7ser", "R\u00e4\u00b7cher", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Weh, wen der ins Herz nicht trifft!", "tokens": ["Weh", ",", "wen", "der", "ins", "Herz", "nicht