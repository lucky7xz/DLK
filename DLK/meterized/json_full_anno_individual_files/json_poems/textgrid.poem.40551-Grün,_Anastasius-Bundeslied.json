": "dactylic.tri"}, "line.4": {"text": "Unserm Recht noch, unversehrt.", "tokens": ["Un\u00b7serm", "Recht", "noch", ",", "un\u00b7ver\u00b7sehrt", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "$,", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.22": {"line.1": {"text": "Unsre Losung, h\u00f6rt sie schallen", "tokens": ["Uns\u00b7re", "Lo\u00b7sung", ",", "h\u00f6rt", "sie", "schal\u00b7len"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Leis und laut im L\u00fcftezug!", "tokens": ["Leis", "und", "laut", "im", "L\u00fcf\u00b7te\u00b7zug", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Vorw\u00e4rts! rauscht der Strom im Wallen,", "tokens": ["Vor\u00b7w\u00e4rts", "!", "rauscht", "der", "Strom", "im", "Wal\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Vorw\u00e4rts! dr\u00f6hnt die Wolk' im Flug.", "tokens": ["Vor\u00b7w\u00e4rts", "!", "dr\u00f6hnt", "die", "Wol\u00b7k'", "im", "Flug", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.23": {"line.1": {"text": "Der Gedanke, der uns b\u00fcndet,", "tokens": ["Der", "Ge\u00b7dan\u00b7ke", ",", "der", "uns", "b\u00fcn\u00b7det", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure