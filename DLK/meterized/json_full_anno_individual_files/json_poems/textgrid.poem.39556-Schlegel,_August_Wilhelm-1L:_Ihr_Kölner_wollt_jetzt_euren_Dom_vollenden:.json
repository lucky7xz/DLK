{"textgrid.poem.39556": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ihr K\u00f6lner wollt jetzt euren Dom vollenden:", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr K\u00f6lner wollt jetzt euren Dom vollenden:", "tokens": ["Ihr", "K\u00f6l\u00b7ner", "wollt", "jetzt", "eu\u00b7ren", "Dom", "voll\u00b7en\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "ADV", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie kommt euch nur so pl\u00f6tzlich diese Lust?", "tokens": ["Wie", "kommt", "euch", "nur", "so", "pl\u00f6tz\u00b7lich", "die\u00b7se", "Lust", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "ADV", "ADJD", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Es lag ja immerfort in euren H\u00e4nden;", "tokens": ["Es", "lag", "ja", "im\u00b7mer\u00b7fort", "in", "eu\u00b7ren", "H\u00e4n\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ihr lie\u00dft ihn doch vierhundert Jahr' im Wust.", "tokens": ["Ihr", "lie\u00dft", "ihn", "doch", "vier\u00b7hun\u00b7dert", "Jahr'", "im", "Wust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "CARD", "NN", "APPRART", "NN", "$."], "meter": "-+-+++-+-+", "measure": "zehnsilber"}}, "stanza.2": {"line.1": {"text": "Drei K\u00f6nigssch\u00e4del liegen drin begraben,", "tokens": ["Drei", "K\u00f6\u00b7nigs\u00b7sch\u00e4\u00b7del", "lie\u00b7gen", "drin", "be\u00b7gra\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Kaiser Friedrich Rothbart hergesandt.", "tokens": ["Die", "Kai\u00b7ser", "Fried\u00b7rich", "Roth\u00b7bart", "her\u00b7ge\u00b7sandt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "NE", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da brachten fromme Pilger reiche Gaben.", "tokens": ["Da", "brach\u00b7ten", "from\u00b7me", "Pil\u00b7ger", "rei\u00b7che", "Ga\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Aus eurem Gut ward wenig aufgewandt.", "tokens": ["Aus", "eu\u00b7rem", "Gut", "ward", "we\u00b7nig", "auf\u00b7ge\u00b7wandt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Eu'r Bischof trug des Canzlers hohe W\u00fcrde,", "tokens": ["Eu'r", "Bi\u00b7schof", "trug", "des", "Canz\u00b7lers", "ho\u00b7he", "W\u00fcr\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des Bau's Beginn ward seiner M\u00fch' verdankt.", "tokens": ["Des", "Bau's", "Be\u00b7ginn", "ward", "sei\u00b7ner", "M\u00fch'", "ver\u00b7dankt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Habt ihr ihm wohl erleichtert diese B\u00fcrde?", "tokens": ["Habt", "ihr", "ihm", "wohl", "er\u00b7leich\u00b7tert", "die\u00b7se", "B\u00fcr\u00b7de", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "ADV", "VVPP", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ihr habt mit eurem Hirten stets gezankt.", "tokens": ["Ihr", "habt", "mit", "eu\u00b7rem", "Hir\u00b7ten", "stets", "ge\u00b7zankt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPOSAT", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Auch andre St\u00e4dte haben hehre Tempel,", "tokens": ["Auch", "and\u00b7re", "St\u00e4d\u00b7te", "ha\u00b7ben", "heh\u00b7re", "Tem\u00b7pel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VAFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die jede selbst erbaut hat und gepflegt.", "tokens": ["Die", "je\u00b7de", "selbst", "er\u00b7baut", "hat", "und", "ge\u00b7pflegt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "ADV", "VVPP", "VAFIN", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Meint ihr, da\u00df euer Chor allein den Stempel", "tokens": ["Meint", "ihr", ",", "da\u00df", "eu\u00b7er", "Chor", "al\u00b7lein", "den", "Stem\u00b7pel"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "PPOSAT", "NN", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Heiligkeit an seiner Stirne tr\u00e4gt?", "tokens": ["Der", "Hei\u00b7lig\u00b7keit", "an", "sei\u00b7ner", "Stir\u00b7ne", "tr\u00e4gt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Wien, Stra\u00dfburg, Rheims, der Marmordom in Mailand,", "tokens": ["Wi\u00b7en", ",", "Stra\u00df\u00b7burg", ",", "Rheims", ",", "der", "Mar\u00b7mor\u00b7dom", "in", "Mai\u00b7land", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NE", "$,", "NE", "$,", "ART", "NN", "APPR", "NE", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Viel' andre, die ich nicht zu nennen wei\u00df;", "tokens": ["Viel'", "and\u00b7re", ",", "die", "ich", "nicht", "zu", "nen\u00b7nen", "wei\u00df", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "$,", "PRELS", "PPER", "PTKNEG", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dann Canterbury in Britanniens Eiland,", "tokens": ["Dann", "Can\u00b7ter\u00b7bu\u00b7ry", "in", "Bri\u00b7tan\u00b7ni\u00b7ens", "Ei\u00b7land", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "APPR", "NE", "NN", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Westminster, York, verdienen gleichen Preis.", "tokens": ["West\u00b7mins\u00b7ter", ",", "Y\u00b7ork", ",", "ver\u00b7die\u00b7nen", "glei\u00b7chen", "Preis", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NE", "$,", "ADJA", "ADJA", "NN", "$."], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}}, "stanza.6": {"line.1": {"text": "Ihr habt wohl nie aus euren dumpfen Mauern", "tokens": ["Ihr", "habt", "wohl", "nie", "aus", "eu\u00b7ren", "dum\u00b7pfen", "Mau\u00b7ern"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Auf Deutschland und Europa rings geblickt:", "tokens": ["Auf", "Deutschland", "und", "Eu\u00b7ro\u00b7pa", "rings", "ge\u00b7blickt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "NE", "ADV", "VVPP", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Wie k\u00f6nnte sonst der leere Stolz noch dauern", "tokens": ["Wie", "k\u00f6nn\u00b7te", "sonst", "der", "lee\u00b7re", "Stolz", "noch", "dau\u00b7ern"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VMFIN", "ADV", "ART", "ADJA", "NN", "ADV", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Auf solch ein Bruchst\u00fcck, \u00e4rmlich ausgeflickt?", "tokens": ["Auf", "solch", "ein", "Bruchs\u00b7t\u00fcck", ",", "\u00e4rm\u00b7lich", "aus\u00b7ge\u00b7flickt", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ART", "NN", "$,", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Nun la\u00dft ihr durch die Zeitung ausposaunen:", "tokens": ["Nun", "la\u00dft", "ihr", "durch", "die", "Zei\u00b7tung", "aus\u00b7po\u00b7sau\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "K\u00f6ln ist die heil'ge Stadt vor allen, wi\u00dft!", "tokens": ["K\u00f6ln", "ist", "die", "heil'\u00b7ge", "Stadt", "vor", "al\u00b7len", ",", "wi\u00dft", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "ADJA", "NN", "APPR", "PIAT", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Deutschland! vernimm's in Demuth und mit Staunen!", "tokens": ["Deutschland", "!", "ver\u00b7nim\u00b7m's", "in", "De\u00b7muth", "und", "mit", "Stau\u00b7nen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "NE", "APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.4": {"text": "Wer nicht am Bau hilft, ist kein wahrer Christ! \u2013", "tokens": ["Wer", "nicht", "am", "Bau", "hilft", ",", "ist", "kein", "wah\u00b7rer", "Christ", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "PTKNEG", "APPRART", "NN", "VVFIN", "$,", "VAFIN", "PIAT", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Geht hin zum Papst, und bittet: Heil'ger Vater,", "tokens": ["Geht", "hin", "zum", "Papst", ",", "und", "bit\u00b7tet", ":", "Heil'\u00b7ger", "Va\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPRART", "NN", "$,", "KON", "VVFIN", "$.", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Borgt uns doch Geld, zu bauen unsern Dom! \u2013", "tokens": ["Borgt", "uns", "doch", "Geld", ",", "zu", "bau\u00b7en", "un\u00b7sern", "Dom", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "ADV", "NN", "$,", "PTKZU", "VVINF", "KON", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ich, wird er sagen, bin des Heils Berather;", "tokens": ["Ich", ",", "wird", "er", "sa\u00b7gen", ",", "bin", "des", "Heils", "Be\u00b7ra\u00b7ther", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "VAFIN", "PPER", "VVINF", "$,", "VAFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Doch Geld zu holen giebt es keins in Rom.", "tokens": ["Doch", "Geld", "zu", "ho\u00b7len", "giebt", "es", "keins", "in", "Rom", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PTKZU", "VVINF", "VVFIN", "PPER", "PIAT", "APPR", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Ihr K\u00f6lner wollt jetzt euren Dom vollenden:", "tokens": ["Ihr", "K\u00f6l\u00b7ner", "wollt", "jetzt", "eu\u00b7ren", "Dom", "voll\u00b7en\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "ADV", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie kommt euch nur so pl\u00f6tzlich diese Lust?", "tokens": ["Wie", "kommt", "euch", "nur", "so", "pl\u00f6tz\u00b7lich", "die\u00b7se", "Lust", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "ADV", "ADJD", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Es lag ja immerfort in euren H\u00e4nden;", "tokens": ["Es", "lag", "ja", "im\u00b7mer\u00b7fort", "in", "eu\u00b7ren", "H\u00e4n\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ihr lie\u00dft ihn doch vierhundert Jahr' im Wust.", "tokens": ["Ihr", "lie\u00dft", "ihn", "do