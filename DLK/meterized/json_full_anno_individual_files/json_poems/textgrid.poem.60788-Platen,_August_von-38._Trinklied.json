{"textgrid.poem.60788": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "38. Trinklied", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wohl bietet der irdische Tag qualvolle Sekunden genug.", "tokens": ["Wohl", "bie\u00b7tet", "der", "ir\u00b7di\u00b7sche", "Tag", "qual\u00b7vol\u00b7le", "Se\u00b7kun\u00b7den", "ge\u00b7nug", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "ADJA", "NN", "ADV", "$."], "meter": "-+--+--+-+-+-+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Wenn tief du gedenkend erw\u00e4gst, was je du verlorst, o Gem\u00fct!", "tokens": ["Wenn", "tief", "du", "ge\u00b7den\u00b7kend", "er\u00b7w\u00e4gst", ",", "was", "je", "du", "ver\u00b7lorst", ",", "o", "Ge\u00b7m\u00fct", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "PPER", "VVPP", "VVFIN", "$,", "PRELS", "ADV", "PPER", "VVFIN", "$,", "FM", "NN", "$."], "meter": "-+--+--+-+--+--+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Feuchteren Auges erblickst du", "tokens": ["Feuch\u00b7te\u00b7ren", "Au\u00b7ges", "er\u00b7blickst", "du"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "PPER"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Rings dann die verschleierte Welt.", "tokens": ["Rings", "dann", "die", "ver\u00b7schlei\u00b7er\u00b7te", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.2": {"line.1": {"text": "Weil s\u00fc\u00dfes Vergessen allein aufw\u00e4gt den unendlichen Schmerz,", "tokens": ["Weil", "s\u00fc\u00b7\u00dfes", "Ver\u00b7ges\u00b7sen", "al\u00b7lein", "auf\u00b7w\u00e4gt", "den", "un\u00b7end\u00b7li\u00b7chen", "Schmerz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+---+--+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Schl\u00fcrft, Freunde, das goldene Na\u00df, hier wo sich ein Zaubergefild", "tokens": ["Schl\u00fcrft", ",", "Freun\u00b7de", ",", "das", "gol\u00b7de\u00b7ne", "Na\u00df", ",", "hier", "wo", "sich", "ein", "Zau\u00b7ber\u00b7ge\u00b7fild"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "NN", "$,", "ART", "ADJA", "NN", "$,", "ADV", "PWAV", "PRF", "ART", "NN"], "meter": "-+--+--+--+-+--+", "measure": "amphibrach.tetra.plus"}, "line.3": {"text": "Breitet um uns und um Baj\u00e4s", "tokens": ["Brei\u00b7tet", "um", "uns", "und", "um", "Ba\u00b7j\u00e4s"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "PPER", "KON", "APPR", "NE"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "R\u00fcckstrahlende, wonnige Bucht!", "tokens": ["R\u00fcck\u00b7strah\u00b7len\u00b7de", ",", "won\u00b7ni\u00b7ge", "Bucht", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.3": {"line.1": {"text": "Kommt unter des Tempelgew\u00f6lbs halbdrohenden Rest! (Es vernahm", "tokens": ["Kommt", "un\u00b7ter", "des", "Tem\u00b7pel\u00b7ge\u00b7w\u00f6l\u00b7bs", "halb\u00b7dro\u00b7hen\u00b7den", "Rest", "!", "(", "Es", "ver\u00b7nahm"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "ADJA", "NN", "$.", "$(", "PPER", "VVFIN"], "meter": "-+--+--+--+--+--+", "measure": "amphibrach.penta.plus"}, "line.2": {"text": "Hier Cypria Wunsch und Gebet) Ruht hier! In den hellen Pokal", "tokens": ["Hier", "Cyp\u00b7ria", "Wunsch", "und", "Ge\u00b7bet", ")", "Ruht", "hier", "!", "In", "den", "hel\u00b7len", "Po\u00b7kal"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "NE", "NN", "KON", "NN", "$(", "VVFIN", "ADV", "$.", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+--+", "measure": "iambic.septa.chol"}, "line.3": {"text": "Tr\u00e4ufe der s\u00fc\u00dfe Falerner,", "tokens": ["Tr\u00e4u\u00b7fe", "der", "s\u00fc\u00b7\u00dfe", "Fa\u00b7ler\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Jahrtausende schon so ber\u00fchmt!", "tokens": ["Jahr\u00b7tau\u00b7sen\u00b7de", "schon", "so", "be\u00b7r\u00fchmt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADV", "ADJD", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.4": {"line.1": {"text": "Aus purpurnen Wogen empor ragt manches antike Gestein,", "tokens": ["Aus", "pur\u00b7pur\u00b7nen", "Wo\u00b7gen", "em\u00b7por", "ragt", "man\u00b7ches", "an\u00b7ti\u00b7ke", "Ge\u00b7stein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKVZ", "VVFIN", "PIAT", "ADJA", "NN", "$,"], "meter": "--+-+--+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Das R\u00f6mer voreinst in die Flut, Prachts\u00e4ulen zu tragen, gesenkt:", "tokens": ["Das", "R\u00f6\u00b7mer", "vor\u00b7e\u00b7inst", "in", "die", "Flut", ",", "Pracht\u00b7s\u00e4u\u00b7len", "zu", "tra\u00b7gen", ",", "ge\u00b7senkt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "ART", "NN", "$,", "NN", "PTKZU", "VVINF", "$,", "VVPP", "$."], "meter": "-+---+--+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "La\u00dft die Verblichenen leben,", "tokens": ["La\u00dft", "die", "Ver\u00b7bli\u00b7che\u00b7nen", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "VVINF", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Die m\u00e4chtige Taten getan!", "tokens": ["Die", "m\u00e4ch\u00b7ti\u00b7ge", "Ta\u00b7ten", "ge\u00b7tan", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.5": {"line.1": {"text": "Anspannend die Kraft des Gem\u00fcts, wirkt Gutes und Sch\u00f6nes erschafft,", "tokens": ["An\u00b7span\u00b7nend", "die", "Kraft", "des", "Ge\u00b7m\u00fcts", ",", "wirkt", "Gu\u00b7tes", "und", "Sch\u00f6\u00b7nes", "er\u00b7schafft", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "ART", "NN", "ART", "NN", "$,", "VVFIN", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+--+--+-+--+--+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Auf da\u00df in der werdenden Zeit bei K\u00fcnftigen t\u00f6ne das Wort:", "tokens": ["Auf", "da\u00df", "in", "der", "wer\u00b7den\u00b7den", "Zeit", "bei", "K\u00fcnf\u00b7ti\u00b7gen", "t\u00f6\u00b7ne", "das", "Wort", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "APPR", "ART", "ADJA", "NN", "APPR", "NN", "VVFIN", "ART", "NN", "$."], "meter": "----+--+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Selig der Tag und die R\u00e4ume,", "tokens": ["Se\u00b7lig", "der", "Tag", "und", "die", "R\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Wo solch ein Ber\u00fchmter gelebt!", "tokens": ["Wo", "solch", "ein", "Be\u00b7r\u00fchm\u00b7ter", "ge\u00b7lebt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "ART", "NN", "VVPP", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.6": {"line.1": {"text": "Wann, Freunde, wir steigen hinab, wo dort sich ein mythisches Volk", "tokens": ["Wann", ",", "Freun\u00b7de", ",", "wir", "stei\u00b7gen", "hin\u00b7ab", ",", "wo", "dort", "sich", "ein", "my\u00b7thi\u00b7sches", "Volk"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "$,", "NN", "$,", "PPER", "VVFIN", "PTKVZ", "$,", "PWAV", "ADV", "PRF", "ART", "ADJA", "NN"], "meter": "-+--+--+-+-+-+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Weissagende Grotte gebohrt, unweit der zertr\u00fcmmerten Stadt,", "tokens": ["Weis\u00b7sa\u00b7gen\u00b7de", "Grot\u00b7te", "ge\u00b7bohrt", ",", "un\u00b7weit", "der", "zer\u00b7tr\u00fcm\u00b7mer\u00b7ten", "Stadt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVPP", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+---+--+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Mag die Sibylle von Kum\u00e4", "tokens": ["Mag", "die", "Si\u00b7byl\u00b7le", "von", "Ku\u00b7m\u00e4"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "ART", "NN", "APPR", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Uns Segen und Ruhm prophezein!", "tokens": ["Uns", "Se\u00b7gen", "und", "Ruhm", "pro\u00b7phe\u00b7zein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.7": {"line.1": {"text": "Dort dr\u00fcben, die H\u00f6hlen entlang, liegt jenes elysische Feld,", "tokens": ["Dort", "dr\u00fc\u00b7ben", ",", "die", "H\u00f6h\u00b7len", "ent\u00b7lang", ",", "liegt", "je\u00b7nes", "e\u00b7ly\u00b7si\u00b7sche", "Feld", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "ART", "NN", "PTKVZ", "$,", "VVFIN", "PDAT", "ADJA", "NN", "$,"], "meter": "-+--+--+-+-+-+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Wo Geister im Felsengeb\u00fcsch hinwandeln am Ufer des Meers:", "tokens": ["Wo", "Geis\u00b7ter", "im", "Fel\u00b7sen\u00b7ge\u00b7b\u00fcsch", "hin\u00b7wan\u00b7deln", "am", "U\u00b7fer", "des", "Meers", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPRART", "NN", "VVFIN", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+--+--+-+--+--+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Gl\u00fcckliche, die mit Heroen", "tokens": ["Gl\u00fcck\u00b7li\u00b7che", ",", "die", "mit", "He\u00b7roen"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "PRELS", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Hinwandeln am Ufer des Meers!", "tokens": ["Hin\u00b7wan\u00b7deln", "am", "U\u00b7fer", "des", "Meers", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.8": {"line.1": {"text": "Wohl ziemt es dem Folgegeschlecht, wo immer ein heiteres Mahl", "tokens": ["Wohl", "ziemt", "es", "dem", "Fol\u00b7ge\u00b7ge\u00b7schlecht", ",", "wo", "im\u00b7mer", "ein", "hei\u00b7te\u00b7res", "Mahl"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,", "PWAV", "ADV", "ART", "ADJA", "NN"], "meter": "-+--+--+-+--+--+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Gastfreunde vereine, mir auch volltriefende Schale zu weihn,", "tokens": ["Gast\u00b7freun\u00b7de", "ver\u00b7ei\u00b7ne", ",", "mir", "auch", "voll\u00b7trie\u00b7fen\u00b7de", "Scha\u00b7le", "zu", "weihn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "PPER", "ADV", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+--+--+-+--+--+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Der ich erfand in der Seele", "tokens": ["Der", "ich", "er\u00b7fand", "in", "der", "See\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Manch liebebefl\u00fcgeltes Lied.", "tokens": ["Manch", "lie\u00b7be\u00b7be\u00b7fl\u00fc\u00b7gel\u00b7tes", "Lied", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.9": {"line.1": {"text": "Wohl bietet der irdische Tag qualvolle Sekunden genug.", "tokens": ["Wohl", "bie\u00b7tet", "der", "ir\u00b7di\u00b7sche", "Tag", "qual\u00b7vol\u00b7le", "Se\u00b7kun\u00b7den", "ge\u00b7nug", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "ADJA", "NN", "ADV", "$."], "meter": "-+--+--+-+-+-+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Wenn tief du gedenkend erw\u00e4gst, was je du verlorst, o Gem\u00fct!", "tokens": ["Wenn", "tief", "du", "ge\u00b7den\u00b7kend", "er\u00b7w\u00e4gst", ",", "was", "je", "du", "ver\u00b7lorst", ",", "o", "Ge\u00b7m\u00fct", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", 