{"dta.poem.8044": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "5.  Dante .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.99"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "War\u2019s ein Thor der Stadt Florenz,", "tokens": ["Wa\u00b7r's", "ein", "Thor", "der", "Stadt", "Flo\u00b7renz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ART", "NN", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Oder war\u2019s ein Thor der Himmel,", "tokens": ["O\u00b7der", "wa\u00b7r's", "ein", "Thor", "der", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Draus am klarsten Fr\u00fchlingsmorgen", "tokens": ["Draus", "am", "klars\u00b7ten", "Fr\u00fch\u00b7lings\u00b7mor\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Zog so festliches Gewimmel?", "tokens": ["Zog", "so", "fest\u00b7li\u00b7ches", "Ge\u00b7wim\u00b7mel", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Kinder, hold wie Engelschaaren,", "tokens": ["Kin\u00b7der", ",", "hold", "wie", "En\u00b7gel\u00b7schaa\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "KOKOM", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Reich geschm\u00fcckt mit Blumenkr\u00e4nzen,", "tokens": ["Reich", "ge\u00b7schm\u00fcckt", "mit", "Blu\u00b7men\u00b7kr\u00e4n\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Zogen in das Rosenthal", "tokens": ["Zo\u00b7gen", "in", "das", "Ro\u00b7sen\u00b7thal"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Zu den frohen Festest\u00e4nzen.", "tokens": ["Zu", "den", "fro\u00b7hen", "Fes\u00b7te\u00b7st\u00e4n\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Unter einem Lorbeerbaume", "tokens": ["Un\u00b7ter", "ei\u00b7nem", "Lor\u00b7beer\u00b7bau\u00b7me"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Stand, damals neunj\u00e4hrig, Dante,", "tokens": ["Stand", ",", "da\u00b7mals", "neun\u00b7j\u00e4h\u00b7rig", ",", "Dan\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "ADV", "ADJD", "$,", "NE", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.11": {"text": "Der im lieblichsten der M\u00e4dchen", "tokens": ["Der", "im", "lieb\u00b7lichs\u00b7ten", "der", "M\u00e4d\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPRART", "VVFIN", "ART", "NN"], "meter": "+-+---+-", "measure": "unknown.measure.tri"}, "line.12": {"text": "Seinen Engel gleich erkannte.", "tokens": ["Sei\u00b7nen", "En\u00b7gel", "gleich", "er\u00b7kann\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Rauschten nicht des Lorbeers Zweige,", "tokens": ["Rauschten", "nicht", "des", "Lor\u00b7beers", "Zwei\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.14": {"text": "Von der Fr\u00fchlingsluft ersch\u00fcttert?", "tokens": ["Von", "der", "Fr\u00fch\u00b7lings\u00b7luft", "er\u00b7sch\u00fct\u00b7tert", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Klang nicht Dante\u2019s junge Seele,", "tokens": ["Klang", "nicht", "Dan\u00b7te's", "jun\u00b7ge", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "NE", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Von der Liebe Hauch durchzittert?", "tokens": ["Von", "der", "Lie\u00b7be", "Hauch", "durch\u00b7zit\u00b7tert", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Ja! ihm ist in jener Stunde", "tokens": ["Ja", "!", "ihm", "ist", "in", "je\u00b7ner", "Stun\u00b7de"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$.", "PPER", "VAFIN", "APPR", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Des Gesanges Quell entsprungen;", "tokens": ["Des", "Ge\u00b7san\u00b7ges", "Quell", "ent\u00b7sprun\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "In Sonetten, in Kanzonen", "tokens": ["In", "So\u00b7net\u00b7ten", ",", "in", "Kan\u00b7zo\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "$,", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Ist die Lieb\u2019 ihm fr\u00fch erklungen.", "tokens": ["Ist", "die", "Lieb'", "ihm", "fr\u00fch", "er\u00b7klun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PPER", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "Als, zur Jungfrau hold erwachsen,", "tokens": ["Als", ",", "zur", "Jung\u00b7frau", "hold", "er\u00b7wach\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "APPRART", "NN", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "Jene wieder ihm begegnet,", "tokens": ["Je\u00b7ne", "wie\u00b7der", "ihm", "be\u00b7geg\u00b7net", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.23": {"text": "Steht auch seine Dichtung schon", "tokens": ["Steht", "auch", "sei\u00b7ne", "Dich\u00b7tung", "schon"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.24": {"text": "Wie ein Baum, der Bl\u00fcthen regnet.", "tokens": ["Wie", "ein", "Baum", ",", "der", "Bl\u00fc\u00b7then", "reg\u00b7net", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.25": {"text": "Aus dem Thore von Florenz", "tokens": ["Aus", "dem", "Tho\u00b7re", "von", "Flo\u00b7renz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "NE"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.26": {"text": "Zogen dichte Schaaren wieder,", "tokens": ["Zo\u00b7gen", "dich\u00b7te", "Schaa\u00b7ren", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.27": {"text": "Aber langsam, trauervoll,", "tokens": ["A\u00b7ber", "lang\u00b7sam", ",", "trau\u00b7er\u00b7voll", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["KON", "ADJD", "$,", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.28": {"text": "Bei dem Klange dumpfer Lieder.", "tokens": ["Bei", "dem", "Klan\u00b7ge", "dum\u00b7pfer", "Lie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.29": {"text": "Unter jenem schwarzen Tuch,", "tokens": ["Un\u00b7ter", "je\u00b7nem", "schwar\u00b7zen", "Tuch", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.30": {"text": "Mit dem weissen Kreutz geschm\u00fccket,", "tokens": ["Mit", "dem", "weis\u00b7sen", "Kreutz", "ge\u00b7schm\u00fc\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.31": {"text": "Tr\u00e4gt man Beatricen hin,", "tokens": ["Tr\u00e4gt", "man", "Be\u00b7at\u00b7ri\u00b7cen", "hin", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "NN", "PTKVZ", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.32": {"text": "Die der Tod so fr\u00fch gepfl\u00fccket.", "tokens": ["Die", "der", "Tod", "so", "fr\u00fch", "ge\u00b7pfl\u00fc\u00b7cket", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ADV", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.33": {"text": "Dante sa\u00df in seiner Kammer,", "tokens": ["Dan\u00b7te", "sa\u00df", "in", "sei\u00b7ner", "Kam\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.34": {"text": "Einsam, still, im Abendlichte,", "tokens": ["Ein\u00b7sam", ",", "still", ",", "im", "A\u00b7bend\u00b7lich\u00b7te", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "$,", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.35": {"text": "H\u00f6rte fern die Glocken t\u00f6nen", "tokens": ["H\u00f6r\u00b7te", "fern", "die", "Glo\u00b7cken", "t\u00f6\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.36": {"text": "Und verh\u00fcllte sein Gesichte.", "tokens": ["Und", "ver\u00b7h\u00fcll\u00b7te", "sein", "Ge\u00b7sich\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.37": {"text": "In der W\u00e4lder tiefste Schatten", "tokens": ["In", "der", "W\u00e4l\u00b7der", "tiefs\u00b7te", "Schat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.38": {"text": "Stieg der edle S\u00e4nger nieder,", "tokens": ["Stieg", "der", "ed\u00b7le", "S\u00e4n\u00b7ger", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.39": {"text": "Gleich den fernen Todtenglocken", "tokens": ["Gleich", "den", "fer\u00b7nen", "Tod\u00b7ten\u00b7glo\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.40": {"text": "T\u00f6nten fortan seine Lieder.", "tokens": ["T\u00f6n\u00b7ten", "for\u00b7tan", "sei\u00b7ne", "Lie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.41": {"text": "Aber in der wildsten Oede,", "tokens": ["A\u00b7ber", "in", "der", "wilds\u00b7ten", "O\u00b7e\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.42": {"text": "Wo er ging mit bangem St\u00f6hnen,", "tokens": ["Wo", "er", "ging", "mit", "ban\u00b7gem", "St\u00f6h\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.43": {"text": "Kam zu ihm ein Abgesandter", "tokens": ["Kam", "zu", "ihm", "ein", "Ab\u00b7ge\u00b7sand\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "APPR", "PPER", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.44": {"text": "Von der hingeschiednen Sch\u00f6nen;", "tokens": ["Von", "der", "hin\u00b7ge\u00b7schied\u00b7nen", "Sch\u00f6\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.45": {"text": "Der ihn f\u00fchrt\u2019 an treuer Hand", "tokens": ["Der", "ihn", "f\u00fchrt'", "an", "treu\u00b7er", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.46": {"text": "Durch der H\u00f6lle tiefste Schluchten,", "tokens": ["Durch", "der", "H\u00f6l\u00b7le", "tiefs\u00b7te", "Schluch\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.47": {"text": "Wo sein ird\u2019scher Schmerz verstummte", "tokens": ["Wo", "sein", "ird'\u00b7scher", "Schmerz", "ver\u00b7stumm\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPOSAT", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.48": {"text": "Bei dem Anblick der Verfluchten.", "tokens": ["Bei", "dem", "An\u00b7blick", "der", "Ver\u00b7fluch\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.49": {"text": "Bald zum sel\u2019gen Licht empor", "tokens": ["Bald", "zum", "sel'\u00b7gen", "Licht", "em\u00b7por"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "ADJA", "NN", "PTKVZ"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.50": {"text": "Kam er auf den dunkeln Wegen,", "tokens": ["Kam", "er", "auf", "den", "dun\u00b7keln", "We\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.51": {"text": "Aus des Paradieses Pforte", "tokens": ["Aus", "des", "Pa\u00b7ra\u00b7die\u00b7ses", "Pfor\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.52": {"text": "Trat die Freundin ihm entgegen.", "tokens": ["Trat", "die", "Freun\u00b7din", "ihm", "ent\u00b7ge\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.53": {"text": "Hoch und h\u00f6her schwebten Beide", "tokens": ["Hoch", "und", "h\u00f6\u00b7her", "schweb\u00b7ten", "Bei\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "KON", "ADJD", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.54": {"text": "Durch des Himmels Glanz und Wonnen,", "tokens": ["Durch", "des", "Him\u00b7mels", "Glanz", "und", "Won\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.55": {"text": "Sie, aufblickend, ungeblendet,", "tokens": ["Sie", ",", "auf\u00b7bli\u00b7ckend", ",", "un\u00b7ge\u00b7blen\u00b7det", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "$,", "VVPP", "$,", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.56": {"text": "Zu der Sonne aller Sonnen;", "tokens": ["Zu", "der", "Son\u00b7ne", "al\u00b7ler", "Son\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PIAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.57": {"text": "Er, die Augen hingewendet", "tokens": ["Er", ",", "die", "Au\u00b7gen", "hin\u00b7ge\u00b7wen\u00b7det"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["PPER", "$,", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.58": {"text": "Nach der Freundin Angesichte,", "tokens": ["Nach", "der", "Freun\u00b7din", "An\u00b7ge\u00b7sich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.59": {"text": "Das, verkl\u00e4rt, ihn schauen lie\u00df", "tokens": ["Das", ",", "ver\u00b7kl\u00e4rt", ",", "ihn", "schau\u00b7en", "lie\u00df"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PDS", "$,", "VVPP", "$,", "PPER", "VVINF", 