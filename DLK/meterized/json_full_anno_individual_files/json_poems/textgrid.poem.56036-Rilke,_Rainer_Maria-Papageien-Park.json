{"textgrid.poem.56036": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "Papageien-Park", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Unter t\u00fcrkischen Linden, die bl\u00fchen, an Rasenr\u00e4ndern,", "tokens": ["Un\u00b7ter", "t\u00fcr\u00b7ki\u00b7schen", "Lin\u00b7den", ",", "die", "bl\u00fc\u00b7hen", ",", "an", "Ra\u00b7sen\u00b7r\u00e4n\u00b7dern", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NE", "$,", "PRELS", "VVFIN", "$,", "APPR", "NN", "$,"], "meter": "+-+--+--+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "in leise von ihrem Heimweh geschaukelten St\u00e4ndern", "tokens": ["in", "lei\u00b7se", "von", "ih\u00b7rem", "Heim\u00b7weh", "ge\u00b7schau\u00b7kel\u00b7ten", "St\u00e4n\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJD", "APPR", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+--+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "atmen die Ara und wissen von ihren L\u00e4ndern,", "tokens": ["at\u00b7men", "die", "A\u00b7ra", "und", "wis\u00b7sen", "von", "ih\u00b7ren", "L\u00e4n\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "KON", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+--+--+--+-+-", "measure": "dactylic.tri.plus"}, "line.4": {"text": "die sich, auch wenn sie nicht hinsehn, nicht ver\u00e4ndern.", "tokens": ["die", "sich", ",", "auch", "wenn", "sie", "nicht", "hin\u00b7sehn", ",", "nicht", "ver\u00b7\u00e4n\u00b7dern", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PRELS", "PRF", "$,", "ADV", "KOUS", "PPER", "PTKNEG", "VVINF", "$,", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}}, "stanza.2": {"line.1": {"text": "Fremd im besch\u00e4ftigten Gr\u00fcnen wie eine Parade,", "tokens": ["Fremd", "im", "be\u00b7sch\u00e4f\u00b7tig\u00b7ten", "Gr\u00fc\u00b7nen", "wie", "ei\u00b7ne", "Pa\u00b7ra\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "ADJA", "NN", "KOKOM", "ART", "NN", "$,"], "meter": "+--+--+--+--+-", "measure": "dactylic.penta"}, "line.2": {"text": "zieren sie sich und f\u00fchlen sich selber zu schade,", "tokens": ["zie\u00b7ren", "sie", "sich", "und", "f\u00fch\u00b7len", "sich", "sel\u00b7ber", "zu", "scha\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "KON", "VVFIN", "PRF", "ADV", "PTKA", "ADJD", "$,"], "meter": "+--+-+--+--+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "und mit den kostbaren Schn\u00e4beln aus Jaspis und Jade", "tokens": ["und", "mit", "den", "kost\u00b7ba\u00b7ren", "Schn\u00e4\u00b7beln", "aus", "Jas\u00b7pis", "und", "Ja\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "APPR", "NE", "KON", "NN"], "meter": "+--+--+--+--+-", "measure": "dactylic.penta"}, "line.4": {"text": "kauen sie Graues, verschleudern es, finden es fade.", "tokens": ["kau\u00b7en", "sie", "Grau\u00b7es", ",", "ver\u00b7schleu\u00b7dern", "es", ",", "fin\u00b7den", "es", "fa\u00b7de", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "$,", "VVFIN", "PPER", "$,", "VVFIN", "PPER", "VVFIN", "$."], "meter": "+--+--+--+--+-", "measure": "dactylic.penta"}}, "stanza.3": {"line.1": {"text": "Unten klauben die duffen Tauben, was sie nicht m\u00f6gen,", "tokens": ["Un\u00b7ten", "klau\u00b7ben", "die", "duf\u00b7fen", "Tau\u00b7ben", ",", "was", "sie", "nicht", "m\u00f6\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,", "PRELS", "PPER", "PTKNEG", "VMFIN", "$,"], "meter": "+-+--+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "w\u00e4hrend sich oben die h\u00f6hnischen V\u00f6gel verbeugen", "tokens": ["w\u00e4h\u00b7rend", "sich", "o\u00b7ben", "die", "h\u00f6h\u00b7ni\u00b7schen", "V\u00f6\u00b7gel", "ver\u00b7beu\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "ADV", "ART", "ADJA", "NN", "VVINF"], "meter": "+--+--+--+--+-", "measure": "dactylic.penta"}, "line.3": {"text": "zwischen den beiden fast leeren vergeudeten Tr\u00f6gen.", "tokens": ["zwi\u00b7schen", "den", "bei\u00b7den", "fast", "lee\u00b7ren", "ver\u00b7geu\u00b7de\u00b7ten", "Tr\u00f6\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PIAT", "ADV", "ADJA", "ADJA", "NN", "$."], "meter": "+--+--+--+--+-", "measure": "dactylic.penta"}}, "stanza.4": {"line.1": {"text": "Aber dann wiegen sie wieder und schl\u00e4fern und \u00e4ugen,", "tokens": ["A\u00b7ber", "dann", "wie\u00b7gen", "sie", "wie\u00b7der", "und", "schl\u00e4\u00b7fern", "und", "\u00e4u\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADV", "KON", "VVINF", "KON", "VVFIN", "$,"], "meter": "+--+--+--+--+-", "measure": "dactylic.penta"}, "line.2": {"text": "spielen mit dunkelen Zungen, die gerne l\u00f6gen,", "tokens": ["spie\u00b7len", "mit", "dun\u00b7ke\u00b7len", "Zun\u00b7gen", ",", "die", "ger\u00b7ne", "l\u00f6\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$