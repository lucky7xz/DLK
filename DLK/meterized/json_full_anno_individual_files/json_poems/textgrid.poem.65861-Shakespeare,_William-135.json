{"textgrid.poem.65861": {"metadata": {"author": {"name": "Shakespeare, William", "birth": "N.A.", "death": "N.A."}, "title": "135", "genre": "verse", "period": "N.A.", "pub_year": 1590, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wird andrer Wunsch erf\u00fcllt, so hast du deinen Willen,", "tokens": ["Wird", "an\u00b7drer", "Wunsch", "er\u00b7f\u00fcllt", ",", "so", "hast", "du", "dei\u00b7nen", "Wil\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "NN", "VVPP", "$,", "ADV", "VAFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und Willen obenein, und Willen \u00fcberviel,", "tokens": ["Und", "Wil\u00b7len", "o\u00b7be\u00b7nein", ",", "und", "Wil\u00b7len", "\u00fc\u00b7ber\u00b7viel", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADV", "$,", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und ich, dein Qu\u00e4lgeist, kann ihn mehr als gr\u00fcndlich stillen.", "tokens": ["Und", "ich", ",", "dein", "Qu\u00e4l\u00b7geist", ",", "kann", "ihn", "mehr", "als", "gr\u00fcnd\u00b7lich", "stil\u00b7len", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "PPOSAT", "NN", "$,", "VMFIN", "PPER", "PIAT", "KOKOM", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Zu deinem holden Willen mach' ich dies Kodizill:", "tokens": ["Zu", "dei\u00b7nem", "hol\u00b7den", "Wil\u00b7len", "mach'", "ich", "dies", "Ko\u00b7di\u00b7zill", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "PPER", "PDS", "VMFIN", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Willst du, die weit und r\u00e4umig ist gewillt,", "tokens": ["Willst", "du", ",", "die", "weit", "und", "r\u00e4u\u00b7mig", "ist", "ge\u00b7willt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "$,", "PRELS", "ADJD", "KON", "ADJD", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Nicht endlich meinen Willen aufnehmen in den deinen?", "tokens": ["Nicht", "end\u00b7lich", "mei\u00b7nen", "Wil\u00b7len", "auf\u00b7neh\u00b7men", "in", "den", "dei\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "PPOSAT", "NN", "VVINF", "APPR", "ART", "PPOSAT", "$."], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "Erh\u00f6rt man andrer Willen gern und mild,", "tokens": ["Er\u00b7h\u00f6rt", "man", "an\u00b7drer", "Wil\u00b7len", "gern", "und", "mild", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADJA", "NN", "ADV", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Und meinem soll kein Stern der Gnade scheinen?", "tokens": ["Und", "mei\u00b7nem", "soll", "kein", "Stern", "der", "Gna\u00b7de", "schei\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "VMFIN", "PIAT", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Den Regen nehmen Seen, Fl\u00fcsse, Meere", "tokens": ["Den", "Re\u00b7gen", "neh\u00b7men", "Seen", ",", "Fl\u00fcs\u00b7se", ",", "Mee\u00b7re"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["ART", "NN", "VVINF", "NN", "$,", "NN", "$,", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Zu ihrem Wasserreichtum dennoch ein:", "tokens": ["Zu", "ih\u00b7rem", "Was\u00b7ser\u00b7reich\u00b7tum", "den\u00b7noch", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "PTKVZ", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "So la\u00df auch du, da\u00df sich dein weiter Wille mehre,", "tokens": ["So", "la\u00df", "auch", "du", ",", "da\u00df", "sich", "dein", "wei\u00b7ter", "Wil\u00b7le", "meh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "ADV", "PPER", "$,", "KOUS", "PRF", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "O Willenreiche! noch dir meinen Willen weihn.", "tokens": ["O", "Wil\u00b7len\u00b7rei\u00b7che", "!", "noch", "dir", "mei\u00b7nen", "Wil\u00b7len", "weihn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "ADV", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "La\u00df keinen sterben! St\u00fcrmisch oder still,", "tokens": ["La\u00df", "kei\u00b7nen", "ster\u00b7ben", "!", "St\u00fcr\u00b7misch", "o\u00b7der", "still", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PIAT", "VVINF", "$.", "NN", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Sie bitten nur was ich, der eine ", "tokens": ["Sie", "bit\u00b7ten", "nur", "was", "ich", ",", "der", "ei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "PWS", "PPER", "$,", "PRELS", "