{"textgrid.poem.36974": {"metadata": {"author": {"name": "Gla\u00dfbrenner, Adolf", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es gl\u00e4nzen golden die Welten, so viele Millionen!", "genre": "verse", "period": "N.A.", "pub_year": 1843, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es gl\u00e4nzen golden die Welten, so viele Millionen!", "tokens": ["Es", "gl\u00e4n\u00b7zen", "gol\u00b7den", "die", "Wel\u00b7ten", ",", "so", "vie\u00b7le", "Mil\u00b7lion\u00b7en", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ART", "NN", "$,", "ADV", "PIAT", "NN", "$."], "meter": "-+-+--+--+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Unnennbar die Zahl der Wesen, die auf den Sternen wohnen!", "tokens": ["Un\u00b7nenn\u00b7bar", "die", "Zahl", "der", "We\u00b7sen", ",", "die", "auf", "den", "Ster\u00b7nen", "woh\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "ART", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Kein menschlich Sinnen wagt es, das All zu \u00fcberdenken,", "tokens": ["Kein", "menschlich", "Sin\u00b7nen", "wagt", "es", ",", "das", "All", "zu", "\u00fc\u00b7ber\u00b7den\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJD", "NN", "VVFIN", "PPER", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Noch des Allm\u00e4cht'gen Gr\u00f6\u00dfe, der's schaffen konnt und lenken!", "tokens": ["Noch", "des", "All\u00b7m\u00e4cht'\u00b7gen", "Gr\u00f6\u00b7\u00dfe", ",", "der's", "schaf\u00b7fen", "konnt", "und", "len\u00b7ken", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$,", "PIS", "VVINF", "VMFIN", "KON", "VVINF", "$."], "meter": "+-+--+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.5": {"text": "Und hier auf diesem Staubkorn, so j\u00e4mmerlich, so klein!", "tokens": ["Und", "hier", "auf", "die\u00b7sem", "Staub\u00b7korn", ",", "so", "j\u00e4m\u00b7mer\u00b7lich", ",", "so", "klein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PDAT", "NN", "$,", "ADV", "ADJD", "$,", "ADV", "ADJD", "$."], "meter": "-+-+-++-+-+-+", "measure": "unknown.measure.septa"}, "line.6": {"text": "Auf diesem K\u00f6nigsneste so viele Noth und Pein!!", "tokens": ["Auf", "die\u00b7sem", "K\u00f6\u00b7nigs\u00b7nes\u00b7te", "so", "vie\u00b7le", "Noth", "und", "Pein", "!!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "ADV", "PIAT", "NN", "KON", "NN", "$."], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "Zertr\u00fcmm're, Herr des Himmels, den einen kleinen Stern!", "tokens": ["Zer\u00b7tr\u00fcm\u00b7m'\u00b7re", ",", "Herr", "des", "Him\u00b7mels", ",", "den", "ei\u00b7nen", "klei\u00b7nen", "Stern", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "ART", "NN", "$,", "PRELS", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Dein Sohn konnt' ihn nicht retten! verdorben ist sein Kern.", "tokens": ["Dein", "Sohn", "konnt'", "ihn", "nicht", "ret\u00b7ten", "!", "ver\u00b7dor\u00b7ben", "ist", "sein", "Kern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PPER", "PTKNEG", "VVINF", "$.", "VVPP", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "Gib Deinen guten Engeln, Allm\u00e4chtiger, ein Fest,", "tokens": ["Gib", "Dei\u00b7nen", "gu\u00b7ten", "En\u00b7geln", ",", "All\u00b7m\u00e4ch\u00b7ti\u00b7ger", ",", "ein", "Fest", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "ADJA", "NN", "$,", "ADJA", "$,", "ART", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "Und st\u00fcrz' vom Bau der Sch\u00f6pfung dies alte K\u00f6nigsnest!", "tokens": ["Und", "st\u00fcrz'", "vom", "Bau", "der", "Sch\u00f6p\u00b7fung", "dies", "al\u00b7te", "K\u00f6\u00b7nigs\u00b7nest", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "ART", "NN", "PDS", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Es gl\u00e4nzen golden die Welten, so viele Millionen!", "tokens": ["Es", "gl\u00e4n\u00b7zen", "gol\u00b7den", "die", "Wel\u00b7ten", ",", "so", "vie\u00b7le", "Mil\u00b7lion\u00b7en", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ART", "NN", "$,", "ADV", "PIAT", "NN", "$."], "meter": "-+-+--+--+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Unnennbar die Zahl der Wesen, die auf den Sternen wohnen!", "tokens": ["Un\u00b7nenn\u00b7bar", "die", "Zahl", "der", "We\u00b7sen", ",", "die", "auf", "den", "Ster\u00b7nen", "woh\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "ART", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+--+-+-+-", "m