{"dta.poem.10486": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "XlIII.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Werdet nur nicht ungeduldig,", "tokens": ["Wer\u00b7det", "nur", "nicht", "un\u00b7ge\u00b7dul\u00b7dig", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PTKNEG", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn von alten Schmerzenskl\u00e4ngen", "tokens": ["Wenn", "von", "al\u00b7ten", "Schmer\u00b7zens\u00b7kl\u00e4n\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Manche noch vernehmlich klingen", "tokens": ["Man\u00b7che", "noch", "ver\u00b7nehm\u00b7lich", "klin\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PIS", "ADV", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In den neuesten Ges\u00e4ngen.", "tokens": ["In", "den", "neu\u00b7es\u00b7ten", "Ge\u00b7s\u00e4n\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wartet nur, es wird verhallen", "tokens": ["War\u00b7tet", "nur", ",", "es", "wird", "ver\u00b7hal\u00b7len"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "ADV", "$,", "PPER", "VAFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dieses Echo meiner Schmerzen,", "tokens": ["Die\u00b7ses", "E\u00b7cho", "mei\u00b7ner", "Schmer\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ein neuer Liederfr\u00fchling", "tokens": ["Und", "ein", "neu\u00b7er", "Lie\u00b7der\u00b7fr\u00fch\u00b7ling"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Sprie\u00dft aus dem geheilten Herzen.", "tokens": ["Sprie\u00dft", "aus", "dem", "ge\u00b7heil\u00b7ten", "Her\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}