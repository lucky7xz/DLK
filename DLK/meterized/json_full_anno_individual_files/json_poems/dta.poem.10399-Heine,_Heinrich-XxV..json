{"dta.poem.10399": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "XxV.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Die Linde bl\u00fchte, die Nachtigall sang,", "tokens": ["Die", "Lin\u00b7de", "bl\u00fch\u00b7te", ",", "die", "Nach\u00b7ti\u00b7gall", "sang", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die Sonne lachte mit freundlicher Lust;", "tokens": ["Die", "Son\u00b7ne", "lach\u00b7te", "mit", "freund\u00b7li\u00b7cher", "Lust", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Da k\u00fc\u00dftest du mich, und dein Arm mich umschlang,", "tokens": ["Da", "k\u00fc\u00df\u00b7test", "du", "mich", ",", "und", "dein", "Arm", "mich", "um\u00b7schlang", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "$,", "KON", "PPOSAT", "NN", "PPER", "VVFIN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Da pre\u00dftest du mich an die schwellende Brust.", "tokens": ["Da", "pre\u00df\u00b7test", "du", "mich", "an", "die", "schwel\u00b7len\u00b7de", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.2": {"line.1": {"text": "Die Bl\u00e4tter fielen, der Rabe schrie hohl,", "tokens": ["Die", "Bl\u00e4t\u00b7ter", "fie\u00b7len", ",", "der", "Ra\u00b7be", "schrie", "hohl", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "ADJD", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die Sonne gr\u00fc\u00dfte verdrie\u00dflichen Blicks;", "tokens": ["Die", "Son\u00b7ne", "gr\u00fc\u00df\u00b7te", "ver\u00b7drie\u00df\u00b7li\u00b7chen", "Blicks", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Da sagten wir frostig einander: \u201eLebwohl!\u201c", "tokens": ["Da", "sag\u00b7ten", "wir", "fros\u00b7tig", "ein\u00b7an\u00b7der", ":", "\u201e", "Leb\u00b7wohl", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "ADV", "$.", "$(", "NN", "$.", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Da knixtest du h\u00f6flichst den h\u00f6flichsten Knix.", "tokens": ["Da", "knix\u00b7test", "du", "h\u00f6f\u00b7lichst", "den", "h\u00f6f\u00b7lichs\u00b7ten", "Knix", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}}}}