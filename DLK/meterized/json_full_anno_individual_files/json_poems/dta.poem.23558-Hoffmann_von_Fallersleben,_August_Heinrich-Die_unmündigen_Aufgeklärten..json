{"dta.poem.23558": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die unm\u00fcndigen Aufgekl\u00e4rten.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1840", "urn": "urn:nbn:de:kobv:b4-200905192626", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201eunm\u00fcndig seid ihr allesamt,", "tokens": ["\u201e", "un\u00b7m\u00fcn\u00b7dig", "seid", "ihr", "al\u00b7le\u00b7samt", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "VAFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dazu hat euch der Staat verdammt,", "tokens": ["Da\u00b7zu", "hat", "euch", "der", "Staat", "ver\u00b7dammt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Und wer einmal unm\u00fcndig ist,", "tokens": ["Und", "wer", "ein\u00b7mal", "un\u00b7m\u00fcn\u00b7dig", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADV", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wird aufgekl\u00e4rt zu keiner Frist.\u201c", "tokens": ["Wird", "auf\u00b7ge\u00b7kl\u00e4rt", "zu", "kei\u00b7ner", "Frist", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "VVPP", "APPR", "PIAT", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wahr mag nun wohl das eine sein,", "tokens": ["Wahr", "mag", "nun", "wohl", "das", "ei\u00b7ne", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "ADV", "ADV", "ART", "ART", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das andre leuchtet uns nicht ein:", "tokens": ["Das", "and\u00b7re", "leuch\u00b7tet", "uns", "nicht", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "VVFIN", "PPER", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sagt an, wo's uns an Licht gebricht?", "tokens": ["Sagt", "an", ",", "wo's", "uns", "an", "Licht", "ge\u00b7bricht", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "PWAV", "PPER", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wir sehn oft nicht vor lauter Licht.", "tokens": ["Wir", "sehn", "oft", "nicht", "vor", "lau\u00b7ter", "Licht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PTKNEG", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}