{"dta.poem.7858": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Harfnerlied am Hochzeitmahle .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.99"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "Festlich ist der Freude Schall", "tokens": ["Fest\u00b7lich", "ist", "der", "Freu\u00b7de", "Schall"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Durch dies hohe Haus geschwebet,", "tokens": ["Durch", "dies", "ho\u00b7he", "Haus", "ge\u00b7schwe\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDS", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ein dumpfer Widerhall", "tokens": ["Und", "ein", "dum\u00b7pfer", "Wi\u00b7der\u00b7hall"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Aus der Gruft emporgebebet.", "tokens": ["Aus", "der", "Gruft", "em\u00b7por\u00b7ge\u00b7be\u00b7bet", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "In der sch\u00f6nen Jubelnacht", "tokens": ["In", "der", "sch\u00f6\u00b7nen", "Ju\u00b7bel\u00b7nacht"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Habt der V\u00e4ter ihr gedacht,", "tokens": ["Habt", "der", "V\u00e4\u00b7ter", "ihr", "ge\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Manche hohe That besungen", "tokens": ["Man\u00b7che", "ho\u00b7he", "That", "be\u00b7sun\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Aus der Vorzeit D\u00e4mmerungen.", "tokens": ["Aus", "der", "Vor\u00b7zeit", "D\u00e4m\u00b7me\u00b7run\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Oft war dieses Saales Raum", "tokens": ["Oft", "war", "die\u00b7ses", "Saa\u00b7les", "Raum"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PDAT", "NN", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Schimmervoll bei frohen Festen,", "tokens": ["Schim\u00b7mer\u00b7voll", "bei", "fro\u00b7hen", "Fes\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie mit jedem Lenz der Baum", "tokens": ["Wie", "mit", "je\u00b7dem", "Lenz", "der", "Baum"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "PIAT", "NN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Prangt in frischen Bl\u00fcthen\u00e4sten.", "tokens": ["Prangt", "in", "fri\u00b7schen", "Bl\u00fc\u00b7the\u00b7n\u00e4s\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ach! die hier in Fr\u00f6hlichkeit", "tokens": ["Ach", "!", "die", "hier", "in", "Fr\u00f6h\u00b7lich\u00b7keit"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "ART", "ADV", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Treuer Liebe Bund geweiht,", "tokens": ["Treu\u00b7er", "Lie\u00b7be", "Bund", "ge\u00b7weiht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Drunten in der Schlummerhalle", "tokens": ["Drun\u00b7ten", "in", "der", "Schlum\u00b7mer\u00b7hal\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Ruhen sie beisammen alle.", "tokens": ["Ru\u00b7hen", "sie", "bei\u00b7sam\u00b7men", "al\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVFIN", "PIS", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Auf des Lebens Bahn dahin", "tokens": ["Auf", "des", "Le\u00b7bens", "Bahn", "da\u00b7hin"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN", "PAV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Fleugt der Mensch mit Sturmeseile,", "tokens": ["Fleugt", "der", "Mensch", "mit", "Stur\u00b7me\u00b7sei\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Dann in treuer Freunde Sinn", "tokens": ["Dann", "in", "treu\u00b7er", "Freun\u00b7de", "Sinn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Dauert er noch kurze Weile.", "tokens": ["Dau\u00b7ert", "er", "noch", "kur\u00b7ze", "Wei\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Durch den Saal, in Erz und Stein,", "tokens": ["Durch", "den", "Saal", ",", "in", "Erz", "und", "Stein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Stehn der Vorwelt lange Reihu,", "tokens": ["Stehn", "der", "Vor\u00b7welt", "lan\u00b7ge", "Rei\u00b7hu", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "K\u00f6nnen nicht das Auge heben,", "tokens": ["K\u00f6n\u00b7nen", "nicht", "das", "Au\u00b7ge", "he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PTKNEG", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Nicht das Wort der Liebe geben.", "tokens": ["Nicht", "das", "Wort", "der", "Lie\u00b7be", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Keine ewig helle That", "tokens": ["Kei\u00b7ne", "e\u00b7wig", "hel\u00b7le", "That"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "ADJD", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Hebt dich aus der Nacht der Gr\u00fcfte;", "tokens": ["Hebt", "dich", "aus", "der", "Nacht", "der", "Gr\u00fcf\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Niemand sah des Donners Pfad,", "tokens": ["Nie\u00b7mand", "sah", "des", "Don\u00b7ners", "Pfad", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Noch den Fittig sanfter L\u00fcfte.", "tokens": ["Noch", "den", "Fit\u00b7tig", "sanf\u00b7ter", "L\u00fcf\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wie du auf zu Gott geblickt,", "tokens": ["Wie", "du", "auf", "zu", "Gott", "ge\u00b7blickt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "APPR", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Wie des Freundes Hand gedr\u00fcckt,", "tokens": ["Wie", "des", "Freun\u00b7des", "Hand", "ge\u00b7dr\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wie der Liebe Ku\u00df gegeben,", "tokens": ["Wie", "der", "Lie\u00b7be", "Ku\u00df", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Das entschwindet mit dem Leben.", "tokens": ["Das", "ent\u00b7schwin\u00b7det", "mit", "dem", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Auch das Kind, das l\u00e4chelnd sich", "tokens": ["Auch", "das", "Kind", ",", "das", "l\u00e4\u00b7chelnd", "sich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "$,", "PRELS", "ADJD", "PRF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "In der Mutter Arm geschmieget;", "tokens": ["In", "der", "Mut\u00b7ter", "Arm", "ge\u00b7schmie\u00b7get", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und der Greis, der wonniglich", "tokens": ["Und", "der", "Greis", ",", "der", "won\u00b7nig\u00b7lich"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Enkel auf dem Schoo\u00df gewieget;", "tokens": ["En\u00b7kel", "auf", "dem", "Schoo\u00df", "ge\u00b7wie\u00b7get", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und die Braut, mit Jugendlust", "tokens": ["Und", "die", "Braut", ",", "mit", "Ju\u00b7gend\u00b7lust"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ART", "NN", "$,", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "H\u00e4ngend an des Treuen Brust:", "tokens": ["H\u00e4n\u00b7gend", "an", "des", "Treu\u00b7en", "Brust", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Alle lebten sch\u00f6nes Leben,", "tokens": ["Al\u00b7le", "leb\u00b7ten", "sch\u00f6\u00b7nes", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Alle soll das Lied erheben!", "tokens": ["Al\u00b7le", "soll", "das", "Lied", "er\u00b7he\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}