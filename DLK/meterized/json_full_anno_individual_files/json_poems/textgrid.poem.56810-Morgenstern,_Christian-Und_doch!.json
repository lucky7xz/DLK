{"textgrid.poem.56810": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Und doch!", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und doch, ich sag es frank und rund,", "tokens": ["Und", "doch", ",", "ich", "sag", "es", "frank", "und", "rund", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "PPER", "VVFIN", "PPER", "VVFIN", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "mir fehlt noch was zum Gl\u00fccke \u2013:", "tokens": ["mir", "fehlt", "noch", "was", "zum", "Gl\u00fc\u00b7cke", "\u2013", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PRELS", "APPRART", "NN", "$(", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ein lieber, s\u00fc\u00dfer M\u00e4dchenmund,", "tokens": ["Ein", "lie\u00b7ber", ",", "s\u00fc\u00b7\u00dfer", "M\u00e4d\u00b7chen\u00b7mund", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADV", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "ein Arm, der meinen dr\u00fccke,", "tokens": ["ein", "Arm", ",", "der", "mei\u00b7nen", "dr\u00fc\u00b7cke", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPOSAT", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "ein Aug, darein ich gl\u00e4nzen k\u00f6nnt'", "tokens": ["ein", "Aug", ",", "da\u00b7rein", "ich", "gl\u00e4n\u00b7zen", "k\u00f6nnt'"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PAV", "PPER", "VVINF", "VMFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "mein jubelndes Empfinden,", "tokens": ["mein", "ju\u00b7beln\u00b7des", "Emp\u00b7fin\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "ein Blondhaar, das ich Keinem g\u00f6nnt'", "tokens": ["ein", "Blond\u00b7haar", ",", "das", "ich", "Kei\u00b7nem", "g\u00f6nnt'"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "PIS", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "sich um die Hand zu winden.", "tokens": ["sich", "um", "die", "Hand", "zu", "win\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Und doch, ich sag es frank und rund,", "tokens": ["Und", "doch", ",", "ich", "sag", "es", "frank", "und", "rund", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "PPER", "VVFIN", "PPER", "VVFIN", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "mir fehlt noch was zum Gl\u00fccke \u2013:", "tokens": ["mir", "fehlt", "noch", "was", "zum", "Gl\u00fc\u00b7cke", "\u2013", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PRELS", "APPRART", "NN", "$(", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ein lieber, s\u00fc\u00dfer M\u00e4dchenmund,", "tokens": ["Ein", "lie\u00b7ber", ",", "s\u00fc\u00b7\u00dfer", "M\u00e4d\u00b7chen\u00b7mund", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADV", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "ein Arm, der mei