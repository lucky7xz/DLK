{"textgrid.poem.58043": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "81. Von der Einsamkeit", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wo du nach deinem Sinn, wie gut er scheint, willst leben,", "tokens": ["Wo", "du", "nach", "dei\u00b7nem", "Sinn", ",", "wie", "gut", "er", "scheint", ",", "willst", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "PPOSAT", "NN", "$,", "PWAV", "ADJD", "PPER", "VVFIN", "$,", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So kann die Einsamkeit dir keine Stille geben;", "tokens": ["So", "kann", "die", "Ein\u00b7sam\u00b7keit", "dir", "kei\u00b7ne", "Stil\u00b7le", "ge\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "PPER", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wer sich von sich entfernt, kann allezeit allein,", "tokens": ["Wer", "sich", "von", "sich", "ent\u00b7fernt", ",", "kann", "al\u00b7le\u00b7zeit", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "APPR", "PRF", "ADJD", "$,", "VMFIN", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Vergn\u00fcgt und ungest\u00f6rt auch selbst bei Menschen sein.", "tokens": ["Ver\u00b7gn\u00fcgt", "und", "un\u00b7ge\u00b7st\u00f6rt", "auch", "selbst", "bei", "Men\u00b7schen", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "AD