{"dta.poem.18186": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "Eine Uhr im Todtenkopfe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198805", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein Mensch der pl\u00f6zlich starb, von j\u00e4hen", "tokens": ["Ein", "Mensch", "der", "pl\u00f6z\u00b7lich", "starb", ",", "von", "j\u00e4\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "ART", "ADJD", "VVFIN", "$,", "APPR", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Schlag ger\u00fchrt,", "tokens": ["Schlag", "ge\u00b7r\u00fchrt", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Ward neulich unvermerkt, aus dieser", "tokens": ["Ward", "neu\u00b7lich", "un\u00b7ver\u00b7merkt", ",", "aus", "die\u00b7ser"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VAFIN", "ADV", "ADJD", "$,", "APPR", "PDAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zeit entf\u00fchrt;", "tokens": ["Zeit", "ent\u00b7f\u00fchrt", ";"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Indem er eine Uhr ins Nachbars Haus getragen", "tokens": ["In\u00b7dem", "er", "ei\u00b7ne", "Uhr", "ins", "Nach\u00b7bars", "Haus", "ge\u00b7tra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "APPRART", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Must seine Lebens-Uhr die lezte Stunde schlagen.", "tokens": ["Must", "sei\u00b7ne", "Le\u00b7bens\u00b7Uhr", "die", "lez\u00b7te", "Stun\u00b7de", "schla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Indem er jenes Werk, hatt in den Gang gebracht,", "tokens": ["In\u00b7dem", "er", "je\u00b7nes", "Werk", ",", "hatt", "in", "den", "Gang", "ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PDAT", "NN", "$,", "VAFIN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ward seine Lebens-Uhr ihm selbst zu nicht gemacht:", "tokens": ["Ward", "sei\u00b7ne", "Le\u00b7bens\u00b7Uhr", "ihm", "selbst", "zu", "nicht", "ge\u00b7macht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "PPER", "ADV", "APPR", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ein \u00e4ngstlich banges Weh durchkroch die schlaffen", "tokens": ["Ein", "\u00e4ngst\u00b7lich", "ban\u00b7ges", "Weh", "durch\u00b7kroch", "die", "schlaf\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "ADJA", "NN", "ADV", "ART", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Glieder,", "tokens": ["Glie\u00b7der", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.11": {"text": "Er sang, als wie entseelt, auf einen Sessel nieder.", "tokens": ["Er", "sang", ",", "als", "wie", "ent\u00b7seelt", ",", "auf", "ei\u00b7nen", "Ses\u00b7sel", "nie\u00b7der", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "KOKOM", "VVPP", "$,", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Man rief mich zu ihn hin, als Lehrer beizustehn,", "tokens": ["Man", "rief", "mich", "zu", "ihn", "hin", ",", "als", "Leh\u00b7rer", "bei\u00b7zu\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "APPR", "PPER", "PTKVZ", "$,", "KOUS", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Die Augen sahen starr; doch konnt er nicht mehr", "tokens": ["Die", "Au\u00b7gen", "sa\u00b7hen", "starr", ";", "doch", "konnt", "er", "nicht", "mehr"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$.", "ADV", "VMFIN", "PPER", "PTKNEG", "ADV"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.14": {"text": "sehn;", "tokens": ["sehn", ";"], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "+", "measure": "single.up"}, "line.15": {"text": "Sein Geist erhohlte sich, doch in sehr wenig Stun-", "tokens": ["Sein", "Geist", "er\u00b7hohl\u00b7te", "sich", ",", "doch", "in", "sehr", "we\u00b7nig", "Stun"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "PRF", "$,", "ADV", "APPR", "ADV", "ADV", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "den,", "tokens": ["den", ","], "token_info": ["word", "punct"], "pos": ["ART", "$,"], "meter": "-", "measure": "single.down"}, "line.17": {"text": "War Sprache, Othem, Geist, gleich wiederum", "tokens": ["War", "Spra\u00b7che", ",", "O\u00b7them", ",", "Geist", ",", "gleich", "wie\u00b7de\u00b7rum"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word"], "pos": ["VAFIN", "NN", "$,", "NN", "$,", "NN", "$,", "ADV", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "verschwunden.", "tokens": ["ver\u00b7schwun\u00b7den", "."], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.19": {"text": "O! pl\u00f6zlich schneller Todt! dacht ich in meinen Sin,", "tokens": ["O", "!", "pl\u00f6z\u00b7lich", "schnel\u00b7ler", "Todt", "!", "dacht", "ich", "in", "mei\u00b7nen", "Sin", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "ADJD", "ADJA", "NN", "$.", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Wer also selig stirbt, dem ist er ein Gewin:", "tokens": ["Wer", "al\u00b7so", "se\u00b7lig", "stirbt", ",", "dem", "ist", "er", "ein", "Ge\u00b7win", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADJD", "VVFIN", "$,", "PDS", "VAFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Wer aber an das End, nie eh es k\u00f6mmt, gedenket,", "tokens": ["Wer", "a\u00b7ber", "an", "das", "End", ",", "nie", "eh", "es", "k\u00f6mmt", ",", "ge\u00b7den\u00b7ket", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "ADV", "APPR", "ART", "NN", "$,", "ADV", "KOUS", "PPER", "VVFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Dem ist er solch ein Weh, das ihm auf ewig kr\u00e4n-", "tokens": ["Dem", "ist", "er", "solch", "ein", "Weh", ",", "das", "ihm", "auf", "e\u00b7wig", "kr\u00e4n"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "PPER", "PIAT", "ART", "NN", "$,", "PRELS", "PPER", "APPR", "ADJD", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "ket.", "tokens": ["ket", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-", "measure": "single.down"}, "line.24": {"text": "Man sagte mir hernach, ein sch\u00f6nes Sinnenbild,", "tokens": ["Man", "sag\u00b7te", "mir", "her\u00b7nach", ",", "ein", "sch\u00f6\u00b7nes", "Sin\u00b7nen\u00b7bild", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "ADV", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Da\u00df dieser Selige, der seine Tag erf\u00fcllt,", "tokens": ["Da\u00df", "die\u00b7ser", "Se\u00b7li\u00b7ge", ",", "der", "sei\u00b7ne", "Tag", "er\u00b7f\u00fcllt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Zu Todts-Erinnerung, in seines Lebens Tagen,", "tokens": ["Zu", "Todt\u00b7sEr\u00b7in\u00b7ne\u00b7rung", ",", "in", "sei\u00b7nes", "Le\u00b7bens", "Ta\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Als eine Taschen-Uhr, bei sich umher getragen.", "tokens": ["Als", "ei\u00b7ne", "Ta\u00b7schen\u00b7Uhr", ",", "bei", "sich", "um\u00b7her", "ge\u00b7tra\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,", "APPR", "PRF", "PTKVZ", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Sie war also gemacht, da\u00df dieses R\u00e4derwerk,", "tokens": ["Sie", "war", "al\u00b7so", "ge\u00b7macht", ",", "da\u00df", "die\u00b7ses", "R\u00e4\u00b7der\u00b7werk", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "VVPP", "$,", "KOUS", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Jm Todtenkopf verstekt. O! sch\u00f6nes Augenmerk", "tokens": ["Jm", "Tod\u00b7ten\u00b7kopf", "vers\u00b7tekt", ".", "O", "!", "sch\u00f6\u00b7nes", "Au\u00b7gen\u00b7merk"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["APPRART", "NN", "VVPP", "$.", "NE", "$.", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Wolt er den Stundenschlag, aus seinen Uhrwerk", "tokens": ["Wolt", "er", "den", "Stun\u00b7den\u00b7schlag", ",", "aus", "sei\u00b7nen", "Uhr\u00b7werk"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ART", "NN", "$,", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.31": {"text": "wissen:", "tokens": ["wis\u00b7sen", ":"], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.32": {"text": "So hat er das Geh\u00e4us, den Todskopf \u00f6fnen m\u00fcssen.", "tokens": ["So", "hat", "er", "das", "Ge\u00b7h\u00e4us", ",", "den", "Tods\u00b7kopf", "\u00f6f\u00b7nen", "m\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "$,", "ART", "NN", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Ich dachte bei mir selbst: Ein lehrreich Sinnenspiel!", "tokens": ["Ich", "dach\u00b7te", "bei", "mir", "selbst", ":", "Ein", "lehr\u00b7reich", "Sin\u00b7nen\u00b7spiel", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "ADV", "$.", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Das Leben gleicht der Uhr, wer es berech-", "tokens": ["Das", "Le\u00b7ben", "gleicht", "der", "Uhr", ",", "wer", "es", "be\u00b7rech"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,", "PWS", "PPER", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.35": {"text": "nen will,", "tokens": ["nen", "will", ","], "token_info": ["word", "word", "punct"], "pos": ["VVINF", "VMFIN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.36": {"text": "Der mu\u00df auch allemahl, an dessen Ende\ndenken,", "tokens": ["Der", "mu\u00df", "auch", "al\u00b7le\u00b7mahl", ",", "an", "des\u00b7sen", "En\u00b7de", "den\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ADV", "ADV", "$,", "APPR", "PRELAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Und dabei auf dem Todt, des Geistes Au-\ngen lenken.", "tokens": ["Und", "da\u00b7bei", "auf", "dem", "Todt", ",", "des", "Geis\u00b7tes", "Au", "gen", "len\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "APPR", "ART", "NN", "$,", "ART", "NN", "TRUNC", "APPR", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Des Leibes Gliederbau, der aneinander h\u00e4ngt,", "tokens": ["Des", "Lei\u00b7bes", "Glie\u00b7der\u00b7bau", ",", "der", "an\u00b7ein\u00b7an\u00b7der", "h\u00e4ngt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "PRELS", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Gleicht einen Uhrwerk auch, das sich bewegend", "tokens": ["Gleicht", "ei\u00b7nen", "Uhr\u00b7werk", "auch", ",", "das", "sich", "be\u00b7we\u00b7gend"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ART", "NN", "ADV", "$,", "PRELS", "PRF", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.40": {"text": "schwenkt;", "tokens": ["schwenkt", ";"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+", "measure": "single.up"}, "line.41": {"text": "So lang die Uhr sich regt, nach ihrer Ordnung ge-", "tokens": ["So", "lang", "die", "Uhr", "sich", "regt", ",", "nach", "ih\u00b7rer", "Ord\u00b7nung", "ge"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "ART", "NN", "PRF", "VVFIN", "$,", "APPR", "PPOSAT", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "het,", "tokens": ["het", ","], "token_info": ["word", "punct"], "pos": ["VAFIN", "$,"], "meter": "-", "measure": "single.down"}, "line.43": {"text": "Wird auch der Zeiger fort in seinem Lauf, gedrehet.", "tokens": ["Wird", "auch", "der", "Zei\u00b7ger", "fort", "in", "sei\u00b7nem", "Lauf", ",", "ge\u00b7dre\u00b7het", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "PTKVZ", "APPR", "PPOSAT", "NN", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Steht aber solche still im abgeme\u00dfnen Lauf;", "tokens": ["Steht", "a\u00b7ber", "sol\u00b7che", "still", "im", "ab\u00b7ge\u00b7me\u00df\u00b7nen", "Lauf", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PIAT", "ADJD", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "So h\u00e4lt sich auch zugleich der Stundenzeiger auf:", "tokens": ["So", "h\u00e4lt", "sich", "auch", "zu\u00b7gleich", "der", "Stun\u00b7den\u00b7zei\u00b7ger", "auf", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ADV", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "So ist es mit uns auch; so lang der Leib sich reget,", "tokens": ["So", "ist", "es", "mit", "uns", "auch", ";", "so", "lang", "der", "Leib", "sich", "re\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "PPER", "ADV", "$.", "ADV", "ADJD", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "Das Blut im Adern wallt, so lang die Puls noch", "tokens": ["Das", "Blut", "im", "A\u00b7dern", "wallt", ",", "so", "lang", "die", "Puls", "noch"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "NN", "VVFIN", "$,", "ADV", "ADJD", "ART", "NN", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.48": {"text": "schl\u00e4get:", "tokens": ["schl\u00e4\u00b7get", ":"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.49": {"text": "So lang ist Leben da; wo dieses stokt, die still,", "tokens": ["So", "lang", "ist", "Le\u00b7ben", "da", ";", "wo", "die\u00b7ses", "stokt", ",", "die", "still", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "NN", "ADV", "$.", "PWAV", "PDS", "VVFIN", "$,", "PRELS", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.50": {"text": "Da ist des Lebens-Laufs bestimmtes End und Ziel.", "tokens": ["Da", "ist", "des", "Le\u00b7bens\u00b7Laufs", "be\u00b7stimm\u00b7tes", "End", "und", "Ziel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.51": {"text": "Wir gehen immer fort, und mit uns auch die Zei-", "tokens": ["Wir", "ge\u00b7hen", "im\u00b7mer", "fort", ",", "und", "mit", "uns", "auch", "die", "Zei"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "PTKVZ", "$,", "KON", "APPR", "PPER", "ADV", "ART", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.52": {"text": "ten,", "tokens": ["ten", ","], "token_info": ["word", "punct"], "pos": ["FM", "$,"], "meter": "-", "measure": "single.down"}, "line.53": {"text": "Die uns ganz unvermerkt, zu unsern Ende leiten.", "tokens": ["Die", "uns", "ganz", "un\u00b7ver\u00b7merkt", ",", "zu", "un\u00b7sern", "En\u00b7de", "lei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADJD", "$,", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.54": {"text": "Wir gleichen einer Uhr, doch unsrer Seelen-Haus,", "tokens": ["Wir", "glei\u00b7chen", "ei\u00b7ner", "Uhr", ",", "doch", "uns\u00b7rer", "See\u00b7len\u00b7Haus", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "li