{"textgrid.poem.46723": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[was blickest du beklommen]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was blickest du beklommen,", "tokens": ["Was", "bli\u00b7ckest", "du", "be\u00b7klom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Von Thr\u00e4nen tr\u00fcb' umschwommen,", "tokens": ["Von", "Thr\u00e4\u00b7nen", "tr\u00fcb'", "um\u00b7schwom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Zum Himmel, wo in Strahlen", "tokens": ["Zum", "Him\u00b7mel", ",", "wo", "in", "Strah\u00b7len"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "PWAV", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Fr\u00fchling angeglommen!", "tokens": ["Der", "Fr\u00fch\u00b7ling", "an\u00b7ge\u00b7glom\u00b7men", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "\u00bbes kann mit seinen Strahlen", "tokens": ["\u00bb", "es", "kann", "mit", "sei\u00b7nen", "Strah\u00b7len"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "PPER", "VMFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Fr\u00fchling mir nicht frommen;", "tokens": ["Der", "Fr\u00fch\u00b7ling", "mir", "nicht", "from\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "PTKNEG", "VVINF", "$