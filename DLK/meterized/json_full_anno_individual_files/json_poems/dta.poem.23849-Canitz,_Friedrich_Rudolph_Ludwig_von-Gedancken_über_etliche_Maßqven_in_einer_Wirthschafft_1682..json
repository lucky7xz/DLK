{"dta.poem.23849": {"metadata": {"author": {"name": "Canitz, Friedrich Rudolph Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "Gedancken \u00fcber etliche Ma\u00dfqven in einer  \n Wirthschafft 1682.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1700", "urn": "urn:nbn:de:kobv:b4-200905197532", "language": ["de:0.99"], "booktitle": "[Canitz, Friedrich Rudolph Ludwig von]: Neben-Stunden Unterschiedener Gedichte. [Hrsg. v. Joachim Lange]. Berlin, 1700."}, "poem": {"stanza.1": {"line.1": {"text": "Dein Diener hatte dir/ geschickte R\u00f6merin/", "tokens": ["Dein", "Die\u00b7ner", "hat\u00b7te", "dir", "/", "ge\u00b7schick\u00b7te", "R\u00f6\u00b7me\u00b7rin", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "$(", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den besten Br\u00e4utigam des R\u00f6mschen Reichs ver-", "tokens": ["Den", "bes\u00b7ten", "Br\u00e4u\u00b7ti\u00b7gam", "des", "R\u00f6m\u00b7schen", "Reichs", "ver"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Es ist vom Neuen Jahr/ da\u00df ich ihn schuldig bin/", "tokens": ["Es", "ist", "vom", "Neu\u00b7en", "Jahr", "/", "da\u00df", "ich", "ihn", "schul\u00b7dig", "bin", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPRART", "ADJA", "NN", "$(", "KOUS", "PPER", "PPER", "ADJD", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Doch der Erf\u00fcllungs-Tag war noch nicht angebroche\u0303;", "tokens": ["Doch", "der", "Er\u00b7f\u00fcl\u00b7lungs\u00b7Tag", "war", "noch", "nicht", "an\u00b7ge\u00b7bro\u00b7ch\u1ebd", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "ADV", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Heut aber stellt er sich mit seiner Cronen ein/", "tokens": ["Heut", "a\u00b7ber", "stellt", "er", "sich", "mit", "sei\u00b7ner", "Cro\u00b7nen", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "PRF", "APPR", "PPOSAT", "NN", "ART", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die er vorgesteren/ als R\u00f6mer ", "tokens": ["Die", "er", "vor\u00b7ges\u00b7te\u00b7ren", "/", "als", "R\u00f6\u00b7mer"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "PPER", "VVINF", "$(", "KOUS", "NN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.7": {"text": "Und w\u00fcnscht an dessen statt dir angenehm zu seyn/", "tokens": ["Und", "w\u00fcnscht", "an", "des\u00b7sen", "statt", "dir", "an\u00b7ge\u00b7nehm", "zu", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PDS", "APPR", "PPER", "ADJD", "PTKZU", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Der bey der Withschafft dich zur R\u00f6merin genom\u0303en.", "tokens": ["Der", "bey", "der", "With\u00b7schafft", "dich", "zur", "R\u00f6\u00b7me\u00b7rin", "ge\u00b7nom\u0303en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "PRF", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Der R\u00f6mer bey dem Spiel/ ist wie du weist verm\u00e4hlt/", "tokens": ["Der", "R\u00f6\u00b7mer", "bey", "dem", "Spiel", "/", "ist", "wie", "du", "weist", "ver\u00b7m\u00e4hlt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$(", "VAFIN", "KOKOM", "PPER", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Der aber bleibet dein/ der itzund nach dir freyet/", "tokens": ["Der", "a\u00b7ber", "blei\u00b7bet", "dein", "/", "der", "it\u00b7zund", "nach", "dir", "fre\u00b7yet", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "PPOSAT", "$(", "ART", "ADV", "APPR", "PPER", "VVFIN", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.11": {"text": "St\u00fcnd er dir auch nicht an/ scheint doch dis ungefehlt/", "tokens": ["St\u00fcnd", "er", "dir", "auch", "nicht", "an", "/", "scheint", "doch", "dis", "un\u00b7ge\u00b7fehlt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPER", "ADV", "PTKNEG", "PTKVZ", "$(", "VVFIN", "ADV", "PDS", "ADJD", "$("], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.12": {"text": "Da\u00df er etwas aus Rom dir k\u00fcnfftig propheceyet.", "tokens": ["Da\u00df", "er", "et\u00b7was", "aus", "Rom", "dir", "k\u00fcnff\u00b7tig", "pro\u00b7phe\u00b7ce\u00b7yet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "NE", "PPER", "ADJD", "VVFIN", "$."], "meter": "--+--+-+--+-+", "measure": "anapaest.di.plus"}}}}}