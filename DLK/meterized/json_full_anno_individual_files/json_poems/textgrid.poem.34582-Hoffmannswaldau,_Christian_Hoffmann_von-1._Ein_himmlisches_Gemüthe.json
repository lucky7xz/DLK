{"textgrid.poem.34582": {"metadata": {"author": {"name": "Hoffmannswaldau, Christian Hoffmann von", "birth": "N.A.", "death": "N.A."}, "title": "1. Ein himmlisches Gem\u00fcthe", "genre": "verse", "period": "N.A.", "pub_year": 1647, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein himmlisches Gem\u00fcthe, so Geist und Feuer h\u00e4lt,", "tokens": ["Ein", "himm\u00b7li\u00b7sches", "Ge\u00b7m\u00fc\u00b7the", ",", "so", "Geist", "und", "Feu\u00b7er", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADV", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und nicht nur dem Gebl\u00fcte zum Diener ist bestellt,", "tokens": ["Und", "nicht", "nur", "dem", "Ge\u00b7bl\u00fc\u00b7te", "zum", "Die\u00b7ner", "ist", "be\u00b7stellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ADV", "ART", "NN", "APPRART", "NN", "VAFIN", "VVPP", "$,"], "meter": "--+--+--+-+-+", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Schaut des Gel\u00fcckes Gaben,", "tokens": ["Schaut", "des", "Ge\u00b7l\u00fc\u00b7ckes", "Ga\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Und alles, was wir haben,", "tokens": ["Und", "al\u00b7les", ",", "was", "wir", "ha\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "$,", "PRELS", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Mit halben Augen an.", "tokens": ["Mit", "hal\u00b7ben", "Au\u00b7gen", "an", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Es rei\u00dft der Sch\u00f6nheit Gl\u00e4ntzen nicht seinen F\u00fcrsatz ein,", "tokens": ["Es", "rei\u00dft", "der", "Sch\u00f6n\u00b7heit", "Gl\u00e4nt\u00b7zen", "nicht", "sei\u00b7nen", "F\u00fcr\u00b7satz", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "PTKNEG", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Er kennt der Liebe Gr\u00e4ntzen, und wei\u00df den falschen Schein", "tokens": ["Er", "kennt", "der", "Lie\u00b7be", "Gr\u00e4nt\u00b7zen", ",", "und", "wei\u00df", "den", "fal\u00b7schen", "Schein"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "$,", "KON", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Dem Wesen abzuziehen,", "tokens": ["Dem", "We\u00b7sen", "ab\u00b7zu\u00b7zie\u00b7hen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVIZU", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und alles di\u00df zu fliehen,", "tokens": ["Und", "al\u00b7les", "di\u00df", "zu", "flie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "PDS", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "So uns verleiten kan.", "tokens": ["So", "uns", "ver\u00b7lei\u00b7ten", "kan", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Er lacht mit leeren H\u00e4nden, die Unschuld ist sein Gold;", "tokens": ["Er", "lacht", "mit", "lee\u00b7ren", "H\u00e4n\u00b7den", ",", "die", "Un\u00b7schuld", "ist", "sein", "Gold", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$,", "ART", "NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Gebrauch kan ihn nicht blenden, die Tugend ist sein Sold.", "tokens": ["Ge\u00b7brauch", "kan", "ihn", "nicht", "blen\u00b7den", ",", "die", "Tu\u00b7gend", "ist", "sein", "Sold", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PTKNEG", "VVFIN", "$,", "ART", "NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Immittelst Roth und Schmertzen", "tokens": ["Im\u00b7mit\u00b7telst", "Roth", "und", "Schmert\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Bleibt in dem reinen Hertzen,", "tokens": ["Bleibt", "in", "dem", "rei\u00b7nen", "Hert\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die Hoffnung unverwand.", "tokens": ["Die", "Hoff\u00b7nung", "un\u00b7ver\u00b7wand", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Er liebt auff dieser Erden nichts mehr, als Geist und Gott,", "tokens": ["Er", "liebt", "auff", "die\u00b7ser", "Er\u00b7den", "nichts", "mehr", ",", "als", "Geist", "und", "Gott", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PDAT", "NN", "PIS", "ADV", "$,", "KOUS", "NN", "KON", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Er mag kein Ketzer werden, verehren Staub und Koth,", "tokens": ["Er", "mag", "kein", "Ket\u00b7zer", "wer\u00b7den", ",", "ver\u00b7eh\u00b7ren", "Staub", "und", "Koth", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIAT", "NN", "VAINF", "$,", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Es trennen seine Sinnen", "tokens": ["Es", "tren\u00b7nen", "sei\u00b7ne", "Sin\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mit eisernem Beginnen", "tokens": ["Mit", "ei\u00b7ser\u00b7nem", "Be\u00b7gin\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.5": {"text": "Der Erden faules Band.", "tokens": ["Der", "Er\u00b7den", "fau\u00b7les", "Band", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Wann alle Felsen wancken, so bleibt er unber\u00fchrt,", "tokens": ["Wann", "al\u00b7le", "Fel\u00b7sen", "wan\u00b7cken", ",", "so", "bleibt", "er", "un\u00b7be\u00b7r\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "VVINF", "$,", "ADV", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Beschlossen in dem Schrancken, der uns zum Himmel f\u00fchrt:", "tokens": ["Be\u00b7schlos\u00b7sen", "in", "dem", "Schran\u00b7cken", ",", "der", "uns", "zum", "Him\u00b7mel", "f\u00fchrt", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,", "PRELS", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Und denckt, wie diese Sachen,", "tokens": ["Und", "denckt", ",", "wie", "die\u00b7se", "Sa\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PWAV", "PDAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So uns offt knechtisch machen,", "tokens": ["So", "uns", "offt", "knech\u00b7tisch", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Verrauschen mit der Zeit.", "tokens": ["Ver\u00b7rau\u00b7schen", "mit", "der", "Zeit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Er traut ihm nicht zu bauen auff Wasser und auff Sand,", "tokens": ["Er", "traut", "ihm", "nicht", "zu", "bau\u00b7en", "auff", "Was\u00b7ser", "und", "auff", "Sand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "PTKZU", "VVINF", "APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Will nicht der Erden trauen, und sucht ein festes Land.", "tokens": ["Will", "nicht", "der", "Er\u00b7den", "trau\u00b7en", ",", "und", "sucht", "ein", "fes\u00b7tes", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PTKNEG", "ART", "NN", "VVINF", "$,", "KON", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Worauff er Hoffnung setzet;", "tokens": ["Wo\u00b7rauff", "er", "Hoff\u00b7nung", "set\u00b7zet", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und was ihn recht ergetzet,", "tokens": ["Und", "was", "ihn", "recht", "er\u00b7get\u00b7zet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ist Gott in Ewigkeit.", "tokens": ["Ist", "Gott", "in", "E\u00b7wig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Ein himmlisches Gem\u00fcthe, so Geist und Feuer h\u00e4lt,", "tokens": ["Ein", "himm\u00b7li\u00b7sches", "Ge\u00b7m\u00fc\u00b7the", ",", "so", "Geist", "und", "Feu\u00b7er", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADV", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und nicht nur dem Gebl\u00fcte zum Diener ist bestellt,", "tokens": ["Und", "nicht", "nur", "dem", "Ge\u00b7bl\u00fc\u00b7te", "zum", "Die\u00b7ner", "ist", "be\u00b7stellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ADV", "ART", "NN", "APPRART", "NN", "VAFIN", "VVPP", "$,"], "meter": "--+--+--+-+-+", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Schaut des Gel\u00fcckes Gaben,", "tokens": ["Schaut", "des", "Ge\u00b7l\u00fc\u00b7ckes", "Ga\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "$,