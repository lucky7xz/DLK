, ",", "so", "bleibt", "er", "un\u00b7be\u00b7r\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "VVINF", "$,", "ADV", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Beschlossen in dem Schrancken, der uns zum Himmel f\u00fchrt:", "tokens": ["Be\u00b7schlos\u00b7sen", "in", "dem", "Schran\u00b7cken", ",", "der", "uns", "zum", "Him\u00b7mel", "f\u00fchrt", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,", "PRELS", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Und denckt, wie diese Sachen,", "tokens": ["Und", "denckt", ",", "wie", "die\u00b7se", "Sa\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PWAV", "PDAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So uns offt knechtisch machen,", "tokens": ["So", "uns", "offt", "knech\u00b7tisch", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Verrauschen mit der Zeit.", "tokens": ["Ver\u00b7rau\u00b7schen", "mit", "der", "Zeit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Er traut ihm nicht zu bauen auff Wasser und auff Sand,", "tokens": ["Er", "traut", "ihm", "nicht", "zu", "bau\u00b7en", "auff", "Was\u00b7ser", "und", "auff", "Sand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "PTKZU", "VVINF", "APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Will nicht der Erden trauen, und sucht ein festes Land.", "tokens": ["Will", "nicht", "der", "Er\u00b7den", "trau\u00b7en", ",", "und", "sucht", "ein", "fes\u00b7tes", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PTKNEG", "ART", "NN", "VVINF", "$,", "KON", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Worauff er Hoffnung setzet;", "tokens": ["Wo\u00b7rauff", "er", "Hoff\u00b7nung", "set\u00b7zet", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und was ihn recht ergetzet,", "tokens": ["Und", "was", "ihn", "recht", "er\u00b7get\u00b7zet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ist Gott in Ewigkeit.", "tokens": ["Ist", "Gott", "in", "E\u00b7wig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}