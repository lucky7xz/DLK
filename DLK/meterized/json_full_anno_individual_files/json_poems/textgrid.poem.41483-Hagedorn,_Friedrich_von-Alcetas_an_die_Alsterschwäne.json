{"textgrid.poem.41483": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Alcetas an die Alsterschw\u00e4ne", "genre": "verse", "period": "N.A.", "pub_year": 1731, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie sehr ist euch das Schicksal hold,", "tokens": ["Wie", "sehr", "ist", "euch", "das", "Schick\u00b7sal", "hold", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VAFIN", "PPER", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr Schw\u00e4ne, die ich fast beneide!", "tokens": ["Ihr", "Schw\u00e4\u00b7ne", ",", "die", "ich", "fast", "be\u00b7nei\u00b7de", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ihr S\u00e4ufer trinkt so viel ihr wollt,", "tokens": ["Ihr", "S\u00e4u\u00b7fer", "trinkt", "so", "viel", "ihr", "wollt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "ADV", "PPER", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und bleibt auch dann der Sch\u00f6nen Freude.", "tokens": ["Und", "bleibt", "auch", "dann", "der", "Sch\u00f6\u00b7nen", "Freu\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ich wei\u00df es, Bacchus schenkte mir", "tokens": ["Ich", "wei\u00df", "es", ",", "Bac\u00b7chus", "schenk\u00b7te", "mir"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$,", "NE", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Den Epheu, welcher ihm geh\u00f6ret,", "tokens": ["Den", "E\u00b7pheu", ",", "wel\u00b7cher", "ihm", "ge\u00b7h\u00f6\u00b7ret", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "H\u00e4tt' ich so einen Hals, wie ihr,", "tokens": ["H\u00e4tt'", "ich", "so", "ei\u00b7nen", "Hals", ",", "wie", "ihr", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "$,", "PWAV", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Den ihr durch Wasser doch entehret.", "tokens": ["Den", "ihr", "durch", "Was\u00b7ser", "doch", "en\u00b7teh\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wie sehr ist euch das Schicksal hold,", "tokens": ["Wie", "sehr", "ist", "euch", "das", "Schick\u00b7sal", "hold", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VAFIN", "PPER", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr Schw\u00e4ne, die ich fast beneide!", "tokens": ["Ihr", "Schw\u00e4\u00b7ne", ",", "die", "ich", "fast", "be\u00b7nei\u00b7de", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ihr S\u00e4ufer trinkt so viel ihr wollt,", "tokens": ["Ihr", "S\u00e4u\u00b7fer", "trinkt", "so", "viel", "ihr", "wollt", ","], "token_info": ["word", "word", "word", "word", "word"