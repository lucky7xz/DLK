{"dta.poem.23396": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Das neue Jerusalem.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192634", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Welch ein kindlich frommes Streben!", "tokens": ["Welch", "ein", "kind\u00b7lich", "from\u00b7mes", "Stre\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ART", "ADJD", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Welch ein inniger Verein!", "tokens": ["Welch", "ein", "in\u00b7ni\u00b7ger", "Ver\u00b7ein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "An dem Theetisch \u2014", "tokens": ["An", "dem", "Thee\u00b7tisch"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$("], "meter": "--+-", "measure": "anapaest.init"}, "line.4": {"text": "Welch ein heilig reines Leben!", "tokens": ["Welch", "ein", "hei\u00b7lig", "rei\u00b7nes", "Le\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ART", "ADJD", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Welch ein Gottversunkensein!", "tokens": ["Welch", "ein", "Gott\u00b7ver\u00b7sun\u00b7ken\u00b7sein", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "An dem Theetisch.", "tokens": ["An", "dem", "Thee\u00b7tisch", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "--+-", "measure": "anapaest.init"}}, "stanza.2": {"line.1": {"text": "Wenn sie ein Tract\u00e4tchen lesen,", "tokens": ["Wenn", "sie", "ein", "Tract\u00b7\u00e4t\u00b7chen", "le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nimmt die Seele h\u00f6hern Schwung,", "tokens": ["Nimmt", "die", "See\u00b7le", "h\u00f6\u00b7hern", "Schwung", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "An dem Theetisch \u2014", "tokens": ["An", "dem", "Thee\u00b7tisch"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$("], "meter": "--+-", "measure": "anapaest.init"}, "line.4": {"text": "Und es schwelgt ihr ganzes Wesen", "tokens": ["Und", "es", "schwelgt", "ihr", "gan\u00b7zes", "We\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "In der Gottvereinigung", "tokens": ["In", "der", "Gott\u00b7ver\u00b7ei\u00b7ni\u00b7gung"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "An dem Theetisch.", "tokens": ["An", "dem", "Thee\u00b7tisch", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "--+-", "measure": "anapaest.init"}}, "stanza.3": {"line.1": {"text": "Ihres Glaubens s\u00fc\u00dfe Bl\u00fcthe", "tokens": ["Ih\u00b7res", "Glau\u00b7bens", "s\u00fc\u00b7\u00dfe", "Bl\u00fc\u00b7the"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Duftet wie die Rosenflur", "tokens": ["Duf\u00b7tet", "wie", "die", "Ro\u00b7sen\u00b7flur"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "KOKOM", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "An dem Theetisch \u2014", "tokens": ["An", "dem", "Thee\u00b7tisch"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$("], "meter": "--+-", "measure": "anapaest.init"}, "line.4": {"text": "Lauter Milde, Lieb' und G\u00fcte", "tokens": ["Lau\u00b7ter", "Mil\u00b7de", ",", "Lieb'", "und", "G\u00fc\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Tr\u00e4uft von ihren Lippen nur", "tokens": ["Tr\u00e4uft", "von", "ih\u00b7ren", "Lip\u00b7pen", "nur"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "PPOSAT", "NN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "An dem Theetisch.", "tokens": ["An", "dem", "Thee\u00b7tisch", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "--+-", "measure": "anapaest.init"}}, "stanza.4": {"line.1": {"text": "Wie sie ihren Br\u00e4ut'gam preisen,", "tokens": ["Wie", "sie", "ih\u00b7ren", "Br\u00e4ut'\u00b7gam", "prei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "O die Gottesbr\u00e4utlein fein!", "tokens": ["O", "die", "Got\u00b7tes\u00b7br\u00e4ut\u00b7lein", "fein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "An dem Theetisch \u2014", "tokens": ["An", "dem", "Thee\u00b7tisch"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$("], "meter": "--+-", "measure": "anapaest.init"}, "line.4": {"text": "Ihn und sich mit Andacht speisen", "tokens": ["Ihn", "und", "sich", "mit", "An\u00b7dacht", "spei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "KON", "PRF", "APPR", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und mit heil'gen Melodei'n!", "tokens": ["Und", "mit", "heil'\u00b7gen", "Me\u00b7lo\u00b7de\u00b7i'n", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.6": {"text": "An dem Theetisch.", "tokens": ["An", "dem", "Thee\u00b7tisch", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "--+-", "measure": "anapaest.init"}}, "stanza.5": {"line.1": {"text": "Alles was den K\u00f6rper n\u00e4hret", "tokens": ["Al\u00b7les", "was", "den", "K\u00f6r\u00b7per", "n\u00e4h\u00b7ret"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "PWS", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und erquicket, wird verschm\u00e4ht", "tokens": ["Und", "er\u00b7quic\u00b7ket", ",", "wird", "ver\u00b7schm\u00e4ht"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "$,", "VAFIN", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "An dem Theetisch \u2014", "tokens": ["An", "dem", "Thee\u00b7tisch"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$("], "meter": "--+-", "measure": "anapaest.init"}, "line.4": {"text": "Ihre Augen sind verkl\u00e4ret,", "tokens": ["Ih\u00b7re", "Au\u00b7gen", "sind", "ver\u00b7kl\u00e4\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Jeder Blick ist ein Gebet", "tokens": ["Je\u00b7der", "Blick", "ist", "ein", "Ge\u00b7bet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VAFIN", "ART", "NN"], "meter": "+-+-+--", "measure": "unknown.measure.tri"}, "line.6": {"text": "An dem Theetisch.", "tokens": ["An", "dem", "Thee\u00b7tisch", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "--+-", "measure": "anapaest.init"}}}}}