{"dta.poem.14811": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Die verlorene Braut.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.71", "af:0.28"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Es klang, als wollt' sie's rufen", "tokens": ["Es", "klang", ",", "als", "wollt'", "sie's", "ru\u00b7fen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "VMFIN", "PIS", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und gr\u00fc\u00dfen tausendmal \u2014", "tokens": ["Und", "gr\u00fc\u00b7\u00dfen", "tau\u00b7send\u00b7mal"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So stieg sie von den Stufen,", "tokens": ["So", "stieg", "sie", "von", "den", "Stu\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So k\u00fchle rauscht' das Thal.", "tokens": ["So", "k\u00fch\u00b7le", "rauscht'", "das", "Thal", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}