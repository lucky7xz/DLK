{"dta.poem.854": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "XlI.  Auff den Sontag de\u00df Barmhertzigen  \n Vatters/ oder 4. Sontag nach der H. Drey-  \n Einigkeit. Luc. 6.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Sol dich der h\u00f6chste GOTT mit Vatter trew anblicken ", "tokens": ["Sol", "dich", "der", "h\u00f6chs\u00b7te", "GoTT", "mit", "Vat\u00b7ter", "trew", "an\u00b7bli\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PRF", "ART", "ADJA", "NN", "APPR", "NN", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So must du jede zeit voll sanffter Geister sein", "tokens": ["So", "must", "du", "je\u00b7de", "zeit", "voll", "sanff\u00b7ter", "Geis\u00b7ter", "sein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "PIAT", "NN", "ADJD", "ADJA", "NN", "PPOSAT"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wer nichts als richten kan/ wer Rach\u2019 vnd grimme pein", "tokens": ["Wer", "nichts", "als", "rich\u00b7ten", "kan", "/", "wer", "Rach'", "vnd", "grim\u00b7me", "pein"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "PIS", "KOKOM", "VVINF", "VMFIN", "$(", "PWS", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Stets auf den R\u00e4chsten rnfft/ wird endlich in den stricken", "tokens": ["Stets", "auf", "den", "R\u00e4chs\u00b7ten", "rnfft", "/", "wird", "end\u00b7lich", "in", "den", "stri\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "$(", "VAFIN", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "De\u00df Sathans/ in dem Strom der Schwefelbach/ ersticken.", "tokens": ["De\u00df", "Sa\u00b7thans", "/", "in", "dem", "Strom", "der", "Schwe\u00b7fel\u00b7bach", "/", "er\u00b7sti\u00b7cken", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$(", "APPR", "ART", "NN", "ART", "NN", "$(", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gnad ist vmb gnade feyl/ wer gibt nimm\u2019t h\u00e4uffig eyn.", "tokens": ["Gnad", "ist", "vmb", "gna\u00b7de", "feyl", "/", "wer", "gibt", "nim\u00b7m't", "h\u00e4uf\u00b7fig", "eyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "APPR", "ADJA", "NN", "$(", "PWS", "VVFIN", "VVFIN", "ADJD", "VAINF", "$."], "meter": "+--+-+-+--+-+", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Vnd wie dein Ma\u00df; so voll so richtig fest vnd rein", "tokens": ["Vnd", "wie", "dein", "Ma\u00df", ";", "so", "voll", "so", "rich\u00b7tig", "fest", "vnd", "rein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PPOSAT", "NN", "$.", "ADV", "ADJD", "ADV", "ADJD", "PTKVZ", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wird man auff deine Schos die gegenwehre schicken.", "tokens": ["Wird", "man", "auff", "dei\u00b7ne", "Schos", "die", "ge\u00b7gen\u00b7weh\u00b7re", "schi\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "APPR", "PPOSAT", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wer Laster straffen wil vnd selbst verbrechen libt", "tokens": ["Wer", "Las\u00b7ter", "straf\u00b7fen", "wil", "vnd", "selbst", "ver\u00b7bre\u00b7chen", "libt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "NN", "VVINF", "VMFIN", "KON", "ADV", "VVINF", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gleicht dem/ der blind die Faust/ als Leiter/ blinden gibt", "tokens": ["Gleicht", "dem", "/", "der", "blind", "die", "Faust", "/", "als", "Lei\u00b7ter", "/", "blin\u00b7den", "gibt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NN", "ART", "$(", "ART", "ADJD", "ART", "NN", "$(", "KOUS", "NN", "$(", "ADJA", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wilst du ins Brudern Aug auch keinen Splitter leiden/", "tokens": ["Wilst", "du", "ins", "Bru\u00b7dern", "Aug", "auch", "kei\u00b7nen", "Split\u00b7ter", "lei\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPRART", "NN", "NN", "ADV", "PIAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "So fange bey dir an/ vnd nimb die Balcken hin/", "tokens": ["So", "fan\u00b7ge", "bey", "dir", "an", "/", "vnd", "nimb", "die", "Bal\u00b7cken", "hin", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPER", "PTKVZ", "$(", "KON", "VVIMP", "ART", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Balcken die dir selbst verdunckeln Aug vnd Sin/", "tokens": ["Die", "Bal\u00b7cken", "die", "dir", "selbst", "ver\u00b7dun\u00b7ckeln", "Aug", "vnd", "Sin", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "PPER", "ADV", "VVINF", "NN", "KON", "NE", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Vnd fleuch di\u00df selbst was du lehrst deinen N\u00e4chsten meiden.", "tokens": ["Vnd", "fleuch", "di\u00df", "selbst", "was", "du", "lehrst", "dei\u00b7nen", "N\u00e4chs\u00b7ten", "mei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PDS", "ADV", "PWS", "PPER", "VVFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}