{"textgrid.poem.47024": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "21.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Amara, bittre, was du thust, ist bitter,", "tokens": ["A\u00b7ma\u00b7ra", ",", "bitt\u00b7re", ",", "was", "du", "thust", ",", "ist", "bit\u00b7ter", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "$,", "PWS", "PPER", "VVFIN", "$,", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie du die F\u00fc\u00dfe r\u00fchrst, die Arme lenkest,", "tokens": ["Wie", "du", "die", "F\u00fc\u00b7\u00dfe", "r\u00fchrst", ",", "die", "Ar\u00b7me", "len\u00b7kest", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+----+", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Wie du die Augen hebst, wie du sie senkest,", "tokens": ["Wie", "du", "die", "Au\u00b7gen", "hebst", ",", "wie", "du", "sie", "sen\u00b7kest", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "VVFIN", "$,", "PWAV", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Die Lippen aufthust oder zu, ist's bitter.", "tokens": ["Die", "Lip\u00b7pen", "auf\u00b7thust", "o\u00b7der", "zu", ",", "ist's", "bit\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "PTKVZ", "$,", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ein jeder Gru\u00df ist, den du schenkest, bitter,", "tokens": ["Ein", "je\u00b7der", "Gru\u00df", "ist", ",", "den", "du", "schen\u00b7kest", ",", "bit\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VAFIN", "$,", "PRELS", "PPER", "VVFIN", "$,", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Bitter ein jeder Ku\u00df, den du nicht schenkest,", "tokens": ["Bit\u00b7ter", "ein", "je\u00b7der", "Ku\u00df", ",", "den", "du", "nicht", "schen\u00b7kest", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "PIAT", "NN", "$,", "PRELS", "PPER", "PTKNEG", "VVFIN", "$,"], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Bitter ist, was du sprichst und was du denkest,", "tokens": ["Bit\u00b7ter", "ist", ",", "was", "du", "sprichst", "und", "was", "du", "den\u00b7kest", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "$,", "PWS", "PPER", "VVFIN", "KON", "PWS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "Und was du hast und was du bist, ist bitter.", "tokens": ["Und", "was", "du", "hast", "und", "was", "du", "bist", ",", "ist", "bit\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VAFIN", "KON", "PWS", "PPER", "VAFIN", "$,", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Voraus kommt eine Bitterkeit gegangen,", "tokens": ["Vo\u00b7raus", "kommt", "ei\u00b7ne", "Bit\u00b7ter\u00b7keit", "ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "VVPP"