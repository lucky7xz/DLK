{"textgrid.poem.28842": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "57.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Poeten steht was frey, ihr Jungfern! eure Wangen,", "tokens": ["Po\u00b7et\u00b7en", "steht", "was", "frey", ",", "ihr", "Jung\u00b7fern", "!", "eu\u00b7re", "Wan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PIS", "PTKVZ", "$,", "PPOSAT", "NN", "$.", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Worauff die Sch\u00f6nheit spielt, die Charites so prangen", "tokens": ["Wo\u00b7rauff", "die", "Sch\u00f6n\u00b7heit", "spielt", ",", "die", "Cha\u00b7ri\u00b7tes", "so", "pran\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PAV", "ART", "NN", "VVFIN", "$,", "ART", "NN", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und Flora Wohnung h\u00e4lt, die ehr ich. Die Natur", "tokens": ["Und", "Flo\u00b7ra", "Woh\u00b7nung", "h\u00e4lt", ",", "die", "ehr", "ich", ".", "Die", "Na\u00b7tur"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "NE", "NN", "VVFIN", "$,", "ART", "NN", "PPER", "$.", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Hat reichlich bracht hieher in einer vollen Spur", "tokens": ["Hat", "reich\u00b7lich", "bracht", "hie\u00b7her", "in", "ei\u00b7ner", "vol\u00b7len", "Spur"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADJD", "VVFIN", "PAV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die Gaben ihrer Kunst. Hier sind die linden H\u00f6hen,", "tokens": ["Die", "Ga\u00b7ben", "ih\u00b7rer", "Kunst", ".", "Hier", "sind", "die", "lin\u00b7den", "H\u00f6\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$.", "ADV", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "F\u00fcr denen Hybla blast, f\u00fcr denen traurig stehen", "tokens": ["F\u00fcr", "de\u00b7nen", "Hy\u00b7bla", "blast", ",", "f\u00fcr", "de\u00b7nen", "trau\u00b7rig", "ste\u00b7hen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "NE", "VVFIN", "$,", "APPR", "PRELS", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "P\u00e6stanische Gew\u00e4chs und Lilien nichts sind", "tokens": ["P\u00e6s\u00b7ta\u00b7ni\u00b7sche", "Ge\u00b7w\u00e4chs", "und", "Li\u00b7li\u00b7en", "nichts", "sind"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "KON", "NN", "PIS", "VAFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und Helffenbein nicht taug und Purpur wie verblind.", "tokens": ["Und", "Helf\u00b7fen\u00b7bein", "nicht", "taug", "und", "Pur\u00b7pur", "wie", "ver\u00b7blind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PTKNEG", "VVFIN", "KON", "NN", "KOKOM", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Hier ist der runde Zweck, drauff mit viel tausend K\u00fcssen", "tokens": ["Hier", "ist", "der", "run\u00b7de", "Zweck", ",", "drauff", "mit", "viel", "tau\u00b7send", "K\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "$,", "PAV", "APPR", "PIAT", "CARD", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Uns derer Werth mahnt an zu zielen und zu schissen", "tokens": ["Uns", "de\u00b7rer", "Werth", "mahnt", "an", "zu", "zie\u00b7len", "und", "zu", "schis\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "PDS", "NN", "VVFIN", "PTKVZ", "PTKZU", "VVINF", "KON", "PTKZU", "VVINF"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.11": {"text": "Au\u00df Ehrerbittung blo\u00df; (wiewols der Brauch verbeut", "tokens": ["Au\u00df", "Ehr\u00b7er\u00b7bit\u00b7tung", "blo\u00df", ";(", "wie\u00b7wols", "der", "Brauch", "ver\u00b7beut"], "token_info": ["word", "word", "word", "emoticon", "word", "word", "word", "word"], "pos": ["APPR", "NN", "ADV", "$(", "KOUS", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und deutsche Zucht nicht wil, die auch den Argwohn scheut.)", "tokens": ["Und", "deut\u00b7sche", "Zucht", "nicht", "wil", ",", "die", "auch", "den", "Arg\u00b7wohn", "scheut", ".", ")"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADJA", "NN", "PTKNEG", "VMFIN", "$,", "PRELS", "ADV", "ART", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Hier ist das klare Feld, drauff Tugend hin ins lichte", "tokens": ["Hier", "ist", "das", "kla\u00b7re", "Feld", ",", "drauff", "Tu\u00b7gend", "hin", "ins", "lich\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "$,", "PAV", "NN", "ADV", "APPRART", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Streut au\u00df die edle Scham, zu tragen reine Fr\u00fcchte,", "tokens": ["Streut", "au\u00df", "die", "ed\u00b7le", "Scham", ",", "zu", "tra\u00b7gen", "rei\u00b7ne", "Fr\u00fcch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,", "PTKZU", "VVINF", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Die so sch\u00f6n r\u00f6thlich bl\u00fchn, die weit ein mehres werth,", "tokens": ["Die", "so", "sch\u00f6n", "r\u00f6th\u00b7lich", "bl\u00fchn", ",", "die", "weit", "ein", "meh\u00b7res", "werth", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "ADJD", "VVINF", "$,", "PRELS", "ADJD", "ART", "PIS", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Als was die rothe See ie von Corall beschert.", "tokens": ["Als", "was", "die", "ro\u00b7the", "See", "ie", "von", "Co\u00b7rall", "be\u00b7schert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "ADJA", "NN", "ADV", "APPR", "NE", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Hier ist der zart Altar von weissen Marmor-St\u00fccken,", "tokens": ["Hier", "ist", "der", "zart", "Al\u00b7tar", "von", "weis\u00b7sen", "Mar\u00b7mor\u00b7St\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJD", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Drauff jungferliche Zucht pflegt reines Blut zu schicken", "tokens": ["Drauff", "jung\u00b7fer\u00b7li\u00b7che", "Zucht", "pflegt", "rei\u00b7nes", "Blut", "zu", "schi\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "ADJA", "NN", "VVFIN", "ADJA", "NN", "PTKZU", "VVINF"], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.19": {"text": "Zum Opffer keuschem From. Hier ist das flache Rund,", "tokens": ["Zum", "Opf\u00b7fer", "keu\u00b7schem", "From", ".", "Hier", "ist", "das", "fla\u00b7che", "Rund", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJA", "NN", "$.", "ADV", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Drum Zephyrus spielt her, drauff offt Cupido stund", "tokens": ["Drum", "Ze\u00b7phy\u00b7rus", "spielt", "her", ",", "drauff", "offt", "Cu\u00b7pi\u00b7do", "stund"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PAV", "NE", "VVFIN", "PTKVZ", "$,", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.21": {"text": "Und sich um einen Weg f\u00fcr seinen Pfeil umsahe", "tokens": ["Und", "sich", "um", "ei\u00b7nen", "Weg", "f\u00fcr", "sei\u00b7nen", "Pfeil", "um\u00b7sa\u00b7he"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PRF", "APPR", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Und dachte, wie ein Wild f\u00fcr seine K\u00fcch er fahe", "tokens": ["Und", "dach\u00b7te", ",", "wie", "ein", "Wild", "f\u00fcr", "sei\u00b7ne", "K\u00fcch", "er", "fa\u00b7he"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "PWAV", "ART", "NN", "APPR", "PPOSAT", "NN", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Mit seinem Purpur-Zeug. Hier lag er offt im Halt,", "tokens": ["Mit", "sei\u00b7nem", "Pur\u00b7pur\u00b7Zeug", ".", "Hier", "lag", "er", "offt", "im", "Halt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$.", "ADV", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Mit Rosen wol verh\u00e4gt, wann er die Jagt bestalt.", "tokens": ["Mit", "Ro\u00b7sen", "wol", "ver\u00b7h\u00e4gt", ",", "wann", "er", "die", "Jagt", "be\u00b7stalt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "VVFIN", "$,", "PWAV", "PPER", "ART", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Hier tr\u00e4gt Pomona f\u00fcr Vertumnus, ihrem Schatze,", "tokens": ["Hier", "tr\u00e4gt", "Po\u00b7mo\u00b7na", "f\u00fcr", "Ver\u00b7tum\u00b7nus", ",", "ih\u00b7rem", "Schat\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "APPR", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Die roth und weisse Frucht, das sch\u00f6nste Paar vom Platze,", "tokens": ["Die", "roth", "und", "weis\u00b7se", "Frucht", ",", "das", "sch\u00f6ns\u00b7te", "Paar", "vom", "Plat\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "KON", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Den ihre M\u00fch gepflantzt. Hier brennt die n\u00fctze Glut", "tokens": ["Den", "ih\u00b7re", "M\u00fch", "ge\u00b7pflantzt", ".", "Hier", "brennt", "die", "n\u00fct\u00b7ze", "Glut"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "PPOSAT", "NN", "VVPP", "$.", "ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "De\u00df Pharos, der im Meer den Schiffen Bahn und Mut", "tokens": ["De\u00df", "Pha\u00b7ros", ",", "der", "im", "Meer", "den", "Schif\u00b7fen", "Bahn", "und", "Mut"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "APPRART", "NN", "ART", "NN", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Zu sichrem Ufer gibt. Hier scheint das keusche Feuer,", "tokens": ["Zu", "sich\u00b7rem", "U\u00b7fer", "gibt", ".", "Hier", "scheint", "das", "keu\u00b7sche", "Feu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$.", "ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Das mehr als Vesta Flamm ist zu verehren theuer;", "tokens": ["Das", "mehr", "als", "Ves\u00b7ta", "Flamm", "ist", "zu", "ver\u00b7eh\u00b7ren", "theu\u00b7er", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "KOKOM", "NE", "NE", "VAFIN", "PTKZU", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Das bringt den klaren Tag hin in die finstre Nacht;", "tokens": ["Das", "bringt", "den", "kla\u00b7ren", "Tag", "hin", "in", "die", "finst\u00b7re", "Nacht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "ADJA", "NN", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Drau\u00df merckt man, ob da schlieff, drau\u00df merckt man, ob da wacht", "tokens": ["Drau\u00df", "merckt", "man", ",", "ob", "da", "schlieff", ",", "drau\u00df", "merckt", "man", ",", "ob", "da", "wacht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PIS", "$,", "KOUS", "ADV", "VVFIN", "$,", "PAV", "VVFIN", "PIS", "$,", "KOUS", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Die Scham der Redligkeit, (in derer Port zu l\u00e4nden,", "tokens": ["Die", "Scham", "der", "Red\u00b7lig\u00b7keit", ",", "(", "in", "de\u00b7rer", "Port", "zu", "l\u00e4n\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "$(", "APPR", "PDS", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-++--+-+-", "measure": "iambic.hexa.relaxed"}, "line.34": {"text": "Wer redlich anders buhlt, sein Schiff pflegt hin zu wenden", "tokens": ["Wer", "red\u00b7lich", "an\u00b7ders", "buhlt", ",", "sein", "Schiff", "pflegt", "hin", "zu", "wen\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "ADJD", "ADV", "VVFIN", "$,", "PPOSAT", "NN", "VVFIN", "ADV", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Und sonsten nirgend wo, er sey dann so gesinnt,", "tokens": ["Und", "sons\u00b7ten", "nir\u00b7gend", "wo", ",", "er", "sey", "dann", "so", "ge\u00b7sinnt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PWAV", "$,", "PPER", "VAFIN", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Da\u00df bey ihm Ehr und Schmach vergleichten Au\u00dfschlag findt;)", "tokens": ["Da\u00df", "bey", "ihm", "Ehr", "und", "Schmach", "ver\u00b7gleich\u00b7ten", "Au\u00df\u00b7schlag", "findt", ";)"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "emoticon"], "pos": ["KOUS", "APPR", "PPER", "NN", "KON", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Hier hebet sich entpor, hier breitet seine Wellen", "tokens": ["Hier", "he\u00b7bet", "sich", "ent\u00b7por", ",", "hier", "brei\u00b7tet", "sei\u00b7ne", "Wel\u00b7len"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "PTKVZ", "$,", "ADV", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Der Tugend Haupt-Panier; hier lacht sie, wann sie lacht;", "tokens": ["Der", "Tu\u00b7gend", "Haup\u00b7tPa\u00b7nier", ";", "hier", "lacht", "sie", ",", "wann", "sie", "lacht", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$.", "ADV", "VVFIN", "PPER", "$,", "PWAV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Hier ist ihr eigner Schmuck; hier ist ihr eigner Pracht.", "tokens": ["Hier", "ist", "ihr", "eig\u00b7ner", "Schmuck", ";", "hier", "ist", "ihr", "eig\u00b7ner", "Pracht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "ADJA", "NN", "$.", "ADV", "VAFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Poeten steht was frey, ihr Jungfern! eure Wangen,", "tokens": ["Po\u00b7et\