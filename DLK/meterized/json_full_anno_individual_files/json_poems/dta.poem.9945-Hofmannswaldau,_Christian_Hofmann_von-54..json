{"dta.poem.9945": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "54.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn ich ein-wo gehen soll/ bin ich still/ nehm mir die weile;", "tokens": ["Wenn", "ich", "ein\u00b7wo", "ge\u00b7hen", "soll", "/", "bin", "ich", "still", "/", "nehm", "mir", "die", "wei\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVINF", "VMFIN", "$(", "VAFIN", "PPER", "ADJD", "$(", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "--+--+-+-+-+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Aber wenn ich gehe fort/ ich mit grossem poltern eile.", "tokens": ["A\u00b7ber", "wenn", "ich", "ge\u00b7he", "fort", "/", "ich", "mit", "gros\u00b7sem", "pol\u00b7tern", "ei\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "PTKVZ", "$(", "PPER", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "+--+-+-+-+-+-+-", "measure": "iambic.septa.invert"}, "line.3": {"text": "Niemahls bleib ich auff der stelle/ wo man mich hat hingethan/", "tokens": ["Nie\u00b7mahls", "bleib", "ich", "auff", "der", "stel\u00b7le", "/", "wo", "man", "mich", "hat", "hin\u00b7ge\u00b7than", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "ADJA", "$(", "PWAV", "PIS", "PPER", "VAFIN", "VVPP", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Dem/ dem einmahl ich gedienet/ steh ich weiter nicht mehr an.", "tokens": ["Dem", "/", "dem", "ein\u00b7mahl", "ich"