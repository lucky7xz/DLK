{"textgrid.poem.57031": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Bona Fide", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Palmstr\u00f6m geht durch eine fremde Stadt ...", "tokens": ["Palm\u00b7str\u00f6m", "geht", "durch", "ei\u00b7ne", "frem\u00b7de", "Stadt", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Lieber Gott, so denkt er, welch ein Regen!", "tokens": ["Lie\u00b7ber", "Gott", ",", "so", "denkt", "er", ",", "welch", "ein", "Re\u00b7gen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "$,", "ADV", "VVFIN", "PPER", "$,", "PWAT", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Und er spannt den Schirm auf, den er hat.", "tokens": ["Und", "er", "spannt", "den", "Schirm", "auf", ",", "den", "er", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "PTKVZ", "$,", "PRELS", "PPER", "VAFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Doch am Himmel tut sich nichts bewegen,", "tokens": ["Doch", "am", "Him\u00b7mel", "tut", "sich", "nichts", "be\u00b7we\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VVFIN", "PRF", "PIS", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "und kein Windhauch r\u00fchrt ein Blatt.", "tokens": ["und", "kein", "Wind\u00b7hauch", "r\u00fchrt", "ein", "Blatt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Gleichwohl darf man jenen Argwohn hegen.", "tokens": ["Gleich\u00b7wohl", "darf", "man", "je\u00b7nen", "Arg\u00b7wohn", "he\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIS", "PDAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Denn das Pflaster, \u00fcber das er wandelt,", "tokens": ["Denn", "das", "Pflas\u00b7ter", ",", "\u00fc\u00b7ber", "das", "er", "wan\u00b7delt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "APPR", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "ist vom Magistrat voll List \u2013 gesprenkelt.", "tokens": ["ist", "vom", "Ma\u00b7gist\u00b7rat", "voll", "List", "\u2013", "ge\u00b7spren\u00b7kelt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "APPRART", "NN", "ADJD", "NN", "$(", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Bona fide hat der Gast gehandelt.", "tokens": ["Bo\u00b7na", "fi\u00b7de", "hat", "der", "Gast", "ge\u00b7han\u00b7delt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Palmstr\u00f6m geht durch eine fremde Stadt ...", "tokens": ["Palm\u00b7str\u00f6m", "geht", "durch", "ei\u00b7ne", "frem\u00b7de", "Stadt", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "li