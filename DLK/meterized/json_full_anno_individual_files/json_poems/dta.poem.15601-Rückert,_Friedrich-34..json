{"dta.poem.15601": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "34.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Warum beneidest du, was andern ist beschieden,", "tokens": ["Wa\u00b7rum", "be\u00b7nei\u00b7dest", "du", ",", "was", "an\u00b7dern", "ist", "be\u00b7schie\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "$,", "PRELS", "PIS", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und bist mit dem, was dir zu Theil ward, unzufrieden?", "tokens": ["Und", "bist", "mit", "dem", ",", "was", "dir", "zu", "Theil", "ward", ",", "un\u00b7zu\u00b7frie\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "ART", "$,", "PWS", "PPER", "APPR", "NN", "VAFIN", "$,", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Du stehest dir zu nah, um recht dich zu erkennen,", "tokens": ["Du", "ste\u00b7hest", "dir", "zu", "nah", ",", "um", "recht", "dich", "zu", "er\u00b7ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKA", "ADJD", "$,", "KOUI", "ADJD", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und anderen zu fern, um Schein von Seyn zu trennen;", "tokens": ["Und", "an\u00b7de\u00b7ren", "zu", "fern", ",", "um", "Schein", "von", "Seyn", "zu", "tren\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "PTKZU", "VVINF", "$,", "KOUI", "NN", "APPR", "PPOSAT", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wie du die Erd', auf der du stehst, nicht siehest ganz,", "tokens": ["Wie", "du", "die", "Erd'", ",", "auf", "der", "du", "stehst", ",", "nicht", "sie\u00b7hest", "ganz", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "$,", "APPR", "PRELS", "PPER", "VVFIN", "$,", "PTKNEG", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und dir der Mond erscheint in t\u00e4uschungsvollem Glanz.", "tokens": ["Und", "dir", "der", "Mond", "er\u00b7scheint", "in", "t\u00e4u\u00b7schungs\u00b7vol\u00b7lem", "Glanz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Doch tr\u00f6ste dich, es wird im Mond auch einer stehn,", "tokens": ["Doch", "tr\u00f6s\u00b7te", "dich", ",", "es", "wird", "im", "Mond", "auch", "ei\u00b7ner", "stehn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "PPER", "VAFIN", "APPRART", "NN", "ADV", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der dunkel wird den Mond, und hell die Erde sehn.", "tokens": ["Der", "dun\u00b7kel", "wird", "den", "Mond", ",", "und", "hell", "die", "Er\u00b7de", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VAFIN", "ART", "NN", "$,", "KON", "ADJD", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}