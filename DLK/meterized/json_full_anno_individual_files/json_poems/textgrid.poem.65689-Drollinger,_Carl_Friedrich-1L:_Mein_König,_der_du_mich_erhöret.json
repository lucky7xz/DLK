{"textgrid.poem.65689": {"metadata": {"author": {"name": "Drollinger, Carl Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mein K\u00f6nig, der du mich erh\u00f6ret", "genre": "verse", "period": "N.A.", "pub_year": 1737, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein K\u00f6nig, der du mich erh\u00f6ret", "tokens": ["Mein", "K\u00f6\u00b7nig", ",", "der", "du", "mich", "er\u00b7h\u00f6\u00b7ret"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "PPER", "PRF", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In meinem allergr\u00f6sten Leid;", "tokens": ["In", "mei\u00b7nem", "al\u00b7ler\u00b7gr\u00f6s\u00b7ten", "Leid", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der meiner Feinde Lust gest\u00f6ret,", "tokens": ["Der", "mei\u00b7ner", "Fein\u00b7de", "Lust", "ge\u00b7st\u00f6\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die sich auf meinen Fall gefre\u00fct;", "tokens": ["Die", "sich", "auf", "mei\u00b7nen", "Fall", "ge\u00b7fre\u00fct", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der mich des Todes Arm entr\u00fccket;", "tokens": ["Der", "mich", "des", "To\u00b7des", "Arm", "ent\u00b7r\u00fc\u00b7cket", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und, da ich schon zur Grube sank,", "tokens": ["Und", ",", "da", "ich", "schon", "zur", "Gru\u00b7be", "sank", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PPER", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Mit ne\u00fcer Lebenskraft erquicket:", "tokens": ["Mit", "ne\u00b7\u00fcer", "Le\u00b7bens\u00b7kraft", "er\u00b7quic\u00b7ket", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.8": {"text": "Dir sag ich ewig Lob und Dank.", "tokens": ["Dir", "sag", "ich", "e\u00b7wig", "Lob", "und", "Dank", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ihr alle, die sein Schutz umfangen,", "tokens": ["Ihr", "al\u00b7le", ",", "die", "sein", "Schutz", "um\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "$,", "PRELS", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Besingt ihn mit mir sp\u00e4t und fr\u00fch.", "tokens": ["Be\u00b7singt", "ihn", "mit", "mir", "sp\u00e4t", "und", "fr\u00fch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPER", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sein Zorn ist, wie ein Wind, vergangen;", "tokens": ["Sein", "Zorn", "ist", ",", "wie", "ein", "Wind", ",", "ver\u00b7gan\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "$,", "PWAV", "ART", "NN", "$,", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Doch seine G\u00fcte weichet nie.", "tokens": ["Doch", "sei\u00b7ne", "G\u00fc\u00b7te", "wei\u00b7chet", "nie", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wer lebet so in Lust und Segen,", "tokens": ["Wer", "le\u00b7bet", "so", "in", "Lust", "und", "Se\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wie seiner auserw\u00e4hlten Hauff?", "tokens": ["Wie", "sei\u00b7ner", "au\u00b7ser\u00b7w\u00e4hl\u00b7ten", "Hauff", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wenn sie sich weinend niderlegen,", "tokens": ["Wenn", "sie", "sich", "wei\u00b7nend", "ni\u00b7der\u00b7le\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "So stehn sie Morgens jauchzend auf.", "tokens": ["So", "stehn", "sie", "Mor\u00b7gens", "jauch\u00b7zend", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ich aber sprach in meinem Gl\u00fccke:", "tokens": ["Ich", "a\u00b7ber", "sprach", "in", "mei\u00b7nem", "Gl\u00fc\u00b7cke", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nun kan ich nimmer untergehn.", "tokens": ["Nun", "kan", "ich", "nim\u00b7mer", "un\u00b7ter\u00b7gehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich dachte nicht an sein Geschicke,", "tokens": ["Ich", "dach\u00b7te", "nicht", "an", "sein", "Ge\u00b7schi\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-++--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und wollte von mir selber stehn.", "tokens": ["Und", "woll\u00b7te", "von", "mir", "sel\u00b7ber", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "APPR", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Doch, o wie pl\u00f6tzlich wird zu nichte,", "tokens": ["Doch", ",", "o", "wie", "pl\u00f6tz\u00b7lich", "wird", "zu", "nich\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "FM", "KOKOM", "ADJD", "VAFIN", "PTKA", "PIS", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Was nicht sein starker Arm erh\u00e4lt.", "tokens": ["Was", "nicht", "sein", "star\u00b7ker", "Arm", "er\u00b7h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PTKNEG", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Kaum barg er nur sein Angesichte,", "tokens": ["Kaum", "barg", "er", "nur", "sein", "An\u00b7ge\u00b7sich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "So war ich armer schon gef\u00e4llt.", "tokens": ["So", "war", "ich", "ar\u00b7mer", "schon", "ge\u00b7f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJA", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Dann sucht ich wieder sein Erbarmen,", "tokens": ["Dann", "sucht", "ich", "wie\u00b7der", "sein", "Er\u00b7bar\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und flehte seiner Allmacht zu:", "tokens": ["Und", "fleh\u00b7te", "sei\u00b7ner", "All\u00b7macht", "zu", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "O mein Erl\u00f6ser, hilf mir Armen!", "tokens": ["O", "mein", "Er\u00b7l\u00f6\u00b7ser", ",", "hilf", "mir", "Ar\u00b7men", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "$,", "VVIMP", "PPER", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Es kan es niemand sonst, als du.", "tokens": ["Es", "kan", "es", "nie\u00b7mand", "sonst", ",", "als", "du", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PIS", "ADV", "$,", "KOUS", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Was n\u00fctzt dirs, wenn du mich zerst\u00f6ret?", "tokens": ["Was", "n\u00fctzt", "dirs", ",", "wenn", "du", "mich", "zer\u00b7st\u00f6\u00b7ret", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PIS", "$,", "KOUS", "PPER", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wer r\u00fchmt dich in des Todes Nacht?", "tokens": ["Wer", "r\u00fchmt", "dich", "in", "des", "To\u00b7des", "Nacht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PRF", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Hingegen, weil mein Athem w\u00e4hret,", "tokens": ["Hin\u00b7ge\u00b7gen", ",", "weil", "mein", "A\u00b7them", "w\u00e4h\u00b7ret", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "So sing ich stets von deiner Macht.", "tokens": ["So", "sing", "ich", "stets", "von", "dei\u00b7ner", "Macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Du h\u00f6rtest, Herr! und halfst mir wieder", "tokens": ["Du", "h\u00f6r\u00b7test", ",", "Herr", "!", "und", "halfst", "mir", "wie\u00b7der"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "NN", "$.", "KON", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Aus meinem Tod und Untergang.", "tokens": ["Aus", "mei\u00b7nem", "Tod", "und", "Un\u00b7ter\u00b7gang", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du kehrtest meine Trauerlieder", "tokens": ["Du", "kehr\u00b7test", "mei\u00b7ne", "Trau\u00b7er\u00b7lie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In einen frohen Lobgesang.", "tokens": ["In", "ei\u00b7nen", "fro\u00b7hen", "Lob\u00b7ge\u00b7sang", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Du hast mein Serbgewand zerrissen,", "tokens": ["Du", "hast", "mein", "Serb\u00b7ge\u00b7wand", "zer\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und mich mit Purpur angetahn.", "tokens": ["Und", "mich", "mit", "Pur\u00b7pur", "an\u00b7ge\u00b7tahn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ich bin auf deinen Ruhm beflissen,", "tokens": ["Ich", "bin", "auf", "dei\u00b7nen", "Ruhm", "be\u00b7flis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Bi\u00df da\u00df ich nimmer athmen kan.", "tokens": ["Bi\u00df", "da\u00df", "ich", "nim\u00b7mer", "ath\u00b7men", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PPER", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Mein K\u00f6nig, der du mich erh\u00f6ret", "tokens": ["Mein", "K\u00f6\u00b7nig", ",", "der", "du", "mich", "er\u00b7h\u00f6\u00b7ret"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "PPER", "PRF", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In meinem allergr\