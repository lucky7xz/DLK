re": "iambic.tetra"}, "line.8": {"text": "So sing ich stets von deiner Macht.", "tokens": ["So", "sing", "ich", "stets", "von", "dei\u00b7ner", "Macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Du h\u00f6rtest, Herr! und halfst mir wieder", "tokens": ["Du", "h\u00f6r\u00b7test", ",", "Herr", "!", "und", "halfst", "mir", "wie\u00b7der"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "NN", "$.", "KON", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Aus meinem Tod und Untergang.", "tokens": ["Aus", "mei\u00b7nem", "Tod", "und", "Un\u00b7ter\u00b7gang", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du kehrtest meine Trauerlieder", "tokens": ["Du", "kehr\u00b7test", "mei\u00b7ne", "Trau\u00b7er\u00b7lie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In einen frohen Lobgesang.", "tokens": ["In", "ei\u00b7nen", "fro\u00b7hen", "Lob\u00b7ge\u00b7sang", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Du hast mein Serbgewand zerrissen,", "tokens": ["Du", "hast", "mein", "Serb\u00b7ge\u00b7wand", "zer\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und mich mit Purpur angetahn.", "tokens": ["Und", "mich", "mit", "Pur\u00b7pur", "an\u00b7ge\u00b7tahn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ich bin auf deinen Ruhm beflissen,", "tokens": ["Ich", "bin", "auf", "dei\u00b7nen", "Ruhm", "be\u00b7flis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Bi\u00df da\u00df ich nimmer athmen kan.", "tokens": ["Bi\u00df", "da\u00df", "ich", "nim\u00b7mer", "ath\u00b7men", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PPER", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}