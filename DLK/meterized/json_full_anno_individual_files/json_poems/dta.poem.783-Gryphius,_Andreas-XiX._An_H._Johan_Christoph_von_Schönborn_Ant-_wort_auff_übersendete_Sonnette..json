{"dta.poem.783": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "XiX.  \n An H. Johan Christoph von Sch\u00f6nborn Ant-  \n wort/ auff \u00fcbersendete Sonnette.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Betr\u00fcbtes Schlesien/ best\u00fcrtzies Vaterland/ ", "tokens": ["Be\u00b7tr\u00fcb\u00b7tes", "Schle\u00b7si\u00b7en", "/", "be\u00b7st\u00fcrt\u00b7zies", "Va\u00b7ter\u00b7land", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$(", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was hast du das der grim der Seuchen nicht verzehret.", "tokens": ["Was", "hast", "du", "das", "der", "grim", "der", "Seu\u00b7chen", "nicht", "ver\u00b7zeh\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "PDS", "ART", "NN", "ART", "NN", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Das der geschwinde Blitz der Schwerder nicht verheret?", "tokens": ["Das", "der", "ge\u00b7schwin\u00b7de", "Blitz", "der", "Schwer\u00b7der", "nicht", "ver\u00b7he\u00b7ret", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "ADJA", "NN", "ART", "NN", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Was findet man bey dir/ als Leichen/ stanck/ vnd brand.", "tokens": ["Was", "fin\u00b7det", "man", "bey", "dir", "/", "als", "Lei\u00b7chen", "/", "stanck", "/", "vnd", "brand", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PIS", "APPR", "PPER", "$(", "KOUS", "NN", "$(", "VVFIN", "$(", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Du schleust/ in den von Blutt der Menschen/ fetten Sand", "tokens": ["Du", "schleust", "/", "in", "den", "von", "Blutt", "der", "Men\u00b7schen", "/", "fet\u00b7ten", "Sand"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "$(", "APPR", "ART", "APPR", "ADJD", "ART", "NN", "$(", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mir Freund\u2019 vnd Bruder eyn. Du hast ihr grab beschweret", "tokens": ["Mir", "Freund'", "vnd", "Bru\u00b7der", "eyn", ".", "Du", "hast", "ihr", "grab", "be\u00b7schwe\u00b7ret"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "NN", "KON", "NN", "VAINF", "$.", "PPER", "VAFIN", "PPER", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mit deiner Aschen last/ dein Thron ist vmgekehret/", "tokens": ["Mit", "dei\u00b7ner", "A\u00b7schen", "last", "/", "dein", "Thron", "ist", "vm\u00b7ge\u00b7keh\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$(", "PPOSAT", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Vnd deine Freyheit heult/ im Demant-festen band", "tokens": ["Vnd", "dei\u00b7ne", "Frey\u00b7heit", "heult", "/", "im", "De\u00b7mant\u00b7fes\u00b7ten", "band"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "$(", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Nur eines hat dir nicht der rawe sturm genommen:", "tokens": ["Nur", "ei\u00b7nes", "hat", "dir", "nicht", "der", "ra\u00b7we", "sturm", "ge\u00b7nom\u00b7men", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VAFIN", "PPER", "PTKNEG", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Recht vnderf\u00e4lschte trew/ die einen sitz bekommen", "tokens": ["Recht", "vn\u00b7der\u00b7f\u00e4lschte", "trew", "/", "die", "ei\u00b7nen", "sitz", "be\u00b7kom\u00b7men"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ADJD", "$(", "ART", "ART", "NN", "VVINF"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Da wo der trawte Geist von meinem Sch\u00f6ndorn lebt.", "tokens": ["Da", "wo", "der", "traw\u00b7te", "Geist", "von", "mei\u00b7nem", "Sch\u00f6n\u00b7dorn", "lebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWAV", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Ich jrre: Nein/ wo ich! Er lebt in meinem Hertzen:", "tokens": ["Ich", "jr\u00b7re", ":", "Nein", "/", "wo", "ich", "!", "Er", "lebt", "in", "mei\u00b7nem", "Hert\u00b7zen", ":"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PTKANT", "$(", "PWAV", "PPER", "$.", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vnd seine Seel in mir/ Ich f\u00fchle seine schmertzen.", "tokens": ["Vnd", "sei\u00b7ne", "Seel", "in", "mir", "/", "Ich", "f\u00fch\u00b7le", "sei\u00b7ne", "schmert\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "APPR", "PPER", "$(", "PPER", "VVFIN", "PPOSAT", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Er/ meine: Schlesien/ di\u00df ist wa\u00df dich erhebt.", "tokens": ["Er", "/", "mei\u00b7ne", ":", "Schle\u00b7si\u00b7en", "/", "di\u00df", "ist", "wa\u00df", "dich", "er\u00b7hebt", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$(", "PPOSAT", "$.", "NE", "$(", "PDS", "VAFIN", "VVFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}