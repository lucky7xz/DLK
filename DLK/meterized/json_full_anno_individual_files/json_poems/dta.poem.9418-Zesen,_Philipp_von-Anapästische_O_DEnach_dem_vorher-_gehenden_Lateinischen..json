{"dta.poem.9418": {"metadata": {"author": {"name": "Zesen, Philipp von", "birth": "N.A.", "death": "N.A."}, "title": "Anap\u00e4stische O DEnach dem vorher-  \n  gehenden Lateinischen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-20684-8", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Winter vergeht/", "tokens": ["Der", "Win\u00b7ter", "ver\u00b7geht", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "der Fr\u00fchling entsteht/", "tokens": ["der", "Fr\u00fch\u00b7ling", "ent\u00b7steht", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "der Regen entweichet/ die Sonne scheint wieder/", "tokens": ["der", "Re\u00b7gen", "ent\u00b7wei\u00b7chet", "/", "die", "Son\u00b7ne", "scheint", "wie\u00b7der", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$(", "ART", "NN", "VVFIN", "ADV", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Sie scheinet mit macht", "tokens": ["Sie", "schei\u00b7net", "mit", "macht"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "VVFIN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "in g\u00fcldener Pracht:", "tokens": ["in", "g\u00fcl\u00b7de\u00b7ner", "Pracht", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "mann h\u00f6ret die sch\u00f6nen und lieblichen Lieder/", "tokens": ["mann", "h\u00f6\u00b7ret", "die", "sch\u00f6\u00b7nen", "und", "lieb\u00b7li\u00b7chen", "Lie\u00b7der", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "ADJA", "KON", "ADJA", "NN", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.7": {"text": "Komm Venus ernieder", "tokens": ["Komm", "Ve\u00b7nus", "er\u00b7nie\u00b7der"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "NE", "ADJA"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "vom heiligen Thron:", "tokens": ["vom", "hei\u00b7li\u00b7gen", "Thron", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.9": {"text": "Die C\u00f6sische Pallas empfindet die flammen/", "tokens": ["Die", "C\u00f6\u00b7si\u00b7sche", "Pal\u00b7las", "emp\u00b7fin\u00b7det", "die", "flam\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "VVINF", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.10": {"text": "die flammen entz\u00fcnden sich allezusammen/", "tokens": ["die", "flam\u00b7men", "ent\u00b7z\u00fcn\u00b7den", "sich", "al\u00b7le\u00b7zu\u00b7sam\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VVINF", "VVFIN", "PRF", "ADJD", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.11": {"text": "erwecken im Munde den lieblichen toon!", "tokens": ["er\u00b7we\u00b7cken", "im", "Mun\u00b7de", "den", "lieb\u00b7li\u00b7chen", "toon", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.2": {"line.1": {"text": "Es f\u00fcnckelt der muth", "tokens": ["Es", "f\u00fcn\u00b7ckelt", "der", "muth"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "in Salomons gluth:", "tokens": ["in", "Sa\u00b7lo\u00b7mons", "gluth", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Sie l\u00e4sset die liebliche Buchner-Art klingen/", "tokens": ["Sie", "l\u00e4s\u00b7set", "die", "lieb\u00b7li\u00b7che", "Buch\u00b7ner\u00b7Art", "klin\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Sie singet ein Lied/", "tokens": ["Sie", "sin\u00b7get", "ein", "Lied", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "ist jmmer bem\u00fcht", "tokens": ["ist", "jm\u00b7mer", "be\u00b7m\u00fcht"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "ADV", "VVFIN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "dem F\u00fcrsten der Weisen der dieses zu singen", "tokens": ["dem", "F\u00fcrs\u00b7ten", "der", "Wei\u00b7sen", "der", "die\u00b7ses", "zu", "sin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "ART", "PDS", "PTKZU", "VVINF"], "meter": "-+--+--++-+-", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "erfunden in springen/", "tokens": ["er\u00b7fun\u00b7den", "in", "sprin\u00b7gen", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "VVINF", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "zu folgen hinnach.", "tokens": ["zu", "fol\u00b7gen", "hin\u00b7nach", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.9": {"text": "Sie singet die Lieder die heiligen Lieder;", "tokens": ["Sie", "sin\u00b7get", "die", "Lie\u00b7der", "die", "hei\u00b7li\u00b7gen", "Lie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.10": {"text": "Dr\u00fcmb weichet Jhr B\u00f6sen (weil C\u00f6sius wieder", "tokens": ["Dr\u00fcmb", "wei\u00b7chet", "Ihr", "B\u00f6\u00b7sen", "(", "weil", "C\u00f6\u00b7sius", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPOSAT", "NN", "$(", "KOUS", "NE", "ADV"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.11": {"text": "mein C\u00f6stus singet) mit euerer schmach!", "tokens": ["mein", "C\u00f6s\u00b7tus", "sin\u00b7get", ")", "mit", "eu\u00b7e\u00b7rer", "schmach", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NE", "VVFIN", "$(", "APPR", "PPOSAT", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}