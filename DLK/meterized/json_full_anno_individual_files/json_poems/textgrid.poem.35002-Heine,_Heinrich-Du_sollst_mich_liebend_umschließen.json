{"textgrid.poem.35002": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Du sollst mich liebend umschlie\u00dfen", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du sollst mich liebend umschlie\u00dfen,", "tokens": ["Du", "sollst", "mich", "lie\u00b7bend", "um\u00b7schlie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "ADJD", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Geliebtes, sch\u00f6nes Weib!", "tokens": ["Ge\u00b7lieb\u00b7tes", ",", "sch\u00f6\u00b7nes", "Weib", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Umschling mich mit Armen und F\u00fc\u00dfen,", "tokens": ["Um\u00b7schling", "mich", "mit", "Ar\u00b7men", "und", "F\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Und mit dem geschmeidigen Leib.", "tokens": ["Und", "mit", "dem", "ge\u00b7schmei\u00b7di\u00b7gen", "Leib", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "$."], "meter": "----+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Gewaltig hat umfangen,", "tokens": ["Ge\u00b7wal\u00b7tig", "hat", "um\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Umwunden, umschlungen schon", "tokens": ["Um\u00b7wun\u00b7den", ",", "um\u00b7schlun\u00b7gen", "schon"], "token_info": ["word", "punct", "word",