{"dta.poem.9712": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Sie erblickte sich im spiegel.  \n C. E.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Grisette schaute sich in einem spiegel an;", "tokens": ["Gri\u00b7set\u00b7te", "schau\u00b7te", "sich", "in", "ei\u00b7nem", "spie\u00b7gel", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PRF", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So bald sie inne ward ihr runtzlichtes gesichte/", "tokens": ["So", "bald", "sie", "in\u00b7ne", "ward", "ihr", "runtz\u00b7lich\u00b7tes", "ge\u00b7sich\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "PTKVZ", "VAFIN", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der gantz zerbrochnen z\u00e4hn\u2019 halbausgefaulte schichte/", "tokens": ["Der", "gantz", "zer\u00b7broch\u00b7nen", "z\u00e4hn'", "hal\u00b7baus\u00b7ge\u00b7faul\u00b7te", "schich\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "NN", "VVFIN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und was mehr scheu\u00dfliches sich an ihr finden kan/", "tokens": ["Und", "was", "mehr", "scheu\u00df\u00b7li\u00b7ches", "sich", "an", "ihr", "fin\u00b7den", "kan", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADV", "ADJA", "PRF", "APPR", "PPER", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die au\u00dfgedorrte haut der abgefleischten wangen/", "tokens": ["Die", "au\u00df\u00b7ge\u00b7dorr\u00b7te", "haut", "der", "ab\u00b7ge\u00b7fleischten", "wan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Und das begrei\u00dfte haar umb ihren scheitel hangen/", "tokens": ["Und", "das", "be\u00b7grei\u00df\u00b7te", "haar", "umb", "ih\u00b7ren", "schei\u00b7tel", "han\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Rieff sie mit blintzenden und halb erstorbnen augen:", "tokens": ["Rieff", "sie", "mit", "blint\u00b7zen\u00b7den", "und", "halb", "ers\u00b7torb\u00b7nen", "au\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "KON", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Vor funffzig jahren sah\u2019 ich mich weit sch\u00f6ner an/", "tokens": ["Vor", "funff\u00b7zig", "jah\u00b7ren", "sah'", "ich", "mich", "weit", "sch\u00f6\u00b7ner", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "VVFIN", "PPER", "PRF", "ADJD", "ADJD", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Da ich nichts \u00e4hnlichs nun mehr an mir finden kan;", "tokens": ["Da", "ich", "nichts", "\u00e4hn\u00b7lichs", "nun", "mehr", "an", "mir", "fin\u00b7den", "kan", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIS", "PIS", "ADV", "ADV", "APPR", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Die spiegel m\u00fcssen itzt in warheit nicht mehr taugen.", "tokens": ["Die", "spie\u00b7gel", "m\u00fcs\u00b7sen", "itzt", "in", "war\u00b7heit", "nicht", "mehr", "tau\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ADV", "APPR", "NN", "PTKNEG", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}