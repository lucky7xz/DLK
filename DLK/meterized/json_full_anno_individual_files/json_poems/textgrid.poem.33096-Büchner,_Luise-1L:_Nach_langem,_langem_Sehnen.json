{"textgrid.poem.33096": {"metadata": {"author": {"name": "B\u00fcchner, Luise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nach langem, langem Sehnen", "genre": "verse", "period": "N.A.", "pub_year": 1849, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nach langem, langem Sehnen", "tokens": ["Nach", "lan\u00b7gem", ",", "lan\u00b7gem", "Seh\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "An deinem Grab ich stand,", "tokens": ["An", "dei\u00b7nem", "Grab", "ich", "stand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nach vielen, bitt'ren Thr\u00e4nen", "tokens": ["Nach", "vie\u00b7len", ",", "bitt'\u00b7ren", "Thr\u00e4\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPR", "PIAT", "$,", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sah ich dies St\u00fcckchen Land,", "tokens": ["Sah", "ich", "dies", "St\u00fcck\u00b7chen", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PDS", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Das Alles kalt bedecket,", "tokens": ["Das", "Al\u00b7les", "kalt", "be\u00b7de\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Woran voll Z\u00e4rtlichkeit,", "tokens": ["Wo\u00b7ran", "voll", "Z\u00e4rt\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Seit Leben ihm erwecket,", "tokens": ["Seit", "Le\u00b7ben", "ihm", "er\u00b7we\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Das Kind hing allezeit!", "tokens": ["Das", "Kind", "hing", "al\u00b7le\u00b7zeit", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Das Kind \u2013 o, Schmerz! ich habe", "tokens": ["Das", "Kind", "\u2013", "o", ",", "Schmerz", "!", "ich", "ha\u00b7be"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$(", "FM", "$,", "NN", "$.", "PPER", "VAFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dich anders nicht gekannt,", "tokens": ["Dich", "an\u00b7ders", "nicht", "ge\u00b7kannt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Stiegst jetzt du aus dem Grabe,", "tokens": ["Stiegst", "jetzt", "du", "aus", "dem", "Gra\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Du h\u00e4tt'st mich kaum erkannt.", "tokens": ["Du", "h\u00e4tt'st", "mich", "kaum", "er\u00b7kannt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Doch wie ich so hier stehe,", "tokens": ["Doch", "wie", "ich", "so", "hier", "ste\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wird ", "tokens": ["Wird"], "token_info": ["word"], "pos": ["VAFIN"], "meter": "+", "measure": "single.up"}, "line.7": {"text": "Trotz allem Schmerz und Wehe,", "tokens": ["Trotz", "al\u00b7lem", "Schmerz", "und", "We\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Im tiefsten Innern klar.", "tokens": ["Im", "tiefs\u00b7ten", "In\u00b7nern", "klar", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Zu fr\u00fch mir hingeschwunden", "tokens": ["Zu", "fr\u00fch", "mir", "hin\u00b7ge\u00b7schwun\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["PTKA", "ADJD", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Warst du mein Lebensstern,", "tokens": ["Warst", "du", "mein", "Le\u00b7bens\u00b7stern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nach dem in allen Stunden", "tokens": ["Nach", "dem", "in", "al\u00b7len", "Stun\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "APPR", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ich sah zum Himmel gern;", "tokens": ["Ich", "sah", "zum", "Him\u00b7mel", "gern", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sein Strahl ward meine Leuchte,", "tokens": ["Sein", "Strahl", "ward", "mei\u00b7ne", "Leuch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Zog meinem Geist voran,", "tokens": ["Zog", "mei\u00b7nem", "Geist", "vo\u00b7ran", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Zum Guten, Sch\u00f6nen zeigte,", "tokens": ["Zum", "Gu\u00b7ten", ",", "Sch\u00f6\u00b7nen", "zeig\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Zur Wahrheit mir die Bahn.", "tokens": ["Zur", "Wahr\u00b7heit", "mir", "die", "Bahn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und da\u00df in ew'ger Treue", "tokens": ["Und", "da\u00df", "in", "ew'\u00b7ger", "Treu\u00b7e"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ihm stets gefolgt mein Herz,", "tokens": ["Ihm", "stets", "ge\u00b7folgt", "mein", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVPP", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df hier ich steh' ohn' Reue,", "tokens": ["Da\u00df", "hier", "ich", "steh'", "ohn'", "Reu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dies s\u00e4nftigt meinen Schmerz;", "tokens": ["Dies", "s\u00e4nf\u00b7tigt", "mei\u00b7nen", "Schmerz", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Da\u00df tief mir im Gem\u00fcthe", "tokens": ["Da\u00df", "tief", "mir", "im", "Ge\u00b7m\u00fc\u00b7the"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJD", "PPER", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Dasselbe Feuer wacht,", "tokens": ["Das\u00b7sel\u00b7be", "Feu\u00b7er", "wacht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Das deine Brust durchgl\u00fchte", "tokens": ["Das", "dei\u00b7ne", "Brust", "durch\u00b7gl\u00fch\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Mit seltner Liebesmacht.", "tokens": ["Mit", "selt\u00b7ner", "Lie\u00b7bes\u00b7macht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "So f\u00fchl' ich mit Entz\u00fccken,", "tokens": ["So", "f\u00fchl'", "ich", "mit", "Ent\u00b7z\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "St\u00fcnd'st eben du vor mir,", "tokens": ["St\u00fcn\u00b7d'st", "e\u00b7ben", "du", "vor", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PPER", "APPR", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Als Geistesschwester dr\u00fccken", "tokens": ["Als", "Geis\u00b7tes\u00b7schwes\u00b7ter", "dr\u00fc\u00b7cken"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "W\u00fcrd'st du an's Herz mich dir!", "tokens": ["W\u00fcrd'st", "du", "an's", "Herz", "mich", "dir", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPRART", "NN", "PPER", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die H\u00e4nde segnend breiten", "tokens": ["Die", "H\u00e4n\u00b7de", "seg\u00b7nend", "brei\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Auf meine Stirne bleich,", "tokens": ["Auf", "mei\u00b7ne", "Stir\u00b7ne", "bleich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Mich wie in Kinderzeiten", "tokens": ["Mich", "wie", "in", "Kin\u00b7der\u00b7zei\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "KOKOM", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Anl\u00e4cheln mild und weich. \u2013", "tokens": ["An\u00b7l\u00e4\u00b7cheln", "mild", "und", "weich", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "ADJD", "KON", "ADJD", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Mu\u00df wieder von ihm gehen,", "tokens": ["Mu\u00df", "wie\u00b7der", "von", "ihm", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dem schmerzlich theuren Ort,", "tokens": ["Dem", "schmerz\u00b7lich", "theu\u00b7ren", "Ort", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch was mir dort geschehen,", "tokens": ["Doch", "was", "mir", "dort", "ge\u00b7sche\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wirkt muthig in mir fort!", "tokens": ["Wirkt", "mut\u00b7hig", "in", "mir", "fort", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "PPER", "PTKVZ", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Da\u00df so du in mir lebest", "tokens": ["Da\u00df", "so", "du", "in", "mir", "le\u00b7best"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "PPER", "APPR", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "F\u00fcr alle Ewigkeit,", "tokens": ["F\u00fcr", "al\u00b7le", "E\u00b7wig\u00b7keit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Zum H\u00f6chsten mich erhebest \u2013", "tokens": ["Zum", "H\u00f6chs\u00b7ten", "mich", "er\u00b7he\u00b7best", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Dies ist Unsterblichkeit!", "tokens": ["Dies", "ist", "U\u00b7nsterb\u00b7lich\u00b7keit", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.7": {"line.1": {"text": "Nach langem, langem Sehnen", "tokens": ["Nach", "lan\u00b7gem", ",", "lan\u00b7gem", "Seh\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "An deinem Grab ich stand,", "tokens": ["An", "dei\u00b7nem", "Grab", "ich", "stand", ","], "token_info": ["word", "word", "word", "word", "word", "pun