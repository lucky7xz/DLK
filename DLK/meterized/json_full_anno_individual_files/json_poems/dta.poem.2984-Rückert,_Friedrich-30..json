{"dta.poem.2984": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "30.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Am Weihnachtabend sind die Kinder zu beneiden,", "tokens": ["Am", "Weih\u00b7nach\u00b7ta\u00b7bend", "sind", "die", "Kin\u00b7der", "zu", "be\u00b7nei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df ihnen B\u00e4ume sich in Gold und Zucker kleiden.", "tokens": ["Da\u00df", "ih\u00b7nen", "B\u00e4u\u00b7me", "sich", "in", "Gold", "und", "Zu\u00b7cker", "klei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "PRF", "APPR", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Sie glauben kindlich, was ihr kindisch Herz begehrt,", "tokens": ["Sie", "glau\u00b7ben", "kind\u00b7lich", ",", "was", "ihr", "kin\u00b7disch", "Herz", "be\u00b7gehrt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$,", "PWS", "PPER", "ADJD", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das hab' unmittelbar das Himmelskind beschert.", "tokens": ["Das", "hab'", "un\u00b7mit\u00b7tel\u00b7bar", "das", "Him\u00b7mel\u00b7skind", "be\u00b7schert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADJD", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Die Mutter ist dabei, der Vater auch im Spiel,", "tokens": ["Die", "Mut\u00b7ter", "ist", "da\u00b7bei", ",", "der", "Va\u00b7ter", "auch", "im", "Spiel", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PAV", "$,", "ART", "NN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sie ahnen es, allein es k\u00fcmmert sie nicht viel.", "tokens": ["Sie", "ah\u00b7nen", "es", ",", "al\u00b7lein", "es", "k\u00fcm\u00b7mert", "sie", "nicht", "viel", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "ADV", "PPER", "VVFIN", "PPER", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Und in den Hintergrund tritt Vater und Mutter gerne,", "tokens": ["Und", "in", "den", "Hin\u00b7ter\u00b7grund", "tritt", "Va\u00b7ter", "und", "Mut\u00b7ter", "ger\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "NN", "KON", "NN", "ADV", "$,"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und l\u00e4\u00dft aus Kindermund die Ehr dem Himmelsterne.", "tokens": ["Und", "l\u00e4\u00dft", "aus", "Kin\u00b7der\u00b7mund", "die", "Ehr", "dem", "Him\u00b7mels\u00b7ter\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Dem Himmelsterne, der das ganze Jahr beschert,", "tokens": ["Dem", "Him\u00b7mels\u00b7ter\u00b7ne", ",", "der", "das", "gan\u00b7ze", "Jahr", "be\u00b7schert", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch als Bescherer wird an Einem Tag geehrt.", "tokens": ["Doch", "als", "Be\u00b7sche\u00b7rer", "wird", "an", "Ei\u00b7nem", "Tag", "ge\u00b7ehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "NN", "VAFIN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Ja, Kinder, glaubt euch nur beschenkt vom Himmelskind;", "tokens": ["Ja", ",", "Kin\u00b7der", ",", "glaubt", "euch", "nur", "be\u00b7schenkt", "vom", "Him\u00b7mel\u00b7skind", ";"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "NN", "$,", "VVFIN", "PPER", "ADV", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gl\u00fcckselig, die wie ihr im Glauben Kinder sind!", "tokens": ["Gl\u00fcck\u00b7se\u00b7lig", ",", "die", "wie", "ihr", "im", "Glau\u00b7ben", "Kin\u00b7der", "sind", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PRELS", "KOKOM", "PPOSAT", "APPRART", "NN", "NN", "VAFIN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}}}}}