 "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.13": {"text": "Dem hocherstaunten J\u00fcnger vor.", "tokens": ["Dem", "ho\u00b7cher\u00b7staun\u00b7ten", "J\u00fcn\u00b7ger", "vor", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich war indes nicht weit gereist,", "tokens": ["Ich", "war", "in\u00b7des", "nicht", "weit", "ge\u00b7reist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PTKNEG", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hatte ein St\u00fcck Salmen aufgespeist.", "tokens": ["Hat\u00b7te", "ein", "St\u00fcck", "Sal\u00b7men", "auf\u00b7ge\u00b7speist", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "NN", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Vater Basedow, unter dieser Zeit,", "tokens": ["Va\u00b7ter", "Ba\u00b7se\u00b7dow", ",", "un\u00b7ter", "die\u00b7ser", "Zeit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "APPR", "PDAT", "NN", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Packt' einen Tanzmeister an seiner Seit", "tokens": ["Packt'", "ei\u00b7nen", "Tanz\u00b7meis\u00b7ter", "an", "sei\u00b7ner", "Seit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-++-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Und zeigt' ihm, was die Taufe klar", "tokens": ["Und", "zeigt'", "ihm", ",", "was", "die", "Tau\u00b7fe", "klar"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON",