{"dta.poem.958": {"metadata": {"author": {"name": "George, Stefan", "birth": "N.A.", "death": "N.A."}, "title": "S IEG   D ES   S OMMERS", "genre": "Lyrik", "period": "N.A.", "pub_year": "1897", "urn": "urn:nbn:de:kobv:b4-200905191530", "language": ["de:0.99"], "booktitle": "George, Stefan: Das Jahr der Seele. Berlin, 1897."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn von den eichen erste morgenk&#252;hle               ", "tokens": ["Wenn", "von", "den", "ei\u00b7chen", "ers\u00b7te", "mor\u00b7genk", "&#252;", "hle"], "token_info": ["word", "word", "word", "word", "word", "word", "XML_entity", "word"], "pos": ["KOUS", "APPR", "ART", "ADJA", "ADJA", "NN", "$(", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die feuchten perlen uns ins antlitz blies               ", "tokens": ["Die", "feuch\u00b7ten", "per\u00b7len", "uns", "ins", "ant\u00b7litz", "blies"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PPER", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "So knirrte auf dem pfad der spitze kies               ", "tokens": ["So", "knirr\u00b7te", "auf", "dem", "pfad", "der", "spit\u00b7ze", "kies"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Erinnerte die schweigenden gef&#252;hle             ", "tokens": ["E\u00b7rin\u00b7ner\u00b7te", "die", "schwei\u00b7gen\u00b7den", "gef", "&#252;", "hle"], "token_info": ["word", "word", "word", "word", "XML_entity", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$(", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Und auch die eigene stimme schien dir rauh               ", "tokens": ["Und", "auch", "die", "ei\u00b7ge\u00b7ne", "stim\u00b7me", "schien", "dir", "rauh"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "ADJA", "NN", "VVFIN", "PPER", "ADJD"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Wenn du im takt verwandter pulse bangen               ", "tokens": ["Wenn", "du", "im", "takt", "ver\u00b7wand\u00b7ter", "pul\u00b7se", "ban\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPRART", "ADJD", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Vernahmst die enger zu den deinen drangen               ", "tokens": ["Ver\u00b7nahmst", "die", "en\u00b7ger", "zu", "den", "dei\u00b7nen", "dran\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJD", "APPR", "ART", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und laues schmiegen trocknete den tau.             ", "tokens": ["Und", "lau\u00b7es", "schmie\u00b7gen", "trock\u00b7ne\u00b7te", "den", "tau", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "ADJA", "VVFIN", "ART", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}