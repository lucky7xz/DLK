{"dta.poem.8921": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "LiV.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Kommt Pfeil auf Pfeil in meine Brust geflogen:", "tokens": ["Kommt", "Pfeil", "auf", "Pfeil", "in", "mei\u00b7ne", "Brust", "ge\u00b7flo\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "NN", "APPR", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Du hast mir mehr als Einen vorgezogen,", "tokens": ["Du", "hast", "mir", "mehr", "als", "Ei\u00b7nen", "vor\u00b7ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PIAT", "KOKOM", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Den ich als K\u00f6rper ohne Seele kenne.", "tokens": ["Den", "ich", "als", "K\u00f6r\u00b7per", "oh\u00b7ne", "See\u00b7le", "ken\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "KOUS", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Doch w\u00e4hrend ich in deiner Flamme brenne,", "tokens": ["Doch", "w\u00e4h\u00b7rend", "ich", "in", "dei\u00b7ner", "Flam\u00b7me", "bren\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Bek\u00e4mpf ich stets in mir die st\u00fcrm'schen Wogen,", "tokens": ["Be\u00b7k\u00e4mpf", "ich", "stets", "in", "mir", "die", "st\u00fcr\u00b7m'\u00b7schen", "Wo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "APPR", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Damit ich z\u00fcrnend nicht und oft betrogen", "tokens": ["Da\u00b7mit", "ich", "z\u00fcr\u00b7nend", "nicht", "und", "oft", "be\u00b7tro\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "PTKNEG", "KON", "ADV", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mit einem bittern Namen dich benenne!", "tokens": ["Mit", "ei\u00b7nem", "bit\u00b7tern", "Na\u00b7men", "dich", "be\u00b7nen\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "O nein, Geliebter! Keine Klage sch\u00e4nde,", "tokens": ["O", "nein", ",", "Ge\u00b7lieb\u00b7ter", "!", "Kei\u00b7ne", "Kla\u00b7ge", "sch\u00e4n\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PTKANT", "$,", "NN", "$.", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von schwarzem Unmuth weibisch hingerissen,", "tokens": ["Von", "schwar\u00b7zem", "Un\u00b7muth", "wei\u00b7bisch", "hin\u00b7ge\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Den liebensw\u00fcrdigsten der Gegenst\u00e4nde!", "tokens": ["Den", "lie\u00b7bens\u00b7w\u00fcr\u00b7digs\u00b7ten", "der", "Ge\u00b7gen\u00b7st\u00e4n\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}}, "stanza.4": {"line.1": {"text": "Wenn meiner Freundschaft nie du dich beflissen,", "tokens": ["Wenn", "mei\u00b7ner", "Freund\u00b7schaft", "nie", "du", "dich", "be\u00b7flis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "War mein die Schuld: man beut ja nicht die H\u00e4nde", "tokens": ["War", "mein", "die", "Schuld", ":", "man", "beut", "ja", "nicht", "die", "H\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPOSAT", "ART", "NN", "$.", "PIS", "VVFIN", "ADV", "PTKNEG", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Zum Bunde blos, man mu\u00df zu fesseln wissen.", "tokens": ["Zum", "Bun\u00b7de", "blos", ",", "man", "mu\u00df", "zu", "fes\u00b7seln", "wis\u00b7sen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "$,", "PIS", "VMFIN", "PTKZU", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}