{"dta.poem.14533": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Der Kranke .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Vor dem Fenster durch die Linden", "tokens": ["Vor", "dem", "Fens\u00b7ter", "durch", "die", "Lin\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "NE"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Spielt es wie ein linder Gru\u00df,", "tokens": ["Spielt", "es", "wie", "ein", "lin\u00b7der", "Gru\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KOKOM", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "L\u00fcfte, wollt ihr mir verk\u00fcnden,", "tokens": ["L\u00fcf\u00b7te", ",", "wollt", "ihr", "mir", "ver\u00b7k\u00fcn\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VMFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df ich bald hinunter mu\u00df? \u2014", "tokens": ["Da\u00df", "ich", "bald", "hin\u00b7un\u00b7ter", "mu\u00df", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "VMFIN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}