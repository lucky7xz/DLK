{"textgrid.poem.63054": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "Ein Fr\u00fchlingstag", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Leute schnuppern in die Luft wie Hunde,", "tokens": ["Die", "Leu\u00b7te", "schnup\u00b7pern", "in", "die", "Luft", "wie", "Hun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "KOKOM", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die dieses Fr\u00fchlingstages Ruch ersp\u00fcren wollen.", "tokens": ["Die", "die\u00b7ses", "Fr\u00fch\u00b7lings\u00b7ta\u00b7ges", "Ruch", "er\u00b7sp\u00fc\u00b7ren", "wol\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PDAT", "ADJA", "NN", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Sonne steigt sehr langsam aus dem Grunde", "tokens": ["Die", "Son\u00b7ne", "steigt", "sehr", "lang\u00b7sam", "aus", "dem", "Grun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der schwarzen Wolken, wie ein Bergmann aus den Stollen.", "tokens": ["Der", "schwar\u00b7zen", "Wol\u00b7ken", ",", "wie", "ein", "Berg\u00b7mann", "aus", "den", "Stol\u00b7len", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PWAV", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und aus den Menschen zieht sie einen Schatten,", "tokens": ["Und", "aus", "den", "Men\u00b7schen", "zieht", "sie", "ei\u00b7nen", "Schat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Verzerrt sind Kopf und Rumpf und Flanken...", "tokens": ["Ver\u00b7zerrt", "sind", "Kopf", "und", "Rumpf", "und", "Flan\u00b7ken", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "NN", "KON", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "So kriechen unsre heiligsten Gedanken", "tokens": ["So", "krie\u00b7chen", "uns\u00b7re", "hei\u00b7ligs\u00b7ten", "Ge\u00b7dan\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+---+-", "measure": "unknown.measure.tetra"}, "line.8": {"text": "Vor uns am Boden, die das Licht doch hatten.", "tokens": ["Vor", "uns", "am", "Bo\u00b7den", ",", "die", "das", "Licht", "doch", "hat\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPRART", "NN", "$,", "PRELS", "ART", "NN", "ADV", "VAFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Die Leute schnuppern in die Luft wie Hunde,", "tokens": ["Die", "Leu\u00b7te", "schnup\u00b7pern", "in", "die", "Luft", "wie", "Hun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "KOKOM", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die dieses Fr\u00fchlingstages Ruch ersp\u00fcren wollen.", "tokens": ["Die", "die\u00b7ses", "Fr\u00fch\u00b7lings\u00b7ta\u00b7ges", "Ruch", "er\u00b7sp\u00fc\u00b7ren", "wol\u00b7le