{"textgrid.poem.59160": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "379. In dich will er kommen \u2013", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du bist es, Seele, die ich meine,", "tokens": ["Du", "bist", "es", ",", "See\u00b7le", ",", "die", "ich", "mei\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "$,", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da ich hier in der Krippe weine;", "tokens": ["Da", "ich", "hier", "in", "der", "Krip\u00b7pe", "wei\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn dich bewegt mein Liebesschmerz,", "tokens": ["Wenn", "dich", "be\u00b7wegt", "mein", "Lie\u00b7bes\u00b7schmerz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So \u00f6ffne mir geschwind dein Herz!", "tokens": ["So", "\u00f6ff\u00b7ne", "mir", "ge\u00b7schwind", "dein", "Herz", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Du bist es, Seele, die ich meine,", "tokens": ["Du", "bist", "es", ",", "See\u00b7le", ",", "die", "ich", "mei\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "$,", "NN", "$,", "PRELS", "PPE