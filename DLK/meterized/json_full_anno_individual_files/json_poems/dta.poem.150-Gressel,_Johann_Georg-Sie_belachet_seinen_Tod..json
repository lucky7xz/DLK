{"dta.poem.150": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Sie belachet seinen Tod.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Da lieget nun dein Leib/ die Geister sind entwichen/", "tokens": ["Da", "lie\u00b7get", "nun", "dein", "Leib", "/", "die", "Geis\u00b7ter", "sind", "ent\u00b7wi\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PPOSAT", "NN", "$(", "ART", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Liebe hat dir jetzt den Untergang gebracht/", "tokens": ["Die", "Lie\u00b7be", "hat", "dir", "jetzt", "den", "Un\u00b7ter\u00b7gang", "ge\u00b7bracht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADV", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der Athen ist hinweg/ und in der Lufft verstrichen/", "tokens": ["Der", "A\u00b7then", "ist", "hin\u00b7weg", "/", "und", "in", "der", "Lufft", "ver\u00b7stri\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VAFIN", "ADV", "$(", "KON", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Es k\u00fc\u00dft dein geiler Mund die Geister jener Nacht.", "tokens": ["Es", "k\u00fc\u00dft", "dein", "gei\u00b7ler", "Mund", "die", "Geis\u00b7ter", "je\u00b7ner", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "ART", "NN", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Mein Hertze ist nun frey von deinen vielen qu\u00e4hlen/", "tokens": ["Mein", "Hert\u00b7ze", "ist", "nun", "frey", "von", "dei\u00b7nen", "vie\u00b7len", "qu\u00e4h\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VVFIN", "VAFIN", "ADV", "ADJD", "APPR", "PPOSAT", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Dein Seuffzen f\u00fcllet mir die Ohren jetzt nicht mehr/", "tokens": ["Dein", "Seuff\u00b7zen", "f\u00fcl\u00b7let", "mir", "die", "Oh\u00b7ren", "jetzt", "nicht", "mehr", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ART", "NN", "ADV", "PTKNEG", "ADV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Vergn\u00fcgung wohner nun in meiner keuschen Seelen/", "tokens": ["Ver\u00b7gn\u00fc\u00b7gung", "woh\u00b7ner", "nun", "in", "mei\u00b7ner", "keu\u00b7schen", "See\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ADV", "APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Weil ich das Klag-Geschrey und Fluchen nicht mehr h\u00f6r.", "tokens": ["Weil", "ich", "das", "Klag\u00b7Ge\u00b7schrey", "und", "Flu\u00b7chen", "nicht", "mehr", "h\u00f6r", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "KON", "NN", "PTKNEG", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Der Donner wird dir schon durch deine Seele spielen/", "tokens": ["Der", "Don\u00b7ner", "wird", "dir", "schon", "durch", "dei\u00b7ne", "See\u00b7le", "spie\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Die ", "tokens": ["Die"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.11": {"text": "Du wirst da Pech und Blitz zu steten Schmertzen f\u00fchlen/", "tokens": ["Du", "wirst", "da", "Pech", "und", "Blitz", "zu", "ste\u00b7ten", "Schmert\u00b7zen", "f\u00fch\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "NE", "KON", "NN", "APPR", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Wo ihren Sitz gehabt die Liebes-Raserey.", "tokens": ["Wo", "ih\u00b7ren", "Sitz", "ge\u00b7habt", "die", "Lie\u00b7bes\u00b7Ra\u00b7se\u00b7rey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "VAPP", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Buhl mit ", "tokens": ["Buhl", "mit"], "token_info": ["word", "word"], "pos": ["NE", "APPR"], "meter": "-+", "measure": "iambic.single"}, "line.14": {"text": "Sie werden/ Ungl\u00fccks Kind/ dir mehr als grausam seyn.", "tokens": ["Sie", "wer\u00b7den", "/", "Un\u00b7gl\u00fccks", "Kind", "/", "dir", "mehr", "als", "grau\u00b7sam", "seyn", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$(", "ADJA", "NN", "$(", "PPER", "PIAT", "KOKOM", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Der schwartze ", "tokens": ["Der", "schwart\u00b7ze"], "token_info": ["word", "word"], "pos": ["ART", "ADJA"], "meter": "-+-", "measure": "amphibrach.single"}, "line.16": {"text": "Mit ", "tokens": ["Mit"], "token_info": ["word"], "pos": ["APPR"], "meter": "-", "measure": "single.down"}, "line.17": {"text": "Verschmachte in dem Pfuhl/ da Pech und Schweiffel brennet/", "tokens": ["Ver\u00b7schmach\u00b7te", "in", "dem", "Pfuhl", "/", "da", "Pech", "und", "Schweif\u00b7fel", "bren\u00b7net", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$(", "KOUS", "NN", "KON", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Und sey dem ", "tokens": ["Und", "sey", "dem"], "token_info": ["word", "word", "word"], "pos": ["KON", "VAFIN", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.19": {"text": "Dein Unsinn hat dich offt demselben gleich genennet;", "tokens": ["Dein", "Un\u00b7sinn", "hat", "dich", "offt", "dem\u00b7sel\u00b7ben", "gleich", "ge\u00b7nen\u00b7net", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "ADV", "PDAT", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Nun hast du deinen Sitz in dem begehrten Reich.", "tokens": ["Nun", "hast", "du", "dei\u00b7nen", "Sitz", "in", "dem", "be\u00b7gehr\u00b7ten", "Reich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}