{"dta.poem.2134": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Vergn\u00fcgen 1. des Geruchs .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mein, von dem Balsamduft der Bl\u00fcte,", "tokens": ["Mein", ",", "von", "dem", "Bal\u00b7sam\u00b7duft", "der", "Bl\u00fc\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "$,", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Durch den Geruch, erquickt Gem\u00fcthe", "tokens": ["Durch", "den", "Ge\u00b7ruch", ",", "er\u00b7quickt", "Ge\u00b7m\u00fc\u00b7the"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "VVPP", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Erw\u00e4gt mit Lust und mit Bedacht,", "tokens": ["Er\u00b7w\u00e4gt", "mit", "Lust", "und", "mit", "Be\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Weisheit, sammt der Lieb und Macht", "tokens": ["Die", "Weis\u00b7heit", ",", "sammt", "der", "Lieb", "und", "Macht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "VVFIN", "ART", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Des Wesens, das, uns zu vergn\u00fcgen,", "tokens": ["Des", "We\u00b7sens", ",", "das", ",", "uns", "zu", "ver\u00b7gn\u00fc\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PDS", "$,", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der K\u00f6rper Theile so gelenkt,", "tokens": ["Der", "K\u00f6r\u00b7per", "Thei\u00b7le", "so", "ge\u00b7lenkt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Da\u00df sie sich unbegreiflich f\u00fcgen,", "tokens": ["Da\u00df", "sie", "sich", "un\u00b7be\u00b7greif\u00b7lich", "f\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und in uns eine Kraft gesenkt,", "tokens": ["Und", "in", "uns", "ei\u00b7ne", "Kraft", "ge\u00b7senkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Die S\u00fc\u00dfigkeiten zu genie\u00dfen,", "tokens": ["Die", "S\u00fc\u00b7\u00dfig\u00b7kei\u00b7ten", "zu", "ge\u00b7nie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Die aus den Blum- und Bl\u00fcten flie\u00dfen,", "tokens": ["Die", "aus", "den", "Blum", "und", "Bl\u00fc\u00b7ten", "flie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "TRUNC", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Auch uns des Riechens Werkzeug schenkt,", "tokens": ["Auch", "uns", "des", "Rie\u00b7chens", "Werk\u00b7zeug", "schenkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Und den man preiset, wenn man nur,", "tokens": ["Und", "den", "man", "prei\u00b7set", ",", "wenn", "man", "nur", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PIS", "VVFIN", "$,", "KOUS", "PIS", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "In Wundern seiner Kreatur,", "tokens": ["In", "Wun\u00b7dern", "sei\u00b7ner", "Kre\u00b7a\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "An ihren gro\u00dfen Meister denkt.", "tokens": ["An", "ih\u00b7ren", "gro\u00b7\u00dfen", "Meis\u00b7ter", "denkt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}