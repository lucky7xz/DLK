{"dta.poem.19079": {"metadata": {"author": {"name": "Zinzendorf, Nicolaus Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "LxI.  Auf seines Bruders Friedrich Chri-  \n stians zweyte Ehe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20688-6", "language": ["de:0.99"], "booktitle": "Zinzendorf, Nicolaus Ludwig von: Teutscher Gedichte Erster Theil. Herrnhuth, 1735."}, "poem": {"stanza.1": {"line.1": {"text": "Die alte Burg zu Wien umgab ein grauer Hayn,", "tokens": ["Die", "al\u00b7te", "Burg", "zu", "Wi\u00b7en", "um\u00b7gab", "ein", "grau\u00b7er", "Hayn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NE", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Zw\u00f6lf ", "tokens": ["Zw\u00f6lf"], "token_info": ["word"], "pos": ["CARD"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "J\u00fcngst giengen etliche vor grossem Alter ein,", "tokens": ["J\u00fcngst", "gien\u00b7gen", "et\u00b7li\u00b7che", "vor", "gros\u00b7sem", "Al\u00b7ter", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "APPR", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die von dem jungen Busch schon \u00fcberdecket waren.", "tokens": ["Die", "von", "dem", "jun\u00b7gen", "Busch", "schon", "\u00fc\u00b7berd\u00b7e\u00b7cket", "wa\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "ADJA", "NN", "ADV", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der Stamm von Zinzendorf, Einer von den Zw\u00f6lffen. aus welchem Ehren-\nhold, Der erste\nbekannte Dynasta des Zinzendorffischen Hauses, nach welchen\n22. Dynast\u00e4 in absteigender Linie gezehlet worden.", "tokens": ["Der", "Stamm", "von", "Zin\u00b7zen\u00b7dorf", ",", "Ei\u00b7ner", "von", "den", "Zw\u00f6lf\u00b7fen", ".", "aus", "wel\u00b7chem", "Eh\u00b7ren", "hold", ",", "Der", "ers\u00b7te", "be\u00b7kann\u00b7te", "Dy\u00b7nas\u00b7ta", "des", "Zin\u00b7zen\u00b7dorf\u00b7fi\u00b7schen", "Hau\u00b7ses", ",", "nach", "wel\u00b7chen", "22.", "Dy\u00b7nas\u00b7t\u00e4", "in", "ab\u00b7stei\u00b7gen\u00b7der", "Li\u00b7nie", "ge\u00b7zeh\u00b7let", "wor\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "ordinal", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "$,", "PIS", "APPR", "ART", "NN", "$.", "APPR", "PWAT", "TRUNC", "ADJD", "$,", "ART", "ADJA", "ADJA", "NN", "ART", "ADJA", "NN", "$,", "APPR", "PWAT", "ADJA", "NN", "APPR", "ADJA", "NN", "VVPP", "VAPP", "$."], "meter": "-+-+--+-+-+--+-+-+-+--+-+-+-+--+-+--+-+-+-+--+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}, "line.6": {"text": "Der hochber\u00fchmte Herr, so manchen Ast getrieben,", "tokens": ["Der", "hoch\u00b7be\u00b7r\u00fchm\u00b7te", "Herr", ",", "so", "man\u00b7chen", "Ast", "ge\u00b7trie\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADV", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "So dem Ertz-Hertzogthum belebtes Laub gezollt,", "tokens": ["So", "dem", "Ertz\u00b7Hert\u00b7zogt\u00b7hum", "be\u00b7leb\u00b7tes", "Laub", "ge\u00b7zollt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Der wurde, bi\u00df auf Zwey, vom Alter aufgerieben.", "tokens": ["Der", "wur\u00b7de", ",", "bi\u00df", "auf", "Zwey", ",", "vom", "Al\u00b7ter", "auf\u00b7ge\u00b7rie\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "$,", "KOUS", "APPR", "CARD", "$,", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Der ", "tokens": ["Der"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.10": {"text": "Und \u00fcberwuchs daselbst die allerh\u00f6chsten Eichen,", "tokens": ["Und", "\u00fc\u00b7ber\u00b7wuchs", "da\u00b7selbst", "die", "al\u00b7ler\u00b7h\u00f6chs\u00b7ten", "Ei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PAV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Von dannen gieng sein Trieb in zweyen Sprossen aus,", "tokens": ["Von", "dan\u00b7nen", "gieng", "sein", "Trieb", "in", "zwe\u00b7yen", "Spros\u00b7sen", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VVFIN", "PPOSAT", "NN", "APPR", "VVFIN", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Die bi\u00df ins M\u00e4hren-Land ", "tokens": ["Die", "bi\u00df", "ins", "M\u00e4h\u00b7ren\u00b7Land"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADV", "APPRART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "Der ", "tokens": ["Der"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.14": {"text": "Und auf der ", "tokens": ["Und", "auf", "der"], "token_info": ["word", "word", "word"], "pos": ["KON", "APPR", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.15": {"text": "Er war zu seiner Zeit ein Schatten-reicher Baum,", "tokens": ["Er", "war", "zu", "sei\u00b7ner", "Zeit", "ein", "Schat\u00b7ten\u00b7rei\u00b7cher", "Baum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPOSAT", "NN", "ART", "NN", "NE", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Die Reiser deckten drey ", "tokens": ["Die", "Rei\u00b7ser", "deck\u00b7ten", "drey"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "CARD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.17": {"text": "Zwey Aeste ", "tokens": ["Zwey", "A\u00b7es\u00b7te"], "token_info": ["word", "word"], "pos": ["CARD", "NN"], "meter": "-+--", "measure": "dactylic.init"}, "line.18": {"text": "Und dieser Erde Herr ", "tokens": ["Und", "die\u00b7ser", "Er\u00b7de", "Herr"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PDAT", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.19": {"text": "Um an demselben Ort, wo seine Raute bl\u00fcht,", "tokens": ["Um", "an", "dem\u00b7sel\u00b7ben", "Ort", ",", "wo", "sei\u00b7ne", "Rau\u00b7te", "bl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "APPR", "PDAT", "NN", "$,", "PWAV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Jhr schwebend es Gewicht mit Macht zu unterst\u00fctzen.", "tokens": ["Ihr", "schwe\u00b7bend", "es", "Ge\u00b7wicht", "mit", "Macht", "zu", "un\u00b7ter\u00b7st\u00fct\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "PPER", "NN", "APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Der ", "tokens": ["Der"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.22": {"text": "Er machte seinem Plan ein pr\u00e4chtiges Gesichte,", "tokens": ["Er", "mach\u00b7te", "sei\u00b7nem", "Plan", "ein", "pr\u00e4ch\u00b7ti\u00b7ges", "Ge\u00b7sich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Sein ", "tokens": ["Sein"], "token_info": ["word"], "pos": ["PPOSAT"], "meter": "-", "measure": "single.down"}, "line.24": {"text": "Doch, diesen Ast zerbrach sein eigenes Gewichte.", "tokens": ["Doch", ",", "die\u00b7sen", "Ast", "zer\u00b7brach", "sein", "ei\u00b7ge\u00b7nes", "Ge\u00b7wich\u00b7te", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PDAT", "NN", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Der ", "tokens": ["Der"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.26": {"text": "Sein wohlbelebtes Holtz gewann viel neue Sprossen,", "tokens": ["Sein", "wohl\u00b7be\u00b7leb\u00b7tes", "Holtz", "ge\u00b7wann", "viel", "neu\u00b7e", "Spros\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Und barg sich unverhoft ", "tokens": ["Und", "barg", "sich", "un\u00b7ver\u00b7hoft"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.28": {"text": "Nachdem sich \u00fcber ihm ", "tokens": ["Nach\u00b7dem", "sich", "\u00fc\u00b7ber", "ihm"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PRF", "APPR", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.29": {"text": "Der ", "tokens": ["Der"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.30": {"text": "Das ", "tokens": ["Das"], "token_info": ["word"], "pos": ["PDS"], "meter": "-", "measure": "single.down"}, "line.31": {"text": "Allein, er hatte da die Augen kaum erg\u00f6tzt,", "tokens": ["Al\u00b7lein", ",", "er", "hat\u00b7te", "da", "die", "Au\u00b7gen", "kaum", "er\u00b7g\u00f6tzt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PPER", "VAFIN", "ADV", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "So muste man ihn schon aus dem Gesicht verlieren.", "tokens": ["So", "mus\u00b7te", "man", "ihn", "schon", "aus", "dem", "Ge\u00b7sicht", "ver\u00b7lie\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIS", "PPER", "ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+---+-+-+-+-", "measure": "dactylic.init"}, "line.33": {"text": "Zwey Reiser waren noch die ausgeschlagne Bluth,", "tokens": ["Zwey", "Rei\u00b7ser", "wa\u00b7ren", "noch", "die", "aus\u00b7ge\u00b7schlag\u00b7ne", "Bluth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VAFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Des mit dem Seculo verdorrten Asts in Sachsen,", "tokens": ["Des", "mit", "dem", "Se\u00b7cu\u00b7lo", "ver\u00b7dorr\u00b7ten", "Asts", "in", "Sach\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NE", "ADJA", "NN", "APPR", "NE", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Auf deren Triebe nun der Eiche Schicksal ruht,", "tokens": ["Auf", "de\u00b7ren", "Trie\u00b7be", "nun", "der", "Ei\u00b7che", "Schick\u00b7sal", "ruht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "ADV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Dieselbigen sind auch zu ", "tokens": ["Die\u00b7sel\u00b7bi\u00b7gen", "sind", "auch", "zu"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ADV", "APPR"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.37": {"text": "Ast ", "tokens": ["Ast"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.38": {"text": "Er treibet liebliche und wohlbelaubte Reiser;", "tokens": ["Er", "trei\u00b7bet", "lieb\u00b7li\u00b7che", "und", "wohl\u00b7be\u00b7laub\u00b7te", "Rei\u00b7ser", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Er baut di\u00df in der Welt ber\u00fchmte Herren-Hau\u00df,", "tokens": ["Er", "baut", "di\u00df", "in", "der", "Welt", "be\u00b7r\u00fchm\u00b7te", "Her\u00b7ren\u00b7Hau\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PDS", "APPR", "ART", "NN", "VVFIN", "NE", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Und \u00fcberschattet noch viel andre hohe H\u00e4user. ", "tokens": ["Und", "\u00fc\u00b7bersc\u00b7hat\u00b7tet", "noch", "viel", "and\u00b7re", "ho\u00b7he", "H\u00e4u\u00b7ser", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PIAT", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Ich baue seine Pracht di\u00df ", "tokens": ["Ich", "bau\u00b7e", "sei\u00b7ne", "Pracht", "di\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "PDS"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.42": {"text": "Und wohl mit meiner Hand von dieser Art das ", "tokens": ["Und", "wohl", "mit", "mei\u00b7ner", "Hand", "von", "die\u00b7ser", "Art", "das"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "PPOSAT", "NN", "APPR", "PDAT", "NN", "ART"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.43": {"text": "Ich bau es diesen Tag, daran du Br\u00e4utgam bist,", "tokens": ["Ich", "bau", "es", "die\u00b7sen", "Tag", ",", "da\u00b7ran", "du", "Br\u00e4ut\u00b7gam", "bist", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PDAT", "NN", "$,", "PAV", "PPER", "NE", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Und weil ich ", "tokens": ["Und", "weil", "ich"], "token_info": ["word", "word", "word"], "pos": ["KON", "KOUS", "PPER"], "meter": "+--", "measure": "dactylic.init"}, "line.45": {"text": "Der Wahlspruch unsers Stamms von Graf Alberto ", "tokens": ["Der", "Wahl\u00b7spruch", "un\u00b7sers", "Stamms", "von", "Graf", "Al\u00b7ber\u00b7to"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "NN", "APPR", "NE", "NE"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.46": {"text": "Ist der: ", "tokens": ["Ist", "der", ":"], "token_info": ["word", "word", "punct"], "pos": ["VAFIN", "ART", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.47": {"text": "Ich zeih es der Natur: das Weichen wird uns schwer;", "tokens": ["Ich", "zeih", "es", "der", "Na\u00b7tur", ":", "das", "Wei\u00b7chen", "wird", "uns", "schwer", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "$.", "ART", "NN", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.48": {"text": "Vor einem aber ist mein Muth dahin gefallen.", "tokens": ["Vor", "ei\u00b7nem", "a\u00b7ber", "ist", "mein", "Muth", "da\u00b7hin", "ge\u00b7fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADV", "VAFIN", "PPOSAT", "NN", "PAV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.49": {"text": "Der JEsus, der einmal an einem Holtze hieng,", "tokens": ["Der", "Je\u00b7sus", ",", "der", "ein\u00b7mal", "an", "ei\u00b7nem", "Holt\u00b7ze", "hieng", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "$,", "PRELS", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.50": {"text": "Mit dem das Alterthum so schn\u00f6den Spott getrieben,", "tokens": ["Mit", "dem", "das", "Al\u00b7ter\u00b7thum", "so", "schn\u00f6\u00b7den", "Spott", "ge\u00b7trie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "ART", "NN", "ADV", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.51": {"text": "Zu dem nicht lange drauf der Erd-Krei\u00df \u00fcbergieng,", "tokens": ["Zu", "dem", "nicht", "lan\u00b7ge", "drauf", "der", "Erd\u00b7Krei\u00df", "\u00fc\u00b7berg\u00b7ieng", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PTKNEG", "ADV", "PAV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+