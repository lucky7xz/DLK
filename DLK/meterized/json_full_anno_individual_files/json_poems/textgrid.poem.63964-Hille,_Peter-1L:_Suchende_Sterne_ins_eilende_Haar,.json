, "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "So k\u00f6nnt ihr ja geben", "tokens": ["So", "k\u00f6nnt", "ihr", "ja", "ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Und selig sein und selig machen,", "tokens": ["Und", "se\u00b7lig", "sein", "und", "se\u00b7lig", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAINF", "KON", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und innig sein wie Kinderlachen", "tokens": ["Und", "in\u00b7nig", "sein", "wie", "Kin\u00b7der\u00b7la\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VAINF", "KOKOM", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und wie wir von Wundern leben.", "tokens": ["Und", "wie", "wir", "von", "Wun\u00b7dern", "le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "APPR", "NN", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.14": {"line.1": {"text": "Tuet frohe Liebesgaben", "tokens": ["Tu\u00b7et", "fro\u00b7he", "Lie\u00b7bes\u00b7ga\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Einer in des anderen Hand,", "tokens": ["Ei\u00b7ner", "in", "des", "an\u00b7de\u00b7ren", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Tuet ab das Geizgewand", "tokens": ["Tu\u00b7et", "ab", "das", "Geiz\u00b7ge\u00b7wand"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "AR