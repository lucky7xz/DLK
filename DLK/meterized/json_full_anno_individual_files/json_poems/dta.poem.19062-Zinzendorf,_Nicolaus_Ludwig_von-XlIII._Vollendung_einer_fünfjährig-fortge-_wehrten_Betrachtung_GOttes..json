{"dta.poem.19062": {"metadata": {"author": {"name": "Zinzendorf, Nicolaus Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "XlIII.  Vollendung einer f\u00fcnfj\u00e4hrig-fortge-  \n wehrten Betrachtung GOttes.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20688-6", "language": ["de:0.99"], "booktitle": "Zinzendorf, Nicolaus Ludwig von: Teutscher Gedichte Erster Theil. Herrnhuth, 1735."}, "poem": {"stanza.1": {"line.1": {"text": "Allgegenwart! ich mu\u00df gestehn,", "tokens": ["All\u00b7ge\u00b7gen\u00b7wart", "!", "ich", "mu\u00df", "ge\u00b7stehn", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PPER", "VMFIN", "VVINF", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Du unaussprechlich tiefe H\u00f6he", "tokens": ["Du", "un\u00b7aus\u00b7sprech\u00b7lich", "tie\u00b7fe", "H\u00f6\u00b7he"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Erf\u00fcllest, ohne dich zu sehn,", "tokens": ["Er\u00b7f\u00fcl\u00b7lest", ",", "oh\u00b7ne", "dich", "zu", "sehn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUI", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Doch alles, wo ich geh und stehe:", "tokens": ["Doch", "al\u00b7les", ",", "wo", "ich", "geh", "und", "ste\u00b7he", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "$,", "PWAV", "PPER", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die Spur von deinem Allmachts-Pfad,", "tokens": ["Die", "Spur", "von", "dei\u00b7nem", "All\u00b7machts\u00b7Pfad", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die ewiglich nicht auszugr\u00fcnden,", "tokens": ["Die", "e\u00b7wig\u00b7lich", "nicht", "aus\u00b7zu\u00b7gr\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Ist dennoch \u00fcberall zu finden,", "tokens": ["Ist", "den\u00b7noch", "\u00fc\u00b7be\u00b7rall", "zu", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "So weit man Raum zu dencken hat.", "tokens": ["So", "weit", "man", "Raum", "zu", "den\u00b7cken", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PIS", "NN", "PTKZU", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "So kan es ja nicht anders seyn,", "tokens": ["So", "kan", "es", "ja", "nicht", "an\u00b7ders", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "PTKNEG", "ADV", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Weil ich dich allerwegen mercke,", "tokens": ["Weil", "ich", "dich", "al\u00b7ler\u00b7we\u00b7gen", "mer\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "So geb ich mich mit Ernst darein,", "tokens": ["So", "geb", "ich", "mich", "mit", "Ernst", "da\u00b7rein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "NE", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Gr\u00f6sse deiner Macht und St\u00e4rcke,", "tokens": ["Die", "Gr\u00f6s\u00b7se", "dei\u00b7ner", "Macht", "und", "St\u00e4r\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die blendend helle Majest\u00e4t,", "tokens": ["Die", "blen\u00b7dend", "hel\u00b7le", "Ma\u00b7jes\u00b7t\u00e4t", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Vor der die finstre Tiefen weichen,", "tokens": ["Vor", "der", "die", "finst\u00b7re", "Tie\u00b7fen", "wei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Mit einem Liede zu erreichen,", "tokens": ["Mit", "ei\u00b7nem", "Lie\u00b7de", "zu", "er\u00b7rei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Das \u00fcber alle Lieder geht.", "tokens": ["Das", "\u00fc\u00b7ber", "al\u00b7le", "Lie\u00b7der", "geht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "All ein du unbeschriebner Mann,", "tokens": ["All", "ein", "du", "un\u00b7be\u00b7schrieb\u00b7ner", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ART", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wo fing ich meine Lobs-Gedancken", "tokens": ["Wo", "fing", "ich", "mei\u00b7ne", "Lobs\u00b7Ge\u00b7dan\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Den ersten Stein zu setzen an?", "tokens": ["Den", "ers\u00b7ten", "Stein", "zu", "set\u00b7zen", "an", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wohin versetzt ich ihre Schrancken?", "tokens": ["Wo\u00b7hin", "ver\u00b7setzt", "ich", "ih\u00b7re", "Schran\u00b7cken", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "In welchem Lebens-Jahre wird", "tokens": ["In", "wel\u00b7chem", "Le\u00b7bens\u00b7Jah\u00b7re", "wird"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PWAT", "NN", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Erst mein Verstand so aufgekl\u00e4ret,", "tokens": ["Erst", "mein", "Ver\u00b7stand", "so", "auf\u00b7ge\u00b7kl\u00e4\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Da\u00df er hinauf und nieder f\u00e4hret,", "tokens": ["Da\u00df", "er", "hin\u00b7auf", "und", "nie\u00b7der", "f\u00e4h\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKVZ", "KON", "PTKVZ", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und sich nicht \u00fcberall verirrt.", "tokens": ["Und", "sich", "nicht", "\u00fc\u00b7be\u00b7rall", "ver\u00b7irrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "PTKNEG", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Es spreche, du verborgner GOtt,", "tokens": ["Es", "spre\u00b7che", ",", "du", "ver\u00b7borg\u00b7ner", "Gott", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Mensch, was eigentlich dein Wesen!", "tokens": ["Ein", "Mensch", ",", "was", "ei\u00b7gent\u00b7lich", "dein", "We\u00b7sen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und werde nicht dabey zu Spott", "tokens": ["Und", "wer\u00b7de", "nicht", "da\u00b7bey", "zu", "Spott"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PTKNEG", "PAV", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vor allen, die den Ausspruch lesen;", "tokens": ["Vor", "al\u00b7len", ",", "die", "den", "Aus\u00b7spruch", "le\u00b7sen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "$,", "PRELS", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Er wird, mit ausgesuchter Art,", "tokens": ["Er", "wird", ",", "mit", "aus\u00b7ge\u00b7such\u00b7ter", "Art", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Sprache also f\u00fchren m\u00fcssen,", "tokens": ["Die", "Spra\u00b7che", "al\u00b7so", "f\u00fch\u00b7ren", "m\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVINF", "VMINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Da\u00df er und alle nichts mehr wissen,", "tokens": ["Da\u00df", "er", "und", "al\u00b7le", "nichts", "mehr", "wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "KON", "PIAT", "PIS", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Als was du l\u00e4ngst geoffenbahrt.", "tokens": ["Als", "was", "du", "l\u00e4ngst", "ge\u00b7of\u00b7fen\u00b7bahrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PWS", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Wie wagte sich die Zung hinein", "tokens": ["Wie", "wag\u00b7te", "sich", "die", "Zung", "hin\u00b7ein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PRF", "ART", "NN", "APZR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In deine tiefe Eigenschaften?", "tokens": ["In", "dei\u00b7ne", "tie\u00b7fe", "Ei\u00b7gen\u00b7schaf\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die sonderlich und insgemein", "tokens": ["Die", "son\u00b7der\u00b7lich", "und", "ins\u00b7ge\u00b7mein"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJD", "KON", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Genau an deinem Wesen haften?", "tokens": ["Ge\u00b7nau", "an", "dei\u00b7nem", "We\u00b7sen", "haf\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und zu des Nahmens Wunder-H\u00f6hn,", "tokens": ["Und", "zu", "des", "Nah\u00b7mens", "Wun\u00b7der\u00b7H\u00f6hn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der sich zu nennen nicht beliebet,", "tokens": ["Der", "sich", "zu", "nen\u00b7nen", "nicht", "be\u00b7lie\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "PTKZU", "VVINF", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Sich auch nur zu erfahren giebet,", "tokens": ["Sich", "auch", "nur", "zu", "er\u00b7fah\u00b7ren", "gie\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "ADV", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Wo Aug und Sinne stille stehn.", "tokens": ["Wo", "Aug", "und", "Sin\u00b7ne", "stil\u00b7le", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Wer f\u00fchret mich zu deiner Quell,", "tokens": ["Wer", "f\u00fch\u00b7ret", "mich", "zu", "dei\u00b7ner", "Quell", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Unendlichkeit! des Geists erstaunen!", "tokens": ["Un\u00b7end\u00b7lich\u00b7keit", "!", "des", "Geists", "er\u00b7stau\u00b7nen", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wo find ich eine freye Stell", "tokens": ["Wo", "find", "ich", "ei\u00b7ne", "frey\u00b7e", "Stell"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von deinen Wundern zu posaunen,", "tokens": ["Von", "dei\u00b7nen", "Wun\u00b7dern", "zu", "po\u00b7sau\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ich warnte alle Creatur", "tokens": ["Ich", "warn\u00b7te", "al\u00b7le", "Crea\u00b7tur"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIAT", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "Vom F\u00fcrsten an der reinen Geister,", "tokens": ["Vom", "F\u00fcrs\u00b7ten", "an", "der", "rei\u00b7nen", "Geis\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Bi\u00df zu der Weisen Obermeister,", "tokens": ["Bi\u00df", "zu", "der", "Wei\u00b7sen", "O\u00b7ber\u00b7meis\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Vor deiner f\u00fcrchterlichen Spur.", "tokens": ["Vor", "dei\u00b7ner", "f\u00fcrch\u00b7ter\u00b7li\u00b7chen", "Spur", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Ich lasse dich, du bist zu hoch,", "tokens": ["Ich", "las\u00b7se", "dich", ",", "du", "bist", "zu", "hoch", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PPER", "VAFIN", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zu tief, o GOtt! zu gro\u00df und lichte,", "tokens": ["Zu", "tief", ",", "o", "Gott", "!", "zu", "gro\u00df", "und", "lich\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "$,", "FM", "NN", "$.", "PTKA", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Vor einen Geist im Lebens-Joch,", "tokens": ["Vor", "ei\u00b7nen", "Geist", "im", "Le\u00b7bens\u00b7Joch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vor ein umc\u00f6rpertes Gesichte:", "tokens": ["Vor", "ein", "um\u00b7c\u00f6r\u00b7per\u00b7tes", "Ge\u00b7sich\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wie kam das Schaffen dir im Sinn?", "tokens": ["Wie", "kam", "das", "Schaf\u00b7fen", "dir", "im", "Sinn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Verfehlt ein F\u00fcrst der Creaturen", "tokens": ["Ver\u00b7fehlt", "ein", "F\u00fcrst", "der", "Crea\u00b7tu\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Zu dir, dem Sch\u00f6pfer, Bahn und Spuren,", "tokens": ["Zu", "dir", ",", "dem", "Sch\u00f6p\u00b7fer", ",", "Bahn", "und", "Spu\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "ART", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Wo will die andre Sch\u00f6pfung hin.", "tokens": ["Wo", "will", "die", "and\u00b7re", "Sch\u00f6p\u00b7fung", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "G\u00f6ttliche Antwort.", "tokens": ["G\u00f6tt\u00b7li\u00b7che", "Ant\u00b7wort", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "H\u00f6r auf zu suchen, was so fern,", "tokens": ["H\u00f6r", "auf", "zu", "su\u00b7chen", ",", "was", "so", "fern", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PTKZU", "VVINF", "$,", "PRELS", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "H\u00f6r auf zu forschen, was dich fliehet;", "tokens": ["H\u00f6r", "auf", "zu", "for\u00b7schen", ",", "was", "dich", "flie\u00b7het", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PTKZU", "VVINF", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Du hast den ausgemachten Kern,", "tokens": ["Du", "hast", "den", "aus\u00b7ge\u00b7mach\u00b7ten", "Kern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sey nicht ums Aussen-Werck bem\u00fchet", "tokens": ["Sey", "nicht", "ums", "Aus\u00b7sen\u00b7\u00b7Werck", "be\u00b7m\u00fc\u00b7het"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PTKNEG", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"