{"dta.poem.23389": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Philister \u00fcber dir, Simson!", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192634", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die einst mich froh willkommen hie\u00dfen,", "tokens": ["Die", "einst", "mich", "froh", "will\u00b7kom\u00b7men", "hie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PPER", "ADJD", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die sehn mich ernst und schweigend an:", "tokens": ["Die", "sehn", "mich", "ernst", "und", "schwei\u00b7gend", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Was mag sie wohl an mir verdrie\u00dfen?", "tokens": ["Was", "mag", "sie", "wohl", "an", "mir", "ver\u00b7drie\u00b7\u00dfen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "ADV", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Bin ich nicht mehr derselbe Mann?", "tokens": ["Bin", "ich", "nicht", "mehr", "der\u00b7sel\u00b7be", "Mann", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "ADV", "PDAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Bin ich im Hassen und im Lieben,", "tokens": ["Bin", "ich", "im", "Has\u00b7sen", "und", "im", "Lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPRART", "NN", "KON", "APPRART", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Bin ich dem Vaterlande nicht,", "tokens": ["Bin", "ich", "dem", "Va\u00b7ter\u00b7lan\u00b7de", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bin ich nicht Allem treu geblieben,", "tokens": ["Bin", "ich", "nicht", "Al\u00b7lem", "treu", "ge\u00b7blie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "PIS", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Was nur f\u00fcr Recht und Freiheit spricht?", "tokens": ["Was", "nur", "f\u00fcr", "Recht", "und", "Frei\u00b7heit", "spricht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Still, still! ich kenne mein Verbrechen:", "tokens": ["Still", ",", "still", "!", "ich", "ken\u00b7ne", "mein", "Ver\u00b7bre\u00b7chen", ":"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "$.", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "H\u00e4tt' ich behalten nur f\u00fcr mich", "tokens": ["H\u00e4tt'", "ich", "be\u00b7hal\u00b7ten", "nur", "f\u00fcr", "mich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "VVFIN", "ADV", "APPR", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Was ich gewagt frei auszusprechen \u2014", "tokens": ["Was", "ich", "ge\u00b7wagt", "frei", "aus\u00b7zu\u00b7spre\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "ADJD", "VVIZU", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sie gr\u00fc\u00dften auch noch heute mich.", "tokens": ["Sie", "gr\u00fc\u00df\u00b7ten", "auch", "noch", "heu\u00b7te", "mich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ADV", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}