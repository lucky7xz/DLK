{"textgrid.poem.31720": {"metadata": {"author": {"name": "Weerth, Georg", "birth": "N.A.", "death": "N.A."}, "title": "17.", "genre": "verse", "period": "N.A.", "pub_year": 1839, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und als ich einst am fr\u00fchen Tag", "tokens": ["Und", "als", "ich", "einst", "am", "fr\u00fc\u00b7hen", "Tag"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "ADV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den gro\u00dfen Henkelkrug zerbrach:", "tokens": ["Den", "gro\u00b7\u00dfen", "Hen\u00b7kel\u00b7krug", "zer\u00b7brach", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da ist der Wein geflossen", "tokens": ["Da", "ist", "der", "Wein", "ge\u00b7flos\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wohl in die duftigen Sprossen.", "tokens": ["Wohl", "in", "die", "duf\u00b7ti\u00b7gen", "Spros\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Da tranken die Blumen gro\u00df und klein", "tokens": ["Da", "tran\u00b7ken", "die", "Blu\u00b7men", "gro\u00df", "und", "klein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJD", "KON", "ADJD"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Von meinem k\u00fchlen Klosterwein.", "tokens": ["Von", "mei\u00b7nem", "k\u00fch\u00b7len", "Klos\u00b7ter\u00b7wein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Da kamen Schmetterlinge bunt", "tokens": ["Da", "ka\u00b7men", "Schmet\u00b7ter\u00b7lin\u00b7ge", "bunt"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Her\u00fcber aus dem Wiesengrund.", "tokens": ["Her\u00b7\u00fc\u00b7ber", "aus", "dem", "Wie\u00b7sen\u00b7grund", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da kamen lust'ge Fliegen,", "tokens": ["Da", "ka\u00b7men", "lust'\u00b7ge", "Flie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die t\u00e4ten im Kreise liegen,", "tokens": ["Die", "t\u00e4\u00b7ten", "im", "Krei\u00b7se", "lie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Im Kreise wohl bis zum Abendschein", "tokens": ["Im", "Krei\u00b7se", "wohl", "bis", "zum", "A\u00b7bend\u00b7schein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADV", "ADV", "APPRART", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Bei meinem k\u00fchlen Klosterwein.", "tokens": ["Bei", "mei\u00b7nem", "k\u00fch\u00b7len", "Klos\u00b7ter\u00b7wein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Da wurde mancher Trunk getan,", "tokens": ["Da", "wur\u00b7de", "man\u00b7cher", "Trunk", "ge\u00b7tan", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da hub der Maienk\u00e4fer an:", "tokens": ["Da", "hub", "der", "Mai\u00b7en\u00b7k\u00e4\u00b7fer", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u00bbmir ist so wohl zumute,", "tokens": ["\u00bb", "mir", "ist", "so", "wohl", "zu\u00b7mu\u00b7te", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Als ob ich auf Lilien ruhte,", "tokens": ["Als", "ob", "ich", "auf", "Li\u00b7li\u00b7en", "ruh\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "APPR", "NE", "VVFIN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Als bl\u00fchte sch\u00f6ner die Seele mein", "tokens": ["Als", "bl\u00fch\u00b7te", "sch\u00f6\u00b7ner", "die", "See\u00b7le", "mein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "VVFIN", "ADJD", "ART", "NN", "PPOSAT"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Von diesem k\u00fchlen Klosterwein.\u00ab", "tokens": ["Von", "die\u00b7sem", "k\u00fch\u00b7len", "Klos\u00b7ter\u00b7wein", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Da sprach die Bienenk\u00f6nigin:", "tokens": ["Da", "sprach", "die", "Bie\u00b7nen\u00b7k\u00f6\u00b7ni\u00b7gin", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u00bbwie ist so lind mein hoher Sinn!", "tokens": ["\u00bb", "wie", "ist", "so", "lind", "mein", "ho\u00b7her", "Sinn", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOKOM", "VAFIN", "ADV", "ADJD", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Komm her, da\u00df ich dich dr\u00fccke,", "tokens": ["Komm", "her", ",", "da\u00df", "ich", "dich", "dr\u00fc\u00b7cke", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "KOUS", "PPER", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Komm her, verliebte M\u00fccke,", "tokens": ["Komm", "her", ",", "ver\u00b7lieb\u00b7te", "M\u00fc\u00b7cke", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "VVFIN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Komm her, wir tanzen den Ringelreihn", "tokens": ["Komm", "her", ",", "wir", "tan\u00b7zen", "den", "Rin\u00b7gel\u00b7reihn"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PTKVZ", "$,", "PPER", "VVFIN", "ART", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Wohl um den k\u00fchlen Klosterwein!\u00ab", "tokens": ["Wohl", "um", "den", "k\u00fch\u00b7len", "Klos\u00b7ter\u00b7wein", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Da war bes\u00e4uselt gar und ganz", "tokens": ["Da", "war", "be\u00b7s\u00e4u\u00b7selt", "gar", "und", "ganz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADJD", "ADV", "KON", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der jugendliche Schwalbenschwanz,", "tokens": ["Der", "ju\u00b7gend\u00b7li\u00b7che", "Schwal\u00b7ben\u00b7schwanz", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er strich wohl durch die Moose:", "tokens": ["Er", "strich", "wohl", "durch", "die", "Moo\u00b7se", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "\u00bbzieht aus mir Mantel und Hose,", "tokens": ["\u00bb", "zieht", "aus", "mir", "Man\u00b7tel", "und", "Ho\u00b7se", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "APPR", "PPER", "NN", "KON", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.5": {"text": "Ich habe getrunken zu gro\u00dfer Pein", "tokens": ["Ich", "ha\u00b7be", "ge\u00b7trun\u00b7ken", "zu", "gro\u00b7\u00dfer", "Pein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "VVPP", "PTKZU", "ADJA", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Von diesem k\u00fchlen Klosterwein!\u00ab", "tokens": ["Von", "die\u00b7sem", "k\u00fch\u00b7len", "Klos\u00b7ter\u00b7wein", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Die Bremse war schon hoch betagt,", "tokens": ["Die", "Brem\u00b7se", "war", "schon", "hoch", "be\u00b7tagt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie hat kein einzig Wort gesagt,", "tokens": ["Sie", "hat", "kein", "ein\u00b7zig", "Wort", "ge\u00b7sagt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "ADJD", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie klagt' um ihre Tugend", "tokens": ["Sie", "klagt'", "um", "ih\u00b7re", "Tu\u00b7gend"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und die verlorene Jugend.", "tokens": ["Und", "die", "ver\u00b7lo\u00b7re\u00b7ne", "Ju\u00b7gend", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Sie hat sich ers\u00e4ufet so stumm, allein", "tokens": ["Sie", "hat", "sich", "er\u00b7s\u00e4u\u00b7fet", "so", "stumm", ",", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["PPER", "VAFIN", "PRF", "VVFIN", "ADV", "ADJD", "$,", "ADV"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Tief in dem k\u00fchlen Klosterwein!", "tokens": ["Tief", "in", "dem", "k\u00fch\u00b7len", "Klos\u00b7ter\u00b7wein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Und stille ward es rings umher,", "tokens": ["Und", "stil\u00b7le", "ward", "es", "rings", "um\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "VAFIN", "PPER", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Kein Jubeln und kein Singen mehr.", "tokens": ["Kein", "Ju\u00b7beln", "und", "kein", "Sin\u00b7gen", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "PIAT", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es kam die Nacht geschritten,", "tokens": ["Es", "kam", "die", "Nacht", "ge\u00b7schrit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Bremse hat ausgelitten.", "tokens": ["Die", "Brem\u00b7se", "hat", "aus\u00b7ge\u00b7lit\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Sie starb und rief in das Tal hinein:", "tokens": ["Sie", "starb", "und", "rief", "in", "das", "Tal", "hin\u00b7ein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "\u00bbleb wohl, du k\u00fchler Klosterwein!\u00ab", "tokens": ["\u00bb", "leb", "wohl", ",", "du", "k\u00fch\u00b7ler", "Klos\u00b7ter\u00b7wein", "!", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVIMP", "ADV", "$,", "PPER", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Und als ich einst am fr\u00fchen Tag", "tokens": ["Und", "als", "ich", "einst", "am", "fr\u00fc\u00b7hen", "Tag"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "ADV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den gro\u00dfen Henkelkrug zerbrach:", "tokens": ["Den", "gro\u00b7\u00dfen", "Hen\u00b7kel\u00b7kru