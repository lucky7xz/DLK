{"dta.poem.7098": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Entwurff einiger Betrachtungen,  \n \u00fcber den  \n  Zustand der Seeligen.  \n Beym Anfang des 1727sten Jahres.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was werden sich f\u00fcr Freuden-Str\u00f6m\u2019 ergiessen?", "tokens": ["Was", "wer\u00b7den", "sich", "f\u00fcr", "Freu\u00b7den\u00b7Str\u00f6m'", "er\u00b7gies\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PRF", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In welchem Anmuths-Meer von seel\u2019ger S\u00fc\u00dfigkeit", "tokens": ["In", "wel\u00b7chem", "An\u00b7muths\u00b7Meer", "von", "seel'\u00b7ger", "S\u00fc\u00b7\u00dfig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PWAT", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wird der Gerechten Seele schwimmen!", "tokens": ["Wird", "der", "Ge\u00b7rech\u00b7ten", "See\u00b7le", "schwim\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "NN", "VVINF", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Von Anmuth gantz entz\u00fcckt,", "tokens": ["Von", "An\u00b7muth", "gantz", "ent\u00b7z\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Mit ew\u2019ger Klarheit ausgeschm\u00fcckt,", "tokens": ["Mit", "ew'\u00b7ger", "Klar\u00b7heit", "aus\u00b7ge\u00b7schm\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wird sie, von Lieb\u2019 entbrannt, in seel\u2019ger Sehnsucht glimmen.", "tokens": ["Wird", "sie", ",", "von", "Lieb'", "ent\u00b7brannt", ",", "in", "seel'\u00b7ger", "Sehn\u00b7sucht", "glim\u00b7men", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "APPR", "NN", "VVPP", "$,", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wie wird ihr br\u00fcnstig Lob, dem grossen ALL zu Ehren,", "tokens": ["Wie", "wird", "ihr", "br\u00fcns\u00b7tig", "Lob", ",", "dem", "gros\u00b7sen", "AlL", "zu", "Eh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADJD", "NN", "$,", "ART", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "In ihrer Lust sich ewig, ewig mehren,", "tokens": ["In", "ih\u00b7rer", "Lust", "sich", "e\u00b7wig", ",", "e\u00b7wig", "meh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PRF", "ADJD", "$,", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Wenn jede fr\u00f6hlich selbst am Sch\u00f6pfer sp\u00fcret,", "tokens": ["Wenn", "je\u00b7de", "fr\u00f6h\u00b7lich", "selbst", "am", "Sch\u00f6p\u00b7fer", "sp\u00fc\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "ADJD", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Wie ihre Wonne selbst sein G\u00f6ttlich Wesen r\u00fchret,", "tokens": ["Wie", "ih\u00b7re", "Won\u00b7ne", "selbst", "sein", "G\u00f6tt\u00b7lich", "We\u00b7sen", "r\u00fch\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "ADV", "PPOSAT", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Da\u00df Er durch Gnaden-reiche Triebe,", "tokens": ["Da\u00df", "Er", "durch", "Gna\u00b7den\u00b7rei\u00b7che", "Trie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Von einer wahren Vater-Liebe,", "tokens": ["Von", "ei\u00b7ner", "wah\u00b7ren", "Va\u00b7ter\u00b7Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Aus ihren seeligen Vergn\u00fcgen,", "tokens": ["Aus", "ih\u00b7ren", "see\u00b7li\u00b7gen", "Ver\u00b7gn\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Sich Selber ein Vergn\u00fcgen macht.", "tokens": ["Sich", "Sel\u00b7ber", "ein", "Ver\u00b7gn\u00fc\u00b7gen", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Und hierinn wird gewi\u00df", "tokens": ["Und", "hie\u00b7rinn", "wird", "ge\u00b7wi\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.16": {"text": "Ein grosses Theil von ihrer Freude liegen.", "tokens": ["Ein", "gros\u00b7ses", "Theil", "von", "ih\u00b7rer", "Freu\u00b7de", "lie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}