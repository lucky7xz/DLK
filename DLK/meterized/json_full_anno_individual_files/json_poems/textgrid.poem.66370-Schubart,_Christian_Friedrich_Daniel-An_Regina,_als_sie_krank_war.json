{"textgrid.poem.66370": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "An Regina, als sie krank war", "genre": "verse", "period": "N.A.", "pub_year": 1783, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dein Saitenspiel schweigt;", "tokens": ["Dein", "Sai\u00b7ten\u00b7spiel", "schweigt", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Nur beugt sich die Wehmuth dr\u00fcber,", "tokens": ["Nur", "beugt", "sich", "die", "Weh\u00b7muth", "dr\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "PAV", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und ihrer Wehklage Hauch", "tokens": ["Und", "ih\u00b7rer", "Weh\u00b7kla\u00b7ge", "Hauch"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Weckt all die seufzenden T\u00f6ne;", "tokens": ["Weckt", "all", "die", "seuf\u00b7zen\u00b7den", "T\u00f6\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.5": {"text": "Sie wimmern, wie Lispel im Todtenkranze.", "tokens": ["Sie", "wim\u00b7mern", ",", "wie", "Lis\u00b7pel", "im", "Tod\u00b7ten\u00b7kran\u00b7ze", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "NE", "APPRART", "NN", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}}, "stanza.2": {"line.1": {"text": "Du aber, ", "tokens": ["Du", "a\u00b7ber", ","], "token_info": ["word", "word", "punct"], "pos": ["PPER", "ADV", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "K\u00f6nnten Engel erkranken,", "tokens": ["K\u00f6nn\u00b7ten", "En\u00b7gel", "er\u00b7kran\u00b7ken", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Sie l\u00e4gen und duldeten so.", "tokens": ["Sie", "l\u00e4\u00b7gen", "und", "dul\u00b7de\u00b7ten", "so", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "ADV", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Was l\u00e4chelst du, ", "tokens": ["Was", "l\u00e4\u00b7chelst", "du", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "F\u00fchlst du die N\u00e4he des Himmels?", "tokens": ["F\u00fchlst", "du", "die", "N\u00e4\u00b7he", "des", "Him\u00b7mels", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ART", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.6": {"text": "Siehst du auf goldnem Gew\u00f6lke", "tokens": ["Siehst", "du", "auf", "gold\u00b7nem", "Ge\u00b7w\u00f6l\u00b7ke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.7": {"text": "Geister liegen, die dein harren?", "tokens": ["Geis\u00b7ter", "lie\u00b7gen", ",", "die", "dein", "har\u00b7ren", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "PRELS", "PPOSAT", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "\u00bbich sterbe gern\u00ab", "tokens": ["\u00bb", "ich", "ster\u00b7be", "gern", "\u00ab"], "token_info": ["punct", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "ADV", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "So sagst du dem weinenden Vater;", "tokens": ["So", "sagst", "du", "dem", "wei\u00b7nen\u00b7den", "Va\u00b7ter", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "\u00bbich sterbe gern\u00ab", "tokens": ["\u00bb", "ich", "ster\u00b7be", "gern", "\u00ab"], "token_info": ["punct", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "ADV", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "So der gesunkenen Mutter.", "tokens": ["So", "der", "ge\u00b7sun\u00b7ke\u00b7nen", "Mut\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "\u00bbwas hat dies Leben f\u00fcr mich?\u00ab", "tokens": ["\u00bb", "was", "hat", "dies", "Le\u00b7ben", "f\u00fcr", "mich", "?", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PWS", "VAFIN", "PDS", "NN", "APPR", "PPER", "$.", "$("], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.4": {"line.1": {"text": "Und doch sind dir erst", "tokens": ["Und", "doch", "sind", "dir", "erst"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ADV"], "meter": "----+", "measure": "unknown.measure.single"}, "line.2": {"text": "Siebzehn Fr\u00fchlinge verbl\u00fcht;", "tokens": ["Sieb\u00b7zehn", "Fr\u00fch\u00b7lin\u00b7ge", "ver\u00b7bl\u00fcht", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVPP", "$."], "meter": "+-++--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Doch schm\u00fcckt dich Sch\u00f6nheit und Unschuld,", "tokens": ["Doch", "schm\u00fcckt", "dich", "Sch\u00f6n\u00b7heit", "und", "Un\u00b7schuld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "NN", "KON", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und Gotteslieb' und Menschenhuld,", "tokens": ["Und", "Got\u00b7tes\u00b7lieb'", "und", "Men\u00b7schen\u00b7huld", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und des Herzgef\u00fchls Tiefe,", "tokens": ["Und", "des", "Herz\u00b7ge\u00b7f\u00fchls", "Tie\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "Und des Seelenflugs H\u00f6he,", "tokens": ["Und", "des", "See\u00b7len\u00b7flugs", "H\u00f6\u00b7he", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.7": {"text": "Mehr, als sie je", "tokens": ["Mehr", ",", "als", "sie", "je"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["PIS", "$,", "KOUS", "PPER", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Ein sterbliches M\u00e4dchen schm\u00fcckte.", "tokens": ["Ein", "sterb\u00b7li\u00b7ches", "M\u00e4d\u00b7chen", "schm\u00fcck\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Ich aber lieg' auf meinem Berge,", "tokens": ["Ich", "a\u00b7ber", "lieg'", "auf", "mei\u00b7nem", "Ber\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und meine Todtenklage hallt", "tokens": ["Und", "mei\u00b7ne", "Tod\u00b7ten\u00b7kla\u00b7ge", "hallt"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Hinab ins Thal \u2013 hinab ins freie Thal.", "tokens": ["Hin\u00b7ab", "ins", "Thal", "\u2013", "hin\u00b7ab", "ins", "frei\u00b7e", "Thal", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$(", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Ha! was strahlt dort herunter", "tokens": ["Ha", "!", "was", "strahlt", "dort", "her\u00b7un\u00b7ter"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "PWS", "VVFIN", "ADV", "APZR"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Aus dem Gew\u00f6lke der Nacht,", "tokens": ["Aus", "dem", "Ge\u00b7w\u00f6l\u00b7ke", "der", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Und erleuchtet deine H\u00fctte, ", "tokens": ["Und", "er\u00b7leuch\u00b7tet", "dei\u00b7ne", "H\u00fct\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ein Bote des Himmels ist's,", "tokens": ["Ein", "Bo\u00b7te", "des", "Him\u00b7mels", "ist's", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VAFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Er bringt Erh\u00f6rung:", "tokens": ["Er", "bringt", "Er\u00b7h\u00f6\u00b7rung", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.7": {"line.1": {"text": "\u00bb", "tokens": ["\u00bb"], "token_info": ["punct"], "pos": ["$("]}, "line.2": {"text": "Sei die Lust der Welt,", "tokens": ["Sei", "die", "Lust", "der", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Und sp\u00e4t einst", "tokens": ["Und", "sp\u00e4t", "einst"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJD", "ADV"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Der Engel Gespielin!", "tokens": ["Der", "En\u00b7gel", "Ge\u00b7spie\u00b7lin", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Denn so gebot es der Herr!\u00ab", "tokens": ["Denn", "so", "ge\u00b7bot", "es", "der", "Herr", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ART", "NN", "$.", "$("], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.8": {"line.1": {"text": "Du richtest dich auf, ", "tokens": ["Du", "rich\u00b7test", "dich", "auf", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Faltest deine H\u00e4nd', und blickst", "tokens": ["Fal\u00b7test", "dei\u00b7ne", "H\u00e4nd'", ",", "und", "blickst"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "KON", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Weinendl\u00e4chelnd gen Himmel!", "tokens": ["Wei\u00b7nen\u00b7dl\u00e4\u00b7chelnd", "gen", "Him\u00b7mel", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Der Harfe st\u00e4rkster Ton", "tokens": ["Der", "Har\u00b7fe", "st\u00e4rks\u00b7ter", "Ton"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Dr\u00fcckt meine Wonne nicht aus.", "tokens": ["Dr\u00fcckt", "mei\u00b7ne", "Won\u00b7ne", "nicht", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.9": {"line.1": {"text": "Gestreckt lieg' ich auf meines Berges R\u00fccken,", "tokens": ["Ge\u00b7streckt", "lieg'", "ich", "auf", "mei\u00b7nes", "Ber\u00b7ges", "R\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "PPER", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und weine des Entz\u00fcckens s\u00fc\u00dfe Thr\u00e4ne,", "tokens": ["Und", "wei\u00b7ne", "des", "Ent\u00b7z\u00fc\u00b7ckens", "s\u00fc\u00b7\u00dfe", "Thr\u00e4\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Des hei\u00dfen gefl\u00fcgelten Dankes s\u00fc\u00dfe Thr\u00e4ne;", "tokens": ["Des", "hei\u00b7\u00dfen", "ge\u00b7fl\u00fc\u00b7gel\u00b7ten", "Dan\u00b7kes", "s\u00fc\u00b7\u00dfe", "Thr\u00e4\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Denn, ach! du lebst, ", "tokens": ["Denn", ",", "ach", "!", "du", "lebst", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KO