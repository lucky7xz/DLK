ang.", "tokens": ["Die", "Na\u00b7se", "wird", "er\u00b7staun\u00b7lich", "lang", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.36": {"line.1": {"text": "Und dick und dicker schwillt der Kopf;", "tokens": ["Und", "dick", "und", "di\u00b7cker", "schwillt", "der", "Kopf", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KON", "ADJD", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er ist von Blech, er wird zum Topf;", "tokens": ["Er", "ist", "von", "Blech", ",", "er", "wird", "zum", "Topf", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "$,", "PPER", "VAFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.37": {"line.1": {"text": "Wobei ein Teufel voller List", "tokens": ["Wo\u00b7bei", "ein", "Teu\u00b7fel", "vol\u00b7ler", "List"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Als Musikus besch\u00e4ftigt ist.", "tokens": ["Als", "Mu\u00b7si\u00b7kus", "be\u00b7sch\u00e4f\u00b7tigt", "ist", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.38": {"line.1": {"text": "Wie er erwacht, das sieht man hier:", "tokens": ["Wie", "er", "er\u00b7wacht", ",", "das", "sieht", "man", "hier", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVPP", "$,", "PDS", "VVFIN", "PIS", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein jedes Haar ein Pfropfenziehr.", "tokens": [