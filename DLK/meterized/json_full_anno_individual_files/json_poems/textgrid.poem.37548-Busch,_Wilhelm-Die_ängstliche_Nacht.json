{"textgrid.poem.37548": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Die \u00e4ngstliche Nacht", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Heut bleibt der Herr mal wieder lang.", "tokens": ["Heut", "bleibt", "der", "Herr", "mal", "wie\u00b7der", "lang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Still wartet sein Am\u00f6blemang.", "tokens": ["Still", "war\u00b7tet", "sein", "A\u00b7m\u00f6b\u00b7le\u00b7mang", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Da kommt er endlich angestoppelt.", "tokens": ["Da", "kommt", "er", "end\u00b7lich", "an\u00b7ge\u00b7stop\u00b7pelt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die M\u00f6bel haben sich verdoppelt.", "tokens": ["Die", "M\u00f6\u00b7bel", "ha\u00b7ben", "sich", "ver\u00b7dop\u00b7pelt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Was w\u00e4r denn dieses hier? Ei ei!", "tokens": ["Was", "w\u00e4r", "denn", "die\u00b7ses", "hier", "?", "Ei", "ei", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "PDAT", "ADV", "$.", "NN", "NE", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Aus einem Beine werden zwei.", "tokens": ["Aus", "ei\u00b7nem", "Bei\u00b7ne", "wer\u00b7den", "zwei", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "CARD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Der Kleiderhalter, sonst so n\u00fctze,", "tokens": ["Der", "Klei\u00b7der\u00b7hal\u00b7ter", ",", "sonst", "so", "n\u00fct\u00b7ze", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zeigt sich als unbestimmte St\u00fctze.", "tokens": ["Zeigt", "sich", "als", "un\u00b7be\u00b7stimm\u00b7te", "St\u00fct\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "KOUS", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Oha! Jetzt wird ihm aber schwach.", "tokens": ["O\u00b7ha", "!", "Jetzt", "wird", "ihm", "a\u00b7ber", "schwach", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "ADV", "VAFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Willenskr\u00e4fte lassen nach.", "tokens": ["Die", "Wil\u00b7lens\u00b7kr\u00e4f\u00b7te", "las\u00b7sen", "nach", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Er sucht auf seiner Lagerstatt", "tokens": ["Er", "sucht", "auf", "sei\u00b7ner", "La\u00b7ger\u00b7statt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Ruhe, die er n\u00f6tig hat.", "tokens": ["Die", "Ru\u00b7he", ",", "die", "er", "n\u00f6\u00b7tig", "hat", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Auweh! der Fu\u00df ist sehr bedr\u00fcckt;", "tokens": ["Au\u00b7weh", "!", "der", "Fu\u00df", "ist", "sehr", "be\u00b7dr\u00fcckt", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ART", "NN", "VAFIN", "ADV", "ADJD", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Ein harter K\u00e4fer bei\u00dft und zwickt.", "tokens": ["Ein", "har\u00b7ter", "K\u00e4\u00b7fer", "bei\u00dft", "und", "zwickt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Der K\u00e4fer zwickt, der K\u00e4fer kneift;", "tokens": ["Der", "K\u00e4\u00b7fer", "zwickt", ",", "der", "K\u00e4\u00b7fer", "kneift", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit M\u00fche wird er abgestreift.", "tokens": ["Mit", "M\u00fc\u00b7he", "wird", "er", "ab\u00b7ge\u00b7streift", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Jedoch die Ruhe w\u00e4hrt nicht lange;", "tokens": ["Je\u00b7doch", "die", "Ru\u00b7he", "w\u00e4hrt", "nicht", "lan\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Schon wieder zwickt die harte Zange.", "tokens": ["Schon", "wie\u00b7der", "zwickt", "die", "har\u00b7te", "Zan\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Er dreht sich um, so schnell er kann;", "tokens": ["Er", "dreht", "sich", "um", ",", "so", "schnell", "er", "kann", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PTKVZ", "$,", "ADV", "ADJD", "PPER", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da st\u00f6\u00dft ihn wer von hinten an.", "tokens": ["Da", "st\u00f6\u00dft", "ihn", "wer", "von", "hin\u00b7ten", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PWS", "APPR", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Habuh! Da ist er! Steif und kalt;", "tokens": ["Ha\u00b7buh", "!", "Da", "ist", "er", "!", "Steif", "und", "kalt", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ADV", "VAFIN", "PPER", "$.", "NN", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Kerl von scheu\u00dflicher Gestalt.", "tokens": ["Ein", "Kerl", "von", "scheu\u00df\u00b7li\u00b7cher", "Ge\u00b7stalt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Ha, drauf und dran! Du oder ich!", "tokens": ["Ha", ",", "drauf", "und", "dran", "!", "Du", "o\u00b7der", "ich", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PAV", "KON", "PAV", "$.", "PPER", "KON", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Jetzt hei\u00dft es, Alter, wehre dich!", "tokens": ["Jetzt", "hei\u00dft", "es", ",", "Al\u00b7ter", ",", "weh\u00b7re", "dich", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "NN", "$,", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.13": {"line.1": {"text": "Hei\u00df tobt der Kampf, hoch saust das Bein;", "tokens": ["Hei\u00df", "tobt", "der", "Kampf", ",", "hoch", "saust", "das", "Bein", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,", "ADJD", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es mischt sich noch ein Dritter drein.", "tokens": ["Es", "mischt", "sich", "noch", "ein", "Drit\u00b7ter", "drein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.14": {"line.1": {"text": "Doch siehe da, der Feind erliegt.", "tokens": ["Doch", "sie\u00b7he", "da", ",", "der", "Feind", "er\u00b7liegt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "ADV", "$,", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Kampf ist aus, er hat gesiegt.", "tokens": ["Der", "Kampf", "ist", "aus", ",", "er", "hat", "ge\u00b7siegt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKVZ", "$,", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.15": {"line.1": {"text": "Gottlob, so kommt er endlich nun", "tokens": ["Gott\u00b7lob", ",", "so", "kommt", "er", "end\u00b7lich", "nun"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "ADV", "VVFIN", "PPER", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Doch mal dazu, sich auszuruhn.", "tokens": ["Doch", "mal", "da\u00b7zu", ",", "sich", "aus\u00b7zu\u00b7ruhn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "PAV", "$,", "PRF", "VVIZU", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.16": {"line.1": {"text": "Doch nein, ihm ist so dumpf und bang;", "tokens": ["Doch", "nein", ",", "ihm", "ist", "so", "dumpf", "und", "bang", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKANT", "$,", "PPER", "VAFIN", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Nase wird erstaunlich lang.", "tokens": ["Die", "Na\u00b7se", "wird", "er\u00b7staun\u00b7lich", "lang", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.17": {"line.1": {"text": "Und dick und dicker schwillt der Kopf;", "tokens": ["Und", "dick", "und", "di\u00b7cker", "schwillt", "der", "Kopf", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KON", "ADJD", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er ist von Blech, er wird zum Topf;", "tokens": ["Er", "ist", "von", "Blech", ",", "er", "wird", "zum", "Topf", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "$,", "PPER", "VAFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.18": {"line.1": {"text": "Wobei ein Teufel voller List", "tokens": ["Wo\u00b7bei", "ein", "Teu\u00b7fel", "vol\u00b7ler", "List"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Als Musikus besch\u00e4ftigt ist.", "tokens": ["Als", "Mu\u00b7si\u00b7kus", "be\u00b7sch\u00e4f\u00b7tigt", "ist", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.19": {"line.1": {"text": "Wie er erwacht, das sieht man hier:", "tokens": ["Wie", "er", "er\u00b7wacht", ",", "das", "sieht", "man", "hier", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVPP", "$,", "PDS", "VVFIN", "PIS", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein jedes Haar ein Pfropfenziehr.", "tokens": ["Ein", "je\u00b7des", "Haar", "ein", "Pfrop\u00b7fen\u00b7ziehr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.20": {"line.1": {"text": "Heut bleibt der Herr mal wieder lang.", "tokens": ["Heut", "bleibt", "der", "Herr", "mal", "wie\u00b7der", "lang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Still wartet sein Am\u00f6blemang.", "tokens": ["Still", "war\u00b7tet", "sein", "A\u00b7m\u00f6b\u00b7le\u00b7mang", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.21": {"line.1": {"text": "Da kommt er endlich angestoppelt.", "tokens": ["Da", "kommt