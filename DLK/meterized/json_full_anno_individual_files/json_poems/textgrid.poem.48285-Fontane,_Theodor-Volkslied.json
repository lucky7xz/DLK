{"textgrid.poem.48285": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Volkslied", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "All, die ihr schlaft auf Dunen, behaglich, wohlgemut,", "tokens": ["All", ",", "die", "ihr", "schlaft", "auf", "Du\u00b7nen", ",", "be\u00b7hag\u00b7lich", ",", "wohl\u00b7ge\u00b7mut", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPER", "ADJD", "APPR", "ADJA", "$,", "ADJD", "$,", "ADJD", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Denkt unsrer armen Br\u00fcder, die kalt sind bis aufs Blut,", "tokens": ["Denkt", "uns\u00b7rer", "ar\u00b7men", "Br\u00fc\u00b7der", ",", "die", "kalt", "sind", "bis", "aufs", "Blut", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "$,", "PRELS", "ADJD", "VAFIN", "APPR", "APPRART", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Die in den Gr\u00e4bern liegen, krank, hungrig, starr und stumm,", "tokens": ["Die", "in", "den", "Gr\u00e4\u00b7bern", "lie\u00b7gen", ",", "krank", ",", "hung\u00b7rig", ",", "starr", "und", "stumm", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "$,", "ADJD", "$,", "ADJD", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Die Bl\u00fcte unsres Landes, im Schlammbett kommt sie um.", "tokens": ["Die", "Bl\u00fc\u00b7te", "uns\u00b7res", "Lan\u00b7des", ",", "im", "Schlamm\u00b7bett", "kommt", "sie", "um", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$,", "APPRART", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Vom Balaklawa-Hafen bis an die Stadt heran,", "tokens": ["Vom", "Ba\u00b7la\u00b7kla\u00b7wa\u00b7Ha\u00b7fen", "bis", "an", "die", "Stadt", "he\u00b7ran", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Vor Karren und Gesch\u00fctzen sie selber als Gespann,", "tokens": ["Vor", "Kar\u00b7ren", "und", "Ge\u00b7sch\u00fct\u00b7zen", "sie", "sel\u00b7ber", "als", "Ge\u00b7spann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "PPER", "ADV", "KOUS", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "So haben sie's gehalten, dann kam die stille Nacht,", "tokens": ["So", "ha\u00b7ben", "sie's", "ge\u00b7hal\u00b7ten", ",", "dann", "kam", "die", "stil\u00b7le", "Nacht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "VVPP", "$,", "ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "So viele gingen schlafen, so wenig sind erwacht.", "tokens": ["So", "vie\u00b7le", "gin\u00b7gen", "schla\u00b7fen", ",", "so", "we\u00b7nig", "sind", "er\u00b7wacht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VVFIN", "VVINF", "$,", "ADV", "PIS", "VAFIN", "VVPP", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "Ich h\u00f6rt' ein M\u00e4dchen klagen, sie rief: \u00bbWas fang' ich an?", "tokens": ["Ich", "h\u00f6rt'", "ein", "M\u00e4d\u00b7chen", "kla\u00b7gen", ",", "sie", "rief", ":", "\u00bb", "Was", "fang'", "ich", "an", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVINF", "$,", "PPER", "VVFIN", "$.", "$(", "PWS", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Mein Vater liegt und schlummert im Tal von Inkerman,", "tokens": ["Mein", "Va\u00b7ter", "liegt", "und", "schlum\u00b7mert", "im", "Tal", "von", "In\u00b7ker\u00b7man", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "KON", "VVFIN", "APPRART", "NN", "APPR", "NE", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Mein Bruder liegt verwundet, genesen wird er nie,", "tokens": ["Mein", "Bru\u00b7der", "liegt", "ver\u00b7wun\u00b7det", ",", "ge\u00b7ne\u00b7sen", "wird", "er", "nie", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "VVPP", "$,", "VVPP", "VAFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Es kann kein Christ genesen in jenem Skutari.\u00ab", "tokens": ["Es", "kann", "kein", "Christ", "ge\u00b7ne\u00b7sen", "in", "je\u00b7nem", "Sku\u00b7ta\u00b7ri", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VMFIN", "PIAT", "NN", "VVPP", "APPR", "PDAT", "NN", "$.", "$("], "meter": "-+-+-+--+----", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Gott, sch\u00fctze unsre Br\u00fcder mit deiner m\u00e4cht'gen Hand,", "tokens": ["Gott", ",", "sch\u00fct\u00b7ze", "uns\u00b7re", "Br\u00fc\u00b7der", "mit", "dei\u00b7ner", "m\u00e4cht'\u00b7gen", "Hand", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPOSAT", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Leih ihnen Sieg und f\u00fchre sie heim ins Vaterland,", "tokens": ["Leih", "ih\u00b7nen", "Sieg", "und", "f\u00fch\u00b7re", "sie", "heim", "ins", "Va\u00b7ter\u00b7land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "NN", "KON", "VVFIN", "PPER", "PTKVZ", "APPRART", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Besch\u00fctz' auch, was sie lieben, Weib, Vater, Mutter, Kind,", "tokens": ["Be\u00b7sch\u00fctz'", "auch", ",", "was", "sie", "lie\u00b7ben", ",", "Weib", ",", "Va\u00b7ter", ",", "Mut\u00b7ter", ",", "Kind", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "PRELS", "PPER", "VVINF", "$,", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und sei ein Tr\u00f6ster aller, die schweren Herzens sind.", "tokens": ["Und", "sei", "ein", "Tr\u00f6s\u00b7ter", "al\u00b7ler", ",", "die", "schwe\u00b7ren", "Her\u00b7zens", "sind", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "PIAT", "$,", "ART", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.5": {"line.1": {"text": "All, die ihr schlaft auf Dunen, behaglich, wohlgemut,", "tokens": ["All", ",", "die", "ihr", "schlaft", "auf", "Du\u00b7nen",