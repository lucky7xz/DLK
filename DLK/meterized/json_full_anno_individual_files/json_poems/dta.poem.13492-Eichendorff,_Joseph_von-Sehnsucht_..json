{"dta.poem.13492": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Sehnsucht .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Es schienen so golden die Sterne,", "tokens": ["Es", "schie\u00b7nen", "so", "gol\u00b7den", "die", "Ster\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Am Fenster ich einsam stand", "tokens": ["Am", "Fens\u00b7ter", "ich", "ein\u00b7sam", "stand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "PPER", "ADJD", "VVFIN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und h\u00f6rte aus weiter Ferne", "tokens": ["Und", "h\u00f6r\u00b7te", "aus", "wei\u00b7ter", "Fer\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADV", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Ein Posthorn im stillen Land.", "tokens": ["Ein", "Post\u00b7horn", "im", "stil\u00b7len", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Das Herz mir im Leib entbrennte,", "tokens": ["Das", "Herz", "mir", "im", "Leib", "ent\u00b7brenn\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Da hab' ich mir heimlich gedacht:", "tokens": ["Da", "hab'", "ich", "mir", "heim\u00b7lich", "ge\u00b7dacht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "ADJD", "VVPP", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.7": {"text": "Ach wer da mitreisen k\u00f6nnte", "tokens": ["Ach", "wer", "da", "mit\u00b7rei\u00b7sen", "k\u00f6nn\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ITJ", "PWS", "ADV", "VVINF", "VMFIN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "In der pr\u00e4chtigen Sommernacht!", "tokens": ["In", "der", "pr\u00e4ch\u00b7ti\u00b7gen", "Som\u00b7mer\u00b7nacht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.2": {"line.1": {"text": "Zwei junge Gesellen gingen", "tokens": ["Zwei", "jun\u00b7ge", "Ge\u00b7sel\u00b7len", "gin\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["CARD", "ADJA", "NN", "VVFIN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Vor\u00fcber am Bergeshang,", "tokens": ["Vor\u00b7\u00fc\u00b7ber", "am", "Ber\u00b7ges\u00b7hang", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ich h\u00f6rte im Wandern sie singen", "tokens": ["Ich", "h\u00f6r\u00b7te", "im", "Wan\u00b7dern", "sie", "sin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "PPER", "VVINF"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Die stille Gegend entlang:", "tokens": ["Die", "stil\u00b7le", "Ge\u00b7gend", "ent\u00b7lang", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Von schwindelnden Felsenschl\u00fcften,", "tokens": ["Von", "schwin\u00b7deln\u00b7den", "Fel\u00b7sen\u00b7schl\u00fcf\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Wo die W\u00e4lder rauschen so sacht,", "tokens": ["Wo", "die", "W\u00e4l\u00b7der", "rau\u00b7schen", "so", "sacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "ADV", "ADJD", "$,"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Von Quellen, die von den Kl\u00fcften", "tokens": ["Von", "Quel\u00b7len", ",", "die", "von", "den", "Kl\u00fcf\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "PRELS", "APPR", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Sich st\u00fcrzen in die Waldesnacht.", "tokens": ["Sich", "st\u00fcr\u00b7zen", "in", "die", "Wal\u00b7des\u00b7nacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Sie sangen von Marmorbildern,", "tokens": ["Sie", "san\u00b7gen", "von", "Mar\u00b7mor\u00b7bil\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Von G\u00e4rten, die \u00fcber'm Gestein", "tokens": ["Von", "G\u00e4r\u00b7ten", ",", "die", "\u00fc\u00b7ber'm", "Ge\u00b7stein"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "PRELS", "APPRART", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "In d\u00e4mmernden Lauben verwildern,", "tokens": ["In", "d\u00e4m\u00b7mern\u00b7den", "Lau\u00b7ben", "ver\u00b7wil\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Pal\u00e4sten im Mondenschein,", "tokens": ["Pa\u00b7l\u00e4s\u00b7ten", "im", "Mon\u00b7den\u00b7schein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Wo die M\u00e4dchen am Fenster lauschen,", "tokens": ["Wo", "die", "M\u00e4d\u00b7chen", "am", "Fens\u00b7ter", "lau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPRART", "NN", "VVINF", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "Wann der Lauten Klang erwacht", "tokens": ["Wann", "der", "Lau\u00b7ten", "Klang", "er\u00b7wacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVPP"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.7": {"text": "Und die Brunnen verschlafen rauschen", "tokens": ["Und", "die", "Brun\u00b7nen", "ver\u00b7schla\u00b7fen", "rau\u00b7schen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVINF", "VVINF"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.8": {"text": "In der pr\u00e4chtigen Sommernacht. \u2014", "tokens": ["In", "der", "pr\u00e4ch\u00b7ti\u00b7gen", "Som\u00b7mer\u00b7nacht", "."], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$.", "$("], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}}}}