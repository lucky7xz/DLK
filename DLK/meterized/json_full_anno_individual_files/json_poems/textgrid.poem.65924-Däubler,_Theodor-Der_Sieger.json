{"textgrid.poem.65924": {"metadata": {"author": {"name": "D\u00e4ubler, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Der Sieger", "genre": "verse", "period": "N.A.", "pub_year": 1905, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Athen bejauchzt seinen berauschten Sieger,", "tokens": ["A\u00b7then", "be\u00b7jauchzt", "sei\u00b7nen", "be\u00b7rauschten", "Sie\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+---+-+-+-", "measure": "dactylic.init"}, "line.2": {"text": "Dem in Olympia Ruhmestat gelang;", "tokens": ["Dem", "in", "O\u00b7lym\u00b7pia", "Ruh\u00b7me\u00b7stat", "ge\u00b7lang", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Von Lauf und Ringen k\u00fcndet leicht der Gang:", "tokens": ["Von", "Lauf", "und", "Rin\u00b7gen", "k\u00fcn\u00b7det", "leicht", "der", "Gang", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Er glich, beim Hinsprung, Ikaros dem Flieger.", "tokens": ["Er", "glich", ",", "beim", "Hin\u00b7sprung", ",", "I\u00b7ka\u00b7ros", "dem", "Flie\u00b7ger", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "APPRART", "NN", "$,", "NE", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wohl w\u00fcnschte k\u00fchn sein Volk: die Glieder schmieg er,", "tokens": ["Wohl", "w\u00fcnschte", "k\u00fchn", "sein", "Volk", ":", "die", "Glie\u00b7der", "schmieg", "er", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "PPOSAT", "NN", "$.", "ART", "NN", "VVFIN", "PPER", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Gewandter als ein Dorer ihn beschlang,", "tokens": ["Ge\u00b7wand\u00b7ter", "als", "ein", "Do\u00b7rer", "ihn", "be\u00b7schlang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Um Gegners Nackenpracht, durch Kampfesschwang,", "tokens": ["Um", "Geg\u00b7ners", "Na\u00b7cken\u00b7pracht", ",", "durch", "Kamp\u00b7fes\u00b7schwang", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUI", "NN", "NN", "$,", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und \u00fcberw\u00e4ltige ihn wie ein Tiger!", "tokens": ["Und", "\u00fc\u00b7berw\u00b7\u00e4l\u00b7ti\u00b7ge", "ihn", "wie", "ein", "Ti\u00b7ger", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Erjubelnde, auf vollen Marmorstufen,", "tokens": ["Er\u00b7ju\u00b7beln\u00b7de", ",", "auf", "vol\u00b7len", "Mar\u00b7mor\u00b7stu\u00b7fen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Umpreisen den Gekr\u00f6nten mit Gesang:", "tokens": ["Um\u00b7prei\u00b7sen", "den", "Ge\u00b7kr\u00f6n\u00b7ten", "mit", "Ge\u00b7sang", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Begr\u00fc\u00dft doch Gl\u00fcckliche gewognes Rufen!", "tokens": ["Be\u00b7gr\u00fc\u00dft", "doch", "Gl\u00fcck\u00b7li\u00b7che", "ge\u00b7wog\u00b7nes", "Ru\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Auch f\u00fcr Athen verlangt ihn nach Empfang:", "tokens": ["Auch", "f\u00fcr", "A\u00b7then", "ver\u00b7langt", "ihn", "nach", "Emp\u00b7fang", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NE", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Ergreiste, die zu seiner Ehre schufen,", "tokens": ["Er\u00b7greis\u00b7te", ",", "die", "zu", "sei\u00b7ner", "Eh\u00b7re", "schu\u00b7fen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PRELS", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Umhalst ein Arm, der Lehrern Sieg errang.", "tokens": ["Um\u00b7halst", "ein", "Arm", ",", "der", "Leh\u00b7rern", "Sieg", "er\u00b7rang", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Athen bejauchzt seinen berauschten Sieger,", "tokens": ["A\u00b7then", "be\u00b7jauchzt", "sei\u00b7nen", "be\u00b7rauschten", "Sie\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+---+-+-+-", "measure": "dactylic.init"}, "line.2": {"text": "Dem in Olympia Ruhmestat gelang;", "tokens": ["Dem", "in", "O\u00b7lym\u00b7pia", "Ruh\u00b7me\u00b7stat", "ge\u00b7lang", ";"], "token_