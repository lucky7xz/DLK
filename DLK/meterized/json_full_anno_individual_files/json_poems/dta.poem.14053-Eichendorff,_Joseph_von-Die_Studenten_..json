{"dta.poem.14053": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Die Studenten .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Quer\u00fcber \u00fcber's Wasser glatt", "tokens": ["Quer\u00b7\u00fc\u00b7ber", "\u00fc\u00b7ber's", "Was\u00b7ser", "glatt"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "La\u00df werben deine Aeugelein,", "tokens": ["La\u00df", "wer\u00b7ben", "dei\u00b7ne", "A\u00b7e\u00b7u\u00b7ge\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und der dir wohlgefallen hat,", "tokens": ["Und", "der", "dir", "wohl\u00b7ge\u00b7fal\u00b7len", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PPER", "VVINF", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der soll dein lieber Buhle sein.", "tokens": ["Der", "soll", "dein", "lie\u00b7ber", "Buh\u00b7le", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPOSAT", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}