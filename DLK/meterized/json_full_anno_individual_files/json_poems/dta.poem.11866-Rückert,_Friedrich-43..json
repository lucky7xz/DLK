{"dta.poem.11866": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "43.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Des Kindes erster Trieb ist sinnliches Bed\u00fcrfen,", "tokens": ["Des", "Kin\u00b7des", "ers\u00b7ter", "Trieb", "ist", "sinn\u00b7li\u00b7ches", "Be\u00b7d\u00fcr\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VAFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und sp\u00e4ter w\u00e4chst die Kraft zu geistigen Entw\u00fcrfen.", "tokens": ["Und", "sp\u00e4\u00b7ter", "w\u00e4chst", "die", "Kraft", "zu", "geis\u00b7ti\u00b7gen", "Ent\u00b7w\u00fcr\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wie alle Menschen nun von Anfang Kinder sind;", "tokens": ["Wie", "al\u00b7le", "Men\u00b7schen", "nun", "von", "An\u00b7fang", "Kin\u00b7der", "sind", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "ADV", "APPR", "NN", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Menschheit selber, war sie Anfangs auch ein Kind?", "tokens": ["Die", "Menschheit", "sel\u00b7ber", ",", "war", "sie", "An\u00b7fangs", "auch", "ein", "Kind", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "VAFIN", "PPER", "ADV", "ADV", "ART", "NN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Sie war's in einem Sinn, im andern Sinne nicht;", "tokens": ["Sie", "wa\u00b7r's", "in", "ei\u00b7nem", "Sinn", ",", "im", "an\u00b7dern", "Sin\u00b7ne", "nicht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ART", "NN", "$,", "APPRART", "ADJA", "NN", "PTKNEG", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.2": {"text": "Die Menschheit war ein Kind wie neugebornes Licht.", "tokens": ["Die", "Menschheit", "war", "ein", "Kind", "wie", "neu\u00b7ge\u00b7bor\u00b7nes", "Licht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "KOKOM", "ADJA", "NN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Wie neugebornes Licht, im Osten angeglommen,", "tokens": ["Wie", "neu\u00b7ge\u00b7bor\u00b7nes", "Licht", ",", "im", "Os\u00b7ten", "an\u00b7ge\u00b7glom\u00b7men", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "$,", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nicht gleich dem Mittag ist, doch ebenso vollkommen;", "tokens": ["Nicht", "gleich", "dem", "Mit\u00b7tag", "ist", ",", "doch", "e\u00b7ben\u00b7so", "voll\u00b7kom\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ART", "NN", "VAFIN", "$,", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Am Licht des Tages wird zur Bl\u00fcte sich entfalten", "tokens": ["Am", "Licht", "des", "Ta\u00b7ges", "wird", "zur", "Bl\u00fc\u00b7te", "sich", "ent\u00b7fal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN", "VAFIN", "APPRART", "NN", "PRF", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nur was im Morgenthau der Knospe war enthalten:", "tokens": ["Nur", "was", "im", "Mor\u00b7gent\u00b7hau", "der", "Knos\u00b7pe", "war", "ent\u00b7hal\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PRELS", "APPRART", "NN", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "So nur entfaltet sich am gro\u00dfen Menschheitstag", "tokens": ["So", "nur", "ent\u00b7fal\u00b7tet", "sich", "am", "gro\u00b7\u00dfen", "Menschheits\u00b7tag"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "PRF", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Was eingewickelt in der Kindheit Wiege lag.", "tokens": ["Was", "ein\u00b7ge\u00b7wi\u00b7ckelt", "in", "der", "Kind\u00b7heit", "Wie\u00b7ge", "lag", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "APPR", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Die Menschheit, Gottes Kind, ist niemals mehr noch minder,", "tokens": ["Die", "Menschheit", ",", "Got\u00b7tes", "Kind", ",", "ist", "nie\u00b7mals", "mehr", "noch", "min\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "NN", "$,", "VAFIN", "ADV", "ADV", "ADV", "ADV", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Nur mehr und minder sind die Menschen Gottes Kinder:", "tokens": ["Nur", "mehr", "und", "min\u00b7der", "sind", "die", "Men\u00b7schen", "Got\u00b7tes", "Kin\u00b7der", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "KON", "ADV", "VAFIN", "ART", "NN", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Wie mehr und minder ganz ist einer Blume Glanz,", "tokens": ["Wie", "mehr", "und", "min\u00b7der", "ganz", "ist", "ei\u00b7ner", "Blu\u00b7me", "Glanz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "KON", "ADV", "ADV", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch ist ein ganzer Glanz der volle Blumenkranz.", "tokens": ["Doch", "ist", "ein", "gan\u00b7zer", "Glanz", "der", "vol\u00b7le", "Blu\u00b7men\u00b7kranz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Wie aber eine Blum' ins gro\u00dfe Kranzgeflecht,", "tokens": ["Wie", "a\u00b7ber", "ei\u00b7ne", "Blum'", "ins", "gro\u00b7\u00dfe", "Kranz\u00b7ge\u00b7flecht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So tritt der Einzelmensch ins menschliche Geschlecht.", "tokens": ["So", "tritt", "der", "Ein\u00b7zel\u00b7mensch", "ins", "menschli\u00b7che", "Ge\u00b7schlecht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.10": {"line.1": {"text": "Die Blume wei\u00df nicht, wie sie an die Stelle kam,", "tokens": ["Die", "Blu\u00b7me", "wei\u00df", "nicht", ",", "wie", "sie", "an", "die", "Stel\u00b7le", "kam", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "$,", "PWAV", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und nicht der Mensch, wozu er seinen Ort einnahm.", "tokens": ["Und", "nicht", "der", "Mensch", ",", "wo\u00b7zu", "er", "sei\u00b7nen", "Ort", "ein\u00b7nahm", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ART", "NN", "$,", "PWAV", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.11": {"line.1": {"text": "An seinem Orte macht er seine Kr\u00e4fte gelten,", "tokens": ["An", "sei\u00b7nem", "Or\u00b7te", "macht", "er", "sei\u00b7ne", "Kr\u00e4f\u00b7te", "gel\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Beherrscht die Welt, und dient nur dem Gesetz der Welten.", "tokens": ["Be\u00b7herrscht", "die", "Welt", ",", "und", "dient", "nur", "dem", "Ge\u00b7setz", "der", "Wel\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "KON", "VVFIN", "ADV", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.12": {"line.1": {"text": "Das echte Herrscherbild ist aber da gepr\u00e4gt,", "tokens": ["Das", "ech\u00b7te", "Herr\u00b7scher\u00b7bild", "ist", "a\u00b7ber", "da", "ge\u00b7pr\u00e4gt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wo menschliches Gem\u00fct die volle Menschheit tr\u00e4gt.", "tokens": ["Wo", "menschli\u00b7ches", "Ge\u00b7m\u00fct", "die", "vol\u00b7le", "Menschheit", "tr\u00e4gt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}}}}}