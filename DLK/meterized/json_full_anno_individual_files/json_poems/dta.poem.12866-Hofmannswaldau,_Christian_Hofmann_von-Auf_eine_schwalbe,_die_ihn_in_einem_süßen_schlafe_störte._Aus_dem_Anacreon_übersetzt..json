{"dta.poem.12866": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auf eine schwalbe, die ihn in einem  \n s\u00fc\u00dfen schlafe st\u00f6rte.  \n Aus dem   Anacreon   \u00fcbersetzt.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "K\u00fchne feindin meiner ruh!", "tokens": ["K\u00fch\u00b7ne", "fein\u00b7din", "mei\u00b7ner", "ruh", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Du verwaschne schwalbe du!", "tokens": ["Du", "ver\u00b7waschne", "schwal\u00b7be", "du", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "VVFIN", "PPER", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Sage, wie soll ich mich r\u00e4chen?", "tokens": ["Sa\u00b7ge", ",", "wie", "soll", "ich", "mich", "r\u00e4\u00b7chen", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "VMFIN", "PPER", "PRF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Soll ich dir die beine brechen?", "tokens": ["Soll", "ich", "dir", "die", "bei\u00b7ne", "bre\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "ART", "PIS", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Feindin meiner lust und ruh!", "tokens": ["Fein\u00b7din", "mei\u00b7ner", "lust", "und", "ruh", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Du verwegner vogel du!", "tokens": ["Du", "ver\u00b7weg\u00b7ner", "vo\u00b7gel", "du", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Soll ich dir die fl\u00fcgel nehmen,", "tokens": ["Soll", "ich", "dir", "die", "fl\u00fc\u00b7gel", "neh\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Oder gar die zunge l\u00e4hmen,", "tokens": ["O\u00b7der", "gar", "die", "zun\u00b7ge", "l\u00e4h\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Wie der grimme Tereus that?", "tokens": ["Wie", "der", "grim\u00b7me", "Te\u00b7reus", "that", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Zum verzeihn ist wol kein rath.", "tokens": ["Zum", "ver\u00b7zeihn", "ist", "wol", "kein", "rath", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "VVINF", "VAFIN", "ADV", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Denn was hast du alle morgen,", "tokens": ["Denn", "was", "hast", "du", "al\u00b7le", "mor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VAFIN", "PPER", "PIS", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Wenn sich Ph\u00f6bus noch verborgen,", "tokens": ["Wenn", "sich", "Ph\u00f6\u00b7bus", "noch", "ver\u00b7bor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "NE", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Und mich s\u00fc\u00dfe tr\u00e4um\u2019 erfreu\u2019n,", "tokens": ["Und", "mich", "s\u00fc\u00b7\u00dfe", "tr\u00e4um'", "er\u00b7freu'n", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.14": {"text": "Mir die ohren voll zu schrey\u2019n?", "tokens": ["Mir", "die", "oh\u00b7ren", "voll", "zu", "schrey'n", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "ADJD", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.15": {"text": "Ja dazu noch Bellarmiren", "tokens": ["Ja", "da\u00b7zu", "noch", "Be\u00b7llar\u00b7mi\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["PTKANT", "PAV", "ADV", "NN"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.16": {"text": "Aus den armen zu entf\u00fchren?", "tokens": ["Aus", "den", "ar\u00b7men", "zu", "ent\u00b7f\u00fch\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}