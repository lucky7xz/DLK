{"dta.poem.21377": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Caput XVII.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-30602-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Eichen sch\u00fcttelten ernsthaft das Haupt,", "tokens": ["Die", "Ei\u00b7chen", "sch\u00fct\u00b7tel\u00b7ten", "ernst\u00b7haft", "das", "Haupt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "ART", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die Birken und Birkenreiser", "tokens": ["Die", "Bir\u00b7ken", "und", "Bir\u00b7ken\u00b7rei\u00b7ser"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Sie nickten so warnend \u2013 und ich rief:", "tokens": ["Sie", "nick\u00b7ten", "so", "war\u00b7nend", "\u2013", "und", "ich", "rief", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "VVPP", "$(", "KON", "PPER", "VVFIN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Vergieb mir, mein theurer Kaiser!", "tokens": ["Ver\u00b7gieb", "mir", ",", "mein", "theu\u00b7rer", "Kai\u00b7ser", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Vergieb mir, o Rothbart, das rasche Wort!", "tokens": ["Ver\u00b7gieb", "mir", ",", "o", "Roth\u00b7bart", ",", "das", "ra\u00b7sche", "Wort", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "FM", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ich wei\u00df, du bist viel weiser", "tokens": ["Ich", "wei\u00df", ",", "du", "bist", "viel", "wei\u00b7ser"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VAFIN", "PIAT", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Als ich, ich habe so wenig Geduld \u2013", "tokens": ["Als", "ich", ",", "ich", "ha\u00b7be", "so", "we\u00b7nig", "Ge\u00b7duld", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "PPER", "VAFIN", "ADV", "PIAT", "NN", "$("], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.4": {"text": "Doch komme du bald, mein Kaiser!", "tokens": ["Doch", "kom\u00b7me", "du", "bald", ",", "mein", "Kai\u00b7ser", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "$,", "PPOSAT", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Behagt dir das Guillotiniren nicht,", "tokens": ["Be\u00b7hagt", "dir", "das", "Guil\u00b7lo\u00b7ti\u00b7ni\u00b7ren", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "So bleib bey den alten Mitteln:", "tokens": ["So", "bleib", "bey", "den", "al\u00b7ten", "Mit\u00b7teln", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.3": {"text": "Das Schwert f\u00fcr Edelleute, der Strick", "tokens": ["Das", "Schwert", "f\u00fcr", "E\u00b7del\u00b7leu\u00b7te", ",", "der", "Strick"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "APPR", "NN", "$,", "ART", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "F\u00fcr B\u00fcrger und Bauern in Kitteln.", "tokens": ["F\u00fcr", "B\u00fcr\u00b7ger", "und", "Bau\u00b7ern", "in", "Kit\u00b7teln", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "APPR", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.4": {"line.1": {"text": "Nur manchmal wechsle ab, und la\u00df", "tokens": ["Nur", "manch\u00b7mal", "wechs\u00b7le", "ab", ",", "und", "la\u00df"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "PTKVZ", "$,", "KON", "XY"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den Adel h\u00e4ngen, und k\u00f6pfe", "tokens": ["Den", "A\u00b7del", "h\u00e4n\u00b7gen", ",", "und", "k\u00f6p\u00b7fe"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVINF", "$,", "KON", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ein bischen die B\u00fcrger und Bauern, wir sind", "tokens": ["Ein", "bi\u00b7schen", "die", "B\u00fcr\u00b7ger", "und", "Bau\u00b7ern", ",", "wir", "sind"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADV", "ART", "NN", "KON", "NN", "$,", "PPER", "VAFIN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Ja alle Gottesgesch\u00f6pfe.", "tokens": ["Ja", "al\u00b7le", "Got\u00b7tes\u00b7ge\u00b7sch\u00f6p\u00b7fe", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKANT", "PIAT", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}}}}