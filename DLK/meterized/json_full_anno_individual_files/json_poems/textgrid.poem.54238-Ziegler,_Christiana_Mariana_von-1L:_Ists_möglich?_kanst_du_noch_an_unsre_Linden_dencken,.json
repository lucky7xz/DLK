{"textgrid.poem.54238": {"metadata": {"author": {"name": "Ziegler, Christiana Mariana von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ists m\u00f6glich? kanst du noch an unsre Linden dencken,", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ists m\u00f6glich? kanst du noch an unsre Linden dencken,", "tokens": ["Ists", "m\u00f6g\u00b7lich", "?", "kanst", "du", "noch", "an", "uns\u00b7re", "Lin\u00b7den", "den\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$.", "VMFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich w\u00fcnschte mir mit dir dich wieder hier zu sehn,", "tokens": ["Ich", "w\u00fcnschte", "mir", "mit", "dir", "dich", "wie\u00b7der", "hier", "zu", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "PPER", "PRF", "ADV", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Das Schicksal l\u00e4\u00dft sich nicht nach unsern Willen lencken,", "tokens": ["Das", "Schick\u00b7sal", "l\u00e4\u00dft", "sich", "nicht", "nach", "un\u00b7sern", "Wil\u00b7len", "len\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "PTKNEG", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So weh uns offtermahls darbey pflegt zu geschehn.", "tokens": ["So", "weh", "uns", "off\u00b7ter\u00b7mahls", "dar\u00b7bey", "pflegt", "zu", "ge\u00b7schehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "ADV", "PAV", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Mein! Wie bezeugst du dich bey dem so strengen Orden,", "tokens": ["Mein", "!", "Wie", "be\u00b7zeugst", "du", "dich", "bey", "dem", "so", "stren\u00b7gen", "Or\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "$.", "PWAV", "VVFIN", "PPER", "PRF", "APPR", "ART", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Der deinen Hoffnungs-Kahn durch wilde Fluthen treibt,", "tokens": ["Der", "dei\u00b7nen", "Hoff\u00b7nungs\u00b7Kahn", "durch", "wil\u00b7de", "Flut\u00b7hen", "treibt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Weil du, du schreibst es selbst, ein Geistlicher geworden.", "tokens": ["Weil", "du", ",", "du", "schreibst", "es", "selbst", ",", "ein", "Geist\u00b7li\u00b7cher", "ge\u00b7wor\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "PPER", "VVFIN", "PPER", "ADV", "$,", "ART", "NN", "VAPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wohl dir! Wofern dein Sinn und Schlu\u00df darbey verbleibt.", "tokens": ["Wohl", "dir", "!", "Wo\u00b7fern", "dein", "Sinn", "und", "Schlu\u00df", "dar\u00b7bey", "ver\u00b7bleibt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "$.", "KOUS", "PPOSAT", "NN", "KON", "NN", "PAV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Du kanst in solchen Stand deswegen doch noch schertzen,", "tokens": ["Du", "kanst", "in", "sol\u00b7chen", "Stand", "des\u00b7we\u00b7gen", "doch", "noch", "schert\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PIAT", "NN", "PAV", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Dergleichen Schreibens-Art verdamt euch leichte nicht;", "tokens": ["Derg\u00b7lei\u00b7chen", "Schrei\u00b7bens\u00b7Art", "ver\u00b7damt", "euch", "leich\u00b7te", "nicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "NN", "VVFIN", "PPER", "VVFIN", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Es klebt doch immer was noch an der M\u00e4nner Hertzen,", "tokens": ["Es", "klebt", "doch", "im\u00b7mer", "was", "noch", "an", "der", "M\u00e4n\u00b7ner", "Hert\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "PWS", "ADV", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Ob gleich die Heiligkeit von aussen wiederspricht.", "tokens": ["Ob", "gleich", "die", "Hei\u00b7lig\u00b7keit", "von", "aus\u00b7sen", "wie\u00b7der\u00b7spricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "APPR", "NE", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Wir Menschen sollen uns als N\u00e4chsten alle lieben,", "tokens": ["Wir", "Men\u00b7schen", "sol\u00b7len", "uns", "als", "N\u00e4chs\u00b7ten", "al\u00b7le", "lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "VMFIN", "PPER", "KOUS", "NN", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Und also hindert dich gar nicht dein Closter-Stand.", "tokens": ["Und", "al\u00b7so", "hin\u00b7dert", "dich", "gar", "nicht", "dein", "Clos\u00b7ter\u00b7Stand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADV", "PTKNEG", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Vielleicht ist von der Welt dir noch was \u00fcbrig blieben,", "tokens": ["Viel\u00b7leicht", "ist", "von", "der", "Welt", "dir", "noch", "was", "\u00fcb\u00b7rig", "blie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "ART", "NN", "PPER", "ADV", "PWS", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Dein artger Spa\u00df und Schertz ist mir gar wohl bekandt.", "tokens": ["Dein", "art\u00b7ger", "Spa\u00df", "und", "Schertz", "ist", "mir", "gar", "wohl", "be\u00b7kandt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "KON", "NN", "VAFIN", "PPER", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Indessen bin ich dir vor deinen Wunsch verbunden,", "tokens": ["In\u00b7des\u00b7sen", "bin", "ich", "dir", "vor", "dei\u00b7nen", "Wunsch", "ver\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Den deine Feder mir zum neuen Jahr geweyht.", "tokens": ["Den", "dei\u00b7ne", "Fe\u00b7der", "mir", "zum", "neu\u00b7en", "Jahr", "ge\u00b7weyht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "PPER", "APPRART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Ich w\u00fcnsche dir daf\u00fcr so viel vergn\u00fcgte Stunden,", "tokens": ["Ich", "w\u00fcn\u00b7sche", "dir", "da\u00b7f\u00fcr", "so", "viel", "ver\u00b7gn\u00fcg\u00b7te", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PAV", "ADV", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Als Flora hier und dar schon Blumen ausgestreut.", "tokens": ["Als", "Flo\u00b7ra", "hier", "und", "dar", "schon", "Blu\u00b7men", "aus\u00b7ge\u00b7streut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADV", "KON", "PAV", "ADV", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Was neues wei\u00df ich dir vor di\u00dfmahl nicht zu schreiben,", "tokens": ["Was", "neu\u00b7es", "wei\u00df", "ich", "dir", "vor", "di\u00df\u00b7mahl", "nicht", "zu", "schrei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJA", "VVFIN", "PPER", "PPER", "APPR", "ADV", "PTKNEG", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Denn es pa\u00dfiret nichts, das meldens w\u00fcrdig w\u00e4r,", "tokens": ["Denn", "es", "pa\u00b7\u00dfi\u00b7ret", "nichts", ",", "das", "mel\u00b7dens", "w\u00fcr\u00b7dig", "w\u00e4r", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PIS", "$,", "PRELS", "ADV", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Die Welt l\u00e4sts immer noch beym alten Conto bleiben,", "tokens": ["Die", "Welt", "l\u00e4sts", "im\u00b7mer", "noch", "beym", "al\u00b7ten", "Con\u00b7to", "blei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADV", "APPRART", "ADJA", "NE", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Ich gebe, wie du weist, Hist\u00f6rgen nicht Geh\u00f6r.", "tokens": ["Ich", "ge\u00b7be", ",", "wie", "du", "weist", ",", "His\u00b7t\u00f6r\u00b7gen", "nicht", "Ge\u00b7h\u00f6r", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "PPER", "VVFIN", "$,", "NN", "PTKNEG", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Du fragst, ob mich noch nicht ein Zauber-Blick gefangen?", "tokens": ["Du", "fragst", ",", "ob", "mich", "noch", "nicht", "ein", "Zau\u00b7ber\u00b7Blick", "ge\u00b7fan\u00b7gen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "ADV", "PTKNEG", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Ich lache, da\u00df man mir von solchen Sachen schreibt,", "tokens": ["Ich", "la\u00b7che", ",", "da\u00df", "man", "mir", "von", "sol\u00b7chen", "Sa\u00b7chen", "schreibt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PIS", "PPER", "APPR", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Du weist ja, da\u00df ich stets des Amors Strick entgangen,", "tokens": ["Du", "weist", "ja", ",", "da\u00df", "ich", "stets", "des", "A\u00b7mors", "Strick", "ent\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "KOUS", "PPER", "ADV", "ART", "NE", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Und da\u00df mein Hertze frey und ungebunden bleibt.", "tokens": ["Und", "da\u00df", "mein", "Hert\u00b7ze", "frey", "und", "un\u00b7ge\u00b7bun\u00b7den", "bleibt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPOSAT", "VVFIN", "ADJD", "KON", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Ein Blick kan meinen Geist nicht, wie man denckt, verf\u00fchren,", "tokens": ["Ein", "Blick", "kan", "mei\u00b7nen", "Geist", "nicht", ",", "wie", "man", "denckt", ",", "ver\u00b7f\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPOSAT", "NN", "PTKNEG", "$,", "PWAV", "PIS", "VVFIN", "$,", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Ein Seuffzer legt mir nicht gleich Band und Fesseln an,", "tokens": ["Ein", "Seuff\u00b7zer", "legt", "mir", "nicht", "gleich", "Band", "und", "Fes\u00b7seln", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKNEG", "ADV", "NN", "KON", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Mein Auge l\u00e4st der Welt nie Brand und Feuer sp\u00fchren,", "tokens": ["Mein", "Au\u00b7ge", "l\u00e4st", "der", "Welt", "nie", "Brand", "und", "Feu\u00b7er", "sp\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN", "ADV", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Und Amor, welchen ich nicht um mich leiden kan,", "tokens": ["Und", "A\u00b7mor", ",", "wel\u00b7chen", "ich", "nicht", "um", "mich", "lei\u00b7den", "kan", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "$,", "PWAT", "PPER", "PTKNEG", "APPR", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Flieht, wann er mich erblickt, er ist mir nicht gewogen,", "tokens": ["Flieht", ",", "wann", "er", "mich", "er\u00b7blickt", ",", "er", "ist", "mir", "nicht", "ge\u00b7wo\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "PPER", "PRF", "VVPP", "$,", "PPER", "VAFIN", "PPER", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Sein sonst so m\u00e4chtiges Gescho\u00df ist ohne Krafft.", "tokens": ["Sein", "sonst", "so", "m\u00e4ch\u00b7ti\u00b7ges", "Ge\u00b7scho\u00df", "ist", "oh\u00b7ne", "Krafft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADV", "ADV", "ADJA", "NN", "VAFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Du weist ja, eh du noch von uns hier weggezogen,", "tokens": ["Du", "weist", "ja", ",", "eh", "du", "noch", "von", "uns", "hier", "weg\u00b7ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "KOUS", "PPER", "ADV", "APPR", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "War diese Vanit\u00e4t bey mir l\u00e4ngst abgeschafft.", "tokens": ["War", "die\u00b7se", "Va\u00b7ni\u00b7t\u00e4t", "bey", "mir", "l\u00e4ngst", "ab\u00b7ge\u00b7schafft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDAT", "NN", "APPR", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Inzwischen lebe wohl und schreibe mir bald wieder.", "tokens": ["I\u00b7nzwi\u00b7schen", "le\u00b7be", "wohl", "und", "schrei\u00b7be", "mir", "bald", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "KON", "VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "H\u00f6r ich, da\u00df Hymen dir bald l\u00e4st den Braut-Crantz weyhn,", "tokens": ["H\u00f6r", "ich", ",", "da\u00df", "Hy\u00b7men", "dir", "bald", "l\u00e4st", "den", "Braut\u00b7Crantz", "weyhn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "$,", "KOUS", "NN", "PPER", "ADV", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "So will ich, ha\u00dft du nicht aus Eckel meine Lieder,", "tokens": ["So", "will", "ich", ",", "ha\u00dft", "du", "nicht", "aus", "E\u00b7ckel", "mei\u00b7ne", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "$,", "VVFIN", "PPER", "PTKNEG", "APPR", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Mit tausend Freuden dir ein Hochzeit-Carmen streun.", "tokens": ["Mit", "tau\u00b7send", "Freu\u00b7den", "dir", "ein", "Hoch\u00b7zeit\u00b7Car\u00b7men", "streun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ists m\u00f6glich? kanst du noch an unsre Linden dencken,", "tokens": ["Ists", "m\u00f6g\u00b7lich", "?", "kanst", "du", "noch", "an", "uns\u00b7re", "Lin\u00b7den", "den\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$.", "VMFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich w\u00fcnschte mir mit dir dich wieder hier zu sehn,", "tokens": ["Ich", "w\u00fcnschte", "mir", "mit", "dir", "dich", "wie\u00b7der", "hier", "zu", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "PPER", "PRF", "ADV", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Das Schicksal l\u00e4\u00dft sich nicht nach unsern Willen lencken,", "tokens": ["Das", "Schick\u00b7sal", "l\u00e4\u00dft", "sich", "nicht", "nach", "un\u00b7sern", "Wil\u00b7len", "len\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "PTKNEG", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So weh uns offtermahls darbey pflegt zu geschehn.", "tokens": ["So", "weh", "uns", "off\u00b7ter\u00b7mahls", "dar\u00b7bey", "pflegt", "zu", "ge\u00b7schehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "ADV", "PAV", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Mein! Wie bezeugst du dich bey dem so strengen Orden,", "tokens": ["Mein", "!", "Wie", "be\u00b7zeugst", "du", "dich", "bey", "dem", "so", "stren\u00b7gen", "Or\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "$.", "PWAV", "VVFIN", "PPER", "PRF", "APPR", "ART", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Der deinen Hoffnungs-Kahn durch wilde Fluthen treibt,", "tokens": ["Der", "dei\u00b7nen", "Hoff\u00b7nungs\u00b7Kahn", "durch", "wil\u00b7de", "Flut\u00b7hen", "treibt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Weil du, du schreibst es selbst, ein Geistlicher geworden.", "tokens": ["Weil", "du", ",", "du", "schreibst", "es", "selbst", ",", "ein", "Geist\u00b7li\u00b7cher", "ge\u00b7wor\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "PPER", "VVFIN", "PPER", "ADV", "$,", "ART", "NN", "VAPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wohl dir! Wofern dein Sinn und Schlu\u00df darbey verbleibt.", "tokens": ["Wohl", "dir", "!", "Wo\u00b7fern", "dein", "Sinn", "und", "Schlu\u00df", "dar\u00b7bey", "ver\u00b7bleibt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "$.", "KOUS", "PPOSAT", "NN", "KON", "NN", "PAV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Du kanst in solchen Stand deswegen doch noch schertzen,", "tokens": ["Du", "kanst", "in", "sol\u00b7chen", "Stand", "des\u00b7we\u00b7gen", "doch", "noch", "schert\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PIAT", "NN", "PAV", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Dergleichen Schreibens-Art verdamt euch leichte nicht;", "tokens": ["Derg\u00b7lei\u00b7chen", "Schrei\u00b7bens\u00b7Art", "ver\u00b7damt", "euch", "leich\u00b7te", "nicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "NN", "VVFIN", "PPER", "VVFIN", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Es klebt doch immer was noch an der M\u00e4nner Hertzen,", "tokens": ["Es", "klebt", "doch", "im\u00b7mer", "was", "noch", "an", "der", "M\u00e4n\u00b7ner", "Hert\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "PWS", "ADV", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Ob gleich die Heiligkeit von aussen wiederspricht.", "tokens": ["Ob", "gleich", "die", "Hei\u00b7lig\u00b7keit", "von", "aus\u00b7sen", "wie\u00b7der\u00b7spricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "APPR", "NE", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Wir Menschen sollen uns als N\u00e4chsten alle lieben,", "tokens": ["Wir", "Men\u00b7schen", "sol\u00b7len", "uns", "als", "N\u00e4chs\u00b7ten", "al\u00b7le", "lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "VMFIN", "PPER", "KOUS", "NN", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Und also hindert dich gar nicht dein Closter-Stand.", "tokens": ["Und", "al\u00b7so", "hin\u00b7dert", "dich", "gar", "nicht", "dein", "Clos\u00b7ter\u00b7Stand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADV", "PTKNEG", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Vielleicht ist von der Welt dir noch was \u00fcbrig blieben,", "tokens": ["Viel\u00b7leicht", "ist", "von", "der", "Welt", "dir", "noch", "was", "\u00fcb\u00b7rig", "blie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "ART", "NN", "PPER", "ADV", "PWS", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Dein artger Spa\u00df und Schertz ist mir gar wohl bekandt.", "tokens": ["Dein", "art\u00b7ger", "Spa\u00df", "und", "Schertz", "ist", "mir", "gar", "wohl", "be\u00b7kandt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "KON", "NN", "VAFIN", "PPER", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Indessen bin ich dir vor deinen Wunsch verbunden,", "tokens": ["In\u00b7des\u00b7sen", "bin", "ich", "dir", "vor", "dei\u00b7nen", "Wunsch", "ver\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Den deine Feder mir zum neuen Jahr geweyht.", "tokens": ["Den", "dei\u00b7ne", "Fe\u00b7der", "mir", "zum", "neu\u00b7en", "Jahr", "ge\u00b7weyht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "PPER", "APPRART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Ich w\u00fcnsche dir daf\u00fcr so viel vergn\u00fcgte Stunden,", "tokens": ["Ich", "w\u00fcn\u00b7sche", "dir", "da\u00b7f\u00fcr", "so", "viel", "ver\u00b7gn\u00fcg\u00b7te", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PAV", "ADV", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Als Flora hier und dar schon Blumen ausgestreut.", "tokens": ["Als", "Flo\u00b7ra", "hier", "und", "dar", "schon", "Blu\u00b7men", "aus\u00b7ge\u00b7streut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADV", "KON", "PAV", "ADV", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Was neues wei\u00df ich dir vor di\u00dfmahl nicht zu schreiben,", "tokens": ["Was", "neu\u00b7es", "wei\u00df", "ich", "dir", "vor", "di\u00df\u00b7mahl", "nicht", "zu", "schrei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJA", "VVFIN", "PPER", "PPER", "APPR", "ADV", "PTKNEG", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Denn es pa\u00dfiret nichts, das meldens w\u00fcrdig w\u00e4r,", "tokens": ["Denn", "es", "pa\u00b7\u00dfi\u00b7ret", "nichts", ",", "das", "mel\u00b7dens", "w\u00fcr\u00b7dig", "w\u00e4r", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PIS", "$,", "PRELS", "ADV", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Die Welt l\u00e4sts immer noch beym alten Conto bleiben,", "tokens": ["Die", "Welt", "l\u00e4sts", "im\u00b7mer", "noch", "beym", "al\u00b7ten", "Con\u00b7to", "blei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADV", "APPRART", "ADJA", "NE", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Ich gebe, wie du weist, Hist\u00f6rgen nicht Geh\u00f6r.", "tokens": ["Ich", "ge\u00b7be", ",", "wie", "du", "weist", ",", "His\u00b7t\u00f6r\u00b7gen", "nicht", "Ge\u00b7h\u00f6r", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "PPER", "VVFIN", "$,", "NN", "PTKNEG", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Du fragst, ob mich noch nicht ein Zauber-Blick gefangen?", "tokens": ["Du", "fragst", ",", "ob", "mich", "noch", "nicht", "ein", "Zau\u00b7ber\u00b7Blick", "ge\u00b7fan\u00b7gen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "ADV", "PTKNEG", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Ich lache, da\u00df man mir von solchen Sachen schreibt,", "tokens": ["Ich", "la\u00b7che", ",", "da\u00df", "man", "mir", "von", "sol\u00b7chen", "Sa\u00b7chen", "schreibt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PIS", "PPER", "APPR", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Du weist ja, da\u00df ich stets des Amors Strick entgangen,", "tokens": ["Du", "weist", "ja", ",", "da\u00df", "ich", "stets", "des", "A\u00b7mors", "Strick", "ent\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "KOUS", "PPER", "ADV", "ART", "NE", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Und da\u00df mein Hertze frey und ungebunden bleibt.", "tokens": ["Und", "da\u00df", "mein", "Hert\u00b7ze", "frey", "und", "un\u00b7ge\u00b7bun\u00b7den", "bleibt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPOSAT", "VVFIN", "ADJD", "KON", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Ein Blick kan meinen Geist nicht, wie man denckt, verf\u00fchren,", "tokens": ["Ein", "Blick", "kan", "mei\u00b7nen", "Geist", "nicht", ",", "wie", "man", "denckt", ",", "ver\u00b7f\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPOSAT", "NN", "PTKNEG", "$,", "PWAV", "PIS", "VVFIN", "$,", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Ein Seuffzer legt mir nicht gleich Band und Fesseln an,", "tokens": ["Ein", "Seuff\u00b7zer", "legt", "mir", "nicht", "gleich", "Band", "und", "Fes\u00b7seln", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKNEG", "ADV", "NN", "KON", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Mein Auge l\u00e4st der Welt nie Brand und Feuer sp\u00fchren,", "tokens": ["Mein", "Au\u00b7ge", "l\u00e4st", "der", "Welt", "nie", "Brand", "und", "Feu\u00b7er", "sp\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN", "ADV", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Und Amor, welchen ich nicht um mich leiden kan,", "tokens": ["Und", "A\u00b7mor", ",", "wel\u00b7chen", "ich", "nicht", "um", "mich", "lei\u00b7den", "kan", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "$,", "PWAT", "PPER", "PTKNEG", "APPR", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Flieht, wann er mich erblickt, er ist mir nicht gewogen,", "tokens": ["Flieht", ",", "wann", "er", "mich", "er\u00b7blickt", ",", "er", "ist", "mir", "nicht", "ge\u00b7wo\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "PPER", "PRF", "VVPP", "$,", "PPER", "VAFIN", "PPER", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Sein sonst so m\u00e4chtiges Gescho\u00df ist ohne Krafft.", "tokens": ["Sein", "sonst", "so", "m\u00e4ch\u00b7ti\u00b7ges", "Ge\u00b7scho\u00df", "ist", "oh\u00b7ne", "Krafft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADV", "ADV", "ADJA", "NN", "VAFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Du weist ja, eh du noch von uns hier weggezogen,", "tokens": ["Du", "weist", "ja", ",", "eh", "du", "noch", "von", "uns", "hier", "weg\u00b7ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "KOUS", "PPER", "ADV", "APPR", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "War diese Vanit\u00e4t bey mir l\u00e4ngst abgeschafft.", "tokens": ["War", "die\u00b7se", "Va\u00b7ni\u00b7t\u00e4t", "bey", "mir", "l\u00e4ngst", "ab\u00b7ge\u00b7schafft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDAT", "NN", "APPR", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Inzwischen lebe wohl und schreibe mir bald wieder.", "tokens": ["I\u00b7nzwi\u00b7schen", "le\u00b7be", "wohl", "und", "schrei\u00b7be", "mir", "bald", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "KON", "VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "H\u00f6r ich, da\u00df Hymen dir bald l\u00e4st den Braut-Crantz weyhn,", "tokens": ["H\u00f6r", "ich", ",", "da\u00df", "Hy\u00b7men", "dir", "bald", "l\u00e4st", "den", "Braut\u00b7Crantz", "weyhn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "$,", "KOUS", "NN", "PPER", "ADV", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "So will ich, ha\u00dft du nicht aus Eckel meine Lieder,", "tokens": ["So", "will", "ich", ",", "ha\u00dft", "du", "nicht", "aus", "E\u00b7ckel", "mei\u00b7ne", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "$,", "VVFIN", "PPER", "PTKNEG", "APPR", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Mit tausend Freuden dir ein Hochzeit-Carmen streun.", "tokens": ["Mit", "tau\u00b7send", "Freu\u00b7den", "dir", "ein", "Hoch\u00b7zeit\u00b7Car\u00b7men", "streun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}