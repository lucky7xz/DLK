{"dta.poem.8650": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1832", "urn": "urn:nbn:de:kobv:b4-200905193572", "language": ["de:0.99"], "booktitle": "Lenau, Nikolaus: Gedichte. Stuttgart, 1832."}, "poem": {"stanza.1": {"line.1": {"text": "Einsamkeit! mein stilles Weinen", "tokens": ["Ein\u00b7sam\u00b7keit", "!", "mein", "stil\u00b7les", "Wei\u00b7nen"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$.", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Rinnt so hei\u00df in deinen Schoos;", "tokens": ["Rinnt", "so", "hei\u00df", "in", "dei\u00b7nen", "Schoos", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch du schweigst, und hast nicht einen", "tokens": ["Doch", "du", "schweigst", ",", "und", "hast", "nicht", "ei\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "$,", "KON", "VAFIN", "PTKNEG", "ART"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Seufzer f\u00fcr mein tr\u00fcbes Loos!", "tokens": ["Seuf\u00b7zer", "f\u00fcr", "mein", "tr\u00fc\u00b7bes", "Loos", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Legen schon die Jugendjahre", "tokens": ["Le\u00b7gen", "schon", "die", "Ju\u00b7gend\u00b7jah\u00b7re"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Abgebl\u00fcht mich auf die Bahre,", "tokens": ["Ab\u00b7ge\u00b7bl\u00fcht", "mich", "auf", "die", "Bah\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Wird kein Auge feuchten sich?", "tokens": ["Wird", "kein", "Au\u00b7ge", "feuch\u00b7ten", "sich", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "VVFIN", "PRF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Wenn sie mich zu Grabe tragen,", "tokens": ["Wenn", "sie", "mich", "zu", "Gra\u00b7be", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "APPR", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Wird kein Busen b\u00e4nger schlagen?", "tokens": ["Wird", "kein", "Bu\u00b7sen", "b\u00e4n\u00b7ger", "schla\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Liebt kein Herz auf Erden mich? \u2014", "tokens": ["Liebt", "kein", "Herz", "auf", "Er\u00b7den", "mich", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PIAT", "NN", "APPR", "NN", "PPER", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Hei\u00dfer str\u00f6mt es von der Wange:", "tokens": ["Hei\u00b7\u00dfer", "str\u00f6mt", "es", "von", "der", "Wan\u00b7ge", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Keines, keines! f\u00fchl' ich bange.", "tokens": ["Kei\u00b7nes", ",", "kei\u00b7nes", "!", "f\u00fchl'", "ich", "ban\u00b7ge", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "PIS", "$.", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}