{"dta.poem.7855": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Vorwort .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.99"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "Lieder sind wir, unser Vater", "tokens": ["Lie\u00b7der", "sind", "wir", ",", "un\u00b7ser", "Va\u00b7ter"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "VAFIN", "PPER", "$,", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schickt uns in die offne Welt,", "tokens": ["Schickt", "uns", "in", "die", "off\u00b7ne", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Auf dem kritischen Theater", "tokens": ["Auf", "dem", "kri\u00b7ti\u00b7schen", "The\u00b7a\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hat er uns zur Schau gestellt.", "tokens": ["Hat", "er", "uns", "zur", "Schau", "ge\u00b7stellt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PRF", "APPRART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Nennt es denn kein frech Erk\u00fchnen,", "tokens": ["Nennt", "es", "denn", "kein", "frech", "Er\u00b7k\u00fch\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PIAT", "ADJD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Leiht uns ein geneigtes Ohr,", "tokens": ["Leiht", "uns", "ein", "ge\u00b7neig\u00b7tes", "Ohr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wenn wir gern vor euch Versammelten", "tokens": ["Wenn", "wir", "gern", "vor", "euch", "Ver\u00b7sam\u00b7mel\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "APPR", "PPER", "NN"], "meter": "-+-++----", "measure": "unknown.measure.tri"}, "line.8": {"text": "Ein empfehlend Vorwort stammelten!", "tokens": ["Ein", "emp\u00b7feh\u00b7lend", "Vor\u00b7wort", "stam\u00b7mel\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VVFIN", "$."], "meter": "+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.9": {"text": "Sprach doch auf den griech\u2019schen B\u00fchnen", "tokens": ["Sprach", "doch", "auf", "den", "griech'\u00b7schen", "B\u00fch\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Einst sogar der Fr\u00f6sche Chor.", "tokens": ["Einst", "so\u00b7gar", "der", "Fr\u00f6\u00b7sche", "Chor", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Anfangs sind wir fast zu kl\u00e4glich,", "tokens": ["An\u00b7fangs", "sind", "wir", "fast", "zu", "kl\u00e4g\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "PTKA", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Str\u00f6men endlos Thr\u00e4nen aus,", "tokens": ["Str\u00f6\u00b7men", "end\u00b7los", "Thr\u00e4\u00b7nen", "aus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Leben d\u00fcnkt uns zu allt\u00e4glich,", "tokens": ["Le\u00b7ben", "d\u00fcnkt", "uns", "zu", "all\u00b7t\u00e4g\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PTKA", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sterben mu\u00df uns Mann und Maus.", "tokens": ["Ster\u00b7ben", "mu\u00df", "uns", "Mann", "und", "Maus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Doch man will von Jugend sagen,", "tokens": ["Doch", "man", "will", "von", "Ju\u00b7gend", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VMFIN", "APPR", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Die von Leben \u00fcberschwillt;", "tokens": ["Die", "von", "Le\u00b7ben", "\u00fc\u00b7bersc\u00b7hwillt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Auch die Rebe weint, die bl\u00fchende,", "tokens": ["Auch", "die", "Re\u00b7be", "weint", ",", "die", "bl\u00fc\u00b7hen\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "$,", "ART", "ADJA", "$,"], "meter": "+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.8": {"text": "Draus der Wein, der purpurgl\u00fchende,", "tokens": ["Draus", "der", "Wein", ",", "der", "pur\u00b7pur\u00b7gl\u00fc\u00b7hen\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.9": {"text": "In des reifen Herbstes Tagen,", "tokens": ["In", "des", "rei\u00b7fen", "Herbs\u00b7tes", "Ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Kraft und Freude gebend, quillt.", "tokens": ["Kraft", "und", "Freu\u00b7de", "ge\u00b7bend", ",", "quillt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "KON", "NN", "VVPP", "$,", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Und, bei Seite mit dem Prahlen!", "tokens": ["Und", ",", "bei", "Sei\u00b7te", "mit", "dem", "Prah\u00b7len", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "APPR", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Andre stehn genug zur Schau,", "tokens": ["And\u00b7re", "stehn", "ge\u00b7nug", "zur", "Schau", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denen heisse Mittagsstralen", "tokens": ["De\u00b7nen", "heis\u00b7se", "Mit\u00b7tags\u00b7stra\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["PDS", "VVFIN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Abgeleckt den Wehmuthsthau.", "tokens": ["Ab\u00b7ge\u00b7leckt", "den", "Weh\u00b7muth\u00b7sthau", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wie bei alten Ritterfesten", "tokens": ["Wie", "bei", "al\u00b7ten", "Rit\u00b7ter\u00b7fes\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Mit dem Tode zog Hanswurst,", "tokens": ["Mit", "dem", "To\u00b7de", "zog", "Hans\u00b7wurst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Also folgen scherzhaft spitzige", "tokens": ["Al\u00b7so", "fol\u00b7gen", "scherz\u00b7haft", "spit\u00b7zi\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADJD", "ADJA"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Und, will\u2019s Gott! ertr\u00e4glich witzige.", "tokens": ["Und", ",", "will's", "Gott", "!", "er\u00b7tr\u00e4g\u00b7lich", "wit\u00b7zi\u00b7ge", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "$,", "VMFIN", "NN", "$.", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.9": {"text": "Aechtes Leid spa\u00dft oft zum besten,", "tokens": ["A\u00b7ech\u00b7tes", "Leid", "spa\u00dft", "oft", "zum", "bes\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "ADV", "APPRART", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Kennt nicht eiteln Thr\u00e4nendurst.", "tokens": ["Kennt", "nicht", "ei\u00b7teln", "Thr\u00e4\u00b7nen\u00b7durst", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Lieder sind wir nur, Romanzen,", "tokens": ["Lie\u00b7der", "sind", "wir", "nur", ",", "Ro\u00b7man\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ADV", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Alles nur von leichtem Schlag,", "tokens": ["Al\u00b7les", "nur", "von", "leich\u00b7tem", "Schlag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie man\u2019s singen oder tanzen,", "tokens": ["Wie", "man's", "sin\u00b7gen", "o\u00b7der", "tan\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VVINF", "KON", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Pfeifen oder klimpern mag.", "tokens": ["Pfei\u00b7fen", "o\u00b7der", "klim\u00b7pern", "mag."], "token_info": ["word", "word", "word", "abbreviation"], "pos": ["NN", "KON", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Doch vielleicht wer stillem Deuten", "tokens": ["Doch", "viel\u00b7leicht", "wer", "stil\u00b7lem", "Deu\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "PWS", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Nachzugehen sich bem\u00fcht,", "tokens": ["Nach\u00b7zu\u00b7ge\u00b7hen", "sich", "be\u00b7m\u00fcht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PRF", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Ahnt in einzelen Gestaltungen", "tokens": ["Ahnt", "in", "ein\u00b7ze\u00b7len", "Ge\u00b7stal\u00b7tun\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "PIAT", "NN"], "meter": "+-+---++-", "measure": "unknown.measure.tetra"}, "line.8": {"text": "Gr\u00f6\u00dferen Gedichts Entfaltungen", "tokens": ["Gr\u00f6\u00b7\u00dfe\u00b7ren", "Ge\u00b7dichts", "Ent\u00b7fal\u00b7tun\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "NN"], "meter": "+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.9": {"text": "Und als Einheit im Zerstreuten", "tokens": ["Und", "als", "Ein\u00b7heit", "im", "Zer\u00b7streu\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "NN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Unsres Dichters ganz Gem\u00fcth.", "tokens": ["Uns\u00b7res", "Dich\u00b7ters", "ganz", "Ge\u00b7m\u00fcth", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Bleibt euch dennoch Manches kleinlich,", "tokens": ["Bleibt", "euch", "den\u00b7noch", "Man\u00b7ches", "klein\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PIS", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nehmt\u2019s f\u00fcr Zeichen jener Zeit,", "tokens": ["Nehmt's", "f\u00fcr", "Zei\u00b7chen", "je\u00b7ner", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "NN", "PDAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Die so dr\u00fcckend und so peinlich", "tokens": ["Die", "so", "dr\u00fc\u00b7ckend", "und", "so", "pein\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ADJD", "KON", "ADV", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Alles Leben eingeschneit!", "tokens": ["Al\u00b7les", "Le\u00b7ben", "ein\u00b7ge\u00b7schneit", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Fehlt das \u00e4u\u00dfre freie Wesen,", "tokens": ["Fehlt", "das", "\u00e4u\u00df\u00b7re", "frei\u00b7e", "We\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Leicht erkrankt auch das Gedicht;", "tokens": ["Leicht", "er\u00b7krankt", "auch", "das", "Ge\u00b7dicht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Aber nun die hingemoderte", "tokens": ["A\u00b7ber", "nun", "die", "hin\u00b7ge\u00b7mo\u00b7der\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Freiheit Deutschlands frisch aufloderte,", "tokens": ["Frei\u00b7heit", "Deutschlands", "frisch", "auf\u00b7lo\u00b7der\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "ADJD", "VVFIN", "$,"], "meter": "+-++-+--", "measure": "unknown.measure.tetra"}, "line.9": {"text": "Wird zugleich das Lied genesen,", "tokens": ["Wird", "zu\u00b7gleich", "das", "Lied", "ge\u00b7ne\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Kr\u00e4ftig steigen an das Licht.", "tokens": ["Kr\u00e4f\u00b7tig", "stei\u00b7gen", "an", "das", "Licht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Seyen denn auch wir Verk\u00fcnder", "tokens": ["Se\u00b7yen", "denn", "auch", "wir", "Ver\u00b7k\u00fcn\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ADV", "PPER", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Einer j\u00fcngern Br\u00fcderschaar,", "tokens": ["Ei\u00b7ner", "j\u00fcn\u00b7gern", "Br\u00fc\u00b7der\u00b7schaar", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Deren Bau und Wuchs ges\u00fcnder,", "tokens": ["De\u00b7ren", "Bau", "und", "Wuchs", "ge\u00b7s\u00fcn\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "KON", "NN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "H\u00f6her sey, als unsrer war!", "tokens": ["H\u00f6\u00b7her", "sey", ",", "als", "uns\u00b7rer", "war", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "$,", "KOUS", "PPER", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Dies ist, was wir nicht geloben,", "tokens": ["Dies", "ist", ",", "was", "wir", "nicht", "ge\u00b7lo\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "$,", "PRELS", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.6": {"text": "Nein! vom Himmel nur erflehn.", "tokens": ["Nein", "!", "vom", "Him\u00b7mel", "nur", "er\u00b7flehn", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "APPRART", "NN", "ADV", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und ihr selbst ja seyd Vern\u00fcnftige,", "tokens": ["Und", "ihr", "selbst", "ja", "seyd", "Ver\u00b7n\u00fcnf\u00b7ti\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "ADV", "VAFIN", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Die im Jetzt erschaun das K\u00fcnftige,", "tokens": ["Die", "im", "Jetzt", "er\u00b7schaun", "das", "K\u00fcnf\u00b7ti\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.9": {"text": "Die an junger Saat erproben,", "tokens": ["Die", "an", "jun\u00b7ger", "Saat", "er\u00b7pro\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.10": {"text": "Wie die Frucht einst wird bestehn.", "tokens": ["Wie", "die", "Frucht", "einst", "wird", "be\u00b7stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADV", "VAFIN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}