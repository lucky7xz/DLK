{"dta.poem.7495": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Byron's Sonett an Chillon.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192430", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Dein himmlisch Lied \u2014 es hat schon manche Labe", "tokens": ["Dein", "himm\u00b7lisch", "Lied", "es", "hat", "schon", "man\u00b7che", "La\u00b7be"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJD", "NN", "$(", "PPER", "VAFIN", "ADV", "PIAT", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "In schwarzen d\u00fcstern Stunden mir bereitet,", "tokens": ["In", "schwar\u00b7zen", "d\u00fcs\u00b7tern", "Stun\u00b7den", "mir", "be\u00b7rei\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "VVFIN", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und wie den J\u00fcngling treulich Du begleitet,", "tokens": ["Und", "wie", "den", "J\u00fcng\u00b7ling", "treu\u00b7lich", "Du", "be\u00b7glei\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "VVFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "So freute Dein sich schon der wilde Knabe.", "tokens": ["So", "freu\u00b7te", "Dein", "sich", "schon", "der", "wil\u00b7de", "Kna\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "PRF", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Die Besten haben \u00fcber Deinem Grabe", "tokens": ["Die", "Bes\u00b7ten", "ha\u00b7ben", "\u00fc\u00b7ber", "Dei\u00b7nem", "Gra\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wetteifernd Lorberkr\u00e4nze hingebreitet,", "tokens": ["Wett\u00b7ei\u00b7fernd", "Lor\u00b7ber\u00b7kr\u00e4n\u00b7ze", "hin\u00b7ge\u00b7brei\u00b7tet", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ach! wo ein Lob das andre niederstreitet,", "tokens": ["Ach", "!", "wo", "ein", "Lob", "das", "and\u00b7re", "nie\u00b7der\u00b7strei\u00b7tet", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PWAV", "ART", "NN", "ART", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wie wenig ist's, was ich zu bieten habe!", "tokens": ["Wie", "we\u00b7nig", "ist's", ",", "was", "ich", "zu", "bie\u00b7ten", "ha\u00b7be", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VAFIN", "$,", "PWS", "PPER", "PTKZU", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wenn ich mich zu Sonettendichtern wende,", "tokens": ["Wenn", "ich", "mich", "zu", "So\u00b7net\u00b7ten\u00b7dich\u00b7tern", "wen\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die auch die Reime str\u00e4ubend nur verschlungen,", "tokens": ["Die", "auch", "die", "Rei\u00b7me", "str\u00e4u\u00b7bend", "nur", "ver\u00b7schlun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "ADJD", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Seh' ich vor Allem G\u00f6the's kleine Spende;", "tokens": ["Seh'", "ich", "vor", "Al\u00b7lem", "G\u00f6\u00b7the's", "klei\u00b7ne", "Spen\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PIAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Doch hat er nicht, wie ", "tokens": ["Doch", "hat", "er", "nicht", ",", "wie"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["KON", "VAFIN", "PPER", "PTKNEG", "$,", "PWAV"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Der seines Liebens Anfang noch und Ende,", "tokens": ["Der", "sei\u00b7nes", "Lie\u00b7bens", "An\u00b7fang", "noch", "und", "En\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "NN", "ADV", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der noch die ", "tokens": ["Der", "noch", "die"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADV", "ART"], "meter": "+-+", "measure": "trochaic.di"}}}}}