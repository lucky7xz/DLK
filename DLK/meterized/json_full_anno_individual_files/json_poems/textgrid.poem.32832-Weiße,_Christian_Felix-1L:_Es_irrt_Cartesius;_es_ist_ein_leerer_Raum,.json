{"textgrid.poem.32832": {"metadata": {"author": {"name": "Wei\u00dfe, Christian Felix", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es irrt Cartesius; es ist ein leerer Raum,", "genre": "verse", "period": "N.A.", "pub_year": 1765, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es irrt Cartesius; es ist ein leerer Raum,", "tokens": ["Es", "irrt", "Car\u00b7te\u00b7sius", ";", "es", "ist", "ein", "lee\u00b7rer", "Raum", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "$.", "PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ich sehs an meinem Magen:", "tokens": ["Ich", "sehs", "an", "mei\u00b7nem", "Ma\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Denn dieser kann das Leere kaum", "tokens": ["Denn", "die\u00b7ser", "kann", "das", "Lee\u00b7re", "kaum"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PDS", "VMFIN", "ART", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Minuten lang vertragen:", "tokens": ["Mi\u00b7nu\u00b7ten", "lang", "ver\u00b7tra\u00b7gen", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Er will mit ausgesuchtem Wein", "tokens": ["Er", "will", "mit", "aus\u00b7ge\u00b7such\u00b7tem", "Wein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Von oben angef\u00fcllet seyn,", "tokens": ["Von", "o\u00b7ben", "an\u00b7ge\u00b7f\u00fcl\u00b7let", "seyn", ","], "token_info