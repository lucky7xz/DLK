{"textgrid.poem.33324": {"metadata": {"author": {"name": "Blumauer, Aloys", "birth": "N.A.", "death": "N.A."}, "title": "Illussion und Gr\u00fcbelei", "genre": "verse", "period": "N.A.", "pub_year": 1776, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O geniesset!", "tokens": ["O", "ge\u00b7nies\u00b7set", "!"], "token_info": ["word", "word", "punct"], "pos": ["NE", "VVPP", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Lernt euch freu'n!", "tokens": ["Lernt", "euch", "freu'n", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVINF", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Liebe giesset", "tokens": ["Lie\u00b7be", "gies\u00b7set"], "token_info": ["word", "word"], "pos": ["NN", "VVFIN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Sonnenschein", "tokens": ["Son\u00b7nen\u00b7schein"], "token_info": ["word"], "pos": ["NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Auf die Wege", "tokens": ["Auf", "die", "We\u00b7ge"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Liebender;", "tokens": ["Lie\u00b7ben\u00b7der", ";"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+--", "measure": "dactylic.init"}, "line.7": {"text": "Thut noch mehr:", "tokens": ["Thut", "noch", "mehr", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADV", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.8": {"text": "Spornt das tr\u00e4ge", "tokens": ["Spornt", "das", "tr\u00e4\u00b7ge"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ART", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.9": {"text": "Rad der Zeit,", "tokens": ["Rad", "der", "Zeit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "+