{"dta.poem.18612": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "D er  H errn  - - -  Symbolum  \n  wegen ihrer Weiber.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1702", "urn": "urn:nbn:de:kobv:b4-200905197766", "language": ["de:0.57", "af:0.42"], "booktitle": "Hunold, Christian Friedrich: Die Edle Bem\u00fchung m\u00fcssiger Stunden. Hamburg, 1702."}, "poem": {"stanza.1": {"line.1": {"text": "So kan die Gedult bekr\u00f6nen", "tokens": ["So", "kan", "die", "Ge\u00b7dult", "be\u00b7kr\u00f6\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ART", "NN", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und so wird ein Argus blind/", "tokens": ["Und", "so", "wird", "ein", "Ar\u00b7gus", "blind", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "ART", "NN", "ADJD", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Drum so dencken die mich h\u00f6hnen", "tokens": ["Drum", "so", "den\u00b7cken", "die", "mich", "h\u00f6h\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "ADV", "VVFIN", "ART", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df wir wohl geschw\u00e4gert sind.", "tokens": ["Da\u00df", "wir", "wohl", "ge\u00b7schw\u00e4\u00b7gert", "sind", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVPP", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Denn die neuste Mod' auf Erden", "tokens": ["Denn", "die", "neus\u00b7te", "Mod'", "auf", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ist allhier: ein Hahnrey werden.", "tokens": ["Ist", "all\u00b7hier", ":", "ein", "Hahn\u00b7rey", "wer\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "$.", "ART", "NN", "VAINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}}}}