{"textgrid.poem.37658": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Viel Knaben und M\u00e4dlein im Laufe hinauf,", "genre": "verse", "period": "N.A.", "pub_year": 1807, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Viel Knaben und M\u00e4dlein im Laufe hinauf,", "tokens": ["Viel", "Kna\u00b7ben", "und", "M\u00e4d\u00b7lein", "im", "Lau\u00b7fe", "hin\u00b7auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "NN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Am Berge wie Lerchen,", "tokens": ["Am", "Ber\u00b7ge", "wie", "Ler\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KOKOM", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Sie singen: Nun ringelt den Rosenkranz", "tokens": ["Sie", "sin\u00b7gen", ":", "Nun", "rin\u00b7gelt", "den", "Ro\u00b7sen\u00b7kranz"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "ADV", "VVFIN", "ART", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Auf Maien, in Reihen, im Morgenglanz.", "tokens": ["Auf", "Mai\u00b7en", ",", "in", "Rei\u00b7hen", ",", "im", "Mor\u00b7gen\u00b7glanz", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "APPR", "NN", "$,", "APPRART", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.2": {"line.1": {"text": "Die M\u00e4dlein bringen mit Rosen im Schoos,", "tokens": ["Die", "M\u00e4d\u00b7lein", "brin\u00b7gen", "mit", "Ro\u00b7sen", "im", "Schoos", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "APPR", "NN", "APPRART", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Zum Binden und Winden;", "tokens": ["Zum", "Bin\u00b7den", "und", "Win\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Sie binden und winden den Rosenglanz,", "tokens": ["Sie", "bin\u00b7den", "und", "win\u00b7den", "den", "Ro\u00b7sen\u00b7glanz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Zusammen sie stecken mit Dornen den Kranz.", "tokens": ["Zu\u00b7sam\u00b7men", "sie", "ste\u00b7cken", "mit", "Dor\u00b7nen", "den", "Kranz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVFIN", "APPR", "NN", "ART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.3": {"line.1": {"text": "Die Knaben bezwingen die Maien mit Schrein,", "tokens": ["Die", "Kna\u00b7ben", "be\u00b7zwin\u00b7gen", "die", "Mai\u00b7en", "mit", "Schrein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Sie brechen und flechten", "tokens": ["Sie", "bre\u00b7chen", "und", "flech\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVINF", "KON", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Die \u00c4ste zum Kreuze im Sonnenglanz,", "tokens": ["Die", "\u00c4s\u00b7te", "zum", "Kreu\u00b7ze", "im", "Son\u00b7nen\u00b7glanz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "APPRART", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Sie h\u00e4ngen darauf auch den Rosenkranz.", "tokens": ["Sie", "h\u00e4n\u00b7gen", "da\u00b7rauf", "auch", "den", "Ro\u00b7sen\u00b7kranz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PAV", "ADV", "ART", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.4": {"line.1": {"text": "Von Knaben und M\u00e4dlein der Wald erschallt,", "tokens": ["Von", "Kna\u00b7ben", "und", "M\u00e4d\u00b7lein", "der", "Wald", "er\u00b7schallt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Sie reichen mit Schreien,", "tokens": ["Sie", "rei\u00b7chen", "mit", "Schrei\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Ja Ringel, Ringel, Rosenkranz,", "tokens": ["Ja", "Rin\u00b7gel", ",", "Rin\u00b7gel", ",", "Ro\u00b7sen\u00b7kranz", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PTKANT", "NN", "$,", "NN", "$,", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sie singen und tanzen im Morgenglanz.", "tokens": ["Sie", "sin\u00b7gen", "und", "tan\u00b7zen", "im", "Mor\u00b7gen\u00b7glanz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "APPRART", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.5": {"line.1": {"text": "Da sehet die Kreuze auf H\u00f6hen hell stehn,", "tokens": ["Da", "se\u00b7het", "die", "Kreu\u00b7ze", "auf", "H\u00f6\u00b7hen", "hell", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "NN", "ADJD", "VVINF", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Zum Freuen an Maien:", "tokens": ["Zum", "Freu\u00b7en", "an", "Mai\u00b7en", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "NE", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Die Knaben und M\u00e4dlein auf Rasen gr\u00fcn,", "tokens": ["Die", "Kna\u00b7ben", "und", "M\u00e4d\u00b7lein", "auf", "Ra\u00b7sen", "gr\u00fcn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "APPR", "NN", "ADJD", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Sie ringeln und reihen, sich niederziehn.", "tokens": ["Sie", "rin\u00b7geln", "und", "rei\u00b7hen", ",", "sich", "nie\u00b7der\u00b7ziehn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "$,", "PRF", "VVINF", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.6": {"line.1": {"text": "Ein Ritter sie schauet, die Brust voll Lust,", "tokens": ["Ein", "Rit\u00b7ter", "sie", "schau\u00b7et", ",", "die", "Brust", "voll", "Lust", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVFIN", "$,", "ART", "NN", "ADJD", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Sie lobt und gelobet", "tokens": ["Sie", "lobt", "und", "ge\u00b7lo\u00b7bet"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVFIN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Zu bauen ein Kloster dem Rosenkranz,", "tokens": ["Zu", "bau\u00b7en", "ein", "Klos\u00b7ter", "dem", "Ro\u00b7sen\u00b7kranz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "ART", "NN", "ART", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Da sollten sie beten bei Ampelnglanz.", "tokens": ["Da", "soll\u00b7ten", "sie", "be\u00b7ten", "bei", "Am\u00b7peln\u00b7glanz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVFIN", "APPR", "NE", "$."], "meter": "-+--+--++-", "measure": "iambic.tetra.relaxed"}}, "stanza.7": {"line.1": {"text": "\u00bbein Kreuz in die Welt zu hauen, ja schaut,", "tokens": ["\u00bb", "ein", "Kreuz", "in", "die", "Welt", "zu", "hau\u00b7en", ",", "ja", "schaut", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "APPR", "ART", "NN", "PTKZU", "VVINF", "$,", "ADV", "VVFIN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Mein Schwert es euch lehrt.", "tokens": ["Mein", "Schwert", "es", "euch", "lehrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "PPER", "VVFIN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "In \u00f6stlichem, westlichen, sonnigen Glanz,", "tokens": ["In", "\u00f6st\u00b7li\u00b7chem", ",", "west\u00b7li\u00b7chen", ",", "son\u00b7ni\u00b7gen", "Glanz", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "$,", "VVFIN", "$,", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Darum ich es hier in die Erde euch pflanz.\u00ab", "tokens": ["Da\u00b7rum", "ich", "es", "hier", "in", "die", "Er\u00b7de", "euch", "pflanz", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PAV", "PPER", "PPER", "ADV", "APPR", "ART", "NN", "PPER", "NE", "$.", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.8": {"line.1": {"text": "\u00bbdas wurzelt und treibet wie balde zum Wald,", "tokens": ["\u00bb", "das", "wur\u00b7zelt", "und", "trei\u00b7bet", "wie", "bal\u00b7de", "zum", "Wald", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PDS", "VVFIN", "KON", "VVFIN", "KOKOM", "ADV", "APPRART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Es gl\u00fchet und bl\u00fchet,", "tokens": ["Es", "gl\u00fc\u00b7het", "und", "bl\u00fc\u00b7het", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Die Rosen umsprossen die Scheide mit Glanz.", "tokens": ["Die", "Ro\u00b7sen", "um\u00b7spros\u00b7sen", "die", "Schei\u00b7de", "mit", "Glanz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Sie kn\u00fcpfen am Hefte den ewigen Kranz.\u00ab", "tokens": ["Sie", "kn\u00fcp\u00b7fen", "am", "Hef\u00b7te", "den", "e\u00b7wi\u00b7gen", "Kranz", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.9": {"line.1": {"text": "Die Knaben darauf es so sch\u00f6ne ansehn,", "tokens": ["Die", "Kna\u00b7ben", "da\u00b7rauf", "es", "so", "sch\u00f6\u00b7ne", "an\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PAV", "PPER", "ADV", "ADJA", "VVINF", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Sie sagen und klagen:", "tokens": ["Sie", "sa\u00b7gen", "und", "kla\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "KON", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "\u00bbdas bl\u00fchet ja nimmer in Rosenglanz,", "tokens": ["\u00bb", "das", "bl\u00fc\u00b7het", "ja", "nim\u00b7mer", "in", "Ro\u00b7sen\u00b7glanz", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PDS", "VVFIN", "ADV", "ADV", "APPR", "NE", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Wir sehn nur vier Spitzen und blutigen Glanz.\u00ab", "tokens": ["Wir", "sehn", "nur", "vier", "Spit\u00b7zen", "und", "blu\u00b7ti\u00b7gen", "Glanz", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "ADV", "CARD", "NN", "KON", "ADJA", "NN", "$.", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.10": {"line.1": {"text": "Der Ritter will tanzen, der Stahl zur Qual", "tokens": ["Der", "Rit\u00b7ter", "will", "tan\u00b7zen", ",", "der", "Stahl", "zur", "Qual"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "VVINF", "$,", "ART", "NN", "APPRART", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Dr\u00fcckt nieder die Glieder:", "tokens": ["Dr\u00fcckt", "nie\u00b7der", "die", "Glie\u00b7der", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Die Kinder, sie singen zum Rosenkranz,", "tokens": ["Die", "Kin\u00b7der", ",", "sie", "sin\u00b7gen", "zum", "Ro\u00b7sen\u00b7kranz", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Du steifer Geselle bleib weg von dem Tanz.", "tokens": ["Du", "stei\u00b7fer", "Ge\u00b7sel\u00b7le", "bleib", "weg", "von", "dem", "Tanz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "VVFIN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.11": {"line.1": {"text": "Ein Weiser das Kreuz von ferne sieht gern,", "tokens": ["Ein", "Wei\u00b7ser", "das", "Kreuz", "von", "fer\u00b7ne", "sieht", "gern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "APPR", "ADV", "VVFIN", "ADV", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Er lehret: \u00bbJa h\u00f6ret!\u00ab", "tokens": ["Er", "leh\u00b7ret", ":", "\u00bb", "Ja", "h\u00f6\u00b7ret", "!", "\u00ab"], "token_info": ["word", "word", "punct", "punct", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "$.", "$(", "PTKANT", "VVFIN", "$.", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Vier Temperamente und Element,", "tokens": ["Vier", "Tem\u00b7pe\u00b7ra\u00b7men\u00b7te", "und", "E\u00b7le\u00b7ment", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "KON", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Sie zeigen sich klar in vier Kreuzesend.", "tokens": ["Sie", "zei\u00b7gen", "sich", "klar", "in", "vier", "Kreu\u00b7ze\u00b7send", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADJD", "APPR", "CARD", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.12": {"line.1": {"text": "Die Kinder