{"dta.poem.8830": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "Ix.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Wir m\u00fchn uns, ach! und kommen nicht zusammen:", "tokens": ["Wir", "m\u00fchn", "uns", ",", "ach", "!", "und", "kom\u00b7men", "nicht", "zu\u00b7sam\u00b7men", ":"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "$,", "ITJ", "$.", "KON", "VVFIN", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mein Name klingt aus deinem Mund melodisch,", "tokens": ["Mein", "Na\u00b7me", "klingt", "aus", "dei\u00b7nem", "Mund", "me\u00b7lo\u00b7disch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch reihst du selten dies Gedicht zusammen;", "tokens": ["Doch", "reihst", "du", "sel\u00b7ten", "dies", "Ge\u00b7dicht", "zu\u00b7sam\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "VVFIN", "PDS", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wie Sonn' und Mond uns stets getrennt zu halten,", "tokens": ["Wie", "Sonn'", "und", "Mond", "uns", "stets", "ge\u00b7trennt", "zu", "hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "PPER", "ADV", "VVPP", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Verschworen Sitte sich und Pflicht zusammen,", "tokens": ["Ver\u00b7schwo\u00b7ren", "Sit\u00b7te", "sich", "und", "Pflicht", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PRF", "KON", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "La\u00df Haupt an Haupt uns lehnen, denn es taugen", "tokens": ["La\u00df", "Haupt", "an", "Haupt", "uns", "leh\u00b7nen", ",", "denn", "es", "tau\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVIMP", "NN", "APPR", "NN", "PPER", "VVFIN", "$,", "KON", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Dein dunkles Haar, mein hell Gesicht zusammen!", "tokens": ["Dein", "dunk\u00b7les", "Haar", ",", "mein", "hell", "Ge\u00b7sicht", "zu\u00b7sam\u00b7men", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PPOSAT", "ADJD", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Doch ach! ich tr\u00e4ume, denn du ziehst von hinnen,", "tokens": ["Doch", "ach", "!", "ich", "tr\u00e4u\u00b7me", ",", "denn", "du", "ziehst", "von", "hin\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$.", "PPER", "VVFIN", "$,", "KON", "PPER", "VVFIN", "APPR", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Eh' noch das Gl\u00fcck uns brachte dicht zusammen:", "tokens": ["Eh'", "noch", "das", "Gl\u00fcck", "uns", "brach\u00b7te", "dicht", "zu\u00b7sam\u00b7men", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "PPER", "VVFIN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Die Seelen bluten, da getrennt die Leiber,", "tokens": ["Die", "See\u00b7len", "blu\u00b7ten", ",", "da", "ge\u00b7trennt", "die", "Lei\u00b7ber", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ADV", "VVPP", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "O w\u00e4ren's Blumen, die man flicht zusammen!", "tokens": ["O", "w\u00e4\u00b7ren's", "Blu\u00b7men", ",", "die", "man", "flicht", "zu\u00b7sam\u00b7men", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "NN", "$,", "PRELS", "PIS", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}