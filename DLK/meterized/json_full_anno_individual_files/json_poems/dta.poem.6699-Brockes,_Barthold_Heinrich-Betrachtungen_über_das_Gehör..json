{"dta.poem.6699": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Betrachtungen \u00fcber das Geh\u00f6r.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wann dort von Mars gereitzt, zwo feindliche Armeen", "tokens": ["Wann", "dort", "von", "Mars", "ge\u00b7reitzt", ",", "zwo", "feind\u00b7li\u00b7che", "Ar\u00b7me\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "ADV", "APPR", "NN", "VVPP", "$,", "CARD", "ADJA", "NN"], "meter": "-+-+-+-+---+-", "measure": "unknown.measure.penta"}, "line.2": {"text": "Voll frecher Grausamkeit zum k\u00e4mpffen fertig stehen;", "tokens": ["Voll", "fre\u00b7cher", "Grau\u00b7sam\u00b7keit", "zum", "k\u00e4mpf\u00b7fen", "fer\u00b7tig", "ste\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "APPRART", "VVFIN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So fl\u00f6sset blo\u00df ein Wort vom tapfern Capitain", "tokens": ["So", "fl\u00f6s\u00b7set", "blo\u00df", "ein", "Wort", "vom", "tap\u00b7fern", "Ca\u00b7pi\u00b7tain"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Auch selbst den Feigesten Hertz Muth und Feuer ein,", "tokens": ["Auch", "selbst", "den", "Fei\u00b7ge\u00b7sten", "Hertz", "Muth", "und", "Feu\u00b7er", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "NN", "NN", "KON", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und treibt sie kr\u00e4fftiger zum W\u00fcrgen und zum T\u00f6dten,", "tokens": ["Und", "treibt", "sie", "kr\u00e4ff\u00b7ti\u00b7ger", "zum", "W\u00fcr\u00b7gen", "und", "zum", "T\u00f6d\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "APPRART", "NN", "KON", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Als der gesch\u00e4rffte Klang der lermenden Trompeten.", "tokens": ["Als", "der", "ge\u00b7sch\u00e4rff\u00b7te", "Klang", "der", "ler\u00b7men\u00b7den", "Trom\u00b7pe\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+--++-", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "Man f\u00fchlet, wie das Hertz von tapffrer Hitz entbrennt,", "tokens": ["Man", "f\u00fch\u00b7let", ",", "wie", "das", "Hertz", "von", "tapf\u00b7frer", "Hitz", "ent\u00b7brennt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "PWAV", "ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wenn sich der Geist ein Bildni\u00df, mit Vergn\u00fcgen,", "tokens": ["Wenn", "sich", "der", "Geist", "ein", "Bild\u00b7ni\u00df", ",", "mit", "Ver\u00b7gn\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "ART", "NN", "$,", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Blo\u00df aus den Nahmen macht von Ehr und Siegen.", "tokens": ["Blo\u00df", "aus", "den", "Nah\u00b7men", "macht", "von", "Ehr", "und", "Sie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}