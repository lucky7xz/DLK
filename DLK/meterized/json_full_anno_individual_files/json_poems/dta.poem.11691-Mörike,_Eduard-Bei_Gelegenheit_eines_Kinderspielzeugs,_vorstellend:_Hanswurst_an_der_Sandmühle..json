{"dta.poem.11691": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Bei Gelegenheit eines Kinderspielzeugs,  \n vorstellend:  \n  Hanswurst an der Sandm\u00fchle.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905193969", "language": ["de:0.99"], "booktitle": "M\u00f6rike, Eduard: Gedichte. Stuttgart, 1838."}, "poem": {"stanza.1": {"line.1": {"text": "Was, du Schuft?", "tokens": ["Was", ",", "du", "Schuft", "?"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["PWS", "$,", "PPER", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Gott der Herr schl\u00e4gt am lustigen Sommertage", "tokens": ["Gott", "der", "Herr", "schl\u00e4gt", "am", "lus\u00b7ti\u00b7gen", "Som\u00b7mer\u00b7ta\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "+-+--+--+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Seinen bunten Reifen in die Luft, \u2014", "tokens": ["Sei\u00b7nen", "bun\u00b7ten", "Rei\u00b7fen", "in", "die", "Luft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "APPR", "ART", "NN", "$,", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Was guckst du scheel, wenn ich den meinen schlage?", "tokens": ["Was", "guckst", "du", "scheel", ",", "wenn", "ich", "den", "mei\u00b7nen", "schla\u00b7ge", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "VVFIN", "$,", "KOUS", "PPER", "ART", "PPOSAT", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Der eine nuzt so wenig wie der ander',", "tokens": ["Der", "ei\u00b7ne", "nuzt", "so", "we\u00b7nig", "wie", "der", "an\u00b7der'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "ADV", "PIS", "KOKOM", "ART", "PIS", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.6": {"text": "Aber Kinder und Narren sehen's gern.", "tokens": ["A\u00b7ber", "Kin\u00b7der", "und", "Nar\u00b7ren", "se\u00b7hen's", "gern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "ADV", "ADV", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.7": {"text": "Ich bin nicht Bonapart' und bin nicht Alexander,", "tokens": ["Ich", "bin", "nicht", "Bo\u00b7na\u00b7part'", "und", "bin", "nicht", "A\u00b7lex\u00b7an\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "NN", "KON", "VAFIN", "PTKNEG", "NE", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und hab' doch meinen Sparr'n so gut wie diese Herrn.", "tokens": ["Und", "hab'", "doch", "mei\u00b7nen", "Sparr'n", "so", "gut", "wie", "die\u00b7se", "Herrn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "PPOSAT", "NN", "ADV", "ADJD", "KOKOM", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "\u2014 Was f\u00fchrt ihr \u00fcberhaupt so hohen Ton", "tokens": ["Was", "f\u00fchrt", "ihr", "\u00fc\u00b7ber\u00b7haupt", "so", "ho\u00b7hen", "Ton"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "PWS", "VVFIN", "PPER", "ADV", "ADV", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.10": {"text": "Und sch\u00e4mt euch schier, mich auch nur zu belachen?", "tokens": ["Und", "sch\u00e4mt", "euch", "schier", ",", "mich", "auch", "nur", "zu", "be\u00b7la\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "$,", "PPER", "ADV", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Ich sah die ganze w\u00fcrdige Nation", "tokens": ["Ich", "sah", "die", "gan\u00b7ze", "w\u00fcr\u00b7di\u00b7ge", "Na\u00b7tion"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Schon viel possierlichere Spr\u00fcnge machen!", "tokens": ["Schon", "viel", "pos\u00b7sier\u00b7li\u00b7che\u00b7re", "Spr\u00fcn\u00b7ge", "ma\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Aus jezt \u2014 wem sein Kopf lieb ist!", "tokens": ["Aus", "jezt", "wem", "sein", "Kopf", "lieb", "ist", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "$(", "PWS", "PPOSAT", "NN", "ADJD", "VAFIN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}}}}