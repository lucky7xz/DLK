{"dta.poem.9400": {"metadata": {"author": {"name": "Zesen, Philipp von", "birth": "N.A.", "death": "N.A."}, "title": "Vi .  \n  Hertze  \n  Von Dactylischen Versen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-20684-8", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "t\u00e4gliche kl\u00e4gliche", "tokens": ["t\u00e4g\u00b7li\u00b7che", "kl\u00e4g\u00b7li\u00b7che"], "token_info": ["word", "word"], "pos": ["ADJA", "ADJA"], "meter": "+--+--", "measure": "dactylic.di.plus"}, "line.2": {"text": "klage soll heute vorgehen/", "tokens": ["kla\u00b7ge", "soll", "heu\u00b7te", "vor\u00b7ge\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "VMFIN", "ADV", "VVINF", "$("], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "alles soll heute vol fr\u00f6ligkeit stehen/", "tokens": ["al\u00b7les", "soll", "heu\u00b7te", "vol", "fr\u00f6\u00b7lig\u00b7keit", "ste\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "ADV", "ADJD", "NN", "VVFIN", "$("], "meter": "+-+--++--+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "weil jtzo mein Bruder dein Namenstag ist/", "tokens": ["weil", "jt\u00b7zo", "mein", "Bru\u00b7der", "dein", "Na\u00b7mens\u00b7tag", "ist", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPOSAT", "NN", "PPOSAT", "NN", "VAFIN", "$("], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.5": {"text": "Da mann des klagens und zagens vergist/", "tokens": ["Da", "mann", "des", "kla\u00b7gens", "und", "za\u00b7gens", "ver\u00b7gist", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ART", "ADJA", "KON", "ADJA", "NN", "$("], "meter": "---+--+--+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Der Sommer vergehet/", "tokens": ["Der", "Som\u00b7mer", "ver\u00b7ge\u00b7het", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "der Winter entstehet/", "tokens": ["der", "Win\u00b7ter", "ent\u00b7ste\u00b7het", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "frieren und frost/", "tokens": ["frie\u00b7ren", "und", "frost", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "KON", "ADJD", "$("], "meter": "+--+", "measure": "iambic.di.chol"}, "line.9": {"text": "hem\u0303et die kost.", "tokens": ["hem\u0303et", "die", "kost", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}}}}}