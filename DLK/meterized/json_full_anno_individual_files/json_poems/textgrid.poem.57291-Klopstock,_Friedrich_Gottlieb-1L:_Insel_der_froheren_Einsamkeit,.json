{"textgrid.poem.57291": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "1L: Insel der froheren Einsamkeit,", "genre": "verse", "period": "N.A.", "pub_year": 1767, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Insel der froheren Einsamkeit,", "tokens": ["In\u00b7sel", "der", "fro\u00b7he\u00b7ren", "Ein\u00b7sam\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Geliebte Gespielin des Wiederhalls", "tokens": ["Ge\u00b7lieb\u00b7te", "Ge\u00b7spie\u00b7lin", "des", "Wie\u00b7der\u00b7halls"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "NN", "ART", "NN"], "meter": "-+--++-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und des Sees, welcher itzt breit, dann, versteckt", "tokens": ["Und", "des", "Sees", ",", "wel\u00b7cher", "itzt", "breit", ",", "dann", ",", "ver\u00b7steckt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "ADV", "ADJD", "$,", "ADV", "$,", "VVFIN"], "meter": "--++-++--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Wie ein Strom, rauscht an des Walds H\u00fcgeln umher,", "tokens": ["Wie", "ein", "Strom", ",", "rauscht", "an", "des", "Walds", "H\u00fc\u00b7geln", "um\u00b7her", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "VVFIN", "APPR", "ART", "NN", "NN", "PTKVZ", "$,"], "meter": "+-+-+-++--+", "measure": "iambic.hexa.chol"}}, "stanza.2": {"line.1": {"text": "Selber von steigenden H\u00fcgeln voll,", "tokens": ["Sel\u00b7ber", "von", "stei\u00b7gen\u00b7den", "H\u00fc\u00b7geln", "voll", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "ADJD", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Auf denen im Rohr die Mor\u00e4ne weilt,", "tokens": ["Auf", "de\u00b7nen", "im", "Rohr", "die", "Mo\u00b7r\u00e4\u00b7ne", "weilt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "APPRART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Sich des Garns T\u00fccke nicht naht, und den Wurm", "tokens": ["Sich", "des", "Garns", "T\u00fc\u00b7cke", "nicht", "naht", ",", "und", "den", "Wurm"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PRF", "ART", "NN", "NN", "PTKNEG", "VVFIN", "$,", "KON", "ART", "NN"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.4": {"text": "An dem Stahl, leidend mit ihm, ferne beklagt.", "tokens": ["An", "dem", "Stahl", ",", "lei\u00b7dend", "mit", "ihm", ",", "fer\u00b7ne", "be\u00b7klagt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ADJD", "APPR", "PPER", "$,", "ADV", "VVFIN", "$."], "meter": "+-++-+-+--+", "measure": "iambic.hexa.chol"}}, "stanza.3": {"line.1": {"text": "Fl\u00fcchtige Stunden verweilt' ich nur", "tokens": ["Fl\u00fcch\u00b7ti\u00b7ge", "Stun\u00b7den", "ver\u00b7weilt'", "ich", "nur"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "ADV"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "An deinem melodischen Schilfger\u00e4usch;", "tokens": ["An", "dei\u00b7nem", "me\u00b7lo\u00b7di\u00b7schen", "Schilf\u00b7ge\u00b7r\u00e4usch", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+---+-+-+", "measure": "dactylic.init"}, "line.3": {"text": "Doch verl\u00e4sst nie dein Phantom meinen Geist,", "tokens": ["Doch", "ver\u00b7l\u00e4sst", "nie", "dein", "Phan\u00b7tom", "mei\u00b7nen", "Geist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PPOSAT", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wie ein Bild, welches mit Lust Geniushand", "tokens": ["Wie", "ein", "Bild", ",", "wel\u00b7ches", "mit", "Lust", "Ge\u00b7nius\u00b7hand"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "$,", "PRELS", "APPR", "NN", "NN"], "meter": "+-+---+-+-", "measure": "unknown.measure.tetra"}}, "stanza.4": {"line.1": {"text": "Bildete, trotzt der Vergessenheit!", "tokens": ["Bil\u00b7de\u00b7te", ",", "trotzt", "der", "Ver\u00b7ges\u00b7sen\u00b7heit", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "ART", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Der Garten des F\u00fcrsten verdorrt, und w\u00e4chst", "tokens": ["Der", "Gar\u00b7ten", "des", "F\u00fcrs\u00b7ten", "ver\u00b7dorrt", ",", "und", "w\u00e4chst"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "VVPP", "$,", "KON", "VVFIN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Zu Gestr\u00e4uch, \u00fcber des Strauchs Wildniss hebt", "tokens": ["Zu", "Ge\u00b7str\u00e4uch", ",", "\u00fc\u00b7ber", "des", "Strauchs", "Wild\u00b7niss", "hebt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "APPR", "ART", "NN", "NE", "VVFIN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.4": {"text": "Sich der Kunst meisterhaft Werk daurend empor.", "tokens": ["Sich", "der", "Kunst", "meis\u00b7ter\u00b7haft", "Werk", "dau\u00b7rend", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "NN", "ADJD", "NN", "ADJD", "PTKVZ", "$."], "meter": "+-++-+-+--+", "measure": "iambic.hexa.chol"}}, "stanza.5": {"line.1": {"text": "Neben dir schattet des Sachsen Wald,", "tokens": ["Ne\u00b7ben", "dir", "schat\u00b7tet", "des", "Sach\u00b7sen", "Wald", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Sein Schwert war entscheidend, und kurz sein Wort!", "tokens": ["Sein", "Schwert", "war", "ent\u00b7schei\u00b7dend", ",", "und", "kurz", "sein", "Wort", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$,", "KON", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Und um dich gl\u00e4nzeten nie Schilde Roms,", "tokens": ["Und", "um", "dich", "gl\u00e4n\u00b7ze\u00b7ten", "nie", "Schil\u00b7de", "Roms", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "VVFIN", "ADV", "NN", "NE", "$,"], "meter": "-+-+---+-+", "measure": "zehnsilber"}, "line.4": {"text": "Sein Despot sendete nie Adler dir zu!", "tokens": ["Sein", "Des\u00b7pot", "sen\u00b7de\u00b7te", "nie", "Ad\u00b7ler", "dir", "zu", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "NN", "PPER", "PTKVZ", "$."], "meter": "--+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "Ruhiger wandelt' in deinem Thal", "tokens": ["Ru\u00b7hi\u00b7ger", "wan\u00b7delt'", "in", "dei\u00b7nem", "Thal"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Der G\u00f6ttinnen beste, die sanfte Hlyn.", "tokens": ["Der", "G\u00f6t\u00b7tin\u00b7nen", "bes\u00b7te", ",", "die", "sanf\u00b7te", "Hlyn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Es erscholl freudiges Klangs Braga's Lied", "tokens": ["Es", "er\u00b7scholl", "freu\u00b7di\u00b7ges", "Klangs", "Bra\u00b7ga's", "Lied"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADJD", "ADJA", "NN", "NE", "NN"], "meter": "--++-+-+-+", "measure": "anapaest.init"}, "line.4": {"text": "Um dich her, mischte nicht ein Rufe der Schlacht.", "tokens": ["Um", "dich", "her", ",", "mischte", "nicht", "ein", "Ru\u00b7fe", "der", "Schlacht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "PTKVZ", "$,", "VVFIN", "PTKNEG", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.7": {"line.1": {"text": "\u00dcber dem stolzeren Strome nur,", "tokens": ["\u00dc\u00b7ber", "dem", "stol\u00b7ze\u00b7ren", "Stro\u00b7me", "nur", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ADV", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Der Ham sich vor\u00fcber ins Meer ergiesst,", "tokens": ["Der", "Ham", "sich", "vor\u00b7\u00fc\u00b7ber", "ins", "Meer", "er\u00b7giesst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "PRF", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Da umgab Blut den Bardiet, liess den Speer", "tokens": ["Da", "um\u00b7gab", "Blut", "den", "Bar\u00b7diet", ",", "liess", "den", "Speer"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NN", "ART", "NN", "$,", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mit des Lieds schreckendem Drohn fliegen der Gott!", "tokens": ["Mit", "des", "Lieds", "schre\u00b7cken\u00b7dem", "Drohn", "flie\u00b7gen", "der", "Gott", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+--+--++--+", "measure": "dactylic.di.plus"}}, "stanza.8": {"line.1": {"text": "Aber wenn Hertha zum Bade zog,", "tokens": ["A\u00b7ber", "wenn", "Her\u00b7tha", "zum", "Ba\u00b7de", "zog", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "NE", "APPRART", "NN", "VVFIN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "So eilete Braga zu dir zur\u00fcck,", "tokens": ["So", "ei\u00b7le\u00b7te", "Bra\u00b7ga", "zu", "dir", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+---+-+-+", "measure": "dactylic.init"}, "line.3": {"text": "So begann Lenzmelodie, liess der Gott", "tokens": ["So", "be\u00b7gann", "Lenz\u00b7me\u00b7lo\u00b7die", ",", "liess", "der", "Gott"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NE", "$,", "VVFIN", "ART", "NN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.4": {"text": "Bey des Lieds Tanze dahin sinken den Speer.", "tokens": ["Bey", "des", "Lieds", "Tan\u00b7ze", "da\u00b7hin", "sin\u00b7ken", "den", "Speer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "PAV", "VVFIN", "ART", "NN", "$."], "meter": "+--+-+-+--+", "measure": "iambic.penta.invert"}}, "stanza.9": {"line.1": {"text": "Seines Gesanges erschallet noch;", "tokens": ["Sei\u00b7nes", "Ge\u00b7san\u00b7ges", "er\u00b7schal\u00b7let", "noch", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Mich lehret er \u00e4lteren deutschen Ton,", "tokens": ["Mich", "leh\u00b7ret", "er", "\u00e4l\u00b7te\u00b7ren", "deut\u00b7schen", "Ton", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJA", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Wenn entw\u00f6lkt wallet der Mond, und es sanft", "tokens": ["Wenn", "ent\u00b7w\u00f6lkt", "wal\u00b7let", "der", "Mond", ",", "und", "es", "sanft"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "ADJD", "VVFIN", "ART", "NN", "$,", "KON", "PPER", "ADJD"], "meter": "--+---+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Um das Grab derer ert\u00f6nt, welchen er sang.", "tokens": ["Um", "das", "Grab", "de\u00b7rer", "er\u00b7t\u00f6nt", ",", "wel\u00b7chen", "er", "sang", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "PDS", "VVFIN", "$,", "PWAT", "PPER", "VVFIN", "$."], "meter": "--+-+-+---+", "measure": "anapaest.init"}}, "stanza.10": {"line.1": {"text": "Horchend dem lehrenden Liede, s\u00e4ng'", "tokens": ["Hor\u00b7chend", "dem", "leh\u00b7ren\u00b7den", "Lie\u00b7de", ",", "s\u00e4ng'"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["NE", "ART", "ADJA", "NN", "$,", "NE"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Ich deinen Bepflanzer, o Insel, n\u00e4hm'", "tokens": ["Ich", "dei\u00b7nen", "Be\u00b7pflan\u00b7zer", ",", "o", "In\u00b7sel", ",", "n\u00e4hm'"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PPER", "PPOSAT", "NN", "$,", "FM", "NN", "$,", "VVFIN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Ich des Hains Fl\u00fcgel, nnd eilt', heilig Laub", "tokens": ["Ich", "des", "Hains", "Fl\u00fc\u00b7gel", ",", "nnd", "eilt'", ",", "hei\u00b7lig", "Laub"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PPER", "ART", "NN", "NN", "$,", "KON", "VVFIN", "$,", "ADJD", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "In der Hand, ihm, wo der Ruhm ewiget, nach!", "tokens": ["In", "der", "Hand", ",", "ihm", ",", "wo", "der", "Ruhm", "e\u00b7wi\u00b7get", ",", "nach", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PPER", "$,", "PWAV", "ART", "NN", "VVFIN", "$,", "APPR", "$."], "meter": "--+-+-+-+-+", "measure": "anapaest.init"}}, "stanza.11": {"line.1": {"text": "Aber entweihet, entweihet ward", "tokens": ["A\u00b7ber", "ent\u00b7wei\u00b7het", ",", "ent\u00b7wei\u00b7het", "ward"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "$,", "VVFIN", "VAFIN"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Die Leyer, die Fl\u00fcge des Lobes flog!", "tokens": ["Die", "Le\u00b7yer", ",", "die", "Fl\u00fc\u00b7ge", "des", "Lo\u00b7bes", "flog", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Dem Verdienst selten getreu, rauschte sie", "tokens": ["Dem", "Ver\u00b7dienst", "sel\u00b7ten", "ge\u00b7treu", ",", "rauschte", "sie"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "ADJD", "ADJD", "$,", "VVFIN", "PPER"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Um das Ohr dess, der an That d\u00fcrftig, verschwand.", "tokens": ["Um", "das", "Ohr", "dess", ",", "der", "an", "That", "d\u00fcrf\u00b7tig", ",", "ver\u00b7schwand", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUI", "ART", "NN", "PDS", "$,", "PRELS", "APPR", "NN", "ADJD", "$,", "VVFIN", "$."], "meter": "+-+---++--+", "measure": "iambic.penta.chol"}}, "stanza.12": {"line.1": {"text": "Leyer des heiligen Bardenhais,", "tokens": ["Le\u00b7yer", "des", "hei\u00b7li\u00b7gen", "Bar\u00b7den\u00b7hais", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Verw\u00fcnsche des Ehreverschwenders Lied,", "tokens": ["Ver\u00b7w\u00fcn\u00b7sche", "des", "E\u00b7hre\u00b7ver\u00b7schwen\u00b7ders", "Lied", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "So zuerst, tr\u00fcgenden Glanz, den besang!", "tokens": ["So", "zu\u00b7erst", ",", "tr\u00fc\u00b7gen\u00b7den", "Glanz", ",", "den", "be\u00b7sang", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "ADJA", "NN", "$,", "PRELS", "ADJD", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und der That lautes Verbot, das nicht vernahm!", "tokens": ["Und", "der", "That", "lau\u00b7tes", "Ver\u00b7bot", ",", "das", "nicht", "ver\u00b7nahm", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJA", "NN", "$,", "PRELS", "PTKNEG", "