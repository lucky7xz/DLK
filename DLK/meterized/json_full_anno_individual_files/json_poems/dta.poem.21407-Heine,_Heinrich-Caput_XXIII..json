{"dta.poem.21407": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Caput XXIII.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-30602-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ihr Antlitz war rund und kerngesund,", "tokens": ["Ihr", "Ant\u00b7litz", "war", "rund", "und", "kern\u00b7ge\u00b7sund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die Augen wie blaue Turkoasen,", "tokens": ["Die", "Au\u00b7gen", "wie", "blau\u00b7e", "Tur\u00b7ko\u00b7a\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KOKOM", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Die Wangen wie Rosen, wie Kirschen der Mund,", "tokens": ["Die", "Wan\u00b7gen", "wie", "Ro\u00b7sen", ",", "wie", "Kir\u00b7schen", "der", "Mund", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KOKOM", "NN", "$,", "PWAV", "NN", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Auch etwas r\u00f6thlich die Nase.", "tokens": ["Auch", "et\u00b7was", "r\u00f6th\u00b7lich", "die", "Na\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ihr Haupt bedeckte eine M\u00fctz\u2019", "tokens": ["Ihr", "Haupt", "be\u00b7deck\u00b7te", "ei\u00b7ne", "M\u00fctz'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von wei\u00dfem gesteiftem Linnen,", "tokens": ["Von", "wei\u00b7\u00dfem", "ge\u00b7steif\u00b7tem", "Lin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Gef\u00e4ltelt wie eine Mauerkron\u2019,", "tokens": ["Ge\u00b7f\u00e4l\u00b7telt", "wie", "ei\u00b7ne", "Mau\u00b7er\u00b7kron'", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ART", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Mit Th\u00fcrmchen und zackigen Zinnen.", "tokens": ["Mit", "Th\u00fcrm\u00b7chen", "und", "za\u00b7cki\u00b7gen", "Zin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.3": {"line.1": {"text": "Sie trug eine wei\u00dfe Tunika,", "tokens": ["Sie", "trug", "ei\u00b7ne", "wei\u00b7\u00dfe", "Tu\u00b7ni\u00b7ka", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Bis an die Waden reichend.", "tokens": ["Bis", "an", "die", "Wa\u00b7den", "rei\u00b7chend", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und welche Waden! Das Fu\u00dfgestell", "tokens": ["Und", "wel\u00b7che", "Wa\u00b7den", "!", "Das", "Fu\u00df\u00b7ge\u00b7stell"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PWAT", "NN", "$.", "ART", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Zwey dorischen S\u00e4ulen gleichend.", "tokens": ["Zwey", "do\u00b7ri\u00b7schen", "S\u00e4u\u00b7len", "glei\u00b7chend", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "ADJA", "NN", "VVPP", "$."], "meter": "----+-+-", "measure": "unknown.measure.di"}}, "stanza.4": {"line.1": {"text": "Die weltlichste Nat\u00fcrlichkeit", "tokens": ["Die", "welt\u00b7lichs\u00b7te", "Na\u00b7t\u00fcr\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Konnt man in den Z\u00fcgen lesen;", "tokens": ["Konnt", "man", "in", "den", "Z\u00fc\u00b7gen", "le\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch das \u00fcbermenschliche Hintertheil", "tokens": ["Doch", "das", "\u00fc\u00b7ber\u00b7menschli\u00b7che", "Hin\u00b7ter\u00b7theil"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Verrieth ein h\u00f6heres Wesen.", "tokens": ["Ver\u00b7rieth", "ein", "h\u00f6\u00b7he\u00b7res", "We\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}}}}