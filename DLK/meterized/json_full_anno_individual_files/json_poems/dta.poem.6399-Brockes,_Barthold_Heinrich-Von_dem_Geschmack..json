{"dta.poem.6399": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von dem Geschmack.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Indem man nun den Regeln, die uns leiten,", "tokens": ["In\u00b7dem", "man", "nun", "den", "Re\u00b7geln", ",", "die", "uns", "lei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "ART", "NN", "$,", "PRELS", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie billig folgt: so lasst uns die Beschaffenheiten", "tokens": ["Wie", "bil\u00b7lig", "folgt", ":", "so", "lasst", "uns", "die", "Be\u00b7schaf\u00b7fen\u00b7hei\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "VVFIN", "$.", "ADV", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der C\u00f6rper wol besehn, die schmackhafft seyn!", "tokens": ["Der", "C\u00f6r\u00b7per", "wol", "be\u00b7sehn", ",", "die", "schmack\u00b7hafft", "seyn", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVINF", "$,", "PRELS", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "An allen Orten, wo wir nur", "tokens": ["An", "al\u00b7len", "Or\u00b7ten", ",", "wo", "wir", "nur"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "$,", "PWAV", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Derselben Aenderungen sehen,", "tokens": ["Der\u00b7sel\u00b7ben", "A\u00b7en\u00b7de\u00b7run\u00b7gen", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VVINF", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Es mogen selbige geschehen,", "tokens": ["Es", "mo\u00b7gen", "sel\u00b7bi\u00b7ge", "ge\u00b7sche\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADJA", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Durch W\u00fcrckung, durch Figur,", "tokens": ["Durch", "W\u00fcr\u00b7ckung", ",", "durch", "Fi\u00b7gur", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "APPR", "NN", "$,"], "meter": "-+-++-", "measure": "unknown.measure.tri"}, "line.8": {"text": "Geweb- und Mischungen; so findet man sodann", "tokens": ["Ge\u00b7we\u00b7b", "und", "Misc\u00b7hun\u00b7gen", ";", "so", "fin\u00b7det", "man", "so\u00b7dann"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["TRUNC", "KON", "NN", "$.", "ADV", "VVFIN", "PIS", "ADV"], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "Und trifft auch im Geschmack dieselbe Aendrung an.", "tokens": ["Und", "trifft", "auch", "im", "Ge\u00b7schmack", "die\u00b7sel\u00b7be", "A\u00b7en\u00b7drung", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPRART", "NN", "PDAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}}}}}