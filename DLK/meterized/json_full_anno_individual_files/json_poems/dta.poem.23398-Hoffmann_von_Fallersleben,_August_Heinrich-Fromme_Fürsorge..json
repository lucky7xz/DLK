{"dta.poem.23398": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Fromme F\u00fcrsorge.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192634", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Dem L\u00e4mmlein h\u00e4ngt man niedliche Gl\u00f6cklein an,", "tokens": ["Dem", "L\u00e4mm\u00b7lein", "h\u00e4ngt", "man", "nied\u00b7li\u00b7che", "Gl\u00f6c\u00b7klein", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIS", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Auf da\u00df es lieblich l\u00e4uten kann:", "tokens": ["Auf", "da\u00df", "es", "lieb\u00b7lich", "l\u00e4u\u00b7ten", "kann", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PPER", "ADJD", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So beh\u00e4ngt man mit Titeln und Orden", "tokens": ["So", "be\u00b7h\u00e4ngt", "man", "mit", "Ti\u00b7teln", "und", "Or\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "APPR", "NN", "KON", "NN"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Wer ein L\u00e4mmelbruder geworden.", "tokens": ["Wer", "ein", "L\u00e4m\u00b7mel\u00b7bru\u00b7der", "ge\u00b7wor\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VAPP", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Das L\u00e4mmlein schickt man ins beste Gras hinein,", "tokens": ["Das", "L\u00e4mm\u00b7lein", "schickt", "man", "ins", "bes\u00b7te", "Gras", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIS", "APPRART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Auf da\u00df es m\u00f6ge gut gedeihn:", "tokens": ["Auf", "da\u00df", "es", "m\u00f6\u00b7ge", "gut", "ge\u00b7deihn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PPER", "VMFIN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So schickt man auch in die besten Stellen", "tokens": ["So", "schickt", "man", "auch", "in", "die", "bes\u00b7ten", "Stel\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Den L\u00e4mmelbruder mit seinen Gesellen.", "tokens": ["Den", "L\u00e4m\u00b7mel\u00b7bru\u00b7der", "mit", "sei\u00b7nen", "Ge\u00b7sel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}}}}}