rd", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "NN", "$,", "ADV", "ADJD", "PPER", "VVFIN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Auch schallt ihm vergeblich der V\u00f6gelein Lied.", "tokens": ["Auch", "schallt", "ihm", "ver\u00b7geb\u00b7lich", "der", "V\u00f6\u00b7ge\u00b7lein", "Lied", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "ART", "NN", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.10": {"line.1": {"text": "Ruht sanft ihr Toten! H\u00f6rt ihr schon", "tokens": ["Ruht", "sanft", "ihr", "To\u00b7ten", "!", "H\u00f6rt", "ihr", "schon"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "PPOSAT", "NN", "$.", "VVIMP", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Kein Lied jetzt mehr erklingen,", "tokens": ["Kein", "Lied", "jetzt", "mehr", "er\u00b7klin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Einst wird zu eurer Gruft der Ton", "tokens": ["Einst", "wird", "zu", "eu\u00b7rer", "Gruft", "der", "Ton"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPR", "PPOSAT", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Des Totenweckers bringen.", "tokens": ["Des", "To\u00b7ten\u00b7we\u00b7ckers", "brin\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Dann leben von Sorgen und Thr\u00e4nen wir frei,", "tokens": ["Dann", "le\u00b7ben", "von", "Sor\u00b7gen", "und", "Thr\u00e4\u00b7nen", "wir", "frei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NN", "KON", "NN", "PPER", "ADJD", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Und droben umbl\u00fcht uns ein ewiger Mai.", "tokens": ["Und", "dro\u00b7ben", "um\u00b7bl\u00fcht", "uns", "ein", "e\u00b7wi\u00b7ger", "Mai", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}}}}