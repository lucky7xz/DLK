{"dta.poem.19365": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "98.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Mensch ist, wie er sagt, ein B\u00fcrger zweier Welten,", "tokens": ["Der", "Mensch", "ist", ",", "wie", "er", "sagt", ",", "ein", "B\u00fcr\u00b7ger", "zwei\u00b7er", "Wel\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$,", "PWAV", "PPER", "VVFIN", "$,", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch kann er nicht zugleich und ganz in jeder gelten.", "tokens": ["Doch", "kann", "er", "nicht", "zu\u00b7gleich", "und", "ganz", "in", "je\u00b7der", "gel\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PTKNEG", "ADV", "KON", "ADV", "APPR", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "In keiner ist er sonst von beiden recht zu Haus,", "tokens": ["In", "kei\u00b7ner", "ist", "er", "sonst", "von", "bei\u00b7den", "recht", "zu", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "VAFIN", "PPER", "ADV", "APPR", "PIAT", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und zwischen beiden schwebt er wie die Fledermaus.", "tokens": ["Und", "zwi\u00b7schen", "bei\u00b7den", "schwebt", "er", "wie", "die", "Fle\u00b7der\u00b7maus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PIS", "VVFIN", "PPER", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Solang die B\u00fcrgerschaft ist hier in voller Kraft,", "tokens": ["So\u00b7lang", "die", "B\u00fcr\u00b7ger\u00b7schaft", "ist", "hier", "in", "vol\u00b7ler", "Kraft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "VAFIN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ist deine dortige nur eine Anwartschaft.", "tokens": ["Ist", "dei\u00b7ne", "dor\u00b7ti\u00b7ge", "nur", "ei\u00b7ne", "An\u00b7wart\u00b7schaft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "ADJA", "ADV", "ART", "NN", "$."], "meter": "-+-+--++-+--", "measure": "iambic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Du mu\u00dft die wirkliche hier wirklich dir erwirken,", "tokens": ["Du", "mu\u00dft", "die", "wirk\u00b7li\u00b7che", "hier", "wirk\u00b7lich", "dir", "er\u00b7wir\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "ADJA", "ADV", "ADJD", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Jedoch dabei nichts thun, dort jene zu verwirken.", "tokens": ["Je\u00b7doch", "da\u00b7bei", "nichts", "thun", ",", "dort", "je\u00b7ne", "zu", "ver\u00b7wir\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PAV", "PIS", "VVINF", "$,", "ADV", "PDS", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "So thust du deine Pflicht gleichzeitig und gleichseitig,", "tokens": ["So", "thust", "du", "dei\u00b7ne", "Pflicht", "gleich\u00b7zei\u00b7tig", "und", "gleich\u00b7sei\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und keine B\u00fcrgerschaft macht dich der andern streitig.", "tokens": ["Und", "kei\u00b7ne", "B\u00fcr\u00b7ger\u00b7schaft", "macht", "dich", "der", "an\u00b7dern", "strei\u00b7tig", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "PPER", "ART", "ADJA", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}