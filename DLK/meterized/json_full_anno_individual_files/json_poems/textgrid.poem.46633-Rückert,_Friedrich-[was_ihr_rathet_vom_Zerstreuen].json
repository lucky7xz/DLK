{"textgrid.poem.46633": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[was ihr rathet vom Zerstreuen]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was ihr rathet vom Zerstreuen,", "tokens": ["Was", "ihr", "ra\u00b7thet", "vom", "Zer\u00b7streu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Vom Entschlagen meines Grames,", "tokens": ["Vom", "Ent\u00b7schla\u00b7gen", "mei\u00b7nes", "Gra\u00b7mes", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Folgt' ich euch und unternahm es,", "tokens": ["Folgt'", "ich", "euch", "und", "un\u00b7ter\u00b7nahm", "es", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "KON", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hatt' ich's zu bereuen.", "tokens": ["Hatt'", "ich's", "zu", "be\u00b7reu\u00b7en", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "PTKZU", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Aus der Hand nicht darf ich schl\u00fcpfen", "tokens": ["Aus", "der", "Hand", "nicht", "darf", "ich", "schl\u00fcp\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PTKNEG", "VMFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lassen meines Kummers Faden,", "tokens": ["Las\u00b7sen", "mei\u00b7nes", "Kum\u00b7mers", "Fa\u00b7den", ","], "token_info": [