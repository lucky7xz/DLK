{"dta.poem.8953": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "IiI.  \n  Amalfi.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Welches am Felsabhang, in der N\u00e4he des sch\u00f6nen Amalfi,", "tokens": ["Wel\u00b7ches", "am", "Fels\u00b7ab\u00b7hang", ",", "in", "der", "N\u00e4\u00b7he", "des", "sch\u00f6\u00b7nen", "A\u00b7mal\u00b7fi", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPRART", "NN", "$,", "APPR", "ART", "NN", "ART", "ADJA", "NE", "$,"], "meter": "+--+-+--+--+-+-+", "measure": "iambic.septa.invert"}, "line.2": {"text": "Fluth und Gebirge beherrscht, und dem Auge behaglichen", "tokens": ["Fluth", "und", "Ge\u00b7bir\u00b7ge", "be\u00b7herrscht", ",", "und", "dem", "Au\u00b7ge", "be\u00b7hag\u00b7li\u00b7chen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "VVFIN", "$,", "KON", "ART", "NN", "VVINF"], "meter": "+--+--+--+--+--", "measure": "diphilius"}, "line.3": {"text": "G\u00f6nnt, zu den F\u00fc\u00dfen das Meer und hinaufw\u00e4rts kantige", "tokens": ["G\u00f6nnt", ",", "zu", "den", "F\u00fc\u00b7\u00dfen", "das", "Meer", "und", "hin\u00b7auf\u00b7w\u00e4rts", "kan\u00b7ti\u00b7ge"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "APPR", "ART", "NN", "ART", "NN", "KON", "ADV", "ADJA"], "meter": "+--+--+--+-+-+", "measure": "dactylic.tri.plus"}, "line.4": {"text": "Steile Terrassen umher, wo in Lauben die Rebe sich auf-", "tokens": ["Stei\u00b7le", "Ter\u00b7ras\u00b7sen", "um\u00b7her", ",", "wo", "in", "Lau\u00b7ben", "die", "Re\u00b7be", "sich", "auf"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "NN", "PTKVZ", "$,", "PWAV", "APPR", "NN", "ART", "NN", "PRF", "TRUNC"], "meter": "+-+---+--+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.5": {"text": "Doch nicht M\u00f6nche bewohnen es mehr, nicht alte Chor\u00e4le", "tokens": ["Doch", "nicht", "M\u00f6n\u00b7che", "be\u00b7woh\u00b7nen", "es", "mehr", ",", "nicht", "al\u00b7te", "Cho\u00b7r\u00e4\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PTKNEG", "NN", "VVFIN", "PPER", "ADV", "$,", "PTKNEG", "ADJA", "NN"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.6": {"text": "Hallen im Kirchengew\u00f6lb' und erwecken das Echo des", "tokens": ["Hal\u00b7len", "im", "Kir\u00b7chen\u00b7ge\u00b7w\u00f6lb'", "und", "er\u00b7we\u00b7cken", "das", "E\u00b7cho", "des"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPRART", "NN", "KON", "VVFIN", "ART", "NN", "ART"], "meter": "+--+--+--+--+--", "measure": "diphilius"}, "line.7": {"text": "Leer steht Saal und Gemach, in den Kalktufgrotten der", "tokens": ["Leer", "steht", "Saal", "und", "Ge\u00b7mach", ",", "in", "den", "Kalk\u00b7tuf\u00b7grot\u00b7ten", "der"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "NN", "KON", "NN", "$,", "APPR", "ART", "NN", "ART"], "meter": "+-+--+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.8": {"text": "Knien, der Gebete beraubt, eingehende Heiligenbilder.", "tokens": ["Kni\u00b7en", ",", "der", "Ge\u00b7be\u00b7te", "be\u00b7raubt", ",", "ein\u00b7ge\u00b7hen\u00b7de", "Hei\u00b7li\u00b7gen\u00b7bil\u00b7der", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "VVPP", "$,", "ADJA", "NN", "$."], "meter": "+---+--+-+--+--+-", "measure": "trochaic.hexa.relaxed"}, "line.9": {"text": "Sonntags aber entschallt den ver\u00f6deten, langen Geb\u00e4uden", "tokens": ["Sonn\u00b7tags", "a\u00b7ber", "ent\u00b7schallt", "den", "ver\u00b7\u00f6\u00b7de\u00b7ten", ",", "lan\u00b7gen", "Ge\u00b7b\u00e4u\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "ART", "ADJA", "$,", "ADJA", "NN"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.10": {"text": "Frohe Musik, es besucht sie die luftige Jugend Amalfi's:", "tokens": ["Fro\u00b7he", "Mu\u00b7sik", ",", "es", "be\u00b7sucht", "sie", "die", "luf\u00b7ti\u00b7ge", "Ju\u00b7gend", "A\u00b7ma\u00b7lfi's", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PPER", "VVFIN", "PPER", "ART", "ADJA", "NN", "NE", "$."], "meter": "+--+--+--+--+-+-+", "measure": "dactylic.tetra.plus"}, "line.11": {"text": "Kinder beschwingen im Hof, blitz\u00e4ugige Knaben, den Kreisel", "tokens": ["Kin\u00b7der", "be\u00b7schwin\u00b7gen", "im", "Hof", ",", "blitz\u00b7\u00e4u\u00b7gi\u00b7ge", "Kna\u00b7ben", ",", "den", "Krei\u00b7sel"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NN", "VVFIN", "APPRART", "NN", "$,", "ADJA", "NN", "$,", "ART", "NN"], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.12": {"text": "Rasch an der Schnur, und fangen den taumelnden dann", "tokens": ["Rasch", "an", "der", "Schnur", ",", "und", "fan\u00b7gen", "den", "tau\u00b7meln\u00b7den", "dann"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NN", "$,", "KON", "VVFIN", "ART", "ADJA", "ADV"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.13": {"text": "Aeltere werfen die Kugel inde\u00df, die Entfernungen messend,", "tokens": ["A\u00b7el\u00b7te\u00b7re", "wer\u00b7fen", "die", "Ku\u00b7gel", "in\u00b7de\u00df", ",", "die", "Ent\u00b7fer\u00b7nun\u00b7gen", "mes\u00b7send", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "ADV", "$,", "ART", "NN", "VVPP", "$,"], "meter": "-+--+--+--+-+-+-+-", "measure": "amphibrach.tetra.plus"}, "line.14": {"text": "Z\u00e4hlen, im Spiele der Morra, die Finger mit hurtigem", "tokens": ["Z\u00e4h\u00b7len", ",", "im", "Spie\u00b7le", "der", "Mor\u00b7ra", ",", "die", "Fin\u00b7ger", "mit", "hur\u00b7ti\u00b7gem"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "APPRART", "NN", "ART", "NN", "$,", "ART", "NN", "APPR", "ADJA"], "meter": "+--+--+--+--+--", "measure": "diphilius"}, "line.15": {"text": "Oder sie stimmen zu rauhem Gesang einfache Guitarren,", "tokens": ["O\u00b7der", "sie", "stim\u00b7men", "zu", "rau\u00b7hem", "Ge\u00b7sang", "ein\u00b7fa\u00b7che", "Gui\u00b7tar\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.16": {"text": "Freudebewegt. Theilnehmend erscheint ein gesitteter J\u00fcng-", "tokens": ["Freu\u00b7de\u00b7be\u00b7wegt", ".", "Theil\u00b7neh\u00b7mend", "er\u00b7scheint", "ein", "ge\u00b7sit\u00b7te\u00b7ter", "J\u00fcng"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$.", "ADJD", "VVFIN", "ART", "ADJA", "TRUNC"], "meter": "+--+-+--+--+--+", "measure": "iambic.hexa.invert"}, "line.17": {"text": "Unter der Schaar, doch nicht in die Spiele sich selbst ein-", "tokens": ["Un\u00b7ter", "der", "Schaar", ",", "doch", "nicht", "in", "die", "Spie\u00b7le", "sich", "selbst", "ein"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "ADV", "PTKNEG", "APPR", "ART", "NN", "PRF", "ADV", "TRUNC"], "meter": "+--+-++-+-+-+", "measure": "iambic.septa.invert"}, "line.18": {"text": "Hoch vom steilen Gebirge, das Fest zu begehn in Amalfi,", "tokens": ["Hoch", "vom", "stei\u00b7len", "Ge\u00b7bir\u00b7ge", ",", "das", "Fest", "zu", "be\u00b7gehn", "in", "A\u00b7mal\u00b7fi", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "ADJA", "NN", "$,", "ART", "NN", "PTKZU", "VVINF", "APPR", "NE", "$,"], "meter": "+-+--+--+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.19": {"text": "Sch\u00f6n wie ein Engel des Herrn, in die Tiefe herunterge-", "tokens": ["Sch\u00f6n", "wie", "ein", "En\u00b7gel", "des", "Herrn", ",", "in", "die", "Tie\u00b7fe", "her\u00b7un\u00b7ter\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "KOKOM", "ART", "NN", "ART", "NN", "$,", "APPR", "ART", "NN", "TRUNC"], "meter": "+--+--+--+--+-+", "measure": "diphilius"}, "line.20": {"text": "Reizend in Ringen umkr\u00e4uselt die Brau'n schwarzlockigen", "tokens": ["Rei\u00b7zend", "in", "Rin\u00b7gen", "um\u00b7kr\u00e4u\u00b7selt", "die", "Brau'n", "schwarz\u00b7lo\u00b7cki\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVPP", "APPR", "NN", "VVFIN", "ART", "NN", "VVINF"], "meter": "+--+--+--++-+-", "measure": "dactylic.tri.plus"}, "line.21": {"text": "Schimmernde Nacht, rein leuchtet die bl\u00fchende Flamme", "tokens": ["Schim\u00b7mern\u00b7de", "Nacht", ",", "rein", "leuch\u00b7tet", "die", "bl\u00fc\u00b7hen\u00b7de", "Flam\u00b7me"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "ADJD", "VVFIN", "ART", "ADJA", "NN"], "meter": "+--+-+--+--+-", "measure": "iambic.penta.invert"}, "line.22": {"text": "Nie von Begierde getr\u00fcbt und dem Blick zweydeutiger", "tokens": ["Nie", "von", "Be\u00b7gier\u00b7de", "ge\u00b7tr\u00fcbt", "und", "dem", "Blick", "zwey\u00b7deu\u00b7ti\u00b7ger"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "NN", "VVPP", "KON", "ART", "NN", "ADJA"], "meter": "+--+--+--+-+-+", "measure": "dactylic.tri.plus"}, "line.23": {"text": "Welche dem kochenden Blut in der s\u00fcdlichen Sonne ge-", "tokens": ["Wel\u00b7che", "dem", "ko\u00b7chen\u00b7den", "Blut", "in", "der", "s\u00fcd\u00b7li\u00b7chen", "Son\u00b7ne", "ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAT", "ART", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "TRUNC"], "meter": "+--+--+--+--+-+", "measure": "diphilius"}, "line.24": {"text": "Doch wer kann, da die Zeit hinrollt, festhalten die Sch\u00f6n-", "tokens": ["Doch", "wer", "kann", ",", "da", "die", "Zeit", "hin\u00b7rollt", ",", "fest\u00b7hal\u00b7ten", "die", "Sch\u00f6n"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PWS", "VMFIN", "$,", "KOUS", "ART", "NN", "VVPP", "$,", "VVFIN", "ART", "TRUNC"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}}, "stanza.2": {"line.1": {"text": "Schweige davon! Rings g\u00e4hnt, wie ein Schlund, die ge-", "tokens": ["Schwei\u00b7ge", "da\u00b7von", "!", "Rings", "g\u00e4hnt", ",", "wie", "ein", "Schlund", ",", "die", "ge"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "PAV", "$.", "NN", "VVFIN", "$,", "PWAV", "ART", "NN", "$,", "ART", "TRUNC"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Tritt auf jene Balkone hinaus, und in duftiger Ferne", "tokens": ["Tritt", "auf", "je\u00b7ne", "Bal\u00b7ko\u00b7ne", "hin\u00b7aus", ",", "und", "in", "duf\u00b7ti\u00b7ger", "Fer\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "PDAT", "NN", "APZR", "$,", "KON", "APPR", "ADJA", "NN"], "meter": "--+-+---+--+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Siehst du das Ufer entlegener Bucht und am Ufer erblickst du", "tokens": ["Siehst", "du", "das", "U\u00b7fer", "ent\u00b7le\u00b7ge\u00b7ner", "Bucht", "und", "am", "U\u00b7fer", "er\u00b7blickst", "du"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADJA", "NN", "KON", "APPRART", "NN", "VVFIN", "PPER"], "meter": "-+-+--+--+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Herrlicher S\u00e4ulen in Reih'n aufstrebendes, dorisches Bild-", "tokens": ["Herr\u00b7li\u00b7cher", "S\u00e4u\u00b7len", "in", "Reih'n", "auf\u00b7stre\u00b7ben\u00b7des", ",", "do\u00b7ri\u00b7sches", "Bild"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "APPR", "NE", "NE", "$,", "ADJA", "TRUNC"], "meter": "+--+--+-+-+-+-+", "measure": "dactylic.di.plus"}, "line.5": {"text": "Nur Eidechsen umklettern es jezt, nur flatternde Raben", "tokens": ["Nur", "Ei\u00b7dech\u00b7sen", "um\u00b7klet\u00b7tern", "es", "jezt", ",", "nur", "flat\u00b7tern\u00b7de", "Ra\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "NN", "VVFIN", "PPER", "ADV", "$,", "ADV", "ADJA", "NN"], "meter": "-+---+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Ziehen geschaart jezt \u00fcber das offene Dach lautkreischend;", "tokens": ["Zie\u00b7hen", "ge\u00b7schaart", "jezt", "\u00fc\u00b7ber", "das", "of\u00b7fe\u00b7ne", "Dach", "laut\u00b7krei\u00b7schend", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ADV", "APPR", "ART", "ADJA", "NN", "ADJD", "$."], "meter": "+--+-+--+--+-+-", "measure": "iambic.hexa.invert"}, "line.7": {"text": "Brombeere decken die Stufen, und viel giftsamiges Unkraut", "tokens": ["Brom\u00b7bee\u00b7re", "de\u00b7cken", "die", "Stu\u00b7fen", ",", "und", "viel", "gift\u00b7sa\u00b7mi\u00b7ges", "Un\u00b7kraut"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN", "$,", "KON", "PIAT", "ADJA", "NN"], "meter": "-+-+--+-+-+-+-+-", "measure": "iambic.septa.relaxed"}, "line.8": {"text": "Kleidet den riesigen Sturz abfallender Tr\u00fcmmer in Gr\u00fcn ein.", "tokens": ["Klei\u00b7det", "den", "rie\u00b7si\u00b7gen", "Sturz", "ab\u00b7fal\u00b7len\u00b7der", "Tr\u00fcm\u00b7mer", "in", "Gr\u00fcn", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "ADJA", "NN", "APPR", "NN", "PTKVZ", "$."], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.9": {"text": "Seit Jahrtausenden ruht, sich selbst hinreichend und einsam,", "tokens": ["Seit", "Jahr\u00b7tau\u00b7sen\u00b7den", "ruht", ",", "sich", "selbst", "hin\u00b7rei\u00b7chend", "und", "ein\u00b7sam", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$,", "PRF", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.10": {"text": "Voll trotzbietender Kraft dein fallender Tempel, Poseidon,", "tokens": ["Voll", "trotz\u00b7bie\u00b7ten\u00b7der", "Kraft", "dein", "fal\u00b7len\u00b7der", "Tem\u00b7pel", ",", "Po\u00b7sei\u00b7don", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "PPOSAT", "ADJA", "NN", "$,", "NN", "$,"], "meter": "+-+--+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.11": {"text": "Mitten im Haidegefild und zun\u00e4chst an des Meers Ein\u00f6