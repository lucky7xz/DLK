{"textgrid.poem.42772": {"metadata": {"author": {"name": "Ringelnatz, Joachim", "birth": "N.A.", "death": "N.A."}, "title": "Zum Wegr\u00e4umen der Ger\u00e4te", "genre": "verse", "period": "N.A.", "pub_year": 1908, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Veterin\u00e4r, gleichzeitig Veteran,", "tokens": ["Ve\u00b7te\u00b7ri\u00b7n\u00e4r", ",", "gleich\u00b7zei\u00b7tig", "Ve\u00b7te\u00b7ran", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ADJD", "PTKVZ", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Ein Mann, der 92 Jahre z\u00e4hlte,", "tokens": ["Ein", "Mann", ",", "der", "92", "Jah\u00b7re", "z\u00e4hl\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "number", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "CARD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df man zuletzt ihn aus Gewohnheit w\u00e4hlte,", "tokens": ["Da\u00df", "man", "zu\u00b7letzt", "ihn", "aus", "Ge\u00b7wohn\u00b7heit", "w\u00e4hl\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und trotzdem biegsam, schmiegsam wie ein Schwan.", "tokens": ["Und", "trotz\u00b7dem", "bieg\u00b7sam", ",", "schmieg\u00b7sam", "wie", "ein", "Schwan", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "ADJD", "$,", "ADJD", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Das war \u2013 trotz eines halbgel\u00e4hmten Beines \u2013", "tokens": ["Das", "war", "\u2013", "trotz", "ei\u00b7nes", "halb\u00b7ge\u00b7l\u00e4hm\u00b7ten", "Bei\u00b7nes", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "$(", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Der Ehrenvorstand unsres Turnvereines.", "tokens": ["Der", "Eh\u00b7ren\u00b7vor\u00b7stand", "uns\u00b7res", "Turn\u00b7ver\u00b7ei\u00b7nes", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und wirklich nahm er's noch im Dauerlauf", "tokens": ["Und", "wirk\u00b7lich", "nahm", "er's", "noch", "im", "Dau\u00b7er\u00b7lauf"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "PIS", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Und Schleuderball mit jedem Rennpferd auf.", "tokens": ["Und", "Schleu\u00b7der\u00b7ball", "mit", "je\u00b7dem", "Renn\u00b7pferd", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wettl\u00e4ufer sah ich \u2013 nun Gott wei\u00df wieviel,", "tokens": ["Wett\u00b7l\u00e4u\u00b7fer", "sah", "ich", "\u2013", "nun", "Gott", "wei\u00df", "wie\u00b7viel", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$(", "ADV", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Doch ihrer keiner hielt wohl mit der gleichen", "tokens": ["Doch", "ih\u00b7rer", "kei\u00b7ner", "hielt", "wohl", "mit", "der", "glei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "PIS", "VVFIN", "ADV", "APPR", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Bescheidenheit gelassen vor dem Ziel.", "tokens": ["Be\u00b7schei\u00b7den\u00b7heit", "ge\u00b7las\u00b7sen", "vor", "dem", "Ziel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Denn niemand konnte ihm das Wasser reichen.", "tokens": ["Denn", "nie\u00b7mand", "konn\u00b7te", "ihm", "das", "Was\u00b7ser", "rei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VMFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Dann griff er abseits zum Pokal. Und Hei!", "tokens": ["Dann", "griff", "er", "ab\u00b7seits", "zum", "Po\u00b7kal", ".", "Und", "Hei", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$.", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Wie Donner klang sein Frisch-Fromm-Fr\u00f6hlich-Frei.", "tokens": ["Wie", "Don\u00b7ner", "klang", "sein", "FrischFrom\u00b7mFr\u00f6h\u00b7lich\u00b7Frei", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Wie sich sein Vollbart, den er gern sich wischte,", "tokens": ["Wie", "sich", "sein", "Voll\u00b7bart", ",", "den", "er", "gern", "sich", "wischte", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "PPOSAT", "NN", "$,", "PRELS", "PPER", "ADV", "PRF", "VVFIN", "$,"], "meter": "-+-++--+-+", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Nach einem 80 cm-Sprung", "tokens": ["Nach", "ei\u00b7nem", "80", "cm"], "token_info": ["word", "word", "measurement", "measurement", "word"], "pos": ["APPR", "ART", "CARD", "ADJA", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.9": {"text": "Mit Kokosfasern einer Matte mischte,", "tokens": ["Mit", "Ko\u00b7kos\u00b7fa\u00b7sern", "ei\u00b7ner", "Mat\u00b7te", "mischte", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Das bleibt mir ewig in Erinnerung.", "tokens": ["Das", "bleibt", "mir", "e\u00b7wig", "in", "E\u00b7rin\u00b7ne\u00b7rung", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.11": {"text": "Im Springen konnte \u00fcberhaupt dem Alten", "tokens": ["Im", "Sprin\u00b7gen", "konn\u00b7te", "\u00fc\u00b7ber\u00b7haupt", "dem", "Al\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VMFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Zuletzt wohl keiner mehr die Stange halten.", "tokens": ["Zu\u00b7letzt", "wohl", "kei\u00b7ner", "mehr", "die", "Stan\u00b7ge", "hal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PIS", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Einmal, nach dem Genu\u00df von sehr viel Wei\u00dfwein,", "tokens": ["Ein\u00b7mal", ",", "nach", "dem", "Ge\u00b7nu\u00df", "von", "sehr", "viel", "Wei\u00df\u00b7wein", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "APPR", "ART", "NN", "APPR", "ADV", "PIAT", "NN", "$,"], "meter": "+-+--+-+--+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Verstauchte er beim Spaltsitz auf dem Reck", "tokens": ["Ver\u00b7stauch\u00b7te", "er", "beim", "Spalt\u00b7sitz", "auf", "dem", "Reck"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ganz unvermutet pl\u00f6tzlich sich das Stei\u00dfbein.", "tokens": ["Ganz", "un\u00b7ver\u00b7mu\u00b7tet", "pl\u00f6tz\u00b7lich", "sich", "das", "Stei\u00df\u00b7bein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADJD", "PRF", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Er aber wich und wankte nicht vom Fleck.", "tokens": ["Er", "a\u00b7ber", "wich", "und", "wank\u00b7te", "nicht", "vom", "Fleck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "KON", "VVFIN", "PTKNEG", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Im Gegenteil, er brach, um uns zu necken,", "tokens": ["Im", "Ge\u00b7gen\u00b7teil", ",", "er", "brach", ",", "um", "uns", "zu", "ne\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PPER", "VVFIN", "$,", "KOUI", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Sich noch den Sitzknorren der Sitzbeine am Becken.", "tokens": ["Sich", "noch", "den", "Sitz\u00b7knor\u00b7ren", "der", "Sitz\u00b7bei\u00b7ne", "am", "Be\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "ART", "NN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-++--+---+-", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Er turnte gern der Jugend etwas vor", "tokens": ["Er", "turn\u00b7te", "gern", "der", "Ju\u00b7gend", "et\u00b7was", "vor"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "ADV", "APPR"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Und m\u00fchte sich vor Buben oder M\u00e4deln,", "tokens": ["Und", "m\u00fch\u00b7te", "sich", "vor", "Bu\u00b7ben", "o\u00b7der", "M\u00e4\u00b7deln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PRF", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Die Beine in die Ringe einzuf\u00e4deln,", "tokens": ["Die", "Bei\u00b7ne", "in", "die", "Rin\u00b7ge", "ein\u00b7zu\u00b7f\u00e4\u00b7deln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Wobei er niemals die Geduld verlor.", "tokens": ["Wo\u00b7bei", "er", "nie\u00b7mals", "die", "Ge\u00b7duld", "ver\u00b7lor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Dann staunte ehrfurchtsvoll solch junges Ding,", "tokens": ["Dann", "staun\u00b7te", "ehr\u00b7furchts\u00b7voll", "solch", "jun\u00b7ges", "Ding", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Wenn er wie Christbaumschmuck im Nesthang hing.", "tokens": ["Wenn", "er", "wie", "Christ\u00b7baum\u00b7schmuck", "im", "Nest\u00b7hang", "hing", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "KOKOM", "NE", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Denn was ein Nesth\u00e4ngchen werden will, kr\u00fcmmt sich beizeiter", "tokens": ["Denn", "was", "ein", "Nest\u00b7h\u00e4ng\u00b7chen", "wer\u00b7den", "will", ",", "kr\u00fcmmt", "sich", "bei\u00b7zei\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PWS", "ART", "NN", "VAINF", "VMFIN", "$,", "VVFIN", "PRF", "APPR"], "meter": "-+-+--+--+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.5": {"line.1": {"text": "Veterin\u00e4r, gleichzeitig Veteran,", "tokens": ["Ve\u00b7te\u00b7ri\u00b7n\u00e4r", ",", "gleich\u00b7zei\u00b7tig", "Ve\u00b7te\u00b7ran", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ADJD", "PTKVZ", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Ein Mann, der 92 Jahre z\u00e4hlte,", "tokens": ["Ein", "Mann", ",", "der", "92", "Jah\u00b7re", "z\u00e4hl\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "number", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "CARD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df man zuletzt ihn aus Gewohnheit w\u00e4hlte,", "tokens": ["Da\u00df", "man", "zu\u00b7letzt", "ihn", "aus", "Ge\u00b7wohn\u00b7heit", "w\u00e4hl\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und trotzdem biegsam, schmiegsam wie ein Schwan.", "tokens": ["Und", "trotz\u00b7dem", "bieg\u00b7sam", ",", "schmieg\u00b7sam", "wie", "ein", "Schwan", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "ADJD", "$,", "ADJD", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "