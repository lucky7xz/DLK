{"dta.poem.400": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "An eine sch\u00f6ne und tugendsahme Jungfer.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.71", "af:0.28"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Die Mutter dieses Runds die g\u00fcnstige Natur/", "tokens": ["Die", "Mut\u00b7ter", "die\u00b7ses", "Runds", "die", "g\u00fcns\u00b7ti\u00b7ge", "Na\u00b7tur", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PDAT", "NN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Will dich in allen Ernst recht unvergleichlich ziehren/", "tokens": ["Will", "dich", "in", "al\u00b7len", "Ernst", "recht", "un\u00b7ver\u00b7gleich\u00b7lich", "zieh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "APPR", "PIAT", "NN", "ADV", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Vor deiner Sch\u00f6nheit mu\u00df ", "tokens": ["Vor", "dei\u00b7ner", "Sch\u00f6n\u00b7heit", "mu\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VMFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Denn an der findet man nur blo\u00df der Sch\u00f6nheit-Spuhr.", "tokens": ["Denn", "an", "der", "fin\u00b7det", "man", "nur", "blo\u00df", "der", "Sch\u00f6n\u00b7heit\u00b7Sp\u00b7uhr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "VVFIN", "PIS", "ADV", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Dein Antlitzt ist ein Schnee in Purpur eingeh\u00fclt/", "tokens": ["Dein", "Ant\u00b7litzt", "ist", "ein", "Schnee", "in", "Pur\u00b7pur", "ein\u00b7ge\u00b7h\u00fclt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "NN", "APPR", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wo sich die Liljen mit sch\u00f6nen Rosen k\u00fcssen;", "tokens": ["Wo", "sich", "die", "Lil\u00b7jen", "mit", "sch\u00f6\u00b7nen", "Ro\u00b7sen", "k\u00fcs\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "ART", "NN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Die Sch\u00f6nheit hat an dir ihr Kunst-Werck drehen m\u00fcssen/", "tokens": ["Die", "Sch\u00f6n\u00b7heit", "hat", "an", "dir", "ihr", "Kunst\u00b7\u00b7Werck", "dre\u00b7hen", "m\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "PPER", "PPOSAT", "NN", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Sie zeiget/ da\u00df du bist ein \u00fcber irrdisch Bild.", "tokens": ["Sie", "zei\u00b7get", "/", "da\u00df", "du", "bist", "ein", "\u00fc\u00b7ber", "irr\u00b7disch", "Bild", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "KOUS", "PPER", "VAFIN", "ART", "APPR", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "So sch\u00f6n nun dein Gesicht/ so ist dein Hertze auch/", "tokens": ["So", "sch\u00f6n", "nun", "dein", "Ge\u00b7sicht", "/", "so", "ist", "dein", "Hert\u00b7ze", "auch", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "PPOSAT", "NN", "$(", "ADV", "VAFIN", "PPOSAT", "VVFIN", "ADV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Die Gutheit hat den Sitz darinnen auffgeschlagen", "tokens": ["Die", "Gut\u00b7heit", "hat", "den", "Sitz", "da\u00b7rin\u00b7nen", "auff\u00b7ge\u00b7schla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "ADV", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Es kan sich mit dem Stoltz im minsten nicht vertragen/", "tokens": ["Es", "kan", "sich", "mit", "dem", "Stoltz", "im", "mins\u00b7ten", "nicht", "ver\u00b7tra\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "APPR", "ART", "NN", "APPRART", "ADJA", "PTKNEG", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Gantz wider die Natur/ und ihrem \u00fcbeln Brauch.", "tokens": ["Gantz", "wi\u00b7der", "die", "Na\u00b7tur", "/", "und", "ih\u00b7rem", "\u00fc\u00b7beln", "Brauch", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$(", "KON", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Denn wenn sie sich mit Flei\u00df von aussen emsig stellt ", "tokens": ["Denn", "wenn", "sie", "sich", "mit", "Flei\u00df", "von", "aus\u00b7sen", "em\u00b7sig", "stellt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "PRF", "APPR", "NN", "APPR", "NE", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Und alle Sch\u00f6nheit denckt in das Gesicht zu setzen/", "tokens": ["Und", "al\u00b7le", "Sch\u00f6n\u00b7heit", "denckt", "in", "das", "Ge\u00b7sicht", "zu", "set\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "APPR", "ART", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "So darf man nur das Hertz nicht allzukostbahr sch\u00e4tzen/", "tokens": ["So", "darf", "man", "nur", "das", "Hertz", "nicht", "all\u00b7zu\u00b7kost\u00b7bahr", "sch\u00e4t\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIS", "ADV", "ART", "NN", "PTKNEG", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Weils denn die Laster-Brutt zusammen in sich h\u00e4lt.", "tokens": ["Weils", "denn", "die", "Las\u00b7ter\u00b7Brutt", "zu\u00b7sam\u00b7men", "in", "sich", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "ADV", "APPR", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}