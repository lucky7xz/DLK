{"textgrid.poem.41456": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Nasidien", "genre": "verse", "period": "N.A.", "pub_year": 1731, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nasidien, ein Herr von hohem Stande,", "tokens": ["Na\u00b7si\u00b7di\u00b7en", ",", "ein", "Herr", "von", "ho\u00b7hem", "Stan\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Ergr\u00fcbelte sich t\u00e4glich neue Pein,", "tokens": ["Er\u00b7gr\u00fc\u00b7bel\u00b7te", "sich", "t\u00e4g\u00b7lich", "neu\u00b7e", "Pein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und hielt es sich f\u00fcr keine kleine Schande,", "tokens": ["Und", "hielt", "es", "sich", "f\u00fcr", "kei\u00b7ne", "klei\u00b7ne", "Schan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PRF", "APPR", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den Bauern gleich, gesund und stark zu sein.", "tokens": ["Den", "Bau\u00b7ern", "gleich", ",", "ge\u00b7sund", "und", "stark", "zu", "sein", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "ADJD", "KON", "ADJD", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Er klagte j\u00fcngst dem Leibarzt, dem er zollte,", "tokens": ["Er", "klag\u00b7te", "j\u00fcngst", "dem", "Leib\u00b7arzt", ",", "dem", "er", "zoll\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ihn qu\u00e4le stets, er wisse selbst nicht was;", "tokens": ["Ihn", "qu\u00e4\u00b7le", "stets", ",", "er", "wis\u00b7se", "selbst", "nicht", "was", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PPER", "VVFIN", "ADV", "PTKNEG", "PWS", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Nur wiss' er wohl, da\u00df ihn nicht Hippocras,", "tokens": ["Nur", "wiss'", "er", "wohl", ",", "da\u00df", "ihn", "nicht", "Hip\u00b7po\u00b7cras", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "KOUS", "PPER", "PTKNEG", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Nicht Chocolad' und Gallert heilen wollte.", "tokens": ["Nicht", "Cho\u00b7co\u00b7lad'", "und", "Gal\u00b7lert", "hei\u00b7len", "woll\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "KON", "NE", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wie ist Ihr Schlaf? h\u00f6rt man den Doctor fragen.", "tokens": ["Wie", "ist", "Ihr", "Schlaf", "?", "h\u00f6rt", "man", "den", "Doc\u00b7tor", "fra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPOSAT", "NN", "$.", "VVFIN", "PIS", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Acht Stunden lang. Noch fehlt die Agrypnie.", "tokens": ["Acht", "Stun\u00b7den", "lang", ".", "Noch", "fehlt", "die", "Ag\u00b7ryp\u00b7nie", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "ADJD", "$.", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sie essen? Stark, ja bei kaum leerem Magen.", "tokens": ["Sie", "es\u00b7sen", "?", "Stark", ",", "ja", "bei", "kaum", "lee\u00b7rem", "Ma\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "$.", "NN", "$,", "ADV", "APPR", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das nennen wir, auf griechisch, Bulimie.", "tokens": ["Das", "nen\u00b7nen", "wir", ",", "auf", "grie\u00b7chisch", ",", "Bu\u00b7li\u00b7mie", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "$,", "APPR", "ADJD", "$,", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ach freilich ist der Menschen kurzes Leben", "tokens": ["Ach", "frei\u00b7lich", "ist", "der", "Men\u00b7schen", "kur\u00b7zes", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "ADV", "VAFIN", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Mit Noth beschwert, wie Avicenna spricht.", "tokens": ["Mit", "Noth", "be\u00b7schwert", ",", "wie", "A\u00b7vi\u00b7cen\u00b7na", "spricht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,", "PWAV", "NE", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Der Fra\u00df! der Schlaf! allein sie sorgen nicht:", "tokens": ["Der", "Fra\u00df", "!", "der", "Schlaf", "!", "al\u00b7lein", "sie", "sor\u00b7gen", "nicht", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct"