{"textgrid.poem.50880": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Im Tiergarten", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich bin ein Fremder hier zu Lande,", "tokens": ["Ich", "bin", "ein", "Frem\u00b7der", "hier", "zu", "Lan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wo Krongewalt herrscht allerw\u00e4rts,", "tokens": ["Wo", "Kron\u00b7ge\u00b7walt", "herrscht", "al\u00b7ler\u00b7w\u00e4rts", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mich binden nicht die starren Bande,", "tokens": ["Mich", "bin\u00b7den", "nicht", "die", "star\u00b7ren", "Ban\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Doch dieser Hain erfreut mein Herz!", "tokens": ["Doch", "die\u00b7ser", "Hain", "er\u00b7freut", "mein", "Herz", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Um dieses gr\u00fcnen Lebens willen,", "tokens": ["Um", "die\u00b7ses", "gr\u00fc\u00b7nen", "Le\u00b7bens", "wil\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PDAT", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Um dieser Weiher sanfte Flut,", "tokens": ["Um", "die\u00b7ser", "Wei\u00b7her", "sanf\u00b7te", "Flut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PDAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Um diese ruhgewiegten, stillen", "tokens": ["Um", "die\u00b7se", "ruh\u00b7ge\u00b7wieg\u00b7ten", ",", "stil\u00b7len"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["KOUI", "PDAT", "ADJA", "$,", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Baumwipfel in der Abendglut,", "tokens": ["Baum\u00b7wip\u00b7fel", "in", "der", "A\u00b7bend\u00b7glut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Um diesen tiefen milden Frieden,", "tokens": ["Um", "die\u00b7sen", "tie\u00b7fen", "mil\u00b7den", "Frie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PDAT", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Den mir ein braver Toter beut,", "tokens": ["Den", "mir", "ein", "bra\u00b7ver", "To\u00b7ter", "beut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sei ihm ein voller Dank beschieden", "tokens": ["Sei", "ihm", "ein", "vol\u00b7ler", "Dank", "be\u00b7schie\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Des Herzens, das dies Gr\u00fcn erfreut!", "tokens": ["Des", "Her\u00b7zens", ",", "das", "dies", "Gr\u00fcn", "er\u00b7freut", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$