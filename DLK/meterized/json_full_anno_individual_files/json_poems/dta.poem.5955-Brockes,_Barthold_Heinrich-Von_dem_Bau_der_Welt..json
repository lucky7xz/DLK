{"dta.poem.5955": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von dem Bau der Welt.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Durch die geschwinde Macht, wodurch absonderlich", "tokens": ["Durch", "die", "ge\u00b7schwin\u00b7de", "Macht", ",", "wo\u00b7durch", "ab\u00b7son\u00b7der\u00b7lich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "PWAV", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein jeder Wirbel sich", "tokens": ["Ein", "je\u00b7der", "Wir\u00b7bel", "sich"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PIAT", "NN", "PRF"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nun drehet, schnell herum geschwungen,", "tokens": ["Nun", "dre\u00b7het", ",", "schnell", "he\u00b7rum", "ge\u00b7schwun\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "ADJD", "PTKVZ", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Scheint jeglicher begr\u00e4ntzt, und in sich selbst gedrungen,", "tokens": ["Scheint", "jeg\u00b7li\u00b7cher", "be\u00b7gr\u00e4ntzt", ",", "und", "in", "sich", "selbst", "ge\u00b7drun\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "VVPP", "$,", "KON", "APPR", "PRF", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Als wenn er gar nicht fl\u00fc\u00dfig w\u00e4re:", "tokens": ["Als", "wenn", "er", "gar", "nicht", "fl\u00fc\u00b7\u00dfig", "w\u00e4\u00b7re", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "ADV", "PTKNEG", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wie oder recht, als ob er, durch Gewalt,", "tokens": ["Wie", "o\u00b7der", "recht", ",", "als", "ob", "er", ",", "durch", "Ge\u00b7walt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "KON", "ADJD", "$,", "KOKOM", "KOUS", "PPER", "$,", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Von einem fest- und dichten Gegenhalt", "tokens": ["Von", "ei\u00b7nem", "fest", "und", "dich\u00b7ten", "Ge\u00b7gen\u00b7halt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "TRUNC", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Befestigt und gehemmet st\u00fcnde.", "tokens": ["Be\u00b7fes\u00b7tigt", "und", "ge\u00b7hem\u00b7met", "st\u00fcn\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "VVFIN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Die unterschiedne W\u00fcrbel drehn,", "tokens": ["Die", "un\u00b7ter\u00b7schied\u00b7ne", "W\u00fcr\u00b7bel", "drehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Sich jeder um sich selbst, in ihrer R\u00fcnde,", "tokens": ["Sich", "je\u00b7der", "um", "sich", "selbst", ",", "in", "ih\u00b7rer", "R\u00fcn\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PRF", "PIS", "APPR", "PRF", "ADV", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Wodurch sie nie vermengt, noch in einander gehn.", "tokens": ["Wo\u00b7durch", "sie", "nie", "ver\u00b7mengt", ",", "noch", "in", "ein\u00b7an\u00b7der", "gehn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "VVPP", "$,", "ADV", "APPR", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}