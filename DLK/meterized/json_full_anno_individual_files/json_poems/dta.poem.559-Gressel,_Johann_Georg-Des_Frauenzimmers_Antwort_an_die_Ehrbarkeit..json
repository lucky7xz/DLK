{"dta.poem.559": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Des Frauenzimmers Antwort an die  \n Ehrbarkeit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Wir lachen Ehrbarkeit/ ob deinen Kinder Sprechen/", "tokens": ["Wir", "la\u00b7chen", "Ehr\u00b7bar\u00b7keit", "/", "ob", "dei\u00b7nen", "Kin\u00b7der", "Spre\u00b7chen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$(", "KOUS", "PPOSAT", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und sehen/ da\u00df du jetzt nicht mehr bey Sinnen bist.", "tokens": ["Und", "se\u00b7hen", "/", "da\u00df", "du", "jetzt", "nicht", "mehr", "bey", "Sin\u00b7nen", "bist", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "$(", "KOUS", "PPER", "ADV", "PTKNEG", "ADV", "APPR", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Du kanst die Satzung nicht der s\u00fcssen Wollust brechen/", "tokens": ["Du", "kanst", "die", "Sat\u00b7zung", "nicht", "der", "s\u00fcs\u00b7sen", "Wol\u00b7lust", "bre\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "PTKNEG", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die alles Frauen-Volck ohn eintzigs weigern k\u00fc\u00dft:", "tokens": ["Die", "al\u00b7les", "Frau\u00b7en\u00b7Volck", "ohn", "eint\u00b7zigs", "wei\u00b7gern", "k\u00fc\u00dft", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "APPR", "PIS", "VVFIN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Was solte unser Leib in solchen Kleidern gehen/", "tokens": ["Was", "sol\u00b7te", "un\u00b7ser", "Leib", "in", "sol\u00b7chen", "Klei\u00b7dern", "ge\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPOSAT", "NN", "APPR", "PIAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Davon der Teuffel nicht zu ", "tokens": ["Da\u00b7von", "der", "Teuf\u00b7fel", "nicht", "zu"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "ART", "NN", "PTKNEG", "PTKZU"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Was wollen wir einmahl das Alterthum besehen?", "tokens": ["Was", "wol\u00b7len", "wir", "ein\u00b7mahl", "das", "Al\u00b7ter\u00b7thum", "be\u00b7se\u00b7hen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So wird der nackte Leib erhalten Palm und Prei\u00df.", "tokens": ["So", "wird", "der", "nack\u00b7te", "Leib", "er\u00b7hal\u00b7ten", "Palm", "und", "Prei\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Drum gehe nur hinweg mit deiner Otter-M\u00fctze/", "tokens": ["Drum", "ge\u00b7he", "nur", "hin\u00b7weg", "mit", "dei\u00b7ner", "Ot\u00b7ter\u00b7M\u00fct\u00b7ze", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ADV", "ADV", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Wir brauchen nicht den Rock der unsern Pracht verdeckt/", "tokens": ["Wir", "brau\u00b7chen", "nicht", "den", "Rock", "der", "un\u00b7sern", "Pracht", "ver\u00b7deckt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ART", "NN", "ART", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Der Schleyr und Kragen wird uns jetzo nichts mehr n\u00fctze/", "tokens": ["Der", "Schleyr", "und", "Kra\u00b7gen", "wird", "uns", "jet\u00b7zo", "nichts", "mehr", "n\u00fct\u00b7ze", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VAFIN", "PPER", "ADV", "PIS", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Noch das geschlo\u00dfne Wamst so fast das Maul versteckt.", "tokens": ["Noch", "das", "ge\u00b7schlo\u00df\u00b7ne", "Wamst", "so", "fast", "das", "Maul", "ver\u00b7steckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "ADV", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Es \u00e4ndert sich die Welt/ und diese setzt die Kleider/", "tokens": ["Es", "\u00e4n\u00b7dert", "sich", "die", "Welt", "/", "und", "die\u00b7se", "setzt", "die", "Klei\u00b7der", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ART", "NN", "$(", "KON", "PDS", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Die wir ihr Eigenthum zu unsern Schmuck anziehn:", "tokens": ["Die", "wir", "ihr", "Ei\u00b7gen\u00b7thum", "zu", "un\u00b7sern", "Schmuck", "an\u00b7ziehn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Wir brauchen dich nicht mehr zu einen ", "tokens": ["Wir", "brau\u00b7chen", "dich", "nicht", "mehr", "zu", "ei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "ADV", "APPR", "ART"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Die Wollust will sich nur anjetzt damit bem\u00fchn.", "tokens": ["Die", "Wol\u00b7lust", "will", "sich", "nur", "an\u00b7jetzt", "da\u00b7mit", "be\u00b7m\u00fchn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PRF", "ADV", "ADV", "PAV", "VVINF", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.17": {"text": "Wir m\u00f6gen nun nicht mehr/ so wie die Alten gehen/", "tokens": ["Wir", "m\u00f6\u00b7gen", "nun", "nicht", "mehr", "/", "so", "wie", "die", "Al\u00b7ten", "ge\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PTKNEG", "ADV", "$(", "ADV", "KOKOM", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Da uns die junge Welt stets frische Moden giebt", "tokens": ["Da", "uns", "die", "jun\u00b7ge", "Welt", "stets", "fri\u00b7sche", "Mo\u00b7den", "giebt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "ADV", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Uns w\u00fcrde ja kein Mensch in solcher Tracht ansehen/", "tokens": ["Uns", "w\u00fcr\u00b7de", "ja", "kein", "Mensch", "in", "sol\u00b7cher", "Tracht", "an\u00b7se\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PIAT", "NN", "APPR", "PIAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Nun alles sich verneut/ und neue Moden liebt.", "tokens": ["Nun", "al\u00b7les", "sich", "ver\u00b7neut", "/", "und", "neu\u00b7e", "Mo\u00b7den", "liebt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "PRF", "VVPP", "$(", "KON", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Wir sind ein freyes Volck/ und leben nicht gebunden/", "tokens": ["Wir", "sind", "ein", "frey\u00b7es", "Volck", "/", "und", "le\u00b7ben", "nicht", "ge\u00b7bun\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$(", "KON", "VVFIN", "PTKNEG", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Wir haben deine Macht schon l\u00e4ngsten abgethan.", "tokens": ["Wir", "ha\u00b7ben", "dei\u00b7ne", "Macht", "schon", "l\u00e4ngs\u00b7ten", "ab\u00b7ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Wie/ da\u00df du den so keck dich wieder eingefunden?", "tokens": ["Wie", "/", "da\u00df", "du", "den", "so", "keck", "dich", "wie\u00b7der", "ein\u00b7ge\u00b7fun\u00b7den", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$(", "KOUS", "PPER", "ART", "ADV", "ADJD", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Wie/ da\u00df dein scharffer Mund greifft unser Mittel an?", "tokens": ["Wie", "/", "da\u00df", "dein", "scharf\u00b7fer", "Mund", "greifft", "un\u00b7ser", "Mit\u00b7tel", "an", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$(", "KOUS", "PPOSAT", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Wer achtet dein Gesetz/ dein Gifft der frohen Zeiten", "tokens": ["Wer", "ach\u00b7tet", "dein", "Ge\u00b7setz", "/", "dein", "Gifft", "der", "fro\u00b7hen", "Zei\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPOSAT", "NN", "$(", "PPOSAT", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Jetzt kahle Ehrbarkeit von deinen Kleidern mehr?", "tokens": ["Jetzt", "kah\u00b7le", "Ehr\u00b7bar\u00b7keit", "von", "dei\u00b7nen", "Klei\u00b7dern", "mehr", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "APPR", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Die Wollust l\u00e4sset uns den Kleider-Schmuck bereiten", "tokens": ["Die", "Wol\u00b7lust", "l\u00e4s\u00b7set", "uns", "den", "Klei\u00b7der\u00b7Schmuck", "be\u00b7rei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Jhr klug-verschlagnes Haupt ist nie von Moden leer.", "tokens": ["Ihr", "klug\u00b7ver\u00b7schlagnes", "Haupt", "ist", "nie", "von", "Mo\u00b7den", "leer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "ADV", "APPR", "NN", "ADJD", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.29": {"text": "Hingegen mu\u00df man stets nach einer Pfeiffen tantzen/", "tokens": ["Hin\u00b7ge\u00b7gen", "mu\u00df", "man", "stets", "nach", "ei\u00b7ner", "Pfeif\u00b7fen", "tant\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIS", "ADV", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Wenn man die Kleider noch dir zu Gefallen macht/", "tokens": ["Wenn", "man", "die", "Klei\u00b7der", "noch", "dir", "zu", "Ge\u00b7fal\u00b7len", "macht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "NN", "ADV", "PPER", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Da mu\u00df die freye Brust sich lassen vest verschantzen/", "tokens": ["Da", "mu\u00df", "die", "frey\u00b7e", "Brust", "sich", "las\u00b7sen", "vest", "ver\u00b7schant\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "ADJA", "NN", "PRF", "VVINF", "VVFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "So da\u00df kein Auge sieht der Liebes-Ballen-Pracht.", "tokens": ["So", "da\u00df", "kein", "Au\u00b7ge", "sieht", "der", "Lie\u00b7bes\u00b7Bal\u00b7len\u00b7Pracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PIAT", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Nun aber legen wir der Wollust reiche Waare/", "tokens": ["Nun", "a\u00b7ber", "le\u00b7gen", "wir", "der", "Wol\u00b7lust", "rei\u00b7che", "Waa\u00b7re", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Als ein ", "tokens": ["Als", "ein"], "token_info": ["word", "word"], "pos": ["KOUS", "ART"], "meter": "-+", "measure": "iambic.single"}, "line.35": {"text": "Sie braucht die blosse Brust zu einen Lust-Altare", "tokens": ["Sie", "braucht", "die", "blos\u00b7se", "Brust", "zu", "ei\u00b7nen", "Lust\u00b7Al\u00b7ta\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Wer ist der ihrer Macht wol wiederstehen kan.", "tokens": ["Wer", "ist", "der", "ih\u00b7rer", "Macht", "wol", "wie\u00b7ders\u00b7te\u00b7hen", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "PPOSAT", "NN", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-++--+", "measure": "iambic.hexa.chol"}, "line.37": {"text": "So macht die Bl\u00f6sse uns im minsten nicht verachtet/", "tokens": ["So", "macht", "die", "Bl\u00f6s\u00b7se", "uns", "im", "mins\u00b7ten", "nicht", "ver\u00b7ach\u00b7tet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPER", "APPRART", "ADJA", "PTKNEG", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Wie deine Albernheit aus grosser Einfalt spricht/", "tokens": ["Wie", "dei\u00b7ne", "Al\u00b7bern\u00b7heit", "aus", "gros\u00b7ser", "Ein\u00b7falt", "spricht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Nein/ sondern unser Schatz/ wird als ein Schatz betrachtet", "tokens": ["Nein", "/", "son\u00b7dern", "un\u00b7ser", "Schatz", "/", "wird", "als", "ein", "Schatz", "be\u00b7trach\u00b7tet"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$(", "KON", "PPOSAT", "NN", "$(", "VAFIN", "KOKOM", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Worauf ein jedes Hertz den heissen Wunsch gericht.", "tokens": ["Wo\u00b7rauf", "ein", "je\u00b7des", "Hertz", "den", "heis\u00b7sen", "Wunsch", "ge\u00b7richt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "PIAT", "NN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Da\u00df aber du uns gar den Barbarn nach wilt setzen/", "tokens": ["Da\u00df", "a\u00b7ber", "du", "uns", "gar", "den", "Bar\u00b7barn", "nach", "wilt", "set\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPER", "PRF", "ADV", "ART", "NN", "APPR", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Das sehn wir als ein Werck von grosser Dummheit an.", "tokens": ["Das", "sehn", "wir", "als", "ein", "Werck", "von", "gros\u00b7ser", "Dumm\u00b7heit", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFI