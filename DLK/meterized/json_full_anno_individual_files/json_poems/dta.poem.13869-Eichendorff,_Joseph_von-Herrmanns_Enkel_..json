{"dta.poem.13869": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Herrmanns Enkel .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Altdeutsch! &#8212; Altdeutsch? &#8212; Nun, das ist,", "tokens": ["Alt\u00b7deutsch", "!", "&#8212;", "Alt\u00b7deutsch", "?", "&#8212;", "Nun", ",", "das", "ist", ","], "token_info": ["word", "punct", "XML_entity", "word", "punct", "XML_entity", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "$.", "$(", "NN", "$.", "$(", "ADV", "$,", "PDS", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Was man so in B\u00fcchern liest: \u2014", "tokens": ["Was", "man", "so", "in", "B\u00fc\u00b7chern", "liest", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "PIS", "ADV", "APPR", "NN", "VVFIN", "$.", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Kluge Rosse \u2014 pr\u00e4cht'ge Decken,", "tokens": ["Klu\u00b7ge", "Ros\u00b7se", "pr\u00e4cht'\u00b7ge", "De\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$(", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "H\u00e4ndel, Kruzifixe, Recken \u2014", "tokens": ["H\u00e4n\u00b7del", ",", "Kru\u00b7zi\u00b7fi\u00b7xe", ",", "Re\u00b7cken"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "O, wie herrlich strahlt dies Leben!", "tokens": ["O", ",", "wie", "herr\u00b7lich", "strahlt", "dies", "Le\u00b7ben", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "ADJD", "VVFIN", "PDS", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "G\u00f6ttlich! \u2014 Doch mit Unterschied.", "tokens": ["G\u00f6tt\u00b7lich", "!", "Doch", "mit", "Un\u00b7ter\u00b7schied", "."], "token_info": ["word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$.", "$(", "KON", "APPR", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.7": {"text": "Es versteht sich, da\u00df man's deute \u2014", "tokens": ["Es", "ver\u00b7steht", "sich", ",", "da\u00df", "man's", "deu\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "$,", "KOUS", "PIS", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "'s w\u00e4r' doch gar zu unbequem,", "tokens": ["'", "s", "w\u00e4r", "'", "doch", "gar", "zu", "un\u00b7be\u00b7quem", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "$(", "ADV", "ADV", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Wenn man Alles w\u00f6rtlich n\u00e4hm',", "tokens": ["Wenn", "man", "Al\u00b7les", "w\u00f6rt\u00b7lich", "n\u00e4hm'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PIS", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Wie's da durcheinander bl\u00fcht! \u2014", "tokens": ["Wie's", "da", "durch\u00b7ein\u00b7an\u00b7der", "bl\u00fcht", "!"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "ADV", "ADV", "VVFIN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Diese Ritter \u2014 gute Leute,", "tokens": ["Die\u00b7se", "Rit\u00b7ter", "gu\u00b7te", "Leu\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PDAT", "NN", "$(", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "Ehrlich, tapfer, brave Reiter \u2014", "tokens": ["Ehr\u00b7lich", ",", "tap\u00b7fer", ",", "bra\u00b7ve", "Rei\u00b7ter"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "$,", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Gegen uns doch B\u00e4renh\u00e4uter!", "tokens": ["Ge\u00b7gen", "uns", "doch", "B\u00e4\u00b7ren\u00b7h\u00e4u\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Eigentlich sind wir wohl weiter.", "tokens": ["Ei\u00b7gent\u00b7lich", "sind", "wir", "wohl", "wei\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "PTKVZ", "$."], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.15": {"text": "Lehnstreu \u2014 Kl\u00f6ster \u2014 Barbarei \u2014", "tokens": ["Lehn\u00b7streu", "Kl\u00f6s\u00b7ter", "Bar\u00b7ba\u00b7rei"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$(", "NN", "$(", "NN", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.16": {"text": "Davon machen wir uns frei. \u2014", "tokens": ["Da\u00b7von", "ma\u00b7chen", "wir", "uns", "frei", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PRF", "ADJD", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.17": {"text": "Fangen wir ", "tokens": ["Fan\u00b7gen", "wir"], "token_info": ["word", "word"], "pos": ["VVFIN", "PPER"], "meter": "+-+", "measure": "trochaic.di"}, "line.18": {"text": "F\u00fcrcht' ich, bleibt es bei Gedichten \u2014", "tokens": ["F\u00fcrcht'", "ich", ",", "bleibt", "es", "bei", "Ge\u00b7dich\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "VVFIN", "PPER", "APPR", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "Nein doch! Eines, geht mir bei,", "tokens": ["Nein", "doch", "!", "Ei\u00b7nes", ",", "geht", "mir", "bei", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADV", "$.", "PIS", "$,", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.20": {"text": "Eines bleibt doch: Dies Vernichten", "tokens": ["Ei\u00b7nes", "bleibt", "doch", ":", "Dies", "Ver\u00b7nich\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PIS", "VVFIN", "ADV", "$.", "PDS", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "Aller Mode-Sklaverei! \u2014", "tokens": ["Al\u00b7ler", "Mo\u00b7de\u00b7S\u00b7kla\u00b7ve\u00b7rei", "!"], "token_info": ["word", "word", "punct", "punct"], "pos": ["ADJA", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.22": {"text": "Hohe Vaterl\u00e4nderei!", "tokens": ["Ho\u00b7he", "Va\u00b7ter\u00b7l\u00e4n\u00b7de\u00b7rei", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.23": {"text": "Schnittst Du los nicht Herrmanns S\u00f6hne", "tokens": ["Schnittst", "Du", "los", "nicht", "Herr\u00b7manns", "S\u00f6h\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "PTKNEG", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "Von des Halstuchs schn\u00f6den Schlingen,", "tokens": ["Von", "des", "Hal\u00b7stuchs", "schn\u00f6\u00b7den", "Schlin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.25": {"text": "In den'n, sonder Kraft und Sch\u00f6ne,", "tokens": ["In", "den'n", ",", "son\u00b7der", "Kraft", "und", "Sch\u00f6\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "KON", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.26": {"text": "Unsre V\u00e4ter schm\u00e4hlich hingen?", "tokens": ["Uns\u00b7re", "V\u00e4\u00b7ter", "schm\u00e4h\u00b7lich", "hin\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.27": {"text": "Gabst Du nicht dem L\u00f6wen M\u00e4hne,", "tokens": ["Gabst", "Du", "nicht", "dem", "L\u00f6\u00b7wen", "M\u00e4h\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.28": {"text": "Die ihm frech die Zeit gestohlen?", "tokens": ["Die", "ihm", "frech", "die", "Zeit", "ge\u00b7stoh\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}