{"dta.poem.5737": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Allgemeine Betrachtungen der Materie  \n und der Bewegung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Durch diese Liebe nun, als Ursprung der Natur,", "tokens": ["Durch", "die\u00b7se", "Lie\u00b7be", "nun", ",", "als", "Ur\u00b7sprung", "der", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "ADV", "$,", "KOUS", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+---+", "measure": "unknown.measure.penta"}, "line.2": {"text": "Die ", "tokens": ["Die"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.3": {"text": "Erkl\u00e4rten sie gar sch\u00f6n den ", "tokens": ["Er\u00b7kl\u00e4r\u00b7ten", "sie", "gar", "sch\u00f6n", "den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ADJD", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So ", "tokens": ["So"], "token_info": ["word"], "pos": ["ADV"], "meter": "-", "measure": "single.down"}, "line.5": {"text": "Die aller Dinge Seyn und Eigenschafft", "tokens": ["Die", "al\u00b7ler", "Din\u00b7ge", "Seyn", "und", "Ei\u00b7gen\u00b7schafft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PIAT", "NN", "PPOSAT", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Aus der Materie hervor hie\u00df steigen;", "tokens": ["Aus", "der", "Ma\u00b7te\u00b7rie", "her\u00b7vor", "hie\u00df", "stei\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKVZ", "VVFIN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Die man jedoch nicht zeigen,", "tokens": ["Die", "man", "je\u00b7doch", "nicht", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.8": {"text": "Und nicht beschreiben kan, die kein Verstand ermiss\u2019t:", "tokens": ["Und", "nicht", "be\u00b7schrei\u00b7ben", "kan", ",", "die", "kein", "Ver\u00b7stand", "er\u00b7miss't", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "VVINF", "VMFIN", "$,", "PRELS", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Weil sie die erstere, weil sie vor allen Dingen", "tokens": ["Weil", "sie", "die", "ers\u00b7te\u00b7re", ",", "weil", "sie", "vor", "al\u00b7len", "Din\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADJA", "$,", "KOUS", "PPER", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Ein\u2019 Ursach jeder Ursach ist:", "tokens": ["Ein'", "Ur\u00b7sach", "je\u00b7der", "Ur\u00b7sach", "ist", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Dahin, wo sie entspringt, kan niemand dringen.", "tokens": ["Da\u00b7hin", ",", "wo", "sie", "ent\u00b7springt", ",", "kan", "nie\u00b7mand", "drin\u00b7gen", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "PWAV", "PPER", "VVFIN", "$,", "VMFIN", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Sie ist zu aller Zeit bekannt und unbekannt:", "tokens": ["Sie", "ist", "zu", "al\u00b7ler", "Zeit", "be\u00b7kannt", "und", "un\u00b7be\u00b7kannt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PIAT", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Kein Zwang, kein\u2019 Arbeit kan zu ihr das Auge bringen.", "tokens": ["Kein", "Zwang", ",", "kein'", "Ar\u00b7beit", "kan", "zu", "ihr", "das", "Au\u00b7ge", "brin\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN", "VMFIN", "APPR", "PPOSAT", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Die Eigenschafften nur erkennet der Verstand;", "tokens": ["Die", "Ei\u00b7gen\u00b7schaff\u00b7ten", "nur", "er\u00b7ken\u00b7net", "der", "Ver\u00b7stand", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Blos durch die W\u00fcrckungen vermag man sie zu mercken.", "tokens": ["Blos", "durch", "die", "W\u00fcr\u00b7ckun\u00b7gen", "ver\u00b7mag", "man", "sie", "zu", "mer\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "PIS", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.16": {"text": "Durch so viel Sch\u00f6nheit, Pracht und Vollenkommenheit,", "tokens": ["Durch", "so", "viel", "Sch\u00f6n\u00b7heit", ",", "Pracht", "und", "Vol\u00b7len\u00b7kom\u00b7men\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "PIAT", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.17": {"text": "Vortrefflichkeit und Herrlichkeit,", "tokens": ["Vor\u00b7treff\u00b7lich\u00b7keit", "und", "Herr\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Die allenthalben ausgespannt;", "tokens": ["Die", "al\u00b7len\u00b7thal\u00b7ben", "aus\u00b7ge\u00b7spannt", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Zumalen in den grossen Wercken,", "tokens": ["Zu\u00b7ma\u00b7len", "in", "den", "gros\u00b7sen", "Wer\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Die unserem Gesicht so wundersch\u00f6n,", "tokens": ["Die", "un\u00b7se\u00b7rem", "Ge\u00b7sicht", "so", "wun\u00b7der\u00b7sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "L\u00e4sst GOTT von sich die Z\u00fcge sehn.", "tokens": ["L\u00e4sst", "GoTT", "von", "sich", "die", "Z\u00fc\u00b7ge", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "APPR", "PRF", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Durch Jhn vereinigte mit C\u00f6rpern, welche schwer,", "tokens": ["Durch", "Jhn", "ver\u00b7ei\u00b7nig\u00b7te", "mit", "C\u00f6r\u00b7pern", ",", "wel\u00b7che", "schwer", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "APPR", "NN", "$,", "PRELS", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Sich die Bewegung erst: Er theilte das, was feucht;", "tokens": ["Sich", "die", "Be\u00b7we\u00b7gung", "erst", ":", "Er", "theil\u00b7te", "das", ",", "was", "feucht", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PRF", "ART", "NN", "ADV", "$.", "PPER", "VVFIN", "PDS", "$,", "PRELS", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Die Erde gr\u00fcndet\u2019 Er; Es flo\u00df durch Jhn das Meer,", "tokens": ["Die", "Er\u00b7de", "gr\u00fcn\u00b7det'", "Er", ";", "Es", "flo\u00df", "durch", "Jhn", "das", "Meer", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$.", "PPER", "VVFIN", "APPR", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Das weite Feld der Lufft bereitet\u2019 Seine Hand.", "tokens": ["Das", "wei\u00b7te", "Feld", "der", "Lufft", "be\u00b7rei\u00b7tet'", "Sei\u00b7ne", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Er hat das Firmament des Himmels ausgespannt,", "tokens": ["Er", "hat", "das", "Fir\u00b7ma\u00b7ment", "des", "Him\u00b7mels", "aus\u00b7ge\u00b7spannt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Vom Schimmer, Glanz und Licht, von dem, was schwer,", "tokens": ["Vom", "Schim\u00b7mer", ",", "Glanz", "und", "Licht", ",", "von", "dem", ",", "was", "schwer", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "NN", "KON", "NN", "$,", "APPR", "ART", "$,", "PRELS", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.28": {"text": "was leicht,", "tokens": ["was", "leicht", ","], "token_info": ["word", "word", "punct"], "pos": ["PWS", "ADJD", "$,"], "meter": "-+", "measure": "iambic.single"}}}}}