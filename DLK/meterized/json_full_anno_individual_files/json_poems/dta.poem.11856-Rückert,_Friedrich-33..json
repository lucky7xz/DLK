{"dta.poem.11856": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "33.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.85", "af:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Seele, die herab ist in den Leib gestiegen,", "tokens": ["Die", "See\u00b7le", ",", "die", "her\u00b7ab", "ist", "in", "den", "Leib", "ge\u00b7stie\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "VAFIN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hat halb, dem Vogel gleich im Baur, verlernt das Fliegen;", "tokens": ["Hat", "halb", ",", "dem", "Vo\u00b7gel", "gleich", "im", "Baur", ",", "ver\u00b7lernt", "das", "Flie\u00b7gen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "$,", "PRELS", "NE", "ADV", "APPRART", "NN", "$,", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Nahm Schwere an und gab dem Leib des Schwunges Kraft,", "tokens": ["Nahm", "Schwe\u00b7re", "an", "und", "gab", "dem", "Leib", "des", "Schwun\u00b7ges", "Kraft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "PTKVZ", "KON", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df sie halb leibhaft ward, der Leib halb seelenhaft.", "tokens": ["Da\u00df", "sie", "halb", "leib\u00b7haft", "ward", ",", "der", "Leib", "halb", "see\u00b7len\u00b7haft", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "ADJD", "VAFIN", "$,", "ART", "NN", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Sie hat ein dunkles Haus mit ihrem Licht erhellt,", "tokens": ["Sie", "hat", "ein", "dunk\u00b7les", "Haus", "mit", "ih\u00b7rem", "Licht", "er\u00b7hellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Deswegen aber selbst ins Dunkle sich gestellt.", "tokens": ["Des\u00b7we\u00b7gen", "a\u00b7ber", "selbst", "ins", "Dunk\u00b7le", "sich", "ge\u00b7stellt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "ADV", "APPRART", "NN", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Sie hat dem todten Leib sein Leben eingegeben,", "tokens": ["Sie", "hat", "dem", "tod\u00b7ten", "Leib", "sein", "Le\u00b7ben", "ein\u00b7ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Aufgebend selbst um Tod ein Theil von ihrem Leben.", "tokens": ["Auf\u00b7ge\u00b7bend", "selbst", "um", "Tod", "ein", "Theil", "von", "ih\u00b7rem", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "ADV", "APPR", "NN", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "---+-+-+-+-+-", "measure": "unknown.measure.penta"}}, "stanza.5": {"line.1": {"text": "Die Liebe wars, die sie zu ihm herniederzog,", "tokens": ["Die", "Lie\u00b7be", "wars", ",", "die", "sie", "zu", "ihm", "her\u00b7nie\u00b7der\u00b7zog", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$,", "PRELS", "PPER", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit ihm in Staub zu gehn, die ohn' ihn droben flog.", "tokens": ["Mit", "ihm", "in", "Staub", "zu", "gehn", ",", "die", "ohn'", "ihn", "dro\u00b7ben", "flog", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPR", "NN", "PTKZU", "VVINF", "$,", "PRELS", "APPR", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Sowie dem Glauben auch herab sich hat gelassen", "tokens": ["So\u00b7wie", "dem", "Glau\u00b7ben", "auch", "her\u00b7ab", "sich", "hat", "ge\u00b7las\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADV", "ADV", "PRF", "VAFIN", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Gottheit menschlich, da\u00df sie Menschenherzen fassen.", "tokens": ["Die", "Got\u00b7theit", "menschlich", ",", "da\u00df", "sie", "Men\u00b7schen\u00b7her\u00b7zen", "fas\u00b7sen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$,", "KOUS", "PPER", "NN", "VVINF", "$."], "meter": "--+-+-+-+-+-", "measure": "anapaest.init"}}, "stanza.7": {"line.1": {"text": "Und wie ein Weiser aus der Weisheit hellen Sf\u00e4ren", "tokens": ["Und", "wie", "ein", "Wei\u00b7ser", "aus", "der", "Weis\u00b7heit", "hel\u00b7len", "Sf\u00e4\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "NN", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Herabsteigt, um die Nacht der Bl\u00f6dheit aufzukl\u00e4ren.", "tokens": ["Her\u00b7ab\u00b7steigt", ",", "um", "die", "Nacht", "der", "Bl\u00f6d\u00b7heit", "auf\u00b7zu\u00b7kl\u00e4\u00b7ren", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUI", "ART", "NN", "ART", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Er will sich eines Theils der Weisheit gern begeben,", "tokens": ["Er", "will", "sich", "ei\u00b7nes", "Theils", "der", "Weis\u00b7heit", "gern", "be\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "ART", "NN", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Um die Unwissenheit zum Wissen zu erheben.", "tokens": ["Um", "die", "Un\u00b7wis\u00b7sen\u00b7heit", "zum", "Wis\u00b7sen", "zu", "er\u00b7he\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "APPRART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "In jeder Lebenssf\u00e4r', in jedem Wirkungskreise,", "tokens": ["In", "je\u00b7der", "Le\u00b7bens\u00b7sf\u00e4r'", ",", "in", "je\u00b7dem", "Wir\u00b7kungs\u00b7krei\u00b7se", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "L\u00e4\u00dft sich der h\u00f6hre Geist herab auf solche Weise.", "tokens": ["L\u00e4\u00dft", "sich", "der", "h\u00f6h\u00b7re", "Geist", "her\u00b7ab", "auf", "sol\u00b7che", "Wei\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "ADJA", "NN", "ADV", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.10": {"line.1": {"text": "Mit Demut, Dienstbarkeit, Lieb' und Aufopferung,", "tokens": ["Mit", "De\u00b7mut", ",", "Dienst\u00b7bar\u00b7keit", ",", "Lieb'", "und", "Auf\u00b7op\u00b7fe\u00b7rung", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Sucht er das Niedre stets, und gibt ihm h\u00f6hern Schwung.", "tokens": ["Sucht", "er", "das", "Nied\u00b7re", "stets", ",", "und", "gibt", "ihm", "h\u00f6\u00b7hern", "Schwung", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "PIS", "ADV", "$,", "KON", "VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.11": {"line.1": {"text": "Mit Tugend, Kraft und That, mit Anmut, Scherz und Witz,", "tokens": ["Mit", "Tu\u00b7gend", ",", "Kraft", "und", "That", ",", "mit", "An\u00b7mut", ",", "Scherz", "und", "Witz", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "$,", "APPR", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie Sonnenschein und Thau, wie Regensturm und Blitz;", "tokens": ["Wie", "Son\u00b7nen\u00b7schein", "und", "Thau", ",", "wie", "Re\u00b7gen\u00b7sturm", "und", "Blitz", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "$,", "PWAV", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.12": {"line.1": {"text": "So manichfaltig steigt der Himmel stets zur Erde,", "tokens": ["So", "ma\u00b7nich\u00b7fal\u00b7tig", "steigt", "der", "Him\u00b7mel", "stets", "zur", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "ART", "NN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Damit das Irdische des Himmels theilhaft werde;", "tokens": ["Da\u00b7mit", "das", "Ir\u00b7di\u00b7sche", "des", "Him\u00b7mels", "theil\u00b7haft", "wer\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "ART", "NN", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.13": {"line.1": {"text": "Damit das Leben so sich mit dem Tod vers\u00f6hne,", "tokens": ["Da\u00b7mit", "das", "Le\u00b7ben", "so", "sich", "mit", "dem", "Tod", "ver\u00b7s\u00f6h\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "ADV", "PRF", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-++--+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und aus dem Staub erbl\u00fch die Luft der Welt, das Sch\u00f6ne.", "tokens": ["Und", "aus", "dem", "Staub", "er\u00b7bl\u00fch", "die", "Luft", "der", "Welt", ",", "das", "Sch\u00f6\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADJD", "ART", "NN", "ART", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}