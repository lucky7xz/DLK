{"textgrid.poem.39294": {"metadata": {"author": {"name": "Strachwitz, Moritz von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mir ist, als h\u00f6rt' ich sagen:", "genre": "verse", "period": "N.A.", "pub_year": 1834, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mir ist, als h\u00f6rt' ich sagen:", "tokens": ["Mir", "ist", ",", "als", "h\u00f6rt'", "ich", "sa\u00b7gen", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "KOUS", "VVFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Es ward einmal in schwarzer Stund',", "tokens": ["Es", "ward", "ein\u00b7mal", "in", "schwar\u00b7zer", "Stund'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Im Waldesgrund,", "tokens": ["Im", "Wal\u00b7des\u00b7grund", ","], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Ein S\u00e4ngersmann erschlagen.", "tokens": ["Ein", "S\u00e4n\u00b7gers\u00b7mann", "er\u00b7schla\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und als er schlief in der Erde tief", "tokens": ["Und", "als", "er", "schlief", "in", "der", "Er\u00b7de", "tief"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "APPR", "ART", "NN", "ADJD"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "An zweigumfl\u00fcsterter Stell',", "tokens": ["An", "zwei\u00b7gum\u00b7fl\u00fcs\u00b7ter\u00b7ter", "Stell'", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Da quoll aus seinem Herzen empor,", "tokens": ["Da", "quoll", "aus", "sei\u00b7nem", "Her\u00b7zen", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.8": {"text": "Aus Blumen hervor", "tokens": ["Aus", "Blu\u00b7men", "her\u00b7vor"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NN", "PTKVZ"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.9": {"text": "Ein sangaufrauschender Quell.", "tokens": ["Ein", "san\u00b7gauf\u00b7rau\u00b7schen\u00b7der", "Quell", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Der s\u00e4uselnde Ast und der Vogel drin,", "tokens": ["Der", "s\u00e4u\u00b7seln\u00b7de", "Ast", "und", "der", "Vo\u00b7gel", "drin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "ART", "NN", "PTKVZ", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Sie lernten die Melodie,", "tokens": ["Sie", "lern\u00b7ten", "die", "Me\u00b7lo\u00b7die", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Es badet die Elfenk\u00f6nigin", "tokens": ["Es", "ba\u00b7det", "die", "El\u00b7fen\u00b7k\u00f6\u00b7ni\u00b7gin"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Im Quelle ihr schneewei\u00df Knie.", "tokens": ["Im", "Quel\u00b7le", "ihr", "schnee\u00b7wei\u00df", "Knie", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "VVFIN", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Er rinnt durch die k\u00f6stliche Waldesruh'", "tokens": ["Er", "rinnt", "durch", "die", "k\u00f6st\u00b7li\u00b7che", "Wal\u00b7des\u00b7ruh'"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Wohl immerzu,", "tokens": ["Wohl", "im\u00b7mer\u00b7zu", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "ADV", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Ihm ist gar still zu Mut \u2013", "tokens": ["Ihm", "ist", "gar", "still", "zu", "Mut", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "APPR", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "O! Dreimal seliger Dichter Du,", "tokens": ["O", "!", "Drei\u00b7mal", "se\u00b7li\u00b7ger", "Dich\u00b7ter", "Du", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "ADV", "ADJA", "NN", "PPER", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Wie schl\u00e4fst Du k\u00fchl und gut!", "tokens": ["Wie", "schl\u00e4fst", "Du", "k\u00fchl", "und", "gut", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Mir ist, als h\u00f6rt' ich sagen:", "tokens": ["Mir", "ist", ",", "als", "h\u00f6rt'", "ich", "sa\u00b7gen", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "KOUS", "VVFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Es ward einma