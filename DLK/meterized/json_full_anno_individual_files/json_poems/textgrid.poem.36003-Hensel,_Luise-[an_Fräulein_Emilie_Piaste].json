{"textgrid.poem.36003": {"metadata": {"author": {"name": "Hensel, Luise", "birth": "N.A.", "death": "N.A."}, "title": "[an Fr\u00e4ulein Emilie Piaste]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "An Fr\u00e4ulein Emilie Piaste,", "tokens": ["An", "Fr\u00e4u\u00b7lein", "E\u00b7mi\u00b7lie", "Pi\u00b7as\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NE", "NE", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die ich ach so gerne heut umfa\u00dfte,", "tokens": ["Die", "ich", "ach", "so", "ger\u00b7ne", "heut", "um\u00b7fa\u00df\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADV", "ADV", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Damit sie den wichtigen Brief auch bald h\u00e4tte,", "tokens": ["Da\u00b7mit", "sie", "den", "wich\u00b7ti\u00b7gen", "Brief", "auch", "bald", "h\u00e4t\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "ADV", "ADV", "VAFIN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Schick ich ihn durch eine Pantoffel-Stafette.", "tokens": ["Schick", "ich", "ihn", "durch", "ei\u00b7ne", "Pan\u00b7tof\u00b7fel\u00b7\u00b7S\u00b7ta\u00b7fet\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPER", "APPR", "ART", "NN", "$."], "meter": "+---+-+--+-+-", "measure": "trochaic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Den Fingerhut ich schicke dir,", "tokens": ["Den", "Fin\u00b7ger\u00b7hut", "ich", "schi\u00b7cke", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das h\u00fcbsche Liedchen auch allhier,", "tokens": ["Das", "h\u00fcb\u00b7sche", "Lied\u00b7chen", "auch", "all\u00b7hier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und auch erfolgt ein Gru\u00df von mir.", "tokens": ["Und", "auch", "er\u00b7folgt", "ein", "Gru\u00df", "von", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Gib Kind, Antonen diesen Kamm. \u2013", "tokens": ["Gib", "Kind", ",", "An\u00b7to\u00b7nen", "die\u00b7sen", "Kamm", ".", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["VVIMP", "NN", "$,", "NN", "PDAT", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn deine Hand dies Briefchen nahm,", "tokens": ["Wenn", "dei\u00b7ne", "Hand", "dies", "Brief\u00b7chen", "nahm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PDS", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sitz' ich an meinem N\u00e4he-Rahm.", "tokens": ["Sitz'", "ich", "an", "mei\u00b7nem", "N\u00e4\u00b7he\u00b7Rahm", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Und sticke auf den wei\u00dfen Grund", "tokens": ["Und", "sti\u00b7cke", "auf", "den", "wei\u00b7\u00dfen", "Grund"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Viel lustige Bl\u00fcmchen h\u00fcbsch und bunt", "tokens": ["Viel", "lus\u00b7ti\u00b7ge", "Bl\u00fcm\u00b7chen", "h\u00fcbsch", "und", "bunt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "ADJA", "NN", "ADJD", "KON", "ADJD"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und denke dein zu jeder Stund.", "tokens": ["Und", "den\u00b7ke", "dein", "zu", "je\u00b7der", "Stund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Gern stickt' ich zarte Rosen mir,", "tokens": ["Gern", "stickt'", "ich", "zar\u00b7te", "Ro\u00b7sen", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJA", "NN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dann h\u00e4tt' ich deine Wangen hier", "tokens": ["Dann", "h\u00e4tt'", "ich", "dei\u00b7ne", "Wan\u00b7gen", "hier"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und sehnte mich nicht so nach dir.", "tokens": ["Und", "sehn\u00b7te", "mich", "nicht", "so", "nach", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Doch weil Mamsell la Garde gebot,", "tokens": ["Doch", "weil", "Mam\u00b7sell", "la", "Gar\u00b7de", "ge\u00b7bot", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "NN", "NE", "NN", "VVFIN", "$,"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.2": {"text": "So stick' ich nach der neusten Mod'", "tokens": ["So", "stick'", "ich", "nach", "der", "neus\u00b7ten", "Mod'"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Des garstigen Klatschmohns dunkles Rot.", "tokens": ["Des", "gars\u00b7ti\u00b7gen", "Klatschmohns", "dunk\u00b7les", "Rot", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Das ist zu meinem gr\u00f6\u00dften Leid,", "tokens": ["Das", "ist", "zu", "mei\u00b7nem", "gr\u00f6\u00df\u00b7ten", "Leid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zu meiner tiefsten B\u00e4nglichkeit,", "tokens": ["Zu", "mei\u00b7ner", "tiefs\u00b7ten", "B\u00e4ng\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Kein Bild von dir, du ros'ge Maid!", "tokens": ["Kein", "Bild", "von", "dir", ",", "du", "ros'\u00b7ge", "Maid", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "PPER", "$,", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Nun greif ich bald mit frohem Sinn,", "tokens": ["Nun", "greif", "ich", "bald", "mit", "fro\u00b7hem", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Um deinen Wuchs mir zu erziehn,", "tokens": ["Um", "dei\u00b7nen", "Wuchs", "mir", "zu", "er\u00b7ziehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nach meiner Stengel sanftem Gr\u00fcn.", "tokens": ["Nach", "mei\u00b7ner", "Sten\u00b7gel", "sanf\u00b7tem", "Gr\u00fcn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Doch alles will mich heut verwirr'n,", "tokens": ["Doch", "al\u00b7les", "will", "mich", "heut", "ver\u00b7wirr'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nur dicke Knubel schafft mein Zwirn,", "tokens": ["Nur", "di\u00b7cke", "Knu\u00b7bel", "schafft", "mein", "Zwirn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Kein Bild von dir, du schlanke Dirn.", "tokens": ["Kein", "Bild", "von", "dir", ",", "du", "schlan\u00b7ke", "Dirn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "PPER", "$,", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Nun stellt' ich deiner Augen Blau", "tokens": ["Nun", "stellt'", "ich", "dei\u00b7ner", "Au\u00b7gen", "Blau"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So gern der kranken Brust zur Schau", "tokens": ["So", "gern", "der", "kran\u00b7ken", "Brust", "zur", "Schau"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und finde \u2013 r\u00f6tlich blasses Grau.", "tokens": ["Und", "fin\u00b7de", "\u2013", "r\u00f6t\u00b7lich", "blas\u00b7ses", "Grau", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$(", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Und was auch meine Sehnsucht klagt,", "tokens": ["Und", "was", "auch", "mei\u00b7ne", "Sehn\u00b7sucht", "klagt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und was auch meine Liebe wagt,", "tokens": ["Und", "was", "auch", "mei\u00b7ne", "Lie\u00b7be", "wagt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Kein Bild von dir, blau\u00e4ug'ge Magd!", "tokens": ["Kein", "Bild", "von", "dir", ",", "blau\u00b7\u00e4ug'\u00b7ge", "Magd", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "PPER", "$,", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.12": {"line.1": {"text": "Nun sag mir eins noch ganz gewi\u00df:", "tokens": ["Nun", "sag", "mir", "eins", "noch", "ganz", "ge\u00b7wi\u00df", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIS", "ADV", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hast Hoffnung du zu dem Servis?", "tokens": ["Hast", "Hoff\u00b7nung", "du", "zu", "dem", "Ser\u00b7vis", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sag mir, o trautes M\u00e4gdlein dies.", "tokens": ["Sag", "mir", ",", "o", "trau\u00b7tes", "M\u00e4gd\u00b7lein", "dies", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "FM", "ADJA", "NN", "PDS", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.13": {"line.1": {"text": "N.S.", "tokens": ["N.", "S."], "token_info": ["abbreviation", "abbreviation"], "pos": ["NE", "NE"]}, "line.2": {"text": "Gr\u00fc\u00df mir die muntre Schwester fein,", "tokens": ["Gr\u00fc\u00df", "mir", "die", "mun\u00b7tre", "Schwes\u00b7ter", "fein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Auch Doris soll gegr\u00fc\u00dfet sein. \u2013", "tokens": ["Auch", "Do\u00b7ris", "soll", "ge\u00b7gr\u00fc\u00b7\u00dfet", "sein", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "NE", "VMFIN", "VVPP", "VAINF", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.14": {"line.1": {"text": "An Fr\u00e4ulein Emilie Piaste,", "tokens": ["An", "Fr\u00e4u\u00b7lein", "E\u00b7mi\u00b7lie", "Pi\u00b7as\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NE", "NE", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die ich ach so gerne heut umfa\u00dfte,", "tokens": ["Die", "ich", "ach", "so", "ger\u00b7ne", "heut", "um\u00b7fa\u00df\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word"