{"textgrid.poem.52550": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: An deinem Aug, o liebes Kind,", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.85", "en:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "An deinem Aug, o liebes Kind,", "tokens": ["An", "dei\u00b7nem", "Aug", ",", "o", "lie\u00b7bes", "Kind", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "FM", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So oft hab ich gehangen,", "tokens": ["So", "oft", "hab", "ich", "ge\u00b7han\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Du schautest in den leeren Wind,", "tokens": ["Du", "schau\u00b7test", "in", "den", "lee\u00b7ren", "Wind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und kanntest mein Verlangen?", "tokens": ["Und", "kann\u00b7test", "mein", "Ver\u00b7lan\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wie umgewandelt bist du heut,", "tokens": ["Wie", "um\u00b7ge\u00b7wan\u00b7delt", "bist", "du", "heut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVPP", "VAFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Warum jetzt so viel K\u00fcsse,", "tokens": ["Wa\u00b7rum", "jetzt", "so", "viel", "K\u00fcs\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Warum f\u00fcr mich kein Herzeleid", "tokens": ["Wa\u00b7rum", "f\u00fcr", "mich", "kein", "Her\u00b7ze\u00b7leid"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "PPER", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und kluge Hindernisse?", "tokens": ["Und", "klu\u00b7ge", "Hin\u00b7der\u00b7nis\u00b7se", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Drum hast du Nichts von Lieb gewu\u00dft,", "tokens": ["Drum", "hast", "du", "Nichts", "von", "Lieb", "ge\u00b7wu\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "PIS", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du hattest mich nur gerne \u2013", "tokens": ["Du", "hat\u00b7test", "mich", "nur", "ger\u00b7ne", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ADV", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Heut aber gingen dir in der Brust", "tokens": ["Heut", "a\u00b7ber", "gin\u00b7gen", "dir", "in", "der", "Brust"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Hochauf die goldnen Sterne.", "tokens": ["Hoch\u00b7auf", "die", "gold\u00b7nen", "Ster\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "An deinem Aug, o liebes Kind,", "tokens": ["An", "dei\