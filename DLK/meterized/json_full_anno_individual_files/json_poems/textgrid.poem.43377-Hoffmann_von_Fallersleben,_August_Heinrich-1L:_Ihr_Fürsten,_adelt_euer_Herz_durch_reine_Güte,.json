{"textgrid.poem.43377": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ihr F\u00fcrsten, adelt euer Herz durch reine G\u00fcte,", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr F\u00fcrsten, adelt euer Herz durch reine G\u00fcte,", "tokens": ["Ihr", "F\u00fcrs\u00b7ten", ",", "a\u00b7delt", "eu\u00b7er", "Herz", "durch", "rei\u00b7ne", "G\u00fc\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "PPOSAT", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Seid gegen Freunde sanft, vor Feinden traget Hochgem\u00fcthe,", "tokens": ["Seid", "ge\u00b7gen", "Freun\u00b7de", "sanft", ",", "vor", "Fein\u00b7den", "tra\u00b7get", "Hoch\u00b7ge\u00b7m\u00fc\u00b7the", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "APPR", "NN", "ADJD", "$,", "APPR", "NN", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.3": {"text": "St\u00e4rkt das Recht und danket Gott der gro\u00dfen Ehren,", "tokens": ["St\u00e4rkt", "das", "Recht", "und", "dan\u00b7ket", "Gott", "der", "gro\u00b7\u00dfen", "Eh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "KON", "VVFIN", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.4": {"text": "Da\u00df Gut und Blut so Mancher mu\u00df zu euren Diensten kehren;", "tokens": ["Da\u00df", "Gut", "und", "Blut", "so", "Man\u00b7cher", "mu\u00df", "zu", "eu\u00b7ren", "Diens\u00b7ten", "keh\u00b7ren", ";"], "token_info": ["word", "word", "word", "word