{"textgrid.poem.60271": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1832, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du Alpenkind, wie mild und klar", "tokens": ["Du", "Al\u00b7pen\u00b7kind", ",", "wie", "mild", "und", "klar"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "NN", "$,", "PWAV", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Strahlt mir dein blaues Augenpaar!", "tokens": ["Strahlt", "mir", "dein", "blau\u00b7es", "Au\u00b7gen\u00b7paar", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wohl ist in diesen Himmelsn\u00e4hen", "tokens": ["Wohl", "ist", "in", "die\u00b7sen", "Him\u00b7mels\u00b7n\u00e4\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein stilles Wunder einst geschehen.", "tokens": ["Ein", "stil\u00b7les", "Wun\u00b7der", "einst", "ge\u00b7sche\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "In deiner L\u00e4mmer frohem Kreise", "tokens": ["In", "dei\u00b7ner", "L\u00e4m\u00b7mer", "fro\u00b7hem", "Krei\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Hinknietest du, zu beten leise,", "tokens": ["Hin\u00b7knie\u00b7test", "du", ",", "zu", "be\u00b7ten", "lei\u00b7se", ","], "token_info": ["word", "word", "punct",