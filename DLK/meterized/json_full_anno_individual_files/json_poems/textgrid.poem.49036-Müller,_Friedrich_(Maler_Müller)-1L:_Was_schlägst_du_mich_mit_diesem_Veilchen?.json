{"textgrid.poem.49036": {"metadata": {"author": {"name": "M\u00fcller, Friedrich (Maler M\u00fcller)", "birth": "N.A.", "death": "N.A."}, "title": "1L: Was schl\u00e4gst du mich mit diesem Veilchen?", "genre": "verse", "period": "N.A.", "pub_year": 1787, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was schl\u00e4gst du mich mit diesem Veilchen?", "tokens": ["Was", "schl\u00e4gst", "du", "mich", "mit", "die\u00b7sem", "Veil\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PRF", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ach liebe Mutter! ach! halt ein! \u2013", "tokens": ["Ach", "lie\u00b7be", "Mut\u00b7ter", "!", "ach", "!", "halt", "ein", "!", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "punct"], "pos": ["ITJ", "VVFIN", "NN", "$.", "XY", "$.", "VVFIN", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Was gabst du Gleimen deine Pfeilchen?", "tokens": ["Was", "gabst", "du", "Glei\u00b7men", "dei\u00b7ne", "Pfeil\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Du mu\u00dft, du mu\u00dft bestrafet seyn! \u2013", "tokens": ["Du", "mu\u00dft", ",", "du", "mu\u00dft", "be\u00b7stra\u00b7fet", "seyn", "!", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VMFIN", "$,", "PPER", "VMFIN", "VVPP", "VAINF", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ach schone mich! mich armen Jungen!", "tokens": ["Ach", "scho\u00b7ne", "mich", "!", "mich", "ar\u00b7men", "Jun\u00b7gen", "!"], "token_info": ["wor