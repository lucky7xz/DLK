{"dta.poem.872": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "LiX.  Auf d\u1ebd Sontag de\u00df von Schuld lo\u00dfspre-  \n chenden vnd verdammenden K\u00f6nigs/ oder  \n  XXII.  Sontag nach dem Fest der H. Dreyeinigkeit/  \n Matth. 18.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Geh! hochbeladne Seel/ betr\u016fbt in deinen S\u00fcnden/", "tokens": ["Geh", "!", "hoch\u00b7be\u00b7lad\u00b7ne", "Seel", "/", "be\u00b7tr\u016fbt", "in", "dei\u00b7nen", "S\u00fcn\u00b7den", "/"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "ADJA", "NN", "$(", "VVFIN", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wirf dich zur Erden hin/ in wahrem Glauben schaw", "tokens": ["Wirf", "dich", "zur", "Er\u00b7den", "hin", "/", "in", "wah\u00b7rem", "Glau\u00b7ben", "schaw"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "APPRART", "NN", "PTKVZ", "$(", "APPR", "ADJA", "NN", "NE"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Den Himmels-K\u00f6nig an/ gantz vngezweiffelt traw/", "tokens": ["Den", "Him\u00b7mels\u00b7K\u00f6\u00b7nig", "an", "/", "gantz", "vn\u00b7ge\u00b7zweif\u00b7felt", "traw", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$(", "ADV", "ADJD", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Setz vest: gefasten Fu\u00df/ du werdest Gnade finden/", "tokens": ["Setz", "vest", ":", "ge\u00b7fas\u00b7ten", "Fu\u00df", "/", "du", "wer\u00b7dest", "Gna\u00b7de", "fin\u00b7den", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$.", "ADJA", "NN", "$(", "PPER", "VAFIN", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "So wird Er gar gewi\u00df de\u00df Geistes Flamm anz\u00fcnden/", "tokens": ["So", "wird", "Er", "gar", "ge\u00b7wi\u00df", "de\u00df", "Geis\u00b7tes", "Flamm", "an\u00b7z\u00fcn\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADV", "ART", "NN", "NE", "VVIZU", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In deines Hertzens Plan. O lieblich-s\u00fcssen Taw/", "tokens": ["In", "dei\u00b7nes", "Hert\u00b7zens", "Plan", ".", "O", "lieb\u00b7lich\u00b7s\u00fcs\u00b7sen", "Taw", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$.", "NE", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Damit Er dich erlabt. Was aber/ was du Saw/", "tokens": ["Da\u00b7mit", "Er", "dich", "er\u00b7labt", ".", "Was", "a\u00b7ber", "/", "was", "du", "Saw", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVFIN", "$.", "PWS", "ADV", "$(", "PWS", "PPER", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Was darfstu dich anietzt nur immer vnter winden?", "tokens": ["Was", "darfs\u00b7tu", "dich", "an\u00b7ietzt", "nur", "im\u00b7mer", "vn\u00b7ter", "win\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "VVFIN", "ADV", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "da\u00df du dich nicht der Noth de\u00df Bruders nimmest an?", "tokens": ["da\u00df", "du", "dich", "nicht", "der", "Noth", "de\u00df", "Bru\u00b7ders", "nim\u00b7mest", "an", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "PTKNEG", "ART", "NN", "ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "O traw! der hohe Gott sich bald verendern kan.", "tokens": ["O", "traw", "!", "der", "ho\u00b7he", "Gott", "sich", "bald", "ver\u00b7en\u00b7dern", "kan", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.xy", "FM.xy", "$.", "ART", "ADJA", "NN", "PRF", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So gn\u00e4dig als Er ist/ so Zornig ist Er wieder/", "tokens": ["So", "gn\u00e4\u00b7dig", "als", "Er", "ist", "/", "so", "Zor\u00b7nig", "ist", "Er", "wie\u00b7der", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "PPER", "VAFIN", "$(", "ADV", "ADJD", "VAFIN", "PPER", "ADV", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Sein Zorn brennt wie ein Fewr/ vnd steigt gleich einer", "tokens": ["Sein", "Zorn", "brennt", "wie", "ein", "Fewr", "/", "vnd", "steigt", "gleich", "ei\u00b7ner"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "KOKOM", "ART", "NN", "$(", "KON", "VVFIN", "ADV", "ART"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Flamm/ ", "tokens": ["Flamm", "/"], "token_info": ["word", "punct"], "pos": ["NE", "$("], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Er reisset alles weg; vnd die dem Nechsten gramem/", "tokens": ["Er", "reis\u00b7set", "al\u00b7les", "weg", ";", "vnd", "die", "dem", "Nechs\u00b7ten", "gra\u00b7mem", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "PTKVZ", "$.", "KON", "ART", "ART", "NN", "ADJA", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die strafft Er zorniglich. dr\u00fcm f\u00fcrchte sich ein jeder.", "tokens": ["Die", "strafft", "Er", "zor\u00b7nig\u00b7lich", ".", "dr\u00fcm", "f\u00fcrch\u00b7te", "sich", "ein", "je\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADJD", "$.", "PAV", "VVFIN", "PRF", "ART", "PIS", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}