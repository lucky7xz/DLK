{"textgrid.poem.40420": {"metadata": {"author": {"name": "Scheffel, Joseph Viktor von", "birth": "N.A.", "death": "N.A."}, "title": "Des Klosterkellermeisters Sommermorgenklaggesang", "genre": "verse", "period": "N.A.", "pub_year": 1856, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Huh weh! mir ist des Tages bang!", "tokens": ["Huh", "weh", "!", "mir", "ist", "des", "Ta\u00b7ges", "bang", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$.", "PPER", "VAFIN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Tret' ich hinaus in den schweigenden Bergwald,", "tokens": ["Tret'", "ich", "hin\u00b7aus", "in", "den", "schwei\u00b7gen\u00b7den", "Berg\u00b7wald", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APZR", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.3": {"text": "Den kaum das erste Fr\u00fchlicht erhellet,", "tokens": ["Den", "kaum", "das", "ers\u00b7te", "Fr\u00fch\u00b7licht", "er\u00b7hel\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wehe! noch lagert die Hitze von gestern", "tokens": ["We\u00b7he", "!", "noch", "la\u00b7gert", "die", "Hit\u00b7ze", "von", "ge\u00b7stern"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$.", "ADV", "VVFIN", "ART", "NN", "APPR", "NN"], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.5": {"text": "\u00dcber versengtem Moos und Gestr\u00e4uch,", "tokens": ["\u00dc\u00b7ber", "ver\u00b7seng\u00b7tem", "Moos", "und", "Ge\u00b7str\u00e4uch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "NN", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Und schon umschwirrt mich ein Bremsengesumm,", "tokens": ["Und", "schon", "um\u00b7schwirrt", "mich", "ein", "Brem\u00b7sen\u00b7ge\u00b7summ", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Stechend und frech,", "tokens": ["Ste\u00b7chend", "und", "frech", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "Als ob die Sonne im Mittag erspr\u00fchte;", "tokens": ["Als", "ob", "die", "Son\u00b7ne", "im", "Mit\u00b7tag", "er\u00b7spr\u00fch\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Klaffende Spr\u00fcnge spalten das Erdreich,", "tokens": ["Klaf\u00b7fen\u00b7de", "Spr\u00fcn\u00b7ge", "spal\u00b7ten", "das", "Er\u00b7dreich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.10": {"text": "Gras d\u00fcrrt zu Heu, bevor es gem\u00e4ht ist,", "tokens": ["Gras", "d\u00fcrrt", "zu", "Heu", ",", "be\u00b7vor", "es", "ge\u00b7m\u00e4ht", "ist", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "NN", "$,", "KOUS", "PPER", "VVPP", "VAFIN", "$,"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.11": {"text": "Und in der Luft schwebt", "tokens": ["Und", "in", "der", "Luft", "schwebt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Huh weh! mir ist des Tages bang!", "tokens": ["Huh", "weh", "!", "mir", "ist", "des", "Ta\u00b7ges", "bang", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$.", "PPER", "VAFIN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Such' ich beim Stamme der riesigen Buche", "tokens": ["Such'", "ich", "beim", "Stam\u00b7me", "der", "rie\u00b7si\u00b7gen", "Bu\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "ART", "ADJA", "NN"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.3": {"text": "K\u00fchlung auf gr\u00f6blich behauenem Steinsitz,", "tokens": ["K\u00fch\u00b7lung", "auf", "gr\u00f6b\u00b7lich", "be\u00b7hau\u00b7e\u00b7nem", "Stein\u00b7sitz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+---+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Wo um achteckige Platte des Tisches", "tokens": ["Wo", "um", "acht\u00b7ec\u00b7ki\u00b7ge", "Plat\u00b7te", "des", "Ti\u00b7sches"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ADJA", "NN", "ART", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Fr\u00f6hlicher Waldrast die Br\u00fcder oft pflegen:", "tokens": ["Fr\u00f6h\u00b7li\u00b7cher", "Wald\u00b7rast", "die", "Br\u00fc\u00b7der", "oft", "pfle\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "ADV", "VVINF", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.6": {"text": "Weh! auch der Stein speit gl\u00fchende Hitze,", "tokens": ["Weh", "!", "auch", "der", "Stein", "speit", "gl\u00fc\u00b7hen\u00b7de", "Hit\u00b7ze", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ADV", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Duldet mich nicht.", "tokens": ["Dul\u00b7det", "mich", "nicht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "H\u00e4her und Spechte und Drosseln, sie lachen,", "tokens": ["H\u00e4\u00b7her", "und", "Spech\u00b7te", "und", "Dros\u00b7seln", ",", "sie", "la\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "KON", "NN", "$,", "PPER", "VVINF", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.9": {"text": "Da\u00df ich, kaum niedergelassen, so j\u00e4hlings", "tokens": ["Da\u00df", "ich", ",", "kaum", "nie\u00b7der\u00b7ge\u00b7las\u00b7sen", ",", "so", "j\u00e4h\u00b7lings"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PPER", "$,", "ADV", "VVPP", "$,", "ADV", "ADV"], "meter": "++-+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.10": {"text": "Auf und davon sprang.", "tokens": ["Auf", "und", "da\u00b7von", "sprang", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "KON", "PAV", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.11": {"text": "Verschlafene Heuschrecken sitzen im Wege,", "tokens": ["Ver\u00b7schla\u00b7fe\u00b7ne", "Heu\u00b7schre\u00b7cken", "sit\u00b7zen", "im", "We\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+--++-+--+-", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "R\u00fchren sich nicht,", "tokens": ["R\u00fch\u00b7ren", "sich", "nicht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PRF", "PTKNEG", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "Huh weh! mir ist des Tages bang!", "tokens": ["Huh", "weh", "!", "mir", "ist", "des", "Ta\u00b7ges", "bang", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$.", "PPER", "VAFIN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das sind die Zeiten, wo Tier und Menschheit", "tokens": ["Das", "sind", "die", "Zei\u00b7ten", ",", "wo", "Tier", "und", "Menschheit"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "PWAV", "NN", "KON", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Gl\u00fchhei\u00df schmachten, gleich Eisen im Schmiedherd;", "tokens": ["Gl\u00fch\u00b7hei\u00df", "schmach\u00b7ten", ",", "gleich", "Ei\u00b7sen", "im", "Schmied\u00b7herd", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "ADV", "NN", "APPRART", "NN", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Tr\u00e4ufst du auch Tropfen und Fluten drauf nieder,", "tokens": ["Tr\u00e4ufst", "du", "auch", "Trop\u00b7fen", "und", "Flu\u00b7ten", "drauf", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "NN", "KON", "NN", "PAV", "PTKVZ", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.5": {"text": "Saugt es sie gierig, doch l\u00f6scht es nicht.", "tokens": ["Saugt", "es", "sie", "gie\u00b7rig", ",", "doch", "l\u00f6scht", "es", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADJD", "$,", "ADV", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.6": {"text": "Weh! kaum l\u00e4utet das Fr\u00fchmorgengl\u00f6cklein", "tokens": ["Weh", "!", "kaum", "l\u00e4u\u00b7tet", "das", "Fr\u00fch\u00b7mor\u00b7gen\u00b7gl\u00f6c\u00b7klein"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$.", "ADV", "VVFIN", "ART", "NN"], "meter": "+-+--+---+", "measure": "trochaic.tetra.relaxed"}, "line.7": {"text": "Und schon zwingt mich allm\u00e4chtige Sehnsucht,", "tokens": ["Und", "schon", "zwingt", "mich", "all\u00b7m\u00e4ch\u00b7ti\u00b7ge", "Sehn\u00b7sucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.8": {"text": "Eiligst zum Klosterkeller zu wandeln ...", "tokens": ["Ei\u00b7ligst", "zum", "Klos\u00b7ter\u00b7kel\u00b7ler", "zu", "wan\u00b7deln", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "PTKZU", "VVFIN", "$("], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.9": {"text": "Ob ich dort harre in trinkender Arbeit,", "tokens": ["Ob", "ich", "dort", "har\u00b7re", "in", "trin\u00b7ken\u00b7der", "Ar\u00b7beit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Bis sich die Nacht neigt,", "tokens": ["Bis", "sich", "die", "Nacht", "neigt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.11": {"text": "Oder ein l\u00e4uternd Gewitter am Himmel", "tokens": ["O\u00b7der", "ein", "l\u00e4u\u00b7ternd", "Ge\u00b7wit\u00b7ter", "am", "Him\u00b7mel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJD", "NN", "APPRART", "NN"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.12": {"text": "Diese unendliche Schw\u00fcle zerbricht?...", "tokens": ["Die\u00b7se", "un\u00b7end\u00b7li\u00b7che", "Schw\u00fc\u00b7le", "zer\u00b7bricht", "?", "..."], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PDAT", "ADJA", "NN", "VVFIN", "$.", "$("], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.13": {"text": "Wei\u00df nicht ...", "tokens": ["Wei\u00df", "nicht", "..."], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "$("], "meter": "-+", "measure": "iambic.single"}, "line.14": {"text": "Aber mein Durst ist", "tokens": ["A\u00b7ber", "mein", "Durst", "ist"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VAFIN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.4": {"line.1": {"text": "Huh weh! mir ist des Tages bang!", "tokens": ["Huh", "weh", "!", "mir", "ist", "des", "Ta\u00b7ges", "bang", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$.", "PPER", "VAFIN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Tret' ich hinaus in den schweigenden Bergwald,", "tokens": ["Tret'", "ich", "hin\u00b7aus", "in", "den", "schwei\u00b7gen\u00b7den", "Berg\u00b7wald", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APZR", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.3": {"text": "Den kaum das erste Fr\u00fchlicht erhellet,", "tokens": ["Den", "kaum", "das", "ers\u00b7te", "Fr\u00fch\u00b7licht", "er\u00b7hel\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "ADJ