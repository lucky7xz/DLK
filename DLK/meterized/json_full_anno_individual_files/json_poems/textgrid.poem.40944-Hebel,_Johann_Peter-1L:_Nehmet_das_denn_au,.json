{"textgrid.poem.40944": {"metadata": {"author": {"name": "Hebel, Johann Peter", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nehmet das denn au,", "genre": "verse", "period": "N.A.", "pub_year": 1793, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nehmet das denn au,", "tokens": ["Neh\u00b7met", "das", "denn", "au", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDS", "ADV", "NE", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "liebe, frummi Frau!", "tokens": ["lie\u00b7be", ",", "frum\u00b7mi", "Frau", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "'s grotet just nit eins wie's ander,", "tokens": ["'s", "gro\u00b7tet", "just", "nit", "eins", "wie's", "an\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PTKNEG", "PIS", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Chorn und Spreu isch unterenander.", "tokens": ["Chorn", "und", "Spreu", "isch", "un\u00b7te\u00b7ren\u00b7an\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ADJD", "PTKVZ", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Leset's F\u00fcrnehmst us,", "tokens": ["Le\u00b7set's", "F\u00fcr\u00b7nehmst", "us", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NN", "NE", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "'s isch, cha si, ne Fund;", "tokens": ["'s", "isch", ",", "cha", "si", ",", "ne", "Fund", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADJD", "$,", "FM.la", "FM.la", "$,", "ADJA", "NN", "$."], "meter":