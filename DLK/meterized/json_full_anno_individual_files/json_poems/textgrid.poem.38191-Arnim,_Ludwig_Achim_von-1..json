{"textgrid.poem.38191": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nach meiner Lieb viel hundert Knaben trachten,", "tokens": ["Nach", "mei\u00b7ner", "Lieb", "viel", "hun\u00b7dert", "Kna\u00b7ben", "trach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "CARD", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Allein der, den ich lieb, will mein nicht achten,", "tokens": ["Al\u00b7lein", "der", ",", "den", "ich", "lieb", ",", "will", "mein", "nicht", "ach\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "$,", "PRELS", "PPER", "ADJD", "$,", "VMFIN", "PPOSAT", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ach weh mir armen Maid, vor Leid mu\u00df ich verschmachten.", "tokens": ["Ach", "weh", "mir", "ar\u00b7men", "Maid", ",", "vor", "Leid", "mu\u00df", "ich", "ver\u00b7schmach\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "PPER", "ADJA", "NN", "$,", "APPR", "NN", "VMFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Jeder begehrt zu mir sich zu verpflichten,", "tokens": ["Je\u00b7der", "be\u00b7gehrt", "zu", "mir", "sich", "zu", "ver\u00b7pflich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVPP", "APPR", "PPER", "PRF", "PTKZU", "VVINF", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"