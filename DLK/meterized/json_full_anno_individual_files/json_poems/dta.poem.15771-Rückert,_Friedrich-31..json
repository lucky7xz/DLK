{"dta.poem.15771": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "31.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was Menschenkunst gemacht, darf man zu nah nicht sehn,", "tokens": ["Was", "Men\u00b7schen\u00b7kunst", "ge\u00b7macht", ",", "darf", "man", "zu", "nah", "nicht", "sehn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "VVPP", "$,", "VMFIN", "PIS", "PTKA", "ADJD", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nicht vorm Vergr\u00f6\u00dfrungsglas kann es die Probe stehn.", "tokens": ["Nicht", "vorm", "Ver\u00b7gr\u00f6\u00df\u00b7rungs\u00b7glas", "kann", "es", "die", "Pro\u00b7be", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPRART", "NN", "VMFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Des Malers sch\u00f6nstes Bild, des Dichters sch\u00f6nstes Wort,", "tokens": ["Des", "Ma\u00b7lers", "sch\u00f6ns\u00b7tes", "Bild", ",", "des", "Dich\u00b7ters", "sch\u00f6ns\u00b7tes", "Wort", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zergliedr' es und zerlegs, so ist der Zauber fort.", "tokens": ["Zer\u00b7gliedr'", "es", "und", "zer\u00b7legs", ",", "so", "ist", "der", "Zau\u00b7ber", "fort", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KON", "ADV", "$,", "ADV", "VAFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Was Gottes Kunst gemacht, erscheint nach vorgenommner", "tokens": ["Was", "Got\u00b7tes", "Kunst", "ge\u00b7macht", ",", "er\u00b7scheint", "nach", "vor\u00b7ge\u00b7nomm\u00b7ner"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "NN", "NN", "VVPP", "$,", "VVFIN", "APPR", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zergliederung, wenn auch nicht sch\u00f6ner, doch vollkommner.", "tokens": ["Zer\u00b7glie\u00b7de\u00b7rung", ",", "wenn", "auch", "nicht", "sch\u00f6\u00b7ner", ",", "doch", "voll\u00b7komm\u00b7ner", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "ADV", "PTKNEG", "ADJD", "$,", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Nicht sch\u00f6ner, weil sich nur auf unsern Sinn bezieht", "tokens": ["Nicht", "sch\u00f6\u00b7ner", ",", "weil", "sich", "nur", "auf", "un\u00b7sern", "Sinn", "be\u00b7zieht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADJD", "$,", "KOUS", "PRF", "ADV", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Sch\u00f6nheit, und zugleich mit dessen T\u00e4uschung flieht.", "tokens": ["Die", "Sch\u00f6n\u00b7heit", ",", "und", "zu\u00b7gleich", "mit", "des\u00b7sen", "T\u00e4u\u00b7schung", "flieht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KON", "ADV", "APPR", "PRELAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Vollkommner aber, weil der Geist viel mehr darinn", "tokens": ["Voll\u00b7komm\u00b7ner", "a\u00b7ber", ",", "weil", "der", "Geist", "viel", "mehr", "da\u00b7rinn"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "$,", "KOUS", "ART", "NN", "ADV", "ADV", "PAV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Entdecket, als vermag zu fassen Menschensinn.", "tokens": ["Ent\u00b7de\u00b7cket", ",", "als", "ver\u00b7mag", "zu", "fas\u00b7sen", "Men\u00b7schen\u00b7sinn", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "NN", "PTKZU", "VVINF", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}