{"textgrid.poem.41447": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Der Fischer und der Schatz", "genre": "verse", "period": "N.A.", "pub_year": 1731, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Fischer, der mit seinen Netzen", "tokens": ["Ein", "Fi\u00b7scher", ",", "der", "mit", "sei\u00b7nen", "Net\u00b7zen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Brod und Zufriedenheit gewann,", "tokens": ["Brod", "und", "Zu\u00b7frie\u00b7den\u00b7heit", "ge\u00b7wann", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "That einen schweren Zug. Voll Mitleid und Entsetzen", "tokens": ["That", "ei\u00b7nen", "schwe\u00b7ren", "Zug", ".", "Voll", "Mit\u00b7leid", "und", "Ent\u00b7set\u00b7zen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN", "$.", "ADJD", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Traf er im Sack des Garns jetzt einen Todten an.", "tokens": ["Traf", "er", "im", "Sack", "des", "Garns", "jetzt", "ei\u00b7nen", "Tod\u00b7ten", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "ART", "NN", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der soll, sprach er, von mir den letzten Dienst erhalten.", "tokens": ["Der", "soll", ",", "sprach", "er", ",", "von", "mir", "den", "letz\u00b7ten", "Dienst", "er\u00b7hal\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "$,", "VVFIN", "PPER", "$,", "APPR", "PPER", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Vielleicht, da\u00df in der Todesnacht", "tokens": ["Viel\u00b7leicht", ",", "da\u00df", "in", "der", "To\u00b7des\u00b7nacht"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "KOUS", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die\u00df seinen Schatten ruhig macht.", "tokens": ["Die\u00df", "sei\u00b7nen", "Schat\u00b7ten", "ru\u00b7hig", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wie der um's Leben kam, so kann ich selbst erkalten.", "tokens": ["Wie", "der", "um's", "Le\u00b7ben", "kam", ",", "so", "kann", "ich", "selbst", "er\u00b7kal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVFIN", "$,", "ADV", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Aus Sorgfalt tr\u00e4gt er ihn an einen sichern Platz,", "tokens": ["Aus", "Sorg\u00b7falt", "tr\u00e4gt", "er", "ihn", "an", "ei\u00b7nen", "si\u00b7chern", "Platz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Den nicht die hohe Flut erreichte.", "tokens": ["Den", "nicht", "die", "ho\u00b7he", "Flut", "er\u00b7reich\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Da grub er tief, und schwitzt', und keichte,", "tokens": ["Da", "grub", "er", "tief", ",", "und", "schwitzt'", ",", "und", "keich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "$,", "KON", "VVFIN", "$,", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Und fand, im Schaufeln, einen Schatz.", "tokens": ["Und", "fand", ",", "im", "Schau\u00b7feln", ",", "ei\u00b7nen", "Schatz", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "APPRART", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Der Schickung Hand ist stets bereit", "tokens": ["Der", "Schi\u00b7ckung", "Hand", "ist", "stets", "be\u00b7reit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VAFIN", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Tugend Werke zu vergelten.", "tokens": ["Der", "Tu\u00b7gend", "Wer\u00b7ke", "zu", "ver\u00b7gel\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PTKZU", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Sie sorgt, mit gleicher Wachsamkeit,", "tokens": ["Sie", "sorgt", ",", "mit", "glei\u00b7cher", "Wach\u00b7sam\u00b7keit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "F\u00fcr jeden Menschen, wie f\u00fcr Welten.", "tokens": ["F\u00fcr", "je\u00b7d