{"textgrid.poem.50857": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Winternacht", "genre": "verse", "period": "N.A.", "pub_year": 1845, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nicht ein Fl\u00fcgelschlag ging durch die Welt,", "tokens": ["Nicht", "ein", "Fl\u00fc\u00b7gel\u00b7schlag", "ging", "durch", "die", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Still und blendend lag der wei\u00dfe Schnee,", "tokens": ["Still", "und", "blen\u00b7dend", "lag", "der", "wei\u00b7\u00dfe", "Schnee", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Nicht ein W\u00f6lklein hing am Sternenzelt,", "tokens": ["Nicht", "ein", "W\u00f6lk\u00b7lein", "hing", "am", "Ster\u00b7nen\u00b7zelt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Keine Welle schlug im starren See.", "tokens": ["Kei\u00b7ne", "Wel\u00b7le", "schlug", "im", "star\u00b7ren", "See", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Aus der Tiefe stieg der Seebaum auf,", "tokens": ["Aus", "der", "Tie\u00b7fe", "stieg", "der", "See\u00b7baum", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Bis sein Wipfel in dem Eis gefror;", "tokens": ["Bis", "sein", "Wip\u00b7fel", "in", "dem", "Eis", "ge\u00b7fror", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "An den \u00c4sten klomm die Nix herauf,", "tokens": ["An", "den", "\u00c4s\u00b7ten", "klomm", "die", "Nix", "her\u00b7auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Schaute durch das gr\u00fcne Eis empor.", "tokens": ["Schau\u00b7te", "durch", "das", "gr\u00fc\u00b7ne", "Eis", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Auf dem d\u00fcnnen Glase stand ich da,", "tokens": ["Auf", "dem", "d\u00fcn\u00b7nen", "Gla\u00b7se", "stand", "ich", "da", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Das die schwarze Tiefe von mir schied;", "tokens": ["Das", "die", "schwar\u00b7ze", "Tie\u00b7fe", "von", "mir", "schied", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "ADJA", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Dicht ich unter meinen F\u00fc\u00dfen sah", "tokens": ["Dicht", "ich", "un\u00b7ter", "mei\u00b7nen", "F\u00fc\u00b7\u00dfen", "sah"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Ihre wei\u00dfe Sch\u00f6nheit Glied f\u00fcr Glied.", "tokens": ["Ih\u00b7re", "wei\u00b7\u00dfe", "Sch\u00f6n\u00b7heit", "Glied", "f\u00fcr", "Glied", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Mit ersticktem Jammer tastet' sie", "tokens": ["Mit", "er\u00b7stick\u00b7tem", "Jam\u00b7mer", "tas\u00b7tet'", "sie"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "An der harten Decke her und hin.", "tokens": ["An", "der", "har\u00b7ten", "De\u00b7cke", "her", "und", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Ich verge\u00df das dunkle Antlitz nie,", "tokens": ["Ich", "ver\u00b7ge\u00df", "das", "dunk\u00b7le", "Ant\u00b7litz", "nie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "ADV", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Immer, immer liegt es mir im Sinn!", "tokens": ["Im\u00b7mer", ",", "im\u00b7mer", "liegt", "es", "mir", "im", "Sinn", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "VVFIN", "PPER", "PPER", "APPRART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Nicht ein Fl\u00fcgelschlag ging durch die Welt,", "tokens": ["Nicht", "ein", "Fl\u00fc\u00b7gel\u00b7schl