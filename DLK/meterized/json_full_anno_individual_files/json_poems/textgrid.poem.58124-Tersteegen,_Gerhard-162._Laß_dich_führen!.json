{"textgrid.poem.58124": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "162. La\u00df dich f\u00fchren!", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer klug ist, stark und gro\u00df, der will sich selber f\u00fchren,", "tokens": ["Wer", "klug", "ist", ",", "stark", "und", "gro\u00df", ",", "der", "will", "sich", "sel\u00b7ber", "f\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VAFIN", "$,", "ADJD", "KON", "ADJD", "$,", "PRELS", "VMFIN", "PRF", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der hat und kennet noch des Hirten Leitung nicht.", "tokens": ["Der", "hat", "und", "ken\u00b7net", "noch", "des", "Hir\u00b7ten", "Lei\u00b7tung", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "KON", "VVFIN", "ADV", "ART", "NN", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mich dummes, schwaches Lamm mein Jesus mu\u00df regieren;", "tokens": ["Mich", "dum\u00b7mes", ",", "schwa\u00b7ches", "Lamm", "mein", "Je\u00b7sus", "mu\u00df", "re\u00b7gie\u00b7ren", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "$,", "ADJA", "NN", "PPOSAT", "NE", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ich merk' auf seine Stimm' und halt' ihn im Gesicht.", "tokens": ["Ich", "merk'", "auf", "sei\u00b7ne", "Stimm'", "und", "halt'", "ihn", "im", "Ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word