{"textgrid.poem.48868": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "93. An Amorn", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach, Amor, fleuch geschwind und sags ihr eilend an.", "tokens": ["Ach", ",", "A\u00b7mor", ",", "fleuch", "ge\u00b7schwind", "und", "sags", "ihr", "ei\u00b7lend", "an", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "NE", "$,", "VVFIN", "ADJD", "KON", "VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es ist um mich geschehn, ich lieg' in letzten Z\u00fcgen.", "tokens": ["Es", "ist", "um", "mich", "ge\u00b7schehn", ",", "ich", "lieg'", "in", "letz\u00b7ten", "Z\u00fc\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "VVPP", "$,", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Das Blut ist ausgedorrt, das hei\u00dfe Mark versiegen,", "tokens": ["Das", "Blut", "ist", "aus\u00b7ge\u00b7dorrt", ",", "das", "hei\u00b7\u00dfe", "Mark", "ver\u00b7sie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "ich singe selbst mein Lied, ich Tode naher Schwan.", "tokens": ["ich", "sin\u00b7ge", "selbst", "mein", "Lied", ",", "ich", "To\u00b7de", "na\u00b7her", "Schwan", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PPOSAT", "NN", "$,", "PPER", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ach, eile, sag es ihr. Es ist um mich getan.", "tokens": ["Ach", ",", "ei\u00b7le", ",", "sag", "es", "ihr", ".", "Es", "ist", "um", "mich", "ge\u00b7tan", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VVFIN", "$,", "VVFIN", "PPER", "PPER", "$.", "PPER", "VAFIN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Wichtigkeit der Pein ist \u00fcber mich gestiegen:", "tokens": ["Die", "Wich\u00b7tig\u00b7keit", "der", "Pein", "ist", "\u00fc\u00b7ber", "mich", "ge\u00b7stie\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VAFIN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Das m\u00fcde Herze klopft, ich kan nicht Odem kriegen.", "tokens": ["Das", "m\u00fc\u00b7de", "Her\u00b7ze", "klopft", ",", "ich", "kan", "nicht", "O\u00b7dem", "krie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,", "PPER", "VMFIN", "PTKNEG", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Es ist mir m\u00f6glich nicht, da\u00df ich mehr leben kan.", "tokens": ["Es", "ist", "mir", "m\u00f6g\u00b7lich", "nicht", ",", "da\u00df", "ich", "mehr", "le\u00b7ben", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADJD", "PTKNEG", "$,", "KOUS", "PPER", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Jedoch, verzeuch noch hier, bis mein gewisser Tod", "tokens": ["Je\u00b7doch", ",", "ver\u00b7zeuch", "noch", "hier", ",", "bis", "mein", "ge\u00b7wis\u00b7ser", "Tod"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "VVFIN", "ADV", "ADV", "$,", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "dich fertigt bald von hier. Di\u00df kanst du hoch bewehren,", "tokens": ["dich", "fer\u00b7tigt", "bald", "von", "hier", ".", "Di\u00df", "kanst", "du", "hoch", "be\u00b7weh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ADV", "$.", "PDS", "VMFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich brenne lichter Loh und schwimm' in meinen Zehren.", "tokens": ["Ich", "bren\u00b7ne", "lich\u00b7ter", "Loh", "und", "schwimm'", "in", "mei\u00b7nen", "Zeh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "NN", "KON", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Erz\u00e4hls ihr, was du siehst, von meiner Todesnot.", "tokens": ["Er\u00b7z\u00e4hls", "ihr", ",", "was", "du", "siehst", ",", "von", "mei\u00b7ner", "To\u00b7des\u00b7not", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "PWS", "PPER", "VVFIN", "$,", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich kan nicht totarm sein. Verschonen mich die Flammen,", "tokens": ["Ich", "kan", "nicht", "to\u00b7tarm", "sein", ".", "Ver\u00b7scho\u00b7nen", "mich", "die", "Flam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "ADJD", "VAINF", "$.", "NN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "so schl\u00e4gt di\u00df Tr\u00e4nenmeer doch \u00fcber mich zusammen.", "tokens": ["so", "schl\u00e4gt", "di\u00df", "Tr\u00e4\u00b7nen\u00b7meer", "doch", "\u00fc\u00b7ber", "mich", "zu\u00b7sam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PDS", "NN", "ADV", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Ach, Amor, fleuch geschwind und sags ihr eilend an.", "tokens": ["Ach", ",", "A\u00b7mor", ",", "fleuch", "ge\u00b7schwind", "und", "sags", "ihr", "ei\u00b7lend", "an", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "NE", "$,", "VVFIN", "ADJD", "KON", "VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es ist um mich geschehn, ich lieg' in letzten Z\u00fcgen.", "tokens": ["Es", "ist", "um", "mich", "ge\u00b7schehn", ",", "ich", "lieg'", "in", "letz\u00b7ten", "Z\u00fc\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "VVPP", "$,", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Das Blut ist ausgedorrt, das hei\u00dfe Mark versiegen,", "tokens": ["Das", "Blut", "ist", "aus\u00b7ge\u00b7dorrt", ",", "das", "hei\u00b7\u00dfe", "Mark", "ver\u00b7sie\u00b7