{"dta.poem.21537": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Dem Gott der Sch\u00f6nheit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn &#252;ber die schneeige Firne               ", "tokens": ["Wenn", "&#252;", "ber", "die", "schne\u00b7ei\u00b7ge", "Fir\u00b7ne"], "token_info": ["word", "XML_entity", "word", "word", "word", "word"], "pos": ["KOUS", "$(", "ADV", "ART", "ADJA", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Von Bergesh\u00e4uptern, steinalten Riesen", "tokens": ["Von", "Ber\u00b7ges\u00b7h\u00e4up\u00b7tern", ",", "stei\u00b7nal\u00b7ten", "Rie\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "$,", "ADJA", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Purpurn aufflammt das Fr\u00fchroth \u2014", "tokens": ["Pur\u00b7purn", "auf\u00b7flammt", "das", "Fr\u00fch\u00b7roth"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "$("], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Wenn n\u00e4chtlicher Weile", "tokens": ["Wenn", "n\u00e4cht\u00b7li\u00b7cher", "Wei\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Ergl\u00fchen die Leuchten, die nimmer zu z\u00e4hlen,", "tokens": ["Er\u00b7gl\u00fc\u00b7hen", "die", "Leuch\u00b7ten", ",", "die", "nim\u00b7mer", "zu", "z\u00e4h\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "PRELS", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.6": {"text": "Des unerme\u00dflichen ewigen Weltalls \u2014", "tokens": ["Des", "un\u00b7er\u00b7me\u00df\u00b7li\u00b7chen", "e\u00b7wi\u00b7gen", "Welt\u00b7alls"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$("], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Wenn durch das Waldthal gehen die Schauer", "tokens": ["Wenn", "durch", "das", "Wald\u00b7thal", "ge\u00b7hen", "die", "Schau\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Des kommenden Morgens, des scheidenden Abends \u2014", "tokens": ["Des", "kom\u00b7men\u00b7den", "Mor\u00b7gens", ",", "des", "schei\u00b7den\u00b7den", "A\u00b7bends"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADV", "$,", "ART", "ADJA", "NN", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.9": {"text": "Dann bebt mir die Seele, ich sp\u00fcre und f\u00fchle", "tokens": ["Dann", "bebt", "mir", "die", "See\u00b7le", ",", "ich", "sp\u00fc\u00b7re", "und", "f\u00fch\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,", "PPER", "VVFIN", "KON", "VVFIN"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.10": {"text": "Dich, o erhabener Gott der Sch\u00f6nheit.", "tokens": ["Dich", ",", "o", "er\u00b7ha\u00b7be\u00b7ner", "Gott", "der", "Sch\u00f6n\u00b7heit", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "FM", "ADJA", "NN", "ART", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Auf T\u00f6nen schwimmst du,", "tokens": ["Auf", "T\u00f6\u00b7nen", "schwimmst", "du", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "$,"], "meter": "-+---", "measure": "dactylic.init"}, "line.2": {"text": "Harmonie deine Rede,", "tokens": ["Har\u00b7mo\u00b7nie", "dei\u00b7ne", "Re\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Und aus den Gestalten des Malers,", "tokens": ["Und", "aus", "den", "Ge\u00b7stal\u00b7ten", "des", "Ma\u00b7lers", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Den Gebilden des Bildners", "tokens": ["Den", "Ge\u00b7bil\u00b7den", "des", "Bild\u00b7ners"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.5": {"text": "Siehst du mit gro\u00dfen,", "tokens": ["Siehst", "du", "mit", "gro\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Blitzenden G\u00f6tteraugen", "tokens": ["Blit\u00b7zen\u00b7den", "G\u00f6t\u00b7ter\u00b7au\u00b7gen"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.7": {"text": "Tief in das Herz mir.", "tokens": ["Tief", "in", "das", "Herz", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "PPER", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Dann wieder r\u00fchrst du die Seele", "tokens": ["Dann", "wie\u00b7der", "r\u00fchrst", "du", "die", "See\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "Und tr\u00e4gst mich empor,", "tokens": ["Und", "tr\u00e4gst", "mich", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.10": {"text": "Hinauf zu den Sternen", "tokens": ["Hin\u00b7auf", "zu", "den", "Ster\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.11": {"text": "Auf Schwingen des Liedes . . . . .", "tokens": ["Auf", "Schwin\u00b7gen", "des", "Lie\u00b7des", ".", ".", ".", ".", "."], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct", "punct", "punct"], "pos": ["APPR", "NN", "ART", "NN", "$.", "$.", "$.", "$.", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "Dann wieder schaust du", "tokens": ["Dann", "wie\u00b7der", "schaust", "du"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "PPER"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Aus schlankem Leibe", "tokens": ["Aus", "schlan\u00b7kem", "Lei\u00b7be"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Mit rosigen Wangen", "tokens": ["Mit", "ro\u00b7si\u00b7gen", "Wan\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Und ringelnden Locken", "tokens": ["Und", "rin\u00b7geln\u00b7den", "Lo\u00b7cken"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Mich an, s\u00fc\u00dfschimmernden Auges,", "tokens": ["Mich", "an", ",", "s\u00fc\u00df\u00b7schim\u00b7mern\u00b7den", "Au\u00b7ges", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "PTKVZ", "$,", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Und triffst mich mit deinem", "tokens": ["Und", "triffst", "mich", "mit", "dei\u00b7nem"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "APPR", "PPOSAT"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Zuckenden Lichtstrahl,", "tokens": ["Zu\u00b7cken\u00b7den", "Licht\u00b7strahl", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "O Gott der Sch\u00f6nheit.", "tokens": ["O", "Gott", "der", "Sch\u00f6n\u00b7heit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ART", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Wie mir so bewegst du", "tokens": ["Wie", "mir", "so", "be\u00b7wegst", "du"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADV", "VVFIN", "PPER"], "meter": "--+---", "measure": "anapaest.init"}, "line.2": {"text": "Das Herz aller Menschen", "tokens": ["Das", "Herz", "al\u00b7ler", "Men\u00b7schen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PIAT", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Seit dem Uranfang,", "tokens": ["Seit", "dem", "U\u00b7ran\u00b7fang", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Jetzt und in Zukunft,", "tokens": ["Jetzt", "und", "in", "Zu\u00b7kunft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "KON", "APPR", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Und weckest die Sehnsucht", "tokens": ["Und", "we\u00b7ckest", "die", "Sehn\u00b7sucht"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Zum Idealen,", "tokens": ["Zum", "I\u00b7dea\u00b7len", ","], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Und f\u00fchrest die Menschheit", "tokens": ["Und", "f\u00fch\u00b7rest", "die", "Menschheit"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "Den Pfad zur Vollendung \u2026", "tokens": ["Den", "Pfad", "zur", "Vol\u00b7len\u00b7dung", "\u2026"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$("], "meter": "-+-+--", "measure": "unknown.measure.di"}, "line.9": {"text": "O Gott der Sch\u00f6nheit.", "tokens": ["O", "Gott", "der", "Sch\u00f6n\u00b7heit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ART", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}}}}