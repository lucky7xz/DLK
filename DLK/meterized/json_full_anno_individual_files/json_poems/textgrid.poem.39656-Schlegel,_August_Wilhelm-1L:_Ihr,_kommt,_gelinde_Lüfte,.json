{"textgrid.poem.39656": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ihr, kommt, gelinde L\u00fcfte,", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr, kommt, gelinde L\u00fcfte,", "tokens": ["Ihr", ",", "kommt", ",", "ge\u00b7lin\u00b7de", "L\u00fcf\u00b7te", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "VVFIN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mir Seufzer wegzuwehn!", "tokens": ["Mir", "Seuf\u00b7zer", "weg\u00b7zu\u00b7wehn", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ihr sollt mir, Blumend\u00fcfte,", "tokens": ["Ihr", "sollt", "mir", ",", "Blu\u00b7men\u00b7d\u00fcf\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "$,", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das matte Herz erh\u00f6hn.", "tokens": ["Das", "mat\u00b7te", "Herz", "er\u00b7h\u00f6hn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ja! dich will ich genie\u00dfen,", "tokens": ["Ja", "!", "dich", "will", "ich", "ge\u00b7nie\u00b7\u00dfen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "PPER", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Du holder Gott des Mais,", "tokens": ["Du", "hol\u00b7der", "Gott", "des", "Mais", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und mit Gesang dich gr\u00fc\u00dfen", "tokens": ["Und", "mit", "Ge\u00b7sang", "dich", "gr\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "In der Gespielen Krei\u00df.", "tokens": ["In", "der", "Ge\u00b7spie\u00b7len", "Krei\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ich will, mit Bl\u00fcthenzweigen", "tokens": ["Ich", "will", ",", "mit", "Bl\u00fc\u00b7then\u00b7zwei\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPER", "VMFIN", "$,", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "An Brust und Locken, gehn,", "tokens": ["An", "Brust", "und", "Lo\u00b7cken", ",", "gehn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "In alle Th\u00e4ler steigen,", "tokens": ["In", "al\u00b7le", "Th\u00e4\u00b7ler", "stei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wo sp\u00e4te Veilchen stehn;", "tokens": ["Wo", "sp\u00e4\u00b7te", "Veil\u00b7chen", "stehn", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Will euch die Arme geben,", "tokens": ["Will", "euch", "die", "Ar\u00b7me", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wo ihr in T\u00e4nzen schwebt,", "tokens": ["Wo", "ihr", "in", "T\u00e4n\u00b7zen", "schwebt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und wo euch w\u00e4rm'res Leben", "tokens": ["Und", "wo", "euch", "w\u00e4r\u00b7m'\u00b7res", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PPER", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "In jeder Ader strebt.", "tokens": ["In", "je\u00b7der", "A\u00b7der", "strebt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Wi\u00dft, M\u00e4dchen, wenig Wochen,", "tokens": ["Wi\u00dft", ",", "M\u00e4d\u00b7chen", ",", "we\u00b7nig", "Wo\u00b7chen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$,", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So ist der Lenz vorbei.", "tokens": ["So", "ist", "der", "Lenz", "vor\u00b7bei", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und Keinem ward's versprochen,", "tokens": ["Und", "Kei\u00b7nem", "ward's", "ver\u00b7spro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Er seh' ihn einst auf's neu.", "tokens": ["Er", "seh'", "ihn", "einst", "auf's", "neu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPRART", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "H\u00f6r ich's, wann Philomele", "tokens": ["H\u00f6r", "ich's", ",", "wann", "Phi\u00b7lo\u00b7me\u00b7le"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NE", "NE", "$,", "PWAV", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.6": {"text": "Am Todtenh\u00fcgel singt,", "tokens": ["Am", "Tod\u00b7ten\u00b7h\u00fc\u00b7gel", "singt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und dann des H\u00f6rers Seele", "tokens": ["Und", "dann", "des", "H\u00f6\u00b7rers", "See\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Mit s\u00fc\u00dfem Gram durchdringt?", "tokens": ["Mit", "s\u00fc\u00b7\u00dfem", "Gram", "durch\u00b7dringt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Werd' ich vom Bl\u00fcthenregen,", "tokens": ["Werd'", "ich", "vom", "Bl\u00fc\u00b7then\u00b7re\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Den meiner Gruft, gemischt", "tokens": ["Den", "mei\u00b7ner", "Gruft", ",", "ge\u00b7mischt"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["ART", "PPOSAT", "NN", "$,", "VVPP"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mit Thr\u00e4nen und mit Segen", "tokens": ["Mit", "Thr\u00e4\u00b7nen", "und", "mit", "Se\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Freundin weiht, erfrischt?", "tokens": ["Die", "Freun\u00b7din", "weiht", ",", "er\u00b7frischt", "?"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Nein! t\u00e4ndelnd m\u00fc\u00dft ihr kosen,", "tokens": ["Nein", "!", "t\u00e4n\u00b7delnd", "m\u00fc\u00dft", "ihr", "ko\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "ADJD", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Mit Blumen froh geschm\u00fcckt,", "tokens": ["Mit", "Blu\u00b7men", "froh", "ge\u00b7schm\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Eh' eurer Wangen Rosen", "tokens": ["Eh'", "eu\u00b7rer", "Wan\u00b7gen", "Ro\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die Hand des Todes pfl\u00fcckt. \u2013", "tokens": ["Die", "Hand", "des", "To\u00b7des", "pfl\u00fcckt", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Ihr schweigt bei meinen Fragen", "tokens": ["Ihr", "schweigt", "bei", "mei\u00b7nen", "Fra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und euer Blick ist na\u00df!", "tokens": ["Und", "eu\u00b7er", "Blick", "ist", "na\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ihr k\u00f6nnt's mir offen sagen:", "tokens": ["Ihr", "k\u00f6nnt's", "mir", "of\u00b7fen", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Was meint ihr, Lieben, was?", "tokens": ["Was", "meint", "ihr", ",", "Lie\u00b7ben", ",", "was", "?"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$,", "ADJA", "$,", "PWS", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "J\u00fcngst sah ich in der Quelle", "tokens": ["J\u00fcngst", "sah", "ich", "in", "der", "Quel\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Mein Antlitz etwas bla\u00df,", "tokens": ["Mein", "Ant\u00b7litz", "et\u00b7was", "bla\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Mein Auge nicht so helle:", "tokens": ["Mein", "Au\u00b7ge", "nicht", "so", "hel\u00b7le", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKNEG", "ADV", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ihr Lieben, meint ihr das?", "tokens": ["Ihr", "Lie\u00b7ben", ",", "meint", "ihr", "das", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "$,", "VVFIN", "PPER", "PDS", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Wenn ich euch sonst genahet,", "tokens": ["Wenn", "ich", "euch", "sonst", "ge\u00b7na\u00b7het", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So h\u00fcpftet ihr heran,", "tokens": ["So", "h\u00fcpf\u00b7tet", "ihr", "he\u00b7ran", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und rieft mir zu und sahet", "tokens": ["Und", "rieft", "mir", "zu", "und", "sa\u00b7het"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PTKVZ", "KON", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mich heiter l\u00e4chelnd an,", "tokens": ["Mich", "hei\u00b7ter", "l\u00e4\u00b7chelnd", "an", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "In jenen Kinderzeiten,", "tokens": ["In", "je\u00b7nen", "Kin\u00b7der\u00b7zei\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die wir so hingespielt,", "tokens": ["Die", "wir", "so", "hin\u00b7ge\u00b7spielt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Auf die wir Wonne streuten,", "tokens": ["Auf", "die", "wir", "Won\u00b7ne", "streu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die nun mein Geist nicht f\u00fchlt.", "tokens": ["Die", "nun", "mein", "Geist", "nicht", "f\u00fchlt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PPOSAT", "NN", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Wie L\u00e4mmchen auf der Weide", "tokens": ["Wie", "L\u00e4mm\u00b7chen", "auf", "der", "Wei\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Fand damals uns der Tag", "tokens": ["Fand", "da\u00b7mals", "uns", "der", "Tag"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "PPER", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "In wechselhafter Freude", "tokens": ["In", "wech\u00b7sel\u00b7haf\u00b7ter", "Freu\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Auf gr\u00fcnen H\u00fcgeln wach.", "tokens": ["Auf", "gr\u00fc\u00b7nen", "H\u00fc\u00b7geln", "wach", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Erst von den Scherzen m\u00fcde,", "tokens": ["Erst", "von", "den", "Scher\u00b7zen", "m\u00fc\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ADJD", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.6": {"text": "Voll g\u00fcldner Phantasei'n,", "tokens": ["Voll", "g\u00fcld\u00b7ner", "Phan\u00b7ta\u00b7sei'n", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Gieng man zum Schlaf; und Friede", "tokens": ["Gieng", "man", "zum", "Schlaf", ";", "und", "Frie\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PIS", "APPRART", "NN", "$.", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Gieng mit zur Wohnung ein.", "tokens": ["Gieng", "mit", "zur", "Woh\u00b7nung", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Ihr, kommt, gelinde L\u00fcfte,", "tokens": ["Ihr", ",", "kommt", ",", "ge\u00b7lin\u00b7de", "L\u00fcf\u00b7te", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "VVFIN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mir Seufzer wegzuwehn!", "tokens": ["Mir", "Seuf\u00b7zer", "weg\u00b7zu\u00b7wehn", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ihr sollt mir, Blumend\u00fcfte,", "tokens": ["Ihr", "sollt", "mir", ",", "Blu\u00b7men\u00b7d\u00fcf\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "$,", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das matte Herz erh\u00f6hn.", "tokens": ["Das", "mat\u00b7te", "Herz", "er\u00b7h\u00f6hn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ja! dich will ich genie\u00dfen,", "tokens": ["Ja", "!", "dich", "will", "ich", "ge\u00b7nie\u00b7\u00dfen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "PPER", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Du holder Gott des Mais,", "tokens": ["Du", "hol\u00b7der", "Gott", "des", "Mais", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und mit Gesang dich gr\u00fc\u00dfen", "tokens": ["Und", "mit", "Ge\u00b7sang", "dich", "gr\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "In der Gespielen Krei\u00df.", "tokens": ["In", "der", "Ge\u00b7spie\u00b7len", "Krei\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Ich will, mit Bl\u00fcthenzweigen", "tokens": ["Ich", "will", ",", "mit", "Bl\u00fc\u00b7then\u00b7zwei\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPER", "VMFIN", "$,", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "An Brust und Locken, gehn,", "tokens": ["An", "Brust", "und", "Lo\u00b7cken", ",", "gehn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "In alle Th\u00e4ler steigen,", "tokens": ["In", "al\u00b7le", "Th\u00e4\u00b7ler", "stei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wo sp\u00e4te Veilchen stehn;", "tokens": ["Wo", "sp\u00e4\u00b7te", "Veil\u00b7chen", "stehn", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Will euch die Arme geben,", "tokens": ["Will", "euch", "die", "Ar\u00b7me", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wo ihr in T\u00e4nzen schwebt,", "tokens": ["Wo", "ihr", "in", "T\u00e4n\u00b7zen", "schwebt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und wo euch w\u00e4rm'res Leben", "tokens": ["Und", "wo", "euch", "w\u00e4r\u00b7m'\u00b7res", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PPER", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "In jeder Ader strebt.", "tokens": ["In", "je\u00b7der", "A\u00b7der", "strebt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Wi\u00dft, M\u00e4dchen, wenig Wochen,", "tokens": ["Wi\u00dft", ",", "M\u00e4d\u00b7chen", ",", "we\u00b7nig", "Wo\u00b7chen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$,", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So ist der Lenz vorbei.", "tokens": ["So", "ist", "der", "Lenz", "vor\u00b7bei", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und Keinem ward's versprochen,", "tokens": ["Und", "Kei\u00b7nem", "ward's", "ver\u00b7spro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Er seh' ihn einst auf's neu.", "tokens": ["Er", "seh'", "ihn", "einst", "auf's", "neu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPRART", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "H\u00f6r ich's, wann Philomele", "tokens": ["H\u00f6r", "ich's", ",", "wann", "Phi\u00b7lo\u00b7me\u00b7le"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NE", "NE", "$,", "PWAV", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.6": {"text": "Am Todtenh\u00fcgel singt,", "tokens": ["Am", "Tod\u00b7ten\u00b7h\u00fc\u00b7gel", "singt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und dann des H\u00f6rers Seele", "tokens": ["Und", "dann", "des", "H\u00f6\u00b7rers", "See\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Mit s\u00fc\u00dfem Gram durchdringt?", "tokens": ["Mit", "s\u00fc\u00b7\u00dfem", "Gram", "durch\u00b7dringt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Werd' ich vom Bl\u00fcthenregen,", "tokens": ["Werd'", "ich", "vom", "Bl\u00fc\u00b7then\u00b7re\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Den meiner Gruft, gemischt", "tokens": ["Den", "mei\u00b7ner", "Gruft", ",", "ge\u00b7mischt"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["ART", "PPOSAT", "NN", "$,", "VVPP"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mit Thr\u00e4nen und mit Segen", "tokens": ["Mit", "Thr\u00e4\u00b7nen", "und", "mit", "Se\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Freundin weiht, erfrischt?", "tokens": ["Die", "Freun\u00b7din", "weiht", ",", "er\u00b7frischt", "?"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Nein! t\u00e4ndelnd m\u00fc\u00dft ihr kosen,", "tokens": ["Nein", "!", "t\u00e4n\u00b7delnd", "m\u00fc\u00dft", "ihr", "ko\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "ADJD", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Mit Blumen froh geschm\u00fcckt,", "tokens": ["Mit", "Blu\u00b7men", "froh", "ge\u00b7schm\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Eh' eurer Wangen Rosen", "tokens": ["Eh'", "eu\u00b7rer", "Wan\u00b7gen", "Ro\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die Hand des Todes pfl\u00fcckt. \u2013", "tokens": ["Die", "Hand", "des", "To\u00b7des", "pfl\u00fcckt", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Ihr schweigt bei meinen Fragen", "tokens": ["Ihr", "schweigt", "bei", "mei\u00b7nen", "Fra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und euer Blick ist na\u00df!", "tokens": ["Und", "eu\u00b7er", "Blick", "ist", "na\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ihr k\u00f6nnt's mir offen sagen:", "tokens": ["Ihr", "k\u00f6nnt's", "mir", "of\u00b7fen", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Was meint ihr, Lieben, was?", "tokens": ["Was", "meint", "ihr", ",", "Lie\u00b7ben", ",", "was", "?"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$,", "ADJA", "$,", "PWS", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "J\u00fcngst sah ich in der Quelle", "tokens": ["J\u00fcngst", "sah", "ich", "in", "der", "Quel\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Mein Antlitz etwas bla\u00df,", "tokens": ["Mein", "Ant\u00b7litz", "et\u00b7was", "bla\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Mein Auge nicht so helle:", "tokens": ["Mein", "Au\u00b7ge", "nicht", "so", "hel\u00b7le", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKNEG", "ADV", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ihr Lieben, meint ihr das?", "tokens": ["Ihr", "Lie\u00b7ben", ",", "meint", "ihr", "das", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "$,", "VVFIN", "PPER", "PDS", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Wenn ich euch sonst genahet,", "tokens": ["Wenn", "ich", "euch", "sonst", "ge\u00b7na\u00b7het", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So h\u00fcpftet ihr heran,", "tokens": ["So", "h\u00fcpf\u00b7tet", "ihr", "he\u00b7ran", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und rieft mir zu und sahet", "tokens": ["Und", "rieft", "mir", "zu", "und", "sa\u00b7het"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PTKVZ", "KON", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mich heiter l\u00e4chelnd an,", "tokens": ["Mich", "hei\u00b7ter", "l\u00e4\u00b7chelnd", "an", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "In jenen Kinderzeiten,", "tokens": ["In", "je\u00b7nen", "Kin\u00b7der\u00b7zei\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die wir so hingespielt,", "tokens": ["Die", "wir", "so", "hin\u00b7ge\u00b7spielt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Auf die wir Wonne streuten,", "tokens": ["Auf", "die", "wir", "Won\u00b7ne", "streu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die nun mein Geist nicht f\u00fchlt.", "tokens": ["Die", "nun", "mein", "Geist", "nicht", "f\u00fchlt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PPOSAT", "NN", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Wie L\u00e4mmchen auf der Weide", "tokens": ["Wie", "L\u00e4mm\u00b7chen", "auf", "der", "Wei\u00b7de"], "token_info": ["word", "word", "wo