{"textgrid.poem.67721": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Schlachtlied der Himmelsst\u00fcrmer", "genre": "verse", "period": "N.A.", "pub_year": 1765, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nein, kein Gigant, himmel- und erdgeboren,", "tokens": ["Nein", ",", "kein", "Gi\u00b7gant", ",", "him\u00b7mel", "und", "erd\u00b7ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PIAT", "NN", "$,", "TRUNC", "KON", "VVINF", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Ertr\u00e4gt's! Die weibischen Jungen! Herauf,", "tokens": ["Er\u00b7tr\u00e4gt's", "!", "Die", "wei\u00b7bi\u00b7schen", "Jun\u00b7gen", "!", "Her\u00b7auf", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "$.", "ART", "ADJA", "NN", "$.", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Titanen, herauf! H\u00f6rt! Mutter und Vater ruft! \u2013", "tokens": ["Ti\u00b7ta\u00b7nen", ",", "her\u00b7auf", "!", "H\u00f6rt", "!", "Mut\u00b7ter", "und", "Va\u00b7ter", "ruft", "!", "\u2013"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "$,", "PTKVZ", "$.", "VVIMP", "$.", "NN", "KON", "NN", "VVFIN", "$.", "$("], "meter": "-+--+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Zum erfrechten R\u00e4uberssitz", "tokens": ["Zum", "er\u00b7frech\u00b7ten", "R\u00e4u\u00b7bers\u00b7sitz"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Des Vaterm\u00f6rders, des H .... stiers", "tokens": ["Des", "Va\u00b7ter\u00b7m\u00f6r\u00b7ders", ",", "des", "H", "....", "stiers"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ART", "NN", "$,", "ART", "XY", "XY", "XY"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "St\u00fcrmen wir heran! Du, Othem, bahne der Faust! (Feldgeschrei.)", "tokens": ["St\u00fcr\u00b7men", "wir", "he\u00b7ran", "!", "Du", ",", "O\u00b7them", ",", "bah\u00b7ne", "der", "Faust", "!", "(", "Feld\u00b7ge\u00b7schrei", ".", ")"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct", "punct", "word", "punct", "punct"], "pos": ["NN", "PPER", "PTKVZ", "$.", "PPER", "$,", "NN", "$,", "VVFIN", "ART", "NN", "$.", "$(", "NN", "$.", "$("], "meter": "+-+-+-+-+-+++-+", "measure": "unknown.measure.octa.plus"}}, "stanza.2": {"line.1": {"text": "Wie's zittert, das Nektarmahl der S\u00e4ufer! \u2013", "tokens": ["Wie's", "zit\u00b7tert", ",", "das", "Nek\u00b7tar\u00b7mahl", "der", "S\u00e4u\u00b7fer", "!", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "VVFIN", "$,", "ART", "NN", "ART", "NN", "$.", "$("], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Zum Nektar herauf! Berg \u00fcber Berg! Du pflanz", "tokens": ["Zum", "Nek\u00b7tar", "her\u00b7auf", "!", "Berg", "\u00fc\u00b7ber", "Berg", "!", "Du", "pflanz"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["APPRART", "NN", "PTKVZ", "$.", "NN", "APPR", "NN", "$.", "PPER", "NE"], "meter": "-+---+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Des Ossa Wurzel auf diesen Scheitel!", "tokens": ["Des", "Os\u00b7sa", "Wur\u00b7zel", "auf", "die\u00b7sen", "Schei\u00b7tel", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Denn hier tanzt schon sein Olymp mir wie der Ball vor,", "tokens": ["Denn", "hier", "tanzt", "schon", "sein", "O\u00b7lymp", "mir", "wie", "der", "Ball", "vor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ADV", "PPOSAT", "NN", "PPER", "KOKOM", "ART", "NN", "PTKVZ", "$,"], "meter": "--+-+-+-+-+-", "measure": "anapaest.init"}, "line.5": {"text": "Und Rh\u00f6kus wirft mit Str\u00f6men und Wald", "tokens": ["Und", "Rh\u00f6\u00b7kus", "wirft", "mit", "Str\u00f6\u00b7men", "und", "Wald"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "NE", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Den Himmelspfeiler auf die Schulter, und Akt\u00e4on stampft", "tokens": ["Den", "Him\u00b7mel\u00b7spfei\u00b7ler", "auf", "die", "Schul\u00b7ter", ",", "und", "Ak\u00b7t\u00e4\u00b7on", "stampft"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,", "KON", "NN", "VVFIN"], "meter": "-+-+-+-+--++-+", "measure": "iambic.septa.relaxed"}, "line.7": {"text": "Schon Wolken, und Heere von H\u00e4nden und Feuerstr\u00f6m'", "tokens": ["Schon", "Wol\u00b7ken", ",", "und", "Hee\u00b7re", "von", "H\u00e4n\u00b7den", "und", "Feu\u00b7er\u00b7str\u00f6m'"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "NN", "$,", "KON", "NN", "APPR", "NN", "KON", "NN"], "meter": "-+--+--+--+-+", "measure": "amphibrach.tetra.plus"}, "line.8": {"text": "Aus tausend allm\u00e4cht'gen Rachen arbeiten", "tokens": ["Aus", "tau\u00b7send", "all\u00b7m\u00e4cht'\u00b7gen", "Ra\u00b7chen", "ar\u00b7bei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "CARD", "ADJA", "NN", "VVINF"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Den Himmel zu Staub und zu Gluth. Triumph! er ersteigt's!", "tokens": ["Den", "Him\u00b7mel", "zu", "Staub", "und", "zu", "Gluth", ".", "Tri\u00b7umph", "!", "er", "er\u00b7steigt's", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "KON", "APPR", "NN", "$.", "NN", "$.", "PPER", "NE", "$."], "meter": "-+--+--+-+--+", "measure": "amphibrach.tri.plus"}, "line.10": {"text": "Schaaren! Triumph! durchbrochen! erstiegen!", "tokens": ["Schaa\u00b7ren", "!", "Tri\u00b7umph", "!", "durch\u00b7bro\u00b7chen", "!", "er\u00b7stie\u00b7gen", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$.", "NN", "$.", "VVPP", "$.", "VVPP", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.11": {"text": "Flieht Ihr! \u2013 Sie fliehen, sie flohn; da schwimmen", "tokens": ["Flieht", "Ihr", "!", "\u2013", "Sie", "flie\u00b7hen", ",", "sie", "flohn", ";", "da", "schwim\u00b7men"], "token_info": ["word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "$.", "$(", "PPER", "VVINF", "$,", "PPER", "VVFIN", "$.", "ADV", "VVINF"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "Nektarg\u00fcsse; der letzte Tropfen ward Stein", "tokens": ["Nek\u00b7tar\u00b7g\u00fcs\u00b7se", ";", "der", "letz\u00b7te", "Trop\u00b7fen", "ward", "Stein"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$.", "ART", "ADJA", "NN", "VAFIN", "NN"], "meter": "--+--+-+--+", "measure": "anapaest.di.plus"}, "line.13": {"text": "In der m\u00e4chtigen G\u00f6tter Munde.", "tokens": ["In", "der", "m\u00e4ch\u00b7ti\u00b7gen", "G\u00f6t\u00b7ter", "Mun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.14": {"text": "Wo sind sie? Die M\u00e4chtige wird Spatz,", "tokens": ["Wo", "sind", "sie", "?", "Die", "M\u00e4ch\u00b7ti\u00b7ge", "wird", "Spatz", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "$.", "ART", "NN", "VAFIN", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.15": {"text": "Sieh, G\u00f6tter zu Eulen und Hasen, und Donnerer Du", "tokens": ["Sieh", ",", "G\u00f6t\u00b7ter", "zu", "Eu\u00b7len", "und", "Ha\u00b7sen", ",", "und", "Don\u00b7ne\u00b7rer", "Du"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "NN", "APPR", "NE", "KON", "NN", "$,", "KON", "NN", "PPER"], "meter": "-+--+--+--+--+", "measure": "amphibrach.penta.plus"}, "line.16": {"text": "Maus! \u2013 Triumph! mit G\u00f6tterm\u00e4usen fechten Giganten!", "tokens": ["Maus", "!", "\u2013", "Tri\u00b7umph", "!", "mit", "G\u00f6t\u00b7ter\u00b7m\u00e4u\u00b7sen", "fech\u00b7ten", "Gi\u00b7gan\u00b7ten", "!"], "token_info": ["word", "punct", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "$(", "NN", "$.", "APPR", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+--+-", "measure": "hexameter"}, "line.17": {"text": "Zum Thron! Und, \u00bbkeuchender Erdesohn Du!", "tokens": ["Zum", "Thron", "!", "Und", ",", "\u00bb", "keu\u00b7chen\u00b7der", "Er\u00b7de\u00b7sohn", "Du", "!"], "token_info": ["word", "word", "punct", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$.", "KON", "$,", "$(", "ADJA", "NN", "PPER", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.18": {"text": "Zwerghund mit dem Stecken!\u00ab (Hercul ruft.) G\u00f6tter heran!", "tokens": ["Zwerg\u00b7hund", "mit", "dem", "Ste\u00b7cken", "!", "\u00ab", "(", "Her\u00b7cul", "ruft", ".", ")", "G\u00f6t\u00b7ter", "he\u00b7ran", "!"], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$.", "$(", "$(", "NE", "VVFIN", "$.", "$(", "NN", "PTKVZ", "$."], "meter": "+---+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.19": {"text": "(sie wandeln sich.) Da liegt er! Ach nein! Er steht und (Hyder,", "tokens": ["(", "sie", "wan\u00b7deln", "sich", ".", ")", "Da", "liegt", "er", "!", "Ach", "nein", "!", "Er", "steht", "und", "(", "Hy\u00b7der", ","], "token_info": ["punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PRF", "$.", "$(", "ADV", "VVFIN", "PPER", "$.", "NN", "PTKANT", "$.", "PPER", "VVFIN", "KON", "$(", "NE", "$,"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.20": {"text": "So stirb, erquetscht in den faulenden Mutterschoo\u00df!)", "tokens": ["So", "stirb", ",", "er\u00b7quetscht", "in", "den", "fau\u00b7len\u00b7den", "Mut\u00b7ter\u00b7schoo\u00df", "!", ")"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "$,", "VVFIN", "APPR", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.21": {"text": "Weh, wie er braust, entathmend ein Ach des Orkans!", "tokens": ["Weh", ",", "wie", "er", "braust", ",", "en\u00b7tath\u00b7mend", "ein", "Ach", "des", "Or\u00b7kans", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "PPER", "VVFIN", "$,", "VVPP", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.22": {"text": "Rache! \u2013 P\u00e4an! da liegt die trotzige Juno", "tokens": ["Ra\u00b7che", "!", "\u2013", "P\u00e4\u00b7an", "!", "da", "liegt", "die", "trot\u00b7zi\u00b7ge", "Ju\u00b7no"], "token_info": ["word", "punct", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$.", "$(", "NE", "$.", "ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.23": {"text": "Porphyrion unter! o weh! \u2013 Rache! da kreischt", "tokens": ["Por\u00b7phy\u00b7ri\u00b7on", "un\u00b7ter", "!", "o", "weh", "!", "\u2013", "Ra\u00b7che", "!", "da", "kreischt"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct", "word", "punct", "word", "word"], "pos": ["NN", "APPR", "$.", "FM", "PTKVZ", "$.", "$(", "NN", "$.", "ADV", "VVFIN"], "meter": "-+--+-+-+--+", "measure": "iambic.penta.relaxed"}, "line.24": {"text": "Der eisenfressende Mars wie Ochsenmyriaden! \u2013", "tokens": ["Der", "ei\u00b7sen\u00b7fres\u00b7sen\u00b7de", "Mars", "wie", "Och\u00b7sen\u00b7my\u00b7ria\u00b7den", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "KOKOM", "NN", "$.", "$("], "meter": "-+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.25": {"text": "Flieht nicht! \u2013 Manne Dich, Mann, dem der Blitz", "tokens": ["Flieht", "nicht", "!", "\u2013", "Man\u00b7ne", "Dich", ",", "Mann", ",", "dem", "der", "Blitz"], "token_info": ["word", "word", "punct", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PTKNEG", "$.", "$(", "NN", "PPER", "$,", "NN", "$,", "PRELS", "ART", "NN"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.26": {"text": "Auf der Stirn gl\u00fcht! Rache, ergl\u00fch! Stampf, da\u00df der Berg", "tokens": ["Auf", "der", "Stirn", "gl\u00fcht", "!", "Ra\u00b7che", ",", "er\u00b7gl\u00fch", "!", "Stampf", ",", "da\u00df", "der", "Berg"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["AP