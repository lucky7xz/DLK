{"textgrid.poem.34134": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Virtuosen-Portraits", "genre": "verse", "period": "N.A.", "pub_year": 1838, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Also die\u00df ist der Mann, durch welchen mich Mozart entz\u00fcckte!", "tokens": ["Al\u00b7so", "die\u00df", "ist", "der", "Mann", ",", "durch", "wel\u00b7chen", "mich", "Mo\u00b7zart", "ent\u00b7z\u00fcck\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDS", "VAFIN", "ART", "NN", "$,", "APPR", "PWAT", "PPER", "NE", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "S\u00e4h' ich die Geige doch auch, die ihm so wacker gedient!", "tokens": ["S\u00e4h'", "ich", "die", "Gei\u00b7ge", "doch", "auch", ",", "die", "ihm", "so", "wa\u00b7cker", "ge\u00b7dient", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADV", "ADV", "$,", "PRELS", "PPER", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "S\u00e4h' ich das n\u00fctzliche Schaf, das dieser die Saiten geliefert,", "tokens": ["S\u00e4h'", "ich", "das", "n\u00fctz\u00b7li\u00b7che", "Schaf", ",", "das", "die\u00b7ser", "die", "Sai\u00b7ten", "ge\u00b7lie\u00b7fert", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$,", "PRELS", "PDAT", "ART", "NN", "VVPP", "$,"], "meter": "-+-+--+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und das geduldige Pferd, das ihm den Bogen bezog!", "tokens": ["Und", "das", "ge\u00b7dul\u00b7di\u00b7ge", "Pferd", ",", "das", "ihm", "den", "Bo\u00b7gen", "be\u00b7zog", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$,", "PRELS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+--+-+-+--+", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Also die\u00df ist der Mann, durch welchen mich Mozart entz\u00fcckte!", "tokens": ["Al\u00b7so", "die\u00df", "ist", "der", "Mann", ",", "durch", "wel\u00b7chen", "mich", "Mo\u00b7zart", "ent\u00b7z\u00fcck\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDS", "VAFIN", "ART", "NN", "$,", "APPR", "PWAT", "PPER", "NE", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "S\u00e4h' ich die Geige doch auch, die ihm so wacker gedient!", "tokens": ["S\u00e4h'", "ich", "die", "Gei\u00b7ge", "doch", "auch", ",", "die", "ihm", "so", "wa\u00b7cker", "ge\u00b7dient", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADV", "ADV", "$,", "PRELS", "PPER", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "S\u00e4h' ich das n\u00fctzliche Schaf, das dieser die Saiten geliefert,", "tokens": ["S\u00e4h'", "ich", "das", "n\u00fctz\u00b7li\u00b7che", "Schaf", ",", "das", "die\u00b7ser", "die", "Sai\u00b7ten", "ge\u00b7lie\u00b7fert", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$,", "PRELS", "PDAT", "ART", "NN", "VVPP", "$,"], "meter": "-+-+--+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und das geduldige Pferd, das ihm den Bogen bezog!", "tokens": ["Und", "das", "ge\u00b7dul\u00b7di\u00b7ge", "Pferd", ",", "d