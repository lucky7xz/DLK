{"textgrid.poem.34125": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Monolog eines Modellj\u00e4gers", "genre": "verse", "period": "N.A.", "pub_year": 1838, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Welch ein herrlicher Kopf! Und einer der vielen des P\u00f6bels!", "tokens": ["Welch", "ein", "herr\u00b7li\u00b7cher", "Kopf", "!", "Und", "ei\u00b7ner", "der", "vie\u00b7len", "des", "P\u00f6\u00b7bels", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ART", "ADJA", "NN", "$.", "KON", "ART", "ART", "PIAT", "ART", "NN", "$."], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Macht sie nicht heut' das Modell macht sie es morgen gewi\u00df,", "tokens": ["Macht", "sie", "nicht", "heut'", "das", "Mo\u00b7dell", "macht", "sie", "es", "mor\u00b7gen", "ge\u00b7wi\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PTKNEG", "ADV", "ART", "NN", "VVFIN", "PPER", "PPER", "ADV", "ADV", "$,"], "meter": "+--+-+-+--+--+", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Wenn sie des Hutes bedarf, ihn gegen die Sonne zu sch\u00fctzen,", "tokens": ["Wenn", "sie", "des", "Hu\u00b7tes", "be\u00b7darf", ",", "ihn", "ge\u00b7gen", "die", "Son\u00b7ne", "zu", "sch\u00fct\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$,", "PPER", "APPR", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+--+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Welchem Rumpfe jedoch setzt man am besten ihn auf?", "tokens": ["Wel\u00b7chem", "Rump\u00b7fe", "je\u00b7doch", "setzt", "man", "am", "bes\u00b7ten", "ihn", "auf