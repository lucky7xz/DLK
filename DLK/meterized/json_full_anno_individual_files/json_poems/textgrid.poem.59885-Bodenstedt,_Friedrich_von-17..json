{"textgrid.poem.59885": {"metadata": {"author": {"name": "Bodenstedt, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "17.", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So singt Mirza-Schaffy: Wir wollen sorglos", "tokens": ["So", "singt", "Mir\u00b7za\u00b7Schaffy", ":", "Wir", "wol\u00b7len", "sorg\u00b7los"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NE", "$.", "PPER", "VMFIN", "ADJD"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "In der Gefahr sein \u2013", "tokens": ["In", "der", "Ge\u00b7fahr", "sein", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAINF", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Im Bund mit Wein, mit Rosen und mit Frauen", "tokens": ["Im", "Bund", "mit", "Wein", ",", "mit", "Ro\u00b7sen", "und", "mit", "Frau\u00b7en"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "APPR", "NN", "$,", "APPR", "NN", "KON", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Des Kummers bar sein!", "tokens": ["Des", "Kum\u00b7mers", "bar", "sein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VAINF", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Mag Heuchelei mit Hochmut sich verb\u00fcnden,", "tokens": ["Mag", "Heu\u00b7che\u00b7lei", "mit", "Hoch\u00b7mut", "sich", "ver\u00b7b\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "APPR", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Bosheit mit Dummheit \u2013", "tokens": ["Bos\u00b7heit",