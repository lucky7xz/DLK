{"dta.poem.1548": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Gl\u00fcckw\u00fcnschungen  \n an  \n Bekr\u00f6nte  \n und  \n Erlauchte H\u00e4ubter.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "F\u00fcrsten sind das Licht der Welt und der Unterthanen Leben:", "tokens": ["F\u00fcrs\u00b7ten", "sind", "das", "Licht", "der", "Welt", "und", "der", "Un\u00b7ter\u00b7tha\u00b7nen", "Le\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "ART", "NN", "KON", "ART", "NN", "NN", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Drum wir ihnen billig auch Hertz und Licht zum Opffer", "tokens": ["Drum", "wir", "ih\u00b7nen", "bil\u00b7lig", "auch", "Hertz", "und", "Licht", "zum", "Opf\u00b7fer"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "PPER", "PPER", "ADJD", "ADV", "NN", "KON", "NN", "APPRART", "NN"], "meter": "--+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "geben.", "tokens": ["ge\u00b7ben", "."], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "So viel Licht- und Fackel-Stralen die geraume Stadt izt hegt/", "tokens": ["So", "viel", "Licht", "und", "Fa\u00b7ckel\u00b7S\u00b7tra\u00b7len", "die", "ge\u00b7rau\u00b7me", "Stadt", "izt", "hegt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "TRUNC", "KON", "NN", "ART", "ADJA", "NN", "ADV", "VVFIN", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.5": {"text": "So viel Leben/ Heyl und Gl\u00fccke sey dir Hertzog zugelegt!", "tokens": ["So", "viel", "Le\u00b7ben", "/", "Heyl", "und", "Gl\u00fc\u00b7cke", "sey", "dir", "Hert\u00b7zog", "zu\u00b7ge\u00b7legt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "$(", "NN", "KON", "NN", "VAFIN", "PPER", "NE", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}}}}