{"dta.poem.16802": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "161.  Jm jnnern Wohnt man wol.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Was meines Geistes Geist/ meins wesens wesen ist/", "tokens": ["Was", "mei\u00b7nes", "Geis\u00b7tes", "Geist", "/", "meins", "we\u00b7sens", "we\u00b7sen", "ist", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "NN", "$(", "FM.fr", "FM.fr", "FM.fr", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das ists/ das ich f\u00fcr mich zur Wohnung hab erkiest.", "tokens": ["Das", "ists", "/", "das", "ich", "f\u00fcr", "mich", "zur", "Woh\u00b7nung", "hab", "er\u00b7kiest", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "$(", "PRELS", "PPER", "APPR", "PPER", "APPRART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}