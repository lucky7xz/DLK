{"textgrid.poem.36467": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "17. Der Esel und das Pferd", "genre": "verse", "period": "N.A.", "pub_year": 1761, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Esel ging, mit steifem Gange,", "tokens": ["Ein", "E\u00b7sel", "ging", ",", "mit", "stei\u00b7fem", "Gan\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zu einem Pferde: Sage mir,", "tokens": ["Zu", "ei\u00b7nem", "Pfer\u00b7de", ":", "Sa\u00b7ge", "mir", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$.", "NN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sprach er zum Pferde, nehmen dir,", "tokens": ["Sprach", "er", "zum", "Pfer\u00b7de", ",", "neh\u00b7men", "dir", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPER", "APPRART", "NN", "$,", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mein Bruder, Zaum und Stange", "tokens": ["Mein", "Bru\u00b7der", ",", "Zaum", "und", "Stan\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die Freiheit nicht auf deinem Wege?", "tokens": ["Die", "Frei\u00b7heit", "nicht", "auf", "dei\u00b7nem", "We\u00b7ge", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ja, leider! gab das Pferd zur Antwort, aber doch", "tokens"