{"dta.poem.18173": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "Der rothe  \n Johannis Beeren Busch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198805", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein gr\u00fcn belaubter Busch der voll Jo-", "tokens": ["Ein", "gr\u00fcn", "be\u00b7laub\u00b7ter", "Busch", "der", "voll", "Jo"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "ADJA", "NN", "ART", "ADJD", "TRUNC"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "hannis Beeren,", "tokens": ["han\u00b7nis", "Bee\u00b7ren", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Und von des G\u00e4rtners Hand mit woll-", "tokens": ["Und", "von", "des", "G\u00e4rt\u00b7ners", "Hand", "mit", "woll"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "NN", "APPR", "TRUNC"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "geschlifnen Scheren,", "tokens": ["ge\u00b7schlif\u00b7nen", "Sche\u00b7ren", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Als wie ein Krantz geschnitzt; der r\u00fchr-", "tokens": ["Als", "wie", "ein", "Krantz", "ge\u00b7schnitzt", ";", "der", "r\u00fchr"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "KOKOM", "ART", "NN", "VVPP", "$.", "ART", "TRUNC"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "te mein Gesicht,", "tokens": ["te", "mein", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Da ich zum Garten kam, durch sein gef\u00e4rbtes Licht:", "tokens": ["Da", "ich", "zum", "Gar\u00b7ten", "kam", ",", "durch", "sein", "ge\u00b7f\u00e4rb\u00b7tes", "Licht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "VVFIN", "$,", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ich ward dadurch entflammt aus dessen Kostbarkeiten,", "tokens": ["Ich", "ward", "da\u00b7durch", "ent\u00b7flammt", "aus", "des\u00b7sen", "Kost\u00b7bar\u00b7kei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PAV", "VVPP", "APPR", "PRELAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Zu ihres Sch\u00f6pfers Ehr ein Loblied herzuleiten.", "tokens": ["Zu", "ih\u00b7res", "Sch\u00f6p\u00b7fers", "Ehr", "ein", "Lob\u00b7lied", "her\u00b7zu\u00b7lei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Mein GOtt! welch eine Lust! wenn durch sma-", "tokens": ["Mein", "Gott", "!", "welch", "ei\u00b7ne", "Lust", "!", "wenn", "durch", "sma"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$.", "PWAT", "ART", "NN", "$.", "KOUS", "APPR", "TRUNC"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.11": {"text": "ragdnes Gr\u00fcn", "tokens": ["ragd\u00b7nes", "Gr\u00fcn"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.12": {"text": "Jm rothen Schimmer strahlt die Beerlein, wie Rubin,", "tokens": ["Jm", "ro\u00b7then", "Schim\u00b7mer", "strahlt", "die", "Be\u00b7er\u00b7lein", ",", "wie", "Ru\u00b7bin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "ART", "NN", "$,", "PWAV", "NE", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Die nach dem Augenschein an sch\u00f6nen Schn\u00fcren han-", "tokens": ["Die", "nach", "dem", "Au\u00b7gen\u00b7schein", "an", "sch\u00f6\u00b7nen", "Schn\u00fc\u00b7ren", "han"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "APPR", "ADJA", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "gen,", "tokens": ["gen", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "-", "measure": "single.down"}, "line.15": {"text": "Und in gezogner Reih wie rothe Perlen prangen.", "tokens": ["Und", "in", "ge\u00b7zog\u00b7ner", "Reih", "wie", "ro\u00b7the", "Per\u00b7len", "pran\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "KOKOM", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Sieht man den Busch recht an, wenn bei der Mor-", "tokens": ["Sieht", "man", "den", "Busch", "recht", "an", ",", "wenn", "bei", "der", "Mor"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "ART", "NN", "ADJD", "PTKVZ", "$,", "KOUS", "APPR", "ART", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "genzeit,", "tokens": ["gen\u00b7zeit", ","], "token_info": ["word", "punct"], "pos": ["ADJD", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.18": {"text": "Die Sonn den Feuerstrahl recht auf die Beeren streut:", "tokens": ["Die", "Sonn", "den", "Feu\u00b7er\u00b7strahl", "recht", "auf", "die", "Bee\u00b7ren", "streut", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "ADJD", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "So sieht man eine Meng von kleinen Sonnen glim-", "tokens": ["So", "sieht", "man", "ei\u00b7ne", "Meng", "von", "klei\u00b7nen", "Son\u00b7nen", "glim"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "ART", "NN", "APPR", "ADJA", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "men,", "tokens": ["men", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "-", "measure": "single.down"}, "line.21": {"text": "In deren Inbegrif verg\u00fcldte K\u00f6rner schwimmen.", "tokens": ["In", "de\u00b7ren", "In\u00b7be\u00b7grif", "ver\u00b7g\u00fcld\u00b7te", "K\u00f6r\u00b7ner", "schwim\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "So wunderbar ist ", "tokens": ["So", "wun\u00b7der\u00b7bar", "ist"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADJD", "VAFIN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.23": {"text": "Kein K\u00f6nig gl\u00e4nzet so mit seiner Kronen Pracht,", "tokens": ["Kein", "K\u00f6\u00b7nig", "gl\u00e4n\u00b7zet", "so", "mit", "sei\u00b7ner", "Kro\u00b7nen", "Pracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Als dieses B\u00fcschlein thut, daran die Beeren scheinen,", "tokens": ["Als", "die\u00b7ses", "B\u00fc\u00b7schlein", "thut", ",", "da\u00b7ran", "die", "Bee\u00b7ren", "schei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "VVFIN", "$,", "PAV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Wie runde K\u00fcgelchen von glatten Edelsteinen.", "tokens": ["Wie", "run\u00b7de", "K\u00fc\u00b7gel\u00b7chen", "von", "glat\u00b7ten", "E\u00b7del\u00b7stei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Jhr Sch\u00f6nen dieser Welt! seht dieses Busches Kranz,", "tokens": ["Ihr", "Sch\u00f6\u00b7nen", "die\u00b7ser", "Welt", "!", "seht", "die\u00b7ses", "Bu\u00b7sches", "Kranz", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PDAT", "NN", "$.", "VVFIN", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Beschaut den lieblichen, durchsichtgen Wunderglanz", "tokens": ["Be\u00b7schaut", "den", "lieb\u00b7li\u00b7chen", ",", "durch\u00b7sicht\u00b7gen", "Wun\u00b7der\u00b7glanz"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Welch ungez\u00e4hlte Meng von pr\u00e4chtigen Carbunkeln,", "tokens": ["Welch", "un\u00b7ge\u00b7z\u00e4hl\u00b7te", "Meng", "von", "pr\u00e4ch\u00b7ti\u00b7gen", "Car\u00b7bun\u00b7keln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Seht ihr an seinen Haupt, in sch\u00f6nster Ordnung fun-", "tokens": ["Seht", "ihr", "an", "sei\u00b7nen", "Haupt", ",", "in", "sch\u00f6ns\u00b7ter", "Ord\u00b7nung", "fun"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,", "APPR", "ADJA", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "keln!", "tokens": ["keln", "!"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-", "measure": "single.down"}, "line.31": {"text": "Wie pranget eine Braut, wenn ihre Mirten Kron,", "tokens": ["Wie", "pran\u00b7get", "ei\u00b7ne", "Braut", ",", "wenn", "ih\u00b7re", "Mir\u00b7ten", "Kron", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "$,", "KOUS", "PPOSAT", "NN", "NE", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Der Keuschheit gr\u00fcner Schmuk, der reinen Tugend", "tokens": ["Der", "Keuschheit", "gr\u00fc\u00b7ner", "Schmuk", ",", "der", "rei\u00b7nen", "Tu\u00b7gend"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.33": {"text": "Lohn", "tokens": ["Lohn"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.34": {"text": "Mit Perlen durchgestikt, mit Diamant besezzet,", "tokens": ["Mit", "Per\u00b7len", "durch\u00b7ges\u00b7tikt", ",", "mit", "Di\u00b7a\u00b7mant", "be\u00b7sez\u00b7zet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Und andern Steinen mehr, die man von Werthe", "tokens": ["Und", "an\u00b7dern", "Stei\u00b7nen", "mehr", ",", "die", "man", "von", "Wert\u00b7he"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "ADV", "$,", "PRELS", "PIS", "APPR", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.36": {"text": "sch\u00e4zzet!", "tokens": ["sch\u00e4z\u00b7zet", "!"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.37": {"text": "Und was ist dieser Schmuk, bei dem, was die Natur,", "tokens": ["Und", "was", "ist", "die\u00b7ser", "Schmuk", ",", "bei", "dem", ",", "was", "die", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VAFIN", "PDAT", "NN", "$,", "APPR", "ART", "$,", "PRELS", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "An der Johannis Beer und ihres Buschs Figur", "tokens": ["An", "der", "Jo\u00b7han\u00b7nis", "Beer", "und", "ih\u00b7res", "Buschs", "Fi\u00b7gur"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NE", "NE", "KON", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Vor herrliches gemacht? Wie ist die Braut zu achten,", "tokens": ["Vor", "herr\u00b7li\u00b7ches", "ge\u00b7macht", "?", "Wie", "ist", "die", "Braut", "zu", "ach\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "VVPP", "$.", "PWAV", "VAFIN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Wenn wir im Gegentheil dies Kron Geb\u00fcsch be-", "tokens": ["Wenn", "wir", "im", "Ge\u00b7gen\u00b7theil", "dies", "Kron", "Ge\u00b7b\u00fcsch", "be"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPRART", "NN", "PDS", "NN", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.41": {"text": "trachten?", "tokens": ["trach\u00b7ten", "?"], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.42": {"text": "Und woher k\u00f6mt es doch, da\u00df wir das k\u00fcnstlich Gr\u00fcn", "tokens": ["Und", "wo\u00b7her", "k\u00f6mt", "es", "doch", ",", "da\u00df", "wir", "das", "k\u00fcnst\u00b7lich", "Gr\u00fcn"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "VVFIN", "PPER", "ADV", "$,", "KOUS", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Mit Edelstein besetzt, dem, von Natur vorziehn,", "tokens": ["Mit", "E\u00b7del\u00b7stein", "be\u00b7setzt", ",", "dem", ",", "von", "Na\u00b7tur", "vor\u00b7ziehn", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,", "PRELS", "$,", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Da das Nat\u00fcrliche, doch noch viel sch\u00f6ner gl\u00e4nzzet,", "tokens": ["Da", "das", "Na\u00b7t\u00fcr\u00b7li\u00b7che", ",", "doch", "noch", "viel", "sch\u00f6\u00b7ner", "gl\u00e4nz\u00b7zet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,", "ADV", "ADV", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Als alles K\u00fcnstliche, da\u00df diese Sch\u00f6nen kr\u00e4nzzet?", "tokens": ["Als", "al\u00b7les", "K\u00fcnst\u00b7li\u00b7che", ",", "da\u00df", "die\u00b7se", "Sch\u00f6\u00b7nen", "kr\u00e4nz\u00b7zet", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "$,", "KOUS", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "Es komt aus einen Trieb verkehrter Einbildung,", "tokens": ["Es", "komt", "aus", "ei\u00b7nen", "Trieb", "ver\u00b7kehr\u00b7ter", "Ein\u00b7bil\u00b7dung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.47": {"text": "Die zeuget unvermerkt hernach Verwunderung,", "tokens": ["Die", "zeu\u00b7get", "un\u00b7ver\u00b7merkt", "her\u00b7nach", "Ver\u00b7wun\u00b7de\u00b7rung", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ADV", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.48": {"text": "Da wir demjenigen, was wir gar selten sehen,", "tokens": ["Da", "wir", "dem\u00b7je\u00b7ni\u00b7gen", ",", "was", "wir", "gar", "sel\u00b7ten", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PDS", "$,", "PRELS", "PPER", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.49": {"text": "Bei dem Gewohnheitsschlaf, den Vorzug eingeste-", "tokens": ["Bei", "dem", "Ge\u00b7wohn\u00b7heits\u00b7schlaf", ",", "den", "Vor\u00b7zug", "ein\u00b7ge\u00b7ste"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.50": {"text": "hen.", "tokens": ["hen", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-", "measure": "single.down"}, "line.51": {"text": "M