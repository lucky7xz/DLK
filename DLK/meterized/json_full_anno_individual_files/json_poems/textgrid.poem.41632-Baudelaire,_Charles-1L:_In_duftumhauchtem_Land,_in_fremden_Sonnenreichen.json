{"textgrid.poem.41632": {"metadata": {"author": {"name": "Baudelaire, Charles", "birth": "N.A.", "death": "N.A."}, "title": "1L: In duftumhauchtem Land, in fremden Sonnenreichen", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In duftumhauchtem Land, in fremden Sonnenreichen", "tokens": ["In", "duf\u00b7tum\u00b7hauch\u00b7tem", "Land", ",", "in", "frem\u00b7den", "Son\u00b7nen\u00b7rei\u00b7chen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sah unter B\u00e4umen, die ein Purpurglanz umrinnt,", "tokens": ["Sah", "un\u00b7ter", "B\u00e4u\u00b7men", ",", "die", "ein", "Pur\u00b7pur\u00b7glanz", "um\u00b7rinnt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "$,", "PRELS", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wo Schlaf von Palmen tropft, Traumregen zu vergleichen,", "tokens": ["Wo", "Schlaf", "von", "Pal\u00b7men", "tropft", ",", "Traum\u00b7re\u00b7gen", "zu", "ver\u00b7glei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "NN", "VVFIN", "$,", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Ich eine Dame, die ein Zauber fremd umspinnt.", "tokens": ["Ich", "ei\u00b7ne", "Da\u00b7me", ",", "die", "ein", "Zau\u00b7ber", "fremd", "um\u00b7spinnt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "$,", "PRELS", "ART", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Der Sch\u00f6nen zierem Hals, dem Angesicht, dem bleichen,", "tokens": ["Der", "Sch\u00f6\u00b7nen", "zie\u00b7rem", "Hals", ",", "dem", "An\u00b7ge\u00b7sicht", ",", "dem", "blei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,", "ART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Entleuchtet stolzer Reiz, der Herzen ihr gewinnt.", "tokens": ["Ent\u00b7leuch\u00b7tet", "stol\u00b7zer", "Reiz", ",", "der", "Her\u00b7zen", "ihr", "ge\u00b7winnt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "$,", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Gleich schlanker J\u00e4gerin scheint sie durchs Land zu streichen,", "tokens": ["Gleich", "schlan\u00b7ker", "J\u00e4\u00b7ge\u00b7rin", "scheint", "sie", "durchs", "Land", "zu", "strei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VVFIN", "PPER", "APPRART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ihr Aug ist klare Ruh, ihr L\u00e4cheln stumm-gelind.", "tokens": ["Ihr", "Aug", "ist", "kla\u00b7re", "Ruh", ",", "ihr", "L\u00e4\u00b7cheln", "stum\u00b7mge\u00b7lind", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJA", "NN", "$,", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "K\u00e4mt, Herrin, Ihr dereinst zum wahren Ruhmeslande,", "tokens": ["K\u00e4mt", ",", "Her\u00b7rin", ",", "Ihr", "d