{"textgrid.poem.46013": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "[es war einmal die Liebe]", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es war einmal die Liebe,", "tokens": ["Es", "war", "ein\u00b7mal", "die", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die himmelklare Liebe,", "tokens": ["Die", "him\u00b7mel\u00b7kla\u00b7re", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sie war in edlem Zorn", "tokens": ["Sie", "war", "in", "ed\u00b7lem", "Zorn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und sprach zum blinden Triebe,", "tokens": ["Und", "sprach", "zum", "blin\u00b7den", "Trie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Verzeih, heut kriegst du Hiebe", "tokens": ["Ver\u00b7zeih", ",", "heut", "kriegst", "du", "Hie\u00b7be"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "ADV", "VVFIN", "PPER", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ganz ernst mit einem Dorn.", "tokens": ["Ganz", "ernst", "mit", "ei\u00b7nem", "Dorn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Der Trieb h\u00f6rt dies betroffen,", "tokens": ["Der", "Trieb", "h\u00f6rt", "dies", "be\u00b7trof\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PDS", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Doch kaum hat ihn getroffen", "tokens": ["Doch", "kaum", "hat", "ihn", "ge\u00b7trof\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "PPER", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der Liebe Dornenstreich,", "tokens": ["Der", "Lie\u00b7be", "Dor\u00b7nen\u00b7streich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Da war die Knospe offen,", "tokens": ["Da", "war", "die", "Knos\u00b7pe", "of\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der Dorn ganz ohn' Verhoffen", "tokens": ["Der", "Dorn", "ganz", "ohn'", "Ver\u00b7hof\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Schlug aus voll Rosen gleich.", "tokens": ["Schlug", "aus", "voll", "Ro\u00b7sen", "gleich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJD", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Es war einmal die Liebe", "tokens": ["Es", "war", "ein\u00b7mal", "die", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die himmelklare Liebe", "tokens": ["Die", "him\u00b7mel\u00b7kla\u00b7re", "Lie\u00b7be"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sie war vom Trieb betr\u00fcbt,", "tokens": ["Sie", "war", "vom", "Trieb", "be\u00b7tr\u00fcbt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Sprach drum zum blinden Triebe", "tokens": ["Sprach", "drum", "zum", "blin\u00b7den", "Trie\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PAV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "An dir dem Friedensdiebe,", "tokens": ["An", "dir", "dem", "Frie\u00b7dens\u00b7die\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wird Rache heut ge\u00fcbt.", "tokens": ["Wird", "Ra\u00b7che", "heut", "ge\u00b7\u00fcbt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Doch als sie sich wollt' r\u00e4chen", "tokens": ["Doch", "als", "sie", "sich", "wollt'", "r\u00e4\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "PRF", "VMFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da st\u00fcrzt in Tr\u00e4nenb\u00e4chen", "tokens": ["Da", "st\u00fcrzt", "in", "Tr\u00e4\u00b7nen\u00b7b\u00e4\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das Herz ihr aus der Brust,", "tokens": ["Das", "Herz", "ihr", "aus", "der", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Sie kann den Stab nicht brechen", "tokens": ["Sie", "kann", "den", "Stab", "nicht", "bre\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ART", "NN", "PTKNEG", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die Lieb' wird aller Schw\u00e4chen", "tokens": ["Die", "Lieb'", "wird", "al\u00b7ler", "Schw\u00e4\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Des Triebes sich bewu\u00dft.", "tokens": ["Des", "Trie\u00b7bes", "sich", "be\u00b7wu\u00dft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Es war einmal die Liebe,", "tokens": ["Es", "war", "ein\u00b7mal", "die", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die himmelklare Liebe,", "tokens": ["Die", "him\u00b7mel\u00b7kla\u00b7re", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sie war vom Trieb gekr\u00e4nkt,", "tokens": ["Sie", "war", "vom", "Trieb", "ge\u00b7kr\u00e4nkt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und sprach zum blinden Triebe,", "tokens": ["Und", "sprach", "zum", "blin\u00b7den", "Trie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wenn dir kein Trost auch bliebe,", "tokens": ["Wenn", "dir", "kein", "Trost", "auch", "blie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Heut wird dir's nicht geschenkt.", "tokens": ["Heut", "wird", "dir's", "nicht", "ge\u00b7schenkt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Und um ihm's zu gedenken", "tokens": ["Und", "um", "ihm's", "zu", "ge\u00b7den\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PIS", "PTKZU", "VVINF"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Tut sie ein F\u00fcllhorn senken,", "tokens": ["Tut", "sie", "ein", "F\u00fcll\u00b7horn", "sen\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "++-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Voll von Gerechtigkeit,", "tokens": ["Voll", "von", "Ge\u00b7rech\u00b7tig\u00b7keit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "Und hat des Triebes R\u00e4nken", "tokens": ["Und", "hat", "des", "Trie\u00b7bes", "R\u00e4n\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Den Richtplatz mit Geschenken", "tokens": ["Den", "Richt\u00b7platz", "mit", "Ge\u00b7schen\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der Gnade \u00fcberstreut.", "tokens": ["Der", "Gna\u00b7de", "\u00fc\u00b7bers\u00b7treut", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Ei sag einmal du Liebe,", "tokens": ["Ei", "sag", "ein\u00b7mal", "du", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "PPER", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Du himmelklare Liebe", "tokens": ["Du", "him\u00b7mel\u00b7kla\u00b7re", "Lie\u00b7be"], "token_info": ["word", "word", "word"], "pos": ["PPER", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wer hat dich das gelehrt,", "tokens": ["Wer", "hat", "dich", "das", "ge\u00b7lehrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "PDS", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Da\u00df man dem blinden Triebe", "tokens": ["Da\u00df", "man", "dem", "blin\u00b7den", "Trie\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "F\u00fcr scharfe Dornenhiebe", "tokens": ["F\u00fcr", "schar\u00b7fe", "Dor\u00b7nen\u00b7hie\u00b7be"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Nur Rosenglut beschert.", "tokens": ["Nur", "Ro\u00b7seng\u00b7lut", "be\u00b7schert", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Und da\u00df man f\u00fcr die Rute", "tokens": ["Und", "da\u00df", "man", "f\u00fcr", "die", "Ru\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PIS", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dem blinden \u00dcbermute", "tokens": ["Dem", "blin\u00b7den", "\u00dc\u00b7ber\u00b7mu\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Nur s\u00fc\u00dfen Zucker giebt", "tokens": ["Nur", "s\u00fc\u00b7\u00dfen", "Zu\u00b7cker", "giebt"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Das lehrte mich der Gute", "tokens": ["Das", "lehr\u00b7te", "mich", "der", "Gu\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PRF", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der mich mit seinem Blute", "tokens": ["Der", "mich", "mit", "sei\u00b7nem", "Blu\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ob meiner Schuld geliebt.", "tokens": ["Ob", "mei\u00b7ner", "Schuld", "ge\u00b7liebt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Da sang einmal der Liebe", "tokens": ["Da", "sang", "ein\u00b7mal", "der", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der klaren Himmelsliebe", "tokens": ["Der", "kla\u00b7ren", "Him\u00b7mels\u00b7lie\u00b7be"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der Trieb dies Liebeslied,", "tokens": ["Der", "Trieb", "dies", "Lie\u00b7bes\u00b7lied", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PDS", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Da\u00df sich dem blinden Triebe", "tokens": ["Da\u00df", "sich", "dem", "blin\u00b7den", "Trie\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ein Licht ins Aug' er schriebe,", "tokens": ["Ein", "Licht", "ins", "Aug'", "er", "schrie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Da\u00df er im Auge sieht.", "tokens": ["Da\u00df", "er", "im", "Au\u00b7ge", "sieht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Und als sie es geh\u00f6ret,", "tokens": ["Und", "als", "sie", "es", "ge\u00b7h\u00f6\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da f\u00fchlt sie sich erh\u00f6ret,", "tokens": ["Da", "f\u00fchlt", "sie", "sich", "er\u00b7h\u00f6\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und sprach in Demut fromm", "tokens": ["Und", "sprach", "in", "De\u00b7mut", "fromm"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "NN", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Wer hat mich dir gelehret", "tokens": ["Wer", "hat", "mich", "dir", "ge\u00b7leh\u00b7ret"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "PPER", "PPER", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wer hat dich mir bescheret,", "tokens": ["Wer", "hat", "dich", "mir", "be\u00b7sche\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Trieb sei der Lieb willkomm.", "tokens": ["Trieb", "sei", "der", "Lieb", "will\u00b7komm", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Da fa\u00dft einmal die Liebe", "tokens": ["Da", "fa\u00dft", "ein\u00b7mal", "die", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die himmelklare Liebe", "tokens": ["Die", "him\u00b7mel\u00b7kla\u00b7re", "Lie\u00b7be"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sich einen frischen Mut,", "tokens": ["Sich", "ei\u00b7nen", "fri\u00b7schen", "Mut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und war dem blinden Triebe", "tokens": ["Und", "war", "dem", "blin\u00b7den", "Trie\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Da\u00df er nicht irrend bliebe", "tokens": ["Da\u00df", "er", "nicht", "ir\u00b7rend", "blie\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PTKNEG", "VVPP", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ein Blinde