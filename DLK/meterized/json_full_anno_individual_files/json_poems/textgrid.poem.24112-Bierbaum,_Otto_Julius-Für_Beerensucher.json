{"textgrid.poem.24112": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "F\u00fcr Beerensucher", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gingen zwei in einen Beerenwald;", "tokens": ["Gin\u00b7gen", "zwei", "in", "ei\u00b7nen", "Bee\u00b7ren\u00b7wald", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "CARD", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Fand der Eine s\u00fc\u00dfe Beeren bald;", "tokens": ["Fand", "der", "Ei\u00b7ne", "s\u00fc\u00b7\u00dfe", "Bee\u00b7ren", "bald", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ART", "ADJA", "NN", "ADV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Hat sich flei\u00dfig geb\u00fcckt", "tokens": ["Hat", "sich", "flei\u00b7\u00dfig", "ge\u00b7b\u00fcckt"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "PRF", "ADJD", "VVPP"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Und emsig gepfl\u00fcckt;", "tokens": ["Und", "em\u00b7sig", "ge\u00b7pfl\u00fcckt", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVPP", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "That nichts als essen.", "tokens": ["That", "nichts", "als", "es\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PIS", "KOKOM", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Der Andre indessen", "tokens": ["Der", "And\u00b7re", "in\u00b7des\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["ART", "PIS", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Trug immer die Nase gen