{"textgrid.poem.39645": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Ballade vom Raube der Sabinerinnen und von der neuentdeckten Stadt Quirium", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "H\u00f6rt, ihr Quiriten, insgesammt", "tokens": ["H\u00f6rt", ",", "ihr", "Qui\u00b7ri\u00b7ten", ",", "ins\u00b7ge\u00b7sammt"], "token_info": ["word", "punct", "word", "word", "punct", "word"], "pos": ["VVFIN", "$,", "PPOSAT", "NN", "$,", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die wunderneue M\u00e4re!", "tokens": ["Die", "wun\u00b7der\u00b7neu\u00b7e", "M\u00e4\u00b7re", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Von wannen euer Name stammt,", "tokens": ["Von", "wan\u00b7nen", "eu\u00b7er", "Na\u00b7me", "stammt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das ist's, was ich euch lehre.", "tokens": ["Das", "ist's", ",", "was", "ich", "euch", "leh\u00b7re", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "$,", "PWS", "PPER", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Von Quirium! von Quirium!", "tokens": ["Von", "Qui\u00b7ri\u00b7um", "!", "von", "Qui\u00b7ri\u00b7um", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NE", "$.", "APPR", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Trallirum larum lirium!", "tokens": ["Tral\u00b7li\u00b7rum", "la\u00b7rum", "li\u00b7ri\u00b7um", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.2": {"line.1": {"text": "Von Cures im Sabinerland", "tokens": ["Von", "Cu\u00b7res", "im", "Sa\u00b7bi\u00b7ner\u00b7land"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NE", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Lie\u00df man Quiriten kommen.", "tokens": ["Lie\u00df", "man", "Qui\u00b7ri\u00b7ten", "kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "NN", "VVINF", "$."], "meter": "+----+-", "measure": "dactylic.init"}, "line.3": {"text": "Doch das ist nur ein alter Tand:", "tokens": ["Doch", "das", "ist", "nur", "ein", "al\u00b7ter", "Tand", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Jetzt hab ich's ba\u00df vernommen:", "tokens": ["Jetzt", "hab", "ich's", "ba\u00df", "ver\u00b7nom\u00b7men", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "ADV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Von Quirium! von Quirium!", "tokens": ["Von", "Qui\u00b7ri\u00b7um", "!", "von", "Qui\u00b7ri\u00b7um", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NE", "$.", "APPR", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Trallirum larum lirium!", "tokens": ["Tral\u00b7li\u00b7rum", "la\u00b7rum", "li\u00b7ri\u00b7um", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.3": {"line.1": {"text": "Dort auf dem Capitolium,", "tokens": ["Dort", "auf", "dem", "Ca\u00b7pi\u00b7to\u00b7li\u00b7um", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da lag das feine St\u00e4dtchen.", "tokens": ["Da", "lag", "das", "fei\u00b7ne", "St\u00e4dt\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Leute waren nicht so dumm,", "tokens": ["Die", "Leu\u00b7te", "wa\u00b7ren", "nicht", "so", "dumm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKNEG", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sie hatten h\u00fcbsche M\u00e4dchen", "tokens": ["Sie", "hat\u00b7ten", "h\u00fcb\u00b7sche", "M\u00e4d\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "In Quirium! in Quirium!", "tokens": ["In", "Qui\u00b7ri\u00b7um", "!", "in", "Qui\u00b7ri\u00b7um", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NE", "$.", "APPR", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Trallirum larum lirium!", "tokens": ["Tral\u00b7li\u00b7rum", "la\u00b7rum", "li\u00b7ri\u00b7um", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.4": {"line.1": {"text": "Da wurde noch nicht Jovis Dom", "tokens": ["Da", "wur\u00b7de", "noch", "nicht", "Jo\u00b7vis", "Dom"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "PTKNEG", "NE", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In voller Pracht geschauet.", "tokens": ["In", "vol\u00b7ler", "Pracht", "ge\u00b7schau\u00b7et", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Es hatte Romulus sein Rom", "tokens": ["Es", "hat\u00b7te", "Ro\u00b7mu\u00b7lus", "sein", "Rom"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "NE", "PPOSAT", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Am Palatin erbauet,", "tokens": ["Am", "Pa\u00b7la\u00b7tin", "er\u00b7bau\u00b7et", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.5": {"text": "Bei Quirium! bei Quirium!", "tokens": ["Bei", "Qui\u00b7ri\u00b7um", "!", "bei", "Qui\u00b7ri\u00b7um", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NE", "$.", "APPR", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Trallirum larum lirium!", "tokens": ["Tral\u00b7li\u00b7rum", "la\u00b7rum", "li\u00b7ri\u00b7um", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.5": {"line.1": {"text": "Doch wohnten am Palatium", "tokens": ["Doch", "wohn\u00b7ten", "am", "Pa\u00b7la\u00b7ti\u00b7um"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Fast lauter Galgenschwengel,", "tokens": ["Fast", "lau\u00b7ter", "Gal\u00b7gen\u00b7schwen\u00b7gel", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und kein Sabiner wollte drum", "tokens": ["Und", "kein", "Sa\u00b7bi\u00b7ner", "woll\u00b7te", "drum"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "VMFIN", "PAV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zum Eidam solchen Bengel", "tokens": ["Zum", "Ei\u00b7dam", "sol\u00b7chen", "Ben\u00b7gel"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "In Quirium! in Quirium!", "tokens": ["In", "Qui\u00b7ri\u00b7um", "!", "in", "Qui\u00b7ri\u00b7um", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NE", "$.", "APPR", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Trallirum larum lirium!", "tokens": ["Tral\u00b7li\u00b7rum", "la\u00b7rum", "li\u00b7ri\u00b7um", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.6": {"line.1": {"text": "Der Romulus, an Listen reich,", "tokens": ["Der", "Ro\u00b7mu\u00b7lus", ",", "an", "Lis\u00b7ten", "reich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Verk\u00fcndigt ein Spektakel:", "tokens": ["Ver\u00b7k\u00fcn\u00b7digt", "ein", "Spek\u00b7ta\u00b7kel", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da kamen die Sabiner gleich,", "tokens": ["Da", "ka\u00b7men", "die", "Sa\u00b7bi\u00b7ner", "gleich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zu schauen das Mirakel,", "tokens": ["Zu", "schau\u00b7en", "das", "Mi\u00b7ra\u00b7kel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Aus Quirium! aus Quirium!", "tokens": ["Aus", "Qui\u00b7ri\u00b7um", "!", "aus", "Qui\u00b7ri\u00b7um", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NE", "$.", "APPR", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Trallirum larum lirium!", "tokens": ["Tral\u00b7li\u00b7rum", "la\u00b7rum", "li\u00b7ri\u00b7um", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.7": {"line.1": {"text": "Die R\u00f6mer hatten bald gepackt", "tokens": ["Die", "R\u00f6\u00b7mer", "hat\u00b7ten", "bald", "ge\u00b7packt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADV", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der M\u00e4dchen R\u00f6ck' und Mieder.", "tokens": ["Der", "M\u00e4d\u00b7chen", "R\u00f6ck'", "und", "Mie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Beim Str\u00e4uben wurde manche nackt;", "tokens": ["Beim", "Str\u00e4u\u00b7ben", "wur\u00b7de", "man\u00b7che", "nackt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PIS", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sie schrie'n und wollten wieder", "tokens": ["Sie", "schrie'n", "und", "woll\u00b7ten", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VMFIN", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Nach Quirium! nach Quirium!", "tokens": ["Nach", "Qui\u00b7ri\u00b7um", "!", "nach", "Qui\u00b7ri\u00b7um", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NE", "$.", "APPR", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Trallirum larum lirium.", "tokens": ["Tral\u00b7li\u00b7rum", "la\u00b7rum", "li\u00b7ri\u00b7um", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.8": {"line.1": {"text": "Es half nicht, schrie'n sie noch so laut:", "tokens": ["Es", "half", "nicht", ",", "schrie'n", "sie", "noch", "so", "laut", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "VVFIN", "PPER", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Man schleppte sie nach Hause,", "tokens": ["Man", "schlepp\u00b7te", "sie", "nach", "Hau\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und jeder schlief bei seiner Braut", "tokens": ["Und", "je\u00b7der", "schlief", "bei", "sei\u00b7ner", "Braut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nach lust'gem Hochzeitschmause,", "tokens": ["Nach", "lust'\u00b7gem", "Hoch\u00b7zeit\u00b7schmau\u00b7se", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "---+-+-", "measure": "unknown.measure.di"}, "line.5": {"text": "Aus Quirium! aus Quirium!", "tokens": ["Aus", "Qui\u00b7ri\u00b7um", "!", "aus", "Qui\u00b7ri\u00b7um", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NE", "$.", "APPR", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Trallirum larum lirium!", "tokens": ["Tral\u00b7li\u00b7rum", "la\