{"dta.poem.11521": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Sch\u00f6n-Rohtraut.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905193969", "language": ["de:0.99"], "booktitle": "M\u00f6rike, Eduard: Gedichte. Stuttgart, 1838."}, "poem": {"stanza.1": {"line.1": {"text": "Rohtraut, Sch\u00f6n-Rohtraut.", "tokens": ["Roh\u00b7traut", ",", "Sch\u00f6n\u00b7Rohtraut", "."], "token_info": ["word", "punct", "word", "punct"], "pos": ["ADJD", "$,", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Was thut sie denn den ganzen Tag,", "tokens": ["Was", "thut", "sie", "denn", "den", "gan\u00b7zen", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da sie wohl nicht spinnen und n\u00e4hen mag?", "tokens": ["Da", "sie", "wohl", "nicht", "spin\u00b7nen", "und", "n\u00e4\u00b7hen", "mag", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PTKNEG", "VVINF", "KON", "VVINF", "VMFIN", "$."], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Thut fischen und jagen.", "tokens": ["Thut", "fi\u00b7schen", "und", "ja\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVINF", "KON", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "O da\u00df ich doch ihr J\u00e4ger w\u00e4r'!", "tokens": ["O", "da\u00df", "ich", "doch", "ihr", "J\u00e4\u00b7ger", "w\u00e4r'", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOUS", "PPER", "ADV", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Fischen und jagen freute mich sehr.", "tokens": ["Fi\u00b7schen", "und", "ja\u00b7gen", "freu\u00b7te", "mich", "sehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVINF", "VVFIN", "PPER", "ADV", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.7": {"text": "\u2014 Schweig' stille, mein Herze!", "tokens": ["Schweig'", "stil\u00b7le", ",", "mein", "Her\u00b7ze", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "NN", "VVFIN", "$,", "PPOSAT", "VVFIN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.2": {"line.1": {"text": "Und \u00fcber eine kleine Weil',", "tokens": ["Und", "\u00fc\u00b7ber", "ei\u00b7ne", "klei\u00b7ne", "Weil'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Rohtraut, Sch\u00f6n-Rohtraut,", "tokens": ["Roh\u00b7traut", ",", "Sch\u00f6n\u00b7Rohtraut", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["ADJD", "$,", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "So dient der Knab' auf Ringangs Schlo\u00df", "tokens": ["So", "dient", "der", "Knab'", "auf", "Rin\u00b7gangs", "Schlo\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In J\u00e4gertracht und hat ein Ro\u00df,", "tokens": ["In", "J\u00e4\u00b7ger\u00b7tracht", "und", "hat", "ein", "Ro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mit Rohtraut zu jagen.", "tokens": ["Mit", "Roh\u00b7traut", "zu", "ja\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "O da\u00df ich doch ein K\u00f6nigssohn w\u00e4r'!", "tokens": ["O", "da\u00df", "ich", "doch", "ein", "K\u00f6\u00b7nigs\u00b7sohn", "w\u00e4r'", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOUS", "PPER", "ADV", "ART", "NN", "VAFIN", "$."], "meter": "+--+-+---", "measure": "iambic.tri.invert"}, "line.7": {"text": "Rohtraut, Sch\u00f6n-Rohtraut lieb' ich so sehr.", "tokens": ["Roh\u00b7traut", ",", "Sch\u00f6n\u00b7Rohtraut", "lieb'", "ich", "so", "sehr", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "NN", "VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "\u2014 Schweig' stille, mein Herze!", "tokens": ["Schweig'", "stil\u00b7le", ",", "mein", "Her\u00b7ze", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "NN", "VVFIN", "$,", "PPOSAT", "VVFIN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.3": {"line.1": {"text": "Einsmals sie ruhten am Eichenbaum,", "tokens": ["Eins\u00b7mals", "sie", "ruh\u00b7ten", "am", "Ei\u00b7chen\u00b7baum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Da lacht Sch\u00f6n-Rohtraut:", "tokens": ["Da", "lacht", "Sch\u00f6n\u00b7Rohtraut", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Was siehst mich an so wunniglich?", "tokens": ["Was", "siehst", "mich", "an", "so", "wun\u00b7nig\u00b7lich", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PRF", "APPR", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn du das Herz hast, k\u00fcsse mich!", "tokens": ["Wenn", "du", "das", "Herz", "hast", ",", "k\u00fcs\u00b7se", "mich", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VAFIN", "$,", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ach! erschrak der Knabe!", "tokens": ["Ach", "!", "er\u00b7schrak", "der", "Kna\u00b7be", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Doch denket er: mir ist's vergunnt,", "tokens": ["Doch", "den\u00b7ket", "er", ":", "mir", "ist's", "ver\u00b7gunnt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$.", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und k\u00fcsset Sch\u00f6n-Rohtraut auf den Mund.", "tokens": ["Und", "k\u00fcs\u00b7set", "Sch\u00f6n\u00b7Rohtraut", "auf", "den", "Mund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "\u2014 Schweig' stille, mein Herze!", "tokens": ["Schweig'", "stil\u00b7le", ",", "mein", "Her\u00b7ze", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "NN", "VVFIN", "$,", "PPOSAT", "VVFIN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.4": {"line.1": {"text": "Darauf sie ritten schweigend heim,", "tokens": ["Da\u00b7rauf", "sie", "rit\u00b7ten", "schwei\u00b7gend", "heim", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "VVFIN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Rohtraut, Sch\u00f6n-Rohtraut;", "tokens": ["Roh\u00b7traut", ",", "Sch\u00f6n\u00b7Rohtraut", ";"], "token_info": ["word", "punct", "word", "punct"], "pos": ["ADJD", "$,", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Es jauchzt der Knab' in seinem Sinn:", "tokens": ["Es", "jauchzt", "der", "Knab'", "in", "sei\u00b7nem", "Sinn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und w\u00fcrd'st du heute Kaiserin,", "tokens": ["Und", "w\u00fcrd'st", "du", "heu\u00b7te", "Kai\u00b7se\u00b7rin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mich sollt's nicht kr\u00e4nken:", "tokens": ["Mich", "sollt's", "nicht", "kr\u00e4n\u00b7ken", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Ihr tausend Bl\u00e4tter im Walde wi\u00dft,", "tokens": ["Ihr", "tau\u00b7send", "Bl\u00e4t\u00b7ter", "im", "Wal\u00b7de", "wi\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "CARD", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Ich hab' Sch\u00f6n-Rohtrauts Mund gek\u00fc\u00dft!", "tokens": ["Ich", "hab'", "Sch\u00f6n\u00b7Rohtrauts", "Mund", "ge\u00b7k\u00fc\u00dft", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "NN", "VVPP", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.8": {"text": "\u2014 Schweig' stille, mein Herze!", "tokens": ["Schweig'", "stil\u00b7le", ",", "mein", "Her\u00b7ze", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "NN", "VVFIN", "$,", "PPOSAT", "VVFIN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}}}}