{"textgrid.poem.38819": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Froh und lustig zwischen Steinen", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Froh und lustig zwischen Steinen", "tokens": ["Froh", "und", "lus\u00b7tig", "zwi\u00b7schen", "Stei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "KON", "ADJD", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Geht der J\u00fcngling auf die Jagd,", "tokens": ["Geht", "der", "J\u00fcng\u00b7ling", "auf", "die", "Jagd", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Seine Beute mu\u00df erscheinen", "tokens": ["Sei\u00b7ne", "Beu\u00b7te", "mu\u00df", "er\u00b7schei\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VMFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In den gr\u00fcnlebendgen Hainen,", "tokens": ["In", "den", "gr\u00fcn\u00b7le\u00b7bend\u00b7gen", "Hai\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Sucht' er auch bis in die Nacht.", "tokens": ["Sucht'", "er", "auch", "bis", "in", "die", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Seine treuen Hunde bellen", "tokens": ["Sei\u00b7ne", "treu\u00b7en", "Hun\u00b7de", "bel\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["P