{"dta.poem.10901": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "No.  5.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1740", "urn": "urn:nbn:de:kobv:b4-200905198572", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Zwischen schroff-und j\u00e4hen Felsen, h\u00f6r ich das Gew\u00e4sser rau-\nschen,", "tokens": ["Zwi\u00b7schen", "schroff\u00b7\u00b7und", "j\u00e4\u00b7hen", "Fel\u00b7sen", ",", "h\u00f6r", "ich", "das", "Ge\u00b7w\u00e4s\u00b7ser", "rau", "schen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ADJA", "NN", "$,", "VVFIN", "PPER", "ART", "NN", "TRUNC", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Seh ich einen muntern Hirsch, hier aufmerksam stehn und", "tokens": ["Seh", "ich", "ei\u00b7nen", "mun\u00b7tern", "Hirsch", ",", "hier", "auf\u00b7merk\u00b7sam", "stehn", "und"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$,", "ADV", "ADJD", "VVINF", "KON"], "meter": "+-+-+-+-+--+-", "measure": "hexameter"}, "line.3": {"text": "Voller innerlichen Gluht, voller br\u00fcnstigen Begier,", "tokens": ["Vol\u00b7ler", "in\u00b7ner\u00b7li\u00b7chen", "Gluht", ",", "vol\u00b7ler", "br\u00fcns\u00b7ti\u00b7gen", "Be\u00b7gier", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Ob er nirgend Wild versp\u00fcr.", "tokens": ["Ob", "er", "nir\u00b7gend", "Wild", "ver\u00b7sp\u00fcr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "NE", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Kaum getraut er seinen Lauft fest zu setzen, um im h\u00f6ren,", "tokens": ["Kaum", "ge\u00b7traut", "er", "sei\u00b7nen", "Lauft", "fest", "zu", "set\u00b7zen", ",", "um", "im", "h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "PPER", "PPOSAT", "NN", "ADJD", "PTKZU", "VVINF", "$,", "KOUI", "APPRART", "VVINF", "$,"], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.6": {"text": "Sich, durch ungef\u00e4hres Rascheln, unvorsichtig nicht zu st\u00f6ren.", "tokens": ["Sich", ",", "durch", "un\u00b7ge\u00b7f\u00e4h\u00b7res", "Ra\u00b7scheln", ",", "un\u00b7vor\u00b7sich\u00b7tig", "nicht", "zu", "st\u00f6\u00b7ren", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PRF", "$,", "APPR", "ADJA", "NN", "$,", "ADJD", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.7": {"text": "Sehet, wie so scharf er sieht! h\u00f6rt, wie stark und laut er schreyt!", "tokens": ["Se\u00b7het", ",", "wie", "so", "scharf", "er", "sieht", "!", "h\u00f6rt", ",", "wie", "stark", "und", "laut", "er", "schreyt", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "ADV", "VVFIN", "PPER", "VVFIN", "$.", "VVFIN", "$,", "PWAV", "ADJD", "KON", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.8": {"text": "Der so sch\u00f6n - als wilden Gegend deutliche Nat\u00fcrlichkeit,", "tokens": ["Der", "so", "sch\u00f6n", "als", "wil\u00b7den", "Ge\u00b7gend", "deut\u00b7li\u00b7che", "Na\u00b7t\u00fcr\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "$(", "KOUS", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+--+", "measure": "iambic.octa.plus.octa.plus.chol"}, "line.9": {"text": "Wo auch das, was schrecket, sch\u00f6n ist, was verwildert, angenehm,", "tokens": ["Wo", "auch", "das", ",", "was", "schre\u00b7cket", ",", "sch\u00f6n", "ist", ",", "was", "ver\u00b7wil\u00b7dert", ",", "an\u00b7ge\u00b7nehm", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "ADV", "PDS", "$,", "PWS", "VVFIN", "$,", "ADJD", "VAFIN", "$,", "PRELS", "VVPP", "$,", "ADJD", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.10": {"text": "Und die wir, in unserm Zimmer, jetzt durch Ridinger bequem,", "tokens": ["Und", "die", "wir", ",", "in", "un\u00b7serm", "Zim\u00b7mer", ",", "jetzt", "durch", "Ri\u00b7din\u00b7ger", "be\u00b7quem", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PPER", "$,", "APPR", "PPOSAT", "NN", "$,", "ADV", "APPR", "NN", "ADJD", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.11": {"text": "Sonder steigen, sehen k\u00f6nnen, bringt uns billig auf die Spur", "tokens": ["Son\u00b7der", "stei\u00b7gen", ",", "se\u00b7hen", "k\u00f6n\u00b7nen", ",", "bringt", "uns", "bil\u00b7lig", "auf", "die", "Spur"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVINF", "$,", "VVINF", "VMINF", "$,", "VVFIN", "PPER", "ADJD", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.12": {"text": "Des hier vorgestellten Urbilds, auf die wirkliche Natur.", "tokens": ["Des", "hier", "vor\u00b7ge\u00b7stell\u00b7ten", "Ur\u00b7bilds", ",", "auf", "die", "wirk\u00b7li\u00b7che", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "NN", "$,", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.13": {"text": "Da wir f\u00fcrs Original, die Gesch\u00f6pfe, leider blind,", "tokens": ["Da", "wir", "f\u00fcrs", "O\u00b7rig\u00b7i\u00b7nal", ",", "die", "Ge\u00b7sch\u00f6p\u00b7fe", ",", "lei\u00b7der", "blind", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "$,", "ART", "NN", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.14": {"text": "Durch Gewohnheit, worden sind:", "tokens": ["Durch", "Ge\u00b7wohn\u00b7heit", ",", "wor\u00b7den", "sind", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "VAPP", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.15": {"text": "Sucht er vom Gewohnheits - Schlaf, uns, durch Zeichnen, zu", "tokens": ["Sucht", "er", "vom", "Ge\u00b7wohn\u00b7heits", "Schlaf", ",", "uns", ",", "durch", "Zeich\u00b7nen", ",", "zu"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "$(", "NN", "$,", "PPER", "$,", "APPR", "NN", "$,", "APPR"], "meter": "----+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.16": {"text": "Und, durch die Copie, im Urbild, selbst den Sch\u00f6pfer zu", "tokens": ["Und", ",", "durch", "die", "Co\u00b7pie", ",", "im", "Ur\u00b7bild", ",", "selbst", "den", "Sch\u00f6p\u00b7fer", "zu"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "$,", "APPR", "ART", "NN", "$,", "APPRART", "NN", "$,", "ADV", "ART", "NN", "PTKZU"], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.17": {"text": "Also seh ich seine Kunst, ja ihn, als ein Werkzeug an,", "tokens": ["Al\u00b7so", "seh", "ich", "sei\u00b7ne", "Kunst", ",", "ja", "ihn", ",", "als", "ein", "Werk\u00b7zeug", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$,", "ADV", "PPER", "$,", "KOUS", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.18": {"text": "Wodurch man sich, im Gesch\u00f6pf, selbst zum Sch\u00f6pfer nahen", "tokens": ["Wo\u00b7durch", "man", "sich", ",", "im", "Ge\u00b7sch\u00f6pf", ",", "selbst", "zum", "Sch\u00f6p\u00b7fer", "na\u00b7hen"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "PIS", "PRF", "$,", "APPRART", "NN", "$,", "ADV", "APPRART", "NN", "ADJA"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}}}}}