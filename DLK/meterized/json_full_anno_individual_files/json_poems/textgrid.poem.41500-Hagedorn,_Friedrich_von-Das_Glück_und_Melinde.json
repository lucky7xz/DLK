{"textgrid.poem.41500": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Das Gl\u00fcck und Melinde", "genre": "verse", "period": "N.A.", "pub_year": 1731, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich sahe j\u00fcngst das Gl\u00fcck, und durft' ihm k\u00fchnlich sagen:", "tokens": ["Ich", "sa\u00b7he", "j\u00fcngst", "das", "Gl\u00fcck", ",", "und", "durft'", "ihm", "k\u00fchn\u00b7lich", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "$,", "KON", "VMFIN", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bereue deinen falschen Tand;", "tokens": ["Be\u00b7re\u00b7ue", "dei\u00b7nen", "fal\u00b7schen", "Tand", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dein flatterhafter Unbestand", "tokens": ["Dein", "flat\u00b7ter\u00b7haf\u00b7ter", "Un\u00b7be\u00b7stand"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Berechtigt alle Welt zu klagen.", "tokens": ["Be\u00b7rech\u00b7tigt", "al\u00b7le", "Welt", "zu", "kla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Was du am Morgen kaum verliehn,", "tokens": ["Was", "du", "am", "Mor\u00b7gen", "kaum", "ver\u00b7liehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPRART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Darfst du am Abend schon entziehn.", "tokens": ["Darfst", "du", "am", "A\u00b7bend", "schon", "ent\u00b7ziehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPRART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Das Gl\u00fcck versetzte mir: Wie kurz ist aller Leben!", "tokens": ["Das", "Gl\u00fcck", "ver\u00b7setz\u00b7te", "mir", ":", "Wie", "kurz", "ist", "al\u00b7ler", "Le\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$.", "PWAV", "ADJD", "VAFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Unendlich ist der G\u00fcter Wahl,", "tokens": ["Un\u00b7end\u00b7lich", "ist", "der", "G\u00fc\u00b7ter", "Wahl", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Unendlich meiner Sklaven Zahl:", "tokens": ["Un\u00b7end\u00b7lich", "mei\u00b7ner", "Skla\u00b7ven", "Zahl", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sollt' ich nicht jedem etwas geben?", "tokens": ["Sollt'", "ich", "nicht", "je\u00b7dem", "et\u00b7was", "ge\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "PIAT", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Dient, was ich einem nehmen mu\u00df,", "tokens": ["Dient", ",", "was", "ich", "ei\u00b7nem", "neh\u00b7men", "mu\u00df", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWS", "PPER", "ART", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Nicht gleich dem andern zum Genu\u00df?", "tokens": ["Nicht", "gleich", "dem", "an\u00b7dern", "zum", "Ge\u00b7nu\u00df", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ART", "ADJA", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ich wandte mich darauf zur scherzenden Melinde,", "tokens": ["Ich", "wand\u00b7te", "mich", "da\u00b7rauf", "zur", "scher\u00b7zen\u00b7den", "Me\u00b7lin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PAV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und sprach: Dem Gl\u00fcck steh' alles frei!", "tokens": ["Und", "sprach", ":", "Dem", "Gl\u00fcck", "steh'", "al\u00b7les", "frei", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "ART", "NN", "VVFIN", "PIS", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn ich nur dich, mein Kind, getreu", "tokens": ["Wenn", "ich", "nur", "dich", ",", "mein", "Kind", ",", "ge\u00b7treu"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["KOUS", "PPER", "ADV", "PPER", "$,", "PPOSAT", "NN", "$,", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und mir so hold als sch\u00f6n befinde,", "tokens": ["Und", "mir", "so", "hold", "als", "sch\u00f6n", "be\u00b7fin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "ADJD", "KOKOM", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und wenn dein Mund, der mich erg\u00f6tzt,", "tokens": ["Und", "wenn",