{"dta.poem.23618": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Classische Gelahrtheit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1840", "urn": "urn:nbn:de:kobv:b4-200905192626", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ja, es war in jenen Tagen", "tokens": ["Ja", ",", "es", "war", "in", "je\u00b7nen", "Ta\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "PPER", "VAFIN", "APPR", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Liebe f\u00fcr das Vaterland:", "tokens": ["Lie\u00b7be", "f\u00fcr", "das", "Va\u00b7ter\u00b7land", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie sich Sparta hat geschlagen,", "tokens": ["Wie", "sich", "Spar\u00b7ta", "hat", "ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "NE", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Macht Thermopyl\u00e4 bekannt.", "tokens": ["Macht", "Ther\u00b7mo\u00b7py\u00b7l\u00e4", "be\u00b7kannt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NE", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Lebt es doch in Aller Munde", "tokens": ["Lebt", "es", "doch", "in", "Al\u00b7ler", "Mun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Was dereinst dies Sparta war,", "tokens": ["Was", "de\u00b7reinst", "dies", "Spar\u00b7ta", "war", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "PDS", "NE", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und es giebt uns sichre Kunde", "tokens": ["Und", "es", "giebt", "uns", "sich\u00b7re", "Kun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPER", "VVFIN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ein Tertianer ja sogar.", "tokens": ["Ein", "Ter\u00b7ti\u00b7a\u00b7ner", "ja", "so\u00b7gar", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Was bei Pforzheim ist geschehen,", "tokens": ["Was", "bei", "Pforz\u00b7heim", "ist", "ge\u00b7sche\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "NE", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Frag die Philologen drum,", "tokens": ["Frag", "die", "Phi\u00b7lo\u00b7lo\u00b7gen", "drum", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PAV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Gieb es ihnen selbst zum Lehen,", "tokens": ["Gieb", "es", "ih\u00b7nen", "selbst", "zum", "Le\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und \u2014 sie bleiben dennoch stumm.", "tokens": ["Und", "sie", "blei\u00b7ben", "den\u00b7noch", "stumm", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$(", "PPER", "VVFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}