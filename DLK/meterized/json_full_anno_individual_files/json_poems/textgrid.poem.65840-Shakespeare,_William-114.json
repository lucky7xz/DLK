{"textgrid.poem.65840": {"metadata": {"author": {"name": "Shakespeare, William", "birth": "N.A.", "death": "N.A."}, "title": "114", "genre": "verse", "period": "N.A.", "pub_year": 1590, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Obwohl mein Sinn, mit ", "tokens": ["Ob\u00b7wohl", "mein", "Sinn", ",", "mit"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["KOUS", "PPOSAT", "NN", "$,", "APPR"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Und K\u00f6nigsgift der Schmeichelei sich n\u00e4hrt?", "tokens": ["Und", "K\u00f6\u00b7nigs\u00b7gift", "der", "Schmei\u00b7che\u00b7lei", "sich", "n\u00e4hrt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ART", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wie? oder sagt die Wahrheit mir mein Auge,", "tokens": ["Wie", "?", "o\u00b7der", "sagt", "die", "Wahr\u00b7heit", "mir", "mein", "Au\u00b7ge", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "KON", "VVFIN", "ART", "NN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dem solche Alchimie dein Lieben lehrt,", "tokens": ["Dem", "sol\u00b7che", "Al\u00b7chi\u00b7mie", "dein", "Lie\u00b7ben", "lehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "PPOSAT", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Da\u00df es die mi\u00dfgeschaffnen rohen Dinge", "tokens": ["Da\u00df", "es", "die", "mi\u00df\u00b7ge\u00b7schaff\u00b7nen", "ro\u00b7hen", "Din\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Zu Cherubinen, die dir gleichen, schafft,", "tokens": ["Zu", "Che\u00b7ru\u00b7bi\u00b7nen", ",", "die", "dir", "glei\u00b7chen", ",", "schafft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "NE", "$,", "PRELS", "PPER", "VVINF", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Zum Kleinod wandelt jegliches Geringe,", "tokens": ["Zum", "Klei\u00b7nod", "wan\u00b7delt", "jeg\u00b7li\u00b7ches", "Ge\u00b7rin\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Wie es begegnet seines Strahles Kraft?", "tokens": ["Wie", "es", "be\u00b7geg\u00b7net", "sei\u00b7nes", "Strah\u00b7les", "Kraft", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "O jenes ist's, ist Augenschmeichelei!", "tokens": ["O", "je\u00b7nes", "ist's", ",", "ist", "Au\u00b7gen\u00b7schmei\u00b7che\u00b7lei", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "PDS", "VAFIN", "$,", "VAFIN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Die saugt h\u00f6chst k\u00f6niglich mein gro\u00dfer Sinn.", "tokens": ["Die", "saugt", "h\u00f6chst", "k\u00f6\u00b7nig\u00b7lich", "mein", "gro\u00b7\u00dfer", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ADJD", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Wohl wei\u00df mein Auge, was dem lieblich sei,", "tokens": ["Wohl", "wei\u00df", "mein", "Au\u00b7ge", ",", "was", "dem", "lieb\u00b7lich", "sei", ",