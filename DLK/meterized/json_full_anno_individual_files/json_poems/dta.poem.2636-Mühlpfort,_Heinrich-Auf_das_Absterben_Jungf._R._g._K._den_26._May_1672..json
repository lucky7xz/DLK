{"dta.poem.2636": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Auf das Absterben/  \n Jungf. R. g. K. den 26. May 1672.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Dein' Au\u00dffahrt/ Seelige/ geschicht zu diesen Zeiten/", "tokens": ["Dein'", "Au\u00df\u00b7fahrt", "/", "See\u00b7li\u00b7ge", "/", "ge\u00b7schicht", "zu", "die\u00b7sen", "Zei\u00b7ten", "/"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "NN", "$(", "VVPP", "APPR", "PDAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da gleich dein Br\u00e4utigam/ dein Heiland/ Aufffahrt", "tokens": ["Da", "gleich", "dein", "Br\u00e4u\u00b7ti\u00b7gam", "/", "dein", "Hei\u00b7land", "/", "Auff\u00b7fahrt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ADV", "ADV", "PPOSAT", "NE", "$(", "PPOSAT", "NN", "$(", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Triumphirende rufft dich an seine Seiten/", "tokens": ["Der", "Tri\u00b7um\u00b7phi\u00b7ren\u00b7de", "rufft", "dich", "an", "sei\u00b7ne", "Sei\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+--+-++-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Umb mit ihm einzugehn des Himmels Ehren-Zelt.", "tokens": ["Umb", "mit", "ihm", "ein\u00b7zu\u00b7gehn", "des", "Him\u00b7mels", "Eh\u00b7ren\u00b7Zelt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "APPR", "PPER", "VVIZU", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So ist dein Freund doch treu bi\u00df in des Todes Schatten/", "tokens": ["So", "ist", "dein", "Freund", "doch", "treu", "bi\u00df", "in", "des", "To\u00b7des", "Schat\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "ADV", "ADJD", "APPR", "APPR", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Bi\u00df sich de\u00df Lebens Tag und Abend hat gek\u00fchlt/", "tokens": ["Bi\u00df", "sich", "de\u00df", "Le\u00b7bens", "Tag", "und", "A\u00b7bend", "hat", "ge\u00b7k\u00fchlt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "ART", "NN", "NN", "KON", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "So will er dein Gebet und Sehnen recht erstatten/", "tokens": ["So", "will", "er", "dein", "Ge\u00b7bet", "und", "Seh\u00b7nen", "recht", "er\u00b7stat\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPOSAT", "NN", "KON", "NN", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Da\u00df der erblaste Leib nun keine Qual mehr f\u00fchlt.", "tokens": ["Da\u00df", "der", "er\u00b7blas\u00b7te", "Leib", "nun", "kei\u00b7ne", "Qual", "mehr", "f\u00fchlt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "ADV", "PIAT", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Unsch\u00e4tzbarer Triumph! Weit \u00fcber das Gepr\u00e4nge/", "tokens": ["Un\u00b7sch\u00e4tz\u00b7ba\u00b7rer", "Tri\u00b7umph", "!", "Weit", "\u00fc\u00b7ber", "das", "Ge\u00b7pr\u00e4n\u00b7ge", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "NN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Worinn das stoltze Rom sein Sieges-Fest begieng/", "tokens": ["Wo\u00b7rinn", "das", "stolt\u00b7ze", "Rom", "sein", "Sie\u00b7ges\u00b7Fest", "be\u00b7gieng", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NE", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Da\u00df der bezwung\u2019nen St\u00e4dt\u2019 und L\u00e4nder Nahm\u2019 und M\u00e4nge/", "tokens": ["Da\u00df", "der", "be\u00b7zwung'\u00b7nen", "St\u00e4dt'", "und", "L\u00e4n\u00b7der", "Nahm'", "und", "M\u00e4n\u00b7ge", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "KON", "NN", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Kunst-artig abgemahlt an hohe S\u00e4ulen hieng/", "tokens": ["Kunst\u00b7ar\u00b7tig", "ab\u00b7ge\u00b7mahlt", "an", "ho\u00b7he", "S\u00e4u\u00b7len", "hieng", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Wenn sich der gantze Rath in weissen Kleidern zeigte/", "tokens": ["Wenn", "sich", "der", "gant\u00b7ze", "Rath", "in", "weis\u00b7sen", "Klei\u00b7dern", "zeig\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Der Uberwinder selbst in Cron und Scepter wie\u00df;", "tokens": ["Der", "U\u00b7ber\u00b7win\u00b7der", "selbst", "in", "Cron", "und", "Scep\u00b7ter", "wie\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Wenn das gefangne Volck sich zu der Erden neigte/", "tokens": ["Wenn", "das", "ge\u00b7fang\u00b7ne", "Volck", "sich", "zu", "der", "Er\u00b7den", "neig\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "PRF", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Ja vielmals ein Tyrann die F\u00fcrsten ziehen hie\u00df.", "tokens": ["Ja", "viel\u00b7mals", "ein", "Ty\u00b7rann", "die", "F\u00fcrs\u00b7ten", "zie\u00b7hen", "hie\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADV", "ART", "NN", "ART", "NN", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Es war nicht nur genug das Helffenbein zum Wagen/", "tokens": ["Es", "war", "nicht", "nur", "ge\u00b7nug", "das", "Helf\u00b7fen\u00b7bein", "zum", "Wa\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADV", "ADV", "ART", "NN", "APPRART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Wenn nicht ein Elefant denselben auch gef\u00fchrt/", "tokens": ["Wenn", "nicht", "ein", "E\u00b7le\u00b7fant", "den\u00b7sel\u00b7ben", "auch", "ge\u00b7f\u00fchrt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "ART", "NN", "PDS", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Und so ins Capitol den Siegenden getragen/", "tokens": ["Und", "so", "ins", "Ca\u00b7pi\u00b7tol", "den", "Sie\u00b7gen\u00b7den", "ge\u00b7tra\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "NN", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Worinn noch gr\u00f6sser Pracht/ und Hochmuth ward gesp\u00fcrt;", "tokens": ["Wo\u00b7rinn", "noch", "gr\u00f6s\u00b7ser", "Pracht", "/", "und", "Hoch\u00b7muth", "ward", "ge\u00b7sp\u00fcrt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADJA", "NN", "$(", "KON", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Man schlachte Menschen ab zum Opfer ihren G\u00f6ttern/", "tokens": ["Man", "schlach\u00b7te", "Men\u00b7schen", "ab", "zum", "Op\u00b7fer", "ih\u00b7ren", "G\u00f6t\u00b7tern", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "NN", "PTKVZ", "APPRART", "NN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Und gab gediegen Gold mit fettem Weyrauch hin/", "tokens": ["Und", "gab", "ge\u00b7die\u00b7gen", "Gold", "mit", "fet\u00b7tem", "Wey\u00b7rauch", "hin", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJA", "NN", "APPR", "ADJA", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Man legt\u2019 in ihren Scho\u00df den Krantz der Lorber-Bl\u00e4ttern/", "tokens": ["Man", "legt'", "in", "ih\u00b7ren", "Scho\u00df", "den", "Krantz", "der", "Lor\u00b7ber\u00b7Bl\u00e4t\u00b7tern", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "PPOSAT", "NN", "ART", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Und lie\u00df die gantze Stadt in Lust und Freuden bl\u00fchn:", "tokens": ["Und", "lie\u00df", "die", "gant\u00b7ze", "Stadt", "in", "Lust", "und", "Freu\u00b7den", "bl\u00fchn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "APPR", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Darbey ward auch das Volck der Knechte frey gelassen/", "tokens": ["Dar\u00b7bey", "ward", "auch", "das", "Volck", "der", "Knech\u00b7te", "frey", "ge\u00b7las\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "ADV", "ART", "NN", "ART", "NN", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Der Jungfern zarte Schaar stimm\u2019t Harff und Seiten an:", "tokens": ["Der", "Jung\u00b7fern", "zar\u00b7te", "Schaar", "stim\u00b7m't", "Harff", "und", "Sei\u00b7ten", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "NE", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.27": {"text": "Der Uberwinder leb! Erklang auf allen Gassen/", "tokens": ["Der", "U\u00b7ber\u00b7win\u00b7der", "leb", "!", "Er\u00b7klang", "auf", "al\u00b7len", "Gas\u00b7sen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "NN", "APPR", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Und so ward dem Triumph sein h\u00f6chstes Recht gethan.", "tokens": ["Und", "so", "ward", "dem", "Tri\u00b7umph", "sein", "h\u00f6chs\u00b7tes", "Recht", "ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "ART", "NN", "PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "--+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.29": {"text": "Allein vern irrter Traum! Ein P\u00fcnctlein von der Erden/", "tokens": ["Al\u00b7lein", "vern", "irr\u00b7ter", "Traum", "!", "Ein", "P\u00fcnct\u00b7lein", "von", "der", "Er\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADJA", "NN", "$.", "ART", "NN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Das mit ", "tokens": ["Das", "mit"], "token_info": ["word", "word"], "pos": ["PDS", "APPR"], "meter": "-+", "measure": "iambic.single"}, "line.31": {"text": "Gar in weit andrem Glantz und herrlichern Geberden/", "tokens": ["Gar", "in", "weit", "an\u00b7drem", "Glantz", "und", "herr\u00b7li\u00b7chern", "Ge\u00b7ber\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJD", "ADJA", "NN", "KON", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Hat unser Lebens-F\u00fcrst erwiesen seine Macht.", "tokens": ["Hat", "un\u00b7ser", "Le\u00b7bens\u00b7F\u00fcrst", "er\u00b7wie\u00b7sen", "sei\u00b7ne", "Macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Er hat zuvor den Feind der gantzen Welt bezwungen/", "tokens": ["Er", "hat", "zu\u00b7vor", "den", "Feind", "der", "gant\u00b7zen", "Welt", "be\u00b7zwun\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "NN", "ART", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Der H\u00f6llen Reich zerst\u00f6rt/ des Todes Zahn zermalmt/", "tokens": ["Der", "H\u00f6l\u00b7len", "Reich", "zer\u00b7st\u00f6rt", "/", "des", "To\u00b7des", "Zahn", "zer\u00b7malmt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVPP", "$(", "ART", "NN", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Bey seiner Auffahrt hat der Engel Mund gesungen/", "tokens": ["Bey", "sei\u00b7ner", "Auf\u00b7fahrt", "hat", "der", "En\u00b7gel", "Mund", "ge\u00b7sun\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "ART", "NN", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Und seine Heiligen Jhm nachgefolgt bepalmt.", "tokens": ["Und", "sei\u00b7ne", "Hei\u00b7li\u00b7gen", "Jhm", "nach\u00b7ge\u00b7folgt", "be\u00b7palmt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "PPER", "VVFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Wir S\u00fcnden-Knechte sind dadurch nun gantz befreyet/", "tokens": ["Wir", "S\u00fcn\u00b7den\u00b7Knech\u00b7te", "sind", "da\u00b7durch", "nun", "gantz", "be\u00b7fre\u00b7yet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "VAFIN", "PAV", "ADV", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.38": {"text": "Und sollen einst vor ihm in weissen Kleidern stehn.", "tokens": ["Und", "sol\u00b7len", "einst", "vor", "ihm", "in", "weis\u00b7sen", "Klei\u00b7dern", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "APPR", "PPER", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Das Seufftzen und Gebet/ so unser Hertz au\u00dfstreuet/", "tokens": ["Das", "Seufft\u00b7zen", "und", "Ge\u00b7bet", "/", "so", "un\u00b7ser", "Hertz", "au\u00df\u00b7streu\u00b7et", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "$(", "ADV", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Sind Opffer die zu ihm durch alle Wolcken gehn.", "tokens": ["Sind", "Opf\u00b7fer", "die", "zu", "ihm", "durch", "al\u00b7le", "Wol\u00b7cken", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "ART", "APPR", "PPER", "APPR", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Ach seliger Triumph! Der uns den Himmel giebet/", "tokens": ["Ach", "se\u00b7li\u00b7ger", "Tri\u00b7umph", "!", "Der", "uns", "den", "Him\u00b7mel", "gie\u00b7bet", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADJA", "NN", "$.", "ART", "PPER", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Der uns die Ewigkeit zu einem Erbtheil schenckt.", "tokens": ["Der", "uns", "die", "E\u00b7wig\u00b7keit", "zu", "ei\u00b7nem", "E\u00b7rbtheil", "schenckt", "."], "token_info": ["word", "word"