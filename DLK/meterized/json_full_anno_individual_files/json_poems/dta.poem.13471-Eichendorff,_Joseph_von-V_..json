{"dta.poem.13471": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "V .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "M\u00fcrrisch sitzen sie und maulen", "tokens": ["M\u00fcr\u00b7risch", "sit\u00b7zen", "sie", "und", "mau\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Auf den B\u00e4nken stumm und breit,", "tokens": ["Auf", "den", "B\u00e4n\u00b7ken", "stumm", "und", "breit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "G\u00e4hnend strecken sich die Faulen,", "tokens": ["G\u00e4h\u00b7nend", "stre\u00b7cken", "sich", "die", "Fau\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und die Kecken suchen Streit.", "tokens": ["Und", "die", "Ke\u00b7cken", "su\u00b7chen", "Streit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Da komm' ich durch's Dorf geschritten,", "tokens": ["Da", "komm'", "ich", "durch's", "Dorf", "ge\u00b7schrit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN", "VVPP", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Fernher durch den Abend k\u00fchl,", "tokens": ["Fern\u00b7her", "durch", "den", "A\u00b7bend", "k\u00fchl", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Stell' mich in des Kreises Mitten,", "tokens": ["Stell'", "mich", "in", "des", "Krei\u00b7ses", "Mit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gr\u00fc\u00df' und zieh' mein Geigenspiel.", "tokens": ["Gr\u00fc\u00df'", "und", "zieh'", "mein", "Gei\u00b7gen\u00b7spiel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Und wie ich den Bogen schwenke,", "tokens": ["Und", "wie", "ich", "den", "Bo\u00b7gen", "schwen\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Ziehn die Kl\u00e4nge in der Rund'", "tokens": ["Ziehn", "die", "Kl\u00e4n\u00b7ge", "in", "der", "Rund'"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Allen recht durch die Gelenke", "tokens": ["Al\u00b7len", "recht", "durch", "die", "Ge\u00b7len\u00b7ke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "ADJD", "APPR", "ART", "NN"], "meter": "+-+-+---", "measure": "unknown.measure.tri"}, "line.4": {"text": "Bis zum tiefsten Herzensgrund.", "tokens": ["Bis", "zum", "tiefs\u00b7ten", "Her\u00b7zens\u00b7grund", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Und nun geht's ans Gl\u00e4serklingen,", "tokens": ["Und", "nun", "geht's", "ans", "Gl\u00e4\u00b7ser\u00b7klin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "An ein Walzen um und um,", "tokens": ["An", "ein", "Wal\u00b7zen", "um", "und", "um", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "KON", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Je mehr ich streich', je mehr sie springen", "tokens": ["Je", "mehr", "ich", "streich'", ",", "je", "mehr", "sie", "sprin\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "PPER", "VVFIN", "$,", "ADV", "ADV", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Keiner fr\u00e4gt erst lang: warum? \u2014", "tokens": ["Kei\u00b7ner", "fr\u00e4gt", "erst", "lang", ":", "wa\u00b7rum", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["PIS", "VVFIN", "ADV", "ADJD", "$.", "PWAV", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Jeder will dem Geiger reichen", "tokens": ["Je\u00b7der", "will", "dem", "Gei\u00b7ger", "rei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "VMFIN", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nun sein Scherflein auf die Hand \u2014", "tokens": ["Nun", "sein", "Scher\u00b7flein", "auf", "die", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "APPR", "ART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da vergeht ihm gleich sein Streichen,", "tokens": ["Da", "ver\u00b7geht", "ihm", "gleich", "sein", "Strei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und fort ist der Musikant.", "tokens": ["Und", "fort", "ist", "der", "Mu\u00b7si\u00b7kant", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKVZ", "VAFIN", "ART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Und sie seh'n ihn fr\u00f6hlich steigen", "tokens": ["Und", "sie", "seh'n", "ihn", "fr\u00f6h\u00b7lich", "stei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nach den Waldesh\u00f6h'n hinaus,", "tokens": ["Nach", "den", "Wal\u00b7des\u00b7h\u00f6h'n", "hin\u00b7aus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APZR", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "H\u00f6ren ihn von fern noch geigen,", "tokens": ["H\u00f6\u00b7ren", "ihn", "von", "fern", "noch", "gei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJD", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und gehn All' vergn\u00fcgt nach Haus.", "tokens": ["Und", "gehn", "All'", "ver\u00b7gn\u00fcgt", "nach", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "VVFIN", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Doch in Waldes gr\u00fcnen Hallen", "tokens": ["Doch", "in", "Wal\u00b7des", "gr\u00fc\u00b7nen", "Hal\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Rast' ich dann noch manche Stund',", "tokens": ["Rast'", "ich", "dann", "noch", "man\u00b7che", "Stund'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "PIAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nur die fernen Nachtigallen", "tokens": ["Nur", "die", "fer\u00b7nen", "Nach\u00b7ti\u00b7gal\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schlagen tief aus n\u00e4cht'gem Grund.", "tokens": ["Schla\u00b7gen", "tief", "aus", "n\u00e4cht'\u00b7gem", "Grund", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Und es rauscht die Nacht so leise", "tokens": ["Und", "es", "rauscht", "die", "Nacht", "so", "lei\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "ADV", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Durch die Waldeseinsamkeit,", "tokens": ["Durch", "die", "Wal\u00b7de\u00b7sein\u00b7sam\u00b7keit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ich sinn' auf neue Weise,", "tokens": ["Und", "ich", "sinn'", "auf", "neu\u00b7e", "Wei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PRF", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die der Menschen Herz erfreut.", "tokens": ["Die", "der", "Men\u00b7schen", "Herz", "er\u00b7freut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}