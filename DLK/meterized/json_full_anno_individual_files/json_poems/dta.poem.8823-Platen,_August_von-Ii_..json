{"dta.poem.8823": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "Ii .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Etwas ist in meinen Liedern, was den Menschen wohl-", "tokens": ["Et\u00b7was", "ist", "in", "mei\u00b7nen", "Lie\u00b7dern", ",", "was", "den", "Men\u00b7schen", "wohl"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPR", "PPOSAT", "NN", "$,", "PRELS", "ART", "NN", "TRUNC"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.2": {"text": "Sollen eures Wortes Pfeile dringen in des Lebens Herz,", "tokens": ["Sol\u00b7len", "eu\u00b7res", "Wor\u00b7tes", "Pfei\u00b7le", "drin\u00b7gen", "in", "des", "Le\u00b7bens", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "NN", "VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "M\u00fc\u00dft ihr sie mit dem befiedern, was den Menschen", "tokens": ["M\u00fc\u00dft", "ihr", "sie", "mit", "dem", "be\u00b7fie\u00b7dern", ",", "was", "den", "Men\u00b7schen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPER", "APPR", "ART", "VVINF", "$,", "PRELS", "ART", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.4": {"text": "Selbst der Herr des achten Himmels mochte diese Welt", "tokens": ["Selbst", "der", "Herr", "des", "ach\u00b7ten", "Him\u00b7mels", "moch\u00b7te", "die\u00b7se", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "PDAT", "NN"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.5": {"text": "Mochte sich zu dem erniedern, was den Menschen wohl-", "tokens": ["Moch\u00b7te", "sich", "zu", "dem", "er\u00b7nie\u00b7dern", ",", "was", "den", "Men\u00b7schen", "wohl"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VMFIN", "PRF", "APPR", "ART", "ADJA", "$,", "PRELS", "ART", "NN", "TRUNC"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.6": {"text": "Vor dem Hochaltar des Sch\u00f6nen, neige sich das Gute", "tokens": ["Vor", "dem", "Hoc\u00b7hal\u00b7tar", "des", "Sch\u00f6\u00b7nen", ",", "nei\u00b7ge", "sich", "das", "Gu\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,", "VVFIN", "PRF", "ART", "NN"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.7": {"text": "Was den Herzen aller Biedern, was den Menschen", "tokens": ["Was", "den", "Her\u00b7zen", "al\u00b7ler", "Bie\u00b7dern", ",", "was", "den", "Men\u00b7schen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "ART", "NN", "PIAT", "NN", "$,", "PRELS", "ART", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.8": {"text": "Hat uns auch der Mai verlassen, Jugend ist im Winter", "tokens": ["Hat", "uns", "auch", "der", "Mai", "ver\u00b7las\u00b7sen", ",", "Ju\u00b7gend", "ist", "im", "Win\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "ART", "NN", "VVINF", "$,", "NN", "VAFIN", "APPRART", "NN"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.9": {"text": "Jugend zeigt in sch\u00f6nen Gliedern, was den Menschen", "tokens": ["Ju\u00b7gend", "zeigt", "in", "sch\u00f6\u00b7nen", "Glie\u00b7dern", ",", "was", "den", "Men\u00b7schen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPR", "ADJA", "NN", "$,", "PRELS", "ART", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}}}}}