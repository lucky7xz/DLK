{"textgrid.poem.58539": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "577. Das Sonnenst\u00e4ublein", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wo find' ich mich?", "tokens": ["Wo", "find'", "ich", "mich", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PRF", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Wie gro\u00df ist Gott \u2013 wie klein bin ich!", "tokens": ["Wie", "gro\u00df", "ist", "Gott", "\u2013", "wie", "klein", "bin", "ich", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "NN", "$(", "PWAV", "ADJD", "VAFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Denk nur, was ist ein St\u00e4ubelein", "tokens": ["Denk", "nur", ",", "was", "ist", "ein", "St\u00e4u\u00b7be\u00b7lein"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVIMP", "ADV", "$,", "PWS", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Im hellen Sonnenschein!", "tokens": ["Im", "hel\u00b7len", "Son\u00b7nen\u00b7schein", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wie, sollt' ich mich nicht selbst und alle Ding' vergessen,", "tokens": ["Wie", ",", "sollt'", "ich", "mich", "nicht", "selbst", "und", "al\u00b7le", "Ding'", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "VMFIN", "PPER", "PRF", "PTKNEG", "ADV", "KON", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "