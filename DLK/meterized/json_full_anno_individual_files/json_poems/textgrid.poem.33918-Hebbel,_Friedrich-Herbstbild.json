{"textgrid.poem.33918": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Herbstbild", "genre": "verse", "period": "N.A.", "pub_year": 1852, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die\u00df ist ein Herbsttag, wie ich keinen sah!", "tokens": ["Die\u00df", "ist", "ein", "Herbst\u00b7tag", ",", "wie", "ich", "kei\u00b7nen", "sah", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "PWAV", "PPER", "PIAT", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Luft ist still, als athmete man kaum,", "tokens": ["Die", "Luft", "ist", "still", ",", "als", "ath\u00b7me\u00b7te", "man", "kaum", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "KOUS", "VVFIN", "PIS", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und dennoch fallen raschelnd, fern und nah',", "tokens": ["Und", "den\u00b7noch", "fal\u00b7len", "ra\u00b7schelnd", ",", "fern", "und", "nah'", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVINF", "VVPP", "$,", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die sch\u00f6nsten Fr\u00fcchte ab von jedem Baum.", "tokens": ["Die", "sch\u00f6ns\u00b7ten", "Fr\u00fcch\u00b7te", "ab", "von", "je\u00b7dem", "Baum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "O st\u00f6rt sie nicht, die Feier der Natur!", "tokens": ["O", "st\u00f6rt", "sie", "nicht", ",", "die", "Fei\u00b7er", "der", "Na\u00b7tur", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "PTKNEG", "$,", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die\u00df ist die Lese, die sie selber h\u00e4lt,", "tokens": ["Die\u00df", "ist", "die", "Le\u00b7se", ",", "die", "sie", "sel\u00b7ber", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "PRELS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Denn heute l\u00f6s't sich von den Zweigen nur,", "tokens": ["Denn", "heu\u00b7te", "l\u00f6s't", "sich", "von", "den", "Zwei\u00b7gen", "nur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PRF", "APPR", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Was vor dem milden Stral der Sonne f\u00e4llt.", "tokens": ["Was", "vor", "dem", "mil\u00b7den", "Stral", "der", "Son\u00b7ne", "f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "ADJA", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Die\u00df ist ein Herbsttag, wie ich keinen sah!", "tokens": ["Die\u00df", "ist", "ein", "Herbst\u00b7tag", ",", "wie", "ich", "kei\u00b7nen", "sah", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "PWAV", "PPER", "PIAT", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Luft ist still, als athmete man kaum,", "tokens": ["Die", "Luft", "ist", "still", ",", "als", "ath\u00b7me