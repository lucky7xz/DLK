{"dta.poem.18837": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang von", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1819", "urn": "urn:nbn:de:kobv:b4-200905191692", "language": ["de:0.99"], "booktitle": "Goethe, Johann Wolfgang von: West-\u00f6stlicher Divan. Stuttgart, 1819."}, "poem": {"stanza.1": {"line.1": {"text": "Ein Kaiser hatte zwey Cassire,", "tokens": ["Ein", "Kai\u00b7ser", "hat\u00b7te", "zwey", "Cas\u00b7si\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "CARD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Einen zum Nehmen, einen zum Spenden;", "tokens": ["Ei\u00b7nen", "zum", "Neh\u00b7men", ",", "ei\u00b7nen", "zum", "Spen\u00b7den", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "$,", "ART", "APPRART", "NN", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Diesem fiel\u2019s nur so aus den H\u00e4nden,", "tokens": ["Die\u00b7sem", "fiel's", "nur", "so", "aus", "den", "H\u00e4n\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Jener wusste nicht woher zu nehmen.", "tokens": ["Je\u00b7ner", "wuss\u00b7te", "nicht", "wo\u00b7her", "zu", "neh\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "VVFIN", "PTKNEG", "ADJD", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Der Spendende starb, der Herrscher wusste nicht", "tokens": ["Der", "Spen\u00b7den\u00b7de", "starb", ",", "der", "Herr\u00b7scher", "wuss\u00b7te", "nicht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "PTKNEG"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "gleich,", "tokens": ["gleich", ","], "token_info": ["word", "punct"], "pos": ["ADV", "$,"], "meter": "-", "measure": "single.down"}, "line.7": {"text": "Wem das Geber-Amt sey anzuvertrauen,", "tokens": ["Wem", "das", "Ge\u00b7ber\u00b7Amt", "sey", "an\u00b7zu\u00b7ver\u00b7trau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VAFIN", "VVIZU", "$,"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.8": {"text": "Und wie man kaum th\u00e4t um sich schauen,", "tokens": ["Und", "wie", "man", "kaum", "th\u00e4t", "um", "sich", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PIS", "ADV", "VVFIN", "APPR", "PRF", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.9": {"text": "So war der Nehmer unendlich reich,", "tokens": ["So", "war", "der", "Neh\u00b7mer", "un\u00b7end\u00b7lich", "reich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADJD", "ADJD", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Man wusste kaum vor Gold zu leben,", "tokens": ["Man", "wuss\u00b7te", "kaum", "vor", "Gold", "zu", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "APPR", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Weil man Einen Tag nichts ausgegeben.", "tokens": ["Weil", "man", "Ei\u00b7nen", "Tag", "nichts", "aus\u00b7ge\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "NN", "PIS", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.12": {"text": "Da ward nun erst dem Kaiser klar", "tokens": ["Da", "ward", "nun", "erst", "dem", "Kai\u00b7ser", "klar"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "ADV", "ART", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Was Schuld an allem Unheil war.", "tokens": ["Was", "Schuld", "an", "al\u00b7lem", "Un\u00b7heil", "war", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "APPR", "PIS", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Den Zufall wusst\u2019 er wohl zu sch\u00e4tzen", "tokens": ["Den", "Zu\u00b7fall", "wusst'", "er", "wohl", "zu", "sch\u00e4t\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Nie wieder die Stelle zu besetzen.", "tokens": ["Nie", "wie\u00b7der", "die", "Stel\u00b7le", "zu", "be\u00b7set\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}}}}}