{"dta.poem.23427": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Notre Dame.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192634", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die B\u00fchne ward zum Schauger\u00fcste", "tokens": ["Die", "B\u00fch\u00b7ne", "ward", "zum", "Schau\u00b7ge\u00b7r\u00fcs\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Des Lasters und der H\u00e4\u00dflichkeit,", "tokens": ["Des", "Las\u00b7ters", "und", "der", "H\u00e4\u00df\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Tummelplatz, gemeiner L\u00fcste,", "tokens": ["Ein", "Tum\u00b7mel\u00b7platz", ",", "ge\u00b7mei\u00b7ner", "L\u00fcs\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Spittel voller Qual und Leid.", "tokens": ["Ein", "Spit\u00b7tel", "vol\u00b7ler", "Qual", "und", "Leid", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ihr wi\u00dft uns weiter nichts zu geben", "tokens": ["Ihr", "wi\u00dft", "uns", "wei\u00b7ter", "nichts", "zu", "ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PIS", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Als Mord und Todschlag, Lug und Trug;", "tokens": ["Als", "Mord", "und", "Tod\u00b7schlag", ",", "Lug", "und", "Trug", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "$,", "NE", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ihr macht noch schrecklicher das Leben,", "tokens": ["Ihr", "macht", "noch", "schreck\u00b7li\u00b7cher", "das", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und schrecklich ist es doch genug.", "tokens": ["Und", "schreck\u00b7lich", "ist", "es", "doch", "ge\u00b7nug", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Soll ", "tokens": ["Soll"], "token_info": ["word"], "pos": ["VMFIN"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "Erh\u00f6hn des Daseyns kurze Lust?", "tokens": ["Er\u00b7h\u00f6hn", "des", "Da\u00b7seyns", "kur\u00b7ze", "Lust", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und mit dem Leben uns vers\u00f6hnen?", "tokens": ["Und", "mit", "dem", "Le\u00b7ben", "uns", "ver\u00b7s\u00f6h\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und Frieden bringen unsrer Brust?", "tokens": ["Und", "Frie\u00b7den", "brin\u00b7gen", "uns\u00b7rer", "Brust", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Gott gab die Kunst dem Menschenleben,", "tokens": ["Gott", "gab", "die", "Kunst", "dem", "Men\u00b7schen\u00b7le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Gott sei auch durch die Kunst geehrt;", "tokens": ["Gott", "sei", "auch", "durch", "die", "Kunst", "ge\u00b7ehrt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ihr aber habt, was Gott gegeben,", "tokens": ["Ihr", "a\u00b7ber", "habt", ",", "was", "Gott", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "$,", "PRELS", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In schn\u00f6den Teufelsspuk verkehrt.", "tokens": ["In", "schn\u00f6\u00b7den", "Teu\u00b7felss\u00b7puk", "ver\u00b7kehrt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}