{"dta.poem.16336": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "72.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie gleichest du, o Mensch, und dein Geschick den Saaten,", "tokens": ["Wie", "glei\u00b7chest", "du", ",", "o", "Mensch", ",", "und", "dein", "Ge\u00b7schick", "den", "Saa\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "$,", "FM", "NN", "$,", "KON", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von denen Niemand wei\u00df zuvor, wie sie gerathen.", "tokens": ["Von", "de\u00b7nen", "Nie\u00b7mand", "wei\u00df", "zu\u00b7vor", ",", "wie", "sie", "ge\u00b7ra\u00b7then", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PIS", "VVFIN", "ADV", "$,", "PWAV", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wie manches Ungemach, Frost, N\u00e4sse, D\u00fcrre, Brand,", "tokens": ["Wie", "man\u00b7ches", "Un\u00b7ge\u00b7mach", ",", "Frost", ",", "N\u00e4s\u00b7se", ",", "D\u00fcr\u00b7re", ",", "Brand", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "$,", "NN", "$,", "NN", "$,", "NE", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gibt ihnen zu bestehn des Himmels Unbestand.", "tokens": ["Gibt", "ih\u00b7nen", "zu", "be\u00b7stehn", "des", "Him\u00b7mels", "Un\u00b7be\u00b7stand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKZU", "VVINF", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Und wenn sie gl\u00fccklich nun bestanden die Beschwerden,", "tokens": ["Und", "wenn", "sie", "gl\u00fcck\u00b7lich", "nun", "be\u00b7stan\u00b7den", "die", "Be\u00b7schwer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADJD", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So ist ihr Ende, da\u00df sie abgeschnitten werden.", "tokens": ["So", "ist", "ihr", "En\u00b7de", ",", "da\u00df", "sie", "ab\u00b7ge\u00b7schnit\u00b7ten", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "$,", "KOUS", "PPER", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}