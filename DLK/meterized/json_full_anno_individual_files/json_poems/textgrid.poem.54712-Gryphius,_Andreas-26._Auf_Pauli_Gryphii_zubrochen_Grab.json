{"textgrid.poem.54712": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "26. Auf Pauli Gryphii zubrochen Grab", "genre": "verse", "period": "N.A.", "pub_year": 1640, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es zitterte f\u00fcr euch wer iemals euch geh\u00f6rt/", "tokens": ["Es", "zit\u00b7ter\u00b7te", "f\u00fcr", "euch", "wer", "ie\u00b7mals", "euch", "ge\u00b7h\u00f6rt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "PWS", "ADV", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Euch hat der libste Freund und h\u00f6chste Feind geehrt.", "tokens": ["Euch", "hat", "der", "libs\u00b7te", "Freund", "und", "h\u00f6chs\u00b7te", "Feind", "ge\u00b7ehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "KON", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Bi\u00df ihr ins Grab versenckt/ da muste diser Stein/", "tokens": ["Bi\u00df", "ihr", "ins", "Grab", "ver\u00b7senckt", "/", "da", "mus\u00b7te", "di\u00b7ser", "Stein", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "VVPP", "$(", "ADV", "VMFIN", "PDAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ein Zeuge grosser Mann/ des grossen Mutes seyn.", "tokens": ["Ein", "Zeu\u00b7ge", "gros\u00b7ser", "Mann", "/", "des", "gros\u00b7sen", "Mu\u00b7tes", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$(", "ART", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Itzt bricht/ ich wei\u00df nicht wer/ den Marmel gantz entzwey/", "tokens": ["Itzt", "bricht", "/", "ich", "wei\u00df", "nicht", "wer", "/", "den", "Mar\u00b7mel", "gantz", "ent\u00b7zwey", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$(", "PPER", "VVFIN", "PTKNEG", "PWS", "$(", "ART", "NN", "ADV", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und wil (so wie es scheint) den d\u00fcrren Glidern bey.", "tokens": ["Und", "wil", "(", "so", "wie", "es", "scheint", ")", "den", "d\u00fcr\u00b7ren", "Gli\u00b7dern", "bey", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "$(", "ADV", "KOKOM", "PPER", "VVFIN", "$(", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Di\u00df ist kein Menschen Werck wenn man den Hund mu\u00df schmeissen/", "tokens": ["Di\u00df", "ist", "kein", "Men\u00b7schen", "Werck", "wenn", "man", "den", "Hund", "mu\u00df", "schmeis\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PIAT", "NN", "NN", "KOUS", "PIS", "ART", "NN", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So pflegt er in den Stein/ mit dem man traff/ zu beissen.", "tokens": ["So", "pflegt", "er", "in", "den", "Stein", "/", "mit", "dem", "man", "traff", "/", "zu", "beis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$(", "APPR", "PRELS", "PIS",