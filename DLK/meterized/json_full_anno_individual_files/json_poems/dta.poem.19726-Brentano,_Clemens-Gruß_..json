{"dta.poem.19726": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Gru\u00df .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519168", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "So viel Stern am Himmel stehen,               ", "tokens": ["So", "viel", "Stern", "am", "Him\u00b7mel", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "So viel Sch\u00e4flein als da gehen", "tokens": ["So", "viel", "Sch\u00e4f\u00b7lein", "als", "da", "ge\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PIAT", "NN", "KOKOM", "ADV", "VVINF"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.3": {"text": "In dem gr\u00fcnen Feld.", "tokens": ["In", "dem", "gr\u00fc\u00b7nen", "Feld", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "So viel V\u00f6gel als da fliegen,", "tokens": ["So", "viel", "V\u00f6\u00b7gel", "als", "da", "flie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "KOKOM", "ADV", "VVINF", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Als da hin und wieder fliegen,", "tokens": ["Als", "da", "hin", "und", "wie\u00b7der", "flie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PTKVZ", "KON", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "So viel mal sey du gegr\u00fc\u00dft.", "tokens": ["So", "viel", "mal", "sey", "du", "ge\u00b7gr\u00fc\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "Soll ich dich dann nimmer sehen,", "tokens": ["Soll", "ich", "dich", "dann", "nim\u00b7mer", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "ADV", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ach das kann ich nicht verstehen,", "tokens": ["Ach", "das", "kann", "ich", "nicht", "ver\u00b7ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PDS", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "O du bittrer Scheidens Schlu\u00df.", "tokens": ["O", "du", "bit\u00b7trer", "Schei\u00b7dens", "Schlu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ADJA", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "W\u00e4r ich lieber schon gestorben,", "tokens": ["W\u00e4r", "ich", "lie\u00b7ber", "schon", "ge\u00b7stor\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Eh ich mir ein Schaz erworben,", "tokens": ["Eh", "ich", "mir", "ein", "Schaz", "er\u00b7wor\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "W\u00e4r ich jetzo nicht betr\u00fcbt.", "tokens": ["W\u00e4r", "ich", "jet\u00b7zo", "nicht", "be\u00b7tr\u00fcbt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "PTKNEG", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Wei\u00df nicht, ob auf dieser Erden", "tokens": ["Wei\u00df", "nicht", ",", "ob", "auf", "die\u00b7ser", "Er\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PTKNEG", "$,", "KOUS", "APPR", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nach viel Tr\u00fcbsal und Beschwerden", "tokens": ["Nach", "viel", "Tr\u00fcb\u00b7sal", "und", "Be\u00b7schwer\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ich dich wieder sehen soll.", "tokens": ["Ich", "dich", "wie\u00b7der", "se\u00b7hen", "soll", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PRF", "ADV", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Was f\u00fcr Wellen, was f\u00fcr Flammen", "tokens": ["Was", "f\u00fcr", "Wel\u00b7len", ",", "was", "f\u00fcr", "Flam\u00b7men"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "APPR", "NN", "$,", "PRELS", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schlagen \u00fcber mir zusammen,", "tokens": ["Schla\u00b7gen", "\u00fc\u00b7ber", "mir", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ach wie gro\u00df ist meine Noth.", "tokens": ["Ach", "wie", "gro\u00df", "ist", "mei\u00b7ne", "Noth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ADJD", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Mit Geduld will ich es tragen,", "tokens": ["Mit", "Ge\u00b7duld", "will", "ich", "es", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VMFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Alle Morgen will ich sagen:", "tokens": ["Al\u00b7le", "Mor\u00b7gen", "will", "ich", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "O mein Schaz wann kommst zu mir.", "tokens": ["O", "mein", "Schaz", "wann", "kommst", "zu", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "PWAV", "VVFIN", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Alle Abend will ich sprechen,", "tokens": ["Al\u00b7le", "A\u00b7bend", "will", "ich", "spre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn mir meine Aeuglein brechen:", "tokens": ["Wenn", "mir", "mei\u00b7ne", "A\u00b7e\u00b7u\u00b7glein", "bre\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "O mein Schaz gedenk an mich.", "tokens": ["O", "mein", "Schaz", "ge\u00b7denk", "an", "mich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "VVFIN", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Ja ich will dich nicht vergessen,", "tokens": ["Ja", "ich", "will", "dich", "nicht", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PPER", "VMFIN", "PPER", "PTKNEG", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wann ich sollte unterdessen", "tokens": ["Wann", "ich", "soll\u00b7te", "un\u00b7ter\u00b7des\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "PPER", "VMFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Auf dem Todbett schlafen ein.", "tokens": ["Auf", "dem", "Tod\u00b7bett", "schla\u00b7fen", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Auf dem Kirchhof will ich liegen", "tokens": ["Auf", "dem", "Kirch\u00b7hof", "will", "ich", "lie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VMFIN", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie das Kindlein in der Wiegen,", "tokens": ["Wie", "das", "Kin\u00b7dlein", "in", "der", "Wie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Das die Lieb thut wiegen ein.", "tokens": ["Das", "die", "Lieb", "thut", "wie\u00b7gen", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "VVFIN", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}