{"dta.poem.13448": {"metadata": {"author": {"name": "Suppius, Christoph Eusebius", "birth": "N.A.", "death": "N.A."}, "title": "Der Fr\u00fchling.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1749", "urn": "urn:nbn:de:kobv:b4-20594-7", "language": ["de:0.99"], "booktitle": "Suppius, Christoph Eusebius: Oden und Lieder. Gotha, 1749."}, "poem": {"stanza.1": {"line.1": {"text": "Der Fr\u00fchling kommt gegangen", "tokens": ["Der", "Fr\u00fch\u00b7ling", "kommt", "ge\u00b7gan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mit seinen Rosenwangen,", "tokens": ["Mit", "sei\u00b7nen", "Ro\u00b7sen\u00b7wan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Jhm folgen muntre Schaaren", "tokens": ["Jhm", "fol\u00b7gen", "mun\u00b7tre", "Schaa\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mit Blumen in den Haaren;", "tokens": ["Mit", "Blu\u00b7men", "in", "den", "Haa\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Das Lachen, Reizen, Scherzen,", "tokens": ["Das", "La\u00b7chen", ",", "Rei\u00b7zen", ",", "Scher\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ein Heer verliebter Herzen,", "tokens": ["Ein", "Heer", "ver\u00b7lieb\u00b7ter", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und muntre Liebes-Knaben,", "tokens": ["Und", "mun\u00b7tre", "Lie\u00b7bes\u00b7Kna\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die ihn umringet haben,", "tokens": ["Die", "ihn", "um\u00b7rin\u00b7get", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Verk\u00fcndigen den Fluhren", "tokens": ["Ver\u00b7k\u00fcn\u00b7di\u00b7gen", "den", "Fluh\u00b7ren"], "token_info": ["word", "word", "word"], "pos": ["NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Der gr\u00fcnen Hoffnung Spuhren,", "tokens": ["Der", "gr\u00fc\u00b7nen", "Hoff\u00b7nung", "Spuh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Und unter seinen F\u00fcssen", "tokens": ["Und", "un\u00b7ter", "sei\u00b7nen", "F\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Der jungen Welt Erspriessen.", "tokens": ["Der", "jun\u00b7gen", "Welt", "Er\u00b7spries\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Bey frischbelaubten B\u00e4umen", "tokens": ["Bey", "frischbe\u00b7laub\u00b7ten", "B\u00e4u\u00b7men"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.14": {"text": "Winkt mancher Hirt den Tr\u00e4umen,", "tokens": ["Winkt", "man\u00b7cher", "Hirt", "den", "Tr\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.15": {"text": "Und sagt dem Westenwinde", "tokens": ["Und", "sagt", "dem", "Wes\u00b7ten\u00b7win\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.16": {"text": "Was vor von seinem Kinde.", "tokens": ["Was", "vor", "von", "sei\u00b7nem", "Kin\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.17": {"text": "Der Nachtigallen Lieder", "tokens": ["Der", "Nach\u00b7ti\u00b7gal\u00b7len", "Lie\u00b7der"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.18": {"text": "Bringt Echo doppelt wieder,", "tokens": ["Bringt", "E\u00b7cho", "dop\u00b7pelt", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "ADJD", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.19": {"text": "Indem sie in den B\u00fcschen", "tokens": ["In\u00b7dem", "sie", "in", "den", "B\u00fc\u00b7schen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.20": {"text": "Adonis will erwischen.", "tokens": ["A\u00b7do\u00b7nis", "will", "er\u00b7wi\u00b7schen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.21": {"text": "Das Federvolk in Ch\u00f6ren", "tokens": ["Das", "Fe\u00b7der\u00b7volk", "in", "Ch\u00f6\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.22": {"text": "L\u00e4\u00dft sanfte Noten h\u00f6ren,", "tokens": ["L\u00e4\u00dft", "sanf\u00b7te", "No\u00b7ten", "h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.23": {"text": "Die auf belaubten Zweigen", "tokens": ["Die", "auf", "be\u00b7laub\u00b7ten", "Zwei\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.24": {"text": "Nicht von der Liebe schweigen.", "tokens": ["Nicht", "von", "der", "Lie\u00b7be", "schwei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.25": {"text": "Ey! bin denn ich alleine", "tokens": ["Ey", "!", "bin", "denn", "ich", "al\u00b7lei\u00b7ne"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$.", "VAFIN", "KON", "PPER", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.26": {"text": "Nicht lebend, und von Steine,", "tokens": ["Nicht", "le\u00b7bend", ",", "und", "von", "Stei\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "$,", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.27": {"text": "Ist Lachen, Spielen, Scherzen", "tokens": ["Ist", "La\u00b7chen", ",", "Spie\u00b7len", ",", "Scher\u00b7zen"], "token_info": ["word", "word", "punct", "word", "punct", "word"], "pos": ["VAFIN", "NN", "$,", "NN", "$,", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.28": {"text": "Verbannt aus meinem Herzen?", "tokens": ["Ver\u00b7bannt", "aus", "mei\u00b7nem", "Her\u00b7zen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.29": {"text": "Wie? st\u00f6hrt ein schwarzer Kummer", "tokens": ["Wie", "?", "st\u00f6hrt", "ein", "schwar\u00b7zer", "Kum\u00b7mer"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "$.", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.30": {"text": "Noch ferner Ruh und Schlummer?", "tokens": ["Noch", "fer\u00b7ner", "Ruh", "und", "Schlum\u00b7mer", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.31": {"text": "O Muse, nein! im K\u00fchlen", "tokens": ["O", "Mu\u00b7se", ",", "nein", "!", "im", "K\u00fch\u00b7len"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["NE", "NN", "$,", "PTKANT", "$.", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.32": {"text": "Will ich eins mit dir spielen,", "tokens": ["Will", "ich", "eins", "mit", "dir", "spie\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PIS", "APPR", "PPER", "VVINF", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.33": {"text": "Da noch mit gr\u00fcnen Jahren", "tokens": ["Da", "noch", "mit", "gr\u00fc\u00b7nen", "Jah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.34": {"text": "Sich Scherz und Anmuth paaren,", "tokens": ["Sich", "Scherz", "und", "An\u00b7muth", "paa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.35": {"text": "Da soll den Zeitvertreiben", "tokens": ["Da", "soll", "den", "Zeit\u00b7ver\u00b7trei\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.36": {"text": "Kein Ach mehr \u00fcbrig bleiben,", "tokens": ["Kein", "Ach", "mehr", "\u00fcb\u00b7rig", "blei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.37": {"text": "Bis da\u00df mit braunen Wangen", "tokens": ["Bis", "da\u00df", "mit", "brau\u00b7nen", "Wan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "KOUS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.38": {"text": "Der Sommer das Verlangen", "tokens": ["Der", "Som\u00b7mer", "das", "Ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.39": {"text": "Der Jugend, und den Lenzen", "tokens": ["Der", "Ju\u00b7gend", ",", "und", "den", "Len\u00b7zen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "KON", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.40": {"text": "Bekr\u00f6nt mit Aehrenkr\u00e4nzen,", "tokens": ["Be\u00b7kr\u00f6nt", "mit", "A\u00b7eh\u00b7ren\u00b7kr\u00e4n\u00b7zen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.41": {"text": "Der Herbst uns was verehret,", "tokens": ["Der", "Herbst", "uns", "was", "ver\u00b7eh\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.42": {"text": "Wenn er sein F\u00fcllhorn leeret,", "tokens": ["Wenn", "er", "sein", "F\u00fcll\u00b7horn", "lee\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.43": {"text": "Bis wir den K\u00fcmmernissen", "tokens": ["Bis", "wir", "den", "K\u00fcm\u00b7mer\u00b7nis\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.44": {"text": "Auf Herden opfern m\u00fcssen,", "tokens": ["Auf", "Her\u00b7den", "op\u00b7fern", "m\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "VMINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.45": {"text": "Wenn uns der Winde Pfeifen", "tokens": ["Wenn", "uns", "der", "Win\u00b7de", "Pfei\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.46": {"text": "Einst heisst die Flucht ergreifen,", "tokens": ["Einst", "heisst", "die", "Flucht", "er\u00b7grei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.47": {"text": "Denn alle Lust der Erden", "tokens": ["Denn", "al\u00b7le", "Lust", "der", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.48": {"text": "Mu\u00df doch zum Winter werden.", "tokens": ["Mu\u00df", "doch", "zum", "Win\u00b7ter", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "APPRART", "NN", "VAINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}