], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "APPR", "NE", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Habt ihr bi\u00dfher nicht gnugsam nachgedacht,", "tokens": ["Habt", "ihr", "bi\u00df\u00b7her", "nicht", "gnug\u00b7sam", "nach\u00b7ge\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "PTKNEG", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "So thut, als w\u00e4r\u2019t ihr aus dem Traum erwacht.", "tokens": ["So", "thut", ",", "als", "w\u00e4r't", "ihr", "aus", "dem", "Traum", "er\u00b7wacht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "KOKOM", "VAFIN", "PPER", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.13": {"line.1": {"text": "O Vater! gieb uns rechte Kinder-Sitten,", "tokens": ["O", "Va\u00b7ter", "!", "gieb", "uns", "rech\u00b7te", "Kin\u00b7der\u00b7Sit\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "VVIMP", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der du uns ja den HErrn zum Bruder giebst,", "tokens": ["Der", "du", "uns", "ja", "den", "Herrn", "zum", "Bru\u00b7der", "giebst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PRF", "ADV", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "O Geist des HErrn! der du Mariam liebst,", "tokens": ["O", "Geist", "des", "Herrn", "!", "der", "du", "Ma\u00b7ri\u00b7am", "liebst", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos"