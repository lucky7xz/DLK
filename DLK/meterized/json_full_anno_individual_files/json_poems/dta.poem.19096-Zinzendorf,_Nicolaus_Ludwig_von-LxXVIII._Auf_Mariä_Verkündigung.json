{"dta.poem.19096": {"metadata": {"author": {"name": "Zinzendorf, Nicolaus Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "LxXVIII.   Auf Mari\u00e4 Verk\u00fcndigung", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20688-6", "language": ["de:0.99"], "booktitle": "Zinzendorf, Nicolaus Ludwig von: Teutscher Gedichte Erster Theil. Herrnhuth, 1735."}, "poem": {"stanza.1": {"line.1": {"text": "Dreyeinigkeit, du allgemeines Wesen,", "tokens": ["Drey\u00b7ei\u00b7nig\u00b7keit", ",", "du", "all\u00b7ge\u00b7mei\u00b7nes", "We\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Du Sch\u00f6pfer und Erstatter der Natur,", "tokens": ["Du", "Sch\u00f6p\u00b7fer", "und", "Er\u00b7stat\u00b7ter", "der", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "KON", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und du der Gottheit Lebens-volle Spur,", "tokens": ["Und", "du", "der", "Got\u00b7theit", "Le\u00b7bens\u00b7vol\u00b7le", "Spur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ART", "NN", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Du hast dir eine Werckstatt auserlesen,", "tokens": ["Du", "hast", "dir", "ei\u00b7ne", "Wercks\u00b7tatt", "au\u00b7ser\u00b7le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Darein du dich in voller Kraft gesenckt,", "tokens": ["Da\u00b7rein", "du", "dich", "in", "vol\u00b7ler", "Kraft", "ge\u00b7senckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "PRF", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und daheraus uns ", "tokens": ["Und", "da\u00b7he\u00b7raus", "uns"], "token_info": ["word", "word", "word"], "pos": ["KON", "PAV", "PPER"], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Maria war die Gnaden-reiche Esther,", "tokens": ["Ma\u00b7ria", "war", "die", "Gna\u00b7den\u00b7rei\u00b7che", "E\u00b7sther", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Der du dich so in Lieb und Huld verbandst,", "tokens": ["Der", "du", "dich", "so", "in", "Lieb", "und", "Huld", "ver\u00b7bandst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PRF", "ADV", "APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dieweil du sie der Gottheit w\u00fcrdig fandst,", "tokens": ["Die\u00b7weil", "du", "sie", "der", "Got\u00b7theit", "w\u00fcr\u00b7dig", "fandst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Maria wars, die liebe selge Schwester,", "tokens": ["Ma\u00b7ria", "wars", ",", "die", "lie\u00b7be", "sel\u00b7ge", "Schwes\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "$,", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Man nahm an ihr nichts ungew\u00f6hnlichs wahr,", "tokens": ["Man", "nahm", "an", "ihr", "nichts", "un\u00b7ge\u00b7w\u00f6hn\u00b7lichs", "wahr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "PPER", "PIS", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Als da\u00df sie still und arm und hertzlich war.", "tokens": ["Als", "da\u00df", "sie", "still", "und", "arm", "und", "hertz\u00b7lich", "war", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "ADJD", "KON", "ADJD", "KON", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Maria war die Mutter des Geweyhten,", "tokens": ["Ma\u00b7ria", "war", "die", "Mut\u00b7ter", "des", "Ge\u00b7weyh\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Der ewiglich der Seelen Br\u00e4utgam ist,", "tokens": ["Der", "e\u00b7wig\u00b7lich", "der", "See\u00b7len", "Br\u00e4ut\u00b7gam", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "NE", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und ehe er die Braut im Throne k\u00fc\u00dft,", "tokens": ["Und", "e\u00b7he", "er", "die", "Braut", "im", "Thro\u00b7ne", "k\u00fc\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Worauf sich schon die Engel-Ch\u00f6re freuten,", "tokens": ["Wo\u00b7rauf", "sich", "schon", "die", "En\u00b7gel\u00b7Ch\u00f6\u00b7re", "freu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PRF", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Vors Weib in Arbeit lebte, litt und starb.", "tokens": ["Vors", "Weib", "in", "Ar\u00b7beit", "leb\u00b7te", ",", "litt", "und", "starb", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "NN", "VVFIN", "$,", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "O Hochzeit! die man Sabbaths-Ruhe nennet,", "tokens": ["O", "Hoch\u00b7zeit", "!", "die", "man", "Sab\u00b7baths\u00b7Ru\u00b7he", "nen\u00b7net", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "ART", "PIS", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "O Tag des HErrn! geheimes Bild der Eh\u2019,", "tokens": ["O", "Tag", "des", "Herrn", "!", "ge\u00b7hei\u00b7mes", "Bild", "der", "Eh'", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ART", "NN", "$.", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Jhr Huren, S\u00e4ue st\u00fcrtzt euch in die See,", "tokens": ["Ihr", "Hu\u00b7ren", ",", "S\u00e4u\u00b7e", "st\u00fcrtzt", "euch", "in", "die", "See", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die ihr in eurer Eh\u2019 Befleckung kennet,", "tokens": ["Die", "ihr", "in", "eu\u00b7rer", "Eh'", "Be\u00b7fle\u00b7ckung", "ken\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "PPOSAT", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und die ihr nichts um Satans Tiefen wi\u00dft,", "tokens": ["Und", "die", "ihr", "nichts", "um", "Sa\u00b7tans", "Tie\u00b7fen", "wi\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PPER", "PIS", "APPR", "NE", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Kommt her und lernt was ehlig werden ist!", "tokens": ["Kommt", "her", "und", "lernt", "was", "eh\u00b7lig", "wer\u00b7den", "ist", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "KON", "VVFIN", "PWS", "ADJD", "VAINF", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Jhr Seelen! die sich in die Eh\u2019 gefunden,", "tokens": ["Ihr", "See\u00b7len", "!", "die", "sich", "in", "die", "Eh'", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "PRELS", "PRF", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nicht, weil sie wider Christum geile sind,", "tokens": ["Nicht", ",", "weil", "sie", "wi\u00b7der", "Chris\u00b7tum", "gei\u00b7le", "sind", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "$,", "KOUS", "PPER", "APPR", "NE", "ADJA", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Nicht, weil Natur sich mit Natur verbindt,", "tokens": ["Nicht", ",", "weil", "Na\u00b7tur", "sich", "mit", "Na\u00b7tur", "ver\u00b7bindt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "$,", "KOUS", "NN", "PRF", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Nein! weil sie GOtt in diesen Stand verbunden;", "tokens": ["Nein", "!", "weil", "sie", "Gott", "in", "die\u00b7sen", "Stand", "ver\u00b7bun\u00b7den", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "KOUS", "PPER", "NN", "APPR", "PDAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Kommt, betet neben mir der Seelen Mann,", "tokens": ["Kommt", ",", "be\u00b7tet", "ne\u00b7ben", "mir", "der", "See\u00b7len", "Mann", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "APPR", "PPER", "ART", "NN", "NN", "$,"], "meter": "-----+-+-+", "measure": "unknown.measure.tri"}, "line.6": {"text": "Das Kind des Geistes und Mari\u00e4 an.", "tokens": ["Das", "Kind", "des", "Geis\u00b7tes", "und", "Ma\u00b7ri\u00e4", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "KON", "NE", "PTKVZ", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "Kommt, schwehrt mit mir dem treuen Zeugen Treue,", "tokens": ["Kommt", ",", "schwehrt", "mit", "mir", "dem", "treu\u00b7en", "Zeu\u00b7gen", "Treu\u00b7e", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "APPR", "PPER", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Kommt, ruft zu ihm um seiner Wei\u00dfheit Licht,", "tokens": ["Kommt", ",", "ruft", "zu", "ihm", "um", "sei\u00b7ner", "Wei\u00df\u00b7heit", "Licht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "APPR", "PPER", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Damit es euch in allen unterricht,", "tokens": ["Da\u00b7mit", "es", "euch", "in", "al\u00b7len", "un\u00b7ter\u00b7richt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und euren Stand den Augenblick verneue,", "tokens": ["Und", "eu\u00b7ren", "Stand", "den", "Au\u00b7gen\u00b7blick", "ver\u00b7neu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Zu Ehren seiner Zeugen opfert euch", "tokens": ["Zu", "Eh\u00b7ren", "sei\u00b7ner", "Zeu\u00b7gen", "op\u00b7fert", "euch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "PPOSAT", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Jhm auf mit Geist und Seel und Leib zugleich.", "tokens": ["Jhm", "auf", "mit", "Geist", "und", "Seel", "und", "Leib", "zu\u00b7gleich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "APPR", "NN", "KON", "NN", "KON", "NN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Jhr wisset zwar, da\u00df englische Geberden", "tokens": ["Ihr", "wis\u00b7set", "zwar", ",", "da\u00df", "eng\u00b7li\u00b7sche", "Ge\u00b7ber\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "$,", "KOUS", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und englisch Wesen blosse Phantasey,", "tokens": ["Und", "eng\u00b7lisch", "We\u00b7sen", "blos\u00b7se", "Phan\u00b7ta\u00b7sey", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "So lange noch der Geist nicht Kercker-frey,", "tokens": ["So", "lan\u00b7ge", "noch", "der", "Geist", "nicht", "Ker\u00b7cker\u00b7frey", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "ART", "NN", "PTKNEG", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Bi\u00df da\u00df wir auch zu Seraphinen werden;", "tokens": ["Bi\u00df", "da\u00df", "wir", "auch", "zu", "Se\u00b7ra\u00b7phi\u00b7nen", "wer\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PPER", "ADV", "APPR", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Drum ist die Eh\u2019 von aussen nicht bewandt,", "tokens": ["Drum", "ist", "die", "Eh'", "von", "aus\u00b7sen", "nicht", "be\u00b7wandt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "ART", "NN", "APPR", "NE", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Als wie der Geist Mariam dort erkannt.", "tokens": ["Als", "wie", "der", "Geist", "Ma\u00b7ri\u00b7am", "dort", "er\u00b7kannt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "ART", "NN", "NE", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Doch wi\u00dft ihr auch, da\u00df eure Hertzen Geister", "tokens": ["Doch", "wi\u00dft", "ihr", "auch", ",", "da\u00df", "eu\u00b7re", "Hert\u00b7zen", "Geis\u00b7ter"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV", "$,", "KOUS", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und Christo v\u00f6llig \u00e4hnlich m\u00fcssen seyn;", "tokens": ["Und", "Chris\u00b7to", "v\u00f6l\u00b7lig", "\u00e4hn\u00b7lich", "m\u00fcs\u00b7sen", "seyn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "ADJD", "ADJD", "VMFIN", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da ma\u00df der Vater durch das Wort hinein,", "tokens": ["Da", "ma\u00df", "der", "Va\u00b7ter", "durch", "das", "Wort", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ART", "NN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Da ist der Heilge Geist der ein\u2019ge Meister,", "tokens": ["Da", "ist", "der", "Heil\u00b7ge", "Geist", "der", "ein'\u00b7ge", "Meis\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und ist der innre Grund voll Geists-Natur,", "tokens": ["Und", "ist", "der", "inn\u00b7re", "Grund", "voll", "Geist\u00b7s\u00b7Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "ADJA", "NN", "ADJD", "NN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.6": {"text": "So heiligt er die \u00e4u\u00dfre Creeatur.", "tokens": ["So", "hei\u00b7ligt", "er", "die", "\u00e4u\u00df\u00b7re", "Cree\u00b7a\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Drum will der HErr, bevor wir ehlich werden,", "tokens": ["Drum", "will", "der", "Herr", ",", "be\u00b7vor", "wir", "eh\u00b7lich", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "wo