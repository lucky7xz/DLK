{"textgrid.poem.50864": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "7. Salome", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Singt mein Schatz wie ein Fink,", "tokens": ["Singt", "mein", "Schatz", "wie", "ein", "Fink", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "KOKOM", "ART", "NN", "$,"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Sing ich Nachtigallensang;", "tokens": ["Sing", "ich", "Nach\u00b7ti\u00b7gal\u00b7len\u00b7sang", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ist mein Liebster ein Luchs,", "tokens": ["Ist", "mein", "Liebs\u00b7ter", "ein", "Luchs", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ART", "NE", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "O so bin ich eine Schlang.", "tokens": ["O", "so", "bin", "ich", "ei\u00b7ne", "Schlang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "VAFIN", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "O ihr Jungfraun im Land,", "tokens": ["O", "ihr", "Jung\u00b7fraun", "im", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "APPRART", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Von dem Berg und \u00fcber See:", "tokens": ["Von", "dem", "Berg", "und", "\u00fc\u00b7ber", "See", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KON", "APPR", "NN", "$."], "meter": "+-+-+