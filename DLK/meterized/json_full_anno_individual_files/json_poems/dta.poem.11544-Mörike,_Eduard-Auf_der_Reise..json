{"dta.poem.11544": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Auf der Reise.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905193969", "language": ["de:0.99"], "booktitle": "M\u00f6rike, Eduard: Gedichte. Stuttgart, 1838."}, "poem": {"stanza.1": {"line.1": {"text": "Zwischen s\u00fc\u00dfem Schmerz,", "tokens": ["Zwi\u00b7schen", "s\u00fc\u00b7\u00dfem", "Schmerz", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Zwischen dumpfem Wohlbehagen", "tokens": ["Zwi\u00b7schen", "dum\u00b7pfem", "Wohl\u00b7be\u00b7ha\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sitz' ich n\u00e4chtlich in dem Reisewagen,", "tokens": ["Sitz'", "ich", "n\u00e4cht\u00b7lich", "in", "dem", "Rei\u00b7se\u00b7wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Lasse mich so weit von dir, mein Herz,", "tokens": ["Las\u00b7se", "mich", "so", "weit", "von", "dir", ",", "mein", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADJD", "APPR", "PPER", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Weit und immer weiter tragen.", "tokens": ["Weit", "und", "im\u00b7mer", "wei\u00b7ter", "tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADV", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Schweigend sitz' ich und allein,", "tokens": ["Schwei\u00b7gend", "sitz'", "ich", "und", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "KON", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ich wiege mich in bunten Tr\u00e4umen,", "tokens": ["Ich", "wie\u00b7ge", "mich", "in", "bun\u00b7ten", "Tr\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Das muntre Posthorn klingt darein,", "tokens": ["Das", "mun\u00b7tre", "Post\u00b7horn", "klingt", "da\u00b7rein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Es tanzt der liebe Mondenschein", "tokens": ["Es", "tanzt", "der", "lie\u00b7be", "Mon\u00b7den\u00b7schein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Nach diesem Ton auf Quellen und auf B\u00e4umen,", "tokens": ["Nach", "die\u00b7sem", "Ton", "auf", "Quel\u00b7len", "und", "auf", "B\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Sogar zu mir durch's enge Fensterlein.", "tokens": ["So\u00b7gar", "zu", "mir", "durch's", "en\u00b7ge", "Fens\u00b7ter\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+++-+-+", "measure": "zehnsilber"}}, "stanza.3": {"line.1": {"text": "Ich w\u00fcnsche mir nun Dies und Das.", "tokens": ["Ich", "w\u00fcn\u00b7sche", "mir", "nun", "Dies", "und", "Das", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PDS", "KON", "PDS", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "O k\u00f6nnt' ich jetzo durch ein Zauberglas", "tokens": ["O", "k\u00f6nnt'", "ich", "jet\u00b7zo", "durch", "ein", "Zau\u00b7ber\u00b7glas"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VMFIN", "PPER", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "In's Goldgewebe deines Traumes blicken!", "tokens": ["In's", "Gold\u00b7ge\u00b7we\u00b7be", "dei\u00b7nes", "Trau\u00b7mes", "bli\u00b7cken", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Vielleicht dann sah' ich wieder mit Entz\u00fccken", "tokens": ["Viel\u00b7leicht", "dann", "sah'", "ich", "wie\u00b7der", "mit", "Ent\u00b7z\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Dich in der Laube wohlbekannt,", "tokens": ["Dich", "in", "der", "Lau\u00b7be", "wohl\u00b7be\u00b7kannt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ich s\u00e4he Genofevens Hand", "tokens": ["Ich", "s\u00e4\u00b7he", "Ge\u00b7no\u00b7fe\u00b7vens", "Hand"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.7": {"text": "Auf deiner Schulter traulich liegen,", "tokens": ["Auf", "dei\u00b7ner", "Schul\u00b7ter", "trau\u00b7lich", "lie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Am Ende s\u00e4h' ich selber mich,", "tokens": ["Am", "En\u00b7de", "s\u00e4h'", "ich", "sel\u00b7ber", "mich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ADV", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Halb keck und halb bescheidentlich,", "tokens": ["Halb", "keck", "und", "halb", "be\u00b7schei\u00b7dent\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "KON", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "An deine holde Wange schmiegen.", "tokens": ["An", "dei\u00b7ne", "hol\u00b7de", "Wan\u00b7ge", "schmie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Doch nein! wie d\u00fcrft' ich auch nur hoffen,", "tokens": ["Doch", "nein", "!", "wie", "d\u00fcrft'", "ich", "auch", "nur", "hof\u00b7fen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKANT", "$.", "PWAV", "VMFIN", "PPER", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Da\u00df jezt mein Schatten bei dir sey!", "tokens": ["Da\u00df", "jezt", "mein", "Schat\u00b7ten", "bei", "dir", "sey", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPOSAT", "NN", "APPR", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Ach, st\u00fcnden deine Tr\u00e4ume f\u00fcr mich offen,", "tokens": ["Ach", ",", "st\u00fcn\u00b7den", "dei\u00b7ne", "Tr\u00e4u\u00b7me", "f\u00fcr", "mich", "of\u00b7fen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VVFIN", "PPOSAT", "NN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Du winktest wohl auch wachend mich herbei!", "tokens": ["Du", "wink\u00b7test", "wohl", "auch", "wa\u00b7chend", "mich", "her\u00b7bei", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ADJD", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}