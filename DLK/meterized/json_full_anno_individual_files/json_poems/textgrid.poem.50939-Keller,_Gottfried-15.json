{"textgrid.poem.50939": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "15", "genre": "verse", "period": "N.A.", "pub_year": 1849, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Weil ich den schwarzen untreu ward", "tokens": ["Weil", "ich", "den", "schwar\u00b7zen", "un\u00b7treu", "ward"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADJA", "ADJD", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und mich zu blauen Augen wandte,", "tokens": ["Und", "mich", "zu", "blau\u00b7en", "Au\u00b7gen", "wand\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Kamst du, zu r\u00e4chen jene, her,", "tokens": ["Kamst", "du", ",", "zu", "r\u00e4\u00b7chen", "je\u00b7ne", ",", "her", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PTKZU", "VVINF", "PDS", "$,", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Du dunkelgl\u00fchende Nachtgesandte!", "tokens": ["Du", "dun\u00b7kel\u00b7gl\u00fc\u00b7hen\u00b7de", "Nacht\u00b7ge\u00b7sand\u00b7te", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Ich sollt auf deiner Augen Grund", "tokens": ["Ich", "sollt", "auf", "dei\u00b7ner", "Au\u00b7gen", "Grund"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Strafe meines Leichtsinns lesen", "tokens": ["Die", "Stra\u00b7fe", "mei\u00b7nes", "Leicht\u00b7sinns", "le\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und schamerr\u00f6tend auch zugleich", "tokens": ["Und", "scha\u00b7mer\u00b7r\u00f6\u00b7tend", "auch", "zu\u00b7gleich"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJD", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der wahren Liebe Glut und Wesen!", "tokens": ["Der", "wah\u00b7ren", "Lie\u00b7be", "Glut", "und", "We\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Der Liebe, die im heiligen Ernst", "tokens": ["Der", "Lie\u00b7be", ",", "die", "im", "hei\u00b7li\u00b7gen", "Ernst"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "APPRART", "ADJA", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Zu lieben denkt und dann zu ", "tokens": ["Zu", "lie\u00b7ben", "denkt", "und", "dann", "zu"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PTKZU", "VVINF", "VVFIN", "KON", "ADV", "APPR"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und deren dunkle Rosen sich", "tokens": ["Und", "de\u00b7ren", "dunk\u00b7le", "Ro\u00b7sen", "sich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PRELAT", "ADJA", "NN", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nur mit dem besten Herzblut f\u00e4rben!", "tokens": ["Nur", "mit", "dem", "bes\u00b7ten", "Herz\u00b7blut", "f\u00e4r\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-