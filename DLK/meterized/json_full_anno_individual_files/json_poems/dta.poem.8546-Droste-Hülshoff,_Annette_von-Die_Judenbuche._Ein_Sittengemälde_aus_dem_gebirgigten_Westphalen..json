{"dta.poem.8546": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Die Judenbuche.  \n Ein Sittengem\u00e4lde aus dem gebirgigten Westphalen.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1860", "urn": "urn:nbn:de:kobv:b4-200905191007", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wo ist die Hand so zart, da\u00df ohne Irren", "tokens": ["Wo", "ist", "die", "Hand", "so", "zart", ",", "da\u00df", "oh\u00b7ne", "Ir\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "ART", "NN", "ADV", "ADJD", "$,", "KOUS", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sie sondern mag beschr\u00e4nkten Hirnes Wirren,", "tokens": ["Sie", "son\u00b7dern", "mag", "be\u00b7schr\u00e4nk\u00b7ten", "Hir\u00b7nes", "Wir\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VMFIN", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So fest, da\u00df ohne Zittern sie den Stein", "tokens": ["So", "fest", ",", "da\u00df", "oh\u00b7ne", "Zit\u00b7tern", "sie", "den", "Stein"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PTKVZ", "$,", "KOUS", "APPR", "NN", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mag schleudern auf ein arm verk\u00fcmmert Sein?", "tokens": ["Mag", "schleu\u00b7dern", "auf", "ein", "arm", "ver\u00b7k\u00fcm\u00b7mert", "Sein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVINF", "APPR", "ART", "ADJD", "VVFIN", "PPOSAT", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wer wagt es, eitlen Blutes Drang zu messen,", "tokens": ["Wer", "wagt", "es", ",", "eit\u00b7len", "Blu\u00b7tes", "Drang", "zu", "mes\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$,", "ADJA", "NN", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Zu w\u00e4gen jedes Wort, das unvergessen", "tokens": ["Zu", "w\u00e4\u00b7gen", "je\u00b7des", "Wort", ",", "das", "un\u00b7ver\u00b7ges\u00b7sen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "VVFIN", "PIAT", "NN", "$,", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "In junge Brust die z\u00e4hen Wurzeln trieb,", "tokens": ["In", "jun\u00b7ge", "Brust", "die", "z\u00e4\u00b7hen", "Wur\u00b7zeln", "trieb", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Des Vorurtheils geheimen Seelendieb?", "tokens": ["Des", "Vor\u00b7urt\u00b7heils", "ge\u00b7hei\u00b7men", "See\u00b7len\u00b7dieb", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Du Gl\u00fccklicher, geboren und gehegt", "tokens": ["Du", "Gl\u00fcck\u00b7li\u00b7cher", ",", "ge\u00b7bo\u00b7ren", "und", "ge\u00b7hegt"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "NN", "$,", "VVPP", "KON", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Im lichten Raum, von frommer Hand gepflegt,", "tokens": ["Im", "lich\u00b7ten", "Raum", ",", "von", "from\u00b7mer", "Hand", "ge\u00b7pflegt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Leg\u2019 hin die ", "tokens": ["Leg'", "hin", "die"], "token_info": ["word", "word", "word"], "pos": ["VVIMP", "ADV", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.12": {"text": "La\u00df ruh\u2019n den Stein \u2014 er trifft dein eignes", "tokens": ["La\u00df", "ruh'n", "den", "Stein", "er", "trifft", "dein", "eig\u00b7nes"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVIMP", "NN", "ART", "NN", "$(", "PPER", "VVFIN", "PPOSAT", "ADJA"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}}}}