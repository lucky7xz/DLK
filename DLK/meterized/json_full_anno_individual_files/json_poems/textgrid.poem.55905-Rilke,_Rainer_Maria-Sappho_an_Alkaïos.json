{"textgrid.poem.55905": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "Sappho an Alka\u00efos", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und was h\u00e4ttest du mir denn zu sagen,", "tokens": ["Und", "was", "h\u00e4t\u00b7test", "du", "mir", "denn", "zu", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VAFIN", "PPER", "PPER", "ADV", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "und was gehst du meine Seele an,", "tokens": ["und", "was", "gehst", "du", "mei\u00b7ne", "See\u00b7le", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VVFIN", "PPER", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "wenn sich deine Augen niederschlagen", "tokens": ["wenn", "sich", "dei\u00b7ne", "Au\u00b7gen", "nie\u00b7der\u00b7schla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "PPOSAT", "NN", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "vor dem nahen Nichtgesagten? Mann,", "tokens": ["vor", "dem", "na\u00b7hen", "Nicht\u00b7ge\u00b7sag\u00b7ten", "?", "Mann", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$.", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "sieh, uns hat das Sagen dieser Dinge", "tokens": ["sieh", ",", "uns", "hat", "das", "Sa\u00b7gen", "die\u00b7ser", "Din\u00b7ge"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "$,", "PPER", "VAFIN", "ART", "NN", "PDAT", "NN"], "meter": "+-+-+-+-