{"dta.poem.9306": {"metadata": {"author": {"name": "Weise, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Das f\u00fcnffte Gespr\u00e4ch.", "genre": "Lyrik; Drama; Prosa", "period": "N.A.", "pub_year": "1701", "urn": "urn:nbn:de:kobv:b4-25043-0", "language": ["de:0.99"], "booktitle": "Weise, Christian: \u00dcberfl\u00fc\u00dfige Gedancken Der gr\u00fcnenden jugend. Leipzig, 1701."}, "poem": {"stanza.1": {"line.1": {"text": "Was hat der unversch\u00e4mte neid", "tokens": ["Was", "hat", "der", "un\u00b7ver\u00b7sch\u00e4m\u00b7te", "neid"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vor hertzeleid", "tokens": ["Vor", "hert\u00b7ze\u00b7leid"], "token_info": ["word", "word"], "pos": ["APPR", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "In unsre freundschafft ausgestreut?", "tokens": ["In", "uns\u00b7re", "freund\u00b7schafft", "aus\u00b7ge\u00b7streut", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Was soll der kalte gifft?", "tokens": ["Was", "soll", "der", "kal\u00b7te", "gifft", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der unsre treu doch nur von aussen trifft.", "tokens": ["Der", "uns\u00b7re", "treu", "doch", "nur", "von", "aus\u00b7sen", "trifft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJD", "ADV", "ADV", "APPR", "NE", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Die hertzen sind von diamant/", "tokens": ["Die", "hert\u00b7zen", "sind", "von", "di\u00b7a\u00b7mant", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "NE", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.7": {"text": "Und das geschlo\u00dfne liebes-band", "tokens": ["Und", "das", "ge\u00b7schlo\u00df\u00b7ne", "lie\u00b7bes\u00b7band"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Hegt keinen unbestand.", "tokens": ["Hegt", "kei\u00b7nen", "un\u00b7be\u00b7stand", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Drum/ je mehr man uns betr\u00fcbt/", "tokens": ["Drum", "/", "je", "mehr", "man", "uns", "be\u00b7tr\u00fcbt", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$(", "ADV", "ADV", "PIS", "PPER", "VVPP", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Desto mehr sind wir verliebt.", "tokens": ["Des\u00b7to", "mehr", "sind", "wir", "ver\u00b7liebt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "2. Wenn gleich der wind mit seiner wut", "tokens": ["Wenn", "gleich", "der", "wind", "mit", "sei\u00b7ner", "wut"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In unsre glut", "tokens": ["In", "uns\u00b7re", "glut"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Den allerst\u00e4rcksten einfall thut/", "tokens": ["Den", "al\u00b7ler\u00b7st\u00e4rcks\u00b7ten", "ein\u00b7fall", "thut", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So ist es nicht gethan/", "tokens": ["So", "ist", "es", "nicht", "ge\u00b7than", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PTKNEG", "VVPP", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Er bl\u00e4set nur die flammen besser an.", "tokens": ["Er", "bl\u00e4\u00b7set", "nur", "die", "flam\u00b7men", "bes\u00b7ser", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "VVFIN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Drum wer das feuer nicht erkennt/", "tokens": ["Drum", "wer", "das", "feu\u00b7er", "nicht", "er\u00b7kennt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PWS", "ART", "NN", "PTKNEG", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und sich nur an die finger brennt/", "tokens": ["Und", "sich", "nur", "an", "die", "fin\u00b7ger", "brennt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "ADV", "APPR", "ART", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der ist doch gantz verblendt;", "tokens": ["Der", "ist", "doch", "gantz", "ver\u00b7blendt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Denn er unterscheidet nicht", "tokens": ["Denn", "er", "un\u00b7ter\u00b7schei\u00b7det", "nicht"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PTKNEG"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Liebes-schein und falsches licht.", "tokens": ["Lie\u00b7bes\u00b7schein", "und", "fal\u00b7sches", "licht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "3. Indessen allerliebstes kind/", "tokens": ["In\u00b7des\u00b7sen", "al\u00b7ler\u00b7liebs\u00b7tes", "kind", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bleib gut gesinnt/", "tokens": ["Bleib", "gut", "ge\u00b7sinnt", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJD", "VVPP", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Und achte keinen falschen wind/", "tokens": ["Und", "ach\u00b7te", "kei\u00b7nen", "fal\u00b7schen", "wind", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "PIAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die treu soll unser schein/", "tokens": ["Die", "treu", "soll", "un\u00b7ser", "schein", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VMFIN", "PPOSAT", "ADJD", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die keusche pflicht des hertzens losung seyn.", "tokens": ["Die", "keu\u00b7sche", "pflicht", "des", "hert\u00b7zens", "lo\u00b7sung", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Verg\u00f6nnt uns ja der falsche neid/", "tokens": ["Ver\u00b7g\u00f6nnt", "uns", "ja", "der", "fal\u00b7sche", "neid", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Nicht \u00fcbrig grosse fr\u00f6ligkeit/", "tokens": ["Nicht", "\u00fcb\u00b7rig", "gros\u00b7se", "fr\u00f6\u00b7lig\u00b7keit", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "So warte nur die zeit.", "tokens": ["So", "war\u00b7te", "nur", "die", "zeit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Denn der tag k\u00f6mmt bald heran/", "tokens": ["Denn", "der", "tag", "k\u00f6mmt", "bald", "he\u00b7ran", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ADV", "PTKVZ", "$("], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.10": {"text": "Da man rosen brechen kan.", "tokens": ["Da", "man", "ro\u00b7sen", "bre\u00b7chen", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVINF", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}