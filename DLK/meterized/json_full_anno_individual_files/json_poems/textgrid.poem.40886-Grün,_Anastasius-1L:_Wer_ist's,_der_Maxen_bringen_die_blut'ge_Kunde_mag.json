{"textgrid.poem.40886": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wer ist's, der Maxen bringen die blut'ge Kunde mag", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer ist's, der Maxen bringen die blut'ge Kunde mag", "tokens": ["Wer", "ist's", ",", "der", "Ma\u00b7xen", "brin\u00b7gen", "die", "blut'\u00b7ge", "Kun\u00b7de", "mag"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "$,", "ART", "NN", "VVFIN", "ART", "ADJA", "NN", "VMFIN"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Von all der Seinen Tode am unheilschwangern Tag?", "tokens": ["Von", "all", "der", "Sei\u00b7nen", "To\u00b7de", "am", "un\u00b7heil\u00b7schwan\u00b7gern", "Tag", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ART", "PPOSAT", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Pirkheimer", "tokens": ["Pirk\u00b7hei\u00b7mer"], "token_info": ["word"], "pos": ["NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Mit Schwert und scharfer Feder f\u00fcr Wahrheit, Recht und Licht.", "tokens": ["Mit", "Schwert", "und", "schar\u00b7fer", "Fe\u00b7der", "f\u00fcr", "Wahr\u00b7heit", ",", "Recht", "und", "Licht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "ADJA", "NN", "APPR", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Wie nahm der F\u00fcrst die Kunde? Wohl war's ihm herbe Pein?", "tokens": ["Wie", "nahm", "der", "F\u00fcrst", "die", "Kun\u00b7de", "?", "Wohl", "wa\u00b7r's", "ihm", "her\u00b7be", "Pein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "ART", "NN", "$.", "ADV", "VAFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "Wohl wird er weinend klagen, verzweifeln gar? \u2013 O nein!", "tokens": ["Wohl", "wird", "er", "wei\u00b7nend", "kla\u00b7gen", ",", "ver\u00b7zwei\u00b7feln", "gar", "?", "\u2013", "O", "nein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "VVINF", "$,", "VVFIN", "ADV", "$.", "$(", "NE", "PTKANT", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Die Eule kreischt wohl wimmernd, wenn sie der Pfeil durchdringt,", "tokens": ["Die", "Eu\u00b7le", "kreischt", "wohl", "wim\u00b7mernd", ",", "wenn", "sie", "der", "Pfeil", "durch\u00b7dringt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "VVPP", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Der K\u00f6nigsschwan, auch todtwund, der \u00e4chzt nicht, sondern singt.", "tokens": ["Der", "K\u00f6\u00b7nigs\u00b7schwan", ",", "auch", "todt\u00b7wund", ",", "der", "\u00e4chzt", "nicht", ",", "son\u00b7dern", "singt", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "ADJD", "$,", "PRELS", "VVFIN", "PTKNEG", "$,", "KON", "VVFIN", "$."], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "Zu Kostniz stieg der K\u00f6nig zu Schiff um Mitternacht,", "tokens": ["Zu", "Kost\u00b7niz", "stieg", "der", "K\u00f6\u00b7nig", "zu", "Schiff", "um", "Mit\u00b7ter\u00b7nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVFIN", "ART", "NN", "APPR", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Vor ihm der See so ruhig, ob ihm der Sterne Pracht!", "tokens": ["Vor", "ihm", "der", "See", "so", "ru\u00b7hig", ",", "ob", "ihm", "der", "Ster\u00b7ne", "Pracht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "ADV", "ADJD", "$,", "KOUS", "PPER", "ART", "NN", "NN", "$."], "meter": "+--+--+-+-+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Der Mond blickt sanft ins Aug' ihm, als spr\u00e4ch' er ihm aus Herz:", "tokens": ["Der", "Mond", "blickt", "sanft", "ins", "Aug'", "ihm", ",", "als", "spr\u00e4ch'", "er", "ihm", "aus", "Herz", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "APPRART", "NN", "PPER", "$,", "KOUS", "VVFIN", "PPER", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Ich habe schon belauschet viel gr\u00f6\u00dfern bittrern Schmerz!", "tokens": ["Ich", "ha\u00b7be", "schon", "be\u00b7lau\u00b7schet", "viel", "gr\u00f6\u00b7\u00dfern", "bit\u00b7trern", "Schmerz", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "VVFIN", "PIAT", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.4": {"line.1": {"text": "Die Wellen spielen ums Schifflein, als fl\u00fcsterten sie ihm zu:", "tokens": ["Die", "Wel\u00b7len", "spie\u00b7len", "ums", "Schif\u00b7flein", ",", "als", "fl\u00fcs\u00b7ter\u00b7ten", "sie", "ihm", "zu", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$,", "KOUS", "VVFIN", "PPER", "PPER", "PTKVZ", "$."], "meter": "-+-+--+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Wir trugen schon so Manchen, der elender als du!", "tokens": ["Wir", "tru\u00b7gen", "schon", "so", "Man\u00b7chen", ",", "der", "e\u00b7len\u00b7der", "als", "du", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "PIS", "$,", "ART", "ADJA", "KOUS", "PPER", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Ums Haupt ihm kosen die L\u00fcfte, als weht' es im Schmeichelwind:", "tokens": ["Ums", "Haupt", "ihm", "ko\u00b7sen", "die", "L\u00fcf\u00b7te", ",", "als", "weht'", "es", "im", "Schmei\u00b7chel\u00b7wind", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "PPER", "VVFIN", "ART", "NN", "$,", "KOUS", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+--+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Wir haben schon getrocknet manch herbe Thr\u00e4ne lind!", "tokens": ["Wir", "ha\u00b7ben", "schon", "ge\u00b7trock\u00b7net", "manch", "her\u00b7be", "Thr\u00e4\u00b7ne", "lind", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "PIAT", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.5": {"line.1": {"text": "Und als der F\u00fcrst des Morgens zu Lindau stieg ans Land,", "tokens": ["Und", "als", "der", "F\u00fcrst", "des", "Mor\u00b7gens", "zu", "Lin\u00b7dau", "stieg", "ans", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "ART", "ADV", "APPR", "NE", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Da schmiegte sich das Fr\u00fchroth um seiner Wangen Rand,", "tokens": ["Da", "schmieg\u00b7te", "sich", "das", "Fr\u00fch\u00b7roth", "um", "sei\u00b7ner", "Wan\u00b7gen", "Rand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Als rief's zu ihm hernieder vom hohen Aetherthron:", "tokens": ["Als", "rie\u00b7f's", "zu", "ihm", "her\u00b7nie\u00b7der", "vom", "ho\u00b7hen", "A\u00b7e\u00b7ther\u00b7thron", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "APPR", "PPER", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "----+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Ich habe wieder ger\u00f6thet viel bleiche Wangen schon!", "tokens": ["Ich", "ha\u00b7be", "wie\u00b7der", "ge\u00b7r\u00f6\u00b7thet", "viel", "blei\u00b7che", "Wan\u00b7gen", "schon", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "VVFIN", "PIAT", "ADJA", "NN", "ADV", "$."], "meter": "-+-+--+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.6": {"line.1": {"text": "So hell und licht wie Mondschein, und wie die L\u00fcfte klar,", "tokens": ["So", "hell", "und", "licht", "wie", "Mond\u00b7schein", ",", "und", "wie", "die", "L\u00fcf\u00b7te", "klar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "NN", "KOKOM", "NN", "$,", "KON", "PWAV", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und wie der See so ruhig nun Maxens Seele war;", "tokens": ["Und", "wie", "der", "See", "so", "ru\u00b7hig", "nun", "Ma\u00b7xens", "See\u00b7le", "war", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "ADV", "ADJD", "ADV", "NE", "NN", "VAFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "In seinem Herzen tagt es wie lichte Morgenstund',", "tokens": ["In", "sei\u00b7nem", "Her\u00b7zen", "tagt", "es", "wie", "lich\u00b7te", "Mor\u00b7gen\u00b7stund'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "KOKOM", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Er neigt sein Haupt am Strande und k\u00fc\u00dft den deutschen Grund.", "tokens": ["Er", "neigt", "sein", "Haupt", "am", "Stran\u00b7de", "und", "k\u00fc\u00dft", "den", "deut\u00b7schen", "Grund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPRART", "NN", "KON", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.7": {"line.1": {"text": "Vor sich die Schweizerberge sieht glanzverkl\u00e4rt er stehn;", "tokens": ["Vor", "sich", "die", "Schwei\u00b7zer\u00b7ber\u00b7ge", "sieht", "glanz\u00b7ver\u00b7kl\u00e4rt", "er", "stehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "ART", "NN", "VVFIN", "VVFIN", "PPER", "VVINF", "$."], "meter": "+--+-+-+--+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "So hat manch F\u00fcrst und S\u00e4nger sie seither noch gesehn.", "tokens": ["So", "hat", "manch", "F\u00fcrst", "und", "S\u00e4n\u00b7ger", "sie", "sei\u00b7ther", "noch", "ge\u00b7sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIAT", "NN", "KON", "NN", "PPER", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Heil jedem edlen F\u00fcrsten, Heil seinem Volk auch dann,", "tokens": ["Heil", "je\u00b7dem", "ed\u00b7len", "F\u00fcrs\u00b7ten", ",", "Heil", "sei\u00b7nem", "Volk", "auch", "dann", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "ADJA", "NN", "$,", "NN", "PPOSAT", "NN", "ADV", "ADV", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Wenn er der Freiheit ruhig ins Antlitz schauen kann!", "tokens": ["Wenn", "er", "der", "Frei\u00b7heit", "ru\u00b7hig", "ins", "Ant\u00b7litz", "schau\u00b7en", "kann", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADJD", "APPRART", "NN", "VVINF", "VMFIN", "$."], "meter": "---+--+-+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.8": {"line.1": {"text": "Wo aber sind die Sieger, die Schweizer hingeflohn?", "tokens": ["Wo", "a\u00b7ber", "sind", "die", "Sie\u00b7ger", ",", "die", "Schwei\u00b7zer", "hin\u00b7ge\u00b7flohn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VAFIN", "ART", "NN", "$,", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Wo lagern jetzt die Helden? was ward ihr Siegeslohn?", "tokens": ["Wo", "la\u00b7gern", "jetzt", "die", "Hel\u00b7den", "?", "was", "ward", "ihr", "Sie\u00b7ges\u00b7lohn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ADV", "ART", "NN", "$.", "PWS", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Wo bleibt das Lied, das brausend dem Preis der Freiheit brennt?", "tokens": ["Wo", "bleibt", "das", "Lied", ",", "das", "brau\u00b7send", "dem", "Preis", "der", "Frei\u00b7heit", "brennt", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "$,", "PRELS", "ADJD", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Wo bauten sich die Tapfern des Ruhmes Monument?", "tokens": ["Wo", "bau\u00b7ten", "sich", "die", "Tap\u00b7fern", "des", "Ruh\u00b7mes", "Mo\u00b7nu\u00b7ment", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PRF", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.9": {"line.1": {"text": "Seht dort den melkenden Sennen, den Fischer hier im Kahn,", "tokens": ["Seht", "dort", "den", "mel\u00b7ken\u00b7den", "Sen\u00b7nen", ",", "den", "Fi\u00b7scher", "hier", "im", "Kahn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "$,", "ART", "NN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+--+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Den Pfl\u00fcger und den Schnitter, den J\u00e4ger auf felsiger Bahn;", "tokens": ["Den", "Pfl\u00fc\u00b7ger", "und", "den", "Schnit\u00b7ter", ",", "den", "J\u00e4\u00b7ger", "auf", "fel\u00b7si\u00b7ger", "Bahn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.3": {"text": "Ihr braucht nicht weit zu schauen, ihr seht die Helden schon!", "tokens": ["Ihr", "braucht", "nicht", "weit", "zu", "schau\u00b7en", ",", "ihr", "seht", "die", "Hel\u00b7den", "schon", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADJD", "PTKZU", "VVINF", "$,", "PPER", "VVFIN", "ART", "NN", "ADV", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Rings freie Luft und Erde, das ist ihr Siegeslohn.", "tokens": ["Rings", "frei\u00b7e", "Luft", "und", "Er\u00b7de", ",", "das", "ist", "ihr", "Sie\u00b7ges\u00b7lohn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "KON", "NN", "$,", "PDS", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.10": {"line.1": {"text": "Horch, Becher klingen beim Mahle, die B\u00fcchse kracht im Wald,", "tokens": ["Horch", ",", "Be\u00b7cher", "klin\u00b7gen", "beim", "Mah\u00b7le", ",", "die", "B\u00fcch\u00b7se", "kracht", "im", "Wald", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "VVFIN", "APPRART", "NN", "$,", "ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "+--+--+--+-+-+", "measure": "dactylic.tri.plus"}, "line.2": {"text": "Die Sensen klirren im Thale, des Aelplers Horn erschallt,", "tokens": ["Die", "Sen\u00b7sen", "klir\u00b7ren", "im", "Tha\u00b7le", ",", "des", "A\u00b7el\u00b7plers", "Horn", "er\u00b7schallt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$,", "ART", "NN", "NE", "VVFIN", "$,"], "meter": "-+-+--+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.3": {"text": "Dort L\u00e4uten der Alpenheerden, fern Abendglockenget\u00f6n!", "tokens": ["Dort", "L\u00e4u\u00b7ten", "der", "Al\u00b7pen\u00b7heer\u00b7den", ",", "fern", "A\u00b7bend\u00b7glo\u00b7cken\u00b7ge\u00b7t\u00f6n", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "NN", "ART", "NN", "$,", "ADJD", "NN", "$."], "meter": "-+-+--+--+-+--+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Das ist das Lied der Freiheit! Klang je ein Lied so sch\u00f6n?", "tokens": ["Das", "ist", "das", "Lied", "der", "Frei\u00b7heit", "!", "Klang", "je", "ein", "Lied", "so", "sch\u00f6n", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "ART", "NN", "$.", "NN", "ADV", "ART", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.11": {"line.1": {"text": "Muth, Wahrheit, Treu' und Liebe und Einfalt, Glaub' und Recht,", "tokens": ["Muth", ",", "Wahr\u00b7heit", ",", "Treu'", "und", "Lie\u00b7be", "und", "Ein\u00b7falt", ",", "Glaub'", "und", "Recht", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "KON", "NN", "KON", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Das ist die heil'ge Sieben im lichten Farbengeschlecht,", "tokens": ["Das", "ist", "die", "heil'\u00b7ge", "Sie\u00b7ben", "im", "lich\u00b7ten", "Far\u00b7ben\u00b7ge\u00b7schlecht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+--+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Das ist der Regenbogen, de\u00df Leuchten ewig brennt", "tokens": ["Das", "ist", "der", "Re\u00b7gen\u00b7bo\u00b7gen", ",", "de\u00df", "Leuch\u00b7ten", "e\u00b7wig", "brennt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "ART", "NN", "ADJD", "VVFIN"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Hoch \u00fcber den Schweizerbergen als Freiheitsmonument!", "tokens": ["Hoch", "\u00fc\u00b7ber", "den", "Schwei\u00b7zer\u00b7ber\u00b7gen", "als", "Frei\u00b7heits\u00b7mo\u00b7nu\u00b7ment", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "KOUS", "NN", "$."], "meter": "-+--+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.12": {"line.1": {"text": "Wer ist's, der Maxen bringen die blut'ge Kunde mag", "tokens": ["Wer", "ist's", ",", "der", "Ma\u00b7xen", "brin\u00b7gen", "die", "blut'\u00b7ge", "Kun\u00b7de", "mag"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "$,", "ART", "NN", "VVFIN", "ART", "ADJA", "NN", "VMFIN"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Von all der Seinen Tode am unheilschwangern Tag?", "tokens": ["Von", "all", "der", "Sei\u00b7nen", "To\u00b7de", "am", "un\u00b7heil\u00b7schwan\u00b7gern", "Tag", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ART", "PPOSAT", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Pirkheimer", "tokens": ["Pirk\u00b7hei\u00b7mer"], "token_info": ["word"], "pos": ["NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Mit Schwert und scharfer Feder f\u00fcr Wahrheit, Recht und Licht.", "tokens": ["Mit", "Schwert", "und", "schar\u00b7fer", "Fe\u00b7der", "f\u00fcr", "Wahr\u00b7heit", ",", "Recht", "und", "Licht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "ADJA", "NN", "APPR", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.13": {"line.1": {"text": "Wie nahm der F\u00fcrst die Kunde? Wohl war's ihm herbe Pein?", "tokens": ["Wie", "nahm", "der", "F\u00fcrst", "die", "Kun\u00b7de", "?", "Wohl", "wa\u00b7r's", "ihm", "her\u00b7be", "Pein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "ART", "NN", "$.", "ADV", "VAFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "Wohl wird er weinend klagen, verzweifeln gar? \u2013 O nein!", "tokens": ["Wohl", "wird", "er", "wei\u00b7nend", "kla\u00b7gen", ",", "ver\u00b7zwei\u00b7feln", "gar", "?", "\u2013", "O", "nein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "VVINF", "$,", "VVFIN", "ADV", "$.", "$(", "NE", "PTKANT", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Die Eule kreischt wohl wimmernd, wenn sie der Pfeil durchdringt,", "tokens": ["Die", "Eu\u00b7le", "kreischt", "wohl", "wim\u00b7mernd", ",", "wenn", "sie", "der", "Pfeil", "durch\u00b7dringt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "VVPP", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Der K\u00f6nigsschwan, auch todtwund, der \u00e4chzt nicht, sondern singt.", "tokens": ["Der", "K\u00f6\u00b7nigs\u00b7schwan", ",", "auch", "todt\u00b7wund", ",", "der", "\u00e4chzt", "nicht", ",", "son\u00b7dern", "singt", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "ADJD", "$,", "PRELS", "VVFIN", "PTKNEG", "$,", "KON", "VVFIN", "$."], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.14": {"line.1": {"text": "Zu Kostniz stieg der K\u00f6nig zu Schiff um Mitternacht,", "tokens": ["Zu", "Kost\u00b7niz", "stieg", "der", "K\u00f6\u00b7nig", "zu", "Schiff", "um", "Mit\u00b7ter\u00b7nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVFIN", "ART", "NN", "APPR", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Vor ihm der See so ruhig, ob ihm der Sterne Pracht!", "tokens": ["Vor", "ihm", "der", "See", "so", "ru\u00b7hig", ",", "ob", "ihm", "der", "Ster\u00b7ne", "Pracht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "ADV", "ADJD", "$,", "KOUS", "PPER", "ART", "NN", "NN", "$."], "meter": "+--+--+-+-+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Der Mond blickt sanft ins Aug' ihm, als spr\u00e4ch' er ihm aus Herz:", "tokens": ["Der", "Mond", "blickt", "sanft", "ins", "Aug'", "ihm", ",", "als", "spr\u00e4ch'", "er", "ihm", "aus", "Herz", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "APPRART", "NN", "PPER", "$,", "KOUS", "VVFIN", "PPER", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Ich habe schon belauschet viel gr\u00f6\u00dfern bittrern Schmerz!", "tokens": ["Ich", "ha\u00b7be", "schon", "be\u00b7lau\u00b7schet", "viel", "gr\u00f6\u00b7\u00dfern", "bit\u00b7trern", "Schmerz", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "VVFIN", "PIAT", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.15": {"line.1": {"text": "Die Wellen spielen ums Schifflein, als fl\u00fcsterten sie ihm zu:", "tokens": ["Die", "Wel\u00b7len", "spie\u00b7len", "ums", "Schif\u00b7flein", ",", "als", "fl\u00fcs\u00b7ter\u00b7ten", "sie", "ihm", "zu", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$,", "KOUS", "VVFIN", "PPER", "PPER", "PTKVZ", "$."], "meter": "-+-+--+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Wir trugen schon so Manchen, der elender als du!", "tokens": ["Wir", "tru\u00b7gen", "schon", "so", "Man\u00b7chen", ",", "der", "e\u00b7len\u00b7der", "als", "du", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "PIS", "$,", "ART", "ADJA", "KOUS", "PPER", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Ums Haupt ihm kosen die L\u00fcfte, als weht' es im Schmeichelwind:", "tokens": ["Ums", "Haupt", "ihm", "ko\u00b7sen", "die", "L\u00fcf\u00b7te", ",", "als", "weht'", "es", "im", "Schmei\u00b7chel\u00b7wind", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "PPER", "VVFIN", "ART", "NN", "$,", "KOUS", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+--+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Wir haben schon getrocknet manch herbe Thr\u00e4ne lind!", "tokens": ["Wir", "ha\u00b7ben", "schon", "ge\u00b7trock\u00b7net", "manch", "her\u00b7be", "Thr\u00e4\u00b7ne", "lind", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "PIAT", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.16": {"line.1": {"text": "Und als der F\u00fcrst des Morgens zu Lindau stieg ans Land,", "tokens": ["Und", "als", "der", "F\u00fcrst", "des", "Mor\u00b7gens", "zu", "Lin\u00b7dau", "stieg", "ans", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "ART", "ADV", "APPR", "NE", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Da schmiegte sich das Fr\u00fchroth um seiner Wangen Rand,", "tokens": ["Da", "schmieg\u00b7te", "sich", "das", "Fr\u00fch\u00b7roth", "um", "sei\u00b7ner", "Wan\u00b7gen", "Rand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Als rief's zu ihm hernieder vom hohen Aetherthron:", "tokens": ["Als", "rie\u00b7f's", "zu", "ihm", "her\u00b7nie\u00b7der", "vom", "ho\u00b7hen", "A\u00b7e\u00b7ther\u00b7thron", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "APPR", "PPER", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "----+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Ich habe wieder ger\u00f6thet viel bleiche Wangen schon!", "tokens": ["Ich", "ha\u00b7be", "wie\u00b7der", "ge\u00b7r\u00f6\u00b7thet", "viel", "blei\u00b7che", "Wan\u00b7gen", "schon", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "VVFIN", "PIAT", "ADJA", "NN", "ADV", "$."], "meter": "-+-+--+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.17": {"line.1": {"text": "So hell und licht wie Mondschein, und wie die L\u00fcfte klar,", "tokens": ["So", "hell", "und", "licht", "wie", "Mond\u00b7schein", ",", "und", "wie", "die", "L\u00fcf\u00b7te", "klar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "NN", "KOKOM", "NN", "$,", "KON", "PWAV", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und wie der See so ruhig nun Maxens Seele war;", "tokens": ["Und", "wie", "der", "See", "so", "ru\u00b7hig", "nun", "Ma\u00b7xens", "See\u00b7le", "war", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "ADV", "ADJD", "ADV", "NE", "NN", "VAFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "In seinem Herzen tagt es wie lichte Morgenstund',", "tokens": ["In", "sei\u00b7nem", "Her\u00b7zen", "tagt", "es", "wie", "lich\u00b7te", "Mor\u00b7gen\u00b7stund'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "KOKOM", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Er neigt sein Haupt am Strande und k\u00fc\u00dft den deutschen Grund.", "tokens": ["Er", "neigt", "sein", "Haupt", "am", "Stran\u00b7de", "und", "k\u00fc\u00dft", "den", "deut\u00b7schen", "Grund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPRART", "NN", "KON", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.18": {"line.1": {"text": "Vor sich die Schweizerberge sieht glanzverkl\u0