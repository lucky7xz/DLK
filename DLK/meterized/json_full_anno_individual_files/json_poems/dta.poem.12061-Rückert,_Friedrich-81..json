{"dta.poem.12061": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "81.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Schwer ist im Wechselnden zu sehn ein Bleibendes,", "tokens": ["Schwer", "ist", "im", "Wech\u00b7seln\u00b7den", "zu", "sehn", "ein", "Blei\u00b7ben\u00b7des", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "APPRART", "NN", "PTKZU", "VVINF", "ART", "NN", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Im Umgetriebenen ein ruhig Treibendes.", "tokens": ["Im", "Um\u00b7ge\u00b7trie\u00b7be\u00b7nen", "ein", "ru\u00b7hig", "Trei\u00b7ben\u00b7des", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJD", "NN", "$."], "meter": "-+-+---+-+--", "measure": "unknown.measure.tetra"}}, "stanza.2": {"line.1": {"text": "Von au\u00dfen ist es schwer, und schwerer noch von innen,", "tokens": ["Von", "au\u00b7\u00dfen", "ist", "es", "schwer", ",", "und", "schwe\u00b7rer", "noch", "von", "in\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VAFIN", "PPER", "ADJD", "$,", "KON", "ADJD", "ADV", "APPR", "ADV", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wo Bild in Bild wie Wog' in Woge scheint zu rinnen.", "tokens": ["Wo", "Bild", "in", "Bild", "wie", "Wo\u00b7g'", "in", "Wo\u00b7ge", "scheint", "zu", "rin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "NN", "KOKOM", "NN", "APPR", "NN", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "Liegts an den Dingen, liegt an dir nur das Gebrechen,", "tokens": ["Liegts", "an", "den", "Din\u00b7gen", ",", "liegt", "an", "dir", "nur", "das", "Ge\u00b7bre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,", "VVFIN", "APPR", "PPER", "ADV", "ART", "NN", "$,"], "meter": "+--+-+--+--+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Da\u00df immer anders dich die \u00e4u\u00dfern Ding' ansprechen?", "tokens": ["Da\u00df", "im\u00b7mer", "an\u00b7ders", "dich", "die", "\u00e4u\u00b7\u00dfern", "Ding'", "an\u00b7spre\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADV", "PPER", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Sie geben Antwort, wie du fragst, und anders nicht;", "tokens": ["Sie", "ge\u00b7ben", "Ant\u00b7wort", ",", "wie", "du", "fragst", ",", "und", "an\u00b7ders", "nicht", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$,", "PWAV", "PPER", "VVFIN", "$,", "KON", "ADV", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Drum liegt es wol am Geist, wie er die Ding' anspricht.", "tokens": ["Drum", "liegt", "es", "wol", "am", "Geist", ",", "wie", "er", "die", "Ding'", "an\u00b7spricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$,", "PWAV", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Darum ists Noth, in dir dich selber zu vereinen,", "tokens": ["Da\u00b7rum", "ists", "Noth", ",", "in", "dir", "dich", "sel\u00b7ber", "zu", "ver\u00b7ei\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "NN", "$,", "APPR", "PPER", "PRF", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Um nicht in jedem Nu ein andrer dir zu scheinen;", "tokens": ["Um", "nicht", "in", "je\u00b7dem", "Nu", "ein", "an\u00b7drer", "dir", "zu", "schei\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PTKNEG", "APPR", "PIS", "ADV", "ART", "ADJA", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Kein Spiegel und kein Wachs, darein sich wechselnd dr\u00fcckt", "tokens": ["Kein", "Spie\u00b7gel", "und", "kein", "Wachs", ",", "da\u00b7rein", "sich", "wech\u00b7selnd", "dr\u00fcckt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "KON", "PIAT", "NN", "$,", "PAV", "PRF", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dies Bild und jenes, das verunziert oder schm\u00fcckt;", "tokens": ["Dies", "Bild", "und", "je\u00b7nes", ",", "das", "ver\u00b7un\u00b7ziert", "o\u00b7der", "schm\u00fcckt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "KON", "PDS", "$,", "PRELS", "VVPP", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "In der Vorstellungen, in der Eindr\u00fccke Schwanken", "tokens": ["In", "der", "Vor\u00b7stel\u00b7lun\u00b7gen", ",", "in", "der", "Ein\u00b7dr\u00fc\u00b7cke", "Schwan\u00b7ken"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "NN"], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Zu f\u00fchlen einen Kern feststehender Gedanken;", "tokens": ["Zu", "f\u00fch\u00b7len", "ei\u00b7nen", "Kern", "fest\u00b7ste\u00b7hen\u00b7der", "Ge\u00b7dan\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Da\u00df du derselbe heut, in andrer Form verborgen,", "tokens": ["Da\u00df", "du", "der\u00b7sel\u00b7be", "heut", ",", "in", "an\u00b7drer", "Form", "ver\u00b7bor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PDAT", "ADV", "$,", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bist, der du gestern warst, und der du seyn wirst morgen.", "tokens": ["Bist", ",", "der", "du", "ge\u00b7stern", "warst", ",", "und", "der", "du", "seyn", "wirst", "mor\u00b7gen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "PRELS", "PPER", "ADV", "VAFIN", "$,", "KON", "ART", "PPER", "VAINF", "VAFIN", "ADV", "$."], "meter": "-+---+-+-+-+-", "measure": "dactylic.init"}}}}}