{"dta.poem.10293": {"metadata": {"author": {"name": "Liliencron, Detlev von", "birth": "N.A.", "death": "N.A."}, "title": "Zwei Sterbende.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1883", "urn": "urn:nbn:de:kobv:b4-200905197184", "language": ["de:0.99"], "booktitle": "Liliencron, Detlev von: Adjutantenritte und andere Gedichte. Leipzig, [1883]."}, "poem": {"stanza.1": {"line.1": {"text": "Der eine hatte Geld und just genug,             ", "tokens": ["Der", "ei\u00b7ne", "hat\u00b7te", "Geld", "und", "just", "ge\u00b7nug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VAFIN", "NN", "KON", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Des Lebens Schwere ruhig zu ertragen,", "tokens": ["Des", "Le\u00b7bens", "Schwe\u00b7re", "ru\u00b7hig", "zu", "er\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nach keinem Menschen braucht Mylord zu fragen,", "tokens": ["Nach", "kei\u00b7nem", "Men\u00b7schen", "braucht", "My\u00b7lord", "zu", "fra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und keines Hospodaren Rock er trug.", "tokens": ["Und", "kei\u00b7nes", "Hos\u00b7po\u00b7da\u00b7ren", "Rock", "er", "trug", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Der andre trieb im Schwei\u00dfe seinen Pflug,", "tokens": ["Der", "and\u00b7re", "trieb", "im", "Schwei\u00b7\u00dfe", "sei\u00b7nen", "Pflug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "APPRART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Hoch wie die Wolken sah das Gl\u00fcck er jagen,", "tokens": ["Hoch", "wie", "die", "Wol\u00b7ken", "sah", "das", "Gl\u00fcck", "er", "ja\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "ART", "NN", "VVFIN", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Auf jeder Rennbahn blieb zur\u00fcck sein Wagen,", "tokens": ["Auf", "je\u00b7der", "Renn\u00b7bahn", "blieb", "zu\u00b7r\u00fcck", "sein", "Wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "PTKVZ", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Statt Weines fand er nur den Wasserkrug.", "tokens": ["Statt", "Wei\u00b7nes", "fand", "er", "nur", "den", "Was\u00b7ser\u00b7krug", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VVFIN", "PPER", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Der erste sprach, als ihn der Tod umfing,", "tokens": ["Der", "ers\u00b7te", "sprach", ",", "als", "ihn", "der", "Tod", "um\u00b7fing", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und ihm den schwarzen Mantel \u00fcberhing:", "tokens": ["Und", "ihm", "den", "schwar\u00b7zen", "Man\u00b7tel", "\u00fc\u00b7ber\u00b7hing", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ich sterbe gern, es rufen mich die Sterne.", "tokens": ["Ich", "ster\u00b7be", "gern", ",", "es", "ru\u00b7fen", "mich", "die", "Ster\u00b7ne", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PPER", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Der zweite rief, als er die Augen schlo\u00df,", "tokens": ["Der", "zwei\u00b7te", "rief", ",", "als", "er", "die", "Au\u00b7gen", "schlo\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und ihn die tr\u00e4ge Welle \u00fcberflo\u00df:", "tokens": ["Und", "ihn", "die", "tr\u00e4\u00b7ge", "Wel\u00b7le", "\u00fc\u00b7ber\u00b7flo\u00df", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Kein Eden will ich, ach, wie sterb\u2019 ich gerne.", "tokens": ["Kein", "E\u00b7den", "will", "ich", ",", "ach", ",", "wie", "sterb'", "ich", "ger\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "PPER", "$,", "ITJ", "$,", "PWAV", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}