{"textgrid.poem.41179": {"metadata": {"author": {"name": "Denis, Michael", "birth": "N.A.", "death": "N.A."}, "title": "1L: Lange, schon lange kenn' ich euch, S\u00e4nger", "genre": "verse", "period": "N.A.", "pub_year": 1764, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Lange, schon lange kenn' ich euch, S\u00e4nger", "tokens": ["Lan\u00b7ge", ",", "schon", "lan\u00b7ge", "kenn'", "ich", "euch", ",", "S\u00e4n\u00b7ger"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["NE", "$,", "ADV", "ADV", "VVFIN", "PPER", "PPER", "$,", "NN"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Fremder Geschlechter, fernerer Himmel!", "tokens": ["Frem\u00b7der", "Ge\u00b7schlech\u00b7ter", ",", "fer\u00b7ne\u00b7rer", "Him\u00b7mel", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Aber ich Hainbewohner, ich Nordensohn", "tokens": ["A\u00b7ber", "ich", "Hain\u00b7be\u00b7woh\u00b7ner", ",", "ich", "Nor\u00b7den\u00b7sohn"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PPER", "NN", "$,", "PPER", "NN"], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.4": {"text": "Bild' euch meine Ges\u00e4nge nicht nach.", "tokens": ["Bild'", "euch", "mei\u00b7ne", "Ge\u00b7s\u00e4n\u00b7ge", "nicht", "nach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPOSAT", "NN", "PTKNEG", "PTKVZ", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Kannten euch einstens unsere V\u00e4ter?", "tokens": ["Kann\u00b7ten", "euch", "eins\u00b7tens", "un\u00b7se\u00b7re", "V\u00e4\u00b7ter", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "PPOSAT", "NN", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Dennoch ert\u00f6nten Bardenges\u00e4nge.", "tokens": ["Den\u00b7noch", "er\u00b7t\u00f6n\u00b7ten", "Bar\u00b7den\u00b7ge\u00b7s\u00e4n\u00b7ge", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "F\u00fcrsten, Druiden, und M\u00e4dchen und J\u00fcnglinge", "tokens": ["F\u00fcrs\u00b7ten", ",", "Dru\u00b7i\u00b7den", ",", "und", "M\u00e4d\u00b7chen", "und", "J\u00fcng\u00b7lin\u00b7ge"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "KON", "NN", "KON", "NN"], "meter": "+--+--+--++-", "measure": "dactylic.tri.plus"}, "line.4": {"text": "Gaben Bardenges\u00e4ngen ihr Herz.", "tokens": ["Ga\u00b7ben", "Bar\u00b7den\u00b7ge\u00b7s\u00e4n\u00b7gen", "ihr", "Herz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "PPOSAT", "NN", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Reizet uns nicht der Tugenden Hoheit?", "tokens": ["Rei\u00b7zet", "uns", "nicht", "der", "Tu\u00b7gen\u00b7den", "Ho\u00b7heit", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "ART", "ADJA", "NN", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "F\u00fchlen wir nicht die Wonne der Wehmuth?", "tokens": ["F\u00fch\u00b7len", "wir", "nicht", "die", "Won\u00b7ne", "der", "Weh\u00b7muth", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "ART", "NN", "ART", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Fachet uns Liebe des Vaterlands, Heldentod,", "tokens": ["Fa\u00b7chet", "uns", "Lie\u00b7be", "des", "Va\u00b7ter\u00b7lands", ",", "Hel\u00b7den\u00b7tod", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "ART", "NN", "$,", "NN", "$,"], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}, "line.4": {"text": "Freundschaft, Sch\u00f6nheit und Ruhmgier nicht an?", "tokens": ["Freund\u00b7schaft", ",", "Sch\u00f6n\u00b7heit", "und", "Ruhm\u00b7gier", "nicht", "an", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "PTKNEG", "PTKVZ", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Herrschet nicht oben m\u00e4chtig Allvater?", "tokens": ["Herr\u00b7schet", "nicht", "o\u00b7ben", "m\u00e4ch\u00b7tig", "All\u00b7va\u00b7ter", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ADV", "ADJD", "NN", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Wohnen nicht oben Geister der Ahnen?", "tokens": ["Woh\u00b7nen", "nicht", "o\u00b7ben", "Geis\u00b7ter", "der", "Ah\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "ADV", "NN", "ART", "NN", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Lachet kein Fr\u00fchling, kein Morgen und Abend uns?", "tokens": ["La\u00b7chet", "kein", "Fr\u00fch\u00b7ling", ",", "kein", "Mor\u00b7gen", "und", "A\u00b7bend", "uns", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "$,", "PIAT", "NN", "KON", "NN", "PPER", "$."], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}, "line.4": {"text": "Kreist bei'm Mahle kein sch\u00e4umendes Horn?", "tokens": ["Kreist", "bei'm", "Mah\u00b7le", "kein", "sch\u00e4u\u00b7men\u00b7des", "Horn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "PIAT", "ADJA", "NN", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "M\u00fc\u00dft ihr uns, S\u00e4nger fremder Geschlechter,", "tokens": ["M\u00fc\u00dft", "ihr", "uns", ",", "S\u00e4n\u00b7ger", "frem\u00b7der", "Ge\u00b7schlech\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "$,", "NN", "ADJA", "NN", "$,"], "meter": "++-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Fernerer Himmel! m\u00fc\u00dft ihr uns zeigen,", "tokens": ["Fer\u00b7ne\u00b7rer", "Him\u00b7mel", "!", "m\u00fc\u00dft", "ihr", "uns", "zei\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "VMFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "+--+-++-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Wie sich des Tages hellflammendes Riesenaug'", "tokens": ["Wie", "sich", "des", "Ta\u00b7ges", "hell\u00b7flam\u00b7men\u00b7des", "Rie\u00b7se\u00b7naug'"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PRF", "ART", "NN", "ADJA", "NN"], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}, "line.4": {"text": "Rastlos blauliche Fernen durchw\u00e4lzt?", "tokens": ["Rast\u00b7los", "blau\u00b7li\u00b7che", "Fer\u00b7nen", "durch\u00b7w\u00e4lzt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "VVPP", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "M\u00fc\u00dft ihr uns zeigen wandelnde Monde,", "tokens": ["M\u00fc\u00dft", "ihr", "uns", "zei\u00b7gen", "wan\u00b7deln\u00b7de", "Mon\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "VVINF", "VVFIN", "NE", "$,"], "meter": "++-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Schwellende Wogen, kreuzende Blitze,", "tokens": ["Schwel\u00b7len\u00b7de", "Wo\u00b7gen", ",", "kreu\u00b7zen\u00b7de", "Blit\u00b7ze", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Zeigen im Winde das wiegende Blumenhaupt?", "tokens": ["Zei\u00b7gen", "im", "Win\u00b7de", "das", "wie\u00b7gen\u00b7de", "Blu\u00b7men\u00b7haupt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}, "line.4": {"text": "Oder k\u00f6nnen wir selber es sehn?", "tokens": ["O\u00b7der", "k\u00f6n\u00b7nen", "wir", "sel\u00b7ber", "es", "sehn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADV", "PPER", "VVINF", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}}, "stanza.7": {"line.1": {"text": "Fliegt nur in eurem Liede der Adler?", "tokens": ["Fliegt", "nur", "in", "eu\u00b7rem", "Lie\u00b7de", "der", "Ad\u00b7ler", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Spro\u00dft nur in eurem Liede der Schatten?", "tokens": ["Spro\u00dft", "nur", "in", "eu\u00b7rem", "Lie\u00b7de", "der", "Schat\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Kann sich im Farbengemische der jungen Flur", "tokens": ["Kann", "sich", "im", "Far\u00b7ben\u00b7ge\u00b7mi\u00b7sche", "der", "jun\u00b7gen", "Flur"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PRF", "APPRART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sined's Auge nicht selber erfreu'n?", "tokens": ["Si\u00b7ne\u00b7d's", "Au\u00b7ge", "nicht", "sel\u00b7ber", "er\u00b7freu'n", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "PTKNEG", "ADV", "VVINF", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.8": {"line.1": {"text": "Schwillt nun in seinem Busen Gesang auf,", "tokens": ["Schwillt", "nun", "in", "sei\u00b7nem", "Bu\u00b7sen", "Ge\u00b7sang", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "PPOSAT", "NN", "NN", "PTKVZ", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Soll er von euch die Weisen erborgen?", "tokens": ["Soll", "er", "von", "euch", "die", "Wei\u00b7sen", "er\u00b7bor\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "PPER", "ART", "NN", "VVINF", "$."], "meter": "---+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "H\u00f6ret er lispelnden Quellen den Schlummerton,", "tokens": ["H\u00f6\u00b7ret", "er", "lis\u00b7peln\u00b7den", "Quel\u00b7len", "den", "Schlum\u00b7mer\u00b7ton", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}, "line.4": {"text": "Tannenwipfeln die Seufzer nicht ab?", "tokens": ["Tan\u00b7nen\u00b7wip\u00b7feln", "die", "Seuf\u00b7zer", "nicht", "ab", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PTKNEG", "PTKVZ", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}}, "stanza.9": {"line.1": {"text": "Lehret ihn nicht das Rollen des Donners?", "tokens": ["Leh\u00b7ret", "ihn", "nicht", "das", "Rol\u00b7len", "des", "Don\u00b7ners", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "ART", "NN", "ART", "NN", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Lehret ihn nicht das Brausen der Fluthen?", "tokens": ["Leh\u00b7ret", "ihn", "nicht", "das", "Brau\u00b7sen", "der", "Flut\u00b7hen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "ART", "NN", "ART", "NN", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Lauscht er im Haine der Drossel und Amsel nicht,", "tokens": ["Lauscht", "er", "im", "Hai\u00b7ne", "der", "Dros\u00b7sel", "und", "Am\u00b7sel", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "ART", "NN", "KON", "NN", "PTKNEG", "$,"], "meter": "+--+--++-+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Nachtigallen in Hecken nicht auf?", "tokens": ["Nach\u00b7ti\u00b7gal\u00b7len", "in", "He\u00b7cken", "nicht", "auf", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "PTKNEG", "PTKVZ", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}}, "stanza.10": {"line.1": {"text": "Soll er vor Manas Enkeln in Liedern", "tokens": ["Soll", "er", "vor", "Ma\u00b7nas", "En\u00b7keln", "in", "Lie\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "APPR", "PIAT", "NN", "APPR", "NN"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Euren verbuhlten Donnerer nennen,", "tokens": ["Eu\u00b7ren", "ver\u00b7buhl\u00b7ten", "Don\u00b7ne\u00b7rer", "nen\u00b7nen", ","], "token_info":