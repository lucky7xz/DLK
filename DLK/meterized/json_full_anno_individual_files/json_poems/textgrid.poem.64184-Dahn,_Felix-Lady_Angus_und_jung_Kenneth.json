{"textgrid.poem.64184": {"metadata": {"author": {"name": "Dahn, Felix", "birth": "N.A.", "death": "N.A."}, "title": "Lady Angus und jung Kenneth", "genre": "verse", "period": "N.A.", "pub_year": 1873, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbo komme, jung Kenneth, dich lieb' ich mit Macht,", "tokens": ["\u00bb", "o", "kom\u00b7me", ",", "jung", "Ken\u00b7neth", ",", "dich", "lieb'", "ich", "mit", "Macht", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM", "VVFIN", "$,", "ADJD", "VVFIN", "$,", "PPER", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "O komme zur Sonnwend um Mitternacht.", "tokens": ["O", "kom\u00b7me", "zur", "Sonn\u00b7wend", "um", "Mit\u00b7ter\u00b7nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPRART", "NN", "APPR", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Vor Sonnwend zieht Lord Angus zu Feld, \u2013", "tokens": ["Vor", "Sonn\u00b7wend", "zieht", "Lord", "An\u00b7gus", "zu", "Feld", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "NN", "VVFIN", "NN", "NE", "APPR", "NN", "$,", "$("], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Nicht kann er uns st\u00f6ren, der graue Held:", "tokens": ["Nicht", "kann", "er", "uns", "st\u00f6\u00b7ren", ",", "der", "grau\u00b7e", "Held", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VMFIN", "PPER", "PRF", "VVINF", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Ich \u00f6ffne dir Garten und Erker und Arme,", "tokens": ["Ich", "\u00f6ff\u00b7ne", "dir", "Gar\u00b7ten", "und", "Er\u00b7ker", "und", "Ar\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "NN", "KON", "NN", "KON", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.6": {"text": "Da\u00df in Flammen dein k\u00fchles Herz erwarme.\u00ab", "tokens": ["Da\u00df", "in", "Flam\u00b7men", "dein", "k\u00fch\u00b7les", "Herz", "er\u00b7war\u00b7me", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "APPR", "NN", "PPOSAT", "ADJA", "NN", "VVFIN", "$.", "$("], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "\u00bbla\u00df ab, Lady Angus, und locke mich nicht!", "tokens": ["\u00bb", "la\u00df", "ab", ",", "La\u00b7dy", "An\u00b7gus", ",", "und", "lo\u00b7cke", "mich", "nicht", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "PTKVZ", "$,", "NE", "NE", "$,", "KON", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Gern schau' ich, gern sing' ich dein sch\u00f6nes Gesicht:", "tokens": ["Gern", "schau'", "ich", ",", "gern", "sing'", "ich", "dein", "sch\u00f6\u00b7nes", "Ge\u00b7sicht", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ADV", "VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Doch scheu' ich Lord Angus im grauen Haar", "tokens": ["Doch", "scheu'", "ich", "Lord", "An\u00b7gus", "im", "grau\u00b7en", "Haar"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "NN", "NE", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und den W\u00e4chter am Tor und den Wolfshund gar:", "tokens": ["Und", "den", "W\u00e4ch\u00b7ter", "am", "Tor", "und", "den", "Wolfs\u00b7hund", "gar", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NE", "APPRART", "NN", "KON", "ART", "NN", "ADV", "$."], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.5": {"text": "Leicht h\u00f6rt man den Schritt auf dem Marmorhofe", "tokens": ["Leicht", "h\u00f6rt", "man", "den", "Schritt", "auf", "dem", "Mar\u00b7mor\u00b7ho\u00b7fe"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PIS", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Und dicht an dem Erker dir schl\u00e4ft die Zofe.\u00ab", "tokens": ["Und", "dicht", "an", "dem", "Er\u00b7ker", "dir", "schl\u00e4ft", "die", "Zo\u00b7fe", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADJD", "APPR", "ART", "NN", "PPER", "VVFIN", "ART", "NN", "$.", "$("], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}}, "stanza.3": {"line.1": {"text": "\u00bbden W\u00e4chter am Tor stillt schwerer Trank, \u2013", "tokens": ["\u00bb", "den", "W\u00e4ch\u00b7ter", "am", "Tor", "stillt", "schwe\u00b7rer", "Trank", ",", "\u2013"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ART", "NE", "APPRART", "NN", "VVFIN", "ADJA", "NN", "$,", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Den Wolfshund kett' ich zur Eichenbank, \u2013", "tokens": ["Den", "Wolfs\u00b7hund", "kett'", "ich", "zur", "Ei\u00b7chen\u00b7bank", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPRART", "NN", "$,", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Den Hof bestreu' ich mit Binsen ganz, \u2013", "tokens": ["Den", "Hof", "be\u00b7streu'", "ich", "mit", "Bin\u00b7sen", "ganz", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "NN", "ADV", "$,", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die Zofe schick' ich zum Sonnwendtanz: \u2013", "tokens": ["Die", "Zo\u00b7fe", "schick'", "ich", "zum", "Sonn\u00b7wend\u00b7tanz", ":", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPRART", "NN", "$.", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Leis \u00f6ffn' ich dir selber den knarrenden Riegel", "tokens": ["Leis", "\u00f6ffn'", "ich", "dir", "sel\u00b7ber", "den", "knar\u00b7ren\u00b7den", "Rie\u00b7gel"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "PPER", "ADV", "ART", "ADJA", "NN"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.6": {"text": "Und schlie\u00dfe den Mund dir mit gl\u00fchendem Siegel.\u00ab", "tokens": ["Und", "schlie\u00b7\u00dfe", "den", "Mund", "dir", "mit", "gl\u00fc\u00b7hen\u00b7dem", "Sie\u00b7gel", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PPER", "APPR", "ADJA", "NN", "$.", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}}, "stanza.4": {"line.1": {"text": "\u00bbla\u00df ab, Lady Angus, und lade mich nicht!\u00ab", "tokens": ["\u00bb", "la\u00df", "ab", ",", "La\u00b7dy", "An\u00b7gus", ",", "und", "la\u00b7de", "mich", "nicht", "!", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVIMP", "PTKVZ", "$,", "NE", "NE", "$,", "KON", "VVFIN", "PPER", "PTKNEG", "$.", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "\u00bbsag', bist du ein Ritter oder ein Wicht?", "tokens": ["\u00bb", "sag'", ",", "bist", "du", "ein", "Rit\u00b7ter", "o\u00b7der", "ein", "Wicht", "?"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "$,", "VAFIN", "PPER", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Wohl schl\u00e4gst du die Laute, den Federball", "tokens": ["Wohl", "schl\u00e4gst", "du", "die", "Lau\u00b7te", ",", "den", "Fe\u00b7der\u00b7ball"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,", "ART", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Und tanzest geschmeidig in bunter Hall'", "tokens": ["Und", "tan\u00b7zest", "ge\u00b7schmei\u00b7dig", "in", "bun\u00b7ter", "Hall'"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD", "APPR", "ADJA", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Und lispelst von Lieb' und Liebesgabe,", "tokens": ["Und", "lis\u00b7pelst", "von", "Lieb'", "und", "Lie\u00b7bes\u00b7ga\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Doch hei\u00dft es ein Mann sein, \u2013 da zittert der Knabe!\u00ab", "tokens": ["Doch", "hei\u00dft", "es", "ein", "Mann", "sein", ",", "\u2013", "da", "zit\u00b7tert", "der", "Kna\u00b7be", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "VAINF", "$,", "$(", "ADV", "VVFIN", "ART", "NN", "$.", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}}, "stanza.5": {"line.1": {"text": "\u00bblady Angus, du machst mir die Wangen rot!", "tokens": ["\u00bb", "la\u00b7dy", "An\u00b7gus", ",", "du", "machst", "mir", "die", "Wan\u00b7gen", "rot", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM.la", "FM.la", "$,", "PPER", "VVFIN", "PPER", "ART", "NN", "ADJD", "$."], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Ich komme lebendig, wohlan, oder tot:", "tokens": ["Ich", "kom\u00b7me", "le\u00b7ben\u00b7dig", ",", "wo\u00b7hlan", ",", "o\u00b7der", "tot", ":"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$,", "PWAV", "$,", "KON", "ADJD", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Und geb' ich uns in der H\u00f6lle Macht: \u2013", "tokens": ["Und", "geb'", "ich", "uns", "in", "der", "H\u00f6l\u00b7le", "Macht", ":", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "PPER", "PRF", "APPR", "ART", "NN", "NN", "$.", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ich komme zur Sonnwend um Mitternacht,", "tokens": ["Ich", "kom\u00b7me", "zur", "Sonn\u00b7wend", "um", "Mit\u00b7ter\u00b7nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "APPR", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Du hast mich bezwungen, du hast mich beschworen \u2013", "tokens": ["Du", "hast", "mich", "be\u00b7zwun\u00b7gen", ",", "du", "hast", "mich", "be\u00b7schwo\u00b7ren", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VVPP", "$,", "PPER", "VAFIN", "PPER", "VVINF", "$("], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.6": {"text": "Ich komme und sind wir beide verloren!\u00ab \u2013 \u2013", "tokens": ["Ich", "kom\u00b7me", "und", "sind", "wir", "bei\u00b7de", "ver\u00b7lo\u00b7ren", "!", "\u00ab", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct", "punct"], "pos": ["PPER", "VVFIN", "KON", "VAFIN", "PPER", "PIS", "VVPP", "$.", "$(", "$(", "$("], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "\u00bbden W\u00e4chter am Tor bannt schwerer Trank, \u2013", "tokens": ["\u00bb", "den", "W\u00e4ch\u00b7ter", "am", "Tor", "bannt", "schwe\u00b7rer", "Trank", ",", "\u2013"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ART", "NE", "APPRART", "NN", "ADJD", "ADJA", "NN", "$,", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der Wolfshund schl\u00e4ft an der Eichenbank, \u2013", "tokens": ["Der", "Wolfs\u00b7hund", "schl\u00e4ft", "an", "der", "Ei\u00b7chen\u00b7bank", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Den Hof bestreut' ich mit Binsen ganz,", "tokens": ["Den", "Hof", "be\u00b7streut'", "ich", "mit", "Bin\u00b7sen", "ganz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "NN", "ADV", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die Zofe tanzt auf dem Sonnwendtanz,", "tokens": ["Die", "Zo\u00b7fe", "tanzt", "auf", "dem", "Sonn\u00b7wend\u00b7tanz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Der Himmel ist dunkel und leer von Sternen, \u2013 \u2013", "tokens": ["Der", "Him\u00b7mel", "ist", "dun\u00b7kel", "und", "leer", "von", "Ster\u00b7nen", ",", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "KON", "ADJD", "APPR", "NN", "$,", "$(", "$("], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Jung Kenneth, nun sollst du das K\u00fcssen lernen!\u00ab", "tokens": ["Jung", "Ken\u00b7neth", ",", "nun", "sollst", "du", "das", "K\u00fcs\u00b7sen", "ler\u00b7nen", "!", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "NE", "$,", "ADV", "VMFIN", "PPER", "ART", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Die Turmuhr schl\u00e4gt die Mitternacht, \u2013", "tokens": ["Die", "Tur\u00b7muhr", "schl\u00e4gt", "die", "Mit\u00b7ter\u00b7nacht", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Lady Angus \u00f6ffnet die Pforte sacht, \u2013", "tokens": ["La\u00b7dy", "An\u00b7gus", "\u00f6ff\u00b7net", "die", "Pfor\u00b7te", "sacht", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "NE", "VVFIN", "ART", "NN", "VVFIN", "$,", "$("], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Da steht er schweigend im Portal:", "tokens": ["Da", "steht", "er", "schwei\u00b7gend", "im", "Por\u00b7tal", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "\u00bbmein S\u00fc\u00dfer, wie bist du so kalt, so fahl?", "tokens": ["\u00bb", "mein", "S\u00fc\u00b7\u00dfer", ",", "wie", "bist", "du", "so", "kalt", ",", "so", "fahl", "?"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "$,", "PWAV", "VAFIN", "PPER", "ADV", "ADJD", "$,", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und auf wei\u00dfem Wams ein dunkler Flecken: \u2013", "tokens": ["Und", "auf", "wei\u00b7\u00dfem", "Wams", "ein", "dunk\u00b7ler", "Fle\u00b7cken", ":", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "ART", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "O la\u00df die Arme, mich t\u00f6tet der Schrecken!\u00ab \u2013 \u2013", "tokens": ["O", "la\u00df", "die", "Ar\u00b7me", ",", "mich", "t\u00f6\u00b7tet", "der", "Schre\u00b7cken", "!", "\u00ab", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct", "punct", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "$.", "$(", "$(", "$("], "meter": "---+--+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.8": {"line.1": {"text": "Doch er schn\u00fcrt die Arme ihr um den Leib", "tokens": ["Doch", "er", "schn\u00fcrt", "die", "Ar\u00b7me", "ihr", "um", "den", "Leib"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "PPER", "APPR", "ART", "NN"], "meter": "--+-+-+--+", 