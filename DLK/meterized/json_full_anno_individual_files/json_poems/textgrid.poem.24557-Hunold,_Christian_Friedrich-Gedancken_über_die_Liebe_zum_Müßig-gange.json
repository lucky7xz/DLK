{"textgrid.poem.24557": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Gedancken \u00fcber die Liebe zum M\u00fc\u00dfig-gange", "genre": "verse", "period": "N.A.", "pub_year": 1701, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer sich ern\u00e4hren kan/ und dennoch sich beschwert/", "tokens": ["Wer", "sich", "er\u00b7n\u00e4h\u00b7ren", "kan", "/", "und", "den\u00b7noch", "sich", "be\u00b7schwert", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "VVINF", "VMFIN", "$(", "KON", "ADV", "PRF", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df ihn die Armuth dr\u00fcckt/ der ist der Peitsche wehrt.", "tokens": ["Da\u00df", "ihn", "die", "Ar\u00b7muth", "dr\u00fcckt", "/", "der", "ist", "der", "Peit\u00b7sche", "wehrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$(", "ART", "VAFIN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So mu\u00df ich mir den Text nun selber lesen.", "tokens": ["So", "mu\u00df", "ich", "mir", "den", "Text", "nun", "sel\u00b7ber", "le\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPER", "ART", "NN", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Gott hat mir zwar ein Pfund verliehn/", "tokens": ["Gott", "hat", "mir", "zwar", "ein", "Pfund", "ver\u00b7liehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ADV", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Daraus ich k\u00f6nnen Nutzen ziehn;", "tokens": ["Da\u00b7raus", "ich", "k\u00f6n\u00b7nen", "Nut\u00b7zen", "ziehn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "VMFIN", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Doch bin ich sein nicht immer wehrt gewesen.", "tokens": ["Doch", "bin", "ich", "sein", "nicht", "im\u00b7mer", "wehrt", "ge\u00b7we\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "VAINF", "PTKNEG", "ADV", "ADJD", "VAPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Auf Wucher hab ich es so offt nicht ausgethan/", "tokens": ["Auf", "Wu\u00b7cher", "hab", "ich", "es", "so", "offt", "nicht", "aus\u00b7ge\u00b7than", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PPER", "PPER", "ADV", "ADV", "PTKNEG", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Als man wohl kan.", "tokens": ["Als", "man", "wohl", "kan", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "VMFIN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.9": {"text": "Die Arbeit war erst mein Vergn\u00fcgen:", "tokens": ["Die", "Ar\u00b7beit", "war", "erst", "mein", "Ver\u00b7gn\u00fc\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Doch blieb sie wieder liegen/", "tokens": ["Doch", "blieb", "sie", "wie\u00b7der", "lie\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Und stanck mich an.", "tokens": ["Und", "stanck", "mich", "an", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.12": {"text": "Gantz nichts zu thun/ war mir nicht wohl gegeben.", "tokens": ["Gantz", "nichts", "zu", "thun", "/", "war", "mir", "nicht", "wohl", "ge\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "PTKZU", "VVINF", "$(", "VAFIN", "PPER", "PTKNEG", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Drum nahm ich lauter Dinge vor/", "tokens": ["Drum", "nahm", "ich", "lau\u00b7ter", "Din\u00b7ge", "vor", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PIAT", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Als lebt ich blo\u00df/ zum Zeit-Vertreib zu leben/", "tokens": ["Als", "lebt", "ich", "blo\u00df", "/", "zum", "Zeit\u00b7Ver\u00b7treib", "zu", "le\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPER", "ADV", "$(", "APPRART", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Um nichts zu thun/ in dem ich doch was thate.", "tokens": ["Um", "nichts", "zu", "thun", "/", "in", "dem", "ich", "doch", "was", "tha\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PIS", "PTKZU", "VVINF", "$(", "APPR", "PRELS", "PPER", "ADV", "PWS", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Wer war wohl hier ein Thor?", "tokens": ["Wer", "war", "wohl", "hier", "ein", "Thor", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "ADV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.17": {"text": "Ich/ in ", "tokens": ["Ich", "/", "in"], "token_info": ["word", "punct", "word"], "pos": ["PPER", "$(", "APPR"], "meter": "-+", "measure": "iambic.single"}, "line.18": {"text": "Und wenn ich denn bi\u00df auf die N\u00e4hte kahl/", "tokens": ["Und", "wenn", "ich", "denn", "bi\u00df", "auf", "die", "N\u00e4h\u00b7te", "kahl", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "ADV", "APPR", "ART", "NN", "ADJD", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Und mich mein eigen Elend bate", "tokens": ["Und", "mich", "mein", "ei\u00b7gen", "E\u00b7lend", "ba\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "PPOSAT", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Die Arbeit zu ergreifen/", "tokens": ["Die", "Ar\u00b7beit", "zu", "er\u00b7grei\u00b7fen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.21": {"text": "So dacht ich ihr zu pfeifen;", "tokens": ["So", "dacht", "ich", "ihr", "zu", "pfei\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.22": {"text": "Allein sie h\u00f6rte nicht.", "tokens": ["Al\u00b7lein", "sie", "h\u00f6r\u00b7te", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.23": {"text": "Ich reckte mich die Quer und in die L\u00e4nge.", "tokens": ["Ich", "reck\u00b7te", "mich", "die", "Quer", "und", "in", "die", "L\u00e4n\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "KON", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "Denn grif ichs an/ denn schmi\u00df ichs wieder hin.", "tokens": ["Denn", "grif", "ichs", "an", "/", "denn", "schmi\u00df", "ichs", "wie\u00b7der", "hin", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "PTKVZ", "$(", "KON", "VVFIN", "PIS", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "Das Hau\u00df war mir zu enge.", "tokens": ["Das", "Hau\u00df", "war", "mir", "zu", "en\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "APPR", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.26": {"text": "Heut will ich noch zu guten Freunden gehn/", "tokens": ["Heut", "will", "ich", "noch", "zu", "gu\u00b7ten", "Freun\u00b7den", "gehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.27": {"text": "So dacht ich denn/ und morgen fr\u00fch aufstehn/", "tokens": ["So", "dacht", "ich", "denn", "/", "und", "mor\u00b7gen", "fr\u00fch", "auf\u00b7stehn", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$(", "KON", "ADV", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.28": {"text": "Denn wei\u00df ich/ da\u00df ich flei\u00dfig bin.", "tokens": ["Denn", "wei\u00df", "ich", "/", "da\u00df", "ich", "flei\u00b7\u00dfig", "bin", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$(", "KOUS", "PPER", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Der Tag/ der zwantzig mahl gekommen/", "tokens": ["Der", "Tag", "/", "der", "zwant\u00b7zig", "mahl", "ge\u00b7kom\u00b7men", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "CARD", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.30": {"text": "Hat gleichwohl nicht das Gold des Morgens mitgenommen.", "tokens": ["Hat", "gleich\u00b7wohl", "nicht", "das", "Gold", "des", "Mor\u00b7gens", "mit\u00b7ge\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PTKNEG", "ART", "NN", "ART", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Weil/ wenn ", "tokens": ["Weil", "/", "wenn"], "token_info": ["word", "punct", "word"], "pos": ["KOUS", "$(", "KOUS"], "meter": "+-", "measure": "trochaic.single"}, "line.32": {"text": "Sie Musen Wei\u00dfheits Gold in ihrem Munde zeigt.", "tokens": ["Sie", "Mu\u00b7sen", "Wei\u00df\u00b7heits", "Gold", "in", "ih\u00b7rem", "Mun\u00b7de", "zeigt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "NE", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Inzwischen/ da die Armuth mich", "tokens": ["I\u00b7nzwi\u00b7schen", "/", "da", "die", "Ar\u00b7muth", "mich"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$(", "KOUS", "ART", "NN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.34": {"text": "Wie ein Gewapneter wolt' \u00fcberfallen/", "tokens": ["Wie", "ein", "Ge\u00b7wap\u00b7ne\u00b7ter", "wolt'", "\u00fc\u00b7berf\u00b7al\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.35": {"text": "Da klaute denn der M\u00fc\u00dfigg\u00e4nger sich/", "tokens": ["Da", "klau\u00b7te", "denn", "der", "M\u00fc\u00b7\u00dfig\u00b7g\u00e4n\u00b7ger", "sich", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "PRF", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.36": {"text": "Und lie\u00df ein Klage-Lied erschallen:", "tokens": ["Und", "lie\u00df", "ein", "Kla\u00b7ge\u00b7Lied", "er\u00b7schal\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.37": {"text": "Die Zeiten w\u00e4ren schlecht/ die Wei\u00dfheit sey gedr\u00fcckt.", "tokens": ["Die", "Zei\u00b7ten", "w\u00e4\u00b7ren", "schlecht", "/", "die", "Wei\u00df\u00b7heit", "sey", "ge\u00b7dr\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$(", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Da er sein eigen Creutz sich selber zugeschickt.", "tokens": ["Da", "er", "sein", "ei\u00b7gen", "Creutz", "sich", "sel\u00b7ber", "zu\u00b7ge\u00b7schickt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "ADJA", "NN", "PRF", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Es klopfte niemand an die Th\u00fcr/", "tokens": ["Es", "klopf\u00b7te", "nie\u00b7mand", "an", "die", "Th\u00fcr", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.40": {"text": "So dacht er gleich/ ein Mahner ist daf\u00fcr.", "tokens": ["So", "dacht", "er", "gleich", "/", "ein", "Mah\u00b7ner", "ist", "da\u00b7f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$(", "ART", "NN", "VAFIN", "PAV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.41": {"text": "Was war zu thun? hier half kein Gr\u00e4men/", "tokens": ["Was", "war", "zu", "thun", "?", "hier", "half", "kein", "Gr\u00e4\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PTKZU", "VVINF", "$.", "ADV", "VVFIN", "PIAT", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.42": {"text": "Der Faule muste sich bequemen", "tokens": ["Der", "Fau\u00b7le", "mus\u00b7te", "sich", "be\u00b7que\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "PRF", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.43": {"text": "Mit doppelt schwerer Pein", "tokens": ["Mit", "dop\u00b7pelt", "schwe\u00b7rer", "Pein"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJD", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.44": {"text": "Und steter Arbeit dran zu seyn/", "tokens": ["Und", "ste\u00b7ter", "Ar\u00b7beit", "dran", "zu", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "PAV", "PTKZU", "VAINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.45": {"text": "Da\u00df er der Schulden sich erwehrte/", "tokens": ["Da\u00df", "er", "der", "Schul\u00b7den", "sich", "er\u00b7wehr\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PRF", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.46": {"text": "Und sich nach Nothdurfft nehrte.", "tokens": ["Und", "sich", "nach", "Noth\u00b7durfft", "nehr\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.47": {"text": "Die Lehre/ hier/ mein Freund/ die deine Seele fa\u00dft/", "tokens": ["Die", "Leh\u00b7re", "/", "hier", "/", "mein", "Freund", "/", "die", "dei\u00b7ne", "See\u00b7le", "fa\u00dft", "/"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ADV", "$(", "PPOSAT", "NN", "$(", "ART", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.48": {"text": "Ist gut/ wenn du sie nicht/ wie ich/ von n\u00f6then hast.", "tokens": ["Ist", "gut", "/", "wenn", "du", "sie", "nicht", "/", "wie", "ich", "/", "von", "n\u00f6\u00b7then", "hast", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "$(", "KOUS", "PPER", "PPER", "PTKNEG", "$(", "PWAV", "PPER", "$(", "APPR", "ADJA", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wer sich ern\u00e4hren kan/ und dennoch sich beschwert/", "tokens": ["Wer", "sich", "er\u00b7n\u00e4h\u00b7ren", "kan", "/", "und", "den\u00b7noch", "sich", "be\u00b7schwert", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "VVINF", "VMFIN", "$(", "KON", "ADV", "PRF", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df ihn die Armuth dr\u00fcckt/ der ist der Peitsche wehr