{"textgrid.poem.39888": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und wie in den fr\u00fchen Tagen", "tokens": ["Und", "wie", "in", "den", "fr\u00fc\u00b7hen", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "APPR", "ART", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Hat der Held den Wandelthron", "tokens": ["Hat", "der", "Held", "den", "Wan\u00b7del\u00b7thron"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Froh errichtet auf dem Wagen,", "tokens": ["Froh", "er\u00b7rich\u00b7tet", "auf", "dem", "Wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ragt empor wie Thetis' Sohn.", "tokens": ["Ragt", "em\u00b7por", "wie", "The\u00b7tis'", "Sohn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "KOKOM", "NE", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "In der Linken wehn die Z\u00fcgel,", "tokens": ["In", "der", "Lin\u00b7ken", "wehn", "die", "Z\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und die Rechte wiegt den Speer,", "tokens": ["Und", "die", "Rech\u00b7te", "wiegt", "den", "Speer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.7": {"text": "Rossesm\u00e4hnen werden Fl\u00fcgel,", "tokens": ["Ros\u00b7ses\u00b7m\u00e4h\u00b7nen", "wer\u00b7den", "Fl\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "G\u00f6ttergleich braus't er einher.", "tokens": ["G\u00f6t\u00b7ter\u00b7gleich", "braus't", "er", "ein\u00b7her", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.2": {"line.1": {"text": "Und ein Andrer der Genossen,", "tokens": ["Und", "ein", "A\u00b7ndrer", "der", "Ge\u00b7nos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Treuer W\u00e4chter, flieget mit,", "tokens": ["Treu\u00b7er", "W\u00e4ch\u00b7ter", ",", "flie\u00b7get", "mit", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Thut zu Fu\u00df es gleich den Rossen,", "tokens": ["Thut", "zu", "Fu\u00df", "es", "gleich", "den", "Ros\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "NN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schneller war nicht Ajas' Schritt.", "tokens": ["Schnel\u00b7ler", "war", "nicht", "A\u00b7jas'", "Schritt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PTKNEG", "NE", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "In des Uferhaines Pfade", "tokens": ["In", "des", "U\u00b7fer\u00b7hai\u00b7nes", "Pfa\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Tieft die dunkle Fahrt sich ein,", "tokens": ["Tieft", "die", "dunk\u00b7le", "Fahrt", "sich", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "PRF", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Dort verst\u00f6rt am Moorgestade", "tokens": ["Dort", "ver\u00b7st\u00f6rt", "am", "Mo\u00b7or\u00b7ge\u00b7sta\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPRART", "NN"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.8": {"text": "Das Gespann ein grauer Stein.", "tokens": ["Das", "Ge\u00b7spann", "ein", "grau\u00b7er", "Stein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}, "stanza.3": {"line.1": {"text": "Wieder hebt sich aus den Hecken", "tokens": ["Wie\u00b7der", "hebt", "sich", "aus", "den", "He\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Das zerfallne K\u00f6nigsgrab,", "tokens": ["Das", "zer\u00b7fall\u00b7ne", "K\u00f6\u00b7nigs\u00b7grab", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und die Rosse hoch im Schrecken", "tokens": ["Und", "die", "Ros\u00b7se", "hoch", "im", "Schre\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJD", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "B\u00e4umen sich zum Strand hinab.", "tokens": ["B\u00e4u\u00b7men", "sich", "zum", "Strand", "hin\u00b7ab", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "APPRART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Die an wilder M\u00e4hne fassend", "tokens": ["Die", "an", "wil\u00b7der", "M\u00e4h\u00b7ne", "fas\u00b7send"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ADJA", "NN", "VVPP"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Rei\u00dft der W\u00e4chter k\u00fchn in's Gleis,", "tokens": ["Rei\u00dft", "der", "W\u00e4ch\u00b7ter", "k\u00fchn", "in's", "Gleis", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NE", "ADJD", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Am Geretteten, erblassend,", "tokens": ["Am", "Ge\u00b7ret\u00b7te\u00b7ten", ",", "er\u00b7blas\u00b7send", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["APPRART", "NN", "$,", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Sinkt er hin in Blut und Schwei\u00df.", "tokens": ["Sinkt", "er", "hin", "in", "Blut", "und", "Schwei\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Als den Rossen er mit Hitze", "tokens": ["Als", "den", "Ros\u00b7sen", "er", "mit", "Hit\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "PPER", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hemmend in den Z\u00fcgel fuhr,", "tokens": ["Hem\u00b7mend", "in", "den", "Z\u00fc\u00b7gel", "fuhr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ri\u00df des K\u00f6nigs Lanzenspitze", "tokens": ["Ri\u00df", "des", "K\u00f6\u00b7nigs", "Lan\u00b7zens\u00b7pit\u00b7ze"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In die Stirn ihm tiefe Spur.", "tokens": ["In", "die", "Stirn", "ihm", "tie\u00b7fe", "Spur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPER", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Der entschwingt sich rasch dem Wagen,", "tokens": ["Der", "ent\u00b7schwingt", "sich", "rasch", "dem", "Wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PRF", "ADJD", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Seine Rosse z\u00e4hmt er schnell,", "tokens": ["Sei\u00b7ne", "Ros\u00b7se", "z\u00e4hmt", "er", "schnell", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Hat den Arm um ihn geschlagen,", "tokens": ["Hat", "den", "Arm", "um", "ihn", "ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "APPR", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Beugt sich auf der Stirne Quell.", "tokens": ["Beugt", "sich", "auf", "der", "Stir\u00b7ne", "Quell", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "W\u00fc\u00dft' ein Band er, welches linde,", "tokens": ["W\u00fc\u00dft'", "ein", "Band", "er", ",", "wel\u00b7ches", "lin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPER", "$,", "PWAT", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Welches wundem Haupt bequem!", "tokens": ["Wel\u00b7ches", "wun\u00b7dem", "Haupt", "be\u00b7quem", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAT", "ADJA", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Armer, er hat keine Binde,", "tokens": ["Ar\u00b7mer", ",", "er", "hat", "kei\u00b7ne", "Bin\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VAFIN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Als im Haar sein Diadem!", "tokens": ["Als", "im", "Haar", "sein", "Di\u00b7a\u00b7dem", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPRART", "NN", "PPOSAT", "NN", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.5": {"text": "Fest und weich w\u00fcrd' es umh\u00fcllen", "tokens": ["Fest", "und", "weich", "w\u00fcrd'", "es", "um\u00b7h\u00fcl\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "ADJD", "VAFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Die verletzte Freundesstirn \u2013", "tokens": ["Die", "ver\u00b7letz\u00b7te", "Freun\u00b7dess\u00b7tirn", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Aber soll er selbst erf\u00fcllen", "tokens": ["A\u00b7ber", "soll", "er", "selbst", "er\u00b7f\u00fcl\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Seinen Traum aus bangem Hirn?", "tokens": ["Sei\u00b7nen", "Traum", "aus", "ban\u00b7gem", "Hirn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Immer dunkler flie\u00dft die Wunde,", "tokens": ["Im\u00b7mer", "dunk\u00b7ler", "flie\u00dft", "die", "Wun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bis sie weckt ein altes Bild,", "tokens": ["Bis", "sie", "weckt", "ein", "al\u00b7tes", "Bild", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bis dem Herrn im Herzensgrunde", "tokens": ["Bis", "dem", "Herrn", "im", "Her\u00b7zens\u00b7grun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Zweier Freunde Herzblut quillt.", "tokens": ["Zwei\u00b7er", "Freun\u00b7de", "Herz\u00b7blut", "quillt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Eines, das er selbst vergossen,", "tokens": ["Ei\u00b7nes", ",", "das", "er", "selbst", "ver\u00b7gos\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "PRELS", "PPER", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Eins, das auf dem Blocke sprang.", "tokens": ["Eins", ",", "das", "auf", "dem", "Blo\u00b7cke", "sprang", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Jetzt ist schon der F\u00fcrst entschlossen,", "tokens": ["Jetzt", "ist", "schon", "der", "F\u00fcrst", "ent\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Fa\u00dft das Band, das ihn umschlang.", "tokens": ["Fa\u00dft", "das", "Band", ",", "das", "ihn", "um\u00b7schlang", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Um des Retters Stirne windet", "tokens": ["Um", "des", "Ret\u00b7ters", "Stir\u00b7ne", "win\u00b7det"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUI", "ART", "NN", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Er's behend mit leichter Hand,", "tokens": ["Er's", "be\u00b7hend", "mit", "leich\u00b7ter", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "+---+-+", "measure": "dactylic.init"}, "line.3": {"text": "Sch\u00f6nes, bleiches Haupt, wie bindet", "tokens": ["Sch\u00f6\u00b7nes", ",", "blei\u00b7ches", "Haupt", ",", "wie", "bin\u00b7det"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADJA", "$,", "ADJA", "NN", "$,", "PWAV", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "F\u00fcrstlich dich das K\u00f6nigsband!", "tokens": ["F\u00fcrst\u00b7lich", "dich", "das", "K\u00f6\u00b7nigs\u00b7band", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Sinnend, aber heiter weilet", "tokens": ["Sin\u00b7nend", ",", "a\u00b7ber", "hei\u00b7ter", "wei\u00b7let"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["VVPP", "$,", "ADV", "ADJD", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Bei dem Anblick lang der Held.", "tokens": ["Bei", "dem", "An\u00b7blick", "lang", "der", "Held", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Dann mit dem Erwachten eilet", "tokens": ["Dann", "mit", "dem", "Er\u00b7wach\u00b7ten", "ei\u00b7let"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Auf dem Wagen er durch's Feld.", "tokens": ["Auf", "dem", "Wa\u00b7gen", "er", "durch's", "Feld", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPER", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Freudig zeigt er den Gekr\u00f6nten", "tokens": ["Freu\u00b7dig", "zeigt", "er", "den", "Ge\u00b7kr\u00f6n\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In den Mauern Babylons,", "tokens": ["In", "den", "Mau\u00b7ern", "Ba\u00b7by\u00b7lons", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Freudig tritt er im vers\u00f6hnten", "tokens": ["Freu\u00b7dig", "tritt", "er", "im", "ver\u00b7s\u00f6hn\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "APPRART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Geist die Stufen seines Throns;", "tokens": ["Geist", "die", "Stu\u00b7fen", "sei\u00b7nes", "Throns", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Gr\u00fc\u00dfet der Genossen Runde,", "tokens": ["Gr\u00fc\u00b7\u00dfet", "der", "Ge\u00b7nos\u00b7sen", "Run\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Voll und lose wallt sein Haar.", "tokens": ["Voll", "und", "lo\u00b7se", "wallt", "sein", "Haar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Seine Stirne, die gesunde,", "tokens": ["Sei\u00b7ne", "Stir\u00b7ne", ",", "die", "ge\u00b7sun\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "F\u00fchlt sich keiner Krone bar.", "tokens": ["F\u00fchlt", "sich", "kei\u00b7ner", "Kro\u00b7ne", "bar", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PIAT", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Und wie in den fr\u00fchen Tagen", "tokens": ["Und", "wie", "in", "den", "fr\u00fc\u00b7hen", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "APPR", "ART", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Hat der Held den Wandelthron", "tokens": ["Hat", "der", "Held", "den", "Wan\u00b7del\u00b7thron"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Froh errichtet auf dem Wagen,", "tokens": ["Froh", "er\u00b7rich\u00b7tet", "auf", "dem", "Wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ragt empor wie Thetis' Sohn.", "tokens": ["Ragt", "em\u00b7por", "wie", "The\u00b7tis'", "Sohn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "KOKOM", "NE", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "In der Linken wehn die Z\u00fcgel,", "tokens": ["In", "der", "Lin\u00b7ken", "wehn", "die", "Z\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und die Rechte wiegt den Speer,", "tokens": ["Und", "die", "Rech\u00b7te", "wiegt", "den", "Speer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.7": {"text": "Rossesm\u00e4hnen werden Fl\u00fcgel,", "tokens": ["Ros\u00b7ses\u00b7m\u00e4h\u00b7nen", "wer\u00b7den", "Fl\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "G\u00f6ttergleich braus't er einher.", "tokens": ["G\u00f6t\u00b7ter\u00b7gleich", "braus't", "er", "ein\u00b7her", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.10": {"line.1": {"text": "Und ein Andrer der Genossen,", "tokens": ["Und", "ein", "A\u00b7ndrer", "der", "Ge\u00b7nos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Treuer W\u00e4chter, flieget mit,", "tokens": ["Treu\u00b7er", "W\u00e4ch\u00b7ter", ",", "flie\u00b7get", "mit", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Thut zu Fu\u00df es gleich den Rossen,", "tokens": ["Thut", "zu", "Fu\u00df", "es", "gleich", "den", "Ros\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "NN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schneller war nicht Ajas' Schritt.", "tokens": ["Schnel\u00b7ler", "war", "nicht", "A\u00b7jas'", "Schritt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PTKNEG", "NE", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "In des Uferhaines Pfade", "tokens": ["In", "des", "U\u00b7fer\u00b7hai\u00b7nes", "Pfa\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Tieft die dunkle Fahrt sich ein,", "tokens": ["Tieft", "die", "dunk\u00b7le", "Fahrt", "sich", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "PRF", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Dort verst\u00f6rt am Moorgestade", "tokens": ["Dort", "ver\u00b7st\u00f6rt", "am", "Mo\u00b7or\u00b7ge\u00b7sta\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPRART", "NN"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.8": {"text": "Das Gespann ein grauer Stein.", "tokens": ["Das", "Ge\u00b7spann", "ein", "grau\u00b7er", "Stein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}, "stanza.11": {"line.1": {"text": "Wieder hebt sich aus den Hecken", "tokens": ["Wie\u00b7der", "hebt", "sich", "aus", "den", "He\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Das zerfallne K\u00f6nigsgrab,", "tokens": ["Das", "zer\u00b7fall\u00b7ne", "K\u00f6\u00b7nigs\u00b7grab", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und die Rosse hoch im Schrecken", "tokens": ["Und", "die", "Ros\u00b7se", "hoch", "im", "Schre\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJD", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "B\u00e4umen sich zum Strand hinab.", "tokens": ["B\u00e4u\u00b7men", "sich", "zum", "Strand", "hin\u00b7ab", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "APPRART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Die an wilder M\u00e4hne fassend", "tokens": ["Die", "an", "wil\u00b7der", "M\u00e4h\u00b7ne", "fas\u00b7send"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ADJA", "NN", "VVPP"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Rei\u00dft der W\u00e4chter k\u00fchn in's Gleis,", "tokens": ["Rei\u00dft", "der", "W\u00e4ch\u00b7ter", "k\u00fchn", "in's", "Gleis", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NE", "ADJD", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Am Geretteten, erblassend,", "tokens": ["Am", "Ge\u00b7ret\u00b7te\u00b7ten", ",", "er\u00b7blas\u00b7send", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["APPRART", "NN", "$,", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Sinkt er hin in Blut und Schwei\u00df.", "tokens": ["Sinkt", "er", "hin", "in", "Blut", "und", "Schwei\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Als den Rossen er mit Hitze", "tokens": ["Als", "den", "Ros\u00b7sen", "er", "mit", "Hit\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "PPER", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hemmend in den Z\u00fcgel fuhr,", "tokens": ["Hem\u00b7mend", "in", "den", "Z\u00fc\u00b7gel", "fuhr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ri\u00df des K\u00f6nigs Lanzenspitze", "tokens": ["Ri\u00df", "des", "K\u00f6\u00b7nigs", "Lan\u00b7zens\u00b7pit\u00b7ze"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In die Stirn ihm tiefe Spur.", "tokens": ["In", "die", "Stirn", "ihm", "tie\u00b7fe", "Spur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPER", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Der entschwingt sich rasch dem Wagen,", "tokens": ["Der", "ent\u00b7schwingt", "sich", "rasch", "dem", "Wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PRF", "ADJD", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Seine Rosse z\u00e4hmt er schnell,", "tokens": ["Sei\u00b7ne", "Ros\u00b7se", "z\u00e4hmt", "er", "schnell", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Hat den Arm um ihn geschlagen,", "tokens": ["Hat", "den", "Arm", "um", "ihn", "ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "APPR", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Beugt sich auf der Stirne Quell.", "tokens": ["Beugt", "sich", "auf", "der", "Stir\u00b7ne", "Quell", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "W\u00fc\u00dft' ein Band er, welches linde,", "tokens": ["W\u00fc\u00dft'", "ein", "Band", "er", ",", "wel\u00b7ches", "lin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPER", "$,", "PWAT", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Welches wundem Haupt bequem!", "tokens": ["Wel\u00b7ches", "wun\u00b7dem", "Haupt", "be\u00b7quem", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAT", "ADJA", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Armer, er hat keine Binde,", "tokens": ["Ar\u00b7mer", ",", "er", "hat", "kei\u00b7ne", "Bin\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VAFIN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Als im Haar sein Diadem!", "tokens": ["Als", "im", "Haar", "sein", "Di\u00b7a\u00b7dem", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPRART", "NN", "PPOSAT", "NN", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.5": {"text": "Fest und weich w\u00fcrd' es umh\u00fcllen", "tokens": ["Fest", "und", "weich", "w\u00fcrd'", "es", "um\u00b7h\u00fcl\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "ADJD", "VAFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Die verletzte Freundesstirn \u2013", "tokens": ["Die", "ver\u00b7letz\u00b7te", "Freun\u00b7dess\u00b7tirn", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Aber soll er selbst erf\u00fcllen", "tokens": ["A\u00b7ber", "soll", "er", "selbst", "er\u00b7f\u00fcl\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Seinen Traum aus bangem Hirn?", "tokens": ["Sei\u00b7nen", "Traum", "aus", "ban\u00b7gem", "Hirn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "Immer dunkler flie\u00dft die Wunde,", "tokens": ["Im\u00b7mer", "dunk\u00b7ler", "flie\u00dft", "die", "Wun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bis sie weckt ein altes Bild,", "tokens": ["Bis", "sie", "weckt", "ein", "al\u00b7tes", "Bild", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bis dem Herrn im Herzensgrunde", "tokens": ["Bis", "dem", "Herrn", "im", "Her\u00b7zens\u00b7grun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Zweier Freunde Herzblut quillt.", "tokens": ["Zwei\u00b7er", "Freun\u00b7de", "Herz\u00b7blut", "quillt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Eines, das er selbst vergossen,", "tokens": ["Ei\u00b7nes", ",", "das", "er", "selbst", "ver\u00b7gos\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "PRELS", "PPER", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Eins, das auf dem Blocke sprang.", "tokens": ["Eins", ",", "das", "auf", "dem", "Blo\u00b7cke", "sprang", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Jetzt ist schon der F\u00fcrst entschlossen,", "tokens": ["Jetzt", "ist", "schon", "der", "F\u00fcrst", "ent\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Fa\u00dft das Band, das ihn umschlang.", "tokens": ["Fa\u00dft", "das", "Band", ",", "das", "ihn", "um\u00b7schlang", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.15": {"line.1": {"text": "Um des Retters Stirne windet", "tokens": ["Um", "des", "Ret\u00b7ters", "Stir\u00b7ne", "win\u00b7det"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUI", "ART", "NN", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Er's behend mit leichter Hand,", "tokens": ["Er's", "be\u00b7hend", "mit", "leich\u00b7ter", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "+---+-+", "measure": "dactylic.init"}, "line.3": {"text": "Sch\u00f6nes, bleiches Haupt, wie bindet", "tokens": ["Sch\u00f6\u00b7nes", ",", "blei\u00b7ches", "Haupt", ",", "wie", "bin\u00b7det"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADJA", "$,", "ADJA", "NN", "$,", "PWAV", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "F\u00fcrstlich dich das K\u00f6nigsband!", "tokens": ["F\u00fcrst\u00b7lich", "dich", "das", "K\u00f6\u00b7nigs\u00b7band", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Sinnend, aber heiter weilet", "tokens": ["Sin\u00b7nend", ",", "a\u00b7ber", "hei\u00b7ter", "wei\u00b7let"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["VVPP", "$,", "ADV", "ADJD", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Bei dem Anblick lang der Held.", "tokens": ["Bei", "dem", "An\u00b7blick", "lang", "der", "Held", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Dann mit dem Erwachten eilet", "tokens": ["Dann", "mit", "dem", "Er\u00b7wach\u00b7ten", "ei\u00b7let"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Auf dem Wagen er durch's Feld.", "tokens": ["Auf", "dem", "Wa\u00b7gen", "er", "durch's", "Feld", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPER", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.16": {"line.1": {"text": "Freudig zeigt er den Gekr\u00f6nten", "tokens": ["Freu\u00b7dig", "zeigt", "er", "den", "Ge\u00b7kr\u00f6n\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In den Mauern Babylons,", "tokens": ["In", "den", "Mau\u00b7ern", "Ba\u00b7by\u00b7lons", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Freudig tritt er im vers\u00f6hnten", "tokens": ["Freu\u00b7dig", "tritt", "er", "im", "ver\u00b7s\u00f6hn\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "APPRART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Geist die Stufen seines Throns;", "tokens": ["Geist", "die", "Stu\u00b7fen", "sei\u00b7nes", "Throns", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Gr\u00fc\u00dfet der Genossen Runde,", "tokens": ["Gr\u00fc\u00b7\u00dfet", "der", "Ge\u00b7nos\u00b7sen", "Run\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Voll und lose wallt sein Haar.", "tokens": ["Voll", "und", "lo\u00b7se", "wallt", "sein", "Haar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Seine Stirne, die gesunde,", "tokens": ["Sei\u00b7ne", "Stir\u00b7ne", ",", "die", "ge\u00b7sun\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "F\u00fchlt sich keiner Krone bar.", "tokens": ["F\u00fchlt", "sich", "kei\u00b7ner", "Kro\u00b7ne", "bar", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PIAT", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}