{"dta.poem.23317": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Titelkram und Ordenbettel.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192634", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein kurzer Titel und ein d\u00fcnnes Band", "tokens": ["Ein", "kur\u00b7zer", "Ti\u00b7tel", "und", "ein", "d\u00fcn\u00b7nes", "Band"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "KON", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Gen\u00fcget f\u00fcr ein lang und schwer Verdienst:", "tokens": ["Ge\u00b7n\u00fc\u00b7get", "f\u00fcr", "ein", "lang", "und", "schwer", "Ver\u00b7dienst", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJD", "KON", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wie lernte sonst dein gutes Vaterland,", "tokens": ["Wie", "lern\u00b7te", "sonst", "dein", "gu\u00b7tes", "Va\u00b7ter\u00b7land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ADV", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Da\u00df du was bist was du ihm niemals schienst?", "tokens": ["Da\u00df", "du", "was", "bist", "was", "du", "ihm", "nie\u00b7mals", "schienst", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIS", "VAFIN", "PWS", "PPER", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Du gehst, und jeder sieht dein B\u00e4ndchen an,", "tokens": ["Du", "gehst", ",", "und", "je\u00b7der", "sieht", "dein", "B\u00e4nd\u00b7chen", "an", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KON", "PIS", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und ist von deiner Ehre hoch entz\u00fcckt:", "tokens": ["Und", "ist", "von", "dei\u00b7ner", "Eh\u00b7re", "hoch", "ent\u00b7z\u00fcckt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "PPOSAT", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Geziemend gr\u00fc\u00dft dich jetzo jedermann,", "tokens": ["Ge\u00b7zie\u00b7mend", "gr\u00fc\u00dft", "dich", "jet\u00b7zo", "je\u00b7der\u00b7mann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "PPER", "ADV", "PIS", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und ist von deinem Titel mitbegl\u00fcckt.", "tokens": ["Und", "ist", "von", "dei\u00b7nem", "Ti\u00b7tel", "mit\u00b7be\u00b7gl\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "F\u00fcrwahr, es ist nur purer blasser Neid,", "tokens": ["F\u00fcr\u00b7wahr", ",", "es", "ist", "nur", "pu\u00b7rer", "blas\u00b7ser", "Neid", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VAFIN", "ADV", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wenn man dir weder Band noch Titel g\u00f6nnt.", "tokens": ["Wenn", "man", "dir", "we\u00b7der", "Band", "noch", "Ti\u00b7tel", "g\u00f6nnt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "KON", "NN", "ADV", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Drum sag' ich auch zu allen jederzeit:", "tokens": ["Drum", "sag'", "ich", "auch", "zu", "al\u00b7len", "je\u00b7der\u00b7zeit", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ADV", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Seid still! er that gewi\u00df was ihr nicht k\u00f6nnt.", "tokens": ["Seid", "still", "!", "er", "that", "ge\u00b7wi\u00df", "was", "ihr", "nicht", "k\u00f6nnt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "$.", "PPER", "VVFIN", "ADV", "PWS", "PPER", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}