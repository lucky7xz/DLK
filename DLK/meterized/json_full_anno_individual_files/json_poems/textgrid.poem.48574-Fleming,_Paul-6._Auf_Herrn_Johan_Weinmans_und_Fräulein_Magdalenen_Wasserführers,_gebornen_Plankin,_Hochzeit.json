{"textgrid.poem.48574": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "6. Auf Herrn Johan Weinmans und Fr\u00e4ulein Magdalenen Wasserf\u00fchrers, gebornen Plankin, Hochzeit", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Recht so, liebe, traute Beide!", "tokens": ["Recht", "so", ",", "lie\u00b7be", ",", "trau\u00b7te", "Bei\u00b7de", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "VVFIN", "$,", "VVFIN", "PIS", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ihr bequemt euch nach der Zeit", "tokens": ["Ihr", "be\u00b7quemt", "euch", "nach", "der", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "und geht an die s\u00fc\u00dfe Freude,", "tokens": ["und", "geht", "an", "die", "s\u00fc\u00b7\u00dfe", "Freu\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "welcher itzt sich Alles freut.", "tokens": ["wel\u00b7cher", "itzt", "sich", "Al\u00b7les", "freut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRELS", "ADV", "PRF", "PIS", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Alles freiet in dem Maien:", "tokens": ["Al\u00b7les", "frei\u00b7et", "in", "dem", "Mai\u00b7en", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "solte denn der Mensch nicht freien?", "tokens": ["sol\u00b7te", "denn", "der", "Mensch", "nicht", "frei\u00b7en", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "NN", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Der verliebte Himmel l\u00e4chelt", "tokens": ["Der", "ver\u00b7lieb\u00b7te", "Him\u00b7mel", "l\u00e4\u00b7chelt"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "in die gleicherw\u00e4rmte Luft,", "tokens": ["in", "die", "glei\u00b7cher\u00b7w\u00e4rm\u00b7te", "Luft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "welche gleichsam K\u00fcsse f\u00e4chelt", "tokens": ["wel\u00b7che", "gleich\u00b7sam", "K\u00fcs\u00b7se", "f\u00e4\u00b7chelt"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAT", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "auf der schwangern Erden-Kluft,", "tokens": ["auf", "der", "schwan\u00b7gern", "Er\u00b7den\u00b7Kluft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "die bald Beiden, so sie liebet,", "tokens": ["die", "bald", "Bei\u00b7den", ",", "so", "sie", "lie\u00b7bet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PIS", "$,", "ADV", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "tausent' sch\u00f6ner Kinder giebet.", "tokens": ["tau\u00b7sent'", "sch\u00f6\u00b7ner", "Kin\u00b7der", "gie\u00b7bet", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Die demantenen Gew\u00e4sser", "tokens": ["Die", "de\u00b7man\u00b7te\u00b7nen", "Ge\u00b7w\u00e4s\u00b7ser"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "fliegen durch den jungen Mo\u00df,", "tokens": ["flie\u00b7gen", "durch", "den", "jun\u00b7gen", "Mo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "und die Wellen flechten besser", "tokens": ["und", "die", "Wel\u00b7len", "flech\u00b7ten", "bes\u00b7ser"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "einen in den andern Sto\u00df,", "tokens": ["ei\u00b7nen", "in", "den", "an\u00b7dern", "Sto\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "da\u00df es an den Ufern klinget,", "tokens": ["da\u00df", "es", "an", "den", "U\u00b7fern", "klin\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "als wenn Mund mit Munde ringet.", "tokens": ["als", "wenn", "Mund", "mit", "Mun\u00b7de", "rin\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOKOM", "KOUS", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Diese Tropfen, die wir schauen,", "tokens": ["Die\u00b7se", "Trop\u00b7fen", ",", "die", "wir", "schau\u00b7en", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "PRELS", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "wenn der Tag noch ist ein Kind,", "tokens": ["wenn", "der", "Tag", "noch", "ist", "ein", "Kind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "auf den aufgedeckten Auen,", "tokens": ["auf", "den", "auf\u00b7ge\u00b7deck\u00b7ten", "Au\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "gl\u00e4ubt es, da\u00df es K\u00fcsse sind,", "tokens": ["gl\u00e4ubt", "es", ",", "da\u00df", "es", "K\u00fcs\u00b7se", "sind", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "PPER", "NN", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "die die bulerischen Sternen", "tokens": ["die", "die", "bu\u00b7le\u00b7ri\u00b7schen", "Ster\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "lassen sinken her von fernen!", "tokens": ["las\u00b7sen", "sin\u00b7ken", "her", "von", "fer\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVINF", "VVFIN", "ADV", "APPR", "ADJA", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Und wer zweifelt an dem Bulen,", "tokens": ["Und", "wer", "zwei\u00b7felt", "an", "dem", "Bu\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "das ihr Federv\u00f6lker treibt", "tokens": ["das", "ihr", "Fe\u00b7der\u00b7v\u00f6l\u00b7ker", "treibt"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "PPOSAT", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "in den gr\u00fcnen W\u00e4lderschulen?", "tokens": ["in", "den", "gr\u00fc\u00b7nen", "W\u00e4l\u00b7der\u00b7schu\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Niemand lebt nicht, der nicht gl\u00e4ubt,", "tokens": ["Nie\u00b7mand", "lebt", "nicht", ",", "der", "nicht", "gl\u00e4ubt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PTKNEG", "$,", "PRELS", "PTKNEG", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "da\u00df die s\u00fc\u00dfen Melodeien", "tokens": ["da\u00df", "die", "s\u00fc\u00b7\u00dfen", "Me\u00b7lo\u00b7dei\u00b7en"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "nichts als Buler-Lieder seien.", "tokens": ["nichts", "als", "Bu\u00b7ler\u00b7Lie\u00b7der", "sei\u00b7en", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "KOKOM", "NE", "VAFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Sei gegr\u00fc\u00dft, du F\u00fcrst der Zeiten,", "tokens": ["Sei", "ge\u00b7gr\u00fc\u00dft", ",", "du", "F\u00fcrst", "der", "Zei\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "$,", "PPER", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "du des Jahrs Apell, o Mai!", "tokens": ["du", "des", "Jahrs", "A\u00b7pell", ",", "o", "Mai", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "NE", "$,", "FM", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wer wird mich wol \u00fcberstreiten,", "tokens": ["Wer", "wird", "mich", "wol", "\u00fc\u00b7bers\u00b7trei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "da\u00df itzt nicht gut freien sei,", "tokens": ["da\u00df", "itzt", "nicht", "gut", "frei\u00b7en", "sei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PTKNEG", "ADJD", "ADJA", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "da doch Alles, was sonst liebet,", "tokens": ["da", "doch", "Al\u00b7les", ",", "was", "sonst", "lie\u00b7bet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PIS", "$,", "PRELS", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "uns befugten Anla\u00df giebet?", "tokens": ["uns", "be\u00b7fug\u00b7ten", "An\u00b7la\u00df", "gie\u00b7bet", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Wenn die Tage l\u00e4nger werden,", "tokens": ["Wenn", "die", "Ta\u00b7ge", "l\u00e4n\u00b7ger", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADJD", "VAINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "und der g\u00fcldnen Sonnen Schein", "tokens": ["und", "der", "g\u00fcld\u00b7nen", "Son\u00b7nen", "Schein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "noch zur Zeit der muntern Erden", "tokens": ["noch", "zur", "Zeit", "der", "mun\u00b7tern", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "nicht gelernt beschwert zu sein,", "tokens": ["nicht", "ge\u00b7lernt", "be\u00b7schwert", "zu", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVPP", "VVPP", "PTKZU", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "wenn ihm Alles, was wir sehen,", "tokens": ["wenn", "ihm", "Al\u00b7les", ",", "was", "wir", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIS", "$,", "PRELS", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "l\u00e4sset billich wol geschehen:", "tokens": ["l\u00e4s\u00b7set", "bil\u00b7lich", "wol", "ge\u00b7sche\u00b7hen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ADV", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "das sind deine sch\u00f6nen Stunden,", "tokens": ["das", "sind", "dei\u00b7ne", "sch\u00f6\u00b7nen", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "o du Herzog aller Zier!", "tokens": ["o", "du", "Her\u00b7zog", "al\u00b7ler", "Zier", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM", "NE", "NE", "PIAT", "NN", "$."], "meter": "+-++--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Alles hastu dir verbunden,", "tokens": ["Al\u00b7les", "has\u00b7tu", "dir", "ver\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "und di\u00df liebe Paar allhier", "tokens": ["und", "di\u00df", "lie\u00b7be", "Paar", "all\u00b7hier"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PDS", "VVFIN", "NN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "giebet dir den Preis der Ehren,", "tokens": ["gie\u00b7bet", "dir", "den", "Preis", "der", "Eh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "da\u00df du wol kanst freien lehren.", "tokens": ["da\u00df", "du", "wol", "kanst", "frei\u00b7en", "leh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VMFIN", "VVINF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Sch\u00f6ne Braut, gedenkt zur\u00fccke", "tokens": ["Sch\u00f6\u00b7ne", "Braut", ",", "ge\u00b7denkt", "zu\u00b7r\u00fc\u00b7cke"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$,", "VVFIN", "PTKVZ"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "und erw\u00e4gt des Himmels Gunst,", "tokens": ["und", "er\u00b7w\u00e4gt", "des", "Him\u00b7mels", "Gunst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "der euch, helfe Gott zu Gl\u00fccke!", "tokens": ["der", "euch", ",", "hel\u00b7fe", "Gott", "zu", "Gl\u00fc\u00b7cke", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "$,", "VVFIN", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "einen ", "tokens": ["ei\u00b7nen"], "token_info": ["word"], "pos": ["ART"], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "einen ", "tokens": ["ei\u00b7nen"], "token_info": ["word"], "pos": ["ART"], "meter": "+-", "measure": "trochaic.single"}, "line.6": {"text": "f\u00fcr den ", "tokens": ["f\u00fcr", "den"], "token_info": ["word", "word"], "pos": ["APPR", "ART"], "meter": "+-", "measure": "trochaic.single"}}, "stanza.10": {"line.1": {"text": "Gehet nun, umfangt den Lieben", "tokens": ["Ge\u00b7het", "nun", ",", "um\u00b7fangt", "den", "Lie\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ADV", "$,", "VVFIN", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "und tut, wie ihr vor getan,", "tokens": ["und", "tut", ",", "wie", "ihr", "vor", "ge\u00b7tan", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PWAV", "PPER", "APPR", "VVPP", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "setzet fort, was vor verblieben,", "tokens": ["set\u00b7zet", "fort", ",", "was", "vor", "ver\u00b7blie\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "PRELS", "APPR", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "weiset euren ", "tokens": ["wei\u00b7set", "eu\u00b7ren"], "token_info": ["word", "word"], "pos": ["VVFIN", "PPOSAT"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "da\u00df er eurer ", "tokens": ["da\u00df", "er", "eu\u00b7rer"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPER", "PPOSAT"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "wol um seinen Weinberg setze!", "tokens": ["wol", "um", "sei\u00b7nen", "Wein\u00b7berg", "set\u00b7ze", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Ich verhoff' es zu erleben,", "tokens": ["Ich", "ver\u00b7hoff'", "es", "zu", "er\u00b7le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "da\u00df, wenn uns der warme Wein", "tokens": ["da\u00df", ",", "wenn", "uns", "der", "war\u00b7me", "Wein"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "$,", "KOUS", "PPER", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "pflegt am meisten Lust zu geben,", "tokens": ["pflegt", "am", "meis\u00b7ten", "Lust", "zu", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "dort, wenn es noch kalt mu\u00df sein,", "tokens": ["dort", ",", "wenn", "es", "noch", "kalt", "mu\u00df", "sein", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PPER", "ADV", "ADJD", "VMFIN", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "zu dem Trunk' ein s\u00fc\u00dfes Lachen", "tokens": ["zu", "dem", "Trunk'", "ein", "s\u00fc\u00b7\u00dfes", "La\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "euch das liebe Kind wird machen.", "tokens": ["euch", "das", "lie\u00b7be", "Kind", "wird", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "VAFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Recht so, liebe, traute Beide!", "tokens": ["Recht", "so", ",", "lie\u00b7be", ",", "trau\u00b7te", "Bei\u00b7de", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "VVFIN", "$,", "VVFIN", "PIS", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ihr bequemt euch nach der Zeit", "tokens": ["Ihr", "be\u00b7quemt", "euch", "nach", "der", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "und geht an die s\u00fc\u00dfe Freude,", "tokens": ["und", "geht", "an", "die", "s\u00fc\u00b7\u00dfe", "Freu\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "welcher itzt sich Alles freut.", "tokens": ["wel\u00b7cher", "itzt", "sich", "Al\u00b7les", "freut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRELS", "ADV", "PRF", "PIS", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Alles freiet in dem Maien:", "tokens": ["Al\u00b7les", "frei\u00b7et", "in", "dem", "Mai\u00b7en", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "solte denn der Mensch nicht freien?", "tokens": ["sol\u00b7te", "denn", "der", "Mensch", "nicht", "frei\u00b7en", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "NN", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Der verliebte Himmel l\u00e4chelt", "tokens": ["Der", "ver\u00b7lieb\u00b7te", "Him\u00b7mel", "l\u00e4\u00b7chelt"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "in die gleicherw\u00e4rmte Luft,", "tokens": ["in", "die", "glei\u00b7cher\u00b7w\u00e4rm\u00b7te", "Luft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "welche gleichsam K\u00fcsse f\u00e4chelt", "tokens": ["wel\u00b7che", "gleich\u00b7sam", "K\u00fcs\u00b7se", "f\u00e4\u00b7chelt"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAT", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "auf der schwangern Erden-Kluft,", "tokens": ["auf", "der", "schwan\u00b7gern", "Er\u00b7den\u00b7Kluft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "die bald Beiden, so sie liebet,", "tokens": ["die", "bald", "Bei\u00b7den", ",", "so", "sie", "lie\u00b7bet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PIS", "$,", "ADV", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "tausent' sch\u00f6ner Kinder giebet.", "tokens": ["tau\u00b7sent'", "sch\u00f6\u00b7ner", "Kin\u00b7der", "gie\u00b7bet", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "Die demantenen Gew\u00e4sser", "tokens": ["Die", "de\u00b7man\u00b7te\u00b7nen", "Ge\u00b7w\u00e4s\u00b7ser"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "fliegen durch den jungen Mo\u00df,", "tokens": ["flie\u00b7gen", "durch", "den", "jun\u00b7gen", "Mo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "und die Wellen flechten besser", "tokens": ["und", "die", "Wel\u00b7len", "flech\u00b7ten", "bes\u00b7ser"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "einen in den andern Sto\u00df,", "tokens": ["ei\u00b7nen", "in", "den", "an\u00b7dern", "Sto\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "da\u00df es an den Ufern klinget,", "tokens": ["da\u00df", "es", "an", "den", "U\u00b7fern", "klin\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "als wenn Mund mit Munde ringet.", "tokens": ["als", "wenn", "Mund", "mit", "Mun\u00b7de", "rin\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOKOM", "KOUS", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.15": {"line.1": {"text": "Diese Tropfen, die wir schauen,", "tokens": ["Die\u00b7se", "Trop\u00b7fen", ",", "die", "wir", "schau\u00b7en", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "PRELS", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "wenn der Tag noch ist ein Kind,", "tokens": ["wenn", "der", "Tag", "noch", "ist", "ein", "Kind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "auf den aufgedeckten Auen,", "tokens": ["auf", "den", "auf\u00b7ge\u00b7deck\u00b7ten", "Au\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "gl\u00e4ubt es, da\u00df es K\u00fcsse sind,", "tokens": ["gl\u00e4ubt", "es", ",", "da\u00df", "es", "K\u00fcs\u00b7se", "sind", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "PPER", "NN", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "die die bulerischen Sternen", "tokens": ["die", "die", "bu\u00b7le\u00b7ri\u00b7schen", "Ster\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "lassen sinken her von fernen!", "tokens": ["las\u00b7sen", "sin\u00b7ken", "her", "von", "fer\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVINF", "VVFIN", "ADV", "APPR", "ADJA", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.16": {"line.1": {"text": "Und wer zweifelt an dem Bulen,", "tokens": ["Und", "wer", "zwei\u00b7felt", "an", "dem", "Bu\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "das ihr Federv\u00f6lker treibt", "tokens": ["das", "ihr", "Fe\u00b7der\u00b7v\u00f6l\u00b7ker", "treibt"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "PPOSAT", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "in den gr\u00fcnen W\u00e4lderschulen?", "tokens": ["in", "den", "gr\u00fc\u00b7nen", "W\u00e4l\u00b7der\u00b7schu\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Niemand lebt nicht, der nicht gl\u00e4ubt,", "tokens": ["Nie\u00b7mand", "lebt", "nicht", ",", "der", "nicht", "gl\u00e4ubt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PTKNEG", "$,", "PRELS", "PTKNEG", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "da\u00df die s\u00fc\u00dfen Melodeien", "tokens": ["da\u00df", "die", "s\u00fc\u00b7\u00dfen", "Me\u00b7lo\u00b7dei\u00b7en"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "nichts als Buler-Lieder seien.", "tokens": ["nichts", "als", "Bu\u00b7ler\u00b7Lie\u00b7der", "sei\u00b7en", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "KOKOM", "NE", "VAFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.17": {"line.1": {"text": "Sei gegr\u00fc\u00dft, du F\u00fcrst der Zeiten,", "tokens": ["Sei", "ge\u00b7gr\u00fc\u00dft", ",", "du", "F\u00fcrst", "der", "Zei\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "$,", "PPER", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "du des Jahrs Apell, o Mai!", "tokens": ["du", "des", "Jahrs", "A\u00b7pell", ",", "o", "Mai", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "NE", "$,", "FM", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wer wird mich wol \u00fcberstreiten,", "tokens": ["Wer", "wird", "mich", "wol", "\u00fc\u00b7bers\u00b7trei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "da\u00df itzt nicht gut freien sei,", "tokens": ["da\u00df", "itzt", "nicht", "gut", "frei\u00b7en", "sei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PTKNEG", "ADJD", "ADJA", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "da doch Alles, was sonst liebet,", "tokens": ["da", "doch", "Al\u00b7les", ",", "was", "sonst", "lie\u00b7bet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PIS", "$,", "PRELS", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "uns befugten Anla\u00df giebet?", "tokens": ["uns", "be\u00b7fug\u00b7ten", "An\u00b7la\u00df", "gie\u00b7bet", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.18": {"line.1": {"text": "Wenn die Tage l\u00e4nger werden,", "tokens": ["Wenn", "die", "Ta\u00b7ge", "l\u00e4n\u00b7ger", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADJD", "VAINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "und der g\u00fcldnen Sonnen Schein", "tokens": ["und", "der", "g\u00fcld\u00b7nen", "Son\u00b7nen", "Schein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "noch zur Zeit der muntern Erden", "tokens": ["noch", "zur", "Zeit", "der", "mun\u00