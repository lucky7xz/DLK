asure": "trochaic.tetra"}, "line.4": {"text": "und di\u00df liebe Paar allhier", "tokens": ["und", "di\u00df", "lie\u00b7be", "Paar", "all\u00b7hier"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PDS", "VVFIN", "NN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "giebet dir den Preis der Ehren,", "tokens": ["gie\u00b7bet", "dir", "den", "Preis", "der", "Eh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "da\u00df du wol kanst freien lehren.", "tokens": ["da\u00df", "du", "wol", "kanst", "frei\u00b7en", "leh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VMFIN", "VVINF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.20": {"line.1": {"text": "Sch\u00f6ne Braut, gedenkt zur\u00fccke", "tokens": ["Sch\u00f6\u00b7ne", "Braut", ",", "ge\u00b7denkt", "zu\u00b7r\u00fc\u00b7cke"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$,", "VVFIN", "PTKVZ"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "und erw\u00e4gt des Himmels Gunst,", "tokens": ["und", "er\u00b7w\u00e4gt", "des", "Him\u00b7mels", "Gunst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "der euch, helfe Gott zu Gl\u00fccke!", "tokens": ["der", "euch", ",", "hel\u00b7fe", "Gott", "zu", "Gl\u00fc\u00b7cke", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "$,", "VVFIN", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "einen ", "tokens": ["ei\u00b7nen"], "token_info": ["word"], "pos