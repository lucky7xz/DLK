{"dta.poem.4269": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die sehr sp\u00e4te Heu-Erndte,  \n 1740. den 4 August.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.85", "sv:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Gott Lob! dort f\u00e4hrt man endlich Heu,", "tokens": ["Gott", "Lob", "!", "dort", "f\u00e4hrt", "man", "end\u00b7lich", "Heu", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$.", "ADV", "VVFIN", "PIS", "ADV", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So, wie in vor\u2019gen Zeiten, ein;", "tokens": ["So", ",", "wie", "in", "vor'\u00b7gen", "Zei\u00b7ten", ",", "ein", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "APPR", "ADJA", "NN", "$,", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wodurch wir all\u2019 erfreuet seyn,", "tokens": ["Wo\u00b7durch", "wir", "all'", "er\u00b7freu\u00b7et", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PIS", "VVFIN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und ich besonders mich erfreu\u2019.", "tokens": ["Und", "ich", "be\u00b7son\u00b7ders", "mich", "er\u00b7freu'", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da die fast nie erh\u00f6rte K\u00e4lt\u2019,", "tokens": ["Da", "die", "fast", "nie", "er\u00b7h\u00f6r\u00b7te", "K\u00e4lt'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADV", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und nie erlebte Witterung,", "tokens": ["Und", "nie", "er\u00b7leb\u00b7te", "Wit\u00b7te\u00b7rung", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Dem Ansehn nach, der ganzen Welt,", "tokens": ["Dem", "An\u00b7sehn", "nach", ",", "der", "gan\u00b7zen", "Welt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Durch Mangel n\u00f6ht\u2019ger F\u00fctterung,", "tokens": ["Durch", "Man\u00b7gel", "n\u00f6ht'\u00b7ger", "F\u00fct\u00b7te\u00b7rung", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Ein grosses Ungl\u00fcck, Hungers-Noht,", "tokens": ["Ein", "gros\u00b7ses", "Un\u00b7gl\u00fcck", ",", "Hun\u00b7ger\u00b7sNoht", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Ja, Vieh und Menschen fast den Tod", "tokens": ["Ja", ",", "Vieh", "und", "Men\u00b7schen", "fast", "den", "Tod"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "NN", "KON", "NN", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Zumtheil gewirkt, zumtheil gedroht,", "tokens": ["Zumt\u00b7heil", "ge\u00b7wirkt", ",", "zumt\u00b7heil", "ge\u00b7droht", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "VVPP", "$,", "KOUS", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Ist dennoch nicht nur so viel Gras,", "tokens": ["Ist", "den\u00b7noch", "nicht", "nur", "so", "viel", "Gras", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PTKNEG", "ADV", "ADV", "PIAT", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.13": {"text": "Zu unsers Viehes vollem Fras,", "tokens": ["Zu", "un\u00b7sers", "Vie\u00b7hes", "vol\u00b7lem", "Fras", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "In s\u00fcssem Ueberflu\u00df, erschienen;", "tokens": ["In", "s\u00fcs\u00b7sem", "Ue\u00b7berf\u00b7lu\u00df", ",", "er\u00b7schie\u00b7nen", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Gott hat uns auch, wie wir hier seh\u2019n,", "tokens": ["Gott", "hat", "uns", "auch", ",", "wie", "wir", "hier", "seh'n", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ADV", "$,", "PWAV", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Jm Winter unser Vieh zu dienen,", "tokens": ["Jm", "Win\u00b7ter", "un\u00b7ser", "Vieh", "zu", "die\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Auch auf das K\u00fcnftige verseh\u2019n.", "tokens": ["Auch", "auf", "das", "K\u00fcnf\u00b7ti\u00b7ge", "ver\u00b7seh'", "n."], "token_info": ["word", "word", "word", "word", "word", "abbreviation"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Ach, m\u00f6chte kein zu starker Regen", "tokens": ["Ach", ",", "m\u00f6ch\u00b7te", "kein", "zu", "star\u00b7ker", "Re\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "VMFIN", "PIAT", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Das, was uns noch beschehret, mindern,", "tokens": ["Das", ",", "was", "uns", "noch", "be\u00b7scheh\u00b7ret", ",", "min\u00b7dern", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PDS", "$,", "PRELS", "PPER", "ADV", "VVFIN", "$,", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Und uns es einzufahren hindern!", "tokens": ["Und", "uns", "es", "ein\u00b7zu\u00b7fah\u00b7ren", "hin\u00b7dern", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PPER", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ist man nun je, durch diesen Segen,", "tokens": ["Ist", "man", "nun", "je", ",", "durch", "die\u00b7sen", "Se\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "ADV", "ADV", "$,", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zu Dem, Der alle Welt regiert,", "tokens": ["Zu", "Dem", ",", "Der", "al\u00b7le", "Welt", "re\u00b7giert", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$,", "ART", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In schuld\u2019ger Dankbarkeit, gef\u00fchrt;", "tokens": ["In", "schuld'\u00b7ger", "Dank\u00b7bar\u00b7keit", ",", "ge\u00b7f\u00fchrt", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So sollte man itzt \u00fcberlegen,", "tokens": ["So", "soll\u00b7te", "man", "itzt", "\u00fc\u00b7berl\u00b7e\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIS", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "(da wir die Folgen angeseh\u2019n,", "tokens": ["(", "da", "wir", "die", "Fol\u00b7gen", "an\u00b7ge\u00b7seh'n", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die, wenn nur blo\u00df das Gras uns fehlet,", "tokens": ["Die", ",", "wenn", "nur", "blo\u00df", "das", "Gras", "uns", "feh\u00b7let", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "KOUS", "ADV", "ADV", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Aus dessen Mangel uns entsteh\u2019n,", "tokens": ["Aus", "des\u00b7sen", "Man\u00b7gel", "uns", "ent\u00b7steh'n", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da er so Vieh als Menschen qu\u00e4let,)", "tokens": ["Da", "er", "so", "Vieh", "als", "Men\u00b7schen", "qu\u00e4\u00b7let", ",", ")"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "ADV", "NN", "KOUS", "NN", "VVFIN", "$,", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wie Dem Lob, Preis und Dank geb\u00fchrt,", "tokens": ["Wie", "Dem", "Lob", ",", "Preis", "und", "Dank", "ge\u00b7b\u00fchrt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der uns, in vor\u2019ger Jahres-Zeit,", "tokens": ["Der", "uns", ",", "in", "vor'\u00b7ger", "Jah\u00b7res\u00b7Zeit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Recht augenscheinlich sehen lassen,", "tokens": ["Recht", "au\u00b7gen\u00b7schein\u00b7lich", "se\u00b7hen", "las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Es sey des Grases Fruchtbarkeit", "tokens": ["Es", "sey", "des", "Gra\u00b7ses", "Frucht\u00b7bar\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Kein schuldigs Gut; und da\u00df wir fassen,", "tokens": ["Kein", "schul\u00b7digs", "Gut", ";", "und", "da\u00df", "wir", "fas\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$.", "KON", "KOUS", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Es schenk\u2019 es uns kein Ungefehr,", "tokens": ["Es", "schenk'", "es", "uns", "kein", "Un\u00b7ge\u00b7fehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PPER", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Kein blinder Noht-Zwang, sondern Er.", "tokens": ["Kein", "blin\u00b7der", "Noht\u00b7Zwang", ",", "son\u00b7dern", "Er", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$,", "KON", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Auf! la\u00dft uns denn, mit froher Seelen,", "tokens": ["Auf", "!", "la\u00dft", "uns", "denn", ",", "mit", "fro\u00b7her", "See\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "$.", "VVIMP", "PPER", "ADV", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Des Sch\u00f6pfers Macht und Lieb\u2019 erzehlen,", "tokens": ["Des", "Sch\u00f6p\u00b7fers", "Macht", "und", "Lieb'", "er\u00b7zeh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Bewundern seine weise G\u00fcte,", "tokens": ["Be\u00b7wun\u00b7dern", "sei\u00b7ne", "wei\u00b7se", "G\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mit recht erkenntlichem Gem\u00fchte,", "tokens": ["Mit", "recht", "er\u00b7kennt\u00b7li\u00b7chem", "Ge\u00b7m\u00fch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Da Er, zu unserm eignen Heil,", "tokens": ["Da", "Er", ",", "zu", "un\u00b7serm", "eig\u00b7nen", "Heil", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Uns, auch durch Mangel, zu sich ziehet,", "tokens": ["Uns", ",", "auch", "durch", "Man\u00b7gel", ",", "zu", "sich", "zie\u00b7het", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ADV", "APPR", "NN", "$,", "APPR", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Sich gleichsam v\u00e4terlich bem\u00fchet,", "tokens": ["Sich", "gleich\u00b7sam", "v\u00e4\u00b7ter\u00b7lich", "be\u00b7m\u00fc\u00b7het", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Uns aus der Unempfindlichkeit,", "tokens": ["Uns", "aus", "der", "Un\u00b7emp\u00b7find\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Die der Gewohnheit Frucht, zu reissen,", "tokens": ["Die", "der", "Ge\u00b7wohn\u00b7heit", "Frucht", ",", "zu", "reis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "NN", "$,", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Damit wir Jhm die Ehre g\u00f6nnen,", "tokens": ["Da\u00b7mit", "wir", "Jhm", "die", "Eh\u00b7re", "g\u00f6n\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Jhn, als die Quell\u2019 des Guten, kennen,", "tokens": ["Jhn", ",", "als", "die", "Quell'", "des", "Gu\u00b7ten", ",", "ken\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "$,", "KOUS", "ART", "NN", "ART", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Uns nicht durch Undank von Jhm trennen,", "tokens": ["Uns", "nicht", "durch", "Un\u00b7dank", "von", "Jhm", "tren\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "APPR", "NN", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Wodurch wir, blo\u00df auf dieser Erden,", "tokens": ["Wo\u00b7durch", "wir", ",", "blo\u00df", "auf", "die\u00b7ser", "Er\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "$,", "ADV", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Vergn\u00fcgt, und dorten selig werden.", "tokens": ["Ver\u00b7gn\u00fcgt", ",", "und", "dor\u00b7ten", "se\u00b7lig", "wer\u00b7den", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KON", "ADV", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "La\u00dft uns zugleich dabey erwegen,", "tokens": ["La\u00dft", "uns", "zu\u00b7gleich", "da\u00b7bey", "er\u00b7we\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "PAV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie der Zusammenhang so klar,", "tokens": ["Wie", "der", "Zu\u00b7sam\u00b7men\u00b7hang", "so", "klar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "So n\u00f6tig und so wunderbar,", "tokens": ["So", "n\u00f6\u00b7tig", "und", "so", "wun\u00b7der\u00b7bar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der in des n\u00e4hr\u2019nden Grases Segen,", "tokens": ["Der", "in", "des", "n\u00e4hr'n\u00b7den", "Gra\u00b7ses", "Se\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Durch Gottes Ordnung, herrscht und liegt;", "tokens": ["Durch", "Got\u00b7tes", "Ord\u00b7nung", ",", "herrscht", "und", "liegt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "$,", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da Er, durchs Gras, uns und das Vieh,", "tokens": ["Da", "Er", ",", "durchs", "Gras", ",", "uns", "und", "das", "Vieh", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "APPRART", "NN", "$,", "PPER", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Zu beyder Nutzen, so gef\u00fcget,", "tokens": ["Zu", "bey\u00b7der", "Nut\u00b7zen", ",", "so", "ge\u00b7f\u00fc\u00b7get", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Da man das Vieh, die\u00df uns vergn\u00fcget.", "tokens": ["Da", "man", "das", "Vieh", ",", "die\u00df", "uns", "ver\u00b7gn\u00fc\u00b7get", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "NN", "$,", "PDS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Da jenes uns in unserm Leben", "tokens": ["Da", "je\u00b7nes", "uns", "in", "un\u00b7serm", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PDS", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zwar dient, doch wir auch wieder ihnen,", "tokens": ["Zwar", "dient", ",", "doch", "wir", "auch", "wie\u00b7der", "ih\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "KON", "PPER", "ADV", "ADV", "PPER", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Da wir ihm Heu im Winter geben,", "tokens": ["Da", "wir", "ihm", "Heu", "im", "Win\u00b7ter", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "NN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn man es recht bedenket, dienen.", "tokens": ["Wenn", "man", "es", "recht", "be\u00b7den\u00b7ket", ",", "die\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "ADJD", "VVFIN", "$,", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Durch welche kleine Wechsel-M\u00fch", "tokens": ["Durch", "wel\u00b7che", "klei\u00b7ne", "Wech\u00b7sel\u00b7M\u00fch"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PWAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die grosse Ordnung in der Welt", "tokens": ["Die", "gros\u00b7se", "Ord\u00b7nung", "in", "der", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Sich bindet und zusammen h\u00e4lt:", "tokens": ["Sich", "bin\u00b7det", "und", "zu\u00b7sam\u00b7men", "h\u00e4lt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "KON", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Hiedurch erh\u00e4lt sich Mensch und Vieh.", "tokens": ["Hie\u00b7durch", "er\u00b7h\u00e4lt", "sich", "Mensch", "und", "Vieh", "."], "tok