{"dta.poem.12949": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Als er sie vergebens suchte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wo bist du denn, mein holdes Licht?", "tokens": ["Wo", "bist", "du", "denn", ",", "mein", "hol\u00b7des", "Licht", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So frag\u2019 ich; aber ach! we\u00dfwegen frag\u2019 ich nicht,", "tokens": ["So", "frag'", "ich", ";", "a\u00b7ber", "ach", "!", "we\u00df\u00b7we\u00b7gen", "frag'", "ich", "nicht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "$.", "ADV", "ADV", "$.", "PWAV", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wo ich mich selbst befinde?", "tokens": ["Wo", "ich", "mich", "selbst", "be\u00b7fin\u00b7de", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "ADV", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Leb ich in dir,", "tokens": ["Leb", "ich", "in", "dir", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "PPER", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Und du in mir?", "tokens": ["Und", "du", "in", "mir", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "So bin ich ja bey meinem liebsten Kinde.", "tokens": ["So", "bin", "ich", "ja", "bey", "mei\u00b7nem", "liebs\u00b7ten", "Kin\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Indessen such\u2019 ich, wo ich kan,", "tokens": ["In\u00b7des\u00b7sen", "such'", "ich", ",", "wo", "ich", "kan", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PWAV", "PPER", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und treffe dich doch nirgends an.", "tokens": ["Und", "tref\u00b7fe", "dich", "doch", "nir\u00b7gends", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Ja was unglaublich scheint, mein angenehmstes Licht!", "tokens": ["Ja", "was", "un\u00b7glaub\u00b7lich", "scheint", ",", "mein", "an\u00b7ge\u00b7nehms\u00b7tes", "Licht", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "PRELS", "ADJD", "VVFIN", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Ich wei\u00df es, wo ich bin, und wei\u00df es doch auch nicht.", "tokens": ["Ich", "wei\u00df", "es", ",", "wo", "ich", "bin", ",", "und", "wei\u00df", "es", "doch", "auch", "nicht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PWAV", "PPER", "VAFIN", "$,", "KON", "VVFIN", "PPER", "ADV", "ADV", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}