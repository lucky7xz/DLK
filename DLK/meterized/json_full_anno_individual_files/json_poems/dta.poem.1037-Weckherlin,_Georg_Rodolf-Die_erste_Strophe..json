{"dta.poem.1037": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "Die erste Strophe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1618", "urn": "urn:nbn:de:kobv:b4-20628-9", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Als der Rhein f\u00fcr gewi\u00df verstanden", "tokens": ["Als", "der", "Rhein", "f\u00fcr", "ge\u00b7wi\u00df", "ver\u00b7stan\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NE", "APPR", "ADV", "VVINF"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Da\u00df einer G\u00f6ttin treflichkeit", "tokens": ["Da\u00df", "ei\u00b7ner", "G\u00f6t\u00b7tin", "tref\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Seines F\u00fcrsten hertzens freyheit", "tokens": ["Sei\u00b7nes", "F\u00fcrs\u00b7ten", "hert\u00b7zens", "frey\u00b7heit"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADV", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gef\u00e4ncklich hielt in jhren banden:", "tokens": ["Ge\u00b7f\u00e4nck\u00b7lich", "hielt", "in", "jhren", "ban\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Hat Er/ ligend ab solcher lieb", "tokens": ["Hat", "Er", "/", "li\u00b7gend", "ab", "sol\u00b7cher", "lieb"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "$(", "ADJD", "APPR", "PIAT", "ADJD"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.6": {"text": "In seinem gew\u00f6lbe verdrossen/", "tokens": ["In", "sei\u00b7nem", "ge\u00b7w\u00f6l\u00b7be", "ver\u00b7dros\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "VVINF", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.7": {"text": "Ein solchen zeherflu\u00df vergossen/", "tokens": ["Ein", "sol\u00b7chen", "ze\u00b7her\u00b7flu\u00df", "ver\u00b7gos\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Da\u00df seine herrschafft davon tr\u00fcb.", "tokens": ["Da\u00df", "sei\u00b7ne", "herr\u00b7schafft", "da\u00b7von", "tr\u00fcb", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PAV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Er f\u00fchret Amors list zu hertzen;", "tokens": ["Er", "f\u00fch\u00b7ret", "A\u00b7mors", "list", "zu", "hert\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Sarpedons vnd Leanders schmertzen/", "tokens": ["Sar\u00b7pe\u00b7dons", "vnd", "Le\u00b7an\u00b7ders", "schmert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Ja de\u00df Peleiden vngl\u00fcck", "tokens": ["Ja", "de\u00df", "Pe\u00b7lei\u00b7den", "vn\u00b7gl\u00fcck"], "token_info": ["word", "word", "word", "word"], "pos": ["PTKANT", "ART", "ADJA", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.12": {"text": "Verblaichen jhn all augenblick;", "tokens": ["Ver\u00b7blai\u00b7chen", "jhn", "all", "au\u00b7gen\u00b7blick", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "In dem ein pl\u00f6tzliches get\u00fcmmel", "tokens": ["In", "dem", "ein", "pl\u00f6tz\u00b7li\u00b7ches", "ge\u00b7t\u00fcm\u00b7mel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Welches Er vnversehens h\u00f6rt/", "tokens": ["Wel\u00b7ches", "Er", "vn\u00b7ver\u00b7se\u00b7hens", "h\u00f6rt", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "VVFIN", "$("], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.15": {"text": "F\u00fcllet den abgrund vnd die himmel/", "tokens": ["F\u00fcl\u00b7let", "den", "ab\u00b7grund", "vnd", "die", "him\u00b7mel", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "KON", "ART", "NN", "$("], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.16": {"text": "Vnd seine sehl noch mehr beth\u00f6rt.", "tokens": ["Vnd", "sei\u00b7ne", "sehl", "noch", "mehr", "be\u00b7th\u00f6rt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}