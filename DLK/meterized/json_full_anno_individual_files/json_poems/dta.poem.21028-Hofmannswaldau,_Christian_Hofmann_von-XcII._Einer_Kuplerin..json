{"dta.poem.21028": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "XcII.  \n Einer Kuplerin.", "genre": "Lyrik; Prosa; Drama", "period": "N.A.", "pub_year": "1679", "urn": "urn:nbn:de:kobv:b4-20289-1", "language": ["de:0.99"], "booktitle": "Hofmann von Hofmannswaldau, Christian: Deutsche Ubersetzungen und Gedichte. Breslau, 1679."}, "poem": {"stanza.1": {"line.1": {"text": "Die Jugend hab ich stets mit Bulen zugebracht/", "tokens": ["Die", "Ju\u00b7gend", "hab", "ich", "stets", "mit", "Bu\u00b7len", "zu\u00b7ge\u00b7bracht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADV", "APPR", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als nun das Alter kam und meiner Jugend Nacht/", "tokens": ["Als", "nun", "das", "Al\u00b7ter", "kam", "und", "mei\u00b7ner", "Ju\u00b7gend", "Nacht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "VVFIN", "KON", "PPOSAT", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So war ich K\u00f6hlen gleich die junges Holtz entz\u00fcn-", "tokens": ["So", "war", "ich", "K\u00f6h\u00b7len", "gleich", "die", "jun\u00b7ges", "Holtz", "ent\u00b7z\u00fcn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "NN", "ADV", "ART", "ADJA", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "den/", "tokens": ["den", "/"], "token_info": ["word", "punct"], "pos": ["ART", "$("], "meter": "-", "measure": "single.down"}, "line.5": {"text": "Die Asche wird man noch umb diese Gegend finden.", "tokens": ["Die", "A\u00b7sche", "wird", "man", "noch", "umb", "die\u00b7se", "Ge\u00b7gend", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PIS", "ADV", "APPR", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}