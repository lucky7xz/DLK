{"textgrid.poem.42146": {"metadata": {"author": {"name": "Wedekind, Frank", "birth": "N.A.", "death": "N.A."}, "title": "Idyll", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zum Kellner sprach die Kellnerin:", "tokens": ["Zum", "Kell\u00b7ner", "sprach", "die", "Kell\u00b7ne\u00b7rin", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mir wird so sonderbar zu Sinn,", "tokens": ["Mir", "wird", "so", "son\u00b7der\u00b7bar", "zu", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich finde mich ganz ver\u00e4ndert.", "tokens": ["Ich", "fin\u00b7de", "mich", "ganz", "ver\u00b7\u00e4n\u00b7dert", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Wie bin ich \u00c4rmste doch bisher", "tokens": ["Wie", "bin", "ich", "\u00c4rms\u00b7te", "doch", "bis\u00b7her"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "PPER", "NN", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Empfindungsbar, gedankenleer", "tokens": ["Emp\u00b7fin\u00b7dungs\u00b7bar", ",", "ge\u00b7dan\u00b7ken\u00b7leer"], "token_info": ["word", "punct", "word"], "pos": ["ADJD", "$,", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Durchs Gastlokal geschlendert!", "tokens": ["Durchs", "Gast\u00b7lo\u00b7kal", "ge\u00b7schlen\u00b7dert", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Nun m\u00f6cht ich jauchzen und m\u00f6chte schrein,", "tokens": ["Nun", "m\u00f6cht", "ich", "jauch\u00b7zen", "und", "m\u00f6ch\u00b7te", "schrein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVINF", "KON", "VMFIN", "ADJD", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "M\u00f6cht leise wimmern und selig sein", "tokens": ["M\u00f6cht", "lei\u00b7se", "wim\u00b7mern", "und", "se\u00b7lig", "sein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADJD", "VVINF", "KON", "ADJD", "VAINF"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und sehne mich fort ins Weite;", "tokens": ["Und", "seh\u00b7ne", "mich", "fort", "ins", "Wei\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKVZ", "APPRART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Ich sehne mich tief in die Einsamkeit,", "tokens": ["Ich", "seh\u00b7ne", "mich", "tief", "in", "die", "Ein\u00b7sam\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Und trotzdem wird mir so weich, so weit", "tokens": ["Und", "trotz\u00b7dem", "wird", "mir", "so", "weich", ",", "so", "weit"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PAV", "VAFIN", "PPER", "ADV", "ADJD", "$,", "ADV", "ADJD"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "So wohlig an deiner Seite.", "tokens": ["So", "woh\u00b7lig", "an", "dei\u00b7ner", "Sei\u00b7te", "."], "token_info": ["word", "word", "word", 