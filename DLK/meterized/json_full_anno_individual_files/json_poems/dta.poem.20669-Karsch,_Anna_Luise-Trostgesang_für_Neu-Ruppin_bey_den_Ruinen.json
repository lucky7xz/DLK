{"dta.poem.20669": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "Trostgesang f\u00fcr Neu-Ruppin  \n bey den Ruinen", "genre": "Lyrik", "period": "N.A.", "pub_year": "1792", "urn": "urn:nbn:de:kobv:b4-200905193016", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Blick auf! blick auf von deinem Aschenh\u00fcgel,                 ", "tokens": ["Blick", "auf", "!", "blick", "auf", "von", "dei\u00b7nem", "A\u00b7schen\u00b7h\u00fc\u00b7gel", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$.", "VVFIN", "APPR", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Hinauf zum Herrn, den keiner fragen darf,", "tokens": ["Hin\u00b7auf", "zum", "Herrn", ",", "den", "kei\u00b7ner", "fra\u00b7gen", "darf", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$,", "PRELS", "PIS", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Warum er schnell durch seines Sturmwinds Fl\u00fcgel", "tokens": ["Wa\u00b7rum", "er", "schnell", "durch", "sei\u00b7nes", "Sturm\u00b7winds", "Fl\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADJD", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In deinen Kranz den Feuerwirbel warf?", "tokens": ["In", "dei\u00b7nen", "Kranz", "den", "Feu\u00b7er\u00b7wir\u00b7bel", "warf", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Im vollen Schmuck sah dich der Mittag schimmern,", "tokens": ["Im", "vol\u00b7len", "Schmuck", "sah", "dich", "der", "Mit\u00b7tag", "schim\u00b7mern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "PRF", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und traurig sah die Abendsonne sich", "tokens": ["Und", "trau\u00b7rig", "sah", "die", "A\u00b7bend\u00b7son\u00b7ne", "sich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "PRF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Noch einmal um, du lagst bei deinen Tr\u00fcmmern", "tokens": ["Noch", "ein\u00b7mal", "um", ",", "du", "lagst", "bei", "dei\u00b7nen", "Tr\u00fcm\u00b7mern"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "PTKVZ", "$,", "PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Verh\u00fcllt in Dampf, und weintest bitterlich.", "tokens": ["Ver\u00b7h\u00fcllt", "in", "Dampf", ",", "und", "wein\u00b7test", "bit\u00b7ter\u00b7lich", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "$,", "KON", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Gott h\u00f6rt die Brut verla\u00dfner Waldesnester,", "tokens": ["Gott", "h\u00f6rt", "die", "Brut", "ver\u00b7la\u00df\u00b7ner", "Wal\u00b7des\u00b7nes\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Er h\u00f6rt nach Brod auch deine Kinder schreyn;", "tokens": ["Er", "h\u00f6rt", "nach", "Brod", "auch", "dei\u00b7ne", "Kin\u00b7der", "schreyn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "ADV", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Er haucht in deine k\u00f6nigliche Schwester,", "tokens": ["Er", "haucht", "in", "dei\u00b7ne", "k\u00f6\u00b7nig\u00b7li\u00b7che", "Schwes\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In sein Berlin, den Geist des Mitleids ein.", "tokens": ["In", "sein", "Ber\u00b7lin", ",", "den", "Geist", "des", "Mit\u00b7leids", "ein", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NE", "$,", "ART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Blick auf! und schau dahin nach jener Seite,", "tokens": ["Blick", "auf", "!", "und", "schau", "da\u00b7hin", "nach", "je\u00b7ner", "Sei\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$.", "KON", "ADJD", "PAV", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da kam der Sturm, gewaltig wie das Meer,", "tokens": ["Da", "kam", "der", "Sturm", ",", "ge\u00b7wal\u00b7tig", "wie", "das", "Meer", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "ADJD", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und st\u00fcrzte dich zum Staub herab, und heute", "tokens": ["Und", "st\u00fcrz\u00b7te", "dich", "zum", "Staub", "her\u00b7ab", ",", "und", "heu\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "APPRART", "NN", "ADV", "$,", "KON", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "K\u00f6mmt wie vom Himmel Trost f\u00fcr dich daher.", "tokens": ["K\u00f6mmt", "wie", "vom", "Him\u00b7mel", "Trost", "f\u00fcr", "dich", "da\u00b7her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOKOM", "APPRART", "NN", "NN", "APPR", "PRF", "PAV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}