{"dta.poem.2042": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der Leopard.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Beschauet einen Leoparden, betrachtet seiner Glieder", "tokens": ["Be\u00b7schau\u00b7et", "ei\u00b7nen", "Le\u00b7o\u00b7par\u00b7den", ",", "be\u00b7trach\u00b7tet", "sei\u00b7ner", "Glie\u00b7der"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "$,", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Pracht,", "tokens": ["Pracht", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Die Sch\u00f6nheit der gefleckten Haut, den Muth, das Feu\u2019r,", "tokens": ["Die", "Sch\u00f6n\u00b7heit", "der", "ge\u00b7fleck\u00b7ten", "Haut", ",", "den", "Muth", ",", "das", "Feu'r", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "die schlanke St\u00e4rke,", "tokens": ["die", "schlan\u00b7ke", "St\u00e4r\u00b7ke", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Die Ebenmaa\u00dfe seiner Glieder, und in den allen dessen", "tokens": ["Die", "E\u00b7ben\u00b7maa\u00b7\u00dfe", "sei\u00b7ner", "Glie\u00b7der", ",", "und", "in", "den", "al\u00b7len", "des\u00b7sen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "NN", "$,", "KON", "APPR", "ART", "PIAT", "PDS"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.6": {"text": "Werke,", "tokens": ["Wer\u00b7ke", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.7": {"text": "Der unter so viel andern Thieren die\u00df sch\u00f6ne Thier her-", "tokens": ["Der", "un\u00b7ter", "so", "viel", "an\u00b7dern", "Thie\u00b7ren", "die\u00df", "sch\u00f6\u00b7ne", "Thier", "her"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ADV", "PIAT", "ADJA", "NN", "PDS", "ADJA", "NN", "TRUNC"], "meter": "-+--+--+--+-+-", "measure": "amphibrach.tetra.plus"}, "line.8": {"text": "vorgebracht,", "tokens": ["vor\u00b7ge\u00b7bracht", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.9": {"text": "Der die dazu geh\u00f6r\u2019ge Muskeln, das Blut, den schnellen", "tokens": ["Der", "die", "da\u00b7zu", "ge\u00b7h\u00f6r'\u00b7ge", "Mus\u00b7keln", ",", "das", "Blut", ",", "den", "schnel\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ART", "ART", "PAV", "ADJA", "NN", "$,", "ART", "NN", "$,", "ART", "ADJA"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "Geist, das Leben,", "tokens": ["Geist", ",", "das", "Le\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.11": {"text": "Die starken Kiefern, Z\u00e4hne, Klauen, und all\u2019 am rech-", "tokens": ["Die", "star\u00b7ken", "Kie\u00b7fern", ",", "Z\u00e4h\u00b7ne", ",", "Klau\u00b7en", ",", "und", "all'", "am", "rech"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "NN", "$,", "NN", "$,", "KON", "PIS", "APPRART", "TRUNC"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "ten Ort gegeben,", "tokens": ["ten", "Ort", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.13": {"text": "Der auch f\u00fcr seine Nahrung sorgt, der, ob die\u00df Thier", "tokens": ["Der", "auch", "f\u00fcr", "sei\u00b7ne", "Nah\u00b7rung", "sorgt", ",", "der", ",", "ob", "die\u00df", "Thier"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$,", "PRELS", "$,", "KOUS", "PDS", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "gleich f\u00fcrchterlich", "tokens": ["gleich", "f\u00fcrch\u00b7ter\u00b7lich"], "token_info": ["word", "word"], "pos": ["ADV", "ADJD"], "meter": "-+-+", "measure": "iambic.di"}, "line.15": {"text": "Und auch zuweilen sch\u00e4dlich ist, durch die Vernunft doch", "tokens": ["Und", "auch", "zu\u00b7wei\u00b7len", "sch\u00e4d\u00b7lich", "ist", ",", "durch", "die", "Ver\u00b7nunft", "doch"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "ADJD", "VAFIN", "$,", "APPR", "ART", "NN", "ADV"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "solche Waffen,", "tokens": ["sol\u00b7che", "Waf\u00b7fen", ","], "token_info": ["word", "word", "punct"], "pos": ["PIAT", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.17": {"text": "Sie, uns zum Nutzen, aufzureiben, und da\u00df f\u00fcr ihren", "tokens": ["Sie", ",", "uns", "zum", "Nut\u00b7zen", ",", "auf\u00b7zu\u00b7rei\u00b7ben", ",", "und", "da\u00df", "f\u00fcr", "ih\u00b7ren"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "$,", "PPER", "APPRART", "NN", "$,", "VVIZU", "$,", "KON", "KOUS", "APPR", "PPOSAT"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.18": {"text": "Muth man sich", "tokens": ["Muth", "man", "sich"], "token_info": ["word", "word", "word"], "pos": ["NN", "PIS", "PRF"], "meter": "+-+", "measure": "trochaic.di"}, "line.19": {"text": "Nicht eben gro\u00df zu f\u00fcrchten habe, gew\u00fcrdiget uns zu ver-", "tokens": ["Nicht", "e\u00b7ben", "gro\u00df", "zu", "f\u00fcrch\u00b7ten", "ha\u00b7be", ",", "ge\u00b7w\u00fcr\u00b7di\u00b7get", "uns", "zu", "ver"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADV", "ADJD", "PTKZU", "VVINF", "VAFIN", "$,", "VVFIN", "PPER", "APPR", "TRUNC"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.20": {"text": "schaffen;", "tokens": ["schaf\u00b7fen", ";"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.21": {"text": "Was wird mit ihren sch\u00f6nen B\u00e4lgen f\u00fcr gro\u00dfer Handel", "tokens": ["Was", "wird", "mit", "ih\u00b7ren", "sch\u00f6\u00b7nen", "B\u00e4l\u00b7gen", "f\u00fcr", "gro\u00b7\u00dfer", "Han\u00b7del"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "APPR", "PPOSAT", "ADJA", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.22": {"text": "nicht getrieben!", "tokens": ["nicht", "ge\u00b7trie\u00b7ben", "!"], "token_info": ["word", "word", "punct"], "pos": ["PTKNEG", "VVPP", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.23": {"text": "Man sieht denn auch in ihm die Spuren von Macht,", "tokens": ["Man", "sieht", "denn", "auch", "in", "ihm", "die", "Spu\u00b7ren", "von", "Macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "ADV", "APPR", "PPER", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.24": {"text": "von Weisheit und vom Lieben.", "tokens": ["von", "Weis\u00b7heit", "und", "vom", "Lie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPRART", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}