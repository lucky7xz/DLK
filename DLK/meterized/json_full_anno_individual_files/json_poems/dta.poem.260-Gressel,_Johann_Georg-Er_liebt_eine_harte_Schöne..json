{"dta.poem.260": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Er liebt eine harte Sch\u00f6ne.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Mein Vergn\u00fcgen heist nur Schauen/", "tokens": ["Mein", "Ver\u00b7gn\u00fc\u00b7gen", "heist", "nur", "Schau\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mir ist weiter nichts verg\u00f6nnt/", "tokens": ["Mir", "ist", "wei\u00b7ter", "nichts", "ver\u00b7g\u00f6nnt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PIS", "VVPP", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ich darf nicht auf Hoffnung bauen/", "tokens": ["Ich", "darf", "nicht", "auf", "Hoff\u00b7nung", "bau\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "APPR", "NN", "VVINF", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Mein Hertz ohne H\u00fclffe brennt.", "tokens": ["Mein", "Hertz", "oh\u00b7ne", "H\u00fclf\u00b7fe", "brennt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Ich darff nichts von Lieben sagen", "tokens": ["Ich", "darff", "nichts", "von", "Lie\u00b7ben", "sa\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PIS", "APPR", "ADJA", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Die Pein nicht klagen/", "tokens": ["Die", "Pein", "nicht", "kla\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "VVINF", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Welche kein Auffh\u00f6ren kennt.", "tokens": ["Wel\u00b7che", "kein", "Auff\u00b7h\u00f6\u00b7ren", "kennt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAT", "PIAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Meine Sch\u00f6ne lacht der Flammen/", "tokens": ["Mei\u00b7ne", "Sch\u00f6\u00b7ne", "lacht", "der", "Flam\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sie verwirfft die reine Gluht/", "tokens": ["Sie", "ver\u00b7wirfft", "die", "rei\u00b7ne", "Gluht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und wil meine Brust verdammen/", "tokens": ["Und", "wil", "mei\u00b7ne", "Brust", "ver\u00b7dam\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Ob sie gleich die Wirckung thut/", "tokens": ["Ob", "sie", "gleich", "die", "Wir\u00b7ckung", "thut", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Jhr Gesichte-Flammen blitzet", "tokens": ["Ihr", "Ge\u00b7sich\u00b7te\u00b7Flam\u00b7men", "blit\u00b7zet"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und mehr erhitzet", "tokens": ["Und", "mehr", "er\u00b7hit\u00b7zet"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADV", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Da\u00df ohnedem entflammte Blut.", "tokens": ["Da\u00df", "oh\u00b7ne\u00b7dem", "ent\u00b7flamm\u00b7te", "Blut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PAV", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Doch gedult! wer kan das wenden", "tokens": ["Doch", "ge\u00b7dult", "!", "wer", "kan", "das", "wen\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "NN", "$.", "PWS", "VMFIN", "PDS", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Was das Schicksahl haben will?", "tokens": ["Was", "das", "Schick\u00b7sahl", "ha\u00b7ben", "will", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VAINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Man mu\u00df ihm an allen Enden", "tokens": ["Man", "mu\u00df", "ihm", "an", "al\u00b7len", "En\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VMFIN", "PPER", "APPR", "PIAT", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Unverweigert halten still.", "tokens": ["Un\u00b7ver\u00b7wei\u00b7gert", "hal\u00b7ten", "still", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Endlich wird es sich bedencken", "tokens": ["End\u00b7lich", "wird", "es", "sich", "be\u00b7den\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PRF", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und meinen Kr\u00e4ncken", "tokens": ["Und", "mei\u00b7nen", "Kr\u00e4n\u00b7cken"], "token_info": ["word", "word", "word"], "pos": ["KON", "PPOSAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Setzen gleichfals Maa\u00df und Ziel.", "tokens": ["Set\u00b7zen", "gleich\u00b7fals", "Maa\u00df", "und", "Ziel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}