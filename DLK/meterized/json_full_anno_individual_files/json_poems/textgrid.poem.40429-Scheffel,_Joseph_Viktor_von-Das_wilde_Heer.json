's", "nir\u00b7gend", "mehr", "'nen", "Trop\u00b7fen", "Wein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "PIAT", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Des Nachts um halber zw\u00f6lf?\u00ab", "tokens": ["Des", "Nachts", "um", "hal\u00b7ber", "zw\u00f6lf", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADV", "APPR", "APPO", "CARD", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Und alles, was im Odenwald", "tokens": ["Und", "al\u00b7les", ",", "was", "im", "O\u00b7den\u00b7wald"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PIS", "$,", "PRELS", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sein' Durst noch nicht gestillt,", "tokens": ["Sein'", "Durst", "noch", "nicht", "ge\u00b7stillt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das folgt ihm bald, das schallt und knallt,", "tokens": ["Das", "folgt", "ihm", "bald", ",", "das", "schallt", "und", "knallt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "$,", "PDS", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das klafft und stampft und br\u00fcllt:", "tokens": ["Das", "klafft", "und", "stampft", "und", "br\u00fcllt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "\u00bb'raus da! 'raus aus dem Haus da!", "tokens": ["\u00bb", "'raus", "da", "!", "'raus", "aus", "dem", "Haus", "da", "!"], "token_i