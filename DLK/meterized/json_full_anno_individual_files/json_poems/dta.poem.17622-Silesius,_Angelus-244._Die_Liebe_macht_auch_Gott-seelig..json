{"dta.poem.17622": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "244.  Die Liebe macht auch Gott-seelig.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Die Lieb beseeligt alls/ auch Gott den Herrn darzu;", "tokens": ["Die", "Lieb", "be\u00b7see\u00b7ligt", "alls", "/", "auch", "Gott", "den", "Herrn", "dar\u00b7zu", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "$(", "ADV", "NN", "ART", "NN", "PAV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "H\u00e4tt\u2019 er die Liebe nicht/ er s\u00e4sse nicht in Ruh.", "tokens": ["H\u00e4tt'", "er", "die", "Lie\u00b7be", "nicht", "/", "er", "s\u00e4s\u00b7se", "nicht", "in", "Ruh", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKNEG", "$(", "PPER", "VVFIN", "PTKNEG", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}