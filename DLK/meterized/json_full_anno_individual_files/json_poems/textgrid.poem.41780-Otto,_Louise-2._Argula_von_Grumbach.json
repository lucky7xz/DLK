{"textgrid.poem.41780": {"metadata": {"author": {"name": "Otto, Louise", "birth": "N.A.", "death": "N.A."}, "title": "2. Argula von Grumbach", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Argula hat des Lebens Gl\u00fcck genossen \u2013", "tokens": ["Ar\u00b7gu\u00b7la", "hat", "des", "Le\u00b7bens", "Gl\u00fcck", "ge\u00b7nos\u00b7sen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "NN", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Liebe f\u00fchrte sie zum Traualtar,", "tokens": ["Die", "Lie\u00b7be", "f\u00fchr\u00b7te", "sie", "zum", "Trau\u00b7al\u00b7tar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Froh ward das heilig feste Band geschlossen,", "tokens": ["Froh", "ward", "das", "hei\u00b7lig", "fes\u00b7te", "Band", "ge\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "ADJD", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das \u00bbJa\u00ab entquoll der Lippe frei und wahr,", "tokens": ["Das", "\u00bb", "Ja", "\u00ab", "ent\u00b7quoll", "der", "Lip\u00b7pe", "frei", "und", "wahr", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$(", "PTKANT", "$(", "ADJD", "ART", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und treu vereint dem liebenden Gef\u00e4hrten", "tokens": ["Und", "treu", "ver\u00b7eint", "dem", "lie\u00b7ben\u00b7den", "Ge\u00b7f\u00e4hr\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ward ihr des Weibes sch\u00f6nstes Los auf Erden!", "tokens": ["Ward", "ihr", "des", "Wei\u00b7bes", "sch\u00f6ns\u00b7tes", "Los", "auf", "Er\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "ADJA", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Doch lange nicht \u2013 in edler Liebe Feier,", "tokens": ["Doch", "lan\u00b7ge", "nicht", "\u2013", "in", "ed\u00b7ler", "Lie\u00b7be", "Fei\u00b7er", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PTKNEG", "$(", "APPR", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nur wenig Jahre waren hingerauscht,", "tokens": ["Nur", "we\u00b7nig", "Jah\u00b7re", "wa\u00b7ren", "hin\u00b7ge\u00b7rauscht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da hat ihr junges Haupt den Wittwenschleier", "tokens": ["Da", "hat", "ihr", "jun\u00b7ges", "Haupt", "den", "Witt\u00b7wensc\u00b7hlei\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPOSAT", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nur zu schnell f\u00fcr den Brautkranz eingetauscht!", "tokens": ["Nur", "zu", "schnell", "f\u00fcr", "den", "Braut\u00b7kranz", "ein\u00b7ge\u00b7tauscht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKA", "ADJD", "APPR", "ART", "NN", "VVPP", "$."], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "Die bl\u00fchnden Kinder wiegt als vaterlose", "tokens": ["Die", "bl\u00fchn\u00b7den", "Kin\u00b7der", "wiegt", "als", "va\u00b7ter\u00b7lo\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "KOUS", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die Trauernde auf ihrem Mutterscho\u00dfe.", "tokens": ["Die", "Trau\u00b7ern\u00b7de", "auf", "ih\u00b7rem", "Mut\u00b7ter\u00b7scho\u00b7\u00dfe", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}}, "stanza.3": {"line.1": {"text": "Im tiefen Schmerze m\u00f6chte sie vergehen,", "tokens": ["Im", "tie\u00b7fen", "Schmer\u00b7ze", "m\u00f6ch\u00b7te", "sie", "ver\u00b7ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sich fl\u00fcchten aus dem \u00f6den Weltgew\u00fchl,", "tokens": ["Sich", "fl\u00fcch\u00b7ten", "aus", "dem", "\u00f6\u00b7den", "Welt\u00b7ge\u00b7w\u00fchl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Durch Thr\u00e4nenfl\u00f6re nur zum Himmel sehen", "tokens": ["Durch", "Thr\u00e4\u00b7nen\u00b7fl\u00f6\u00b7re", "nur", "zum", "Him\u00b7mel", "se\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "ADV", "APPRART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zum Gatten auf im sehnenden Gef\u00fchl \u2013", "tokens": ["Zum", "Gat\u00b7ten", "auf", "im", "seh\u00b7nen\u00b7den", "Ge\u00b7f\u00fchl", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "APPRART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Doch ihre Kinder mahnen sie ans Leben,", "tokens": ["Doch", "ih\u00b7re", "Kin\u00b7der", "mah\u00b7nen", "sie", "ans", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Sie mu\u00df als Mutter, Vater f\u00fcr sie streben.", "tokens": ["Sie", "mu\u00df", "als", "Mut\u00b7ter", ",", "Va\u00b7ter", "f\u00fcr", "sie", "stre\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "KOUS", "NN", "$,", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "So sei das heil'ge Erbe angetreten!", "tokens": ["So", "sei", "das", "heil'\u00b7ge", "Er\u00b7be", "an\u00b7ge\u00b7tre\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sie weiht sich ganz des Lebens ernster Pflicht,", "tokens": ["Sie", "weiht", "sich", "ganz", "des", "Le\u00b7bens", "erns\u00b7ter", "Pflicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADV", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Recht Handeln gilt ihr mehr als weinend Beten,", "tokens": ["Recht", "Han\u00b7deln", "gilt", "ihr", "mehr", "als", "wei\u00b7nend", "Be\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "PPER", "PIAT", "KOKOM", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und mehr als Dulden, Streben nach dem Licht;", "tokens": ["Und", "mehr", "als", "Dul\u00b7den", ",", "Stre\u00b7ben", "nach", "dem", "Licht", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "KOUS", "NN", "$,", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Die Mutterpflicht gibt ihr den Mut, die St\u00e4rke,", "tokens": ["Die", "Mut\u00b7ter\u00b7pflicht", "gibt", "ihr", "den", "Mut", ",", "die", "St\u00e4r\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ihr Teil zu fordern an dem Fortschrittswerke.", "tokens": ["Ihr", "Teil", "zu", "for\u00b7dern", "an", "dem", "Fort\u00b7schritts\u00b7wer\u00b7ke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKZU", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Dem Fortschrittswerke, das der M\u00f6nch begonnen,", "tokens": ["Dem", "Fort\u00b7schritts\u00b7wer\u00b7ke", ",", "das", "der", "M\u00f6nch", "be\u00b7gon\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der k\u00fchne Luther, durch den Kampf mit Rom.", "tokens": ["Der", "k\u00fch\u00b7ne", "Lu\u00b7ther", ",", "durch", "den", "Kampf", "mit", "Rom", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "APPR", "ART", "NN", "APPR", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Auch ihr war ja die Bibel l\u00e4ngst der Bronnen,", "tokens": ["Auch", "ihr", "war", "ja", "die", "Bi\u00b7bel", "l\u00e4ngst", "der", "Bron\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VAFIN", "ADV", "ART", "NN", "ADV", "ART", "NN", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Aus dem sie sch\u00f6pfte der Begeistrung Strom,", "tokens": ["Aus", "dem", "sie", "sch\u00f6pf\u00b7te", "der", "Be\u00b7geis\u00b7trung", "Strom", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Die Kraft auf seine Seite sich zu stellen,", "tokens": ["Die", "Kraft", "auf", "sei\u00b7ne", "Sei\u00b7te", "sich", "zu", "stel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "PRF", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Mit solchem Licht die Menschheit zu erhellen.", "tokens": ["Mit", "sol\u00b7chem", "Licht", "die", "Menschheit", "zu", "er\u00b7hel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "Um ihrer Kinder, um der Menschheit willen", "tokens": ["Um", "ih\u00b7rer", "Kin\u00b7der", ",", "um", "der", "Menschheit", "wil\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUI", "PPOSAT", "NN", "$,", "KOUI", "ART", "NN", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Tritt Argula aus ihrem Fraungemach", "tokens": ["Tritt", "Ar\u00b7gu\u00b7la", "aus", "ih\u00b7rem", "Fraun\u00b7ge\u00b7mach"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "NE", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Hinaus ins Leben, so den Drang zu stillen", "tokens": ["Hin\u00b7aus", "ins", "Le\u00b7ben", ",", "so", "den", "Drang", "zu", "stil\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "$,", "ADV", "ART", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der auch in ihr von Licht und Freiheit sprach;", "tokens": ["Der", "auch", "in", "ihr", "von", "Licht", "und", "Frei\u00b7heit", "sprach", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "PPER", "APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Vor Luther selbst weiht sie sich seiner Sache", "tokens": ["Vor", "Lu\u00b7ther", "selbst", "weiht", "sie", "sich", "sei\u00b7ner", "Sa\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "ADV", "VVFIN", "PPER", "PRF", "PPOSAT", "NN"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Und h\u00e4lt f\u00fcr ihn, f\u00fcr Glaubensfreiheit Wache.", "tokens": ["Und", "h\u00e4lt", "f\u00fcr", "ihn", ",", "f\u00fcr", "Glau\u00b7bens\u00b7frei\u00b7heit", "Wa\u00b7che", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPER", "$,", "APPR", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Sie sucht die Welt, nicht nur ein St\u00fcck vom Himm", "tokens": ["Sie", "sucht", "die", "Welt", ",", "nicht", "nur", "ein", "St\u00fcck", "vom", "Himm"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PTKNEG", "ADV", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ihr Horizont ist unbegrenzt und weit.", "tokens": ["Ihr", "Ho\u00b7ri\u00b7zont", "ist", "un\u00b7be\u00b7grenzt", "und", "weit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sie dient dem Ew'gen in der Welt Get\u00fcmmel,", "tokens": ["Sie", "dient", "dem", "Ew'\u00b7gen", "in", "der", "Welt", "Ge\u00b7t\u00fcm\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sie dient mit freiem Geiste ihrer Zeit.", "tokens": ["Sie", "dient", "mit", "frei\u00b7em", "Geis\u00b7te", "ih\u00b7rer", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Sie f\u00fcrchtet nicht, da\u00df was im Innern bl\u00fche", "tokens": ["Sie", "f\u00fcrch\u00b7tet", "nicht", ",", "da\u00df", "was", "im", "In\u00b7nern", "bl\u00fc\u00b7he"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "KOUS", "PIS", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "In Sonn' und Sturm und frischer Luft vergl\u00fche.", "tokens": ["In", "Sonn'", "und", "Sturm", "und", "fri\u00b7scher", "Luft", "ver\u00b7gl\u00fc\u00b7he", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "KON", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Sie sucht nicht im Gebet in Klostermauern", "tokens": ["Sie", "sucht", "nicht", "im", "Ge\u00b7bet", "in", "Klos\u00b7ter\u00b7mau\u00b7ern"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "APPRART", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des Gottes gnadenreiche Gegenwart;", "tokens": ["Des", "Got\u00b7tes", "gna\u00b7den\u00b7rei\u00b7che", "Ge\u00b7gen\u00b7wart", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ihr hat er sich in gleichen Ahnungsschauern", "tokens": ["Ihr", "hat", "er", "sich", "in", "glei\u00b7chen", "Ah\u00b7nungs\u00b7schau\u00b7ern"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "PRF", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Im Tempel der Natur geoffenbart;", "tokens": ["Im", "Tem\u00b7pel", "der", "Na\u00b7tur", "ge\u00b7of\u00b7fen\u00b7bart", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "So dient sie ihm bis auf des Todes Winken", "tokens": ["So", "dient", "sie", "ihm", "bis", "auf", "des", "To\u00b7des", "Win\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ADV", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "