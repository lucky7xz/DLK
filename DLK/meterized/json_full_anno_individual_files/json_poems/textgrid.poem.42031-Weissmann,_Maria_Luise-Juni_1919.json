{"textgrid.poem.42031": {"metadata": {"author": {"name": "Weissmann, Maria Luise", "birth": "N.A.", "death": "N.A."}, "title": "Juni 1919", "genre": "verse", "period": "N.A.", "pub_year": 1914, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die dunkle Fr\u00fche tr\u00e4gt mich schwer im Scho\u00df,", "tokens": ["Die", "dunk\u00b7le", "Fr\u00fc\u00b7he", "tr\u00e4gt", "mich", "schwer", "im", "Scho\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sterbend die mich gebar dem blassen Morgen;", "tokens": ["Ster\u00b7bend", "die", "mich", "ge\u00b7bar", "dem", "blas\u00b7sen", "Mor\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "PRELS", "PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Mit Heckenrosen werd ich langsam gro\u00df,", "tokens": ["Mit", "He\u00b7cken\u00b7ro\u00b7sen", "werd", "ich", "lang\u00b7sam", "gro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PPER", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Berg mu\u00df mir seine blauen Schatten borgen,", "tokens": ["Berg", "mu\u00df", "mir", "sei\u00b7ne", "blau\u00b7en", "Schat\u00b7ten", "bor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PPOSAT", "ADJA", "NN", "VVINF", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.5": {"text": "Wenn Mittag mich in steiler Glut versehrt.", "tokens": ["Wenn", "Mit\u00b7tag", "mich", "in", "stei\u00b7ler", "Glut", "ver\u00b7sehrt", "."], "token_i