{"textgrid.poem.38070": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Schuld", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es ging ein Knab spazieren,", "tokens": ["Es", "ging", "ein", "Knab", "spa\u00b7zie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zu Augsburg in den Wald,", "tokens": ["Zu", "Augs\u00b7burg", "in", "den", "Wald", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da begegnet ihm ein M\u00e4gdlein,", "tokens": ["Da", "be\u00b7geg\u00b7net", "ihm", "ein", "M\u00e4gd\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "--+--++-", "measure": "anapaest.di.plus"}, "line.4": {"text": "War achtzehn Jahre alt,", "tokens": ["War", "acht\u00b7zehn", "Jah\u00b7re", "alt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "CARD", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Gar sch\u00f6n war sie gestallt.", "tokens": ["Gar", "sch\u00f6n", "war", "sie", "ge\u00b7stallt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Er nahm das M\u00e4del gefangen,", "tokens": ["Er", "nahm", "das", "M\u00e4\u00b7del", "ge\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Gefangen must du sein!", "tokens": ["Ge\u00b7fan\u00b7gen", "must", "du", "sein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er zog ihr aus die Kleider,", "tokens": ["Er", "zog", "ihr", "aus", "die", "Klei\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und schlug sie also sehr,", "tokens": ["Und", "schlug", "sie", "al\u00b7so", "sehr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Hat ihr genommen die Ehr.", "tokens": ["Hat", "ihr", "ge\u00b7nom\u00b7men", "die", "Ehr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "ART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "Zu Augsburg in dem Wirthshaus", "tokens": ["Zu", "Augs\u00b7burg", "in", "dem", "Wirths\u00b7haus"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sa\u00df er bei Speis und Trank;", "tokens": ["Sa\u00df", "er", "bei", "Speis", "und", "Trank", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da kam dasselb'ge M\u00e4gdlein,", "tokens": ["Da", "kam", "das\u00b7selb'\u00b7ge", "M\u00e4gd\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PDAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Griff ihn an seine Hand,", "tokens": ["Griff", "ihn", "an", "sei\u00b7ne", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Schlo\u00df ihn in Ketten und Band.", "tokens": ["Schlo\u00df", "ihn", "in", "Ket\u00b7ten", "und", "Band", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "NN", "KON", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.4": {"line.1": {"text": "Zu Augsburg auf dem Thurme,", "tokens": ["Zu", "Augs\u00b7burg", "auf", "dem", "Thur\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wo er gefangen sa\u00df,", "tokens": ["Wo", "er", "ge\u00b7fan\u00b7gen", "sa\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da kam seine liebste Frau Mutter:", "tokens": ["Da", "kam", "sei\u00b7ne", "liebs\u00b7te", "Frau", "Mut\u00b7ter", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "ADJA", "NN", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Mein Sohn was machst du da?", "tokens": ["Mein", "Sohn", "was", "machst", "du", "da", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PWS", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Was hast du da gemacht?", "tokens": ["Was", "hast", "du", "da", "ge\u00b7macht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Was ich allhier wohl mache,", "tokens": ["Was", "ich", "all\u00b7hier", "wohl", "ma\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das darf ich euch schon sag'n:", "tokens": ["Das", "darf", "ich", "euch", "schon", "sag'n", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich hab das schwarzbraun M\u00e4gdelein", "tokens": ["Ich", "hab", "das", "schwarz\u00b7braun", "M\u00e4g\u00b7del\u00b7ein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Geschlagen also sehr,", "tokens": ["Ge\u00b7schla\u00b7gen", "al\u00b7so", "sehr", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Hab ihr genommen die Ehr.", "tokens": ["Hab", "ihr", "ge\u00b7nom\u00b7men", "die", "Ehr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVPP", "ART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.6": {"line.1": {"text": "Ach J\u00fcngling! liebster J\u00fcngling,", "tokens": ["Ach", "J\u00fcng\u00b7ling", "!", "liebs\u00b7ter", "J\u00fcng\u00b7ling", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "NN", "$.", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ist das nicht Schand und Spott?", "tokens": ["Ist", "das", "nicht", "Schand", "und", "Spott", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "PTKNEG", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dein Kopf der geh\u00f6rt an Galgen,", "tokens": ["Dein", "Kopf", "der", "ge\u00b7h\u00f6rt", "an", "Gal\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "VVFIN", "APPR", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Dein K\u00f6rper auf das Rad,", "tokens": ["Dein", "K\u00f6r\u00b7per", "auf", "das", "Rad", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Weil du's verschuldet hast.", "tokens": ["Weil", "du's", "ver\u00b7schul\u00b7det", "hast", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVPP", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Ach Mutter, liebste Mutter mein!", "tokens": ["Ach", "Mut\u00b7ter", ",", "liebs\u00b7te", "Mut\u00b7ter", "mein", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "$,", "ADJA", "NN", "PPOSAT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ist denn der Bericht schon da?", "tokens": ["Ist", "denn", "der", "Be\u00b7richt", "schon", "da", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "ADV", "ADV", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "So bestellt mir Ro\u00df und Wagen,", "tokens": ["So", "be\u00b7stellt", "mir", "Ro\u00df", "und", "Wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ich geh nicht mehr zu Fu\u00df,", "tokens": ["Ich", "geh", "nicht", "mehr", "zu", "Fu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Weil ich wei\u00df, da\u00df ich sterben mu\u00df.", "tokens": ["Weil", "ich", "wei\u00df", ",", "da\u00df", "ich", "ster\u00b7ben", "mu\u00df", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "KOUS", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Ihr lieben Herrn von Augsburg!", "tokens": ["Ihr", "lie\u00b7ben", "Herrn", "von", "Augs\u00b7burg", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "APPR", "NE", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Noch eine Bitt an euch:", "tokens": ["Noch", "ei\u00b7ne", "Bitt", "an", "euch", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Den Kirchhof thut mir schenken,", "tokens": ["Den", "Kirch\u00b7hof", "thut", "mir", "schen\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dazu ein seidenes Ki\u00df'n,", "tokens": ["Da\u00b7zu", "ein", "sei\u00b7de\u00b7nes", "Ki\u00df'n", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "PPOSAT", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Wo's gut drauf rasten ist.", "tokens": ["Wo's", "gut", "drauf", "ras\u00b7ten", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "PAV", "VVFIN", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Ach J\u00fcngling, liebster J\u00fcngling mein!", "tokens": ["Ach", "J\u00fcng\u00b7ling", ",", "liebs\u00b7ter", "J\u00fcng\u00b7ling", "mein", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "$,", "ADJA", "NN", "PPOSAT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das geht nicht bei der Stadt,", "tokens": ["Das", "geht", "nicht", "bei", "der", "Stadt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PTKNEG", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Kopf geh\u00f6rt an Galgen,", "tokens": ["Der", "Kopf", "ge\u00b7h\u00f6rt", "an", "Gal\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der K\u00f6rper auf das Rad,", "tokens": ["Der", "K\u00f6r\u00b7per", "auf", "das", "Rad", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Weil dus verschuldet hast!", "tokens": ["Weil", "dus", "ver\u00b7schul\u00b7det", "hast", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "VVPP", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Es ging ein Knab spazieren,", "tokens": ["Es", "ging", "ein", "Knab", "spa\u00b7zie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zu Augsburg in den Wald,", "tokens": ["Zu", "Augs\u00b7burg", "in", "den", "Wald", ","], "token_info": ["word", "word", "word", "wo