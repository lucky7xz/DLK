{"dta.poem.23803": {"metadata": {"author": {"name": "Canitz, Friedrich Rudolph Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "Der 103. Psalm.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1700", "urn": "urn:nbn:de:kobv:b4-200905197532", "language": ["de:0.99"], "booktitle": "[Canitz, Friedrich Rudolph Ludwig von]: Neben-Stunden Unterschiedener Gedichte. [Hrsg. v. Joachim Lange]. Berlin, 1700."}, "poem": {"stanza.1": {"line.1": {"text": "Entz\u00fcnde dich in Andacht meine Seele/", "tokens": ["Ent\u00b7z\u00fcn\u00b7de", "dich", "in", "An\u00b7dacht", "mei\u00b7ne", "See\u00b7le", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "APPR", "NN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und lobe GOtt aus tiefster Hertzens-H\u00f6hle!", "tokens": ["Und", "lo\u00b7be", "Gott", "aus", "tiefs\u00b7ter", "Hert\u00b7zens\u00b7H\u00f6h\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sein Nahme sey recht inniglich gepriesen/", "tokens": ["Sein", "Nah\u00b7me", "sey", "recht", "in\u00b7nig\u00b7lich", "ge\u00b7prie\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und was Er dir/ O Seele/ guts erwiesen/", "tokens": ["Und", "was", "Er", "dir", "/", "O", "See\u00b7le", "/", "guts", "er\u00b7wie\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "PPER", "$(", "NE", "NN", "$(", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Das la\u00df mit Danck zu seinem Ruhm erschallen/", "tokens": ["Das", "la\u00df", "mit", "Danck", "zu", "sei\u00b7nem", "Ruhm", "er\u00b7schal\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und nimmermehr aus dem Ged\u00e4chtni\u00df fallen.", "tokens": ["Und", "nim\u00b7mer\u00b7mehr", "aus", "dem", "Ge\u00b7d\u00e4cht\u00b7ni\u00df", "fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "An statt da\u00df Er an dir sich k\u00f6nte r\u00e4chen/", "tokens": ["An", "statt", "da\u00df", "Er", "an", "dir", "sich", "k\u00f6n\u00b7te", "r\u00e4\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "KOUS", "PPER", "APPR", "PPER", "PRF", "VMFIN", "VVINF", "$("], "meter": "-+-+---+-+-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Spricht Er dich lo\u00df/ und heilet dein Gebrechen;", "tokens": ["Spricht", "Er", "dich", "lo\u00df", "/", "und", "hei\u00b7let", "dein", "Ge\u00b7bre\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "PTKVZ", "$(", "KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Errettet dich von des Verderbens Stricken;", "tokens": ["Er\u00b7ret\u00b7tet", "dich", "von", "des", "Ver\u00b7der\u00b7bens", "Stri\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und kr\u00f6nt dein Haupt mit lauter Gnaden-Blicken/", "tokens": ["Und", "kr\u00f6nt", "dein", "Haupt", "mit", "lau\u00b7ter", "Gna\u00b7den\u00b7Bli\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "APPR", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Da\u00df du dich kanst mit deinem Munde freuen/", "tokens": ["Da\u00df", "du", "dich", "kanst", "mit", "dei\u00b7nem", "Mun\u00b7de", "freu\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VMFIN", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und an der Krafft dem Adler gleich erneuen.", "tokens": ["Und", "an", "der", "Krafft", "dem", "Ad\u00b7ler", "gleich", "er\u00b7neu\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Gerechtigkeit schafft Er an allen Enden/", "tokens": ["Ge\u00b7rech\u00b7tig\u00b7keit", "schafft", "Er", "an", "al\u00b7len", "En\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPR", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und l\u00e4sset nicht die Unschuld ewig sch\u00e4nden.", "tokens": ["Und", "l\u00e4s\u00b7set", "nicht", "die", "Un\u00b7schuld", "e\u00b7wig", "sch\u00e4n\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "ART", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Er hat sein Volck zu einem Bund verpflichtet/", "tokens": ["Er", "hat", "sein", "Volck", "zu", "ei\u00b7nem", "Bund", "ver\u00b7pflich\u00b7tet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "APPR", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den Er schon l\u00e4ngst durch Mosen aufgerichtet;", "tokens": ["Den", "Er", "schon", "l\u00e4ngst", "durch", "Mo\u00b7sen", "auf\u00b7ge\u00b7rich\u00b7tet", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADV", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Der zeigt uns an/ was wir zu leisten schuldig/", "tokens": ["Der", "zeigt", "uns", "an", "/", "was", "wir", "zu", "leis\u00b7ten", "schul\u00b7dig", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PTKVZ", "$(", "PWS", "PPER", "PTKZU", "VVINF", "ADJD", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Doch ist der HERR barmhertzig und geduldig.", "tokens": ["Doch", "ist", "der", "HeRR", "barm\u00b7hert\u00b7zig", "und", "ge\u00b7dul\u00b7dig", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Sein sanffter Sinn h\u00e4lt keine Maa\u00df noch Schrancken/", "tokens": ["Sein", "sanff\u00b7ter", "Sinn", "h\u00e4lt", "kei\u00b7ne", "Maa\u00df", "noch", "Schran\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "PIAT", "NN", "ADV", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Er segnet gern/ und wil nicht immer zancken/", "tokens": ["Er", "seg\u00b7net", "gern", "/", "und", "wil", "nicht", "im\u00b7mer", "zan\u00b7cken", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$(", "KON", "VMFIN", "PTKNEG", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und ob wir uns gleich t\u00e4glich von Ihm trennen/", "tokens": ["Und", "ob", "wir", "uns", "gleich", "t\u00e4g\u00b7lich", "von", "Ihm", "tren\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PRF", "ADV", "ADJD", "APPR", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "L\u00e4\u00dft Er den Zorn nicht unaufh\u00f6rlich brennen/", "tokens": ["L\u00e4\u00dft", "Er", "den", "Zorn", "nicht", "un\u00b7auf\u00b7h\u00f6r\u00b7lich", "bren\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKNEG", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Die st\u00e4rckste Glut bricht aus in Liebes-Flammen/", "tokens": ["Die", "st\u00e4rcks\u00b7te", "Glut", "bricht", "aus", "in", "Lie\u00b7bes\u00b7Flam\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die hindern Ihn/ uns S\u00fcnder zu verdammen.", "tokens": ["Die", "hin\u00b7dern", "Ihn", "/", "uns", "S\u00fcn\u00b7der", "zu", "ver\u00b7dam\u00b7men", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "$(", "PPER", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "So hoch Er hat gespannt des Himmels Decken/", "tokens": ["So", "hoch", "Er", "hat", "ge\u00b7spannt", "des", "Him\u00b7mels", "De\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "VAFIN", "VVPP", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mu\u00df seine Huld sich \u00fcber die erstrecken/", "tokens": ["Mu\u00df", "sei\u00b7ne", "Huld", "sich", "\u00fc\u00b7ber", "die", "er\u00b7stre\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "PRF", "APPR", "ART", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Busse thun. Denn mu\u00df der Dampf der S\u00fcnden/", "tokens": ["Die", "Bus\u00b7se", "thun", ".", "Denn", "mu\u00df", "der", "Dampf", "der", "S\u00fcn\u00b7den", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$.", "KON", "VMFIN", "ART", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "So weit der Ost von Westen ist/ verschwinden/", "tokens": ["So", "weit", "der", "Ost", "von", "Wes\u00b7ten", "ist", "/", "ver\u00b7schwin\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "APPR", "NN", "VAFIN", "$(", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Sein Vater-Hertz f\u00e4ngt hefftig an zu wallen/", "tokens": ["Sein", "Va\u00b7ter\u00b7Hertz", "f\u00e4ngt", "heff\u00b7tig", "an", "zu", "wal\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJD", "PTKVZ", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "So bald wir Ihm/ in Furcht/ zu Fusse fallen.", "tokens": ["So", "bald", "wir", "Ihm", "/", "in", "Furcht", "/", "zu", "Fus\u00b7se", "fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "PPER", "$(", "APPR", "NN", "$(", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Der Sch\u00f6pffer kennet sich und sein Gesch\u00f6pffe/", "tokens": ["Der", "Sch\u00f6pf\u00b7fer", "ken\u00b7net", "sich", "und", "sein", "Ge\u00b7sch\u00f6pf\u00b7fe", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "KON", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wir sind nur Staub/ zubrechlich/ schwache T\u00f6pffe/", "tokens": ["Wir", "sind", "nur", "Staub", "/", "zu\u00b7brech\u00b7lich", "/", "schwa\u00b7che", "T\u00f6pf\u00b7fe", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "NN", "$(", "ADJD", "$(", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Zwar ist der Mensch im Leben anzuschauen/", "tokens": ["Zwar", "ist", "der", "Mensch", "im", "Le\u00b7ben", "an\u00b7zu\u00b7schau\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "APPRART", "NN", "VVIZU", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Als frisches Gra\u00df auf den bebl\u00fcmten Auen/", "tokens": ["Als", "fri\u00b7sches", "Gra\u00df", "auf", "den", "be\u00b7bl\u00fcm\u00b7ten", "Au\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Doch wenn sich kaum die rauhen 