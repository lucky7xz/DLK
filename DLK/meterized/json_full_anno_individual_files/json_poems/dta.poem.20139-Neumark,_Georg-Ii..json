{"dta.poem.20139": {"metadata": {"author": {"name": "Neumark, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Ii.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1666", "urn": "urn:nbn:de:kobv:b4-20424-1", "language": ["de:0.99"], "booktitle": "Neumark, Georg: Poetisch-Historischer Lustgarten. Frankfurt (Main), 1666."}, "poem": {"stanza.1": {"line.1": {"text": "Wer einen Garten-Platz beliebet anzulegen/", "tokens": ["Wer", "ei\u00b7nen", "Gar\u00b7ten\u00b7Platz", "be\u00b7lie\u00b7bet", "an\u00b7zu\u00b7le\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VVFIN", "VVIZU", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So neben s\u016fsser Lust zugleich auch Nutz sol", "tokens": ["So", "ne\u00b7ben", "s\u016fs\u00b7ser", "Lust", "zu\u00b7gleich", "auch", "Nutz", "sol"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN", "ADV", "ADV", "NN", "VMFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der ist auf gut Gew\u00e4chs\u2019/ und was die", "tokens": ["Der", "ist", "auf", "gut", "Ge\u00b7w\u00e4chs'", "/", "und", "was", "die"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "VAFIN", "APPR", "ADJD", "NN", "$(", "KON", "PWS", "ART"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Bey Tag und Nacht bedacht/ bi\u00df Er Ver-", "tokens": ["Bey", "Tag", "und", "Nacht", "be\u00b7dacht", "/", "bi\u00df", "Er", "Ver"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "VVPP", "$(", "KOUS", "PPER", "TRUNC"], "meter": "-+-+-+---", "measure": "unknown.measure.tri"}, "line.5": {"text": "So lest Herr Neumark auch itzt einen Garten", "tokens": ["So", "lest", "Herr", "Neu\u00b7mark", "auch", "itzt", "ei\u00b7nen", "Gar\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NN", "NE", "ADV", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Darinnen anders Nichts/ denn sch\u00f6ne Fr\u00fcchte", "tokens": ["Da\u00b7rin\u00b7nen", "an\u00b7ders", "Nichts", "/", "denn", "sch\u00f6\u00b7ne", "Fr\u00fcch\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADV", "PIS", "$(", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Die von sehr gro\u00dfem Nutz und Ziehrden unter-", "tokens": ["Die", "von", "sehr", "gro\u00b7\u00dfem", "Nutz", "und", "Ziehr\u00b7den", "un\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ADV", "ADJA", "NN", "KON", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Die Jhm\u2019 Apollo selbst von seinem Pindus", "tokens": ["Die", "Jhm'", "A\u00b7pol\u00b7lo", "selbst", "von", "sei\u00b7nem", "Pin\u00b7dus"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NE", "NE", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Wer nun Vergn\u00fcgung sucht vor seine kluge Sin-", "tokens": ["Wer", "nun", "Ver\u00b7gn\u00fc\u00b7gung", "sucht", "vor", "sei\u00b7ne", "klu\u00b7ge", "Sin"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "NN", "VVFIN", "APPR", "PPOSAT", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Der wandle nur hinein/ Er findet Sie darinnen/", "tokens": ["Der", "wand\u00b7le", "nur", "hin\u00b7ein", "/", "Er", "fin\u00b7det", "Sie", "da\u00b7rin\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "PTKVZ", "$(", "PPER", "VVFIN", "PPER", "ADV", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Er wird/ (ich wei\u00df gewi\u00df) auch w\u00fcnschen ne-", "tokens": ["Er", "wird", "/", "(", "ich", "wei\u00df", "ge\u00b7wi\u00df", ")", "auch", "w\u00fcn\u00b7schen", "ne"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "$(", "$(", "PPER", "VVFIN", "ADV", "$(", "ADV", "VVINF", "TRUNC"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "Der G\u00e4rtner lebe wol/ und bring\u2019 Uns", "tokens": ["Der", "G\u00e4rt\u00b7ner", "le\u00b7be", "wol", "/", "und", "bring'", "Uns"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "$(", "KON", "VVFIN", "PPER"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}