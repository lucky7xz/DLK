{"textgrid.poem.41293": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "An Omphus", "genre": "verse", "period": "N.A.", "pub_year": 1731, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Erdichte stets: man g\u00f6nnt dir das Vergn\u00fcgen.", "tokens": ["Er\u00b7dich\u00b7te", "stets", ":", "man", "g\u00f6nnt", "dir", "das", "Ver\u00b7gn\u00fc\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$.", "PIS", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Doch nur der Witz bringt der Erfindung Lob.", "tokens": ["Doch", "nur", "der", "Witz", "bringt", "der", "Er\u00b7fin\u00b7dung", "Lob", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Du t\u00e4uschest dich, statt andre zu betr\u00fcgen.", "tokens": ["Du", "t\u00e4u\u00b7schest", "dich", ",", "statt", "and\u00b7re", "zu", "be\u00b7tr\u00fc\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "APPR", "PIS", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nimm Unterricht: dein M\u00e4rchen ist zu grob;", "tokens": ["Nimm", "Un\u00b7ter\u00b7richt", ":", "dein", "M\u00e4r\u00b7chen", "ist", "zu", "grob", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "PPOSAT", "NN", "VAFIN", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Beehre mich mit einer feinern L\u00fcgen.", "tokens": ["Be\u00b7eh\u00b7re", "mich", "mit", "ei\u