{"dta.poem.19371": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "104.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Schlie\u00df aus der ewigen Vollkommenheit der Welt", "tokens": ["Schlie\u00df", "aus", "der", "e\u00b7wi\u00b7gen", "Voll\u00b7kom\u00b7men\u00b7heit", "der", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Auf die Vollkommenheit des, der sie so erh\u00e4lt.", "tokens": ["Auf", "die", "Voll\u00b7kom\u00b7men\u00b7heit", "des", ",", "der", "sie", "so", "er\u00b7h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "$,", "PRELS", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Weil er vollkommen ist, ist all sein Thun vollkommen;", "tokens": ["Weil", "er", "voll\u00b7kom\u00b7men", "ist", ",", "ist", "all", "sein", "Thun", "voll\u00b7kom\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VAFIN", "$,", "VAFIN", "PIAT", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von dem Vollkommnen kann nichts kommen unvollkommen.", "tokens": ["Von", "dem", "Voll\u00b7komm\u00b7nen", "kann", "nichts", "kom\u00b7men", "un\u00b7voll\u00b7kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VMFIN", "PIS", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Zwar unvollkommen f\u00fchlst du dich, o Mensch, auf Erden;", "tokens": ["Zwar", "un\u00b7voll\u00b7kom\u00b7men", "f\u00fchlst", "du", "dich", ",", "o", "Mensch", ",", "auf", "Er\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "PPER", "PRF", "$,", "FM", "NN", "$,", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch auch den Trieb in dir vollkommner stets zu werden.", "tokens": ["Doch", "auch", "den", "Trieb", "in", "dir", "voll\u00b7komm\u00b7ner", "stets", "zu", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "APPR", "PPER", "ADJD", "ADV", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Er selber kann dich auch nicht lassen unvollkommen;", "tokens": ["Er", "sel\u00b7ber", "kann", "dich", "auch", "nicht", "las\u00b7sen", "un\u00b7voll\u00b7kom\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VMFIN", "PPER", "ADV", "PTKNEG", "VVINF", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vollkommen will er dich, und all dein Thun vollkommen.", "tokens": ["Voll\u00b7kom\u00b7men", "will", "er", "dich", ",", "und", "all", "dein", "Thun", "voll\u00b7kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PRF", "$,", "KON", "PIAT", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Vollkommen wirst du seyn, weil er vollkommen ist;", "tokens": ["Voll\u00b7kom\u00b7men", "wirst", "du", "seyn", ",", "weil", "er", "voll\u00b7kom\u00b7men", "ist", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "VAINF", "$,", "KOUS", "PPER", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vollkommen ist er nur, wenn du vollkommen bist.", "tokens": ["Voll\u00b7kom\u00b7men", "ist", "er", "nur", ",", "wenn", "du", "voll\u00b7kom\u00b7men", "bist", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "ADV", "$,", "KOUS", "PPER", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}