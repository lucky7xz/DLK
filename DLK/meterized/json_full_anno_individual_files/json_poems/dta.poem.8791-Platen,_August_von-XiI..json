{"dta.poem.8791": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "XiI.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Ich \u00f6ffne liebend im Gesange Bl\u00fcthen;", "tokens": ["Ich", "\u00f6ff\u00b7ne", "lie\u00b7bend", "im", "Ge\u00b7san\u00b7ge", "Bl\u00fc\u00b7then", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPRART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Alpenrose spendet tiefgewurzelt", "tokens": ["Die", "Al\u00b7pen\u00b7ro\u00b7se", "spen\u00b7det", "tief\u00b7ge\u00b7wur\u00b7zelt"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Noch am granitnen d\u00fcrren Hange Bl\u00fcthen;", "tokens": ["Noch", "am", "gra\u00b7nit\u00b7nen", "d\u00fcr\u00b7ren", "Han\u00b7ge", "Bl\u00fc\u00b7then", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sogar im unfruchtbaren Schoos entfaltet", "tokens": ["So\u00b7gar", "im", "un\u00b7frucht\u00b7ba\u00b7ren", "Schoos", "ent\u00b7fal\u00b7tet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Des wilden Meers der Lotos bange Bl\u00fcthen;", "tokens": ["Des", "wil\u00b7den", "Meers", "der", "Lo\u00b7tos", "ban\u00b7ge", "Bl\u00fc\u00b7then", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "ADV", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Zur\u00fccke schauend in der Jugend Spiegel,", "tokens": ["Zu\u00b7r\u00fc\u00b7cke", "schau\u00b7end", "in", "der", "Ju\u00b7gend", "Spie\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Erblick' ich ewig deiner Wange Bl\u00fcthen.", "tokens": ["Er\u00b7blick'", "ich", "e\u00b7wig", "dei\u00b7ner", "Wan\u00b7ge", "Bl\u00fc\u00b7then", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADJD", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}