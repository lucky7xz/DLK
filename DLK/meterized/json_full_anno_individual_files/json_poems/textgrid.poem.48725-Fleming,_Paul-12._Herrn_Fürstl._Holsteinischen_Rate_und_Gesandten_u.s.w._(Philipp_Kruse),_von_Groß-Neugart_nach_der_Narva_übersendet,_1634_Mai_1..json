{"textgrid.poem.48725": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "12. Herrn F\u00fcrstl. Holsteinischen Rate und Gesandten u.s.w. (Philipp Kruse), von Gro\u00df-Neugart nach der Narva \u00fcbersendet, 1634 Mai 1.", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mehr auf ein ander Mal! Itzt schreibe nur ein Lied,", "tokens": ["Mehr", "auf", "ein", "an\u00b7der", "Mal", "!", "Itzt", "schrei\u00b7be", "nur", "ein", "Lied", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$.", "ADV", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Thalia, meine Zier, von zweimal sieben Zeilen;", "tokens": ["Tha\u00b7lia", ",", "mei\u00b7ne", "Zier", ",", "von", "zwei\u00b7mal", "sie\u00b7ben", "Zei\u00b7len", ";"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPOSAT", "NN", "$,", "APPR", "ADV", "CARD", "NN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "la\u00df lange Sachen sein, dieweil wir m\u00fcssen eilen.", "tokens": ["la\u00df", "lan\u00b7ge", "Sa\u00b7chen", "sein", ",", "die\u00b7weil", "wir", "m\u00fcs\u00b7sen", "ei\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "NN", "VAINF", "$,", "KOUS", "PPER", "VMFIN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der gro\u00dfe Musenfreund, um den du bist bem\u00fcht,", "tokens": ["Der", "gro\u00b7\u00dfe", "Mu\u00b7sen\u00b7freund", ",", "um", "den", "du", "bist", "be\u00b7m\u00fcht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "KOUI", "ART", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "ist ganz ein solcher nicht, der auf viel Worte sieht.", "tokens": ["ist", "ganz", "ein", "sol\u00b7cher", "nicht", ",", "der", "auf", "viel", "Wor\u00b7te", "sieht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "PIAT", "PTKNEG", "$,", "PRELS", "APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "W\u00fcndsch' ihm viel tausent Guts, sprich, da\u00df sich sein Verweilen", "tokens": ["W\u00fcnd\u00b7sch'", "ihm", "viel", "tau\u00b7sent", "Guts", ",", "sprich", ",", "da\u00df", "sich", "sein", "Ver\u00b7wei\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "CARD", "NN", "$,", "ADJD", "$,", "KOUS", "PRF", "PPOSAT", "NN"], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "doch durch des Himmels Gunst noch heute mag zerteilen,", "tokens": ["doch", "durch", "des", "Him\u00b7mels", "Gunst", "noch", "heu\u00b7te", "mag", "zer\u00b7tei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "NN", "ADV", "ADV", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "noch heute, weil die Lust in voller Rose bl\u00fcht.", "tokens": ["noch", "heu\u00b7te", ",", "weil", "die", "Lust", "in", "vol\u00b7ler", "Ro\u00b7se", "bl\u00fcht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "KOUS", "ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Vergi\u00df auch dieses nicht, da\u00df wenn nach kurzen Tagen", "tokens": ["Ver\u00b7gi\u00df", "auch", "die\u00b7ses", "nicht", ",", "da\u00df", "wenn", "nach", "kur\u00b7zen", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "ADV", "PDS", "PTKNEG", "$,", "KOUS", "KOUS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "ihn wird ein k\u00fchnes Schiff hier haben her getragen", "tokens": ["ihn", "wird", "ein", "k\u00fch\u00b7nes", "Schiff", "hier", "ha\u00b7ben", "her", "ge\u00b7tra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "ADV", "VAFIN", "ADV", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "auf unsrer ", "tokens": ["auf", "uns\u00b7rer"], "token_info": ["word", "word"], "pos": ["APPR", "PPOSAT"], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.4": {"line.1": {"text": "er soll empfangen sein von dir und deinen Schwestern,", "tokens": ["er", "soll", "emp\u00b7fan\u00b7gen", "sein", "von", "dir", "und", "dei\u00b7nen", "Schwes\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVPP", "VAINF", "APPR", "PPER", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "davon ich auch gesagt itzt schon vor dreien Gestern.", "tokens": ["da\u00b7von", "ich", "auch", "ge\u00b7sagt", "itzt", "schon", "vor", "drei\u00b7en", "Ge\u00b7stern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "ADV", "VVPP", "ADV", "ADV", "APPR", "CARD", "NN", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.3": {"text": "Du bists, der ich vertrau' auch, was ich selbst kaum wei\u00df.", "tokens": ["Du", "bists", ",", "der", "ich", "ver\u00b7trau'", "auch", ",", "was", "ich", "selbst", "kaum", "wei\u00df", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "PRELS", "PPER", "VVFIN", "ADV", "$,", "PWS", "PPER", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Mehr auf ein ander Mal! Itzt schreibe nur ein Lied,", "tokens": ["Mehr", "auf", "ein", "an\u00b7der", "Mal", "!", "Itzt", "schrei\u00b7be", "nur", "ein", "Lied", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$.", "ADV", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Thalia, meine Zier, von zweimal sieben Zeilen;", "tokens": ["Tha\u00b7lia", ",", "mei\u00b7ne", "Zier", ",", "von", "zwei\u00b7mal", "sie\u00b7ben", "Zei\u00b7len", ";"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPOSAT", "NN", "$,", "APPR", "ADV", "CARD", "NN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "la\u00df lange Sachen sein, dieweil wir m\u00fcssen eilen.", "tokens": ["la\u00df", "lan\u00b7ge", "Sa\u00b7chen", "sein", ",", "die\u00b7weil", "wir", "m\u00fcs\u00b7sen", "ei\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "NN", "VAINF", "$,", "KOUS", "PPER", "VMFIN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der gro\u00dfe