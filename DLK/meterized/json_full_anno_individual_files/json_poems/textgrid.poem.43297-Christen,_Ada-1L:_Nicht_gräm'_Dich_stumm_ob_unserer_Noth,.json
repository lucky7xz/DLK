{"textgrid.poem.43297": {"metadata": {"author": {"name": "Christen, Ada", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nicht gr\u00e4m' Dich stumm ob unserer Noth,", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nicht gr\u00e4m' Dich stumm ob unserer Noth,", "tokens": ["Nicht", "gr\u00e4m'", "Dich", "stumm", "ob", "un\u00b7se\u00b7rer", "Noth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "PPER", "ADJD", "KOUS", "PPOSAT", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Viel ist uns noch geblieben.", "tokens": ["Viel", "ist", "uns", "noch", "ge\u00b7blie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbdie Liebe ist st\u00e4rker als der Tod!\u00ab", "tokens": ["\u00bb", "die", "Lie\u00b7be", "ist", "st\u00e4r\u00b7ker", "als", "der", "Tod", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ART", "NN", "VAFIN", "ADJD", "KOKOM", "ART", "NN", "$.", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "So steht es, mein Weib, geschrieben.", "tokens": ["So", "steht", "es", ",", "mein", "Weib", ",", "ge\u00b7schrie\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PPOSAT", "NN", "$,", "VVPP", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Nicht halte Deine Thr\u00e4nen zur\u00fcck,", "tokens": ["Nicht", "hal\u00b7te", "Dei\u00b7ne", "Thr\u00e4\u00b7nen", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "wo