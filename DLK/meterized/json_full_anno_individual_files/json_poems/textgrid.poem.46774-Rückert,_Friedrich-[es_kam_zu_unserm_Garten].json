{"textgrid.poem.46774": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[es kam zu unserm Garten]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es kam zu unserm Garten", "tokens": ["Es", "kam", "zu", "un\u00b7serm", "Gar\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein Blumenkenner her,", "tokens": ["Ein", "Blu\u00b7men\u00b7ken\u00b7ner", "her", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Blumen aller Arten", "tokens": ["Die", "Blu\u00b7men", "al\u00b7ler", "Ar\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Darin betrachtet' er.", "tokens": ["Da\u00b7rin", "be\u00b7trach\u00b7tet'", "er", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "$."], "meter": "-+-+--", "measure": "unknown.measure.di"}}, "stanza.2": {"line.1": {"text": "Er roch an jeder Blume", "tokens": ["Er", "roch", "an", "je\u00b7der", "Blu\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Nur im Vor\u00fcbergehn,", "tokens": ["Nur", "im", "Vor\u00b7\u00fc\u00b7ber\u00b7gehn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch sich zum Eigenthume", "tokens": ["Doch", "sich", "zum", "Ei\u00b7gen\u00b7thu\u00b7me"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PRF", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Hat er die Ros' ersehn.", "tokens": ["Hat", "er", "die", "Ros'", "er\u00b7sehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Zu seinen Heiligthumen", "tokens": ["Zu", "sei\u00b7nen", "Hei\u00b7lig\u00b7thu\u00b7men"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Trug er die Rose fort;", "tokens": ["Trug", "er", "die", "Ro\u00b7se", "fort", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Und lie\u00df die andern Blumen", "tokens": ["Und", "lie\u00df", "die", "an\u00b7dern", "Blu\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und uns betr\u00fcbt am Ort.", "tokens": ["Und", "uns", "be\u00b7tr\u00fcbt", "am", "Ort", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Wir m\u00fcssen sie ihm g\u00f6nnen", "tokens": ["Wir", "m\u00fcs\u00b7sen", "sie", "ihm", "g\u00f6n\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und m\u00fcssen schweigen still;", "tokens": ["Und", "m\u00fcs\u00b7sen", "schwei\u00b7gen", "still", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er konnt' und w\u00fcrde k\u00f6nnen", "tokens": ["Er", "konnt'", "und", "w\u00fcr\u00b7de", "k\u00f6n\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "KON", "VAFIN", "VMFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Noch nehmen was er will.", "tokens": ["Noch", "neh\u00b7men", "was", "er", "will", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "PWS", "PPER", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "La\u00df uns nur freudig warten;", "tokens": ["La\u00df", "uns", "nur", "freu\u00b7dig", "war\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Uns ist noch zugez\u00e4hlt", "tokens": ["Uns", "ist", "noch", "zu\u00b7ge\u00b7z\u00e4hlt"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "VVPP"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ein sch\u00f6ner Blumengarten,", "tokens": ["Ein", "sch\u00f6\u00b7ner", "Blu\u00b7men\u00b7gar\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wenn auch die Rose fehlt.", "tokens": ["Wenn", "auch", "die", "Ro\u00b7se", "fehlt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Es kam zu unserm Garten", "tokens": ["Es", "kam", "zu", "un\u00b7serm", "Gar\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein Blumenkenner her,", "tokens": ["Ein", "Blu\u00b