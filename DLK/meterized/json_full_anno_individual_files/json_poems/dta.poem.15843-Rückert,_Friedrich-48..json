{"dta.poem.15843": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "48.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Schwer zu vertragen ist f\u00fcr eines Mannes Magen", "tokens": ["Schwer", "zu", "ver\u00b7tra\u00b7gen", "ist", "f\u00fcr", "ei\u00b7nes", "Man\u00b7nes", "Ma\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "PTKZU", "VVFIN", "VAFIN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein Weib, das niemal wei\u00df, wieviel die Uhr geschlagen.", "tokens": ["Ein", "Weib", ",", "das", "nie\u00b7mal", "wei\u00df", ",", "wie\u00b7viel", "die", "Uhr", "ge\u00b7schla\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "VVFIN", "$,", "PWAV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Er hat zu rechter Zeit nicht Fr\u00fch- noch Abendschmaus,", "tokens": ["Er", "hat", "zu", "rech\u00b7ter", "Zeit", "nicht", "Fr\u00fch", "noch", "A\u00b7bendsc\u00b7hmaus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ADJA", "NN", "PTKNEG", "TRUNC", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und Ordnung fehlt der Welt, weil sie ihm fehlt im Haus.", "tokens": ["Und", "Ord\u00b7nung", "fehlt", "der", "Welt", ",", "weil", "sie", "ihm", "fehlt", "im", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN"