{"textgrid.poem.41668": {"metadata": {"author": {"name": "Baudelaire, Charles", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du volkdurchstr\u00f6mte Stadt, Stadt, wo die Tr\u00e4ume schweben,", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du volkdurchstr\u00f6mte Stadt, Stadt, wo die Tr\u00e4ume schweben,", "tokens": ["Du", "volk\u00b7durch\u00b7str\u00f6m\u00b7te", "Stadt", ",", "Stadt", ",", "wo", "die", "Tr\u00e4u\u00b7me", "schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,", "NN", "$,", "PWAV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-++--+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Wo das Gespenst uns krallt im hellsten Tagesschein!", "tokens": ["Wo", "das", "Ge\u00b7spenst", "uns", "krallt", "im", "hells\u00b7ten", "Ta\u00b7ges\u00b7schein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PPER", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die R\u00e4tsel fluten rings, gleichwie die S\u00e4fte streben", "tokens": ["Die", "R\u00e4t\u00b7sel", "flu\u00b7ten", "rings", ",", "gleich\u00b7wie", "die", "S\u00e4f\u00b7te", "stre\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "$,", "KON", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Durch alle Adern, die dem Riesen Kraft verleihn.", "tokens": ["Durch", "al\u00b7le", "A\u00b7dern", ",", "die", "dem", "Rie\u00b7sen", "Kraft", "ver\u00b7leihn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "PRELS", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "An einem Morgen, als in trauer\u00f6der Gasse", "tokens": ["An", "ei\u00b7nem", "Mor\u00b7gen", ",", "als", "in", "trau\u00b7e\u00b7r\u00f6\u00b7der", "Gas\u00b7se"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "KOUS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die H\u00e4user, \u00fcberh\u00f6ht vom Nebel, tr\u00fcb und bleich,", "tokens": ["Die", "H\u00e4u\u00b7ser", ",", "\u00fc\u00b7berh\u00b7\u00f6ht", "vom", "Ne\u00b7bel", ",", "tr\u00fcb", "und", "bleich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "APPRART", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Erschienen wie ein Deich l\u00e4ngs m\u00e4chtger Wassermasse,", "tokens": ["Er\u00b7schie\u00b7nen", "wie", "ein", "Deich", "l\u00e4ngs", "m\u00e4cht\u00b7ger", "Was\u00b7ser\u00b7mas\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "KOKOM", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und als Kulisse, die des Mimen Seele gleich,", "tokens": ["Und", "als", "Ku\u00b7lis\u00b7se", ",", "die", "des", "Mi\u00b7men", "See\u00b7le", "gleich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "NN", "$,", "PRELS", "ART", "NN", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ein gelber Nebel rings die Weiten \u00fcberschwemmte,", "tokens": ["Ein", "gel\u00b7ber", "Ne\u00b7bel", "rings", "die", "Wei\u00b7ten", "\u00fc\u00b7bersc\u00b7hwemm\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Ging ich, gest\u00e4hlter Kraft und hohen Mutes voll,", "tokens": ["Ging", "ich", ",", "ge\u00b7st\u00e4hl\u00b7ter", "Kraft", "und", "ho\u00b7hen", "Mu\u00b7tes", "voll", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ADJA", "NN", "KON", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mit meiner Seel im Streit, die schon Erschlaffung hemmte,", "tokens": ["Mit", "mei\u00b7ner", "Seel", "im", "Streit", ",", "die", "schon", "Er\u00b7schlaf\u00b7fung", "hemm\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPRART", "NN", "$,", "PRELS", "ADV", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die Stra\u00dfe, sch\u00fctternd von der Karren dumpf Geroll.", "tokens": ["Die", "Stra\u00b7\u00dfe", ",", "sch\u00fct\u00b7ternd", "von", "der", "Kar\u00b7ren", "dumpf", "Ge\u00b7roll", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "APPR", "ART", "NN", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Da sah ich einen Greis, der sich mit Lumpen deckte,", "tokens": ["Da", "sah", "ich", "ei\u00b7nen", "Greis", ",", "der", "sich", "mit", "Lum\u00b7pen", "deck\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,", "PRELS", "PRF", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "\u2013 Sie ahmten tiefstes Grau des Regenhimmels nach \u2013", "tokens": ["\u2013", "Sie", "ahm\u00b7ten", "tiefs\u00b7tes", "Grau", "des", "Re\u00b7gen\u00b7him\u00b7mels", "nach", "\u2013"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "ADJA", "NN", "ART", "NN", "APPR", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und dessen Anblick leicht der Wohltat F\u00fclle weckte,", "tokens": ["Und", "des\u00b7sen", "An\u00b7blick", "leicht", "der", "Wohl\u00b7tat", "F\u00fcl\u00b7le", "weck\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRELAT", "NN", "ADJD", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "War nicht die Bosheit, die ihm aus den Augen brach.", "tokens": ["War", "nicht", "die", "Bos\u00b7heit", ",", "die", "ihm", "aus", "den", "Au\u00b7gen", "brach", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "ART", "NN", "$,", "PRELS", "PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Sein Augenstern erschien, als ob ihn Galle tr\u00e4nkte,", "tokens": ["Sein", "Au\u00b7gens\u00b7tern", "er\u00b7schien", ",", "als", "ob", "ihn", "Gal\u00b7le", "tr\u00e4nk\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,", "KOKOM", "KOUS", "PPER", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Aus seinem Blick rann Frost, indessen langgehaart", "tokens": ["Aus", "sei\u00b7nem", "Blick", "rann", "Frost", ",", "in\u00b7des\u00b7sen", "lang\u00b7ge\u00b7haart"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "NN", "$,", "ADV", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sein steifer Bart sich wie ein Degen niedersenkte,", "tokens": ["Sein", "stei\u00b7fer", "Bart", "sich", "wie", "ein", "De\u00b7gen", "nie\u00b7der\u00b7senk\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PRF", "KOKOM", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Erstarrt und f\u00fcrchterlich gleichwie des Judas Bart.", "tokens": ["Er\u00b7starrt", "und", "f\u00fcrch\u00b7ter\u00b7lich", "gleich\u00b7wie", "des", "Ju\u00b7das", "Bart", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "ADJD", "KON", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Gebrochen war er, nicht gebeugt. Es war sein R\u00fccken", "tokens": ["Ge\u00b7bro\u00b7chen", "war", "er", ",", "nicht", "ge\u00b7beugt", ".", "Es", "war", "sein", "R\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "PPER", "$,", "PTKNEG", "VVPP", "$.", "PPER", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In rechtem Winkel zu dem Bein, so da\u00df sein Stab,", "tokens": ["In", "rech\u00b7tem", "Win\u00b7kel", "zu", "dem", "Bein", ",", "so", "da\u00df", "sein", "Stab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "ART", "NN", "$,", "ADV", "KOUS", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Vollendend dies Gebild der Furcht, in allen St\u00fccken", "tokens": ["Voll\u00b7en\u00b7dend", "dies", "Ge\u00b7bild", "der", "Furcht", ",", "in", "al\u00b7len", "St\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "PDS", "NN", "ART", "NN", "$,", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ihm das Erscheinen und das irre Straucheln gab,", "tokens": ["Ihm", "das", "Er\u00b7schei\u00b7nen", "und", "das", "ir\u00b7re", "Strau\u00b7cheln", "gab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "KON", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Das einem Juden mit drei F\u00fc\u00dfen zu vergleichen,", "tokens": ["Das", "ei\u00b7nem", "Ju\u00b7den", "mit", "drei", "F\u00fc\u00b7\u00dfen", "zu", "ver\u00b7glei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "APPR", "CARD", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie ein gel\u00e4hmtes Tier durch Schnee und Schmutz er ging,", "tokens": ["Wie", "ein", "ge\u00b7l\u00e4hm\u00b7tes", "Tier", "durch", "Schnee", "und", "Schmutz", "er", "ging", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "APPR", "NN", "KON", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Als tr\u00e4te ewig er mit seinen Schuhn auf Leichen,", "tokens": ["Als", "tr\u00e4\u00b7te", "e\u00b7wig", "er", "mit", "sei\u00b7nen", "Schuhn", "auf", "Lei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "ADJD", "PPER", "APPR", "PPOSAT", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Nicht stumpf, nein, ha\u00dferf\u00fcllt der Welt, die ihn umfing.", "tokens": ["Nicht", "stumpf", ",", "nein", ",", "ha\u00b7\u00dfer\u00b7f\u00fcllt", "der", "Welt", ",", "die", "ihn", "um\u00b7fing", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "$,", "PTKANT", "$,", "VVFIN", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Ihm folgt sein Ebenbild: Bart, Blick, Stock, R\u00fccken, Fetzen,", "tokens": ["Ihm", "folgt", "sein", "E\u00b7ben\u00b7bild", ":", "Bart", ",", "Blick", ",", "Stock", ",", "R\u00fc\u00b7cken", ",", "Fet\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$.", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Kein Unterschied! Entstammt demselben H\u00f6llenbrand!", "tokens": ["Kein", "Un\u00b7ter\u00b7schied", "!", "Ent\u00b7stammt", "dem\u00b7sel\u00b7ben", "H\u00f6l\u00b7len\u00b7brand", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$.", "VVFIN", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Dies greise Zwillingspaar zog \u2013 wunderlich Entsetzen \u2013", "tokens": ["Dies", "grei\u00b7se", "Zwil\u00b7lings\u00b7paar", "zog", "\u2013", "wun\u00b7der\u00b7lich", "Ent\u00b7set\u00b7zen", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "ADJA", "NN", "VVFIN", "$(", "ADJD", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mit gleichem Tritte in ein nie erschautes Land.", "tokens": ["Mit", "glei\u00b7chem", "Trit\u00b7te", "in", "ein", "nie", "er\u00b7schau\u00b7tes", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "ART", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Welch b\u00f6ser Anschlag stie\u00df mein Herz in solche Qualen,", "tokens": ["Welch", "b\u00f6\u00b7ser", "An\u00b7schlag", "stie\u00df", "mein", "Herz", "in", "sol\u00b7che", "Qua\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Welch schn\u00f6der Zufall kam, zu Boden mich zu ziehn,", "tokens": ["Welch", "schn\u00f6\u00b7der", "Zu\u00b7fall", "kam", ",", "zu", "Bo\u00b7den", "mich", "zu", "ziehn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVFIN", "$,", "APPR", "NN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Denn nacheinander sah mein Blick zu sieben Malen", "tokens": ["Denn", "na\u00b7ch\u00b7ein\u00b7an\u00b7der", "sah", "mein", "Blick", "zu", "sie\u00b7ben", "Ma\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPOSAT", "NN", "APPR", "CARD", "NN"], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Den greisen Fremdling, wie er furchtbar mir erschien.", "tokens": ["Den", "grei\u00b7sen", "Fremd\u00b7ling", ",", "wie", "er", "furcht\u00b7bar", "mir", "er\u00b7schien", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PWAV", "PPER", "ADJD", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.10": {"line.1": {"text": "Ein jeder, der den Schreck verh\u00f6hnt, der mich umnachtet,", "tokens": ["Ein", "je\u00b7der", ",", "der", "den", "Schreck", "ver\u00b7h\u00f6hnt", ",", "der", "mich", "um\u00b7nach\u00b7tet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "$,", "PRELS", "ART", "NN", "VVPP", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und der nicht sp\u00fcren mag ein br\u00fcderliches Graun,", "tokens": ["Und", "der", "nicht", "sp\u00fc\u00b7ren", "mag", "ein", "br\u00fc\u00b7der\u00b7li\u00b7ches", "Graun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PTKNEG", "VVINF", "VMFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Bedenken soll er das: Des Alters ungeachtet", "tokens": ["Be\u00b7den\u00b7ken", "soll", "er", "das", ":", "Des", "Al\u00b7ters", "un\u00b7ge\u00b7ach\u00b7tet"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VMFIN", "PPER", "PDS", "$.", "ART", "NN", "APPO"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "War in der Sieben Blick die Ewigkeit zu schaun!", "tokens": ["War", "in", "der", "Sie\u00b7ben", "Blick", "die", "E\u00b7wig\u00b