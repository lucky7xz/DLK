{"dta.poem.9540": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Sonnet.  \n An die sterne wegen Barnien.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Jhr kinder s\u00fcsser nacht/ ihr feuer-vollen br\u00fcder/", "tokens": ["Ihr", "kin\u00b7der", "s\u00fcs\u00b7ser", "nacht", "/", "ihr", "feu\u00b7er\u00b7vol\u00b7len", "br\u00fc\u00b7der", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$(", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du kleines heer der luft/ du himmels-b\u00fcrgerey/", "tokens": ["Du", "klei\u00b7nes", "heer", "der", "luft", "/", "du", "him\u00b7mels\u00b7b\u00fcr\u00b7ge\u00b7rey", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "ART", "NN", "$(", "PPER", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die du durchs braune feld nach reiner melodey", "tokens": ["Die", "du", "durchs", "brau\u00b7ne", "feld", "nach", "rei\u00b7ner", "me\u00b7lo\u00b7dey"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "APPRART", "ADJA", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Erhebest deinen tantz/ und deine sch\u00f6ne glteder/", "tokens": ["Er\u00b7he\u00b7best", "dei\u00b7nen", "tantz", "/", "und", "dei\u00b7ne", "sch\u00f6\u00b7ne", "gl\u00b7te\u00b7der", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$(", "KON", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}, "stanza.2": {"line.1": {"text": "Wenn itzt der faule schlaff die m\u00fcden augen-lieder", "tokens": ["Wenn", "itzt", "der", "fau\u00b7le", "schlaff", "die", "m\u00fc\u00b7den", "au\u00b7gen\u00b7lie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "ADJA", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Durch einen faulen sieg den sinnen leget bey/", "tokens": ["Durch", "ei\u00b7nen", "fau\u00b7len", "sieg", "den", "sin\u00b7nen", "le\u00b7get", "bey", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "ADJA", "VVFIN", "APPR", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Damit kein wachen mehr bey uns zu sp\u00fcren sey.", "tokens": ["Da\u00b7mit", "kein", "wa\u00b7chen", "mehr", "bey", "uns", "zu", "sp\u00fc\u00b7ren", "sey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PIAT", "ADJA", "ADV", "APPR", "PPER", "PTKZU", "VVINF", "VAFIN", "$."], "meter": "-+-+-+---+-+", "measure": "unknown.measure.penta"}, "line.4": {"text": "Jhr kinder s\u00fcsser nacht legt eure fackeln nieder/", "tokens": ["Ihr", "kin\u00b7der", "s\u00fcs\u00b7ser", "nacht", "legt", "eu\u00b7re", "fa\u00b7ckeln", "nie\u00b7der", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Was steht ihr wie zuvor/ und lacht den welt-krei\u00df an?", "tokens": ["Was", "steht", "ihr", "wie", "zu\u00b7vor", "/", "und", "lacht", "den", "welt\u00b7krei\u00df", "an", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "KOKOM", "ADV", "$(", "KON", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Laufft durch das g\u00f6ldne hau\u00df/ verlast die fenster-scheiben/", "tokens": ["Laufft", "durch", "das", "g\u00f6ld\u00b7ne", "hau\u00df", "/", "ver\u00b7last", "die", "fens\u00b7ter\u00b7schei\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$(", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Geht r\u00fcckwerts/ wie ihr solt/ ich will euch r\u00fcckwerts treiben/", "tokens": ["Geht", "r\u00fcck\u00b7werts", "/", "wie", "ihr", "solt", "/", "ich", "will", "euch", "r\u00fcck\u00b7werts", "trei\u00b7ben", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$(", "PWAV", "PPER", "VMFIN", "$(", "PPER", "VMFIN", "PPER", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Geht r\u00fcckwerts wieder hin die alte finstre bahn.", "tokens": ["Geht", "r\u00fcck\u00b7werts", "wie\u00b7der", "hin", "die", "al\u00b7te", "finst\u00b7re", "bahn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "ADV", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Geht kinder wie ihr solt/ flieht lichter/ flieht von mir/", "tokens": ["Geht", "kin\u00b7der", "wie", "ihr", "solt", "/", "flieht", "lich\u00b7ter", "/", "flieht", "von", "mir", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KOKOM", "PPER", "VMFIN", "$(", "VVFIN", "ADJD", "$(", "VVFIN", "APPR", "PPER", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mein licht/ mein augen-stern/ mein lieb ist nicht allhier.", "tokens": ["Mein", "licht", "/", "mein", "au\u00b7gen\u00b7stern", "/", "mein", "lieb", "ist", "nicht", "all\u00b7hier", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "PPOSAT", "NN", "$(", "PPOSAT", "ADJD", "VAFIN", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}