{"dta.poem.20991": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Lv .  \n Eines Alchimisten.", "genre": "Lyrik; Prosa; Drama", "period": "N.A.", "pub_year": "1679", "urn": "urn:nbn:de:kobv:b4-20289-1", "language": ["de:0.99"], "booktitle": "Hofmann von Hofmannswaldau, Christian: Deutsche Ubersetzungen und Gedichte. Breslau, 1679."}, "poem": {"stanza.1": {"line.1": {"text": "Ich war ein Alchimist/ ich dachte Tag und Stunden/", "tokens": ["Ich", "war", "ein", "Al\u00b7chi\u00b7mist", "/", "ich", "dach\u00b7te", "Tag", "und", "Stun\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$(", "PPER", "VVFIN", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Auf eine neue Kunst des Todes frey zu seyn/", "tokens": ["Auf", "ei\u00b7ne", "neu\u00b7e", "Kunst", "des", "To\u00b7des", "frey", "zu", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN", "ADJD", "PTKZU", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Di\u00df was ich stets gesucht/ das hab ich nicht gefunde\u0303/", "tokens": ["Di\u00df", "was", "ich", "stets", "ge\u00b7sucht", "/", "das", "hab", "ich", "nicht", "ge\u00b7fun\u00b7d\u1ebd", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PRELS", "PPER", "ADV", "VVPP", "$(", "PDS", "VAFIN", "PPER", "PTKNEG", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und was ich nicht gesucht/ das stellt sich selbsten ein.", "tokens": ["Und", "was", "ich", "nicht", "ge\u00b7sucht", "/", "das", "stellt", "sich", "selbs\u00b7ten", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "PTKNEG", "VVPP", "$(", "PDS", "VVFIN", "PRF", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}