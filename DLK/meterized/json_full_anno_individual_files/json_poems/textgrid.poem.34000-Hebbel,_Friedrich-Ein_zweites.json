{"textgrid.poem.34000": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Ein zweites", "genre": "verse", "period": "N.A.", "pub_year": 1847, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und ob mich diese Zweifel brennen m\u00fcssen?", "tokens": ["Und", "ob", "mich", "die\u00b7se", "Zwei\u00b7fel", "bren\u00b7nen", "m\u00fcs\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PDAT", "NN", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So rufst du aus und m\u00f6gtest es verneinen,", "tokens": ["So", "rufst", "du", "aus", "und", "m\u00f6g\u00b7test", "es", "ver\u00b7nei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "KON", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Auch mag der Frost dir unertr\u00e4glich scheinen,", "tokens": ["Auch", "mag", "der", "Frost", "dir", "un\u00b7er\u00b7tr\u00e4g\u00b7lich", "schei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der oft dich sch\u00fcttelt bei der Muse K\u00fcssen.", "tokens": ["Der", "oft", "dich", "sch\u00fct\u00b7telt", "bei", "der", "Mu\u00b7se", "K\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PPER", "VVFIN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Doch sprich: wenn deinen sch\u00f6pf'rischen Erg\u00fcssen,", "tokens": ["Doch", "sprich", ":", "wenn", "dei\u00b7nen", "sch\u00f6pf'\u00b7ri\u00b7schen", "Er\u00b7g\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$.", "KOUS", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In denen alle Wonnen sich vereinen,", "tokens": ["In", "de\u00b7nen", "al\u00b7le", "Won\u00b7nen", "sich", "ver\u00b7ei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PIAT", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Schmerzen fehlten, st\u00fcnden nicht mit Weinen", "tokens": ["Die", "Schmer\u00b7zen", "fehl\u00b7ten", ",", "st\u00fcn\u00b7den", "nicht", "mit", "Wei\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "VVFIN", "PTKNEG", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Br\u00fcder fern so einzigen Gen\u00fcssen?", "tokens": ["Die", "Br\u00fc\u00b7der", "fern", "so", "ein\u00b7zi\u00b7gen", "Ge\u00b7n\u00fcs\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+---+-", "measure": "unknown.measure.tetra"}}, "stanza.3": {"line.1": {"text": "Drum nimm sie hin, die Ungerechtigkeiten", "tokens": ["Drum", "nimm", "sie", "hin", ",", "die", "Un\u00b7ge\u00b7rech\u00b7tig\u00b7kei\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "PTKVZ", "$,", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Welt, die dir die Luft des Daseins tr\u00fcben", "tokens": ["Der", "Welt", ",", "die", "dir", "die", "Luft", "des", "Da\u00b7seins", "tr\u00fc\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word",