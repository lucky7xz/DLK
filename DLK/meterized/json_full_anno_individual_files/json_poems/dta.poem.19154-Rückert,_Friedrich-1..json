{"dta.poem.19154": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Das Veilchen f\u00fcllt die Luft mit Wohlgeruch von Amber;", "tokens": ["Das", "Veil\u00b7chen", "f\u00fcllt", "die", "Luft", "mit", "Wohl\u00b7ge\u00b7ruch", "von", "Am\u00b7ber", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "APPR", "NN", "APPR", "NE", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "An Persiens Grenzen ists genannt Guli Peigamber.", "tokens": ["An", "Per\u00b7siens", "Gren\u00b7zen", "ists", "ge\u00b7nannt", "Gu\u00b7li", "Pei\u00b7gam\u00b7ber", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "VAFIN", "VVPP", "NE", "NE", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Guli Peigamber, das besagt Profetenblume;", "tokens": ["Gu\u00b7li", "Pei\u00b7gam\u00b7ber", ",", "das", "be\u00b7sagt", "Pro\u00b7fe\u00b7ten\u00b7blu\u00b7me", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "PDS", "VVFIN", "NN", "$."], "meter": "+----+-+-+-+-", "measure": "dactylic.init"}, "line.2": {"text": "Wie gerne huldigen wir dem Profetenthume!", "tokens": ["Wie", "ger\u00b7ne", "hul\u00b7di\u00b7gen", "wir", "dem", "Pro\u00b7fe\u00b7ten\u00b7thu\u00b7me", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Profetenblum' ist es, weil es uns profezeit", "tokens": ["Pro\u00b7fe\u00b7ten\u00b7blum'", "ist", "es", ",", "weil", "es", "uns", "pro\u00b7fe\u00b7zeit"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "PPER", "$,", "KOUS", "PPER", "PRF", "NN"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Des Fr\u00fchlings Himmelreich, der Rose Herrlichkeit.", "tokens": ["Des", "Fr\u00fch\u00b7lings", "Him\u00b7mel\u00b7reich", ",", "der", "Ro\u00b7se", "Herr\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}