{"dta.poem.8399": {"metadata": {"author": {"name": "H\u00f6lty, Ludwig Christoph Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "H\u00f6ltys Leben .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1783", "urn": "urn:nbn:de:kobv:b4-200905192568", "language": ["de:0.99"], "booktitle": "H\u00f6lty, Ludwig Christoph Heinrich: Gedichte. Hamburg, 1783."}, "poem": {"stanza.1": {"line.1": {"text": "Alhier auf dieser St\u00e4tte", "tokens": ["Al\u00b7hier", "auf", "die\u00b7ser", "St\u00e4t\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "PDAT", "NN"], "meter": "---+-+-", "measure": "unknown.measure.di"}, "line.2": {"text": "Liegt begraben Nette.", "tokens": ["Liegt", "be\u00b7gra\u00b7ben", "Net\u00b7te", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Zu Horst ist er geboren,", "tokens": ["Zu", "Horst", "ist", "er", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zu Mariensee gestorben,", "tokens": ["Zu", "Ma\u00b7ri\u00b7en\u00b7see", "ge\u00b7stor\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Dies Grab hat er erworben.", "tokens": ["Dies", "Grab", "hat", "er", "er\u00b7wor\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}