{"textgrid.poem.51891": {"metadata": {"author": {"name": "Czepko von Reigersfeld, Daniel", "birth": "N.A.", "death": "N.A."}, "title": "6.", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hier beschleust das Sechste Hundert, numehr f\u00e4lt der Sabbath ein:", "tokens": ["Hier", "be\u00b7schleust", "das", "Sechs\u00b7te", "Hun\u00b7dert", ",", "nu\u00b7mehr", "f\u00e4lt", "der", "Sab\u00b7ba\u00b7th", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,", "ADV", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Sencke Sinnen, Seel und Geist tieff in deines Gottes Willen,", "tokens": ["Sen\u00b7cke", "Sin\u00b7nen", ",", "Seel", "und", "Geist", "tieff", "in", "dei\u00b7nes", "Got\u00b7tes", "Wil\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "NN", "KON", "NN", "ADJD", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "So wird seine Herrlichkeit deines Hertzens Kirch erf\u00fcllen.", "tokens": ["So", "wird", "sei\u00b7ne", "Herr\u00b7lich\u00b7keit", "dei\u00b7nes", "Hert\u00b7zens", "Kirch", "er\u00b7f\u00fcl\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "PPOSAT", "NN", "NN", "VVINF", "$."], "meter": "-+--+--+-+-+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Die Dreyfaltigkeit wird selber allda gegenw\u00e4rtig seyn.", "tokens": ["Die", "Drey\u00b7fal\u00b7tig\u00b7keit", "wird", "sel\u00b7ber", "all\u00b7da", "ge\u00b7gen\u00b7w\u00e4r\u00b7tig", "seyn", "."], "