{"textgrid.poem.52536": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Damals", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es war in sch\u00f6nen Tagen,", "tokens": ["Es", "war", "in", "sch\u00f6\u00b7nen", "Ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Als ich so mit ihr ging,", "tokens": ["Als", "ich", "so", "mit", "ihr", "ging", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mein Auge mit Behagen", "tokens": ["Mein", "Au\u00b7ge", "mit", "Be\u00b7ha\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "An ihren Z\u00fcgen hing.", "tokens": ["An", "ih\u00b7ren", "Z\u00fc\u00b7gen", "hing", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Dort unter Schattenb\u00e4umen", "tokens": ["Dort", "un\u00b7ter", "Schat\u00b7ten\u00b7b\u00e4u\u00b7men"], "token_info": ["word", "word", "word"], "pos": ["ADV", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "War eine traute Bank,", "tokens": ["War", "ei\u00b7ne", "trau\u00b7te", "Bank", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dort fa\u00dft ich ohne S\u00e4umen", "tokens": ["Dort", "fa\u00dft", "ich", "oh\u00b7ne", "S\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sie um die H\u00fcfte schlank.", "tokens": ["Sie", "um", "die", "H\u00fcf\u00b7te", "schlank", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ihr K\u00f6pfchen legt sie leise,", "tokens": ["Ihr", "K\u00f6pf\u00b7chen", "legt", "sie", "lei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Err\u00f6thend sanft an mich,", "tokens": ["Er\u00b7r\u00f6\u00b7thend", "sanft", "an", "mich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "APPR", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Eine Thr\u00e4ne eine hei\u00dfe,", "tokens": ["Ei\u00b7ne", "Thr\u00e4\u00b7ne", "ei\u00b7ne", "hei\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Aus ihrer Wimper schlich.", "tokens": ["Aus", "ih\u00b7rer", "Wim\u00b7per", "schlich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "In Armen halt ich selig", "tokens": ["In", "Ar\u00b7men", "halt", "ich", "se\u00b7lig"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "PPER", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das jugendreiche Kind,", "tokens": ["Das", "ju\u00b7gen\u00b7drei\u00b7che", "Kind", ","], "token_info": ["word", "word", "