{"dta.poem.20993": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "LvII.  \n Eines Lasterhafftigen.", "genre": "Lyrik; Prosa; Drama", "period": "N.A.", "pub_year": "1679", "urn": "urn:nbn:de:kobv:b4-20289-1", "language": ["de:0.99"], "booktitle": "Hofmann von Hofmannswaldau, Christian: Deutsche Ubersetzungen und Gedichte. Breslau, 1679."}, "poem": {"stanza.1": {"line.1": {"text": "Die Leber ist zu Wien/ das Glied zu Rom geblieben/", "tokens": ["Die", "Le\u00b7ber", "ist", "zu", "Wi\u00b7en", "/", "das", "Glied", "zu", "Rom", "ge\u00b7blie\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "NE", "$(", "ART", "NN", "APPR", "NE", "VVPP", "$("], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Das Hertz in einer Schlacht/ und das Gehirn im Lie-", "tokens": ["Das", "Hertz", "in", "ei\u00b7ner", "Schlacht", "/", "und", "das", "Ge\u00b7hirn", "im", "Lie"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$(", "KON", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "ben.", "tokens": ["ben", "."], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "-", "measure": "single.down"}, "line.4": {"text": "Doch da\u00df der Leib nicht gantz verlohren m\u00f6chte seyn/", "tokens": ["Doch", "da\u00df", "der", "Leib", "nicht", "gantz", "ver\u00b7loh\u00b7ren", "m\u00f6ch\u00b7te", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "PTKNEG", "ADV", "VVINF", "VMFIN", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So legte man den Rest hier unter diesen Stein.", "tokens": ["So", "leg\u00b7te", "man", "den", "Rest", "hier", "un\u00b7ter", "die\u00b7sen", "Stein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ART", "NN", "ADV", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}