{"textgrid.poem.42421": {"metadata": {"author": {"name": "Liliencron, Detlev von", "birth": "N.A.", "death": "N.A."}, "title": "K\u00f6nig Abels Tod", "genre": "verse", "period": "N.A.", "pub_year": 1876, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der K\u00f6nig schl\u00e4ft im purpurnen Zelt,", "tokens": ["Der", "K\u00f6\u00b7nig", "schl\u00e4ft", "im", "pur\u00b7pur\u00b7nen", "Zelt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Der Posten klirrt auf und nieder.", "tokens": ["Der", "Pos\u00b7ten", "klirrt", "auf", "und", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Blauampellicht gefangen h\u00e4lt", "tokens": ["Blau\u00b7am\u00b7pel\u00b7licht", "ge\u00b7fan\u00b7gen", "h\u00e4lt"], "token_info": ["word", "word", "word"], "pos": ["NN", "ADJD", "VVFIN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Des K\u00f6nigs schwere Lider.", "tokens": ["Des", "K\u00f6\u00b7nigs", "schwe\u00b7re", "Li\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Vor den Deichen ebben die Wasser dumpf,", "tokens": ["Vor", "den", "Dei\u00b7chen", "eb\u00b7ben", "die", "Was\u00b7ser", "dumpf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Die Wachtfeuer qualmen und knistern,", "tokens": ["Die", "Wacht\u00b7feu\u00b7er", "qual\u00b7men", "und", "knis\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "KON", "VVFIN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Durch die Nacht wiehert ein Pferd. Die Fr\u00f6sch' im Sumpf,", "tokens": ["Durch", "die", "Nacht", "wie\u00b7hert", "ein", "Pferd", ".", "Die", "Fr\u00f6sch'", "im", "Sumpf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "$.", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "Sie stimmen in tausend Registern.", "tokens": ["Sie", "stim\u00b7men", "in", "tau\u00b7send", "Re\u00b7gis\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "CARD", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.3": {"line.1": {"text": "Auf heimlichen Wegen, mit Axt und Beil,", "tokens": ["Auf", "heim\u00b7li\u00b7chen", "We\u00b7gen", ",", "mit", "Axt", "und", "Beil", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Mit Keulen und Morgensternen,", "tokens": ["Mit", "Keu\u00b7len", "und", "Mor\u00b7gens\u00b7ter\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Es kommen die freien Friesen in Eil,", "tokens": ["Es", "kom\u00b7men", "die", "frei\u00b7en", "Frie\u00b7sen", "in", "Eil", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Sie kommen aus N\u00e4h' und Fernen.", "tokens": ["Sie", "kom\u00b7men", "aus", "N\u00e4h'", "und", "Fer\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Das Bild des heiligen Christian,", "tokens": ["Das", "Bild", "des", "hei\u00b7li\u00b7gen", "Chris\u00b7ti\u00b7an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NE", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Es rumpelt auf dem Wagen.", "tokens": ["Es", "rum\u00b7pelt", "auf", "dem", "Wa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Bitt' f\u00fcr uns betet der Kapellan,", "tokens": ["Bitt'", "f\u00fcr", "uns", "be\u00b7tet", "der", "Ka\u00b7pel\u00b7lan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Wir wollen mit Gold dich beschlagen.", "tokens": ["Wir", "wol\u00b7len", "mit", "Gold", "dich", "be\u00b7schla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "NN", "PPER", "VVFIN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.5": {"line.1": {"text": "Mit Gold schon beschl\u00e4gt ihn der gelbe Mond", "tokens": ["Mit", "Gold", "schon", "be\u00b7schl\u00e4gt", "ihn", "der", "gel\u00b7be", "Mond"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "ADV", "VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und leuchtet auf Freund' und Feinde.", "tokens": ["Und", "leuch\u00b7tet", "auf", "Freund'", "und", "Fein\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wenn morgen er wieder am Himmel thront,", "tokens": ["Wenn", "mor\u00b7gen", "er", "wie\u00b7der", "am", "Him\u00b7mel", "thront", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPER", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Er sieht eine stille Gemeinde.", "tokens": ["Er", "sieht", "ei\u00b7ne", "stil\u00b7le", "Ge\u00b7mein\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.6": {"line.1": {"text": "Der K\u00f6nig tr\u00e4umt im Pupurzelt,", "tokens": ["Der", "K\u00f6\u00b7nig", "tr\u00e4umt", "im", "Pu\u00b7pur\u00b7zelt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Posten klirrt auf und nieder.", "tokens": ["Der", "Pos\u00b7ten", "klirrt", "auf", "und", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Der blauen Ampel D\u00e4mmer f\u00e4llt", "tokens": ["Der", "blau\u00b7en", "Am\u00b7pel", "D\u00e4m\u00b7mer", "f\u00e4llt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auf des K\u00f6nigs zuckende Lider.", "tokens": ["Auf", "des", "K\u00f6\u00b7nigs", "zu\u00b7cken\u00b7de", "Li\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.7": {"line.1": {"text": "K\u00f6nig Erich steht vor ihm, na\u00df aus der Flut,", "tokens": ["K\u00f6\u00b7nig", "E\u00b7rich", "steht", "vor", "ihm", ",", "na\u00df", "aus", "der", "Flut", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "APPR", "PPER", "$,", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "+-+--+-+--+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Und streckt den Arm nach oben.", "tokens": ["Und", "streckt", "den", "Arm", "nach", "o\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbhinweg, hinweg, bei Christi Blut,", "tokens": ["\u00bb", "hin\u00b7weg", ",", "hin\u00b7weg", ",", "bei", "Chris\u00b7ti", "Blut", ","], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PTKVZ", "$,", "ADV", "$,", "APPR", "NE", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zehn Kl\u00f6ster will ich geloben.\u00ab", "tokens": ["Zehn", "Kl\u00f6s\u00b7ter", "will", "ich", "ge\u00b7lo\u00b7ben", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["CARD", "NN", "VMFIN", "PPER", "VVINF", "$.", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.8": {"line.1": {"text": "Steilauf der K\u00f6nig: \u00bbGratias.", "tokens": ["Stei\u00b7lauf", "der", "K\u00f6\u00b7nig", ":", "\u00bb", "Gra\u00b7ti\u00b7as", "."], "token_info": ["word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["NN", "ART", "NN", "$.", "$(", "NE", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Wulff Bokwoldt! Helm und Schienen,", "tokens": ["Wulff", "Bok\u00b7woldt", "!", "Helm", "und", "Schie\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$.", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mein Schuppenhemd, und rufe rasch", "tokens": ["Mein", "Schup\u00b7pen\u00b7hemd", ",", "und", "ru\u00b7fe", "rasch"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "KON", "VVFIN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Uk Rugmoor und Caj Thienen.\u00ab", "tokens": ["Uk", "Rug\u00b7moor", "und", "Caj", "Thie\u00b7nen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "NE", "KON", "NE", "NN", "$.", "$("], "meter": "-++--+-", "measure": "iambic.tri.relaxed"}}, "stanza.9": {"line.1": {"text": "Wulff Bokwoldt, der Page, wie der Hund", "tokens": ["Wulff", "Bok\u00b7woldt", ",", "der", "Pa\u00b7ge", ",", "wie", "der", "Hund"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NE", "$,", "ART", "NN", "$,", "PWAV", "ART", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Schlief treu zu des K\u00f6nigs F\u00fc\u00dfen.", "tokens": ["Schlief", "treu", "zu", "des", "K\u00f6\u00b7nigs", "F\u00fc\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "ART", "NN", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Im Traume l\u00e4chelt sein junger Mund,", "tokens": ["Im", "Trau\u00b7me", "l\u00e4\u00b7chelt", "sein", "jun\u00b7ger", "Mund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Sch\u00f6n Heilwig sieht er gr\u00fc\u00dfen.", "tokens": ["Sch\u00f6n", "Heil\u00b7wig", "sieht", "er", "gr\u00fc\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Im Walde, voll des s\u00fc\u00dfen Schalls,", "tokens": ["Im", "Wal\u00b7de", ",", "voll", "des", "s\u00fc\u00b7\u00dfen", "Schalls", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er und Sch\u00f6n Heilwig gingen.", "tokens": ["Er", "und", "Sch\u00f6n", "Heil\u00b7wig", "gin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "KON", "NE", "NE", "VVFIN", "$."], "meter": "+-++-+-", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Sie knotet lustig um seinen Hals", "tokens": ["Sie", "kno\u00b7tet", "lus\u00b7tig", "um", "sei\u00b7nen", "Hals"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "PPOSAT", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ihr Langhaar in Maschen und Schlingen.", "tokens": ["Ihr", "Lang\u00b7haar", "in", "Ma\u00b7schen", "und", "Schlin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.11": {"line.1": {"text": "Zwei Ritter, mit schwarzem Panzer bewehrt,", "tokens": ["Zwei", "Rit\u00b7ter", ",", "mit", "schwar\u00b7zem", "Pan\u0