token_info": ["punct", "word", "word", "word", "punct", "word", "word"], "pos": ["$(", "ART", "NN", "PTKVZ", "$.", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mu\u00df heut noch in die Pfl\u00fcge.\u00ab", "tokens": ["Mu\u00df", "heut", "noch", "in", "die", "Pfl\u00fc\u00b7ge", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VMFIN", "ADV", "ADV", "APPR", "ART", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Der K\u00f6nig ruft es, die Sonne glitzt,", "tokens": ["Der", "K\u00f6\u00b7nig", "ruft", "es", ",", "die", "Son\u00b7ne", "glitzt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Gekrach und Lanzensplitter.", "tokens": ["Ge\u00b7krach", "und", "Lan\u00b7zen\u00b7split\u00b7ter", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Des K\u00f6nigs goldene R\u00fcstung blitzt,", "tokens": ["Des", "K\u00f6\u00b7nigs", "gol\u00b7de\u00b7ne", "R\u00fcs\u00b7tung", "blitzt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Seit' jagen die schwarzen Ritter.", "tokens": ["Seit'", "ja\u00b7gen", "die", "schwar\u00b7zen", "Rit\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "Dicht dr\u00e4ngt Wulff Bokwoldt den Schecken heran,", "tokens": ["Dicht", "dr\u00e4ngt", "Wulff", "Bok\u00b7woldt", "den", "Sche\u00b7cken"