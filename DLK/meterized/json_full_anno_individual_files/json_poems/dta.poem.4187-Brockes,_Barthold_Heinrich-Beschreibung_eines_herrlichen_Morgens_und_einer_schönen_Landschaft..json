{"dta.poem.4187": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Beschreibung  \n eines herrlichen Morgens  \n und  \n  einer sch\u00f6nen Landschaft.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Man sah annoch auf gr\u00fcnen Feldern, vom j\u00fcngst gefall-", "tokens": ["Man", "sah", "an\u00b7noch", "auf", "gr\u00fc\u00b7nen", "Fel\u00b7dern", ",", "vom", "j\u00fcngst", "ge\u00b7fall"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PIS", "VVFIN", "ADV", "APPR", "ADJA", "NN", "$,", "APPRART", "ADV", "TRUNC"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "nen k\u00fchlen Thau,", "tokens": ["nen", "k\u00fch\u00b7len", "Thau", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Ein durch der D\u00e4mmrung graues Licht gef\u00e4rbtes saftigs", "tokens": ["Ein", "durch", "der", "D\u00e4mm\u00b7rung", "grau\u00b7es", "Licht", "ge\u00b7f\u00e4rb\u00b7tes", "saf\u00b7tigs"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "ADJA", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Silber-Grau.", "tokens": ["Sil\u00b7ber\u00b7Grau", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Das Morgenroht fing eben an, der Berge Gipfel zu ver-", "tokens": ["Das", "Mor\u00b7gen\u00b7roht", "fing", "e\u00b7ben", "an", ",", "der", "Ber\u00b7ge", "Gip\u00b7fel", "zu", "ver"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "PTKVZ", "$,", "ART", "NN", "NN", "APPR", "TRUNC"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.6": {"text": "g\u00fclden,", "tokens": ["g\u00fcl\u00b7den", ","], "token_info": ["word", "punct"], "pos": ["ADJD", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.7": {"text": "Des glatten Wassers Fl\u00e4che gleichfalls die nahen Vorw\u00fcrf\u2019", "tokens": ["Des", "glat\u00b7ten", "Was\u00b7sers", "Fl\u00e4\u00b7che", "gleich\u00b7falls", "die", "na\u00b7hen", "Vor\u00b7w\u00fcr\u00b7f'"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.8": {"text": "abzubilden.", "tokens": ["ab\u00b7zu\u00b7bil\u00b7den", "."], "token_info": ["word", "punct"], "pos": ["VVIZU", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.9": {"text": "Die M\u00fccken machten denen Fliegen, dem Licht die dunklen", "tokens": ["Die", "M\u00fc\u00b7cken", "mach\u00b7ten", "de\u00b7nen", "Flie\u00b7gen", ",", "dem", "Licht", "die", "dunk\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PDS", "NN", "$,", "ART", "NN", "ART", "ADJA"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "Schatten, Raum.", "tokens": ["Schat\u00b7ten", ",", "Raum", "."], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.11": {"text": "Der V\u00f6gel gurgelnde Gesang erschallte fast von jedem Baum;", "tokens": ["Der", "V\u00f6\u00b7gel", "gur\u00b7geln\u00b7de", "Ge\u00b7sang", "er\u00b7schall\u00b7te", "fast", "von", "je\u00b7dem", "Baum", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "ADV", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.12": {"text": "Als ich, nachdem ich wohl geruht, mich fr\u00f6hlich auf das Feld", "tokens": ["Als", "ich", ",", "nach\u00b7dem", "ich", "wohl", "ge\u00b7ruht", ",", "mich", "fr\u00f6h\u00b7lich", "auf", "das", "Feld"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "$,", "KOUS", "PPER", "ADV", "VVPP", "$,", "PPER", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.13": {"text": "verf\u00fcgte,", "tokens": ["ver\u00b7f\u00fcg\u00b7te", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.14": {"text": "Und, halb erstaunet, an der Pracht der Creaturen mich", "tokens": ["Und", ",", "halb", "er\u00b7stau\u00b7net", ",", "an", "der", "Pracht", "der", "Crea\u00b7tu\u00b7ren", "mich"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "$,", "ADJD", "VVPP", "$,", "APPR", "ART", "NN", "ART", "NN", "PPER"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.15": {"text": "vergn\u00fcgte.", "tokens": ["ver\u00b7gn\u00fcg\u00b7te", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.2": {"line.1": {"text": "Wie nun der C\u00f6rper Seel\u2019 und Wonne, der Sonnen Licht", "tokens": ["Wie", "nun", "der", "C\u00f6r\u00b7per", "Seel'", "und", "Won\u00b7ne", ",", "der", "Son\u00b7nen", "Licht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "ADV", "ART", "NN", "NN", "KON", "NN", "$,", "ART", "NN", "NN"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "und Lebens-Strahl,", "tokens": ["und", "Le\u00b7bens\u00b7Strahl", ","], "token_info": ["word", "word", "punct"], "pos": ["KON", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Der Erden Fl\u00e4che ganz beflo\u00df, und W\u00e4lder, Felder, Berg\u2019", "tokens": ["Der", "Er\u00b7den", "Fl\u00e4\u00b7che", "ganz", "be\u00b7flo\u00df", ",", "und", "W\u00e4l\u00b7der", ",", "Fel\u00b7der", ",", "Ber\u00b7g'"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word"], "pos": ["ART", "NN", "NN", "ADV", "VVFIN", "$,", "KON", "NN", "$,", "NN", "$,", "NN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.4": {"text": "und Thal", "tokens": ["und", "Thal"], "token_info": ["word", "word"], "pos": ["KON", "NN"], "meter": "-+", "measure": "iambic.single"}, "line.5": {"text": "Ins helle Licht-Reich eingetreten; ward alles, was das Aug\u2019", "tokens": ["Ins", "hel\u00b7le", "Licht\u00b7Reich", "ein\u00b7ge\u00b7tre\u00b7ten", ";", "ward", "al\u00b7les", ",", "was", "das", "Aug'"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "VVPP", "$.", "VAFIN", "PIS", "$,", "PRELS", "ART", "NN"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.6": {"text": "erblickt,", "tokens": ["er\u00b7blickt", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.7": {"text": "Mit einem mehr als irdschem Glanz, verkl\u00e4rt, erleuchtet", "tokens": ["Mit", "ei\u00b7nem", "mehr", "als", "ird\u00b7schem", "Glanz", ",", "ver\u00b7kl\u00e4rt", ",", "er\u00b7leuch\u00b7tet"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["APPR", "ART", "PIAT", "KOKOM", "ADJA", "NN", "$,", "VVPP", "$,", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "und geschm\u00fcckt.", "tokens": ["und", "ge\u00b7schm\u00fcckt", "."], "token_info": ["word", "word", "punct"], "pos": ["KON", "VVPP", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.3": {"line.1": {"text": "Kurz: Alles, was man auf der Erden, in Luft und Wasser", "tokens": ["Kurz", ":", "Al\u00b7les", ",", "was", "man", "auf", "der", "Er\u00b7den", ",", "in", "Luft", "und", "Was\u00b7ser"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "$.", "PIS", "$,", "PRELS", "PIS", "APPR", "ART", "NN", "$,", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "sehen kann,", "tokens": ["se\u00b7hen", "kann", ","], "token_info": ["word", "word", "punct"], "pos": ["VVINF", "VMFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "F\u00e4llt uns voll Anmuht in die Augen, und fieht uns gleichsam", "tokens": ["F\u00e4llt", "uns", "voll", "An\u00b7muht", "in", "die", "Au\u00b7gen", ",", "und", "fieht", "uns", "gleich\u00b7sam"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADJD", "NN", "APPR", "ART", "NN", "$,", "KON", "VVFIN", "PPER", "ADJD"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "l\u00e4chelnd an.", "tokens": ["l\u00e4\u00b7chelnd", "an", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.4": {"line.1": {"text": "Man sah bereits den Sonnen-Strahl sich mit der grauen", "tokens": ["Man", "sah", "be\u00b7reits", "den", "Son\u00b7nen\u00b7Strahl", "sich", "mit", "der", "grau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "ADV", "ART", "NN", "PRF", "APPR", "ART", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "D\u00e4mmrung mischen,", "tokens": ["D\u00e4mm\u00b7rung", "mi\u00b7schen", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Und ihn vom nassen Felde schon der Wolken Thr\u00e4nen gleich-", "tokens": ["Und", "ihn", "vom", "nas\u00b7sen", "Fel\u00b7de", "schon", "der", "Wol\u00b7ken", "Thr\u00e4\u00b7nen", "gleich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "APPRART", "ADJA", "NN", "ADV", "ART", "NN", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "sam wischen.", "tokens": ["sam", "wi\u00b7schen", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVINF", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "Die Welt, durch ihr erquickend Feuer und heitern Schimmer", "tokens": ["Die", "Welt", ",", "durch", "ihr", "er\u00b7qui\u00b7ckend", "Feu\u00b7er", "und", "hei\u00b7tern", "Schim\u00b7mer"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "APPR", "PPER", "VVPP", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "angestrahlt,", "tokens": ["an\u00b7ge\u00b7strahlt", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Ward \u00fcberall mit einem Glanz, wodurch sich, samt den gr\u00fc-", "tokens": ["Ward", "\u00fc\u00b7be\u00b7rall", "mit", "ei\u00b7nem", "Glanz", ",", "wo\u00b7durch", "sich", ",", "samt", "den", "gr\u00fc"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "ADV", "APPR", "ART", "NN", "$,", "PWAV", "PRF", "$,", "APPR", "ART", "TRUNC"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.8": {"text": "nen B\u00fcschen,", "tokens": ["nen", "B\u00fc\u00b7schen", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.9": {"text": "Der Kr\u00e4uter und der Bluhmen Heere, recht, als aufs neu", "tokens": ["Der", "Kr\u00e4u\u00b7ter", "und", "der", "Bluh\u00b7men", "Hee\u00b7re", ",", "recht", ",", "als", "aufs", "neu"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NN", "NN", "$,", "ADJD", "$,", "KOUS", "APPRART", "ADJD"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.10": {"text": "belebt, erfrischen,", "tokens": ["be\u00b7lebt", ",", "er\u00b7fri\u00b7schen", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["VVPP", "$,", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.11": {"text": "Und der auf ihren Bl\u00e4ttern schwebet, mit einer gr\u00fcnen", "tokens": ["Und", "der", "auf", "ih\u00b7ren", "Bl\u00e4t\u00b7tern", "schwe\u00b7bet", ",", "mit", "ei\u00b7ner", "gr\u00fc\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ART", "APPR", "PPOSAT", "NN", "VVFIN", "$,", "APPR", "ART", "ADJA"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "Gluht bemahlt.", "tokens": ["Gluht", "be\u00b7mahlt", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.13": {"text": "Es streckte nah an diesem Walde, voll Nahrungs-reicher", "tokens": ["Es", "streck\u00b7te", "nah", "an", "die\u00b7sem", "Wal\u00b7de", ",", "voll", "Nah\u00b7rungs\u00b7rei\u00b7cher"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "PDAT", "NN", "$,", "ADJD", "NN"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.14": {"text": "fetter Weide,", "tokens": ["fet\u00b7ter", "Wei\u00b7de", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.15": {"text": "Ein angenehmer gr\u00fcner H\u00fcgel den bunt-bebl\u00fchmten R\u00fccken", "tokens": ["Ein", "an\u00b7ge\u00b7neh\u00b7mer", "gr\u00fc\u00b7ner", "H\u00fc\u00b7gel", "den", "bunt\u00b7be\u00b7bl\u00fchm\u00b7ten", "R\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.16": {"text": "her,", "tokens": ["her", ","], "token_info": ["word", "punct"], "pos": ["PTKVZ", "$,"], "meter": "-", "measure": "single.down"}, "line.17": {"text": "Von vielen Wollen-reichen Heerden und Horn-Vieh schien", "tokens": ["Von", "vie\u00b7len", "Wol\u00b7len\u00b7rei\u00b7chen", "Heer\u00b7den", "und", "Horn\u00b7Vieh", "schien"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "NN", "KON", "NE", "VVFIN"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.18": {"text": "er gleichsam schwehr,", "tokens": ["er", "gleich\u00b7sam", "schwehr", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ADJD", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.19": {"text": "Der Seiten sanfter Abhang gl\u00e4nzt\u2019 vom gelben reifenden", "tokens": ["Der", "Sei\u00b7ten", "sanf\u00b7ter", "Ab\u00b7hang", "gl\u00e4nzt'", "vom", "gel\u00b7ben", "rei\u00b7fen\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+--", "measure": "unknown.measure.hexa"}, "line.20": {"text": "Getrayde.", "tokens": ["Ge\u00b7tray\u00b7de", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.5": {"line.1": {"text": "Dort sah man weisse, fette Schafe besch\u00e4ftiget, das feinste", "tokens": ["Dort", "sah", "man", "weis\u00b7se", ",", "fet\u00b7te", "Scha\u00b7fe", "be\u00b7sch\u00e4f\u00b7ti\u00b7get", ",", "das", "feins\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "VVFIN", "$,", "ADJA", "NN", "VVFIN", "$,", "ART", "ADJA"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Gras,", "tokens": ["Gras", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Mit regem Zahn und kurzem Ruck, von unten aufwerts", "tokens": ["Mit", "re\u00b7gem", "Zahn", "und", "kur\u00b7zem", "Ruck", ",", "von", "un\u00b7ten", "auf\u00b7werts"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "KON", "ADJA", "NN", "$,", "APPR", "ADV", "ADV"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "abzurupfen,", "tokens": ["ab\u00b7zu\u00b7rup\u00b7fen", ","], "token_info": ["word", "punct"], "pos": ["VVIZU", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Dort, durch verwachsne dunkle B\u00fcsche, oft muntre junge", "tokens": ["Dort", ",", "durch", "ver\u00b7wachs\u00b7ne", "dunk\u00b7le", "B\u00fc\u00b7sche", ",", "oft", "mun\u00b7tre", "jun\u00b7ge"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "$,", "APPR", "ADJA", "ADJA", "NN", "$,", "ADV", "ADJA", "ADJA"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "L\u00e4mmer schlupfen,", "tokens": ["L\u00e4m\u00b7mer", "schlup\u00b7fen", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Hier, wie so manches glatte Kalb das Laub von schla