{"textgrid.poem.34482": {"metadata": {"author": {"name": "Hartleben, Otto Erich", "birth": "N.A.", "death": "N.A."}, "title": "[die Wolken lasten auf der Wintererde]", "genre": "verse", "period": "N.A.", "pub_year": 1888, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Wolken lasten auf der Wintererde.", "tokens": ["Die", "Wol\u00b7ken", "las\u00b7ten", "auf", "der", "Win\u00b7ter\u00b7er\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Es d\u00e4mmert, und die Flocken wirbeln mir", "tokens": ["Es", "d\u00e4m\u00b7mert", ",", "und", "die", "Flo\u00b7cken", "wir\u00b7beln", "mir"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KON", "ART", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "ums Haupt. \u2013 Hinschreit ich auf bekannten Strassen,", "tokens": ["ums", "Haupt", ".", "\u2013", "Hin\u00b7schreit", "ich", "auf", "be\u00b7kann\u00b7ten", "Stras\u00b7sen", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$.", "$(", "NN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "die ich nach langen Tagen wiederfinde.", "tokens": ["die", "ich", "nach", "lan\u00b7gen", "Ta\u00b7gen", "wie\u00b7der\u00b7fin\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRELS", "PPER", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Soll ich die alten Wege wieder wandeln,", "tokens": ["Soll", "ich", "die", "al\u00b7ten", "We\u00b7ge", "wie\u00b7der", "wan\u00b7de