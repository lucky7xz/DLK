{"dta.poem.11782": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "68.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich sprach am Abend, als ich meinen Stock bego\u00df:", "tokens": ["Ich", "sprach", "am", "A\u00b7bend", ",", "als", "ich", "mei\u00b7nen", "Stock", "be\u00b7go\u00df", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$,", "KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sag' an, warum sich heut nicht diese Bl\u00fct' erschlo\u00df?", "tokens": ["Sag'", "an", ",", "wa\u00b7rum", "sich", "heut", "nicht", "die\u00b7se", "Bl\u00fct'", "er\u00b7schlo\u00df", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "PWAV", "PRF", "ADV", "PTKNEG", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ger\u00f6thet hat ihr Mund der Sonne Ku\u00df empfangen,", "tokens": ["Ge\u00b7r\u00f6\u00b7thet", "hat", "ihr", "Mund", "der", "Son\u00b7ne", "Ku\u00df", "emp\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPOSAT", "NN", "ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ihr Busen schwoll; warum ist sie nicht aufgegangen?", "tokens": ["Ihr", "Bu\u00b7sen", "schwoll", ";", "wa\u00b7rum", "ist", "sie", "nicht", "auf\u00b7ge\u00b7gan\u00b7gen", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "$.", "PWAV", "VAFIN", "PPER", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Da wiegte sanft der Stock sein Haupt im Abendwinde,", "tokens": ["Da", "wieg\u00b7te", "sanft", "der", "Stock", "sein", "Haupt", "im", "Ab\u00b7end\u00b7win\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "ART", "NN", "PPOSAT", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und sprach: Ich hab' es selbst gerathen meinem Kinde.", "tokens": ["Und", "sprach", ":", "Ich", "hab'", "es", "selbst", "ge\u00b7ra\u00b7then", "mei\u00b7nem", "Kin\u00b7de", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "PPER", "VAFIN", "PPER", "ADV", "VVPP", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Sie w\u00e4re heut nur unvollkommen aufgebl\u00fcht,", "tokens": ["Sie", "w\u00e4\u00b7re", "heut", "nur", "un\u00b7voll\u00b7kom\u00b7men", "auf\u00b7ge\u00b7bl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Denn viele schlo\u00df ich auf, und meine Kraft ist m\u00fcd.", "tokens": ["Denn", "vie\u00b7le", "schlo\u00df", "ich", "auf", ",", "und", "mei\u00b7ne", "Kraft", "ist", "m\u00fcd", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "PPER", "PTKVZ", "$,", "KON", "PPOSAT", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Wir wollen sammeln ihr im Schlummer frischen Duft,", "tokens": ["Wir", "wol\u00b7len", "sam\u00b7meln", "ihr", "im", "Schlum\u00b7mer", "fri\u00b7schen", "Duft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "PPER", "APPRART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und morgen w\u00fcrzen soll ihr Hauch die Morgenluft.", "tokens": ["Und", "mor\u00b7gen", "w\u00fcr\u00b7zen", "soll", "ihr", "Hauch", "die", "Mor\u00b7gen\u00b7luft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVINF", "VMFIN", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "So sprach der Strauch; ich gieng und hielt in mir zum Gl\u00fcck", "tokens": ["So", "sprach", "der", "Strauch", ";", "ich", "gieng", "und", "hielt", "in", "mir", "zum", "Gl\u00fcck"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "$.", "PPER", "VVFIN", "KON", "VVFIN", "APPR", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein halberschlossnes Lied auf morgen auch zur\u00fcck.", "tokens": ["Ein", "hal\u00b7ber\u00b7schloss\u00b7nes", "Lied", "auf", "mor\u00b7gen", "auch", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADV", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}