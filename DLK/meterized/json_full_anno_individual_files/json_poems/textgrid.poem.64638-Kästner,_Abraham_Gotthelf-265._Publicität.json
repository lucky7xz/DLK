{"textgrid.poem.64638": {"metadata": {"author": {"name": "K\u00e4stner, Abraham Gotthelf", "birth": "N.A.", "death": "N.A."}, "title": "265. Publicit\u00e4t", "genre": "verse", "period": "N.A.", "pub_year": 1759, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was sie versehn, die F\u00fcrsten und die Staaten,", "tokens": ["Was", "sie", "ver\u00b7sehn", ",", "die", "F\u00fcrs\u00b7ten", "und", "die", "Staa\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVINF", "$,", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zeigt er mit deutscher Freiheit an;", "tokens": ["Zeigt", "er", "mit", "deut\u00b7scher", "Frei\u00b7heit", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch, soll ich euch f\u00fcr euren Frieden rathen,", "tokens": ["Doch", ",", "soll", "ich", "euch", "f\u00fcr", "eu\u00b7ren", "Frie\u00b7den", "ra\u00b7then", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VMFIN", "PPER", "PRF", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "So schreibt ja Freunden nichts von dem, was er gethan.", "tokens": ["So", "schreibt", "ja", "Freun\u00b7den", "nichts", "von", "dem", ",", "was", "er", "ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "NN", "PIS", "APPR", "ART", "$,", "PWS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Was sie versehn, die F\u00fc