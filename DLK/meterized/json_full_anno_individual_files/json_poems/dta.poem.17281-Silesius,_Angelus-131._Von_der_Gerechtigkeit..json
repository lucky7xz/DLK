{"dta.poem.17281": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "131.  Von der Gerechtigkeit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Gerechtigkeit ist weg! wohin? sie ist inn Himmel/", "tokens": ["Ge\u00b7rech\u00b7tig\u00b7keit", "ist", "weg", "!", "wo\u00b7hin", "?", "sie", "ist", "inn", "Him\u00b7mel", "/"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "$.", "PWAV", "$.", "PPER", "VAFIN", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Warum\u0304? sie traute sich nicht mehr bey dem Get\u00fcm\u0304el.", "tokens": ["War\u00b7um\u0304", "?", "sie", "trau\u00b7te", "sich", "nicht", "mehr", "bey", "dem", "Ge\u00b7t\u00fcm\u0304el", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "PPER", "VVFIN", "PRF", "PTKNEG", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+--+-+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Was kont\u2019 jhr dan\u0304 geschehn? sie w\u00e4re von der Welt", "tokens": ["Was", "kont'", "jhr", "dan\u0304", "ge\u00b7schehn", "?", "sie", "w\u00e4\u00b7re", "von", "der", "Welt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "VMFIN", "PPER", "ADV", "VVPP", "$.", "PPER", "VAFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Schon l\u00e4ngst an jhren Ehrn geschw\u00e4chet und gef\u00e4llt.", "tokens": ["Schon", "l\u00e4ngst", "an", "jhren", "Ehrn", "ge\u00b7schw\u00e4\u00b7chet", "und", "ge\u00b7f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "PPOSAT", "NN", "VVPP", "KON", "VVPP", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}}}}