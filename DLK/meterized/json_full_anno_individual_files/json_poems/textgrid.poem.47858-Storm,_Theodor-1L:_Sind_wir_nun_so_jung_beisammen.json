{"textgrid.poem.47858": {"metadata": {"author": {"name": "Storm, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sind wir nun so jung beisammen", "genre": "verse", "period": "N.A.", "pub_year": 1852, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sind wir nun so jung beisammen", "tokens": ["Sind", "wir", "nun", "so", "jung", "bei\u00b7sam\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In der holden Morgenfruh,", "tokens": ["In", "der", "hol\u00b7den", "Mor\u00b7gen\u00b7fruh", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "S\u00fc\u00dfes, rosenrotes M\u00fcndchen,", "tokens": ["S\u00fc\u00b7\u00dfes", ",", "ro\u00b7sen\u00b7ro\u00b7tes", "M\u00fcnd\u00b7chen", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Plaudre, plaudre immerzu!", "tokens": ["Plaud\u00b7re", ",", "plaud\u00b7re", "im\u00b7mer\u00b7zu", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Organiste sollt ich werden", "tokens": ["Or\u00b7ga\u00b7nis\u00b7te", "sollt", "ich", "wer\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VMFIN", "PPER", "VAINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "An dem neuen Kirchlein hier? \u2013", "tokens": ["An", "dem", "neu\u00b7en", "Kirch\u00b7lein", "hier", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ADV", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Kind! wer geigte dann den Finken", "tokens": ["Kind", "!", "wer", "geig\u00b7te", "dann", "den", "Fin\u00b7ken"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$.", "PWS", "VVFIN", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Feiertags im Waldrevier?", "tokens": ["Fei\u00b7er\u00b7tags", "im", "Wald\u00b7re\u00b7vier", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Doch du meinest, Amt und W\u00fcrden,", "tokens": ["Doch", "du", "mei\u00b7nest", ",", "Amt", "und", "W\u00fcr\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Eigner Herd sei goldeswert! \u2013", "tokens": ["Eig\u00b7ner", "Herd", "sei", "gol\u00b7des\u00b7wert", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ADJA", "NN", "VAFIN", "VVPP", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Machst du mich doch schier beklommen;", "tokens": ["Machst", "du", "mich", "doch", "schier", "be\u00b7klom\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "So was hab ich nie begehrt.", "tokens": ["So", "was", "hab", "ich", "nie", "be\u00b7gehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWS", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Was? Und auch der Stadttrompeter", "tokens": ["Was", "?", "Und", "auch", "der", "Stadt\u00b7trom\u00b7pe\u00b7ter"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "$.", "KON", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Starb vergangne Woche nur?", "tokens": ["Starb", "ver\u00b7gang\u00b7ne", "Wo\u00b7che", "nur", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und du meinst, zu solchem Posten", "tokens": ["Und", "du", "meinst", ",", "zu", "sol\u00b7chem", "Pos\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "ADV", "$,", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "H\u00e4tt ich just die Positur? \u2013", "tokens": ["H\u00e4tt", "ich", "just", "die", "Po\u00b7si\u00b7tur", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "VVFIN", "ART", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Hei! Wie kr\u00e4ht der Hahn so grimmig!", "tokens": ["Hei", "!", "Wie", "kr\u00e4ht", "der", "Hahn", "so", "grim\u00b7mig", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PWAV", "VVFIN", "ART", "NN", "ADV", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schatz, ade! Gedenk an mich!", "tokens": ["Schatz", ",", "a\u00b7de", "!", "Ge\u00b7denk", "an", "mich", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "$.", "NN", "APPR", "PPER", "$."], "meter": "+---+-+", "measure": "dactylic.init"}, "line.3": {"text": "Mach den Hahn zum Stadttrompeter!", "tokens": ["Mach", "den", "Hahn", "zum", "Stadt\u00b7trom\u00b7pe\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Der kann's besser noch als ich!", "tokens": ["Der", "kann's", "bes\u00b7ser", "noch", "als", "ich", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ADJD", "ADV", "KOUS", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Sind wir nun so jung beisammen", "tokens": ["Sind", "wir", "nun", "so", "jung", "bei\u00b7sam\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In der holden Morgenfruh,", "tokens": ["In", "der", "hol\u00b7den", "Mor\u00b7gen\u00b7fruh", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "S\u00fc\u00dfes, rosenrotes M\u00fcndchen,", "tokens": ["S\u00fc\u00b7\u00dfes", ",", "ro\u00b7sen\u00b7ro\u00b7tes", "M\u00fcnd\u00b7chen", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Plaudre, plaudre immerzu!", "tokens": ["Plaud\u00b7re", ",", "plaud\u00b7re", "im\u00b7mer\u00b7zu", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Organiste sollt ich werden", "tokens": ["Or\u00b7ga\u00b7nis\u00b7te", "sollt", "ich", "wer\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VMFIN", "PPER", "VAINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "An dem neuen Kirc