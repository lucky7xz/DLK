,", "tokens": ["Nun", "dankt", "Er", "Gott", "f\u00fcr", "sei\u00b7ne", "Macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und singt: Viktoria!", "tokens": ["Und", "singt", ":", "Vik\u00b7to\u00b7ria", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "NE", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Und alles Blut aus dieser Schlacht", "tokens": ["Und", "al\u00b7les", "Blut", "aus", "die\u00b7ser", "Schlacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Flie\u00dft nach ", "tokens": ["Flie\u00dft", "nach"], "token_info": ["word", "word"], "pos": ["VVFIN", "APPR"], "meter": "-+", "measure": "iambic.single"}}, "stanza.30": {"line.1": {"text": "Und weigert sie auf diesen Tag,", "tokens": ["Und", "wei\u00b7gert", "sie", "auf", "die\u00b7sen", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den Frieden vorzuziehn;", "tokens": ["Den", "Frie\u00b7den", "vor\u00b7zu\u00b7ziehn", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So st\u00fcrme, ", "tokens": ["So", "st\u00fcr\u00b7me", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Und dann f\u00fchr uns nach Wien.", "tokens": ["Und", "dann", "f\u00fchr", "uns", "nach", "Wi\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "po