{"textgrid.poem.36560": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Siegeslied nach der Schlacht bei Prag", "genre": "verse", "period": "N.A.", "pub_year": 1757, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Viktoria! mit uns ist Gott,", "tokens": ["Vik\u00b7to\u00b7ria", "!", "mit", "uns", "ist", "Gott", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "APPR", "PPER", "VAFIN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Der stolze Feind liegt da!", "tokens": ["Der", "stol\u00b7ze", "Feind", "liegt", "da", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er liegt, gerecht ist unser Gott,", "tokens": ["Er", "liegt", ",", "ge\u00b7recht", "ist", "un\u00b7ser", "Gott", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADJD", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Er liegt, Viktoria!", "tokens": ["Er", "liegt", ",", "Vik\u00b7to\u00b7ria", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "NE", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Zwar unser Vater ist nicht mehr,", "tokens": ["Zwar", "un\u00b7ser", "Va\u00b7ter", "ist", "nicht", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VAFIN", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Jedoch er starb ein Held,", "tokens": ["Je\u00b7doch", "er", "starb", "ein", "Held", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und sieht nun unser Siegesheer,", "tokens": ["Und", "sieht", "nun", "un\u00b7ser", "Sie\u00b7ges\u00b7heer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vom hohen Sternenzelt.", "tokens": ["Vom", "ho\u00b7hen", "Ster\u00b7nen\u00b7zelt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Er ging voran, der edle Greis!", "tokens": ["Er", "ging", "vo\u00b7ran", ",", "der", "ed\u00b7le", "Greis", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Voll Gott und Vaterland.", "tokens": ["Voll", "Gott", "und", "Va\u00b7ter\u00b7land", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sein alter Kopf war kaum so wei\u00df,", "tokens": ["Sein", "al\u00b7ter", "Kopf", "war", "kaum", "so", "wei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als tapfer seine Hand.", "tokens": ["Als", "tap\u00b7fer", "sei\u00b7ne", "Hand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Mit jugendlicher Heldenkraft", "tokens": ["Mit", "ju\u00b7gend\u00b7li\u00b7cher", "Hel\u00b7den\u00b7kraft"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ergriff sie eine Fahn,", "tokens": ["Er\u00b7griff", "sie", "ei\u00b7ne", "Fahn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Hielt sie empor an ihrem Schaft,", "tokens": ["Hielt", "sie", "em\u00b7por", "an", "ih\u00b7rem", "Schaft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df wir sie alle sahn;", "tokens": ["Da\u00df", "wir", "sie", "al\u00b7le", "sahn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "PIS", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Und sagte: \u00bbKinder, Berg hinan,", "tokens": ["Und", "sag\u00b7te", ":", "\u00bb", "Kin\u00b7der", ",", "Berg", "hi\u00b7nan", ","], "token_info": ["word", "word", "punct", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "$(", "NN", "$,", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auf Schanzen und Gesch\u00fctz!\u00ab", "tokens": ["Auf", "Schan\u00b7zen", "und", "Ge\u00b7sch\u00fctz", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wir folgten alle, Mann vor Mann,", "tokens": ["Wir", "folg\u00b7ten", "al\u00b7le", ",", "Mann", "vor", "Mann", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "$,", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Geschwinder wie der Blitz.", "tokens": ["Ge\u00b7schwin\u00b7der", "wie", "der", "Blitz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Ach! aber unser Vater fiel,", "tokens": ["Ach", "!", "a\u00b7ber", "un\u00b7ser", "Va\u00b7ter", "fiel", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Fahne sank auf ihn.", "tokens": ["Die", "Fah\u00b7ne", "sank", "auf", "ihn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ha! welch glorreiches Lebensziel,", "tokens": ["Ha", "!", "welch", "glor\u00b7rei\u00b7ches", "Le\u00b7bens\u00b7ziel", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PWAT", "ADJA", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Gl\u00fcckseliger ", "tokens": ["Gl\u00fcck\u00b7se\u00b7li\u00b7ger"], "token_info": ["word"], "pos": ["NN"], "meter": "+---", "measure": "dactylic.init"}}, "stanza.7": {"line.1": {"text": "Dein ", "tokens": ["Dein"], "token_info": ["word"], "pos": ["PPOSAT"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "Indem er uns gebot;", "tokens": ["In\u00b7dem", "er", "uns", "ge\u00b7bot", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wir aber st\u00fcrzten in den Feind,", "tokens": ["Wir", "a\u00b7ber", "st\u00fcrz\u00b7ten", "in", "den", "Feind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zu r\u00e4chen deinen Tod.", "tokens": ["Zu", "r\u00e4\u00b7chen", "dei\u00b7nen", "Tod", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Du, ", "tokens": ["Du", ","], "token_info": ["word", "punct"], "pos": ["PPER", "$,"], "meter": "-", "measure": "single.down"}, "line.2": {"text": "Du fochtest k\u00f6niglich!", "tokens": ["Du", "foch\u00b7test", "k\u00f6\u00b7nig\u00b7lich", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wir sahen alle, That vor That,", "tokens": ["Wir", "sa\u00b7hen", "al\u00b7le", ",", "That", "vor", "That", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "$,", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Du junger L\u00f6w', auf dich!", "tokens": ["Du", "jun\u00b7ger", "L\u00f6\u00b7w'", ",", "auf", "dich", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,", "APPR", "PPER", "$."], "meter": "-+-+-++", "measure": "unknown.measure.tetra"}}, "stanza.9": {"line.1": {"text": "Der Pommer und der M\u00e4rker stritt", "tokens": ["Der", "Pom\u00b7mer", "und", "der", "M\u00e4r\u00b7ker", "stritt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit rechtem Christenmut:", "tokens": ["Mit", "rech\u00b7tem", "Chris\u00b7ten\u00b7mut", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Rot ward sein Schwert, auf jedem Schritt", "tokens": ["Rot", "ward", "sein", "Schwert", ",", "auf", "je\u00b7dem", "Schritt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "VAFIN", "PPOSAT", "NN", "$,", "APPR", "PIAT", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Flo\u00df dick Pandurenblut.", "tokens": ["Flo\u00df", "dick", "Pan\u00b7du\u00b7ren\u00b7blut", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJD", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.10": {"line.1": {"text": "Aus sieben Schanzen jagten wir", "tokens": ["Aus", "sie\u00b7ben", "Schan\u00b7zen", "jag\u00b7ten", "wir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "CARD", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die M\u00fctzen von dem B\u00e4r.", "tokens": ["Die", "M\u00fct\u00b7zen", "von", "dem", "B\u00e4r", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da, ", "tokens": ["Da", ","], "token_info": ["word", "punct"], "pos": ["ADV", "$,"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Auf Leichen hoch einher.", "tokens": ["Auf", "Lei\u00b7chen", "hoch", "ein\u00b7her", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Dacht' in dem m\u00f6rderischen Kampf", "tokens": ["Dacht'", "in", "dem", "m\u00f6r\u00b7de\u00b7ri\u00b7schen", "Kampf"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gott, Vaterland, und dich;", "tokens": ["Gott", ",", "Va\u00b7ter\u00b7land", ",", "und", "dich", ";"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "KON", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sah, tief in schwarzem Rauch und Dampf,", "tokens": ["Sah", ",", "tief", "in", "schwar\u00b7zem", "Rauch", "und", "Dampf", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADJD", "APPR", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dich, seinen ", "tokens": ["Dich", ",", "sei\u00b7nen"], "token_info": ["word", "punct", "word"], "pos": ["PPER", "$,", "PPOSAT"], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.12": {"line.1": {"text": "Und zitterte, ward feuerrot,", "tokens": ["Und", "zit\u00b7ter\u00b7te", ",", "ward", "feu\u00b7er\u00b7rot", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im kriegrischen Gesicht,", "tokens": ["Im", "krie\u00b7gri\u00b7schen", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "(er zitterte vor Deinem Tod,", "tokens": ["(", "er", "zit\u00b7ter\u00b7te", "vor", "Dei\u00b7nem", "Tod", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vor seinem aber nicht.)", "tokens": ["Vor", "sei\u00b7nem", "a\u00b7ber", "nicht", ".", ")"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PPOSAT", "ADV", "PTKNEG", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Verachtete die Kugelsaat,", "tokens": ["Ver\u00b7ach\u00b7te\u00b7te", "die", "Ku\u00b7gel\u00b7saat", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der St\u00fccke Donnerton,", "tokens": ["Der", "St\u00fc\u00b7cke", "Don\u00b7ner\u00b7ton", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Stritt w\u00fctender, that Heldenthat,", "tokens": ["Stritt", "w\u00fc\u00b7ten\u00b7der", ",", "that", "Hel\u00b7den\u00b7that", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bis deine Feinde floh'n.", "tokens": ["Bis", "dei\u00b7ne", "Fein\u00b7de", "floh'", "n."], "token_info": ["word", "word", "word", "word", "abbreviation"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "NE"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Nun dankt Er Gott f\u00fcr seine Macht,", "tokens": ["Nun", "dankt", "Er", "Gott", "f\u00fcr", "sei\u00b7ne", "Macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und singt: Viktoria!", "tokens": ["Und", "singt", ":", "Vik\u00b7to\u00b7ria", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "NE", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Und alles Blut aus dieser Schlacht", "tokens": ["Und", "al\u00b7les", "Blut", "aus", "die\u00b7ser", "Schlacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Flie\u00dft nach ", "tokens": ["Flie\u00dft", "nach"], "token_info": ["word", "word"], "pos": ["VVFIN", "APPR"], "meter": "-+", "measure": "iambic.single"}}, "stanza.15": {"line.1": {"text": "Und weigert sie auf diesen Tag,", "tokens": ["Und", "wei\u00b7gert", "sie", "auf", "die\u00b7sen", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den Frieden vorzuziehn;", "tokens": ["Den", "Frie\u00b7den", "vor\u00b7zu\u00b7ziehn", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So st\u00fcrme, ", "tokens": ["So", "st\u00fcr\u00b7me", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Und dann f\u00fchr uns nach Wien.", "tokens": ["Und", "dann", "f\u00fchr", "uns", "nach", "Wi\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "APPR", "NE", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}}, "stanza.16": {"line.1": {"text": "Viktoria! mit uns ist Gott,", "tokens": ["Vik\u00b7to\u00b7ria", "!", "mit", "uns", "ist", "Gott", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "APPR", "PPER", "VAFIN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Der stolze Feind liegt da!", "tokens": ["Der", "stol\u00b7ze", "Feind", "liegt", "da", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er liegt, gerecht ist unser Gott,", "tokens": ["Er", "liegt", ",", "ge\u00b7recht", "ist", "un\u00b7ser", "Gott", ","], "token_info": ["word", "word", "punct", "word", "word", "