{"textgrid.poem.48617": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "28. Auf M. Heinrich L\u00fctgens, revlischen Musikantens, Namenstag", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "ohngefehr auf seinen Bauch", "tokens": ["ohn\u00b7ge\u00b7fehr", "auf", "sei\u00b7nen", "Bauch"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "hinter einen Haselstrauch,", "tokens": ["hin\u00b7ter", "ei\u00b7nen", "Ha\u00b7sel\u00b7strauch", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "da so mancher Vogel hecket,", "tokens": ["da", "so", "man\u00b7cher", "Vo\u00b7gel", "he\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PIAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "und lie\u00df seiner Heerden Zahl", "tokens": ["und", "lie\u00df", "sei\u00b7ner", "Heer\u00b7den", "Zahl"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "grasen durch das gr\u00fcne Tal.", "tokens": ["gra\u00b7sen", "durch", "das", "gr\u00fc\u00b7ne", "Tal", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Eben war sein lieber Name", "tokens": ["E\u00b7ben", "war", "sein", "lie\u00b7ber", "Na\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "gleich hinwieder kommen an,", "tokens": ["gleich", "hin\u00b7wie\u00b7der", "kom\u00b7men", "an", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "der auch alsobalden kame", "tokens": ["der", "auch", "al\u00b7so\u00b7bal\u00b7den", "ka\u00b7me"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADV", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "und sang: O der sch\u00f6nen Zeit!", "tokens": ["und", "sang", ":", "O", "der", "sch\u00f6\u00b7nen", "Zeit", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "NE", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Die gelobten Nymphen alle", "tokens": ["Die", "ge\u00b7lob\u00b7ten", "Nym\u00b7phen", "al\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PIAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "durch das ausgestreckte Tal", "tokens": ["durch", "das", "aus\u00b7ge\u00b7streck\u00b7te", "Tal"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "huben hierauf allzumal", "tokens": ["hu\u00b7ben", "hier\u00b7auf", "all\u00b7zu\u00b7mal"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PAV", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "an mit lautem Lobeschalle:", "tokens": ["an", "mit", "lau\u00b7tem", "Lo\u00b7be\u00b7schal\u00b7le", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Echo sprach: So lang' ihr seid!", "to