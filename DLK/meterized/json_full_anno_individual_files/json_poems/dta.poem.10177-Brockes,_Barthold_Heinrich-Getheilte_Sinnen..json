{"dta.poem.10177": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Getheilte Sinnen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20086-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Indem ich j\u00fcngst, im Frost annoch,", "tokens": ["In\u00b7dem", "ich", "j\u00fcngst", ",", "im", "Frost", "an\u00b7noch", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "$,", "APPRART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vor dick gefrornen Fenstern stand,", "tokens": ["Vor", "dick", "ge\u00b7fror\u00b7nen", "Fens\u00b7tern", "stand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und meiner Bluhmen Balsam roch,", "tokens": ["Und", "mei\u00b7ner", "Bluh\u00b7men", "Bal\u00b7sam", "roch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den ich recht ungemein erquickend lieblich fand,", "tokens": ["Den", "ich", "recht", "un\u00b7ge\u00b7mein", "er\u00b7qui\u00b7ckend", "lieb\u00b7lich", "fand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ADV", "ADV", "ADJD", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und ich zu gleicher Zeit, bey diesem s\u00fcssen Dufft,", "tokens": ["Und", "ich", "zu", "glei\u00b7cher", "Zeit", ",", "bey", "die\u00b7sem", "s\u00fcs\u00b7sen", "Dufft", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "ADJA", "NN", "$,", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Des warmen Zimmers laue Lufft,", "tokens": ["Des", "war\u00b7men", "Zim\u00b7mers", "lau\u00b7e", "Lufft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die sanft mir \u00fcm die Glieder spielte,", "tokens": ["Die", "sanft", "mir", "\u00fcm", "die", "Glie\u00b7der", "spiel\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PPER", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Mit nicht geringer Anmuth, f\u00fchlte;", "tokens": ["Mit", "nicht", "ge\u00b7rin\u00b7ger", "An\u00b7muth", ",", "f\u00fchl\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PTKNEG", "ADJA", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Doch auch zugleich, wie draussen alles wei\u00df,", "tokens": ["Doch", "auch", "zu\u00b7gleich", ",", "wie", "draus\u00b7sen", "al\u00b7les", "wei\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "$,", "PWAV", "ADV", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Und sich die Welt mit Reiff, mit Schnee und Eis,", "tokens": ["Und", "sich", "die", "Welt", "mit", "Reiff", ",", "mit", "Schnee", "und", "Eis", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "ART", "NN", "APPR", "NN", "$,", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "In Silber-gleichem Schimmer schm\u00fcckte,", "tokens": ["In", "Sil\u00b7ber\u00b7glei\u00b7chem", "Schim\u00b7mer", "schm\u00fcck\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Durchs dichte Fenster-Glas erblickte;", "tokens": ["Durchs", "dich\u00b7te", "Fens\u00b7ter\u00b7Glas", "er\u00b7blick\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Und auch zugleich von Wagen und von Karren", "tokens": ["Und", "auch", "zu\u00b7gleich", "von", "Wa\u00b7gen", "und", "von", "Kar\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "APPR", "NN", "KON", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Gefrorne R\u00e4der pfeiffen, knarren,", "tokens": ["Ge\u00b7fror\u00b7ne", "R\u00e4\u00b7der", "pfeif\u00b7fen", ",", "knar\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Und vom getretnen Schnee ein lautes knirschen h\u00f6rte;", "tokens": ["Und", "vom", "ge\u00b7tret\u00b7nen", "Schnee", "ein", "lau\u00b7tes", "knir\u00b7schen", "h\u00f6r\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Gedacht ich bey mir selbst; wie soll ich dieses fassen?", "tokens": ["Ge\u00b7dacht", "ich", "bey", "mir", "selbst", ";", "wie", "soll", "ich", "die\u00b7ses", "fas\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "PPER", "ADV", "$.", "PWAV", "VMFIN", "PPER", "PDS", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Kann meine Seele sich denn theilen lassen?", "tokens": ["Kann", "mei\u00b7ne", "See\u00b7le", "sich", "denn", "thei\u00b7len", "las\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "PRF", "ADV", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Sie kann zu einer Zeit durch zweyer Sinnen Th\u00fcren", "tokens": ["Sie", "kann", "zu", "ei\u00b7ner", "Zeit", "durch", "zwey\u00b7er", "Sin\u00b7nen", "Th\u00fc\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "ART", "NN", "APPR", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Jm Winter, und durch zwey im Fr\u00fchling, sich versp\u00fcren.", "tokens": ["Jm", "Win\u00b7ter", ",", "und", "durch", "zwey", "im", "Fr\u00fch\u00b7ling", ",", "sich", "ver\u00b7sp\u00fc\u00b7ren", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "KON", "APPR", "CARD", "APPRART", "NN", "$,", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Wo sind denn ihrer Kr\u00e4ffte Grentzen?", "tokens": ["Wo", "sind", "denn", "ih\u00b7rer", "Kr\u00e4ff\u00b7te", "Grent\u00b7zen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ADV", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Sie ist im Winter halb, und halb im Lentzen.", "tokens": ["Sie", "ist", "im", "Win\u00b7ter", "halb", ",", "und", "halb", "im", "Lent\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPRART", "NN", "ADJD", "$,", "KON", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}