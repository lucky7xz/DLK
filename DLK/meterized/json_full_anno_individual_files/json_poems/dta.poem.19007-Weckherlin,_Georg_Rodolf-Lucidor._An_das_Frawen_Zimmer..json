{"dta.poem.19007": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "Lucidor.  \n An das Frawen Zimmer.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-200905198111", "language": ["de:0.99"], "booktitle": "Weckherlin, Georg Rodolf: Gaistliche und Weltliche Gedichte. Amsterdam, 1641."}, "poem": {"stanza.1": {"line.1": {"text": "Jhr Nymfen diser welt der einig wehrte pracht", "tokens": ["Ihr", "Nym\u00b7fen", "di\u00b7ser", "welt", "der", "ei\u00b7nig", "wehr\u00b7te", "pracht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "PDAT", "NN", "ART", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit deren lieblichkeit die himmel sich erlaben;", "tokens": ["Mit", "de\u00b7ren", "lieb\u00b7lich\u00b7keit", "die", "him\u00b7mel", "sich", "er\u00b7la\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "ART", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die G\u00f6tter haben sich schier mangelhafft gemacht", "tokens": ["Die", "G\u00f6t\u00b7ter", "ha\u00b7ben", "sich", "schier", "man\u00b7gel\u00b7hafft", "ge\u00b7macht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PRF", "ADJA", "NN", "VVPP"], "meter": "-+-+-+---+-+", "measure": "unknown.measure.penta"}, "line.4": {"text": "Euch reichlich zu begaben.", "tokens": ["Euch", "reich\u00b7lich", "zu", "be\u00b7ga\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Auch ist das lob so gro\u00df von der vollkommenheit", "tokens": ["Auch", "ist", "das", "lob", "so", "gro\u00df", "von", "der", "voll\u00b7kom\u00b7men\u00b7heit"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADV", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dadurch die Helden jhr der freyheit gern beraubet/", "tokens": ["Da\u00b7durch", "die", "Hel\u00b7den", "jhr", "der", "frey\u00b7heit", "gern", "be\u00b7rau\u00b7bet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "PPER", "ART", "NN", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da\u00df es das grobe Volck f\u00fcr eine eytelkeit", "tokens": ["Da\u00df", "es", "das", "gro\u00b7be", "Volck", "f\u00fcr", "ei\u00b7ne", "ey\u00b7tel\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Schier haltend vngern glaubet.", "tokens": ["Schier", "hal\u00b7tend", "vn\u00b7gern", "glau\u00b7bet", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "KON", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Weil aber ich nu sih da\u00df ewer angesicht", "tokens": ["Weil", "a\u00b7ber", "ich", "nu", "sih", "da\u00df", "e\u00b7wer", "an\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "PPER", "ADV", "VVFIN", "KOUS", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit so liebreicher brunst das beste hertz versehret/", "tokens": ["Mit", "so", "lieb\u00b7rei\u00b7cher", "brunst", "das", "bes\u00b7te", "hertz", "ver\u00b7seh\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Sprich Ich da\u00df ewer lob mit einigem gedicht", "tokens": ["Sprich", "Ich", "da\u00df", "e\u00b7wer", "lob", "mit", "ei\u00b7ni\u00b7gem", "ge\u00b7dicht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "KOUS", "PPOSAT", "NN", "APPR", "PIAT", "NN"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Die warheit nicht vermehret.", "tokens": ["Die", "war\u00b7heit", "nicht", "ver\u00b7meh\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Dan ewrer augenschein ist so s\u00fc\u00df/ keusch vnd klar", "tokens": ["Dan", "ew\u00b7rer", "au\u00b7gen\u00b7schein", "ist", "so", "s\u00fc\u00df", "/", "keusch", "vnd", "klar"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "NN", "VAFIN", "ADV", "ADJD", "$(", "ADJD", "KON", "ADJD"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Da\u00df wer sich jmmer Euch zu loben wolt bem\u00fchen/", "tokens": ["Da\u00df", "wer", "sich", "jm\u00b7mer", "Euch", "zu", "lo\u00b7ben", "wolt", "be\u00b7m\u00fc\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PWS", "PRF", "ADV", "PPER", "PTKZU", "VVINF", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der kan/ wie wei\u00df er auch/ mit seiner witz gefahr", "tokens": ["Der", "kan", "/", "wie", "wei\u00df", "er", "auch", "/", "mit", "sei\u00b7ner", "witz", "ge\u00b7fahr"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VMFIN", "$(", "PWAV", "VVFIN", "PPER", "ADV", "$(", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Kaum seiner schand entfliehen.", "tokens": ["Kaum", "sei\u00b7ner", "schand", "ent\u00b7flie\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Ich meines theils kam nu weit vber Meer hieher/", "tokens": ["Ich", "mei\u00b7nes", "theils", "kam", "nu", "weit", "vber", "Meer", "hie\u00b7her", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "ADV", "VVFIN", "ADV", "ADJD", "APPR", "NN", "PAV", "$("], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Euch allen nach geb\u00fchr ehr vnd dienst zu erzaigen:", "tokens": ["Euch", "al\u00b7len", "nach", "ge\u00b7b\u00fchr", "ehr", "vnd", "dienst", "zu", "er\u00b7zai\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "APPR", "ADJA", "NN", "KON", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Venus ist fro vor Euch (als jhrer herschung ehr)", "tokens": ["Ve\u00b7nus", "ist", "fro", "vor", "Euch", "(", "als", "jhrer", "her\u00b7schung", "ehr", ")"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ADJD", "APPR", "PPER", "$(", "KOUS", "PPOSAT", "NN", "NN", "$("], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Geb\u00fchrlich sich zu naigen.", "tokens": ["Ge\u00b7b\u00fchr\u00b7lich", "sich", "zu", "nai\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PRF", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Wolan so lasset nu mich Ewrer augen glantz/", "tokens": ["Wo\u00b7lan", "so", "las\u00b7set", "nu", "mich", "Ew\u00b7rer", "au\u00b7gen", "glantz", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "ADV", "PPER", "PPOSAT", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vil klarer dan die Sonn/ inbr\u00fcnstiglich erquicken!", "tokens": ["Vil", "kla\u00b7rer", "dan", "die", "Sonn", "/", "in\u00b7br\u00fcns\u00b7tig\u00b7lich", "er\u00b7qui\u00b7cken", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "ART", "NN", "$(", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Nur ein liebreicher blick kan mit dem Lorb\u00f6r-Krantz", "tokens": ["Nur", "ein", "lieb\u00b7rei\u00b7cher", "blick", "kan", "mit", "dem", "Lor\u00b7b\u00f6r\u00b7Krantz"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN", "VMFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mich alsobald beglicken.", "tokens": ["Mich", "al\u00b7so\u00b7bald", "be\u00b7gli\u00b7cken", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}