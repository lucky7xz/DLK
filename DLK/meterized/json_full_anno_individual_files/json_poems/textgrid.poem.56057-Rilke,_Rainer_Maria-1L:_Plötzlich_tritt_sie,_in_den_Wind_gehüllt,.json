{"textgrid.poem.56057": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "1L: Pl\u00f6tzlich tritt sie, in den Wind geh\u00fcllt,", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Pl\u00f6tzlich tritt sie, in den Wind geh\u00fcllt,", "tokens": ["Pl\u00f6tz\u00b7lich", "tritt", "sie", ",", "in", "den", "Wind", "ge\u00b7h\u00fcllt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "$,", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "licht in Lichtes, wie herausgegriffen,", "tokens": ["licht", "in", "Lich\u00b7tes", ",", "wie", "her\u00b7aus\u00b7ge\u00b7grif\u00b7fen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "$,", "PWAV", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "w\u00e4hrend jetzt die Stube wie geschliffen", "tokens": ["w\u00e4h\u00b7rend", "jetzt", "die", "Stu\u00b7be", "wie", "ge\u00b7schlif\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "NN", "KOKOM", "VVPP"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "hinter ihr die T\u00fcre f\u00fcllt", "tokens": ["hin\u00b7ter", "ihr", "die", "T\u00fc\u00b7re", "f\u00fcllt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "ART", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "dunkel wie der Grund einer Kamee,", "tokens": ["dun\u00b7kel", "wie", "der", "Grund", "ei\u00b7ner", "Ka\u00b7mee", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "ART", "NN", "ART", "NN", "$,"], "meter":