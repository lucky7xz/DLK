{"dta.poem.998": {"metadata": {"author": {"name": "George, Stefan", "birth": "N.A.", "death": "N.A."}, "title": "T RAURIGE   T \u00c4NZE", "genre": "Lyrik", "period": "N.A.", "pub_year": "1897", "urn": "urn:nbn:de:kobv:b4-200905191530", "language": ["de:0.99"], "booktitle": "George, Stefan: Das Jahr der Seele. Berlin, 1897."}, "poem": {"stanza.1": {"line.1": {"text": "Die wachen auen lockten wonnesam               ", "tokens": ["Die", "wa\u00b7chen", "au\u00b7en", "lock\u00b7ten", "won\u00b7ne\u00b7sam"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Im veilchenteppich kam sie an das gitter               ", "tokens": ["Im", "veil\u00b7chen\u00b7tep\u00b7pich", "kam", "sie", "an", "das", "git\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJD", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Geschm&#252;ckt wie j&#228;hrig f&#252;r den br&#228;utigam               ", "tokens": ["Ge\u00b7schm", "&#252;", "ckt", "wie", "j", "&#228;", "hrig", "f", "&#252;", "r", "den", "br", "&#228;", "u\u00b7ti\u00b7gam"], "token_info": ["word", "XML_entity", "word", "word", "word", "XML_entity", "word", "word", "XML_entity", "word", "word", "word", "XML_entity", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "-+--+--+--+-+", "measure": "amphibrach.tetra.plus"}, "line.4": {"text": "Und dachte sein bis nach dem fest der schnitter             ", "tokens": ["Und", "dach\u00b7te", "sein", "bis", "nach", "dem", "fest", "der", "schnit\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "ADV", "APPR", "ART", "NN", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Nur eine lerche die im haine schlug               ", "tokens": ["Nur", "ei\u00b7ne", "ler\u00b7che", "die", "im", "hai\u00b7ne", "schlug"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "VVFIN", "ART", "APPRART", "ADJA", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Bemerkte ihr err\u00f6ten und erschrecken               ", "tokens": ["Be\u00b7merk\u00b7te", "ihr", "er\u00b7r\u00f6\u00b7ten", "und", "er\u00b7schre\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "VVINF", "KON", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und wie in sommer-langer tage zug               ", "tokens": ["Und", "wie", "in", "som\u00b7mer\u00b7lan\u00b7ger", "ta\u00b7ge", "zug"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "APPR", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Sie sann und welkte bei den eiben-hecken             ", "tokens": ["Sie", "sann", "und", "welk\u00b7te", "bei", "den", "ei\u00b7ben\u00b7he\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Von ihrer schlanken anmut spricht allein               ", "tokens": ["Von", "ih\u00b7rer", "schlan\u00b7ken", "an\u00b7mut", "spricht", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Bei perlen-schn&#252;ren eine seidne locke               ", "tokens": ["Bei", "per\u00b7len\u00b7schn", "&#252;", "ren", "ei\u00b7ne", "seid\u00b7ne", "lo\u00b7cke"], "token_info": ["word", "word", "XML_entity", "word", "word", "word", "word"], "pos": ["APPR", "NE", "$(", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die eine fromme freundin birgt im schrein               ", "tokens": ["Die", "ei\u00b7ne", "from\u00b7me", "freun\u00b7din", "birgt", "im", "schrein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ART", "ADJA", "NN", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und schlichtes gras mit einem marmorblocke.             ", "tokens": ["Und", "schlich\u00b7tes", "gras", "mit", "ei\u00b7nem", "mar\u00b7mor\u00b7blo\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}