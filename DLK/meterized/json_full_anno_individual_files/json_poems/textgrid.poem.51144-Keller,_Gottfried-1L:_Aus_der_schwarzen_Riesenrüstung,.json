{"textgrid.poem.51144": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1L: Aus der schwarzen Riesenr\u00fcstung,", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Aus der schwarzen Riesenr\u00fcstung,", "tokens": ["Aus", "der", "schwar\u00b7zen", "Rie\u00b7sen\u00b7r\u00fcs\u00b7tung", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lehnend an der Fensterbr\u00fcstung,", "tokens": ["Leh\u00b7nend", "an", "der", "Fens\u00b7ter\u00b7br\u00fcs\u00b7tung", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Scheint mir mit verwesungsgrauen", "tokens": ["Scheint", "mir", "mit", "ver\u00b7we\u00b7sungs\u00b7grau\u00b7en"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Z\u00fcgen ein Gespenst zu schauen.", "tokens": ["Z\u00fc\u00b7gen", "ein", "Ge\u00b7spenst", "zu", "schau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Frecher Hohn gl\u00fcht aus den Augen,", "tokens": ["Fre\u00b7cher", "Hohn", "gl\u00fcht", "aus", "den", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die nur Gott zu kr\u00e4nken taugen,", "tokens": ["Die", "nur", "Gott", "zu", "kr\u00e4n\u00b7ken", "tau\u00b7gen", ","], "toke