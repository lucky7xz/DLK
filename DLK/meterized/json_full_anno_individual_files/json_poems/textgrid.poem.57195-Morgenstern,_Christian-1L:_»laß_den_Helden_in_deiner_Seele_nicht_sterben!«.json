{"textgrid.poem.57195": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: \u00bbla\u00df den Helden in deiner Seele nicht sterben!\u00ab", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbla\u00df den Helden in deiner Seele nicht sterben!\u00ab", "tokens": ["\u00bb", "la\u00df", "den", "Hel\u00b7den", "in", "dei\u00b7ner", "See\u00b7le", "nicht", "ster\u00b7ben", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVIMP", "ART", "NN", "APPR", "PPOSAT", "NN", "PTKNEG", "VVINF", "$.", "$("], "meter": "+-+--+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Welkst du hin wie die Blume, der Baum im Herbst, \u2013", "tokens": ["Welkst", "du", "hin", "wie", "die", "Blu\u00b7me", ",", "der", "Baum", "im", "Herbst", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "PPER", "ADV", "KOKOM", "ART", "NN", "$,", "ART", "NN", "APPRART", "NN", "$,", "$("], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "h\u00f6re nimmer doch auf, um den Kranz zu werben!", "tokens": ["h\u00f6\u00b7re", "nim\u00b7mer", "doch", "auf", ",", "um", "den", "Kranz", "zu", "wer\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "PTKVZ", "$,", "KOUI", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}}, "stanza.2": {"line.1": {"text": "Alle andern Kr\u00e4nze bleiben zur\u00fccke,", "tokens": ["Al\u00b7le", "an\u00b7dern", "Kr\u00e4n\u00b7ze", "blei\u00b7ben", "zu\u00b7r\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVINF", "VVFIN", "$,"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "schwinden hin wie die Glieder, die sie bedecken ...", "tokens": ["schwin\u00b7den", "hin", "wie", "die", "Glie\u00b7der", ",", "die", "sie", "be\u00b7de\u00b7cken", "..."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "KOKOM", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$("], "meter": "+-+--+--+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Dieser bleibt dir allein auf der gro\u00dfen Br\u00fccke \u2013", "tokens": ["Die\u00b7ser", "bleibt", "dir", "al\u00b7lein", "auf", "der", "gro\u00b7\u00dfen", "Br\u00fc\u00b7cke", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "APPR", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}}, "stanza.3": {"line.1": {"text": "h\u00e4lt dir droben die Geisterstirn noch umschlossen;", "tokens": ["h\u00e4lt", "dir", "dro\u00b7ben", "die", "Geis\u00b7ters\u00b7tirn", "noch", "um\u00b7schlos\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "ADV", "VVINF", "$."], "meter": "+-+--+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "und dereinst, wenn du wieder hinabgestiegen,", "tokens": ["und", "de\u00b7reinst", ",", "wenn", "du", "wie\u00b7der", "hin\u00b7ab\u00b7ge\u00b7stie\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "KOUS", "PPER", "ADV", "VVINF", "$,"], "meter": "--+--+--+-+-", "measure": "anapaest.tri.plus"}, "line.3": {"text": "wirst du gehn, wie von heiligem Schein umflossen.", "tokens": ["wirst", "du", "gehn", ",", "wie", "von", "hei\u00b7li\u00b7gem", "Schein", "um\u00b7flos\u00b7se