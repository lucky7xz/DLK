{"dta.poem.14439": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Jugendandacht .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Nun ziehen Nebel, falbe Bl\u00e4tter fallen,", "tokens": ["Nun", "zie\u00b7hen", "Ne\u00b7bel", ",", "fal\u00b7be", "Bl\u00e4t\u00b7ter", "fal\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$,", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Oed' alle Stellen, die uns oft entz\u00fccket!", "tokens": ["O\u00b7ed'", "al\u00b7le", "Stel\u00b7len", ",", "die", "uns", "oft", "ent\u00b7z\u00fc\u00b7cket", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "$,", "PRELS", "PPER", "ADV", "VVFIN", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und noch einmal tief' R\u00fchrung uns begl\u00fccket,", "tokens": ["Und", "noch", "ein\u00b7mal", "tie\u00b7f'", "R\u00fch\u00b7rung", "uns", "be\u00b7gl\u00fc\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.4": {"text": "Wie aus der Flucht die Abschiedslieder schallen.", "tokens": ["Wie", "aus", "der", "Flucht", "die", "Ab\u00b7schieds\u00b7lie\u00b7der", "schal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}