{"dta.poem.18674": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang von", "birth": "N.A.", "death": "N.A."}, "title": "Fetwa .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1819", "urn": "urn:nbn:de:kobv:b4-200905191692", "language": ["de:0.99"], "booktitle": "Goethe, Johann Wolfgang von: West-\u00f6stlicher Divan. Stuttgart, 1819."}, "poem": {"stanza.1": {"line.1": {"text": "Der Mufti las des ", "tokens": ["Der", "Muf\u00b7ti", "las", "des"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Eins nach dem andern, alle zusammen,", "tokens": ["Eins", "nach", "dem", "an\u00b7dern", ",", "al\u00b7le", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "$,", "PIS", "PTKVZ", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und wohlbed\u00e4chtig warf sie in die Flammen,", "tokens": ["Und", "wohl\u00b7be\u00b7d\u00e4ch\u00b7tig", "warf", "sie", "in", "die", "Flam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das sch\u00f6ngeschriebne Buch es ging zu nichte.", "tokens": ["Das", "sch\u00f6n\u00b7ge\u00b7schrieb\u00b7ne", "Buch", "es", "ging", "zu", "nich\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "VVFIN", "APPR", "PIS", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Verbrannt sey jeder, sprach der hohe Richter,", "tokens": ["Ver\u00b7brannt", "sey", "je\u00b7der", ",", "sprach", "der", "ho\u00b7he", "Rich\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PIS", "$,", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wer spricht und glaubt wie ", "tokens": ["Wer", "spricht", "und", "glaubt", "wie"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "KON", "VVFIN", "KOKOM"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Sey ausgenommen von des Feuers Pein:", "tokens": ["Sey", "aus\u00b7ge\u00b7nom\u00b7men", "von", "des", "Feu\u00b7ers", "Pein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Denn Allah gab die Gabe jedem Dichter.", "tokens": ["Denn", "Al\u00b7lah", "gab", "die", "Ga\u00b7be", "je\u00b7dem", "Dich\u00b7ter."], "token_info": ["word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["KON", "NN", "VVFIN", "ART", "NN", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Misbraucht er sie im Wandel seiner S\u00fcnden,", "tokens": ["Mis\u00b7braucht", "er", "sie", "im", "Wan\u00b7del", "sei\u00b7ner", "S\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "APPRART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "So seh\u2019 er zu mit Gott sich abzufinden.", "tokens": ["So", "seh'", "er", "zu", "mit", "Gott", "sich", "ab\u00b7zu\u00b7fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "APPR", "NN", "PRF", "VVIZU", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}