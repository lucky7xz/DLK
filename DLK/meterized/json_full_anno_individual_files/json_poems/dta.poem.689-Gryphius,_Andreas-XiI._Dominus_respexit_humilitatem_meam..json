{"dta.poem.689": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "XiI.  \n Dominus respexit humilitatem meam.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Noch hab' ich den tag erlebet! ", "tokens": ["Noch", "hab'", "ich", "den", "tag", "er\u00b7le\u00b7bet", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Den so offt gew\u00fcnschten tag!", "tokens": ["Den", "so", "offt", "ge\u00b7w\u00fcnschten", "tag", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "An dem der/ der oben schwebet/", "tokens": ["An", "dem", "der", "/", "der", "o\u00b7ben", "schwe\u00b7bet", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "$(", "ART", "ADV", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Der die ", "tokens": ["Der", "die"], "token_info": ["word", "word"], "pos": ["ART", "ART"], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Hat meiner schmertzen strenge macht", "tokens": ["Hat", "mei\u00b7ner", "schmert\u00b7zen", "stren\u00b7ge", "macht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPOSAT", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Vnd seinen wehrten eydt bedacht!", "tokens": ["Vnd", "sei\u00b7nen", "wehr\u00b7ten", "eydt", "be\u00b7dacht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Nun hat er mir was ich begehret;", "tokens": ["Nun", "hat", "er", "mir", "was", "ich", "be\u00b7ge\u00b7hret", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "PWS", "PPER", "VVFIN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.8": {"text": "Vom Himmel vnversehns gewehret.", "tokens": ["Vom", "Him\u00b7mel", "vn\u00b7ver\u00b7sehns", "ge\u00b7weh\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "2. Alles ", "tokens": ["Al\u00b7les"], "token_info": ["word"], "pos": ["PIS"], "meter": "+-", "measure": "trochaic.single"}, "line.2": {"text": "Kein erretten war zu sehn/ ", "tokens": ["Kein", "er\u00b7ret\u00b7ten", "war", "zu", "sehn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "VAFIN", "PTKZU", "VVINF", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mittel wahren nicht zu finden; ", "tokens": ["Mit\u00b7tel", "wah\u00b7ren", "nicht", "zu", "fin\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ja es war vmb ", "tokens": ["Ja", "es", "war", "vmb"], "token_info": ["word", "word", "word", "word"], "pos": ["PTKANT", "PPER", "VAFIN", "APPR"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Der vngewitter grimme noth/ ", "tokens": ["Der", "vn\u00b7ge\u00b7wit\u00b7ter", "grim\u00b7me", "noth", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Der auff mich au\u00dfger\u00fcste todt/", "tokens": ["Der", "auff", "mich", "au\u00df\u00b7ge\u00b7r\u00fcs\u00b7te", "todt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPER", "VVFIN", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des donners Schwefel-lichte flammen", "tokens": ["Des", "don\u00b7ners", "Schwe\u00b7fel\u00b7lich\u00b7te", "flam\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die schlugen vber mir zusammen.", "tokens": ["Die", "schlu\u00b7gen", "vber", "mir", "zu\u00b7sam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "3. Wie ein Schiffer bebt vnd zaget/", "tokens": ["Wie", "ein", "Schif\u00b7fer", "bebt", "vnd", "za\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "KON", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn die vmbgekehrte ", "tokens": ["Wenn", "die", "vmb\u00b7ge\u00b7kehr\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Sich bi\u00df an die Sterne waget/", "tokens": ["Sich", "bi\u00df", "an", "die", "Ster\u00b7ne", "wa\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Vnd den grundt spr\u00fctzt in die h\u00f6h/", "tokens": ["Vnd", "den", "grundt", "spr\u00fctzt", "in", "die", "h\u00f6h", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "ART", "ADJD", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wenn sich das schwache Schiff fast trennt;", "tokens": ["Wenn", "sich", "das", "schwa\u00b7che", "Schiff", "fast", "trennt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "ADJA", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Vnd vber klip auff klippen rennt;", "tokens": ["Vnd", "vber", "klip", "auff", "klip\u00b7pen", "rennt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NE", "APPR", "VVINF", "VVFIN", "$."], "meter": "-++-+-+", "measure": "unknown.measure.tetra"}, "line.7": {"text": "Wenn nun die seiten bretter knacken/ ", "tokens": ["Wenn", "nun", "die", "sei\u00b7ten", "bret\u00b7ter", "kna\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "ADJA", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Wenn er den Mast selbst vmb mu\u00df hacken: ", "tokens": ["Wenn", "er", "den", "Mast", "selbst", "vmb", "mu\u00df", "ha\u00b7cken", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "APPR", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "4. Wenn er West vnd Ost verlohren:", "tokens": ["Wenn", "er", "West", "vnd", "Ost", "ver\u00b7loh\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NE", "KON", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Vnd schier keinen windt mehr kennt", "tokens": ["Vnd", "schier", "kei\u00b7nen", "windt", "mehr", "kennt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "PIAT", "VVFIN", "ADV", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Vnd die sich auff jhn verschworen/", "tokens": ["Vnd", "die", "sich", "auff", "jhn", "ver\u00b7schwo\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRELS", "PRF", "APPR", "PPER", "VVINF", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Mit nicht rechten namen nennt:", "tokens": ["Mit", "nicht", "rech\u00b7ten", "na\u00b7men", "nennt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PTKNEG", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wenn jhm die Na", "tokens": ["Wenn", "jhm", "die", "Na"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN"], "meter": "++-+", "measure": "iambic.di"}, "line.6": {"text": "Wenn jhn das brausen \u00fcberstimbt? ", "tokens": ["Wenn", "jhn", "das", "brau\u00b7sen", "\u00fc\u00b7bers\u00b7timbt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PDS", "VVFIN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wenn er nunmehr nicht kan entgehen;", "tokens": ["Wenn", "er", "nun\u00b7mehr", "nicht", "kan", "ent\u00b7ge\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PTKNEG", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Vnd schon den Todt siht vor sich stehen:", "tokens": ["Vnd", "schon", "den", "Todt", "siht", "vor", "sich", "ste\u00b7hen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VVFIN", "APPR", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "5. Eben so war mir zu mute:", "tokens": ["E\u00b7ben", "so", "war", "mir", "zu", "mu\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "PTKZU", "VVFIN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Eben so war ich erstarrt:", "tokens": ["E\u00b7ben", "so", "war", "ich", "er\u00b7starrt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "VVPP", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Ach/ dacht ich/ ist di\u00df das gute", "tokens": ["Ach", "/", "dacht", "ich", "/", "ist", "di\u00df", "das", "gu\u00b7te"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$(", "VVFIN", "PPER", "$(", "VAFIN", "PDS", "ART", "ADJA"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Drauf ich jederzeit geharrt/", "tokens": ["Drauf", "ich", "je\u00b7der\u00b7zeit", "ge\u00b7harrt", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "ADV", "VVPP", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Mein hertz erstarb/ mein mundt erblich/ ", "tokens": ["Mein", "hertz", "er\u00b7starb", "/", "mein", "mundt", "er\u00b7blich", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$(", "PPOSAT", "ADJD", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die kraft verging/ der Geist entwich/ ", "tokens": ["Die", "kraft", "ver\u00b7ging", "/", "der", "Geist", "ent\u00b7wich", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$(", "ART", "NN", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der mund erstum\u0303t; die Augen stunden:", "tokens": ["Der", "mund", "er\u00b7stum\u0303t", ";", "die", "Au\u00b7gen", "stun\u00b7den", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Die f\u00fc\u00dfe waren als gebunden.", "tokens": ["Die", "f\u00fc\u00b7\u00dfe", "wa\u00b7ren", "als", "ge\u00b7bun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VAFIN", "KOKOM", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "6. Kein trost m\u00f6chte mich erquicken:", "tokens": ["Kein", "trost", "m\u00f6ch\u00b7te", "mich", "er\u00b7qui\u00b7cken", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "PRF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Aller anspruch ward zur pein:", "tokens": ["Al\u00b7ler", "an\u00b7spruch", "ward", "zur", "pein", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Weil die b\u00fcrd\u2019 auff meinem r\u00fccken:", "tokens": ["Weil", "die", "b\u00fcrd'", "auff", "mei\u00b7nem", "r\u00fc\u00b7cken", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPR", "PPOSAT", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Weil mich dieser schwere Stein/ ", "tokens": ["Weil", "mich", "die\u00b7ser", "schwe\u00b7re", "Stein", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PDAT", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Bi\u00df auff die Erden niederbog: ", "tokens": ["Bi\u00df", "