{"textgrid.poem.38205": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Vision", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ueber den Kirchhof gieng ich allein,", "tokens": ["Ue\u00b7ber", "den", "Kirch\u00b7hof", "gieng", "ich", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Zu meines Liebchens K\u00e4mmerlein,", "tokens": ["Zu", "mei\u00b7nes", "Lieb\u00b7chens", "K\u00e4m\u00b7mer\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und als ich wollt von dannen gehn,", "tokens": ["Und", "als", "ich", "wollt", "von", "dan\u00b7nen", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VMFIN", "APPR", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da hielt es mich, ich mu\u00dft da stehn.", "tokens": ["Da", "hielt", "es", "mich", ",", "ich", "mu\u00dft", "da", "stehn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "$,", "PPER", "VMFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ein Seel stand traurig an eim Grab,", "tokens": ["Ein", "Seel", "stand", "trau\u00b7rig", "an", "eim", "Grab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und schrie mit heller Stimm hinab,", "tokens": ["Und", "schrie", "mit", "hel\u00b7ler", "Stimm", "hin\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u00bbsteh auf mein Leib, verantwort dich,", "tokens": ["\u00bb", "steh", "auf", "mein", "Leib", ",", "ver\u00b7ant\u00b7wort", "dich", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "VVFIN", "APPR", "PPOSAT", "NN", "$,", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dann ich bin hier, beschuldge dich.\u00ab", "tokens": ["Dann", "ich", "bin", "hier", ",", "be\u00b7schuld\u00b7ge", "dich", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["ADV", "PPER", "VAFIN", "ADV", "$,", "VVFIN", "PPER", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Da hebet sich des Grabes Stein,", "tokens": ["Da", "he\u00b7bet", "sich", "des", "Gra\u00b7bes", "Stein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und geht hervor ein wei\u00df Gebein,", "tokens": ["Und", "geht", "her\u00b7vor", "ein", "wei\u00df", "Ge\u00b7bein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKVZ", "PTKVZ", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Leib steht auf gar bald und schnell,", "tokens": ["Der", "Leib", "steht", "auf", "gar", "bald", "und", "schnell", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADV", "ADV", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und geht dahin, spricht zu der Seel:", "tokens": ["Und", "geht", "da\u00b7hin", ",", "spricht", "zu", "der", "Seel", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PAV", "$,", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.4": {"line.1": {"text": "\u00bbwer ist daraus, der mein begehrt,", "tokens": ["\u00bb", "wer", "ist", "da\u00b7raus", ",", "der", "mein", "be\u00b7gehrt", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VAFIN", "PAV", "$,", "PRELS", "PPOSAT", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der mich da rufet aus der Erd,", "tokens": ["Der", "mich", "da", "ru\u00b7fet", "aus", "der", "Erd", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bist du es Seele, die vor Jahren", "tokens": ["Bist", "du", "es", "See\u00b7le", ",", "die", "vor", "Jah\u00b7ren"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "PPER", "PPER", "NN", "$,", "PRELS", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Aus meinem Leibe ist gefahren?\u00ab", "tokens": ["Aus", "mei\u00b7nem", "Lei\u00b7be", "ist", "ge\u00b7fah\u00b7ren", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "VVPP", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Die Seel sprach: \u00bbHab ich beten w\u00f6llen,", "tokens": ["Die", "Seel", "sprach", ":", "\u00bb", "Hab", "ich", "be\u00b7ten", "w\u00f6l\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "$(", "NN", "PPER", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da pflegtest du dich krank zu stellen,", "tokens": ["Da", "pfleg\u00b7test", "du", "dich", "krank", "zu", "stel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn ich anfieng das Abendgebet,", "tokens": ["Wenn", "ich", "an\u00b7fi\u00b7eng", "das", "A\u00b7bend\u00b7ge\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Da hast du dich gleich schlafen gelegt.\u00ab", "tokens": ["Da", "hast", "du", "dich", "gleich", "schla\u00b7fen", "ge\u00b7legt", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PRF", "ADV", "VVINF", "VVPP", "$.", "$("], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.6": {"line.1": {"text": "Der Leib sprach: \u00bbAch ich schien nur faul,", "tokens": ["Der", "Leib", "sprach", ":", "\u00bb", "Ach", "ich", "schien", "nur", "faul", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "$(", "ITJ", "PPER", "VVFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und g\u00e4hnte, macht ein schiefes Maul,", "tokens": ["Und", "g\u00e4hn\u00b7te", ",", "macht", "ein", "schie\u00b7fes", "Maul", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und war zum niederknien verdrossen,", "tokens": ["Und", "war", "zum", "nie\u00b7der\u00b7kni\u00b7en", "ver\u00b7dros\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPRART", "PIS", "VVINF", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Denn ich hatt einen Bettgenossen.\u00ab", "tokens": ["Denn", "ich", "hatt", "ei\u00b7nen", "Bett\u00b7ge\u00b7nos\u00b7sen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPER", "VAFIN", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "\u00bbach weh! Ach weh, antwort die Seel,", "tokens": ["\u00bb", "ach", "weh", "!", "Ach", "weh", ",", "ant\u00b7wort", "die", "Seel", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "PTKVZ", "$.", "NN", "PTKVZ", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Da\u00df ich gewesen dein Gesell,", "tokens": ["Da\u00df", "ich", "ge\u00b7we\u00b7sen", "dein", "Ge\u00b7sell", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VAPP", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wovon die Ursach du allein", "tokens": ["Wo\u00b7von", "die", "Ur\u00b7sach", "du", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Darum leid ich die H\u00f6llenpein.", "tokens": ["Da\u00b7rum", "leid", "ich", "die", "H\u00f6l\u00b7len\u00b7pein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.8": {"line.1": {"text": "Im Thal Josaphat am J\u00fcngsten Tag,", "tokens": ["Im", "Thal", "Jo\u00b7sa\u00b7phat", "am", "J\u00fcng\u00b7sten", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NE", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Da will ich f\u00fchren grosse Klag,", "tokens": ["Da", "will", "ich", "f\u00fch\u00b7ren", "gros\u00b7se", "Klag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVINF", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Alsdann wird angehn auch dein Leid,", "tokens": ["Als\u00b7dann", "wird", "an\u00b7gehn", "auch", "dein", "Leid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "VVPP", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Du wirst brennen in Ewigkeit.\u00ab", "tokens": ["Du", "wirst", "bren\u00b7nen", "in", "E\u00b7wig\u00b7keit", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "VVINF", "APPR", "NN", "$.", "$("], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.9": {"line.1": {"text": "Da sprach der Leib: \u00bbDu seyst verklagt,", "tokens": ["Da", "sprach", "der", "Leib", ":", "\u00bb", "Du", "seyst", "ver\u00b7klagt", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$.", "$(", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du warst die Frau, und ich die Magd,", "tokens": ["Du", "warst", "die", "Frau", ",", "und", "ich", "die", "Magd", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "KON", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du tr\u00e4gst mit mir die S\u00fcndenlast,", "tokens": ["Du", "tr\u00e4gst", "mit", "mir", "die", "S\u00fcn\u00b7den\u00b7last", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Weil du mich b\u00f6s gef\u00fchret hast.\u00ab", "tokens": ["Weil", "du", "mich", "b\u00f6s", "ge\u00b7f\u00fch\u00b7ret", "hast", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADJD", "VVPP", "VAFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Die Seel wollt da noch widersprechen,", "tokens": ["Die", "Seel", "wollt", "da", "noch", "wi\u00b7der\u00b7spre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da th\u00e4t der Morgenstern anbrechen,", "tokens": ["Da", "th\u00e4t", "der", "Mor\u00b7gens\u00b7tern", "an\u00b7bre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+---+-", "measure": "unknown.measure.tri"}, "line.3": {"text": "Sankt Petrus Vogel th\u00e4t auch kr\u00e4hen,", "tokens": ["Sankt", "Pet\u00b7rus", "Vo\u00b7gel", "th\u00e4t", "auch", "kr\u00e4\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "NE", "VVFIN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da waren beid nicht mehr zu sehn.", "tokens": ["Da", "wa\u00b7ren", "beid", "nicht", "mehr", "zu", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "PTKNEG", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Ich aber schrieb dies Liedelein,", "tokens": ["Ich", "a\u00b7ber", "schrieb", "dies", "Lie\u00b7de\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PDS", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und steckts an Liebchens Fensterlein,", "tokens": ["Und", "steckts", "an", "Lieb\u00b7chens", "Fens\u00b7ter\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u00bbich war mit Leib und Seel zu Gast,", "tokens": ["\u00bb", "ich", "war", "mit", "Leib", "und", "Seel", "zu", "Gast", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "APPR", "NN", "KON", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "'s ist mir leid, wenn du auf mich gewartet hast.\u00ab", "tokens": ["'s", "ist", "mir", "leid", ",", "wenn", "du", "auf", "mich", "ge\u00b7war\u00b7tet", "hast", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADJD", "$,", "KOUS", "PPER", "APPR", "PPER", "VVPP", "VAFIN", "$.", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.12": {"line.1": {"text": "Ueber den Kirchhof gieng ich allein,", "tokens": ["Ue\u00b7ber", "den", "Kirch\u00b7hof", "gieng", "ich", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "