{"dta.poem.11148": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Die himmlische Clarille.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der himmel wird bewegt, die erde stehet stille:", "tokens": ["Der", "him\u00b7mel", "wird", "be\u00b7wegt", ",", "die", "er\u00b7de", "ste\u00b7het", "stil\u00b7le", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,", "ART", "NN", "VVFIN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So glaubt man insgemein, so glaubet auch Clarille;", "tokens": ["So", "glaubt", "man", "ins\u00b7ge\u00b7mein", ",", "so", "glau\u00b7bet", "auch", "Cla\u00b7ril\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ADV", "$,", "ADV", "VVFIN", "ADV", "NE", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Denn ihre wanckelmuth bezeuget f\u00fcr und f\u00fcr,", "tokens": ["Denn", "ih\u00b7re", "wan\u00b7ckel\u00b7muth", "be\u00b7zeu\u00b7get", "f\u00fcr", "und", "f\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "APPR", "KON", "APPR", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Es sey viel himmlisches, nichts irrdisches an ihr.", "tokens": ["Es", "sey", "viel", "himm\u00b7li\u00b7sches", ",", "nichts", "irr\u00b7di\u00b7sches", "an", "ihr", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "ADJA", "$,", "PIS", "ADJA", "APPR", "PPER", "$."], "meter":