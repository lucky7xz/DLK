{"dta.poem.20381": {"metadata": {"author": {"name": "Hofmann von Hofmannswaldau, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Uber das kupffer-bild eines f\u00fcrneh-  \n men und gelehrten mannes.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1695", "urn": "urn:nbn:de:kobv:b4-200905197751", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Zurnt nicht/ ihr Gratien/ da\u00df dieser mund die krafft/", "tokens": ["Zurnt", "nicht", "/", "ihr", "Gra\u00b7ti\u00b7en", "/", "da\u00df", "die\u00b7ser", "mund", "die", "krafft", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "$(", "PPOSAT", "NN", "$(", "KOUS", "PDAT", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die euch allein geb\u00fchrt/ so v\u00f6llig eingesogen.", "tokens": ["Die", "euch", "al\u00b7lein", "ge\u00b7b\u00fchrt", "/", "so", "v\u00f6l\u00b7lig", "ein\u00b7ge\u00b7so\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VVPP", "$(", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ihr Musen z\u00fcrnet nicht/ da\u00df alle wissenschafft", "tokens": ["Ihr", "Mu\u00b7sen", "z\u00fcr\u00b7net", "nicht", "/", "da\u00df", "al\u00b7le", "wis\u00b7sen\u00b7schafft"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "PTKNEG", "$(", "KOUS", "PIS", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Von euren bergen ist in diesen kopff geflogen.", "tokens": ["Von", "eu\u00b7ren", "ber\u00b7gen", "ist", "in", "die\u00b7sen", "kopff", "ge\u00b7flo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], 