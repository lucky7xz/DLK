{"textgrid.poem.48588": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "20. Auf Herrn M. Niklas von H\u00f6veln und Jungfrauen Elisabeth Niehusens ihre Hochzeit, 1638 Januar 8.", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sagts nun \u00f6ffentlich und frei,", "tokens": ["Sagts", "nun", "\u00f6f\u00b7fent\u00b7lich", "und", "frei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.2": {"text": "Liebe sei", "tokens": ["Lie\u00b7be", "sei"], "token_info": ["word", "word"], "pos": ["NN", "VAFIN"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "eine Sucht, die an kan stecken!", "tokens": ["ei\u00b7ne", "Sucht", ",", "die", "an", "kan", "ste\u00b7cken", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sagts, sie sei ein starker Gift!", "tokens": ["Sagts", ",", "sie", "sei", "ein", "star\u00b7ker", "Gift", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wen sie trifft,", "tokens": ["Wen", "sie", "trifft", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "der mu\u00df Ander' auch beflecken.", "tokens": ["der", "mu\u00df", "An\u00b7der'", "auch", "be\u00b7fle\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "NE", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Zunder ist sie bei der Glut,", "tokens": ["Zun\u00b7der", "ist", "sie", "bei", "der", "Glut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "bei der Flut", "tokens": ["bei", "der", "Flut"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Triebsand, der die Feuchte trinket,", "tokens": ["Trie\u00b7bsand", ",", "der", "die", "Feuch\u00b7te", "trin\u00b7ket", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Unsre Jungfrau wird zur Braut,", "tokens": ["Uns\u00b7re", "Jung\u00b7frau", "wird", "zur", "Braut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "weil sie schaut,", "tokens": ["weil", "sie", "schaut", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "da\u00df ihr ihre Schwester winket.", "tokens": ["da\u00df", "ihr", "ih\u00b7re", "Schwes\u00b7ter", "win\u00b7ket", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wol denn! Die gesunde Sucht", "tokens": ["Wol", "denn", "!", "Die", "ge\u00b7sun\u00b7de", "Sucht"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADV", "$.", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "bringt die Frucht,", "tokens": ["bringt", "die", "Frucht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "die f\u00fcr diese Krankheit dienet.", "tokens": ["die", "f\u00fcr", "die\u00b7se", "Krank\u00b7heit", "die\u00b7net", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PDAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ihr seid frisch itzt, die ihr doch", "tokens": ["Ihr", "seid", "frisch", "itzt", ",", "die", "ihr", "doch"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADJD", "ADV", "$,", "PRELS", "PPER", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "neulich noch", "tokens": ["neu\u00b7lich", "noch"], "token_info": ["word", "word"], "pos": ["ADV", "ADV"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "fast wie nicht zu heilen schienet.", "tokens": ["fast", "wie", "nicht", "zu", "hei\u00b7len", "schie\u00b7net", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "PTKNEG", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Gott, der Liebstern eurer Brunst,", "tokens": ["Gott", ",", "der", "Liebs\u00b7tern", "eu\u00b7rer", "Brunst", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "gie\u00dfe Gunst", "tokens": ["gie\u00b7\u00dfe", "Gunst"], "token_info": ["word", "word"], "pos": ["VVFIN", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "in das wolbeflammte Feuer,", "tokens": ["in", "das", "wol\u00b7be\u00b7flamm\u00b7te", "Feu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "und der Himmel, der euch paart,", "tokens": ["und", "der", "Him\u00b7mel", ",", "der", "euch", "paart", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "geb' auch Art", "tokens": ["geb'", "auch", "Art"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ADV", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "aus der sch\u00f6nen Glut noch heuer!", "tokens": ["aus", "der", "sch\u00f6\u00b7nen", "Glut", "noch", "heu\u00b7er", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ADV", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Wie der trefliche Smaragd", "tokens": ["Wie", "der", "tref\u00b7li\u00b7che", "Sma\u00b7ragd"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "g\u00fcldner macht", "tokens": ["g\u00fcld\u00b7ner", "macht"], "token_info": ["word", "word"], "pos": ["ADJD", "VVFIN"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "des ber\u00fchmten Goldes Stralen,", "tokens": ["des", "be\u00b7r\u00fchm\u00b7ten", "Gol\u00b7des", "Stra\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "wie der ungeschminkte Wein", "tokens": ["wie", "der", "un\u00b7ge\u00b7schmink\u00b7te", "Wein"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "seinen Schein", "tokens": ["sei\u00b7nen", "Schein"], "token_info": ["word", "word"], "pos": ["PPOSAT", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "doppelt in Venedigs Schalen:", "tokens": ["dop\u00b7pelt", "in", "Ve\u00b7ne\u00b7digs", "Scha\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NE", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.6": {"line.1": {"text": "so verm\u00e4hlt sich Bl\u00fctt' und Frucht,", "tokens": ["so", "ver\u00b7m\u00e4hlt", "sich", "Bl\u00fctt'", "und", "Frucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Zier und Zucht,", "tokens": ["Zier", "und", "Zucht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "so steht Scham bei sch\u00f6nen Jungen.", "tokens": ["so", "steht", "Scham", "bei", "sch\u00f6\u00b7nen", "Jun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gleiches Paar an beiderlei,", "tokens": ["Glei\u00b7ches", "Paar", "an", "bei\u00b7der\u00b7lei", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "PIAT", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "saget frei,", "tokens": ["sa\u00b7get", "frei", ","], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "ADJD", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "da\u00df euch Freien ist gelungen!", "tokens": ["da\u00df", "euch", "Frei\u00b7en", "ist", "ge\u00b7lun\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Aller Erden Fruchtbarkeit", "tokens": ["Al\u00b7ler", "Er\u00b7den", "Frucht\u00b7bar\u00b7keit"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "liegt verschneit,", "tokens": ["liegt", "ver\u00b7schneit", ","], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Hornung, unter deinen F\u00fc\u00dfen:", "tokens": ["Hor\u00b7nung", ",", "un\u00b7ter", "dei\u00b7nen", "F\u00fc\u00b7\u00dfen", ":"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "ihr auch deckt euch in der Ruh'", "tokens": ["ihr", "auch", "deckt", "euch", "in", "der", "Ruh'"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "sanfte zu", "tokens": ["sanf\u00b7te", "zu"], "token_info": ["word", "word"], "pos": ["VVFIN", "APPR"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "und erw\u00e4rmet euch mit K\u00fcssen.", "tokens": ["und", "er\u00b7w\u00e4r\u00b7met", "euch", "mit", "K\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "See und Luft und Land wird bald", "tokens": ["See", "und", "Luft", "und", "Land", "wird", "bald"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "KON", "NN", "VAFIN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "mit Gewalt", "tokens": ["mit", "Ge\u00b7walt"], "token_info": ["word", "word"], "pos": ["APPR", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "zur Geburt und Mehrung eilen;", "tokens": ["zur", "Ge\u00b7burt", "und", "Meh\u00b7rung", "ei\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "schauet zu, da\u00df ihr euch denn,", "tokens": ["schau\u00b7et", "zu", ",", "da\u00df", "ihr", "euch", "denn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "KOUS", "PPER", "PPER", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "ihr wi\u00dft, wenn,", "tokens": ["ihr", "wi\u00dft", ",", "wenn", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "nicht zu lange m\u00f6gt verweilen!", "tokens": ["nicht", "zu", "lan\u00b7ge", "m\u00f6gt", "ver\u00b7wei\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PTKA", "ADV", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "H\u00f6velt euer neues Haus,", "tokens": ["H\u00f6\u00b7velt", "eu\u00b7er", "neu\u00b7es", "Haus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Br\u00e4utgam, aus!", "tokens": ["Br\u00e4ut\u00b7gam", ",", "aus", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$,", "PTKVZ", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Malet, wie sie vor wird rei\u00dfen,", "tokens": ["Ma\u00b7let", ",", "wie", "sie", "vor", "wird", "rei\u00b7\u00dfen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "PPER", "APPR", "VAFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "da\u00df die itzt noch Junge-Frau", "tokens": ["da\u00df", "die", "itzt", "noch", "Jun\u00b7ge\u00b7Frau"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADV", "ADV", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "h\u00e4lt f\u00fcr rau,", "tokens": ["h\u00e4lt", "f\u00fcr", "rau", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJD", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "bald gar m\u00fcsse Mutter hei\u00dfen!", "tokens": ["bald", "gar", "m\u00fcs\u00b7se", "Mut\u00b7ter", "hei\u00b7\u00dfen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Sagts nun \u00f6ffentlich und frei,", "tokens": ["Sagts", "nun", "\u00f6f\u00b7fent\u00b7lich", "und", "frei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.2": {"text": "Liebe sei", "tokens": ["Lie\u00b7be", "sei"], "token_info": ["word", "word"], "pos": ["NN", "VAFIN"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "eine Sucht, die an kan stecken!", "tokens": ["ei\u00b7ne", "Sucht", ",", "die", "an", "kan", "ste\u00b7cken", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sagts, sie sei ein starker Gift!", "tokens": ["Sagts", ",", "sie", "sei", "ein", "star\u00b7ker", "Gift", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wen sie trifft,", "tokens": ["Wen", "sie", "trifft", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "der mu\u00df Ander' auch beflecken.", "tokens": ["der", "mu\u00df", "An\u00b7der'", "auch", "be\u00b7fle\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "NE", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Zunder ist sie bei der Glut,", "tokens": ["Zun\u00b7der", "ist", "sie", "bei", "der", "Glut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "bei der Flut", "tokens": ["bei", "der", "Flut"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Triebsand, der die Feuchte trinket,", "tokens": ["Trie\u00b7bsand", ",", "der", "die", "Feuch\u00b7te", "trin\u00b7ket", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Unsre Jungfrau wird zur Braut,", "tokens": ["Uns\u00b7re", "Jung\u00b7frau", "wird", "zur", "Braut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "weil sie schaut,", "tokens": ["weil", "sie", "schaut", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "da\u00df ihr ihre Schwester winket.", "tokens": ["da\u00df", "ihr", "ih\u00b7re", "Schwes\u00b7ter", "win\u00b7ket", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Wol denn! Die gesunde Sucht", "tokens": ["Wol", "denn", "!", "Die", "ge\u00b7sun\u00b7de", "Sucht"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADV", "$.", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "bringt die Frucht,", "tokens": ["bringt", "die", "Frucht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "die f\u00fcr diese Krankheit dienet.", "tokens": ["die", "f\u00fcr", "die\u00b7se", "Krank\u00b7heit", "die\u00b7net", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PDAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ihr seid frisch itzt, die ihr doch", "tokens": ["Ihr", "seid", "frisch", "itzt", ",", "die", "ihr", "doch"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADJD", "ADV", "$,", "PRELS", "PPER", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "neulich noch", "tokens": ["neu\u00b7lich", "noch"], "token_info": ["word", "word"], "pos": ["ADV", "ADV"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "fast wie nicht zu heilen schienet.", "tokens": ["fast", "wie", "nicht", "zu", "hei\u00b7len", "schie\u00b7net", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "PTKNEG", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Gott, der Liebstern eurer Brunst,", "tokens": ["Gott", ",", "der", "Liebs\u00b7tern", "eu\u00b7rer", "Brunst", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "gie\u00dfe Gunst", "tokens": ["gie\u00b7\u00dfe", "Gunst"], "token_info": ["word", "word"], "pos": ["VVFIN", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "in das wolbeflammte Feuer,", "tokens": ["in", "das", "wol\u00b7be\u00b7flamm\u00b7te", "Feu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "und der Himmel, der euch paart,", "tokens": ["und", "der", "Him\u00b7mel", ",", "der", "euch", "paart", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "geb' auch Art", "tokens": ["geb'", "auch", "Art"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ADV", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "aus der sch\u00f6nen Glut noch heuer!", "tokens": ["aus", "der", "sch\u00f6\u00b7nen", "Glut", "noch", "heu\u00b7er", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ADV", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "Wie der trefliche Smaragd", "tokens": ["Wie", "der", "tref\u00b7li\u00b7che", "Sma\u00b7ragd"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "g\u00fcldner macht", "tokens": ["g\u00fcld\u00b7ner", "macht"], 