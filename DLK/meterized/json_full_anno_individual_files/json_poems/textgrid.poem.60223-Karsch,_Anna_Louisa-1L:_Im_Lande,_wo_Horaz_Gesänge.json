{"textgrid.poem.60223": {"metadata": {"author": {"name": "Karsch, Anna Louisa", "birth": "N.A.", "death": "N.A."}, "title": "1L: Im Lande, wo Horaz Ges\u00e4nge", "genre": "verse", "period": "N.A.", "pub_year": 1762, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im Lande, wo Horaz Ges\u00e4nge", "tokens": ["Im", "Lan\u00b7de", ",", "wo", "Ho\u00b7raz", "Ge\u00b7s\u00e4n\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "PWAV", "NE", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Umher erschallen lie\u00df, wo unter gr\u00fcne G\u00e4nge", "tokens": ["Um\u00b7her", "er\u00b7schal\u00b7len", "lie\u00df", ",", "wo", "un\u00b7ter", "gr\u00fc\u00b7ne", "G\u00e4n\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVINF", "VVFIN", "$,", "PWAV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Zu jeder Jahreszeit der J\u00fcngling hoffend geht,", "tokens": ["Zu", "je\u00b7der", "Jah\u00b7res\u00b7zeit", "der", "J\u00fcng\u00b7ling", "hof\u00b7fend", "geht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ART", "NN", "VVPP", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der mit dem M\u00e4dchen sich versteht:", "tokens": ["Der", "mit", "dem", "M\u00e4d\u00b7chen", "sich", "ver\u00b7steht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "In Welschland war ein Hirtenknabe,", "tokens": ["In", "Wel\u00b7schland", "war", "ein", "Hir\u00b7ten\u00b7kna\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der niemals las, und niemals schrieb,", "tokens": ["Der", "nie\u00b7mals", "las", ",", "und", "nie\u00b7mals", "schrieb", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "$,", "KON", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und von der Kindheit an, bey stillen Schaafen blieb,", "tokens": ["Und", "von", "der", "Kind\u00b7heit", "an", ",", "bey", "stil\u00b7len", "Schaa\u00b7fen", "blieb", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "PTKVZ", "$,", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ganz unbekannt mit der in ihm verborgnen Gabe.", "tokens": ["Ganz", "un\u00b7be\u00b7kannt", "mit", "der", "in", "ihm", "ver\u00b7borg\u00b7nen", "Ga\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "ART", "APPR", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Einst stand er hingelehnt an seinem Hirtenstabe,", "tokens": ["Einst", "stand", "er", "hin\u00b7ge\u00b7lehnt", "an", "sei\u00b7nem", "Hir\u00b7ten\u00b7sta\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Da kam ein P\u00e4chter, las ihm seinen Tasso vor;", "tokens": ["Da", "kam", "ein", "P\u00e4ch\u00b7ter", ",", "las", "ihm", "sei\u00b7nen", "Tas\u00b7so", "vor", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "VVFIN", "PPER", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Der Sch\u00e4fer stand, war lauter Ohr,", "tokens": ["Der", "Sch\u00e4\u00b7fer", "stand", ",", "war", "lau\u00b7ter", "Ohr", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "$,", "VAFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Und lie\u00df das Heldenlied sich in die Seele dringen,", "tokens": ["Und", "lie\u00df", "das", "Hel\u00b7den\u00b7lied", "sich", "in", "die", "See\u00b7le", "drin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PRF", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Und fing den n\u00e4chsten Tag den Sch\u00e4fern auf der Flur", "tokens": ["Und", "fing", "den", "n\u00e4chs\u00b7ten", "Tag", "den", "Sch\u00e4\u00b7fern", "auf", "der", "Flur"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Ein neues Lied an vorzusingen.", "tokens": ["Ein", "neu\u00b7es", "Lied", "an", "vor\u00b7zu\u00b7sin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Er sang die Sch\u00f6nheit der Natur,", "tokens": ["Er", "sang", "die", "Sch\u00f6n\u00b7heit", "der", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Sang den Citronenwald, fruchtbare Feigenb\u00e4ume,", "tokens": ["Sang", "den", "Cit\u00b7ro\u00b7nen\u00b7wald", ",", "frucht\u00b7ba\u00b7re", "Fei\u00b7gen\u00b7b\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-++-+-+-+-+-", "measure": "unknown.measure.septa"}, "line.17": {"text": "Den Weinstock und ein bl\u00fchend Thal.", "tokens": ["Den", "Wein\u00b7stock", "und", "ein", "bl\u00fc\u00b7hend", "Thal", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Er z\u00e4hlte Sylben, und fand Reime,", "tokens": ["Er", "z\u00e4hl\u00b7te", "Syl\u00b7ben", ",", "und", "fand", "Rei\u00b7me", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "$,", "KON", "VVFIN", "NN", "$,"], "meter": "-+-+-+++-", "measure": "unknown.measure.penta"}, "line.19": {"text": "Ohn da\u00df ein Lehrer ihm die Wahl", "tokens": ["Ohn", "da\u00df", "ein", "Leh\u00b7rer", "ihm", "die", "Wahl"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "KOUS", "ART", "NN", "PPER", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Des sch\u00f6nen Ausdrucks wie\u00df. Die Z\u00e4rtlichkeit befahl", "tokens": ["Des", "sch\u00f6\u00b7nen", "Aus\u00b7drucks", "wie\u00df", ".", "Die", "Z\u00e4rt\u00b7lich\u00b7keit", "be\u00b7fahl"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$.", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "In ihm oft den Gesang. Er dichtete sich Tr\u00e4ume,", "tokens": ["In", "ihm", "oft", "den", "Ge\u00b7sang", ".", "Er", "dich\u00b7te\u00b7te", "sich", "Tr\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "ART", "NN", "$.", "PPER", "VVFIN", "PRF", "NN", "$,"], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.22": {"text": "Und bracht sie in das Lied, das er der Hirtin sang,", "tokens": ["Und", "bracht", "sie", "in", "das", "Lied", ",", "das", "er", "der", "Hir\u00b7tin", "sang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN", "$,", "PRELS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "So r\u00fchrend, da\u00df er sie zu seiner Liebe zwang.", "tokens": ["So", "r\u00fch\u00b7rend", ",", "da\u00df", "er", "sie", "zu", "sei\u00b7ner", "Lie\u00b7be", "zwang", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "$,", "KOUS", "PPER", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Mit jedem Tag ward ein Gesang", "tokens": ["Mit", "je\u00b7dem", "Tag", "ward", "ein", "Ge\u00b7sang"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Dem Sch\u00e4fervolk bekannt. Oft prie\u00df er in dem Liede", "tokens": ["Dem", "Sch\u00e4\u00b7fer\u00b7volk", "be\u00b7kannt", ".", "Oft", "prie\u00df", "er", "in", "dem", "Lie\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PTKVZ", "$.", "ADV", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Etruriens Gl\u00fcckseligkeit;", "tokens": ["E\u00b7tru\u00b7ri\u00b7ens", "Gl\u00fcck\u00b7se\u00b7lig\u00b7keit", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADV", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.27": {"text": "Denn eben zu derselben Zeit", "tokens": ["Denn", "e\u00b7ben", "zu", "der\u00b7sel\u00b7ben", "Zeit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "War weit umher ein tiefer Friede!", "tokens": ["War", "weit", "um\u00b7her", "ein", "tie\u00b7fer", "Frie\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "PTKVZ", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.29": {"text": "Der Ruf von diesem S\u00e4nger flog", "tokens": ["Der", "Ruf", "von", "die\u00b7sem", "S\u00e4n\u00b7ger", "flog"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PDAT", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "Bis an des Herzogs Hof. Bewunderung beweg", "tokens": ["Bis", "an", "des", "Her\u00b7zogs", "Hof", ".", "Be\u00b7wun\u00b7de\u00b7rung", "be\u00b7weg"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "APPR", "ART", "NN", "NE", "$.", "NN", "PTKVZ"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Den F\u00fcrsten, da\u00df er schnell befohlen,", "tokens": ["Den", "F\u00fcrs\u00b7ten", ",", "da\u00df", "er", "schnell", "be\u00b7foh\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.32": {"text": "Den sch\u00e4frischen Ovid in den Pallast zu holen.", "tokens": ["Den", "sch\u00e4f\u00b7ri\u00b7schen", "O\u00b7vid", "in", "den", "Pal\u00b7last", "zu", "ho\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NE", "APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+--+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.33": {"text": "Er kam in seiner Hirtentracht,", "tokens": ["Er", "kam", "in", "sei\u00b7ner", "Hir\u00b7ten\u00b7tracht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.34": {"text": "Und, wie man sagt, hat er in zweymal drei\u00dfig Tagen", "tokens": ["Und", ",", "wie", "man", "sagt", ",", "hat", "er", "in", "zwey\u00b7mal", "drei\u00b7\u00dfig", "Ta\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "$,", "PWAV", "PIS", "VVFIN", "$,", "VAFIN", "PPER", "APPR", "ADV", "CARD", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Zwey hundert Lieder ihm gemacht.", "tokens": ["Zwey", "hun\u00b7dert", "Lie\u00b7der", "ihm", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "CARD", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.36": {"text": "Doch l\u00e4nger konnt er nicht ertragen", "tokens": ["Doch", "l\u00e4n\u00b7ger", "konnt", "er", "nicht", "er\u00b7tra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VMFIN", "PPER", "PTKNEG", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.37": {"text": "Des Hofes Schmeicheley, die Falschheit unter Pracht", "tokens": ["Des", "Ho\u00b7fes", "Schmei\u00b7che\u00b7ley", ",", "die", "Falschheit", "un\u00b7ter", "Pracht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "$,", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.38": {"text": "Verdeckt, und sch\u00f6n verh\u00fcllt, wie Gift in bunter Schlange.", "tokens": ["Ver\u00b7deckt", ",", "und", "sch\u00f6n", "ver\u00b7h\u00fcllt", ",", "wie", "Gift", "in", "bun\u00b7ter", "Schlan\u00b7ge", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "KON", "ADJD", "VVPP", "$,", "PWAV", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Der Hirt, zu redlich, kam und trat", "tokens": ["Der", "Hirt", ",", "zu", "red\u00b7lich", ",", "kam", "und", "trat"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PTKA", "ADJD", "$,", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.40": {"text": "Vor seinen Herzog mit Gesange,", "tokens": ["Vor", "sei\u00b7nen", "Her\u00b7zog", "mit", "Ge\u00b7san\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "NN", "$,"], "meter": "-+-+++-+-", "measure": "unknown.measure.penta"}, "line.41": {"text": "Worin er um Erlaubni\u00df bat,", "tokens": ["Wo\u00b7rin", "er", "um", "Er\u00b7laub\u00b7ni\u00df", "bat", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.42": {"text": "Auf seine stille Trift sich wieder zu begeben.", "tokens": ["Auf", "sei\u00b7ne", "stil\u00b7le", "Trift", "sich", "wie\u00b7der", "zu", "be\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "PRF", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Herr Herzog! sang er, gieb du mir", "tokens": ["Herr", "Her\u00b7zog", "!", "sang", "er", ",", "gieb", "du", "mir"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "NE", "$.", "VVFIN", "PPER", "$,", "VVIMP", "PPER", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.44": {"text": "Nur so viel Brodt, da\u00df ich mit Laura k\u00f6nne leben,", "tokens": ["Nur", "so", "viel", "Brodt", ",", "da\u00df", "ich", "mit", "Lau\u00b7ra", "k\u00f6n\u00b7ne", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PIAT", "NN", "$,", "KOUS", "PPER", "APPR", "NE", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Die ganze Welt hab ich in ihr.", "tokens": ["Die", "gan\u00b7ze", "Welt", "hab", "ich", "in", "ihr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPER", "APPR", "PPOSAT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.46": {"text": "Der Herzog war ein r\u00f6mischer August;", "tokens": ["Der", "Her\u00b7zog", "war", "ein", "r\u00f6\u00b7mi\u00b7scher", "Au\u00b7gust", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.47": {"text": "Mit einer Meyerey belehnt er seinen Hirten;", "tokens": ["Mit", "ei\u00b7ner", "Me\u00b7ye\u00b7rey", "be\u00b7lehnt", "er", "sei\u00b7nen", "Hir\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.ia