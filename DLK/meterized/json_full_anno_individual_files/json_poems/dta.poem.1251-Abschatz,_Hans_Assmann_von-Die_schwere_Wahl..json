{"dta.poem.1251": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Die schwere Wahl.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Wie Hercules im Zweifel stand/", "tokens": ["Wie", "Her\u00b7cu\u00b7les", "im", "Zwei\u00b7fel", "stand", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPRART", "NN", "VVFIN", "$("], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.2": {"text": "Auff welchem Weg er solte treten/", "tokens": ["Auff", "wel\u00b7chem", "Weg", "er", "sol\u00b7te", "tre\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "NN", "PPER", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Da Tugend auff der rechten Hand", "tokens": ["Da", "Tu\u00b7gend", "auff", "der", "rech\u00b7ten", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und Lust zur Lincken ihn gebeten/", "tokens": ["Und", "Lust", "zur", "Lin\u00b7cken", "ihn", "ge\u00b7be\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPRART", "NN", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "So stehen auch izt meine Sinnen", "tokens": ["So", "ste\u00b7hen", "auch", "izt", "mei\u00b7ne", "Sin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "In Furcht und Hoffnung mitten innen.", "tokens": ["In", "Furcht", "und", "Hoff\u00b7nung", "mit\u00b7ten", "in\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ADV", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Doch sah er ihren Unterscheid/", "tokens": ["Doch", "sah", "er", "ih\u00b7ren", "Un\u00b7ter\u00b7scheid", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und konte nicht im Urtheil fehlen/", "tokens": ["Und", "kon\u00b7te", "nicht", "im", "Ur\u00b7theil", "feh\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PTKNEG", "APPRART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ihm f\u00fcr die Bahn der Sinnligkeit", "tokens": ["Ihm", "f\u00fcr", "die", "Bahn", "der", "Sinn\u00b7lig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den Pfad der Ehre zu erwehlen:", "tokens": ["Den", "Pfad", "der", "Eh\u00b7re", "zu", "er\u00b7weh\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wer aber lehret mich ergr\u00fcnden", "tokens": ["Wer", "a\u00b7ber", "leh\u00b7ret", "mich", "er\u00b7gr\u00fcn\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "VVFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wo ich das beste Theil soll finden.", "tokens": ["Wo", "ich", "das", "bes\u00b7te", "Theil", "soll", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "ADJA", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ich sehe gleichen Stand f\u00fcr mir/", "tokens": ["Ich", "se\u00b7he", "glei\u00b7chen", "Stand", "f\u00fcr", "mir", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "APPR", "PPER", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und frische Bl\u00fcthe gleicher Jugend/", "tokens": ["Und", "fri\u00b7sche", "Bl\u00fc\u00b7the", "glei\u00b7cher", "Ju\u00b7gend", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Den Augen weist sich gleiche Zier/", "tokens": ["Den", "Au\u00b7gen", "weist", "sich", "glei\u00b7che", "Zier", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dem Hertzen gleiche Frucht der Tugend:", "tokens": ["Dem", "Hert\u00b7zen", "glei\u00b7che", "Frucht", "der", "Tu\u00b7gend", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wer hier den Unterscheid kan kennen/", "tokens": ["Wer", "hier", "den", "Un\u00b7ter\u00b7scheid", "kan", "ken\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ART", "NN", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ist wohl ein Oedipus zu nennen.", "tokens": ["Ist", "wohl", "ein", "O\u00b7e\u00b7di\u00b7pus", "zu", "nen\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Verblendet einer Sonne Licht/", "tokens": ["Ver\u00b7blen\u00b7det", "ei\u00b7ner", "Son\u00b7ne", "Licht", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was soll von mehrern nicht geschehen?", "tokens": ["Was", "soll", "von", "meh\u00b7rern", "nicht", "ge\u00b7sche\u00b7hen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "APPR", "ADV", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn man dort braune N\u00e4geln bricht/", "tokens": ["Wenn", "man", "dort", "brau\u00b7ne", "N\u00e4\u00b7geln", "bricht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "L\u00e4st sich der Liljen Schnee hier sehen/", "tokens": ["L\u00e4st", "sich", "der", "Lil\u00b7jen", "Schnee", "hier", "se\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "NN", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die beyderseits den Liebes-Bienen", "tokens": ["Die", "bey\u00b7der\u00b7seits", "den", "Lie\u00b7bes\u00b7Bie\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Zu angenehmer Nahrung dienen.", "tokens": ["Zu", "an\u00b7ge\u00b7neh\u00b7mer", "Nah\u00b7rung", "die\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Di\u00df ist des Zweiflers \u00e4rgste Qual/", "tokens": ["Di\u00df", "ist", "des", "Zweif\u00b7lers", "\u00e4rgs\u00b7te", "Qual", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn er ihm keinen Schlu\u00df kan fassen.", "tokens": ["Wenn", "er", "ihm", "kei\u00b7nen", "Schlu\u00df", "kan", "fas\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "PIAT", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ich mu\u00df dem Hertzen schon die Wahl", "tokens": ["Ich", "mu\u00df", "dem", "Hert\u00b7zen", "schon", "die", "Wahl"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ART", "NN", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nach seiner Neigung \u00fcberlassen/", "tokens": ["Nach", "sei\u00b7ner", "Nei\u00b7gung", "\u00fc\u00b7ber\u00b7las\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und nachzufolgen mich bem\u00fchen", "tokens": ["Und", "nach\u00b7zu\u00b7fol\u00b7gen", "mich", "be\u00b7m\u00fc\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wohin mich Gl\u00fcck und Sternen ziehen.", "tokens": ["Wo\u00b7hin", "mich", "Gl\u00fcck", "und", "Ster\u00b7nen", "zie\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Vielleichte weist sich der Magnet/", "tokens": ["Viel\u00b7leich\u00b7te", "weist", "sich", "der", "Mag\u00b7net", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PRF", "ART", "NN", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Der meiner Seele Stahl gezogen/", "tokens": ["Der", "mei\u00b7ner", "See\u00b7le", "Stahl", "ge\u00b7zo\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "NN", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "(wie mein getreues Hoffen steht/)", "tokens": ["(", "wie", "mein", "ge\u00b7treu\u00b7es", "Hof\u00b7fen", "steht", "/", ")"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "KOKOM", "PPOSAT", "ADJA", "NN", "VVFIN", "$(", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auch desto eher mir gewogen.", "tokens": ["Auch", "des\u00b7to", "e\u00b7her", "mir", "ge\u00b7wo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ich wag\u2019 es drauff: Verh\u00e4ngnis schicke", "tokens": ["Ich", "wag'", "es", "drauff", ":", "Ver\u00b7h\u00e4ng\u00b7nis", "schi\u00b7cke"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$.", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Zu meinem F\u00fcrsatz Heyl und Gl\u00fccke!", "tokens": ["Zu", "mei\u00b7nem", "F\u00fcr\u00b7satz", "Heyl", "und", "Gl\u00fc\u00b7cke", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}