{"dta.poem.4217": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Geistige Copie der Welt.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mein GOtt! da ich hier ganz allein (die Augen auf", "tokens": ["Mein", "Gott", "!", "da", "ich", "hier", "ganz", "al\u00b7lein", "(", "die", "Au\u00b7gen", "auf"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$.", "KOUS", "PPER", "ADV", "ADV", "ADV", "$(", "ART", "NN", "APPR"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "die Welt gekehrt,", "tokens": ["die", "Welt", "ge\u00b7kehrt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Die jetzt der Lenz bebl\u00fchmet) sitze, auf alle Sch\u00f6nheit", "tokens": ["Die", "jetzt", "der", "Lenz", "be\u00b7bl\u00fch\u00b7met", ")", "sit\u00b7ze", ",", "auf", "al\u00b7le", "Sch\u00f6n\u00b7heit"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADV", "ART", "NN", "VVFIN", "$(", "VVFIN", "$,", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Achtung gebe,", "tokens": ["Ach\u00b7tung", "ge\u00b7be", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Die Himmel, Erd\u2019 und Wasser zeigen, worinn Dich alles", "tokens": ["Die", "Him\u00b7mel", ",", "Erd'", "und", "Was\u00b7ser", "zei\u00b7gen", ",", "wo\u00b7rinn", "Dich", "al\u00b7les"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "NN", "KON", "NN", "VVINF", "$,", "PWAV", "PPER", "PIS"], "meter": "-+-+-+-+-+-+--", "measure": "unknown.measure.hexa"}, "line.6": {"text": "zeigt und ehrt,", "tokens": ["zeigt", "und", "ehrt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Dich, HErr, bewundre, mich vergn\u00fcge, in meiner Lust", "tokens": ["Dich", ",", "Herr", ",", "be\u00b7wund\u00b7re", ",", "mich", "ver\u00b7gn\u00fc\u00b7ge", ",", "in", "mei\u00b7ner", "Lust"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "$,", "NN", "$,", "VVFIN", "$,", "PPER", "VVFIN", "$,", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Dein Lob erhebe;", "tokens": ["Dein", "Lob", "er\u00b7he\u00b7be", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.9": {"text": "So deucht mich, da\u00df nur diese Zeit die wahre Zeit sey,", "tokens": ["So", "deucht", "mich", ",", "da\u00df", "nur", "die\u00b7se", "Zeit", "die", "wah\u00b7re", "Zeit", "sey", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "ADV", "PDAT", "NN", "ART", "ADJA", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "die ich lebe.", "tokens": ["die", "ich", "le\u00b7be", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PRELS", "PPER", "VVFIN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.2": {"line.1": {"text": "Ich f\u00fchle Dich in meiner Lust, ich finde, da\u00df ich Dich", "tokens": ["Ich", "f\u00fch\u00b7le", "Dich", "in", "mei\u00b7ner", "Lust", ",", "ich", "fin\u00b7de", ",", "da\u00df", "ich", "Dich"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "$,", "KOUS", "PPER", "PRF"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "entdecke", "tokens": ["ent\u00b7de\u00b7cke"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "In allem, was ich ", "tokens": ["In", "al\u00b7lem", ",", "was", "ich"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPR", "PIS", "$,", "PWS", "PPER"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "riech' und schmecke.", "tokens": ["riech'", "und", "schme\u00b7cke", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Ich finde, da sich meine Sinnen auf Dich, in Deinen", "tokens": ["Ich", "fin\u00b7de", ",", "da", "sich", "mei\u00b7ne", "Sin\u00b7nen", "auf", "Dich", ",", "in", "Dei\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PRF", "PPOSAT", "NN", "APPR", "PPER", "$,", "APPR", "PPOSAT"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Werken, richten,", "tokens": ["Wer\u00b7ken", ",", "rich\u00b7ten", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Da sich mein Geist, von andern Dingen entfernet, nur", "tokens": ["Da", "sich", "mein", "Geist", ",", "von", "an\u00b7dern", "Din\u00b7gen", "ent\u00b7fer\u00b7net", ",", "nur"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["KOUS", "PRF", "PPOSAT", "NN", "$,", "APPR", "ADJA", "NN", "VVFIN", "$,", "ADV"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "mit Dir befa\u00dft,", "tokens": ["mit", "Dir", "be\u00b7fa\u00dft", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVPP", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "In einer Unschuld-vollen Stille, in einer rechten Seelen-", "tokens": ["In", "ei\u00b7ner", "Un\u00b7schuld\u00b7vol\u00b7len", "Stil\u00b7le", ",", "in", "ei\u00b7ner", "rech\u00b7ten", "See\u00b7len"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "APPR", "ART", "ADJA", "TRUNC"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.10": {"text": "Rast,", "tokens": ["Rast", ","], "token_info": ["word", "punct"], "pos": ["NE", "$,"], "meter": "+", "measure": "single.up"}, "line.11": {"text": "Sich Dein, in Deinen Wundern, freuet; da\u00df die\u00df die wah-", "tokens": ["Sich", "Dein", ",", "in", "Dei\u00b7nen", "Wun\u00b7dern", ",", "freu\u00b7et", ";", "da\u00df", "die\u00df", "die", "wah"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["PRF", "PPOSAT", "$,", "APPR", "PPOSAT", "NN", "$,", "VVFIN", "$.", "KOUS", "PDS", "ART", "TRUNC"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "ren Menschen-Pflichten,", "tokens": ["ren", "Men\u00b7schen\u00b7Pflich\u00b7ten", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.13": {"text": "Zu welchen wir erschaffen, scheinen. HErr, la\u00df, was mir", "tokens": ["Zu", "wel\u00b7chen", "wir", "er\u00b7schaf\u00b7fen", ",", "schei\u00b7nen", ".", "Herr", ",", "la\u00df", ",", "was", "mir"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "word"], "pos": ["APPR", "PWAT", "PPER", "VVPP", "$,", "VVFIN", "$.", "NN", "$,", "VVIMP", "$,", "PWS", "PPER"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.14": {"text": "von dieser Welt,", "tokens": ["von", "die\u00b7ser", "Welt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.15": {"text": "Wenn ich sie als Dein Werk betrachte, so unausdr\u00fccklich", "tokens": ["Wenn", "ich", "sie", "als", "Dein", "Werk", "be\u00b7trach\u00b7te", ",", "so", "un\u00b7aus\u00b7dr\u00fcck\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "KOKOM", "PPOSAT", "NN", "VVFIN", "$,", "ADV", "ADJD"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.16": {"text": "wohl gef\u00e4llt,", "tokens": ["wohl", "ge\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVPP", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.17": {"text": "Dir auch aus Gnaden wohl gefallen! So oft als sich in", "tokens": ["Dir", "auch", "aus", "Gna\u00b7den", "wohl", "ge\u00b7fal\u00b7len", "!", "So", "oft", "als", "sich", "in"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "APPR", "NN", "ADV", "VVPP", "$.", "ADV", "ADV", "KOUS", "PRF", "APPR"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.18": {"text": "meinem Geist", "tokens": ["mei\u00b7nem", "Geist"], "token_info": ["word", "word"], "pos": ["PPOSAT", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.19": {"text": "Dein grosses Werk verkleinert, weis\u2019t,", "tokens": ["Dein", "gros\u00b7ses", "Werk", "ver\u00b7klei\u00b7nert", ",", "weis't", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVPP", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Als eine zarte Schilderey,", "tokens": ["Als", "ei\u00b7ne", "zar\u00b7te", "Schil\u00b7de\u00b7rey", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von den Betrachtungen gebildet und gemahlet,", "tokens": ["Von", "den", "Be\u00b7trach\u00b7tun\u00b7gen", "ge\u00b7bil\u00b7det", "und", "ge\u00b7mah\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Von Deinem Gnaden-Licht bestrahlet,", "tokens": ["Von", "Dei\u00b7nem", "Gna\u00b7den\u00b7Licht", "be\u00b7strah\u00b7let", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "W\u00fcnsch ich, da\u00df die so kleine Welt aus Gnaden Dir ge-", "tokens": ["W\u00fcnsch", "ich", ",", "da\u00df", "die", "so", "klei\u00b7ne", "Welt", "aus", "Gna\u00b7den", "Dir", "ge"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "$,", "KOUS", "ART", "ADV", "ADJA", "NN", "APPR", "NN", "PPER", "TRUNC"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "f\u00e4llig sey!", "tokens": ["f\u00e4l\u00b7lig", "sey", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VAFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Und da\u00df aus dieser nach dem Leben formierten \u00e4hnlichen", "tokens": ["Und", "da\u00df", "aus", "die\u00b7ser", "nach", "dem", "Le\u00b7ben", "for\u00b7mier\u00b7ten", "\u00e4hn\u00b7li\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "APPR", "PDAT", "APPR", "ART", "NN", "VVFIN", "ADJA"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.7": {"text": "Copey,", "tokens": ["Co\u00b7pey", ","], "token_info": ["word", "punct"], "pos": ["NE", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.8": {"text": "In welcher, da sich unsre Lust zum Ruhm des grossen", "tokens": ["In", "wel\u00b7cher", ",", "da", "sich", "uns\u00b7re", "Lust", "zum", "Ruhm", "des", "gros\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PWAT", "$,", "KOUS", "PRF", "PPOSAT", "NN", "APPRART", "NN", "ART", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Sch\u00f6pfers f\u00fcget,", "tokens": ["Sch\u00f6p\u00b7fers", "f\u00fc\u00b7get", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.10": {"text": "Der eigentliche Zweck des Sch\u00f6pfers, wozu Er Bilder", "tokens": ["Der", "ei\u00b7gent\u00b7li\u00b7che", "Zweck", "des", "Sch\u00f6p\u00b7fers", ",", "wo\u00b7zu", "Er", "Bil\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$,", "PWAV", "PPER", "NN"], "meter": "-++--+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.11": {"text": "formte, lieget,", "tokens": ["form\u00b7te", ",", "lie\u00b7get", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.12": {"text": "Denn ebenfalls erhellen m\u00f6ge, was uns die Schrift zu", "tokens": ["Denn", "e\u00b7ben\u00b7falls", "er\u00b7hel\u00b7len", "m\u00f6\u00b7ge", ",", "was", "uns", "die", "Schrift", "zu"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVINF", "VMFIN", "$,", "PRELS", "PPER", "ART", "NN", "PTKZU"], "meter": "-+-+-+-+-+-++-", "measure": "unknown.measure.septa"}, "line.13": {"text": "wissen thut", "tokens": ["wis\u00b7sen", "thut"], "token_info": ["word", "word"], "pos": ["VVINF", "VVFIN"], "meter": "+-+", "measure": "trochaic.di"}, "line.14": {"text": "Vom grossen Welt-Original: Was GOtt gemacht, ist", "tokens": ["Vom", "gros\u00b7sen", "Welt\u00b7O\u00b7ri\u00b7gi\u00b7nal", ":", "Was", "Gott", "ge\u00b7macht", ",", "ist"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["APPRART", "ADJA", "NN", "$.", "PWS", "NN", "VVPP", "$,", "VAFIN"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.15": {"text": "alles gut!", "tokens": ["al\u00b7les", "gut", "!"], "token_info": ["word", "word", "punct"], "pos": ["PIS", "ADJD", "$."], "meter": "+-+", "measure": "trochaic.di"}}}}}