{"textgrid.poem.41746": {"metadata": {"author": {"name": "Otto, Louise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Das Alphorn t\u00f6nt, die Hirtin zieht zur Senne,", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das Alphorn t\u00f6nt, die Hirtin zieht zur Senne,", "tokens": ["Das", "Al\u00b7phorn", "t\u00f6nt", ",", "die", "Hir\u00b7tin", "zieht", "zur", "Sen\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Heerdenglocken klingeln vor ihr her,", "tokens": ["Die", "Heer\u00b7den\u00b7glo\u00b7cken", "klin\u00b7geln", "vor", "ihr", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und sicher, da\u00df er keinen Stein verkenne", "tokens": ["Und", "si\u00b7cher", ",", "da\u00df", "er", "kei\u00b7nen", "Stein", "ver\u00b7ken\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "$,", "KOUS", "PPER", "PIAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Steigt dort der Bursche mit dem Schie\u00dfgewehr", "tokens": ["Steigt", "dort", "der", "Bur\u00b7sche", "mit", "dem", "Schie\u00df\u00b7ge\u00b7wehr"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Empor zur Alp', die Gemse zu erjagen,", "tokens": ["Em\u00b7por", "zur", "Al\u00b7p'", ",", "die", "Gem\u00b7se", "zu", "er\u00b7ja\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Auf Felsenpfaden, die ins Blaue ragen.", "tokens": ["Auf", "Fel\u00b7sen\u00b7pfa\u00b7den", ",", "die", "ins", "Blau\u00b7e", "ra\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Das Gletschereis gl\u00e4nzt bunt in Fr\u00fchrotsschimmer,", "tokens": ["Das", "Glet\u00b7sche\u00b7reis", "gl\u00e4nzt", "bunt", "in", "Fr\u00fch\u00b7rots\u00b7schim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Rot gl\u00fcht die Firn und dunkel dampft der See,", "tokens": ["Rot", "gl\u00fcht", "die", "Firn", "und", "dun\u00b7kel", "dampft", "der", "See", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "KON", "ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Hier Bl\u00fcthenpracht, dort ewges Eisgeflimmer,", "tokens": ["Hier", "Bl\u00fc\u00b7then\u00b7pracht", ",", "dort", "ew\u00b7ges", "Eis\u00b7ge\u00b7flim\u00b7mer", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "$,", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Hier gr\u00fcne Matte, droben wei\u00dfer Schnee!", "tokens": ["Hier", "gr\u00fc\u00b7ne", "Mat\u00b7te", ",", "dro\u00b7ben", "wei\u00b7\u00dfer", "Schnee", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$,", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Lawinen drohen von der Berge Warten \u2013", "tokens": ["La\u00b7wi\u00b7nen", "dro\u00b7hen", "von", "der", "Ber\u00b7ge", "War\u00b7ten", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "NN", "$("], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.6": {"text": "Das ist die Schweiz der sch\u00f6ne Gottesgarten", "tokens": ["Das", "ist", "die", "Schweiz", "der", "sch\u00f6\u00b7ne", "Got\u00b7tes\u00b7gar\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und feig versteckt im weiten Priesterkleide", "tokens": ["Und", "feig", "ver\u00b7steckt", "im", "wei\u00b7ten", "Pries\u00b7ter\u00b7klei\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit Gift und Kette wandelt der Verrat,", "tokens": ["Mit", "Gift", "und", "Ket\u00b7te", "wan\u00b7delt", "der", "Ver\u00b7rat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ein Volk zu knechten, das ein Tell befreite,", "tokens": ["Ein", "Volk", "zu", "knech\u00b7ten", ",", "das", "ein", "Tell", "be\u00b7frei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Aus dessen Mitte einst ein Zwingli trat,", "tokens": ["Aus", "des\u00b7sen", "Mit\u00b7te", "einst", "ein", "Zwing\u00b7li", "trat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Gar finstre Macht wohnt in des Landes Mitten", "tokens": ["Gar", "finst\u00b7re", "Macht", "wohnt", "in", "des", "Lan\u00b7des", "Mit\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NN", "VVFIN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Kanonen donnern dumpf und Kugeln fliegen \u2013", "tokens": ["Ka\u00b7no\u00b7nen", "don\u00b7nern", "dumpf", "und", "Ku\u00b7geln", "flie\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ADJD", "KON", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "O, das ist mehr, ist mehr als Gemsenjagd.", "tokens": ["O", ",", "das", "ist", "mehr", ",", "ist", "mehr", "als", "Gem\u00b7sen\u00b7jagd", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PDS", "VAFIN", "ADV", "$,", "VAFIN", "ADV", "KOUS", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ach, das sind Br\u00fcder, die sich wild bekriegen", "tokens": ["Ach", ",", "das", "sind", "Br\u00fc\u00b7der", ",", "die", "sich", "wild", "be\u00b7krie\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "PDS", "VAFIN", "NN", "$,", "PRELS", "PRF", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das Bruderblut ist's das zum Himmel klagt,", "tokens": ["Das", "Bru\u00b7der\u00b7blut", "ist's", "das", "zum", "Him\u00b7mel", "klagt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ein Fluch, der allw\u00e4rts folgt der Heuchler Schritten \u2013", "tokens": ["Ein", "Fluch", ",", "der", "all\u00b7w\u00e4rts", "folgt", "der", "Heuch\u00b7ler", "Schrit\u00b7ten", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "VVFIN", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Ich schaue hier und bebe \u2013 soll ich beten?", "tokens": ["Ich", "schau\u00b7e", "hier", "und", "be\u00b7be", "\u2013", "soll", "ich", "be\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "KON", "VVFIN", "$(", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So reich gesegnet hat der Herr dies Land.", "tokens": ["So", "reich", "ge\u00b7seg\u00b7net", "hat", "der", "Herr", "dies", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "VAFIN", "ART", "NN", "PDS", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Frei darf nicht nur der Strom, der Vogel reden,", "tokens": ["Frei", "darf", "nicht", "nur", "der", "Strom", ",", "der", "Vo\u00b7gel", "re\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PTKNEG", "ADV", "ART", "NN", "$,", "ART", "NN", "VVINF", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Frei auch der Mensch die Feder in der Hand,", "tokens": ["Frei", "auch", "der", "Mensch", "die", "Fe\u00b7der", "in", "der", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Frei darf das Volk im freien Rate sitzen,", "tokens": ["Frei", "darf", "das", "Volk", "im", "frei\u00b7en", "Ra\u00b7te", "sit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "ART", "NN", "APPRART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Frei die Vertriebnen andrer Staaten sch\u00fctzen.", "tokens": ["Frei", "die", "Ver\u00b7trieb\u00b7nen", "an\u00b7drer", "Staa\u00b7ten", "sch\u00fct\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADJA", "NN", "VVINF", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}, "stanza.6": {"line.1": {"text": "So viel, so viel hat Gott der Schweiz gegeben \u2013", "tokens": ["So", "viel", ",", "so", "viel", "hat", "Gott", "der", "Schweiz", "ge\u00b7ge\u00b7ben", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "ADV", "ADV", "VAFIN", "NN", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Traun viel um das es uns zu bitten not \u2013", "tokens": ["Traun", "viel", "um", "das", "es", "uns", "zu", "bit\u00b7ten", "not", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "PRELS", "PPER", "PRF", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Er gab ihr Alles, was sie braucht zum Leben,", "tokens": ["Er", "gab", "ihr", "Al\u00b7les", ",", "was", "sie", "braucht", "zum", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIS", "$,", "PRELS", "PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zum freien Leben \u2013 andres ist nur Tod.", "tokens": ["Zum", "frei\u00b7en", "Le\u00b7ben", "\u2013", "and\u00b7res", "ist", "nur", "Tod", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$(", "PIS", "VAFIN", "ADV", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Fast Frevel scheint's bei so viel Heil zu bitten \u2013", "tokens": ["Fast", "Fre\u00b7vel", "scheint's", "bei", "so", "viel", "Heil", "zu", "bit\u00b7ten", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVFIN", "APPR", "ADV", "PIAT", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "So sei's denn Fluch \u2013 \u2013 ", "tokens": ["So", "sei's", "denn", "Fluch", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "NE", "KON", "NN", "$(", "$("], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.7": {"line.1": {"text": "Ich schaue hin und bebe \u2013 soll ich fluchen?", "tokens": ["Ich", "schau\u00b7e", "hin", "und", "be\u00b7be", "\u2013", "soll", "ich", "flu\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "KON", "VVFIN", "$(", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ja, ja ich darf's, ich ruf den Himmel an!", "tokens": ["Ja", ",", "ja", "ich", "dar\u00b7f's", ",", "ich", 