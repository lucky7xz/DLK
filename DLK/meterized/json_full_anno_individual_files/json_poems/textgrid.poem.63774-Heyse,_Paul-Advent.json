{"textgrid.poem.63774": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "Advent", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Am Himmel Wolkenjagd, bleifarb'ge Helle,", "tokens": ["Am", "Him\u00b7mel", "Wol\u00b7ken\u00b7jagd", ",", "blei\u00b7fa\u00b7rb'\u00b7ge", "Hel\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "In Frost erschauernd lag die Flur, die nackte;", "tokens": ["In", "Frost", "er\u00b7schau\u00b7ernd", "lag", "die", "Flur", ",", "die", "nack\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "VVFIN", "ART", "NN", "$,", "PRELS", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Fern sah her\u00fcber spukhaft der Soracte,", "tokens": ["Fern", "sah", "her\u00b7\u00fc\u00b7ber", "spuk\u00b7haft", "der", "So\u00b7rac\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und lautlos schlich die gelbe Tiberwelle.", "tokens": ["Und", "laut\u00b7los", "schlich", "die", "gel\u00b7be", "Ti\u00b7ber\u00b7wel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ein junges Hirtenpaar, in Ziegenfelle", "tokens": ["Ein", "jun\u00b7ges", "Hir\u00b7ten\u00b7paar", ",", "in", "Zie\u00b7gen\u00b7fel\u00b7le"], "token_info": ["word", "word", "word", "punct",