{"textgrid.poem.33955": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Unter'm Baum", "genre": "verse", "period": "N.A.", "pub_year": 1840, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Unter'm Baum im Sonnenstrale", "tokens": ["Un\u00b7ter'm", "Baum", "im", "Son\u00b7nen\u00b7stra\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NE", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Liegt ein rothes, tr\u00e4ges Kind,", "tokens": ["Liegt", "ein", "ro\u00b7thes", ",", "tr\u00e4\u00b7ges", "Kind", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schl\u00e4ft so lange, bis zum Mahle", "tokens": ["Schl\u00e4ft", "so", "lan\u00b7ge", ",", "bis", "zum", "Mah\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ADV", "$,", "KOUS", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Fr\u00fcchte abgefallen sind.", "tokens": ["Fr\u00fcch\u00b7te", "ab\u00b7ge\u00b7fal\u00b7len", "sind", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVPP", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Einer h\u00e4ngt der schweren Aeste", "tokens": ["Ei\u00b7ner", "h\u00e4ngt", "der", "schwe\u00b7ren", "A\u00b7es\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Fast herab auf sein Gesicht,", "tokens": ["Fast", "her\u00b7ab", "auf", "sein", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV",