{"dta.poem.11074": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Der dritte brief.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich schicke dir, mein schatz! kein eingebisamt schreiben,", "tokens": ["Ich", "schi\u00b7cke", "dir", ",", "mein", "schatz", "!", "kein", "ein\u00b7ge\u00b7bi\u00b7samt", "schrei\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PPOSAT", "NN", "$.", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nachdem du eckel selbst vor lieb und bisam hast:", "tokens": ["Nach\u00b7dem", "du", "ec\u00b7kel", "selbst", "vor", "lieb", "und", "bi\u00b7sam", "hast", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "ADV", "APPR", "ADJD", "KON", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich kan in unsrer eh nicht l\u00e4nger eh-los bleiben;", "tokens": ["Ich", "kan", "in", "uns\u00b7rer", "eh", "nicht", "l\u00e4n\u00b7ger", "eh\u00b7los", "blei\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PPOSAT", "KOUS", "PTKNEG", "ADJD", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Di\u00df ist es, was in sich mein gantzes schreiben fa\u00dft.", "tokens": ["Di\u00df", "ist", "es", ",", "was", "in", "sich", "mein", "gant\u00b7zes", "schrei\u00b7ben", "fa\u00dft", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "$,", "PRELS", "APPR", "PRF", "PPOSAT", "ADJA", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wie schwer kommt mich es an, die schwartze tinte brauchen!", "tokens": ["Wie", "schwer", "kommt", "mich", "es", "an", ",", "die", "schwart\u00b7ze", "tin\u00b7te", "brau\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "PPER", "PPER", "PTKVZ", "$,", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ach m\u00f6chte diese schrifft durchaus zinober seyn!", "tokens": ["Ach", "m\u00f6ch\u00b7te", "die\u00b7se", "schrifft", "durc\u00b7haus", "zi\u00b7no\u00b7ber", "seyn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PDS", "VVFIN", "ADV", "ADV", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Allein mein hertz und wunsch mu\u00df wie der berg verrauchen,", "tokens": ["Al\u00b7lein", "mein", "hertz", "und", "wunsch", "mu\u00df", "wie", "der", "berg", "ver\u00b7rau\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "KON", "ADJD", "VMFIN", "KOKOM", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Der in J\u00dfl\u00e4ndisch ei\u00df h\u00fcllt seine flammen ein.", "tokens": ["Der", "in", "J\u00df\u00b7l\u00e4n\u00b7disch", "ei\u00df", "h\u00fcllt", "sei\u00b7ne", "flam\u00b7men", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NE", "ADJD", "VVFIN", "PPOSAT", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Die winde, die in mir der liebe feur auffachen,", "tokens": ["Die", "win\u00b7de", ",", "die", "in", "mir", "der", "lie\u00b7be", "feur", "auf\u00b7fa\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "PRELS", "APPR", "PPER", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "F\u00fchr\u2019n eitel unmuths-schnee aus deiner brust empor.", "tokens": ["F\u00fchr'n", "ei\u00b7tel", "un\u00b7muths\u00b7schnee", "aus", "dei\u00b7ner", "brust", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "NN", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Nichts kan dich zorniger als meine liebe machen,", "tokens": ["Nichts", "kan", "dich", "zor\u00b7ni\u00b7ger", "als", "mei\u00b7ne", "lie\u00b7be", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PRF", "ADJD", "KOKOM", "PPOSAT", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Ja unser sauer-sehn zeuchst du dem liebreitz vor.", "tokens": ["Ja", "un\u00b7ser", "sau\u00b7e\u00b7rsehn", "zeuchst", "du", "dem", "lieb\u00b7reitz", "vor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PPOSAT", "ADJD", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Dein demant-hertze wird dir mein betr\u00fcbnis sagen,", "tokens": ["Dein", "de\u00b7mant\u00b7hert\u00b7ze", "wird", "dir", "mein", "be\u00b7tr\u00fcb\u00b7nis", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.14": {"text": "Wie dir ein l\u00f6ffel lust um nichts nicht feil gewest,", "tokens": ["Wie", "dir", "ein", "l\u00f6f\u00b7fel", "lust", "um", "nichts", "nicht", "feil", "ge\u00b7west", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "VVFIN", "APPR", "PIS", "PTKNEG", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Wie du offt einen ku\u00df dem eh-herrn abgeschlagen,", "tokens": ["Wie", "du", "offt", "ei\u00b7nen", "ku\u00df", "dem", "eh\u00b7herrn", "ab\u00b7ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Und seinen mund geflohn mehr als ein wespen-nest,", "tokens": ["Und", "sei\u00b7nen", "mund", "ge\u00b7flohn", "mehr", "als", "ein", "wes\u00b7pen\u00b7nest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVPP", "PIAT", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Den speichel \u00e4ngstlicher als t\u00f6dtend gifft verschm\u00e4het,", "tokens": ["Den", "spei\u00b7chel", "\u00e4ngst\u00b7li\u00b7cher", "als", "t\u00f6d\u00b7tend", "gifft", "ver\u00b7schm\u00e4\u00b7het", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "ADJD", "KOKOM", "ADJD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Dich aber selbst dadurch zur natter nur gemacht,", "tokens": ["Dich", "a\u00b7ber", "selbst", "da\u00b7durch", "zur", "nat\u00b7ter", "nur", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "PAV", "APPRART", "ADJA", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Den spinnen das gesicht, den r\u00fccken mir gedrehet,", "tokens": ["Den", "spin\u00b7nen", "das", "ge\u00b7sicht", ",", "den", "r\u00fc\u00b7cken", "mir", "ge\u00b7dre\u00b7het", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PDS", "VVPP", "$,", "ART", "VVFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Dein bett und zimmer mir verschlossen jede nacht.", "tokens": ["Dein", "bett", "und", "zim\u00b7mer", "mir", "ver\u00b7schlos\u00b7sen", "je\u00b7de", "nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "KON", "NN", "PPER", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Erwege deinen grimm nur selbst, und meine schmertzen,", "tokens": ["Er\u00b7we\u00b7ge", "dei\u00b7nen", "grimm", "nur", "selbst", ",", "und", "mei\u00b7ne", "schmert\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJD", "ADV", "ADV", "$,", "KON", "PPOSAT", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Dein sch\u00f6n-seyn locket mich, dein gram-seyn st\u00f6\u00dft mich weg,", "tokens": ["Dein", "sch\u00f6n\u00b7seyn", "lo\u00b7cket", "mich", ",", "dein", "gram\u00b7seyn", "st\u00f6\u00dft", "mich", "weg", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "$,", "PPOSAT", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Magnet beseelt dein aug\u2019, und demant steckt im hertzen,", "tokens": ["Mag\u00b7net", "be\u00b7seelt", "dein", "aug'", ",", "und", "de\u00b7mant", "steckt", "im", "hert\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "$,", "KON", "ADJD", "VVFIN", "APPRART", "NN", "$,"], "meter": "+--+-+--++-+-", "measure": "iambic.hexa.invert"}, "line.24": {"text": "Dein arm verr\u00fccket mir den selbst gesetzten zweck.", "tokens": ["Dein", "arm", "ver\u00b7r\u00fc\u00b7cket", "mir", "den", "selbst", "ge\u00b7setz\u00b7ten", "zweck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "VVFIN", "PPER", "ART", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Du fleuchst f\u00fcr meiner gunst, wie schatten vor den f\u00fcssen,", "tokens": ["Du", "fleuchst", "f\u00fcr", "mei\u00b7ner", "gunst", ",", "wie", "schat\u00b7ten", "vor", "den", "f\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,", "PWAV", "VVFIN", "APPR", "ART", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Doch schwermt dein bild um mich, wie motten um das", "tokens": ["Doch", "schwermt", "dein", "bild", "um", "mich", ",", "wie", "mot\u00b7ten", "um", "das"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "APPR", "PPER", "$,", "PWAV", "VMFIN", "APPR", "ART"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.27": {"text": "licht;", "tokens": ["licht", ";"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}, "line.28": {"text": "So offt ich aber dich umfangen will und k\u00fcssen,", "tokens": ["So", "offt", "ich", "a\u00b7ber", "dich", "um\u00b7fan\u00b7gen", "will", "und", "k\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "ADV", "PPER", "VVINF", "VMFIN", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Kan irrwisch und gespenst so bald verschwinden nicht.", "tokens": ["Kan", "irr\u00b7wisch", "und", "ge\u00b7spenst", "so", "bald", "ver\u00b7schwin\u00b7den", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADJD", "KON", "ADV", "ADV", "ADV", "VVFIN", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Ein ambo\u00df h\u00e4rtet sich nur von den hammer-schl\u00e4gen;", "tokens": ["Ein", "am\u00b7bo\u00df", "h\u00e4r\u00b7tet", "sich", "nur", "von", "den", "ham\u00b7mer\u00b7schl\u00e4\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Je mehr mein hertze klopfft, je eiserner wirst du.", "tokens": ["Je", "mehr", "mein", "hert\u00b7ze", "klopfft", ",", "je", "ei\u00b7ser\u00b7ner", "wirst", "du", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPOSAT", "NN", "VVFIN", "$,", "ADV", "ADJD", "VAFIN", "PPER", "$."], "meter": "+--+-+-+--+-", "measure": "iambic.penta.invert"}, "line.32": {"text": "Stein-eichen geben nach den winden und dem regen;", "tokens": ["Stein\u00b7ei\u00b7chen", "ge\u00b7ben", "nach", "den", "win\u00b7den", "und", "dem", "re\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "APPR", "ART", "ADJA", "KON", "ART", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Mein thr\u00e4uend seufftzen legt dir nur mehr h\u00e4rte zu.", "tokens": ["Mein", "thr\u00e4u\u00b7end", "seufft\u00b7zen", "legt", "dir", "nur", "mehr", "h\u00e4r\u00b7te", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "VVINF", "VVFIN", "PPER", "ADV", "ADV", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "So mu\u00df ich dich denn nur mit gallen-\u00e4pffeln speisen,", "tokens": ["So", "mu\u00df", "ich", "dich", "denn", "nur", "mit", "gal\u00b7len\u00b7\u00e4pf\u00b7feln", "spei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "ADV", "ADV", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Weil lieb und honig dir ein eckelnd essen ist.", "tokens": ["Weil", "lieb", "und", "ho\u00b7nig", "dir", "ein", "ec\u00b7kelnd", "es\u00b7sen", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "KON", "NN", "PPER", "ART", "ADJD", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Und nun ich mehr verdaut, als strausse stahl und eisen,", "tokens": ["Und", "nun", "ich", "mehr", "ver\u00b7daut", ",", "als", "straus\u00b7se", "stahl", "und", "ei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPER", "ADV", "VVPP", "$,", "KOUS", "ADJA", "NN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "So hat der himmel mir gelindre kost erkiest.", "tokens": ["So", "hat", "der", "him\u00b7mel", "mir", "ge\u00b7lind\u00b7re", "kost", "er\u00b7kiest", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "PPER", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Ich bin zeither wie hartz vom wasser brennend blieben,", "tokens": ["Ich", "bin", "zeit\u00b7her", "wie", "hartz", "vom", "was\u00b7ser", "bren\u00b7nend", "blie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "KOKOM", "ADJD", "APPRART", "NN", "VVPP", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Jtzt heischt mir die natur selbst warmen zunder ab.", "tokens": ["Jtzt", "heischt", "mir", "die", "na\u00b7tur", "selbst", "war\u00b7men", "zun\u00b7der", "ab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "ADV", "VVFIN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Ich hab ein neues band der heyrath unterschrieben,", "tokens": ["Ich", "hab", "ein", "neu\u00b7es", "band", "der", "hey\u00b7rath", "un\u00b7ter\u00b7schrie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Mit einer, die dir selbst offt viel vergn\u00fcgung gab.", "tokens": ["Mit", "ei\u00b7ner", ",", "die", "dir", "selbst", "offt", "viel", "ver\u00b7gn\u00fc\u00b7gung", "gab", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$,", "PRELS", "PPER", "ADV", "ADV", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Die werd\u2019 ich morgen mir den priester trauen lassen;", "tokens": ["Die", "werd'", "ich", "mor\u00b7gen", "mir", "den", "pries\u00b7ter", "trau\u00b7en", "las\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ADV", "PPER", "ART", "NN", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Du aber solst hierdurch gantz nicht verstossen seyn.", "tokens": ["Du", "a\u00b7ber", "solst", "hier\u00b7durch", "gantz", "nicht", "ver\u00b7stos\u00b7sen", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VMFIN", "PAV", "ADV", "PTKNEG", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Mein lincker arm soll sie, die rechte dich umfassen,", "tokens": ["Mein", "lin\u00b7cker", "arm", "soll", "sie", ",", "die", "rech\u00b7te", "dich", "um\u00b7fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "ADJD", "VMFIN", "PPER", "$,", "PRELS", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Du wirst zu deinem knie ihr zutritt ja verleihn!", "tokens": ["Du", "wirst", "zu", "dei\u00b7nem", "knie", "ihr", "zu\u00b7tritt", "ja", "ver\u00b7leihn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPOSAT", "NN", "PPER", "VVFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.46": {"text": "Sie wird als halbe magd dir h\u00e4nd und f\u00fcsse k\u00fcssen,", "tokens": ["Sie", "wird", "als", "hal\u00b7be", "magd", "dir", "h\u00e4nd", "und", "f\u00fcs\u00b7se", "k\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "KOKOM", "ADJA", "NN", "PPER", "ADJD", "KON", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "Jhr bl\u00f6des auge kennt der Hagar hochmuth nicht.", "tokens": ["Ihr", "bl\u00f6\u00b7des", "au\u00b7ge", "kennt", "der", "Ha\u00b7gar", "hoch\u00b7muth", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "ART", "NE", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.48": {"text": "Mein hertze soll dein bild vollkommen in sich schliessen,", "tokens": ["Mein", "hert\u00b7ze", "soll", "dein", "bild", "voll\u00b7kom\u00b7men", "in", "sich", "schlies\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PPOSAT", "NN", "ADJD", "APPR", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.49": {"text": "Wie wenn ein spiegel-gla\u00df gleich in zwey st\u00fccke bricht.", "tokens": ["Wie", "wenn", "ein", "spie\u00b7gel\u00b7gla\u00df", "gleich", "in", "zwey", "st\u00fc\u00b7cke", "bricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOKOM", "KOUS", "ART", "NN", "ADV", "APPR", "CARD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.50": {"text": "Da\u00df eine seele nicht recht lieb zwey seelen h\u00e4tte,", "tokens": ["Da\u00df", "ei\u00b7ne", "see\u00b7le", "nicht", "recht", "lieb", "zwey", "see\u00b7len", "h\u00e4t\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PTKNEG", "ADV", "ADJD", "CARD", "VVINF", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.51": {"text": "Ist ein verdammter wahn, der eyfersucht ihr kind:", "tokens": ["Ist", "ein", "ver\u00b7damm\u00b7ter", "wahn", ",", "der", "ey\u00b7fer\u00b7sucht", "ihr", "kind", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "$,", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.52": {"text": "Man sah zwey gr\u00e4finnen vergn\u00fcgt in einem bette,", "tokens": ["Man", "sah", "zwey", "gr\u00e4\u00b7fin\u00b7nen", "ver\u00b7gn\u00fcgt", "in", "ei\u00b7nem", "bet\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "CARD", "ADJA", "VVFIN", "APPR", "ART", "VVFIN", "$,"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.53": {"text": "Woraus sie in ein grab zu Erfurth kommen sind.", "tokens": ["Wo\u00b7raus", "sie", "in", "ein", "grab", "zu", "Er\u00b7furth", "kom\u00b7men", "sind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "ART", "NN", "APPR", "NN", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.54": {"text": "Ein strom, der \u00fcberl\u00e4ufft und fremde wiesen w\u00e4ssert,", "tokens": ["Ein", "strom", ",", "der", "\u00fc\u00b7berl\u00b7\u00e4ufft", "und", "frem\u00b7de", "wie\u00b7sen", "w\u00e4s\u00b7sert", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD", "KON", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.55": {"text": "L\u00e4\u00dft eigne bethe ja darum nicht unbenetzt;", "tokens": ["L\u00e4\u00dft", "eig\u00b7ne", "be\u00b7the", "ja", "da\u00b7rum", "nicht", "un\u00b7be\u00b7netzt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "VVFIN", "ADV", "PAV", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.56": {"text": "Von vielem sch\u00f6pffen wird der brunnen-quell verbessert,", "tokens": ["Von", "vie\u00b7lem", "sch\u00f6pf\u00b7fen", "wird", "der", "brun\u00b7nen\u00b7quell", "ver\u00b7bes\u00b7sert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "VVINF", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.57": {"text": "Durch wechsel und gebrauch so lieb als stahl gewetzt.", "tokens": ["Durch", "wech\u00b7sel", "und", "ge\u00b7brauch", "so", "lieb", "als", "stahl", "ge\u00b7wetzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ADV", "ADJD", "KOKOM", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.58": {"text": "Ists aber auch ihr ernst, da\u00df sie vor liebe grauen,", "tokens": ["Ists", "a\u00b7ber", "auch", "ihr", "ernst", ",", "da\u00df", "sie", "vor", "lie\u00b7be", "grau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADV", "PPER", "ADJD", "$,", "KOUS", "PPER", "APPR", "ADJA", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.59": {"text": "Und keine s\u00fc\u00dfigkeit von ihrem k\u00fctzel f\u00fchlt,", "tokens": ["Und", "kei\u00b7ne", "s\u00fc\u00b7\u00dfig\u00b7keit", "von", "ih\u00b7rem", "k\u00fct\u00b7zel", "f\u00fchlt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.60": {"text": "Kan sie um so vielmehr der lust vergn\u00fcgt zuschauen,", "tokens": ["Kan", "sie", "um", "so", "viel\u00b7mehr", "der", "lust", "ver\u00b7gn\u00fcgt", "zu\u00b7schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "ADV", "ADV", "ART", "NN", "VVPP", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.61": {"text": "Die ihr nichts s\u00fcsses raubt, und unsern durst doch k\u00fchlt.", "tokens": ["Die", "ihr", "nichts", "s\u00fcs\u00b7ses", "raubt", ",", "und", "un\u00b7sern", "durst", "doch", "k\u00fchlt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PIS", "ADJA", "VVFIN", "$,", "KON", "PPOSAT", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.62": {"text": "Die sonne sieht nicht scheel auf zwey, drey neben-sonnen,", "tokens": ["Die", "son\u00b7ne", "sieht", "nicht", "scheel", "auf", "zwey", ",", "drey", "ne\u00b7ben\u00b7son\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "ADJD", "APPR", "CARD", "$,", "CARD", "NN", "$,"], "meter": "-+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.63": {"text": "Wie da\u00df sie, sonne! denn mit einer eyfern will,", "tokens": ["Wie", "da\u00df", "sie", ",", "son\u00b7ne", "!", "denn", "mit", "ei\u00b7ner", "ey\u00b7fern", "will", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOKOM", "KOUS", "PPER", "$,", "VVFIN", "$.", "ADV", "APPR", "ART", "NN", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.64": {"text": "Die mein verh\u00e4ngnis eh, als ich, hat lieb gewonnen,", "tokens": ["Die", "mein", "ver\u00b7h\u00e4ng\u00b7nis", "eh", ",", "als", "ich", ",", "hat", "lieb", "ge\u00b7won\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJA", "KOUS", "$,", "KOUS", "PPER", "$,", "VAFIN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.65": {"text": "Di\u00df steckt dem lieben ja, wie allem, maas und ziel.", "tokens": ["Di\u00df", "steckt", "dem", "lie\u00b7ben", "ja", ",", "wie", "al\u00b7lem", ",", "maas", "und", "ziel", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "ADJA", "PTKANT", "$,", "PWAV", "PIS", "$,", "NE", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.66": {"text": "Es ist der h\u00f6chste witz, dem himmel beyfall geben,", "tokens": ["Es", "ist", "der", "h\u00f6chs\u00b7te", "witz", ",", "dem", "him\u00b7mel", "bey\u00b7fall", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.67": {"text": "Wer seine schl\u00fcsse st\u00fcrmt, der st\u00fcrtzt sich selbst in graus;", "tokens": ["Wer", "sei\u00b7ne", "schl\u00fcs\u00b7se", "st\u00fcrmt", ",", "der", "st\u00fcrtzt", "sich", "selbst", "in", "graus", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "ADJA", "VVFIN", "$,", "PRELS", "VVFIN", "PRF", "ADV", "APPR", "NE", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.68": {"text": "Der f\u00fcrsten, wolstand ist, gem\u00e4\u00df dem stande leben,", "tokens": ["Der", "f\u00fcrs\u00b7ten", ",", "wol\u00b7stand", "ist", ",", "ge\u00b7m\u00e4\u00df", "dem", "stan\u00b7de", "le\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "PWAV", "VAFIN", "$,", "ADJD", "ART", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.69": {"text": "Obgleich die wollust sich theilt in mehr r\u00f6hren aus.", "tokens": ["Ob\u00b7gleich", "die", "wol\u00b7lust", "sich", "theilt", "in", "mehr", "r\u00f6h\u00b7ren", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "VMFIN", "PRF", "VVFIN", "APPR", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.70": {"text": "Die eh ist ohne dem mit pfropffern unterst\u00fctzet,", "tokens": ["Die", "eh", "ist", "oh\u00b7ne", "dem", "mit", "pfropf\u00b7fern", "un\u00b7ter\u00b7st\u00fct\u00b7zet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "ART", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.71": {"text": "Der f\u00fcrsten stamm-baum ist, wie die, geartet nicht,", "tokens": ["Der", "f\u00fcrs\u00b7ten", "stam\u00b7mbaum", "ist", ",", "wie", "die", ",", "ge\u00b7ar\u00b7tet", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "$,", "PWAV", "ART", "$,", "VVPP", "PTKNEG", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.72": {"text": "Die mit viel zweigen stehn f\u00fcr sonn und sturm besch\u00fctzet,", "tokens": ["Die", "mit", "viel", "zwei\u00b7gen", "stehn", "f\u00fcr", "sonn", "und", "sturm", "be\u00b7sch\u00fct\u00b7zet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PIS", "VVINF", "VVFIN", "APPR", "NE", "KON", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.73": {"text": "Weil den zertheilten stock der \u00e4ste last zerbricht.", "tokens": ["Weil", "den", "zer\u00b7theil\u00b7ten", "stock", "der", "\u00e4s\u00b7te", "last", "zer\u00b7bricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.74": {"text": "Was m\u00fch ich aber mich erst gegen dir vergebens,", "tokens": ["Was", "m\u00fch", "ich", "a\u00b7ber", "mich", "erst", "ge\u00b7gen", "dir", "ver\u00b7ge\u00b7bens", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "ADV", "PPER", "ADV", "APPR", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.75": {"text": "Da\u00df du zufrieden m\u00f6chst mit unserm schlusse seyn?", "tokens": ["Da\u00df", "du", "zu\u00b7frie\u00b7den", "m\u00f6chst", "mit", "un\u00b7serm", "schlus\u00b7se", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VMFIN", "APPR", "PPOSAT", "ADJA", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.76": {"text": "Die klugheit, die du heist den circkel dieses lebens,", "tokens": ["Die", "klug\u00b7heit", ",", "die", "du", "heist", "den", "cir\u00b7ckel", "die\u00b7ses", "le\u00b7bens", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "ART", "NN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.77": {"text": "Des gl\u00fcckes mittel-punct, die redet dir selbst ein.", "tokens": ["Des", "gl\u00fc\u00b7ckes", "mit\u00b7tel\u00b7punct", ",", "die", "re\u00b7det", "dir", "selbst", "ein", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "VVFIN", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.78": {"text": "Zwar jedes ding sieht aus, nachdem es wird gedrehet,", "tokens": ["Zwar", "je\u00b7des", "ding", "sieht", "aus", ",", "nach\u00b7dem", "es", "wird", "ge\u00b7dre\u00b7het", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "VVFIN", "PTKVZ", "$,", "KOUS", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.79": {"text": "Scheint demant doch und gold offt glas und me\u00dfing kaum;", "tokens": ["Scheint", "de\u00b7mant", "doch", "und", "gold", "offt", "glas", "und", "me\u00b7\u00dfing", "kaum", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ADV", "KON", "NN", "ADV", "VVFIN", "KON", "VVFIN", "ADV", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.80": {"text": "Die tugend selber wird als laster offt geschm\u00e4het,", "tokens": ["Die", "tu\u00b7gend", "sel\u00b7ber", "wird", "als", "las\u00b7ter", "offt", "ge\u00b7schm\u00e4\u00b7het", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VAFIN", "KOKOM", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.81": {"text": "Der neid wirfft auf napell und rosen seinen schaum.", "tokens": ["Der", "neid", "wirfft", "auf", "na\u00b7pell", "und", "ro\u00b7sen", "sei\u00b7nen", "schaum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJD", "KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.82": {"text": "Dein urtheil aber f\u00fchlt den puls in allen sachen,", "tokens": ["Dein", "ur\u00b7theil", "a\u00b7ber", "f\u00fchlt", "den", "puls", "in", "al\u00b7len", "sa\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "VVFIN", "ART", "NE", "APPR", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.83": {"text": "Di\u00df l\u00e4sst ohn zweifel dir nicht mi\u00dffall\u2019n unsern schlu\u00df.", "tokens": ["Di\u00df", "l\u00e4sst", "ohn", "zwei\u00b7fel", "dir", "nicht", "mi\u00df\u00b7fall'n", "un\u00b7sern", "schlu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "NN", "PPER", "PTKNEG", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.84": {"text": "Der f\u00e4hrt mit crocodiln, und reitet auf den drachen,", "tokens": ["Der", "f\u00e4hrt", "mit", "cro\u00b7co\u00b7diln", ",", "und", "rei\u00b7tet", "auf", "den", "dra\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "NN", "$,", "KON", "VVFIN", "APPR", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.85": {"text": "Der den begierden stets den z\u00fcgel lassen mu\u00df.", "tokens": ["Der", "den", "be\u00b7gier\u00b7den", "stets", "den", "z\u00fc\u00b7gel", "las\u00b7sen", "mu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "ADV", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.86": {"text": "Auf solchen fall soll dir nichts an vergn\u00fcgung fehlen:", "tokens": ["Auf", "sol\u00b7chen", "fall", "soll", "dir", "nichts", "an", "ver\u00b7gn\u00fc\u00b7gung", "feh\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VMFIN", "PPER", "PIS", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.87": {"text": "Ich und der Rhein wird dich als sonn und haupt verehr\u2019n,", "tokens": ["Ich", "und", "der", "Rhein", "wird", "dich", "als", "sonn", "und", "haupt", "ver\u00b7ehr'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "KON", "ART", "NE", "VAFIN", "PPER", "KOUS", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.88": {"text": "So lange du nur die, der wir uns itzt verm\u00e4hlen,", "tokens": ["So", "lan\u00b7ge", "du", "nur", "die", ",", "der", "wir", "uns", "itzt", "ver\u00b7m\u00e4h\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "ADV", "ART", "$,", "PRELS", "PPER", "PRF", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.89": {"text": "Wirst lassen monde seyn, und sie in nichts versehr\u2019n.", "tokens": ["Wirst", "las\u00b7sen", "mon\u00b7de", "seyn", ",", "und", "sie", "in", "nichts", "ver\u00b7sehr'", "n."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["VAFIN", "VVINF", "VMFIN", "VAINF", "$,", "KON", "PPER", "APPR", "PIS", "VVFIN", "NE"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.90": {"text": "Wer aber sich auf sie wird was gel\u00fcsten lassen,", "tokens": ["Wer", "a\u00b7ber", "sich", "auf", "sie", "wird", "was", "ge\u00b7l\u00fcs\u00b7ten", "las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "PRF", "APPR", "PPER", "VAFIN", "PIS", "VVINF", "VVINF", "$,"], "meter": "-+--+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.91": {"text": "Greifft bi\u00df zum hertzen uns den augen-apffel an,", "tokens": ["Greifft", "bi\u00df", "zum", "hert\u00b7zen", "uns", "den", "au\u00b7gen\u00b7apf\u00b7fel", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "APPRART", "NN", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.92": {"text": "Der soll mit schimpff und ach von unsrer faust erblassen;", "tokens": ["Der", "soll", "mit", "schimpff", "und", "ach", "von", "uns\u00b7rer", "faust", "er\u00b7blas\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "APPR", "ADJD", "KON", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.93": {"text": "Du weist wohl, was die rach erz\u00f6rnter liebe kan.", "tokens": ["Du", "weist", "wohl", ",", "was", "die", "rach", "er\u00b7z\u00f6rn\u00b7ter", "lie\u00b7be", "kan", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PRELS", "ART", "NN", "ADJA", "VVFIN", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.94": {"text": "Willst du der einsamkeit denn deine tage weyhen,", "tokens": ["Willst", "du", "der", "ein\u00b7sam\u00b7keit", "denn", "dei\u00b7ne", "ta\u00b7ge", "wey\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "KON", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.95": {"text": "Und dich von bett, und tisch, wie vormahls, scheiden ab,", "tokens": ["Und", "dich", "von", "bett", ",", "und", "tisch", ",", "wie", "vor\u00b7mahls", ",", "schei\u00b7den", "ab", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "ADJD", "$,", "KON", "ADJD", "$,", "PWAV", "ADV", "$,", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.96": {"text": "Wird man das innre schlo\u00df zur wohnung dir verleihen,", "tokens": ["Wird", "man", "das", "inn\u00b7re", "schlo\u00df", "zur", "woh\u00b7nung", "dir", "ver\u00b7lei\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "ART", "ADJA", "VVFIN", "APPRART", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.97": {"text": "Das deiner bangsamkeit offt einen aufhalt gab.", "tokens": ["Das", "dei\u00b7ner", "bang\u00b7sam\u00b7keit", "offt", "ei\u00b7nen", "auf\u00b7halt", "gab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "NN", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.98": {"text": "Du hast hieraus die wahl, auch witz, dich zu bestreiten,", "tokens": ["Du", "hast", "hier\u00b7aus", "die", "wahl", ",", "auch", "witz", ",", "dich", "zu", "be\u00b7strei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PAV", "ART", "NN", "$,", "ADV", "PTKVZ", "$,", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.99": {"text": "Nicht lege meinem thun mehr fluch als vorsicht bey,", "tokens": ["Nicht", "le\u00b7ge", "mei\u00b7nem", "thun", "mehr", "fluch", "als", "vor\u00b7sicht", "bey", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "PPOSAT", "VVFIN", "PIAT", "NN", "KOKOM", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.100": {"text": "Und dencke, wenn wir ja auf diesem eise gleiten,", "tokens": ["Und", "den\u00b7cke", ",", "wenn", "wir", "ja", "auf", "die\u00b7sem", "ei\u00b7se", "glei\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PPER", "ADV", "APPR", "PDAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.101": {"text": "Da\u00df mancher Salomo hier gar gefallen sey.", "tokens": ["Da\u00df", "man\u00b7cher", "Sa\u00b7lo\u00b7mo", "hier", "gar", "ge\u00b7fal\u00b7len", "sey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NE", "ADV", "ADV", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}