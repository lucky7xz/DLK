{"dta.poem.11819": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "105.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Bauer hat ein Hun und eine Kuh dazu;", "tokens": ["Der", "Bau\u00b7er", "hat", "ein", "Hun", "und", "ei\u00b7ne", "Kuh", "da\u00b7zu", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "KON", "ART", "NN", "PAV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Schuldigkeit will thun doch weder Hun noch Kuh.", "tokens": ["Die", "Schul\u00b7dig\u00b7keit", "will", "thun", "doch", "we\u00b7der", "Hun", "noch", "Kuh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "VVINF", "KON", "KON", "NN", "ADV", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Er hofft, ihm soll ein Ei vom Hun ein Mahl bereiten,", "tokens": ["Er", "hofft", ",", "ihm", "soll", "ein", "Ei", "vom", "Hun", "ein", "Mahl", "be\u00b7rei\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VMFIN", "ART", "NN", "APPRART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und von der Kuh dabei die Milch den Trunk bestreiten.", "tokens": ["Und", "von", "der", "Kuh", "da\u00b7bei", "die", "Milch", "den", "Trunk", "be\u00b7strei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "PAV", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Er hofft, es soll ein Ei ein K\u00fcchlein auch geb\u00e4ren,", "tokens": ["Er", "hofft", ",", "es", "soll", "ein", "Ei", "ein", "K\u00fcch\u00b7lein", "auch", "ge\u00b7b\u00e4\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VMFIN", "ART", "NN", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und da\u00df die Kuh ihm sei bereit ein Kalb zu n\u00e4hren.", "tokens": ["Und", "da\u00df", "die", "Kuh", "ihm", "sei", "be\u00b7reit", "ein", "Kalb", "zu", "n\u00e4h\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "PPER", "VAFIN", "ADJD", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Es fressen ihm die zwei umsonst nur Korn und Kr\u00e4uter;", "tokens": ["Es", "fres\u00b7sen", "ihm", "die", "zwei", "um\u00b7sonst", "nur", "Korn", "und", "Kr\u00e4u\u00b7ter", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "CARD", "ADV", "ADV", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das Hun fri\u00dft selbst sein Ei, die Kuh trinkt selbst ihr Euter.", "tokens": ["Das", "Hun", "fri\u00dft", "selbst", "sein", "Ei", ",", "die", "Kuh", "trinkt", "selbst", "ihr", "Eu\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "PPOSAT", "NN", "$,", "ART", "NN", "VVFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "O schlimme Eigenschaft, sich selbst nur zu beachten,", "tokens": ["O", "schlim\u00b7me", "Ei\u00b7gen\u00b7schaft", ",", "sich", "selbst", "nur", "zu", "be\u00b7ach\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "PRF", "ADV", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weder Nachkommenschaft noch Haushalt zu betrachten!", "tokens": ["We\u00b7der", "Nach\u00b7kom\u00b7men\u00b7schaft", "noch", "Haus\u00b7halt", "zu", "be\u00b7trach\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADV", "NN", "PTKZU", "VVINF", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}}, "stanza.6": {"line.1": {"text": "Was kann im Haus der Bund von Hun und Kuh dir taugen,", "tokens": ["Was", "kann", "im", "Haus", "der", "Bund", "von", "Hun", "und", "Kuh", "dir", "tau\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "APPRART", "NN", "ART", "NN", "APPR", "NN", "KON", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die ihre Eier und ihr Euter selbst aussaugen?", "tokens": ["Die", "ih\u00b7re", "Ei\u00b7er", "und", "ihr", "Eu\u00b7ter", "selbst", "aus\u00b7sau\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Drum sollst du in das Haus so Kuh als Henne schlachten,", "tokens": ["Drum", "sollst", "du", "in", "das", "Haus", "so", "Kuh", "als", "Hen\u00b7ne", "schlach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "APPR", "ART", "NN", "ADV", "NN", "KOUS", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und neue kaufen aus dem Geld, das sie dir brachten.", "tokens": ["Und", "neu\u00b7e", "kau\u00b7fen", "aus", "dem", "Geld", ",", "das", "sie", "dir", "brach\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "VVFIN", "APPR", "ART", "NN", "$,", "PRELS", "PPER", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}