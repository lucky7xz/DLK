{"textgrid.poem.67767": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Die Sch\u00f6pfung", "genre": "verse", "period": "N.A.", "pub_year": 1769, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Einst war im weiten Sch\u00f6pfungsraum", "tokens": ["Einst", "war", "im", "wei\u00b7ten", "Sch\u00f6p\u00b7fungs\u00b7raum"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Noch Alles \u00f6d und w\u00fcst und leer,", "tokens": ["Noch", "Al\u00b7les", "\u00f6d", "und", "w\u00fcst", "und", "leer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "ADJD", "KON", "VVFIN", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Erd' ohn' H\u00fcgel, Thier und Baum,", "tokens": ["Die", "Erd'", "ohn'", "H\u00fc\u00b7gel", ",", "Thier", "und", "Baum", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$,", "NN", "KON", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein gro\u00dfes, schwarzes, wildes Meer,", "tokens": ["Ein", "gro\u00b7\u00dfes", ",", "schwar\u00b7zes", ",", "wil\u00b7des", "Meer", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und Gottes Hauch schwebt' dr\u00fcber her.", "tokens": ["Und", "Got\u00b7tes", "Hauch", "schwebt'", "dr\u00fc\u00b7ber", "her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "NN", "VVFIN", "PAV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da sprach der Sch\u00f6pfer aller Welt:", "tokens": ["Da", "sprach", "der", "Sch\u00f6p\u00b7fer", "al\u00b7ler", "Welt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "\u00bbsei Licht!\u00ab Da stand das Meer erhellt,", "tokens": ["\u00bb", "sei", "Licht", "!", "\u00ab", "Da", "stand", "das", "Meer", "er\u00b7hellt", ","], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "NN", "$.", "$(", "ADV", "VVFIN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Das Licht war g\u00fclden, klar und sch\u00f6n;", "tokens": ["Das", "Licht", "war", "g\u00fcl\u00b7den", ",", "klar", "und", "sch\u00f6n", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Gott sah's und freut' sich, 's anzusehn,", "tokens": ["Gott", "sah's", "und", "freut'", "sich", ",", "'s", "an\u00b7zu\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NE", "KON", "VVFIN", "PRF", "$,", "PPER", "VVIZU", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.10": {"text": "Und nannt's und gab es seiner Welt.", "tokens": ["Und", "nannt's", "und", "gab", "es", "sei\u00b7ner", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "KON", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Das war der erste Tag.", "tokens": ["Das", "war", "der", "ers\u00b7te", "Tag", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Drauf nahm Gott silberhelles Meer", "tokens": ["Drauf", "nahm", "Gott", "sil\u00b7ber\u00b7hel\u00b7les", "Meer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und hub's so weit und breit hinan.", "tokens": ["Und", "hub's", "so", "weit", "und", "breit", "hi\u00b7nan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "ADV", "ADJD", "KON", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da flo\u00df es oben blau daher", "tokens": ["Da", "flo\u00df", "es", "o\u00b7ben", "blau", "da\u00b7her"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADJD", "PAV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hoch \u00fcberm alten Ocean,", "tokens": ["Hoch", "\u00fc\u00b7berm", "al\u00b7ten", "O\u00b7cean", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Und zwischen ward hellweite Bahn,", "tokens": ["Und", "zwi\u00b7schen", "ward", "hell\u00b7wei\u00b7te", "Bahn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VAFIN", "ADJA", "NN", "$,"], "meter": "-+--++-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Die nannt' der Sch\u00f6pfer aller Welt", "tokens": ["Die", "nannt'", "der", "Sch\u00f6p\u00b7fer", "al\u00b7ler", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ART", "NN", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Stand blau und hell und klar und sch\u