{"textgrid.poem.51008": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Tr\u00fcbes Wetter", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es ist ein stiller Regentag,", "tokens": ["Es", "ist", "ein", "stil\u00b7ler", "Re\u00b7gen\u00b7tag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So weich, so ernst, und doch so klar,", "tokens": ["So", "weich", ",", "so", "ernst", ",", "und", "doch", "so", "klar", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADV", "ADJD", "$,", "KON", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wo durch den D\u00e4mmer brechen mag", "tokens": ["Wo", "durch", "den", "D\u00e4m\u00b7mer", "bre\u00b7chen", "mag"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN", "VVINF", "VMFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Sonne wei\u00df und sonderbar.", "tokens": ["Die", "Son\u00b7ne", "wei\u00df", "und", "son\u00b7der\u00b7bar", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ein wunderliches Zwielicht spielt", "tokens": ["Ein", "wun\u00b7der\u00b7li\u00b7ches", "Zwie\u00b7licht", "spielt"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Beschaulich \u00fcber Berg und Tal;", "tokens": ["Be\u00b7schau\u00b7lich", "\u00fc\u00b7ber", "Be