{"dta.poem.2582": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Cupido.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich sehe freylich hier die sch\u00f6nen Sorten an/", "tokens": ["Ich", "se\u00b7he", "frey\u00b7lich", "hier", "die", "sch\u00f6\u00b7nen", "Sor\u00b7ten", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ART", "ADJA", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zeug/ in den blo\u00df allein die Venus einzukleiden;", "tokens": ["Zeug", "/", "in", "den", "blo\u00df", "al\u00b7lein", "die", "Ve\u00b7nus", "ein\u00b7zu\u00b7klei\u00b7den", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "APPR", "ART", "ADV", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Gewebe/ das gemacht ist von der besten Seiden.", "tokens": ["Ge\u00b7we\u00b7be", "/", "das", "ge\u00b7macht", "ist", "von", "der", "bes\u00b7ten", "Sei\u00b7den", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "PDS", "VVPP", "VAFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Jedoch was meine Hand vor Arbeit wircken kan/", "tokens": ["Je\u00b7doch", "was", "mei\u00b7ne", "Hand", "vor", "Ar\u00b7beit", "wir\u00b7cken", "kan", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWS", "PPOSAT", "NN", "APPR", "NN", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Sticht das Gespenste weg/ das uns Venedig schicket/", "tokens": ["Sticht", "das", "Ge\u00b7spens\u00b7te", "weg", "/", "das", "uns", "Ve\u00b7ne\u00b7dig", "schi\u00b7cket", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PTKVZ", "$(", "PRELS", "PPER", "NE", "VVFIN", "$("], "meter": "+--+-+--+--+-", "measure": "iambic.penta.invert"}, "line.6": {"text": "Womit sich Pisa r\u00fchmt und Genua sonst schm\u00fccket.", "tokens": ["Wo\u00b7mit", "sich", "Pi\u00b7sa", "r\u00fchmt", "und", "Ge\u00b7nua", "sonst", "schm\u00fc\u00b7cket", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "NE", "VVFIN", "KON", "NE", "ADV", "VVFIN", "$."], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}}}}}