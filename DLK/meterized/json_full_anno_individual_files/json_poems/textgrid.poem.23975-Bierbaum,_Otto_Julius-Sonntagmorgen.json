{"textgrid.poem.23975": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Sonntagmorgen", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Durch den breiten Fensterbogen", "tokens": ["Durch", "den", "brei\u00b7ten", "Fens\u00b7ter\u00b7bo\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Blick ich hinaus in st\u00fcrmischen Fr\u00fchling.", "tokens": ["Blick", "ich", "hin\u00b7aus", "in", "st\u00fcr\u00b7mi\u00b7schen", "Fr\u00fch\u00b7ling", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APZR", "APPR", "ADJA", "NN", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Grobgraue Wolken in dicken Flocken", "tokens": ["Grob\u00b7grau\u00b7e", "Wol\u00b7ken", "in", "di\u00b7cken", "Flo\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Schieben sich dr\u00e4ngend \u00fcber das bleiige", "tokens": ["Schie\u00b7ben", "sich", "dr\u00e4n\u00b7gend", "\u00fc\u00b7ber", "das", "blei\u00b7i\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "ADJD", "APPR", "ART", "ADJA"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.5": {"text": "Blau des Himmels, schwarze, geballte", "tokens": ["Blau", "des", "Him\u00b7mels", ",", "schwar\u00b7ze", ",", "ge\u00b7ball\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word"], "pos": ["NN", "ART", "NN", "$,", "VVFIN", "$,", "VVFIN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "Wolkenf\u00e4uste drohend voran.", "tokens": ["Wol\u00b7ken\u00b7f\u00e4us\u00b7te", "dro\u00b7hend", "vo\u00b7ran", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "ADJD", "PTKVZ", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.7": {"text": "Unten der Sturm faucht in das junge Gr\u00fcn", "tokens": ["Un\u00b7ten", "der", "Sturm", "faucht", "in", "das", "jun\u00b7ge", "Gr\u00fcn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.8": {"text": "Wie eine gierige L\u00f6wenkatze,", "tokens": ["Wie", "ei\u00b7ne", "gie\u00b7ri\u00b7ge", "L\u00f6\u00b7wen\u00b7kat\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Zaust die buschigen Wipfel, rauft,", "tokens": ["Zaust", "die", "bu\u00b7schi\u00b7gen", "Wip\u00b7fel", ",", "rauft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,", "VVFIN", "$,"], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.10": {"text": "Zerrt in den zitternden Locken des Laubs.", "tokens": ["Zerrt", "in", "den", "zit\u00b7tern\u00b7den", "Lo\u00b7cken", "des", "Laubs", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.11": {"text": "Steinern starr, spitzig schlank,", "tokens": ["Stei\u00b7nern", "starr", ",", "spit\u00b7zig", "schlank", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ADJD", "$,", "ADJD", "VVFIN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.12": {"text": "Ragt im gr\u00fcnen Sturmgeschwank,", "tokens": ["Ragt", "im", "gr\u00fc\u00b7nen", "Sturm\u00b7ge\u00b7schwank", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "Schn\u00f6rkelbl\u00fctig, rankenumklettert,", "tokens": ["Schn\u00f6r\u00b7kel\u00b7bl\u00fc\u00b7tig", ",", "ran\u00b7ke\u00b7num\u00b7klet\u00b7tert", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["ADJD", "$,", "VVFIN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.14": {"text": "Keck in die H\u00f6h zu den jagenden Wolken,", "tokens": ["Keck", "in", "die", "H\u00f6h", "zu", "den", "ja\u00b7gen\u00b7den", "Wol\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.15": {"text": "Hochaufreckend ein goldenes Kreuz,", "tokens": ["Hoch\u00b7auf\u00b7re\u00b7ckend", "ein", "gol\u00b7de\u00b7nes", "Kreuz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.16": {"text": "Der gotische Turm.", "tokens": ["Der", "go\u00b7ti\u00b7sche", "Turm", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.17": {"text": "Und es klingt durch den Sturm", "tokens": ["Und", "es", "klingt", "durch", "den", "Sturm"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.18": {"text": "Vom Turm herab,", "tokens": ["Vom", "Turm", "her\u00b7ab", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.19": {"text": "Dunkelt\u00f6nig, wellig, breit,", "tokens": ["Dun\u00b7kel\u00b7t\u00f6\u00b7nig", ",", "wel\u00b7lig", ",", "breit", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "$,", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.20": {"text": "Dumpf, ernst, tief", "tokens": ["Dumpf", ",", "ernst", ",", "tief"], "token_info": ["word", "punct", "word", "punct", "word"], "pos": ["NN", "$,", "ADJD", "$,", "ADJD"], "meter": "+-+", "measure": "trochaic.di"}, "line.21": {"text": "Kirchengel\u00e4ute:", "tokens": ["Kir\u00b7chen\u00b7ge\u00b7l\u00e4u\u00b7te", ":"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.22": {"text": "\u00bbkommt \u2013 kommt, kommt \u2013 kommt,", "tokens": ["\u00bb", "kommt", "\u2013", "kommt", ",", "kommt", "\u2013", "kommt", ","], "token_info": ["punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["$(", "VVFIN", "$(", "VVFIN", "$,", "VVFIN", "$(", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.23": {"text": "Gott \u2013 ruft, Gott \u2013 ruft, \u2013 Kommt ...!\u00ab", "tokens": ["Gott", "\u2013", "ruft", ",", "Gott", "\u2013", "ruft", ",", "\u2013", "Kommt", "...", "!", "\u00ab"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct", "punct", "word", "punct", "punct", "punct"], "pos": ["NN", "$(", "VVFIN", "$,", "NN", "$(", "VVFIN", "$,", "$(", "VVIMP", "$(", "$.", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Der Sturm st\u00f6\u00dft weiter, die Glocke verklingt,", "tokens": ["Der", "Sturm", "st\u00f6\u00dft", "wei\u00b7ter", ",", "die", "Glo\u00b7cke", "ver\u00b7klingt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die Wolkenf\u00e4uste spreizen die schwarzen,", "tokens": ["Die", "Wol\u00b7ken\u00b7f\u00e4us\u00b7te", "sprei\u00b7zen", "die", "schwar\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Knolligen Finger: Der Regen tr\u00e4uft.", "tokens": ["Knol\u00b7li\u00b7gen", "Fin\u00b7ger", ":", "Der", "Re\u00b7gen", "tr\u00e4uft", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$.", "ART", "NN", "VVFIN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Da schweigt der Sturm.", "tokens": ["Da", "schweigt", "der", "Sturm", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Ein Nebelgespinnst, eint\u00f6nig, grau,", "tokens": ["Ein", "Ne\u00b7bel\u00b7ge\u00b7spinnst", ",", "ein\u00b7t\u00f6\u00b7nig", ",", "grau", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "$,", "ADJD", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Schwankt vor dem Fenster.", "tokens": ["Schwankt", "vor", "dem", "Fens\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Leises Rieselrauschen flirrt,", "tokens": ["Lei\u00b7ses", "Rie\u00b7sel\u00b7rau\u00b7schen", "flirrt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Frische D\u00fcfte atmenden Lebens", "tokens": ["Fri\u00b7sche", "D\u00fcf\u00b7te", "at\u00b7men\u00b7den", "Le\u00b7bens"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "ADJA", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.9": {"text": "K\u00fchlen herein.", "tokens": ["K\u00fch\u00b7len", "her\u00b7ein", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "PTKVZ", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.10": {"text": "Und ferne, ferne, \u00fcber dem Mosaik", "tokens": ["Und", "fer\u00b7ne", ",", "fer\u00b7ne", ",", "\u00fc\u00b7ber", "dem", "Mo\u00b7saik"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADJA", "$,", "ADJA", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Des langen Kirchendaches (ein Me\u00dfgewand,", "tokens": ["Des", "lan\u00b7gen", "Kir\u00b7chen\u00b7da\u00b7ches", "(", "ein", "Me\u00df\u00b7ge\u00b7wand", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "ART", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "Steif golden hangend von Priesterschultern)", "tokens": ["Steif", "gol\u00b7den", "han\u00b7gend", "von", "Pries\u00b7ter\u00b7schul\u00b7tern", ")"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "VVPP", "APPR", "NN", "$("], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.13": {"text": "Thut lachend ein blaues Himmelsauge", "tokens": ["Thut", "la\u00b7chend", "ein", "blau\u00b7es", "Him\u00b7mel\u00b7sau\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "ADJD", "ART", "ADJA", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.14": {"text": "Sich heiter auf.", "tokens": ["Sich", "hei\u00b7ter", "auf", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.15": {"text": "Fr\u00f6hlichen Lichtes ein kleines, blaues", "tokens": ["Fr\u00f6h\u00b7li\u00b7chen", "Lich\u00b7tes", "ein", "klei\u00b7nes", ",", "blau\u00b7es"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ADJA", "NN", "ART", "ADJA", "$,", "ADJA"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.16": {"text": "Fl\u00e4mmlein, blinzelt es liebensw\u00fcrdig", "tokens": ["Fl\u00e4mm\u00b7lein", ",", "blin\u00b7zelt", "es", "lie\u00b7bens\u00b7w\u00fcr\u00b7dig"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "VVFIN", "PPER", "ADJD"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.17": {"text": "Und ein wenig maliti\u00f6s", "tokens": ["Und", "ein", "we\u00b7nig", "ma\u00b7li\u00b7ti\u00b7\u00f6s"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "PIAT", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.18": {"text": "Ueber das protzige, fromme Dach,", "tokens": ["Ue\u00b7ber", "das", "prot\u00b7zi\u00b7ge", ",", "from\u00b7me", "Dach", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.19": {"text": "Lacht und leuchtet, lacht und leuchtet,", "tokens": ["Lacht", "und", "leuch\u00b7tet", ",", "lacht", "und", "leuch\u00b7tet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "$,", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Und wird gr\u00f6\u00dfer im Lachen und Leuchten,", "tokens": ["Und", "wird", "gr\u00f6\u00b7\u00dfer", "im", "La\u00b7chen", "und", "Leuch\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADJD", "APPRART", "NN", "KON", "NN", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.21": {"text": "Und unerme\u00dfl