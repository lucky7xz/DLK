{"textgrid.poem.39751": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Das Neckarthal bei Canstatt", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zarter Ueberflug von Licht,", "tokens": ["Zar\u00b7ter", "Ue\u00b7ber\u00b7flug", "von", "Licht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Das aus fr\u00fchem Nebel bricht!", "tokens": ["Das", "aus", "fr\u00fc\u00b7hem", "Ne\u00b7bel", "bricht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Welch ein Thal aus fernen Landen", "tokens": ["Welch", "ein", "Thal", "aus", "fer\u00b7nen", "Lan\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ist vor meinem Blick erstanden?", "tokens": ["Ist", "vor", "mei\u00b7nem", "Blick", "er\u00b7stan\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Weiche H\u00fcgel hingestreckt,", "tokens": ["Wei\u00b7che", "H\u00fc\u00b7gel", "hin\u00b7ge\u00b7streckt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Dicht mit Baum und Strauch gedeckt,", "tokens": ["Dicht", "mit", "Baum", "und", "Strauch", "ge\u00b7deckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NE"