"line.4": {"text": "Birget neue Wunderst\u00e4tte.", "tokens": ["Bir\u00b7get", "neu\u00b7e", "Wun\u00b7der\u00b7st\u00e4t\u00b7te", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Aber hell ins Thal hinaus", "tokens": ["A\u00b7ber", "hell", "ins", "Thal", "hin\u00b7aus"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPRART", "NN", "APZR"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Blickt ein heitres S\u00e4ulenhaus,", "tokens": ["Blickt", "ein", "heit\u00b7res", "S\u00e4u\u00b7len\u00b7haus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "L\u00e4dt zu k\u00fchlem Sitz den m\u00fcden", "tokens": ["L\u00e4dt", "zu", "k\u00fch\u00b7lem", "Sitz", "den", "m\u00fc\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "APPR", "ADJA", "NN", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wandrer ein in diesem S\u00fcden.", "tokens": ["Wand\u00b7rer", "ein", "in", "die\u00b7sem", "S\u00fc\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "APPR", "PDAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Ach das Bleiben auf den H\u00f6hn,", "tokens": ["Ach", "das", "Blei\u00b7ben", "auf", "den", "H\u00f6hn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ach das Ziehen ist so sch\u00f6n!", "tokens": ["Ach", "das", "Zie\u00b7hen", "ist", "so", "sch\u00f6n", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ART", "NN", "VAFIN", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Soll ich wandern, soll ich weilen?", "tokens": ["Soll", "ich", "wan\u00b7dern", ",", "soll", "ich", "wei\u00b7len", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVFIN", "$,", "VMFIN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Soll ich ruhen, soll ich eilen?", "tokens": ["Soll", "ich", "ru\u00b7hen", ",", "soll", "ich", "ei\u00b7len", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVINF", "$,", "VMFIN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Doch wie wird mir, ist's kein Traum?", "tokens": ["Doch", "wie", "wird", "mir", ",", "ist's", "kein", "Traum", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "VAFIN", "PPER", "$,", "VAFIN", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Bist du's, trauter Fr\u00fcchtebaum?", "tokens": ["Bist", "du's", ",", "trau\u00b7ter", "Fr\u00fcch\u00b7te\u00b7baum", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Winkst aus wohlbekannter Laube", "tokens": ["Winkst", "aus", "wohl\u00b7be\u00b7kann\u00b7ter", "Lau\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Du mir, heimatliche Traube?", "tokens": ["Du", "mir", ",", "hei\u00b7mat\u00b7li\u00b7che", "Trau\u00b7be", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "PPER", "$,", "ADJA", "NN", "$.