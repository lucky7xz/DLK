{"textgrid.poem.65066": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbdie falsche Welt, sie soll mich nicht beth\u00f6ren", "tokens": ["\u00bb", "die", "fal\u00b7sche", "Welt", ",", "sie", "soll", "mich", "nicht", "be\u00b7th\u00f6\u00b7ren"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["$(", "ART", "ADJA", "NN", "$,", "PPER", "VMFIN", "PPER", "PTKNEG", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wenn sie, von Hohn und Hassesgifte trunken,", "tokens": ["Wenn", "sie", ",", "von", "Hohn", "und", "Has\u00b7ses\u00b7gif\u00b7te", "trun\u00b7ken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "APPR", "NN", "KON", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "In meiner Brust des Glaubens heil'gen Funken", "tokens": ["In", "mei\u00b7ner", "Brust", "des", "Glau\u00b7bens", "heil'\u00b7gen", "Fun\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und deinen Altar will in mir zerst\u00f6ren.", "tokens": ["Und", "dei\u00b7nen", "Al\u00b7tar", "will", "in", "mir", "zer\u00b7st\u00f6\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VMFIN", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Mag sie's mit tausend Eiden mir beschw\u00f6ren,", "tokens": ["Mag", "sie's", "mit", "tau\u00b7send", "Ei\u00b7den", "mir", "be\u00b7schw\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "APPR", "CARD", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df du zu finst'rer Schuld herabgesunken,", "tokens": ["Da\u00df", "du", "zu", "finst'\u00b7rer", "Schuld", "her\u00b7ab\u00b7ge\u00b7sun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Verleumdung ist's! Die\u00df freche Lied der Unken", "tokens": ["Ver\u00b7leum\u00b7dung", "ist's", "!", "Die\u00df", "fre\u00b7che", "Lied", "der", "Un\u00b7ken"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "$.", "PDS", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nicht \u00fcberzeugen kann mich's, nur emp\u00f6ren!", "tokens": ["Nicht", "\u00fc\u00b7berz\u00b7eu\u00b7gen", "kann", "mich's", ",", "nur", "em\u00b7p\u00f6\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "VVINF", "VMFIN", "PIS", "$,", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und f\u00fchlt' ich je zum Zweifel mich getrieben,", "tokens": ["Und", "f\u00fchlt'", "ich", "je", "zum", "Zwei\u00b7fel", "mich", "ge\u00b7trie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPRART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In deines Auges dunkelhellem Scheine", "tokens": ["In", "dei\u00b7nes", "Au\u00b7ges", "dun\u00b7kel\u00b7hel\u00b7lem", "Schei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "F\u00e4nd' L\u00f6sung ich, von Gottes Hand geschrieben.", "tokens": ["F\u00e4nd'", "L\u00f6\u00b7sung", "ich", ",", "von", "Got\u00b7tes", "Hand", "ge\u00b7schrie\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "PPER", "$,", "APPR", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Klar steht dein Bild in w\u00fcrd'gem Tempelschreine,", "tokens": ["Klar", "steht", "dein", "Bild", "in", "w\u00fcrd'\u00b7gem", "Tem\u00b7pel\u00b7schrei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Und wie die Opferflamme loht mein Lieben", "tokens": ["Und", "wie", "die", "Op\u00b7fer\u00b7flam\u00b7me", "loht", "mein", "Lie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "NN", "VVFIN", "PPOSAT", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Zu dir empor, du Makellose, Reine!\u00ab \u2013", "tokens": ["Zu", "dir", "em\u00b7por", ",", "du", "Ma\u00b7kel\u00b7lo\u00b7se", ",", "Rei\u00b7ne", "!", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "punct", "punct"], "pos": ["APPR", "PPER", "PTKVZ", "$,", "PPER", "NE", "$,", "NN", "$.", "$(", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "\u00bbdie falsche Welt, sie soll mich nicht beth\u00f6ren", "tokens": ["\u00bb", "die", "fal\u00b7sche", "Welt", ",", "sie", "soll", "mich", "nicht", "be\u00b7th\u00f6\u00b7ren"], "token_info": ["punct", "word", "word", "word", "punct", "wo