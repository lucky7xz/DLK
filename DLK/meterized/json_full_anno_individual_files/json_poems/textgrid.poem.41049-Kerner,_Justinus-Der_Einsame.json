{"textgrid.poem.41049": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "Der Einsame", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In des Waldes Einsamkeit", "tokens": ["In", "des", "Wal\u00b7des", "Ein\u00b7sam\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Flieh' ich gern zu Baum und Kraut", "tokens": ["Flieh'", "ich", "gern", "zu", "Baum", "und", "Kraut"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "NE", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Tief hinein, wo weit und breit", "tokens": ["Tief", "hin\u00b7ein", ",", "wo", "weit", "und", "breit"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "PTKVZ", "$,", "PWAV", "ADJD", "KON", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Mich kein Menschenauge schaut.", "tokens": ["Mich", "kein", "Men\u00b7schen\u00b7au\u00b7ge", "schaut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Gehet auch ein Reh zum Born,", "tokens": ["Ge\u00b7het", "auch", "ein", "Reh", "zum", "Born", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "APPRART", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Kann mich das bek\u00fcmmern nicht,", "tokens": ["Kann", "mich", "das", "be\u00b7k\u00fcm\u00b7mern", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PDS", "VVFIN", "PTKN