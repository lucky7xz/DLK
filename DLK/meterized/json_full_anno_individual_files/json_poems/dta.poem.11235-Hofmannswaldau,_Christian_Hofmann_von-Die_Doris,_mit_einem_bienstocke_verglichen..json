{"dta.poem.11235": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Die Doris, mit einem bienstocke  \n verglichen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Doris ist ein bienenstock, da viel bienen offt gestecket;", "tokens": ["Do\u00b7ris", "ist", "ein", "bie\u00b7nen\u00b7stock", ",", "da", "viel", "bie\u00b7nen", "offt", "ge\u00b7ste\u00b7cket", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "NN", "$,", "KOUS", "PIS", "VVFIN", "ADV", "VVPP", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Schade! da\u00df die wollust hier l\u00e4ngst den honig abgelecket.", "tokens": ["Scha\u00b7de", "!", "da\u00df", "die", "wol\u00b7lust", "hier", "l\u00e4ngst", "den", "ho\u00b7nig", "ab\u00b7ge\u00b7le\u00b7cket", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$.", "KOUS", "ART", "NN", "ADV", "ADV", "ART", "NN", "VVPP", "$."], "meter": "+--+-+-+-+-+-+-", "measure": "iambic.septa.invert"}}}}}