{"textgrid.poem.60643": {"metadata": {"author": {"name": "La Fontaine, Jean de", "birth": "N.A.", "death": "N.A."}, "title": "1L: Demokratie gefiel dem Fr\u00f6schevolk nicht mehr,", "genre": "verse", "period": "N.A.", "pub_year": 1658, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Demokratie gefiel dem Fr\u00f6schevolk nicht mehr,", "tokens": ["De\u00b7mo\u00b7kra\u00b7tie", "ge\u00b7fiel", "dem", "Fr\u00f6\u00b7sche\u00b7volk", "nicht", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sie schrien und jammerten so sehr,", "tokens": ["Sie", "schri\u00b7en", "und", "jam\u00b7mer\u00b7ten", "so", "sehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "ADV", "ADV", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Da\u00df Zeus im Himmel sie erh\u00f6rte", "tokens": ["Da\u00df", "Zeus", "im", "Him\u00b7mel", "sie", "er\u00b7h\u00f6r\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NE", "APPRART", "NN", "PPER", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und ihnen wunschgem\u00e4\u00df die Monarchie bescherte.", "tokens": ["Und", "ih\u00b7nen", "wunschge\u00b7m\u00e4\u00df", "die", "Mon\u00b7ar\u00b7chie", "be\u00b7scher\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "VVFIN", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Es fiel ein K\u00f6nig, gut und friedevoll, vom Himmel,", "tokens": ["Es", "fiel", "ein", "K\u00f6\u00b7nig", ",", "gut", "und", "frie\u00b7de\u00b7voll", ",", "vom", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ADJD", "KON", "ADJD", "$,", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Doch war der L\u00e4rm, mit dem er sich", "tokens": ["Doch", "war", "der", "L\u00e4rm", ",", "mit", "dem", "er", "sich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "ART", "NN", "$,", "APPR", "PRELS", "PPER", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Im Teiche niederlie\u00df, so arg,", "tokens": ["Im", "Tei\u00b7che", "nie\u00b7der\u00b7lie\u00df", ",", "so", "arg", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Da\u00df unser ganzes Froschgewimmel", "tokens": ["Da\u00df", "un\u00b7ser", "gan\u00b7zes", "Froschge\u00b7wim\u00b7mel"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "T\u00f6richt erschreckt und \u00e4ngstiglich", "tokens": ["T\u00f6\u00b7richt", "er\u00b7schreckt", "und", "\u00e4ngs\u00b7tig\u00b7lich"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVPP", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "In Binsen, Schilf und Uferl\u00f6chern sich verbarg.", "tokens": ["In", "Bin\u00b7sen", ",", "Schilf", "und", "U\u00b7fer\u00b7l\u00f6\u00b7chern", "sich", "ver\u00b7barg", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Die \u00c4rmsten f\u00fcrchteten voll Grauen,", "tokens": ["Die", "\u00c4rms\u00b7ten", "f\u00fcrch\u00b7te\u00b7ten", "voll", "Grau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Dem Herrn von Gottes Gnaden ins Gesicht zu schauen,", "tokens": ["Dem", "Herrn", "von", "Got\u00b7tes", "Gna\u00b7den", "ins", "Ge\u00b7sicht", "zu", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "NN", "APPRART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Sie hielten ihn f\u00fcr einen wilden Riesen gar,", "tokens": ["Sie", "hiel\u00b7ten", "ihn", "f\u00fcr", "ei\u00b7nen", "wil\u00b7den", "Rie\u00b7sen", "gar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ihn \u2013 der doch nur ein Balken war!", "tokens": ["Ihn", "\u2013", "der", "doch", "nur", "ein", "Bal\u00b7ken", "war", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$(", "ART", "ADV", "ADV", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der erste Quaker, der sich, trotzend der Gefahr,", "tokens": ["Der", "ers\u00b7te", "Qua\u00b7ker", ",", "der", "sich", ",", "trot\u00b7zend", "der", "Ge\u00b7fahr", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PRF", "$,", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Aus seinem Loch hervor dem Klotz zu nahen wagte,", "tokens": ["Aus", "sei\u00b7nem", "Loch", "her\u00b7vor", "dem", "Klotz", "zu", "na\u00b7hen", "wag\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "ART", "NN", "APPR", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Erzitterte und zagte.", "tokens": ["Er\u00b7zit\u00b7ter\u00b7te", "und", "zag\u00b7te", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ein zweiter folgte nach und bald die ganze Schar.", "tokens": ["Ein", "zwei\u00b7ter", "folg\u00b7te", "nach", "und", "bald", "die", "gan\u00b7ze", "Schar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "APPR", "KON", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Neugierig wurden sie und schlie\u00dflich so verwegen,", "tokens": ["Neu\u00b7gie\u00b7rig", "wur\u00b7den", "sie", "und", "schlie\u00df\u00b7lich", "so", "ver\u00b7we\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "KON", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Da\u00df sie dem K\u00f6nig auf die Schulter sprangen.", "tokens": ["Da\u00df", "sie", "dem", "K\u00f6\u00b7nig", "auf", "die", "Schul\u00b7ter", "spran\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Der duldet's still und ohne sich zu regen.", "tokens": ["Der", "dul\u00b7det's", "still", "und", "oh\u00b7ne", "sich", "zu", "re\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "PTKVZ", "KON", "APPR", "PRF", "APPR", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Doch bald wird Zeus von neuem angegangen.", "tokens": ["Doch", "bald", "wird", "Zeus", "von", "neu\u00b7em", "an\u00b7ge\u00b7gan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "NE", "APPR", "ADJA", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Die Fr\u00f6sche schreien voll Verlangen:", "tokens": ["Die", "Fr\u00f6\u00b7sche", "schrei\u00b7en", "voll", "Ver\u00b7lan\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "\u00bbo gib uns einen K\u00f6nig, der sich r\u00fchrt!\u00ab", "tokens": ["\u00bb", "o", "gib", "uns", "ei\u00b7nen", "K\u00f6\u00b7nig", ",", "der", "sich", "r\u00fchrt", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "FM", "VVIMP", "PPER", "ART", "NN", "$,", "PRELS", "PRF", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Da setzt die Gottheit einen Kranich in den Teich.", "tokens": ["Da", "setzt", "die", "Got\u00b7theit", "ei\u00b7nen", "Kra\u00b7nich", "in", "den", "Teich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Den hat das V\u00f6lkchen bald gesp\u00fcrt!", "tokens": ["Den", "hat", "das", "V\u00f6lk\u00b7chen", "bald", "ge\u00b7sp\u00fcrt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Er knackt sie, t\u00f6tet sie, verschlingt sie nach Gefallen.", "tokens": ["Er", "knackt", "sie", ",", "t\u00f6\u00b7tet", "sie", ",", "ver\u00b7schlingt", "sie", "nach", "Ge\u00b7fal\u00b7len", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "VVFIN", "PPER", "$,", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Von neuem t\u00f6nen Klagen aus der Fr\u00f6sche Reich.", "tokens": ["Von", "neu\u00b7em", "t\u00f6\u00b7nen", "Kla\u00b7gen", "aus", "der", "Fr\u00f6\u00b7sche", "Reich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Da h\u00f6rt man vom Olymp des Gottes Stimme schallen:", "tokens": ["Da", "h\u00f6rt", "man", "vom", "O\u00b7lymp", "des", "Got\u00b7tes", "Stim\u00b7me", "schal\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "APPRART", "NN", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Ihr h\u00e4ttet, was ihr anfangs hattet, halten sollen!", "tokens": ["Ihr", "h\u00e4t\u00b7tet", ",", "was", "ihr", "an\u00b7fangs", "hat\u00b7tet", ",", "hal\u00b7ten", "sol\u00b7len", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "PWS", "PPER", "ADV", "VAFIN", "$,", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ihr tatet\u2019s nicht; weswegen mu\u00dftet ihr denn grollen", "tokens": ["Ihr", "ta\u00b7tet's", "nicht", ";", "wes\u00b7we\u00b7gen", "mu\u00df\u00b7tet", "ihr", "denn", "grol\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "$.", "PWAV", "VMFIN", "PPER", "ADV", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ob eures ersten K\u00f6nigs, der voll Sanftmut war?", "tokens": ["Ob", "eu\u00b7res", "ers\u00b7ten", "K\u00f6\u00b7nigs", ",", "der", "voll", "Sanft\u00b7mut", "war", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "$,", "PRELS", "ADJD", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Gebt euch mit diesem zweiten nun zufrieden,", "tokens": ["Gebt", "euch", "mit", "die\u00b7sem", "zwei\u00b7ten", "nun", "zu\u00b7frie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "PDAT", "ADJA", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Sonst wird vielleicht ein schlimmrer euch beschieden!\u00ab", "tokens": ["Sonst", "wird", "viel\u00b7leicht", "ein", "schlimm\u00b7rer", "euch", "be\u00b7schie\u00b7den", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ART", "ADJA", "PPER", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Demokratie gefiel dem Fr\u00f6schevolk nicht mehr,", "tokens": ["De\u00b7mo\u00b7kra\u00b7tie", "ge\u00b7fiel", "dem", "Fr\u00f6\u00b7sche\u00b7volk", "nicht", "mehr