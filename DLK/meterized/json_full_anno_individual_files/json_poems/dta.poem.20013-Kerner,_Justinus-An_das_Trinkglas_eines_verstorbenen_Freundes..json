{"dta.poem.20013": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "An das Trinkglas eines verstorbenen Freundes.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905193111", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du herrlich Glas, nun stehst du leer!", "tokens": ["Du", "herr\u00b7lich", "Glas", ",", "nun", "stehst", "du", "leer", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "NN", "$,", "ADV", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Glas, das er oft mit Lust gehoben!", "tokens": ["Glas", ",", "das", "er", "oft", "mit", "Lust", "ge\u00b7ho\u00b7ben", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPER", "ADV", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die Spinne hat rings um dich her", "tokens": ["Die", "Spin\u00b7ne", "hat", "rings", "um", "dich", "her"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADV", "APPR", "PRF", "APZR"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Inde\u00df den d\u00fcstern Flor gewoben.", "tokens": ["In\u00b7de\u00df", "den", "d\u00fcs\u00b7tern", "Flor", "ge\u00b7wo\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Jetzt sollst du mir gef\u00fcllet seyn", "tokens": ["Jetzt", "sollst", "du", "mir", "ge\u00b7f\u00fcl\u00b7let", "seyn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "PPER", "VVPP", "VAINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mondhell mit Gold der deutschen Reben!", "tokens": ["Mond\u00b7hell", "mit", "Gold", "der", "deut\u00b7schen", "Re\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "NN", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "In deiner Tiefe heil'gen Schein", "tokens": ["In", "dei\u00b7ner", "Tie\u00b7fe", "heil'\u00b7gen", "Schein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Schau' ich hinab mit frommem Beben.", "tokens": ["Schau'", "ich", "hin\u00b7ab", "mit", "from\u00b7mem", "Be\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Was ich erschau' in deinem Grund,", "tokens": ["Was", "ich", "er\u00b7schau'", "in", "dei\u00b7nem", "Grund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ist nicht Gew\u00f6hnlichen zu nennen,", "tokens": ["Ist", "nicht", "Ge\u00b7w\u00f6hn\u00b7li\u00b7chen", "zu", "nen\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "NN", "PTKZU", "VVINF", "$,"], "meter": "+--+---+-", "measure": "dactylic.di.plus"}, "line.3": {"text": "Doch wird mir klar zu dieser Stund',", "tokens": ["Doch", "wird", "mir", "klar", "zu", "die\u00b7ser", "Stund'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADJD", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie nichts den Freund vom Freund kann trennen.", "tokens": ["Wie", "nichts", "den", "Freund", "vom", "Freund", "kann", "tren\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ART", "NN", "APPRART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Auf diesen Glauben, Glas so hold!", "tokens": ["Auf", "die\u00b7sen", "Glau\u00b7ben", ",", "Glas", "so", "hold", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Trink' ich dich aus mit hohem Muthe.", "tokens": ["Trink'", "ich", "dich", "aus", "mit", "ho\u00b7hem", "Mu\u00b7the", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Klar spiegelt sich der Sterne Gold,", "tokens": ["Klar", "spie\u00b7gelt", "sich", "der", "Ster\u00b7ne", "Gold", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PRF", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Pokal in deinem theuren Blute.", "tokens": ["Po\u00b7kal", "in", "dei\u00b7nem", "theu\u00b7ren", "Blu\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Still geht der Mond das Thal entlang,", "tokens": ["Still", "geht", "der", "Mond", "das", "Thal", "ent\u00b7lang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ernst t\u00f6nt die mittern\u00e4cht'ge Stunde,", "tokens": ["Ernst", "t\u00f6nt", "die", "mit\u00b7ter\u00b7n\u00e4cht'\u00b7ge", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Leer steht das Glas, der heil'ge Klang", "tokens": ["Leer", "steht", "das", "Glas", ",", "der", "heil'\u00b7ge", "Klang"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "VVFIN", "ART", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "T\u00f6nt nach in dem krystall'nen Grunde.", "tokens": ["T\u00f6nt", "nach", "in", "dem", "kry\u00b7stall'\u00b7nen", "Grun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}