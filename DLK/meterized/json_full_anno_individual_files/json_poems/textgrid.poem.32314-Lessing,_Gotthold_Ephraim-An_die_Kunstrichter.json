{"textgrid.poem.32314": {"metadata": {"author": {"name": "Lessing, Gotthold Ephraim", "birth": "N.A.", "death": "N.A."}, "title": "An die Kunstrichter", "genre": "verse", "period": "N.A.", "pub_year": 1755, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schweigt, unberauschte, finstre Richter!", "tokens": ["Schweigt", ",", "un\u00b7be\u00b7rauschte", ",", "finst\u00b7re", "Rich\u00b7ter", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADJA", "$,", "VVFIN", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Ich trinke Wein, und bin ein Dichter.", "tokens": ["Ich", "trin\u00b7ke", "Wein", ",", "und", "bin", "ein", "Dich\u00b7ter."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "abbreviation"], "pos": ["PPER", "VVFIN", "NN", "$,", "KON", "VAFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Tut mir es nach, und trinket Wein,", "tokens": ["Tut", "mir", "es", "nach", ",", "und", "trin\u00b7ket", "Wein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPER", "PTKVZ", "$,", "KON", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So seht ihr meine Sch\u00f6nheit ein.", "tokens": ["So", "seht", "ihr", "mei\u00b7ne", "Sch\u00f6n\u00b7heit", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sonst wahrlich, unberauschte Richter,", "tokens": ["Sonst", "wahr\u00b7lich", ",", "un\u00b7be\u00b7rauschte", "Rich\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Sonst wahrlich seht ihr sie nicht ein!", "tokens": ["Sonst", "wahr\u00b7lich", "seht", "ihr", "sie", "nicht", "ein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "PPER", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Schweigt, unberauschte, finstre Richter!", "tokens": ["Schweigt", ",", "un\u00b7be\u00b7rauschte", ",", "finst\u00b7re", "Rich\u00b7ter", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADJA", "$,", "VVFIN", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Ich trinke Wein, und bin ein Dichter.", "tokens": ["Ich", "trin\u00b7ke", "Wein", ",", "und", "bin", "ein", "Dich\u00b7ter."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "abbreviation"], "pos": ["PPER", "VVFIN", "NN", "$,", "KON", "VAFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Tut mir es nach, und trinket Wein,", "tokens": ["Tut", "mir", "es", "nach", ",", "und", "trin\u00b7ket", "Wein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPER", "PTKVZ", "$,", "KON", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So seht ihr meine Sch\u00f6nheit ein.", "tokens": ["So", "seht", "ihr", "mei\u00b7ne", "Sch\u00f6n\u00b7heit", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sonst wahrlich, unberauschte Richter,", "tokens": ["Sonst", "wahr\u00b7lich", ",", "un\u00b7be\u00b7rauschte", "Rich\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "