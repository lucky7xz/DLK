{"textgrid.poem.36305": {"metadata": {"author": {"name": "Schlegel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Rheinfahrt", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie k\u00fchn auch andre Quellen sprudeln, brausen,", "tokens": ["Wie", "k\u00fchn", "auch", "and\u00b7re", "Quel\u00b7len", "spru\u00b7deln", ",", "brau\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "ADJD", "ADV", "ADJA", "NN", "VVFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wo sonst die Dichter sch\u00f6ne Weihe tranken,", "tokens": ["Wo", "sonst", "die", "Dich\u00b7ter", "sch\u00f6\u00b7ne", "Wei\u00b7he", "tran\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Den Kunstberg stets anklimmend ohne Wanken,", "tokens": ["Den", "Kunst\u00b7berg", "stets", "an\u00b7klim\u00b7mend", "oh\u00b7ne", "Wan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Bis wo die ewig heitern G\u00f6tter hausen;", "tokens": ["Bis", "wo", "die", "e\u00b7wig", "hei\u00b7tern", "G\u00f6t\u00b7ter", "hau\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAV", "ART", "ADJD", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ich w\u00e4hle dich, o Rhein, der du mit Sausen", "tokens": ["Ich", "w\u00e4h\u00b7le", "dich", ",", "o", "Rhein", ",", "der", "du", "mit", "Sau\u00b7sen"], "to