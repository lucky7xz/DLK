{"textgrid.poem.59480": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Von Engeln und von Bengeln", "genre": "verse", "period": "N.A.", "pub_year": 1822, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im Fr\u00fchling auf gr\u00fcnem H\u00fcgel", "tokens": ["Im", "Fr\u00fch\u00b7ling", "auf", "gr\u00fc\u00b7nem", "H\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "APPR", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Da sa\u00dfen viel Engelein,", "tokens": ["Da", "sa\u00b7\u00dfen", "viel", "En\u00b7ge\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Die putzten sich ihre Fl\u00fcgel", "tokens": ["Die", "putz\u00b7ten", "sich", "ih\u00b7re", "Fl\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PRF", "PPOSAT", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und spielten im Sonnenschein.", "tokens": ["Und", "spiel\u00b7ten", "im", "Son\u00b7nen\u00b7schein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Da kamen St\u00f6rche gezogen,", "tokens": ["Da", "ka\u00b7men", "St\u00f6r\u00b7che", "ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und jeder sich eines nahm,", "tokens": ["Und", "je\u00b7der", "sich", "ei\u00b7nes", "nahm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "PRF", "PIS", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und ist damit fortgeflogen,", "tokens": ["Und", "ist", "da\u00b7mit", "fort\u00b7ge\u00b7flo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PAV", "VVPP", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Bis da\u00df er zu Menschen kam.", "tokens": ["Bis", "da\u00df", "er", "zu", "Men\u00b7schen", "kam", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Und wo er anklopft' bescheiden", "tokens": ["Und", "wo", "er", "an\u00b7klopft'", "be\u00b7schei\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PPER", "VVFIN", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Der kluge Adebar,", "tokens": ["Der", "klu\u00b7ge", "A\u00b7de\u00b7bar", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da war das Haus voller Freuden \u2013", "tokens": ["Da", "war", "das", "Haus", "vol\u00b7ler", "Freu\u00b7den", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "So geht es noch alle Jahr.", "tokens": ["So", "geht", "es", "noch", "al\u00b7le", "Jahr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PIAT", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Die Engel weinten und lachten", "tokens": ["Die", "En\u00b7gel", "wein\u00b7ten", "und", "lach\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und wu\u00dften nicht, wie ihn'n geschehn. \u2013", "tokens": ["Und", "wu\u00df\u00b7ten", "nicht", ",", "wie", "ihn'n", "ge\u00b7schehn", ".", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "$,", "PWAV", "PPER", "VVPP", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die einen doch bald sich bedachten,", "tokens": ["Die", "ei\u00b7nen", "doch", "bald", "sich", "be\u00b7dach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADV", "ADV", "PRF", "VVFIN", "$,"], "meter": "-+-++--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und meinten: das wird wohl gehn!", "tokens": ["Und", "mein\u00b7ten", ":", "das", "wird", "wohl", "gehn", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "PDS", "VAFIN", "ADV", "VVINF", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Die machten bald wichtige Mienen", "tokens": ["Die", "mach\u00b7ten", "bald", "wich\u00b7ti\u00b7ge", "Mie\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ADV", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Und wurden erstaunlich klug,", "tokens": ["Und", "wur\u00b7den", "er\u00b7staun\u00b7lich", "klug", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADJD", "ADJD", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Die Fl\u00fcgel gar unn\u00fctz ihn'n schienen,", "tokens": ["Die", "Fl\u00fc\u00b7gel", "gar", "un\u00b7n\u00fctz", "ihn'n", "schie\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sie sch\u00e4mten sich deren genug.", "tokens": ["Sie", "sch\u00e4m\u00b7ten", "sich", "de\u00b7ren", "ge\u00b7nug", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PDS", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Und mit dem Fl\u00fcgelkleide", "tokens": ["Und", "mit", "dem", "Fl\u00fc\u00b7gel\u00b7klei\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sie lie\u00dfen den Fl\u00fcgelschnack,", "tokens": ["Sie", "lie\u00b7\u00dfen", "den", "Fl\u00fc\u00b7gel\u00b7schnack", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Das war keine kleine Freude:", "tokens": ["Das", "war", "kei\u00b7ne", "klei\u00b7ne", "Freu\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PIAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nun stattlich in Hosen und Frack!", "tokens": ["Nun", "statt\u00b7lich", "in", "Ho\u00b7sen", "und", "Frack", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "NN", "KON", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.7": {"line.1": {"text": "So wurden sie immer gescheuter", "tokens": ["So", "wur\u00b7den", "sie", "im\u00b7mer", "ge\u00b7scheu\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADJA"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Und applizierten sich recht \u2013", "tokens": ["Und", "ap\u00b7pli\u00b7zier\u00b7ten", "sich", "recht", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADJD", "$("], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Das wurden ansehnliche Leute,", "tokens": ["Das", "wur\u00b7den", "an\u00b7sehn\u00b7li\u00b7che", "Leu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADJA", "NN", "$,"], "meter": "-+-++--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Befanden sich gar nicht schlecht.", "tokens": ["Be\u00b7fan\u00b7den", "sich", "gar", "nicht", "schlecht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "PTKNEG", "ADJD", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.8": {"line.1": {"text": "Den andern war's, wenn die Aue", "tokens": ["Den", "an\u00b7dern", "wa\u00b7r's", ",", "wenn", "die", "Au\u00b7e"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "VAFIN", "$,", "KOUS", "ART", "NN"], "meter": "-+--++-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Noch d\u00e4mmert' im Fr\u00fchlingsschein,", "tokens": ["Noch", "d\u00e4m\u00b7mert'", "im", "Fr\u00fch\u00b7lings\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Als z\u00f6ge ein Engel durchs Blaue", "tokens": ["Als", "z\u00f6\u00b7ge", "ein", "En\u00b7gel", "durchs", "Blau\u00b7e"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "VVFIN", "ART", "NN", "APPRART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Und rief' die Gesellen sein.", "tokens": ["Und", "rie\u00b7f'", "die", "Ge\u00b7sel\u00b7len", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Die suchten den alten H\u00fcgel,", "tokens": ["Die", "such\u00b7ten", "den", "al\u00b7ten", "H\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Der lag so hoch und weit \u2013", "tokens": ["Der", "lag", "so", "hoch", "und", "weit", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ADJD", "KON", "ADJD", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und dehnten sehns\u00fcchtig die Fl\u00fcgel", "tokens": ["Und", "dehn\u00b7ten", "sehn\u00b7s\u00fcch\u00b7tig", "die", "Fl\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Mit jeder Fr\u00fchlingszeit.", "tokens": ["Mit", "je\u00b7der", "Fr\u00fch\u00b7lings\u00b7zeit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Die Fl\u00fcgeldecken zersprangen,", "tokens": ["Die", "Fl\u00fc\u00b7gel\u00b7de\u00b7cken", "zer\u00b7spran\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Weit, morgensch\u00f6n strahlt' die Welt,", "tokens": ["Weit", ",", "mor\u00b7gen\u00b7sch\u00f6n", "strahlt'", "die", "Welt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und \u00fcbers Gr\u00fcn sie sich schwangen", "tokens": ["Und", "\u00fc\u00b7bers", "Gr\u00fcn", "sie", "sich", "schwan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "PPER", "PRF", "VVINF"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Bis an das Himmelszelt.", "tokens": ["Bis", "an", "das", "Him\u00b7mels\u00b7zelt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Das fanden sie droben verschlossen,", "tokens": ["Das", "fan\u00b7den", "sie", "dro\u00b7ben", "ver\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Vers\u00e4umten unten die Zeit \u2013", "tokens": ["Ver\u00b7s\u00e4um\u00b7ten", "un\u00b7ten", "die", "Zeit", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$("], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "So irrten die k\u00fchnen Genossen,", "tokens": ["So", "irr\u00b7ten", "die", "k\u00fch\u00b7nen", "Ge\u00b7nos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Verlassen in Lust und Leid. \u2013", "tokens": ["Ver\u00b7las\u00b7sen", "in", "Lust", "und", "Leid", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "$.", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.12": {"line.1": {"text": "Und als es nun kam zum Sterben,", "tokens": ["Und", "als", "es", "nun", "kam", "zum", "Ster\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Gott Vater zur Erden trat,", "tokens": ["Gott", "Va\u00b7ter", "zur", "Er\u00b7den", "trat", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Seine Kinder wieder zu werben,", "tokens": ["Sei\u00b7ne", "Kin\u00b7der", "wie\u00b7der", "zu", "wer\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Die der Storch vertragen hat.", "tokens": ["Die", "der", "Storch", "ver\u00b7tra\u00b7gen", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVFIN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Die einen konnten nicht fliegen,", "tokens": ["Die", "ei\u00b7nen", "konn\u00b7ten", "nicht", "flie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VMFIN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "So wohlleibig, tr\u00e4g und schwer,", "tokens": ["So", "wohl\u00b7lei\u00b7big", ",", "tr\u00e4g", "und", "schwer", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Die mu\u00dft Er da lassen liegen,", "tokens": ["Die", "mu\u00dft", "Er", "da", "las\u00b7sen", "lie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "ADV", "VVINF", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Das tat ihm leid so sehr.", "tokens": ["Das", "tat", "ihm", "leid", "so", "sehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADJD", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Die andern streckten die Schwingen", "tokens": ["Die", "an\u00b7dern", "streck\u00b7ten", "die", "Schwin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "VVFIN", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "In den Morgenglanz hinaus,", "tokens": ["In", "den", "Mor\u00b7gen\u00b7glanz", "hin\u00b7aus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APZR", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und h\u00f6rten die Engel singen,", "tokens": ["Und", "h\u00f6r\u00b7ten", "die", "En\u00b7gel", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und flogen jauchzend nach Haus!", "tokens": ["Und", "flo\u00b7gen", "jauch\u00b7zend", "nach", "Haus", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "APPR", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.15": {"line.1": {"text": "Im Fr\u00fchling auf gr\u00fcnem H\u00fcgel", "tokens": ["Im", "Fr\u00fch\u00b7ling", "auf", "gr\u00fc\u00b7nem", "H\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "APPR", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Da sa\u00dfen viel Engelein,", "tokens": ["Da", "sa\u00b7\u00dfen", "viel", "En\u00b7ge\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Die putzten sich ihre Fl\u00fcgel", "tokens": ["Die", "putz\u00b7ten", "sich", "ih\u00b7re", "Fl\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PRF", "PPOSAT", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und spielten im Sonnenschein.", "tokens": ["Und", "spiel\u00b7ten", "im", "Son\u00b7nen\u00b7schein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.16": {"line.1": {"text": "Da kamen St\u00f6rche gezogen,", "tokens": ["Da", "ka\u00b7men", "St\u00f6r\u00b7che", "ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und jeder sich eines nahm,", "tokens": ["Und", "je\u00b7der", "sich", "ei\u00b7nes", "nahm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "PRF", "PIS", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und ist damit fortgeflogen,", "tokens": ["Und", "ist", "da\u00b7mit", "fort\u00b7ge\u00b7flo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PAV", "VVPP", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Bis da\u00df er zu Menschen kam.", "tokens": ["Bis", "da\u00df", "er", "zu", "Men\u00b7schen", "kam", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.17": {"line.1": {"text": "Und wo er anklopft' bescheiden", "tokens": ["Und", "wo", "er", "an\u00b7klopft'", "be\u00b7schei\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PPER", "VVFIN", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Der kluge Adebar,", "tokens": ["Der", "klu\u00b7ge", "A\u00b7de\u00b7bar", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da war das Haus voller Freuden \u2013", "tokens": ["Da", "war", "das", "Haus", "vol\u00b7ler", "Freu\u00b7den", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "So geht es noch alle Jahr.", "tokens": ["So", "geht", "es", "noch", "al\u00b7le", "Jahr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PIAT", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.18": {"line.1": {"text": "Die Engel weinten und lachten", "tokens": ["Die", "En\u00b7gel", "wein\u00b7ten", "und", "lach\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und wu\u00dften nicht, wie ihn'n geschehn. \u2013", "tokens": ["Und", "wu\u00df\u00b7ten", "nicht", ",", "wie", "ihn'n", "ge\u00b7schehn", ".", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "$,", "PWAV", "PPER", "VVPP", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die einen doch bald sich bedachten,", "tokens": ["Die", "ei\u00b7nen", "doch", "bald", "sich", "be\u00b7dach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADV", "ADV", "PRF", "VVFIN", "$,"], "meter": "-+-++--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und meinten: das wird wohl gehn!", "tokens": ["Und", "mein\u00b7ten", ":", "das", "wird", "wohl", "gehn", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "PDS", "VAFIN", "ADV", "VVINF", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.19": {"line.1": {"text": "Die machten bald wichtige Mienen", "tokens": ["Die", "mach\u00b7ten", "bald", "wich\u00b7ti\u00b7ge", "Mie\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ADV", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Und wurden erstaunlich klug,", "tokens": ["Und", "wur\u00b7den", "er\u00b7staun\u00b7lich", "klug", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADJD", "ADJD", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Die Fl\u00fcgel gar unn\u00fctz ihn'n schienen,", "tokens": ["Die", "Fl\u00fc\u00b7gel", "gar", "un\u00b7n\u00fctz", "ihn'n", "schie\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sie sch\u00e4mten sich deren genug.", "tokens": ["Sie", "sch\u00e4m\u00b7ten", "sich", "de\u00b7ren", "ge\u00b7nug", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PDS", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.20": {"line.1": {"text": "Und mit dem Fl\u00fcgelkleide", "tokens": ["Und", "mit", "dem", "Fl\u00fc\u00b7gel\u00b7klei\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sie lie\u00dfen den Fl\u00fcgelschnack,", "tokens": ["Sie", "lie\u00b7\u00dfen", "den", "Fl\u00fc\u00b7gel\u00b7schnack", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Das war keine kleine Freude:", "tokens": ["Das", "war", "kei\u00b7ne", "klei\u00b7ne", "Freu\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PIAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nun stattlich in Hosen und Frack!", "tokens": ["Nun", "statt\u00b7lich", "in", "Ho\u00b7sen", "und", "Frack", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "NN", "KON", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.21": {"line.1": {"text": "So wurden sie immer gescheuter", "tokens": ["So", "wur\u00b7den", "sie", "im\u00b7mer", "ge\u00b7scheu\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADJA"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Und applizierten sich recht \u2013", "tokens": ["Und", "ap\u00b7pli\u00b7zier\u00b7ten", "sich", "recht", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADJD", "$("], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Das wurden ansehnliche Leute,", "tokens": ["Das", "wur\u00b7den", "an\u00b7sehn\u00b7li\u00b7che", "Leu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADJA", "NN", "$,"], "meter": "-+-++--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Befanden sich gar nicht schlecht.", "tokens": ["Be\u00b7fan\u00b7den", "sich", "gar", "nicht", "schlecht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "PTKNEG", "ADJD", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.22": {"line.1": {"text": "Den andern war's, wenn die Aue", "tokens": ["Den", "an\u00b7dern", "wa\u00b7r's", ",", "wenn", "die", "Au\u00b7e"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "VAFIN", "$,", "KOUS", "ART", "NN"], "meter": "-+--++-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Noch d\u00e4mmert' im Fr\u00fchlingsschein,", "tokens": ["Noch", "d\u00e4m\u00b7mert'", "im", "Fr\u00fch\u00b7lings\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Als z\u00f6ge ein Engel durchs Blaue", "tokens": ["Als", "z\u00f6\u00b7ge", "ein", "En\u00b7gel", "durchs", "Blau\u00b7e"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "VVFIN", "ART", "NN", "APPRART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Und rief' die Gesellen sein.", "tokens": ["Und", "rie\u00b7f'", "die", "Ge\u00b7sel\u00b7len", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.23": {"line.1": {"text": "Die suchten den alten H\u00fcgel,", "tokens": ["Die", "such\u00b7ten", "den", "al\u00b7ten", "H\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Der lag so hoch und weit \u2013", "tokens": ["Der", "lag", "so", "hoch", "und", "weit", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ADJD", "KON", "ADJD", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und dehnten sehns\u00fcchtig die Fl\u00fcgel", "tokens": ["Und", "dehn\u00b7ten", "sehn\u00b7s\u00fcch\u00b7tig", "die", "Fl\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Mit jeder Fr\u00fchlingszeit.", "tokens": ["Mit", "je\u00b7der", "Fr\u00fch\u00b7lings\u00b7zeit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.24": {"line.1": {"text": "Die Fl\u00fcgeldecken zersprangen,", "tokens": ["Die", "Fl\u00fc\u00b7gel\u00b7de\u00b7cken", "zer\u00b7spran\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Weit, morgensch\u00f6n strahlt' die Welt,", "tokens": ["Weit", ",", "mor\u00b7gen\u00b7sch\u00f6n", "strahlt'", "die", "Welt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und \u00fcbers Gr\u00fcn sie sich schwangen", "tokens": ["Und", "\u00fc\u00b7bers", "Gr\u00fcn", "sie", "sich", "schwan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "PPER", "PRF", "VVINF"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Bis an das Himmelszelt.", "tokens": ["Bis", "an", "das", "Him\u00b7mels\u00b7zelt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.25": {"line.1": {"text": "Das fanden sie droben verschlossen,", "tokens": ["Das", "fan\u00b7den", "sie", "dro\u00b7ben", "ver\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Vers\u00e4umten unten die Zeit \u2013", "tokens": ["Ver\u00b7s\u00e4um\u00b7ten", "un\u00b7ten", "die", "Zeit", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$("], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "So irrten die k\u00fchnen Genossen,", "tokens": ["So", "irr\u00b7ten", "die", "k\u00fch\u00b7nen", "Ge\u00b7nos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Verlassen in Lust und Leid. \u2013", "tokens": ["Ver\u00b7las\u00b7sen", "in", "Lust", "und", "Leid", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "$.", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.26": {"line.1": {"text": "Und als es nun kam zum Sterben,", "tokens": ["Und", "als", "es", "nun", "kam", "zum", "Ster\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Gott Vater zur Erden trat,", "tokens": ["Gott", "Va\u00b7ter", "zur", "Er\u00b7den", "trat", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Seine Kinder wieder zu werben,", "tokens": ["Sei\u00b7ne", "Kin\u00b7der", "wie\u00b7der", "zu", "wer\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Die der Storch vertragen hat.", "tokens": ["Die", "der", "Storch", "ver\u00b7tra\u00b7gen", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVFIN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.27": {"line.1": {"text": "Die einen konnten nicht fliegen,", "tokens": ["Die", "ei\u00b7nen", "konn\u00b7ten", "nicht", "flie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VMFIN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "So wohlleibig, tr\u00e4g und schwer,", "tokens": ["So", "wohl\u00b7lei\u00b7big", ",", "tr\u00e4g", "und", "schwer", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Die mu\u00dft Er da lassen liegen,", "tokens": ["Die", "mu\u00dft", "Er", "da", "las\u00b7sen", "lie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "ADV", "VVINF", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Das tat ihm leid so sehr.", "tokens": ["Das", "tat", "ihm", "leid", "so", "sehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADJD", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.28": {"line.1": {"text": "Die andern streckten die Schwingen", "tokens": ["Die", "an\u00b7dern", "streck\u00b7ten", "die", "Schwin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "VVFIN", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "In den Morgenglanz hinaus,", "tokens": ["In", "den", "Mor\u00b7gen\u00b7glanz", "hin\u00b7aus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APZR", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und h\u00f6rten die Engel singen,", "tokens": ["Und", "h\u00f6r\u00b7ten", "die", "En\u00b7gel", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und flogen jauchzend nach Haus!", "tokens": ["Und", "flo\u00b7gen", "jauch\u00b7zend", "nach", "Haus", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "APPR", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}}}}