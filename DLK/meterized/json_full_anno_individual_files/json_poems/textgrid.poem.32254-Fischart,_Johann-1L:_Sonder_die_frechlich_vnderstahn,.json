{"textgrid.poem.32254": {"metadata": {"author": {"name": "Fischart, Johann", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sonder die frechlich vnderstahn,", "genre": "verse", "period": "N.A.", "pub_year": 1568, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sonder die frechlich vnderstahn,", "tokens": ["Son\u00b7der", "die", "frech\u00b7lich", "vn\u00b7der\u00b7stahn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJD", "VVINF", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Sich wider gsatz vnd ohn all wal", "tokens": ["Sich", "wi\u00b7der", "gsatz", "vnd", "ohn", "all", "wal"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PRF", "APPR", "NE", "KON", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zustecken in gesch\u00e4fft vberal,", "tokens": ["Zu\u00b7ste\u00b7cken", "in", "ge\u00b7sch\u00e4fft", "vbe\u00b7ral", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den, sag ich, soll man widerstahn,", "tokens": ["Den", ",", "sag", "ich", ",", "soll", "man", "wi\u00b7der\u00b7stahn", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "PPER", "$,", "VMFIN", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Weil jhn der gewalt nicht zu wil stahn.", "tokens": ["Weil", "jhn", "der", "ge\u00b7walt", "nicht", "zu", "wil", "stahn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PTKNEG", "PTKA", "ADJD", "VVFIN", "$."], "meter": "-+--+-++-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Darumb nur, jr Frantzosen, d