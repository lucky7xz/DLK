{"textgrid.poem.49660": {"metadata": {"author": {"name": "Thoma, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "F\u00fcrstenreifen", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das ist ein aufgeregtes Jahr,", "tokens": ["Das", "ist", "ein", "auf\u00b7ge\u00b7reg\u00b7tes", "Jahr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und so bewegt wie keines war,", "tokens": ["Und", "so", "be\u00b7wegt", "wie", "kei\u00b7nes", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "KOKOM", "PIS", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So reich an Neuigkeiten.", "tokens": ["So", "reich", "an", "Neu\u00b7ig\u00b7kei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Man h\u00f6rt bald hier, man h\u00f6rt bald dort:", "tokens": ["Man", "h\u00f6rt", "bald", "hier", ",", "man", "h\u00f6rt", "bald", "dort", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "ADV", "$,", "PIS", "VVFIN", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ein F\u00fcrst kam an, ein F\u00fcrst ging fort,", "tokens": ["Ein", "F\u00fcrst", "kam", "an", ",", "ein", "F\u00fcrst", "ging", "fort", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,", "ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dem sie ein Fest bereiten