{"dta.poem.13442": {"metadata": {"author": {"name": "Suppius, Christoph Eusebius", "birth": "N.A.", "death": "N.A."}, "title": "An  \n Jhro Hochf\u00fcrstl. Durchlaucht  \n die Frau Herzogin zu Sachsen-  \n Gotha und Altenburg.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1749", "urn": "urn:nbn:de:kobv:b4-20594-7", "language": ["de:0.99"], "booktitle": "Suppius, Christoph Eusebius: Oden und Lieder. Gotha, 1749."}, "poem": {"stanza.1": {"line.1": {"text": "G\u00f6ttin der Liebe! die man hier verehret,", "tokens": ["G\u00f6t\u00b7tin", "der", "Lie\u00b7be", "!", "die", "man", "hier", "ver\u00b7eh\u00b7ret", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$.", "ART", "PIS", "ADV", "VVFIN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Hast du bis itzo mich noch nicht erh\u00f6ret,", "tokens": ["Hast", "du", "bis", "it\u00b7zo", "mich", "noch", "nicht", "er\u00b7h\u00f6\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "PPER", "ADV", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wenn ich voll Inbrunst, so, wie Dichter beten,", "tokens": ["Wenn", "ich", "voll", "In\u00b7brunst", ",", "so", ",", "wie", "Dich\u00b7ter", "be\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "NN", "$,", "ADV", "$,", "PWAV", "NN", "VVINF", "$,"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "O! so sey die\u00dfmahl doch dein Ohr geneiget!", "tokens": ["O", "!", "so", "sey", "die\u00df\u00b7mahl", "doch", "dein", "Ohr", "ge\u00b7nei\u00b7get", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "ADV", "VAFIN", "ADV", "ADV", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.2": {"text": "Da ein Heer Seufzer zu Dir aufw\u00e4rts steiget,", "tokens": ["Da", "ein", "Heer", "Seuf\u00b7zer", "zu", "Dir", "auf\u00b7w\u00e4rts", "stei\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "NN", "APPR", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Noth f\u00fchrt solches, und ihm folgt der Jammer", "tokens": ["Die", "Noth", "f\u00fchrt", "sol\u00b7ches", ",", "und", "ihm", "folgt", "der", "Jam\u00b7mer"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PIS", "$,", "KON", "PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Da ist nie jemand traurig weggegangen,", "tokens": ["Da", "ist", "nie", "je\u00b7mand", "trau\u00b7rig", "weg\u00b7ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "PIS", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dein mildes Herze brennt ja vor Verlangen,", "tokens": ["Dein", "mil\u00b7des", "Her\u00b7ze", "brennt", "ja", "vor", "Ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Um es mit denen m\u00fctterlich zu meynen", "tokens": ["Um", "es", "mit", "de\u00b7nen", "m\u00fct\u00b7ter\u00b7lich", "zu", "mey\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "PPER", "APPR", "PRELS", "ADJD", "PTKZU", "VVINF"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Nicht etwa Reichthum sucht mein d\u00fcrftig Gl\u00fccke,", "tokens": ["Nicht", "et\u00b7wa", "Reicht\u00b7hum", "sucht", "mein", "d\u00fcrf\u00b7tig", "Gl\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "NN", "VVFIN", "PPOSAT", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nicht, da\u00df ich vor Dir mich um Ehre b\u00fccke,", "tokens": ["Nicht", ",", "da\u00df", "ich", "vor", "Dir", "mich", "um", "Eh\u00b7re", "b\u00fc\u00b7cke", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "$,", "KOUS", "PPER", "APPR", "PPER", "PRF", "APPR", "NN", "VVFIN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Oder aus Wohllust um beqveme Tage", "tokens": ["O\u00b7der", "aus", "Wohl\u00b7lust", "um", "be\u00b7qve\u00b7me", "Ta\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "APPR", "ADJA", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}, "stanza.5": {"line.1": {"text": "Nein! grosse G\u00fcter sind gar sehr beschwerlich,", "tokens": ["Nein", "!", "gros\u00b7se", "G\u00fc\u00b7ter", "sind", "gar", "sehr", "be\u00b7schwer\u00b7lich", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "ADJA", "NN", "VAFIN", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Pl\u00f6tzliches Steigen ist dem Geist gef\u00e4hrlich,", "tokens": ["Pl\u00f6tz\u00b7li\u00b7ches", "Stei\u00b7gen", "ist", "dem", "Geist", "ge\u00b7f\u00e4hr\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "ART", "NN", "ADJD", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Dem Bauche dienen sch\u00e4dliches Gesch\u00e4fte,", "tokens": ["Dem", "Bau\u00b7che", "die\u00b7nen", "sch\u00e4d\u00b7li\u00b7ches", "Ge\u00b7sch\u00e4f\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Ach! unumschr\u00e4nkte K\u00f6nigin der Herzen,", "tokens": ["Ach", "!", "un\u00b7um\u00b7schr\u00e4nk\u00b7te", "K\u00f6\u00b7ni\u00b7gin", "der", "Her\u00b7zen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Um welche Gnade, Huld, Erbarmen scherzen,", "tokens": ["Um", "wel\u00b7che", "Gna\u00b7de", ",", "Huld", ",", "Er\u00b7bar\u00b7men", "scher\u00b7zen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PWAT", "NN", "$,", "NN", "$,", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Wissenschaften Schmuck und sch\u00f6ne Krone,", "tokens": ["Der", "Wis\u00b7sen\u00b7schaf\u00b7ten", "Schmuck", "und", "sch\u00f6\u00b7ne", "Kro\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Oder daferne dein g\u00f6ttlich Bezeigen", "tokens": ["O\u00b7der", "da\u00b7fer\u00b7ne", "dein", "g\u00f6tt\u00b7lich", "Be\u00b7zei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "PPOSAT", "ADJD", "NN"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Lieber den Zepter auf mich denkt zu neigen,", "tokens": ["Lie\u00b7ber", "den", "Zep\u00b7ter", "auf", "mich", "denkt", "zu", "nei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "APPR", "PPER", "VVFIN", "PTKZU", "VVFIN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Damit zum voraus deine Gnade lehret,", "tokens": ["Da\u00b7mit", "zum", "vo\u00b7raus", "dei\u00b7ne", "Gna\u00b7de", "leh\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "APPRART", "ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "O! so verleihe, da\u00df von deinen Blicken", "tokens": ["O", "!", "so", "ver\u00b7lei\u00b7he", ",", "da\u00df", "von", "dei\u00b7nen", "Bli\u00b7cken"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$.", "ADV", "VVFIN", "$,", "KOUS", "APPR", "PPOSAT", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Mich nicht der wilde Mangel darf entr\u00fccken,", "tokens": ["Mich", "nicht", "der", "wil\u00b7de", "Man\u00b7gel", "darf", "ent\u00b7r\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "ART", "ADJA", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sondern da\u00df ich kann hier in g\u00fcldnen Auen", "tokens": ["Son\u00b7dern", "da\u00df", "ich", "kann", "hier", "in", "g\u00fcld\u00b7nen", "Au\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "VMFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}, "stanza.9": {"line.1": {"text": "In dem Besitze meiner braunen Doris,", "tokens": ["In", "dem", "Be\u00b7sit\u00b7ze", "mei\u00b7ner", "brau\u00b7nen", "Do\u00b7ris", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPOSAT", "ADJA", "NE", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Oder in Armen einer blonden Chloris,", "tokens": ["O\u00b7der", "in", "Ar\u00b7men", "ei\u00b7ner", "blon\u00b7den", "Chlo\u00b7ris", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "ART", "ADJA", "NE", "$,"], "meter": "+--+-+-+---", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Und wie ich etwa sonst die Dichtkunst nenne,", "tokens": ["Und", "wie", "ich", "et\u00b7wa", "sonst", "die", "Dicht\u00b7kunst", "nen\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ADV", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Sie hat bishero bey den stillen Plagen", "tokens": ["Sie", "hat", "bis\u00b7he\u00b7ro", "bey", "den", "stil\u00b7len", "Pla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mir ganz geduldig heimlich helfen klagen,", "tokens": ["Mir", "ganz", "ge\u00b7dul\u00b7dig", "heim\u00b7lich", "hel\u00b7fen", "kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJD", "ADJD", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ja mich getr\u00f6stet mitten im Herzeleid", "tokens": ["Ja", "mich", "ge\u00b7tr\u00f6s\u00b7tet", "mit\u00b7ten", "im", "Her\u00b7ze\u00b7leid"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "PPER", "VVPP", "ADV", "APPRART", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}}}}}