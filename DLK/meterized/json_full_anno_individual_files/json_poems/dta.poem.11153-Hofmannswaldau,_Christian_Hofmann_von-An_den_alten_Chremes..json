{"dta.poem.11153": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "An den alten Chremes.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Man fragt dich immerfort: Wenn wird die tochter freyn?", "tokens": ["Man", "fragt", "dich", "im\u00b7mer\u00b7fort", ":", "Wenn", "wird", "die", "toch\u00b7ter", "freyn", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "ADV", "$.", "KOUS", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du sprichst: Es ist schon zeit, wenn sie wird mannbar", "tokens": ["Du", "sprichst", ":", "Es", "ist", "schon", "zeit", ",", "wenn", "sie", "wird", "mann\u00b7bar"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "PPER", "VAFIN", "ADV", "NN", "$,", "KOUS", "PPER", "VAFIN", "ADJD"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "seyn.", "tokens": ["seyn", "."], "token_info": ["word", "punct"], "pos": ["VAINF", "$."], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Du narr! sie straft dich selbst mit worten und geb\u00e4rden;", "tokens": ["Du", "narr", "!", "sie", "straft", "dich", "selbst", "mit", "wor\u00b7ten", "und", "ge\u00b7b\u00e4r\u00b7den", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "$.", "PPER", "VVFIN", "PPER", "ADV", "APPR", "NN", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Gieb ihr nur einen mann, sie wird schon mannbar werden.", "tokens": ["Gieb", "ihr", "nur", "ei\u00b7nen", "mann", ",", "sie", "wird", "schon", "mann\u00b7bar", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "ART", "NN", "$,", "PPER", "VAFIN", "ADV", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}