{"textgrid.poem.59332": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Fr\u00fchlingsnacht", "genre": "verse", "period": "N.A.", "pub_year": 1822, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00dcbern Garten durch die L\u00fcfte", "tokens": ["\u00dc\u00b7bern", "Gar\u00b7ten", "durch", "die", "L\u00fcf\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "H\u00f6rt ich Wanderv\u00f6gel ziehn,", "tokens": ["H\u00f6rt", "ich", "Wan\u00b7der\u00b7v\u00f6\u00b7gel", "ziehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "NN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Das bedeutet Fr\u00fchlingsd\u00fcfte,", "tokens": ["Das", "be\u00b7deu\u00b7tet", "Fr\u00fch\u00b7lings\u00b7d\u00fcf\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Unten f\u00e4ngt's schon an zu bl\u00fchn.", "tokens": ["Un\u00b7ten", "f\u00e4ngt's", "schon", "an", "zu", "bl\u00fchn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "APPR", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Jauchzen m\u00f6cht ich, m\u00f6chte weinen,", "tokens": ["Jauch\u00b7zen", "m\u00f6cht", "ich", ",", "m\u00f6ch\u00b7te", "wei\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "$,", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist mir's doch, als k\u00f6nnt's nicht sein!", "tokens": ["Ist", "mir's", "doch", ",", "als", "k\u00f6nnt's", "nicht", "sein", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NE", "ADV", "$,", "KOUS", "NE", "PTKNEG", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Alte Wunder wieder scheinen", "tokens": ["Al\u00b7te", "Wun\u00b7der", "wie\u00b7der", "schei\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mit dem Mondesglanz herein.", "tokens": ["Mit", "dem", "Mon\u00b7des\u00b7glanz", "her\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Und der Mond, die Sterne sagen's,", "tokens": ["Und", "der", "Mond", ",", "die", "Ster\u00b7ne", "sa\u00b7gen's", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Und in Tr\u00e4umen rauscht's der Hain,", "tokens": ["Und", "in", "Tr\u00e4u\u00b7men", "rauscht's", "der", "Hain", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und die Nachtigallen schlagen's:", "tokens": ["Und", "die", "Nach\u00b7ti\u00b7gal\u00b7len", "schla\u00b7gen's", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Sie ist Deine, sie ist dein!", "tokens": ["Sie", "ist", "Dei\u00b7ne", ",", "sie", "ist", "dein", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "