{"dta.poem.19188": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "35.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Am Stromesufer steht erschwungen eine Palme", "tokens": ["Am", "Stro\u00b7me\u00b7su\u00b7fer", "steht", "er\u00b7schwun\u00b7gen", "ei\u00b7ne", "Pal\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hoch ob der D\u00fcnste Kreis und erdenahem Qualme,", "tokens": ["Hoch", "ob", "der", "D\u00fcns\u00b7te", "Kreis", "und", "er\u00b7de\u00b7na\u00b7hem", "Qual\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "ART", "ADJA", "NN", "KON", "ADJA", "NN", "$,"], "meter": "---+-+-+-+-+-", "measure": "unknown.measure.penta"}}, "stanza.2": {"line.1": {"text": "So hoch, da\u00df Menschenwitz nicht ihre Kron' entbl\u00e4ttert,", "tokens": ["So", "hoch", ",", "da\u00df", "Men\u00b7schen\u00b7witz", "nicht", "ih\u00b7re", "Kron'", "ent\u00b7bl\u00e4t\u00b7tert", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "KOUS", "NE", "PTKNEG", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So glatt, da\u00df Affenkunst nicht ihren Stamm erklettert.", "tokens": ["So", "glatt", ",", "da\u00df", "Af\u00b7fen\u00b7kunst", "nicht", "ih\u00b7ren", "Stamm", "er\u00b7klet\u00b7tert", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "KOUS", "NN", "PTKNEG", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Die reifen Fr\u00fcchte wirft sie aus der Luft hinab", "tokens": ["Die", "rei\u00b7fen", "Fr\u00fcch\u00b7te", "wirft", "sie", "aus", "der", "Luft", "hin\u00b7ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "PTKVZ"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ins Wasser, welches ihr dazu die Nahrung gab.", "tokens": ["Ins", "Was\u00b7ser", ",", "wel\u00b7ches", "ihr", "da\u00b7zu", "die", "Nah\u00b7rung", "gab", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PRELS", "PPER", "PAV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Die Fr\u00fcchte, wann sie sind den Strom hinab geschwommen,", "tokens": ["Die", "Fr\u00fcch\u00b7te", ",", "wann", "sie", "sind", "den", "Strom", "hin\u00b7ab", "ge\u00b7schwom\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "PPER", "VAFIN", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Schwimmen dort an ein Land, wo Palmen nicht bekommen.", "tokens": ["Schwim\u00b7men", "dort", "an", "ein", "Land", ",", "wo", "Pal\u00b7men", "nicht", "be\u00b7kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "ART", "NN", "$,", "PWAV", "NN", "PTKNEG", "VVINF", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}}, "stanza.5": {"line.1": {"text": "Willkommen sind sie dort, die Gaben aus der Ferne,", "tokens": ["Will\u00b7kom\u00b7men", "sind", "sie", "dort", ",", "die", "Ga\u00b7ben", "aus", "der", "Fer\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ADV", "$,", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Menschen essen sie und sammeln ihre Kerne.", "tokens": ["Die", "Men\u00b7schen", "es\u00b7sen", "sie", "und", "sam\u00b7meln", "ih\u00b7re", "Ker\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Sie z\u00f6gen aus dem Kern selbst eine Palme gern,", "tokens": ["Sie", "z\u00f6\u00b7gen", "aus", "dem", "Kern", "selbst", "ei\u00b7ne", "Pal\u00b7me", "gern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ADV", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch Erd' und Himmel ist dagegen, Sonn' und Stern.", "tokens": ["Doch", "Erd'", "und", "Him\u00b7mel", "ist", "da\u00b7ge\u00b7gen", ",", "Sonn'", "und", "Stern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "VAFIN", "PAV", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}