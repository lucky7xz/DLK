{"dta.poem.9123": {"metadata": {"author": {"name": "Weise, Christian", "birth": "N.A.", "death": "N.A."}, "title": "I.  \n Thr\u00e4nen der jungferschafft.", "genre": "Lyrik; Drama; Prosa", "period": "N.A.", "pub_year": "1701", "urn": "urn:nbn:de:kobv:b4-25043-0", "language": ["de:0.99"], "booktitle": "Weise, Christian: \u00dcberfl\u00fc\u00dfige Gedancken Der gr\u00fcnenden jugend. Leipzig, 1701."}, "poem": {"stanza.1": {"line.1": {"text": "S\u00fcsser gifft verliebter hertzen/", "tokens": ["S\u00fcs\u00b7ser", "gifft", "ver\u00b7lieb\u00b7ter", "hert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schwaches werck-zeug voller krafft/", "tokens": ["Schwa\u00b7ches", "wer\u00b7ck\u00b7zeug", "vol\u00b7ler", "krafft", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJA", "NN", "$("], "meter": "+-+----+", "measure": "unknown.measure.tri"}, "line.3": {"text": "Werthes ziel der keuschen schmertzen/", "tokens": ["Wert\u00b7hes", "ziel", "der", "keu\u00b7schen", "schmert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "ADJA", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Du ber\u00fchmte jungferschafft!", "tokens": ["Du", "be\u00b7r\u00fchm\u00b7te", "jung\u00b7fer\u00b7schafft", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Freylich gehet deine zier", "tokens": ["Frey\u00b7lich", "ge\u00b7het", "dei\u00b7ne", "zier"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Allen sch\u00f6nen sachen f\u00fcr.", "tokens": ["Al\u00b7len", "sch\u00f6\u00b7nen", "sa\u00b7chen", "f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VVINF", "VVFIN", "APPR", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "2. Wie die rosen in dem Meyen", "tokens": ["Wie", "die", "ro\u00b7sen", "in", "dem", "Me\u00b7yen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-++-+", "measure": "unknown.measure.penta"}, "line.2": {"text": "Jhre bleiche lieblichkeit", "tokens": ["Ih\u00b7re", "blei\u00b7che", "lieb\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Niemals sch\u00f6ner von sich streuen/", "tokens": ["Nie\u00b7mals", "sch\u00f6\u00b7ner", "von", "sich", "streu\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "PRF", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Als wenn ihre sicherheit", "tokens": ["Als", "wenn", "ih\u00b7re", "si\u00b7cher\u00b7heit"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Unber\u00fchrt und unbefleckt", "tokens": ["Un\u00b7be\u00b7r\u00fchrt", "und", "un\u00b7be\u00b7fleckt"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "KON", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "In dem gr\u00fcnen stocke steckt.", "tokens": ["In", "dem", "gr\u00fc\u00b7nen", "sto\u00b7cke", "steckt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "3. Also mu\u00df man dich erheben/", "tokens": ["Al\u00b7so", "mu\u00df", "man", "dich", "er\u00b7he\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIS", "PRF", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Weil du keiner frembden hand", "tokens": ["Weil", "du", "kei\u00b7ner", "fremb\u00b7den", "hand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PIAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dich zum raube wilst ergeben/", "tokens": ["Dich", "zum", "rau\u00b7be", "wilst", "er\u00b7ge\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "VMFIN", "VVPP", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sondern das beliebte pfand", "tokens": ["Son\u00b7dern", "das", "be\u00b7lieb\u00b7te", "pfand"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Aller ruh und lebens rast", "tokens": ["Al\u00b7ler", "ruh", "und", "le\u00b7bens", "rast"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "KON", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "An der s\u00fcssen freyheit hast.", "tokens": ["An", "der", "s\u00fcs\u00b7sen", "frey\u00b7heit", "hast", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "4. Du ergetzst dich an der jugend/", "tokens": ["Du", "er\u00b7getz\u00b7st", "dich", "an", "der", "ju\u00b7gend", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "NN", "$("], "meter": "---+-+-+-", "measure": "unknown.measure.tri"}, "line.2": {"text": "Bist also an dir vergn\u00fcgt/", "tokens": ["Bist", "al\u00b7so", "an", "dir", "ver\u00b7gn\u00fcgt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "PPER", "VVPP", "$("], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Und gebrauchst dich deiner tugend/", "tokens": ["Und", "ge\u00b7brauchst", "dich", "dei\u00b7ner", "tu\u00b7gend", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPOSAT", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Welche dir im hertzen liegt/", "tokens": ["Wel\u00b7che", "dir", "im", "hert\u00b7zen", "liegt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPRART", "NN", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Da sie auch die beste frucht/", "tokens": ["Da", "sie", "auch", "die", "bes\u00b7te", "frucht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "ADJA", "NN", "$("], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.6": {"text": "An der zarten keuschheit sucht.", "tokens": ["An", "der", "zar\u00b7ten", "keuschheit", "sucht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}}, "stanza.5": {"line.1": {"text": "5. Doch wie lange kan es w\u00e4ren?", "tokens": ["Doch", "wie", "lan\u00b7ge", "kan", "es", "w\u00e4\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ADV", "VMFIN", "PPER", "VAFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Endlich mu\u00df die jugend sich", "tokens": ["End\u00b7lich", "mu\u00df", "die", "ju\u00b7gend", "sich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ART", "NN", "PRF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Durch den schnellen lauff verzehren/", "tokens": ["Durch", "den", "schnel\u00b7len", "lauff", "ver\u00b7zeh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Oder es beruffet dich", "tokens": ["O\u00b7der", "es", "be\u00b7ruf\u00b7fet", "dich"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Liebe/ lust und eitelkeit", "tokens": ["Lie\u00b7be", "/", "lust", "und", "ei\u00b7tel\u00b7keit"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$(", "VVFIN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "In der tugend wette-streit.", "tokens": ["In", "der", "tu\u00b7gend", "wet\u00b7te\u00b7streit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "6. Will man bey den \u00e4pffel-b\u00e4umen", "tokens": ["Will", "man", "bey", "den", "\u00e4pf\u00b7fel\u00b7b\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PIS", "APPR", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zu der lust spatziren gehn/", "tokens": ["Zu", "der", "lust", "spat\u00b7zi\u00b7ren", "gehn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "VVINF", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Darff man nicht die zeit vers\u00e4umen", "tokens": ["Darff", "man", "nicht", "die", "zeit", "ver\u00b7s\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PIS", "PTKNEG", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wann sie in der bl\u00fcte stehn/", "tokens": ["Wann", "sie", "in", "der", "bl\u00fc\u00b7te", "stehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "ART", "ADJA", "VVINF", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Eh der G\u00e4rtner nach der saat", "tokens": ["Eh", "der", "G\u00e4rt\u00b7ner", "nach", "der", "saat"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Auch die frucht gebrochen hat.", "tokens": ["Auch", "die", "frucht", "ge\u00b7bro\u00b7chen", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVPP", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "7. Und soll dann der sch\u00f6nen wangen", "tokens": ["Und", "soll", "dann", "der", "sch\u00f6\u00b7nen", "wan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "ADV", "ART", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Halbvermischtes milch und blut", "tokens": ["Halb\u00b7ver\u00b7mischtes", "milch", "und", "blut"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "NE", "KON", "NN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Gantz und gar vergebens prangen/", "tokens": ["Gantz", "und", "gar", "ver\u00b7ge\u00b7bens", "pran\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KON", "ADV", "ADV", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie ein saurer apffel thut/", "tokens": ["Wie", "ein", "sau\u00b7rer", "apf\u00b7fel", "thut", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Welcher nicht so wohl den zahn", "tokens": ["Wel\u00b7cher", "nicht", "so", "wohl", "den", "zahn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAT", "PTKNEG", "ADV", "ADV", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Als das aug ergetzen kan?", "tokens": ["Als", "das", "aug", "er\u00b7get\u00b7zen", "kan", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "8. Wein und bier wird ja zum trincken", "tokens": ["Wein", "und", "bier", "wird", "ja", "zum", "trin\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "ADV", "VAFIN", "ADV", "APPRART", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nicht zum ansehn aufgesetzt/", "tokens": ["Nicht", "zum", "an\u00b7sehn", "auf\u00b7ge\u00b7setzt", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPRART", "VVINF", "VVPP", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und was nutzt ein guter schincken", "tokens": ["Und", "was", "nutzt", "ein", "gu\u00b7ter", "schin\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "VVFIN", "ART", "ADJA", "VVINF"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Wann er nicht den mund ergetzt?", "tokens": ["Wann", "er", "nicht", "den", "mund", "er\u00b7getzt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PTKNEG", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Solte denn der jugend schein", "tokens": ["