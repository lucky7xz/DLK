{"textgrid.poem.58679": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "111.", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der h\u00f6llische Tyrann, der F\u00fcrste dieser Welt,", "tokens": ["Der", "h\u00f6l\u00b7li\u00b7sche", "Ty\u00b7rann", ",", "der", "F\u00fcrs\u00b7te", "die\u00b7ser", "Welt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hat der Vernunft und Sinnen", "tokens": ["Hat", "der", "Ver\u00b7nunft", "und", "Sin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ART", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Von au\u00dfen und von innen", "tokens": ["Von", "au\u00b7\u00dfen", "und", "von", "in\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "KON", "APPR", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Manch' goldnes G\u00f6tzenbild zur Reizung dargestellt.", "tokens": ["Man\u00b7ch'", "gold\u00b7nes", "G\u00f6t\u00b7zen\u00b7bild", "zur", "Rei\u00b7zung", "dar\u00b7ge\u00b7stellt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.5": {"text": "Er kann die Welt bezwingen,", "tokens": ["Er", "kann", "die", "Welt", "be\u00b7zwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wenn seine Pfeifen klingen,", "tokens": ["Wenn", "s