{"textgrid.poem.56033": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "R\u00f6mische Campagna", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Aus der vollgestellten Stadt, die lieber", "tokens": ["Aus", "der", "voll\u00b7ge\u00b7stell\u00b7ten", "Stadt", ",", "die", "lie\u00b7ber"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "PRELS", "ADV"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "schliefe, tr\u00e4umend von den hohen Thermen,", "tokens": ["schlie\u00b7fe", ",", "tr\u00e4u\u00b7mend", "von", "den", "ho\u00b7hen", "Ther\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVPP", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "geht der grade Gr\u00e4berweg ins Fieber;", "tokens": ["geht", "der", "gra\u00b7de", "Gr\u00e4\u00b7ber\u00b7weg", "ins", "Fie\u00b7ber", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "und die Fenster in den letzten Fermen", "tokens": ["und", "die", "Fens\u00b7ter", "in", "den", "letz\u00b7ten", "Fer\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "sehn ihm nach mit einem b\u00f6sen Blick.", "tokens": ["sehn", "ihm", "nach", "mit", "ei\u00b7nem", "b\u00f6\u00b7sen", "Blick", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Und er hat sie immer im Genick,", "tokens": ["Und", "er", "hat", "sie", "im\u00b7mer", "im", "Ge\u00b7nick", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "wenn er hingeht, rechts und links zerst\u00f6rend,", "tokens": ["wenn", "er", "hin\u00b7geht", ",", "rechts", "und", "links", "zer\u00b7st\u00f6\u00b7rend", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "ADV", "KON", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "bis er drau\u00dfen atemlos beschw\u00f6rend", "tokens": ["bis", "er", "drau\u00b7\u00dfen", "a\u00b7tem\u00b7los", "be\u00b7schw\u00f6\u00b7rend"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "VVPP"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "seine Leere zu den Himmeln hebt,", "tokens": ["sei\u00b7ne", "Lee\u00b7re", "zu", "den", "Him\u00b7meln", "hebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "hastig um sich schauend, ob ihn keine", "tokens": ["has\u00b7tig", "um", "sich", "schau\u00b7end", ",", "ob", "ihn", "kei\u00b7ne"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "APPR", "PRF", "ADJD", "$,", "KOUS", "PPER", "PIAT"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Fenster treffen. W\u00e4hrend er den weiten", "tokens": ["Fens\u00b7ter", "tref\u00b7fen", ".", "W\u00e4h\u00b7rend", "er", "den", "wei\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VVINF", "$.", "KOUS", "PPER", "ART", "ADJA"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Aqu\u00e4dukten zuwinkt herzuschreiten,", "tokens": ["A\u00b7qu\u00e4d\u00b7uk\u00b7ten", "zu\u00b7winkt", "her\u00b7zu\u00b7schrei\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "VVFIN", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "geben ihm die Himmel f\u00fcr die seine", "tokens": ["ge\u00b7ben", "ihm", "die", "Him\u00b7mel", "f\u00fcr", "die", "sei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPR", "ART", "PPOSAT"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "ihre Leere, die ihn \u00fcberlebt.", "tokens": ["ih\u00b7re", "Lee\u00b7re", ",", "die", "ihn", "\u00fc\u00b7ber\u00b7lebt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "PPER", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Aus der vollgestellten Stadt, die lieber", "tokens": ["Aus", "der", "voll\u00b7ge\u00b7stell\u00b7ten", "Stadt", ",", "die", "lie\u00b7ber"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "PRELS", "ADV"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "schliefe, tr\u00e4umend von den hohen Thermen,", "tokens": ["schlie\u00b7fe", ",", "tr\u00e4u\u00b7mend", "von", "den", "ho\u00b7hen", "Ther\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVPP", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "geht der grade Gr\u00e