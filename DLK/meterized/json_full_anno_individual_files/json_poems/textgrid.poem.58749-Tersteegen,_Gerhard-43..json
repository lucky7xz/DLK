{"textgrid.poem.58749": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "43.", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich will w\u00e4ssern meinen Garten. Sirach 24, 41", "tokens": ["Ich", "will", "w\u00e4s\u00b7sern", "mei\u00b7nen", "Gar\u00b7ten", ".", "Si\u00b7rach", "24", ",", "41"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "number", "punct", "number"], "pos": ["PPER", "VMFIN", "VVINF", "PPOSAT", "NN", "$.", "NN", "CARD", "$,", "CARD"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.2": {"line.1": {"text": "Pflanz, o du Gotteslieb', pflanz in mein Herze ein", "tokens": ["Pflanz", ",", "o", "du", "Got\u00b7tes\u00b7lieb'", ",", "pflanz", "in", "mein", "Her\u00b7ze", "ein"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "FM", "PPER", "NN", "$,", "ADV", "APPR", "PPOSAT", "VVFIN", "ART"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dein Kreuz und deine Lieb', die Erd' m\u00f6cht' sonst 'was tragen!", "tokens": ["Dein", "Kreuz", "und", "dei\u00b7ne", "Lieb'", ",", "die", "Erd'", "m\u00f6cht'", "sonst", "'was", "tra\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "PPOSAT", "NN", "$,", "ART", "NN", "VMFIN", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich pflanzete dies Bl\u00fcmelein,", "tokens": ["Ich", "pflan\u00b7ze\u00b7te", "dies", "Bl\u00fc\u00b7me\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PDS", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Du kommst und es begie\u00dft, das will was H\u00f6h'res sagen.", "tokens": ["Du", "kommst", "und", "es", "be\u00b7gie\u00dft", ",", "das", "will", "was", "H\u00f6h'\u00b7res", "sa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "PPER", "VVFIN", "$,", "PDS", "VMFIN", "PIS", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Doch dieses ich hiebei auch dacht':", "tokens": ["Doch", "die\u00b7ses", "ich", "hie\u00b7bei", "auch", "dacht'", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "PPER", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was auf der Welt nur w\u00e4chst, ich nicht begehr' noch meine,", "tokens": ["Was", "auf", "der", "Welt", "nur", "w\u00e4chst", ",", "ich", "nicht", "be\u00b7gehr'", "noch", "mei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "ADV", "VVFIN", "$,", "PPER", "PTKNEG", "VVFIN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Was Erd' und Meer gibt, ich veracht',", "tokens": ["Was", "Erd'", "und", "Meer", "gibt", ",", "ich", "ver\u00b7acht'", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "NN", "KON", "NN", "VVFIN", "$,", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df ich dich lieben m\u00f6g' alleine.", "tokens": ["Da\u00df", "ich", "dich", "lie\u00b7ben", "m\u00f6g'", "al\u00b7lei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVINF", "VMFIN", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Mit dir besch\u00e4ftigt sein, ist mein Verlangen nur,", "tokens": ["Mit", "dir", "be\u00b7sch\u00e4f\u00b7tigt", "sein", ",", "ist", "mein", "Ver\u00b7lan\u00b7gen", "nur", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVPP", "VAINF", "$,", "VAFIN", "PPOSAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nichts, was hier unten ist, die Sinne kann vergn\u00fcgen;", "tokens": ["Nichts", ",", "was", "hier", "un\u00b7ten", "ist", ",", "die", "Sin\u00b7ne", "kann", "ver\u00b7gn\u00fc\u00b7gen", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "PRELS", "ADV", "ADV", "VAFIN", "$,", "ART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Bin ich dir unterworfen pur,", "tokens": ["Bin", "ich", "dir", "un\u00b7ter\u00b7wor\u00b7fen", "pur", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "VVPP", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie ruhig werd' ich dann in deinen Armen liegen!", "tokens": ["Wie", "ru\u00b7hig", "werd'", "ich", "dann", "in", "dei\u00b7nen", "Ar\u00b7men", "lie\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Hier find' ich, was mich recht erg\u00f6tzt,", "tokens": ["Hier", "find'", "ich", ",", "was", "mich", "recht", "er\u00b7g\u00f6tzt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["