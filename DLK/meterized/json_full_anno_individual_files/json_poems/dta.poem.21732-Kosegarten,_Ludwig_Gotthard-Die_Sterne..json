{"dta.poem.21732": {"metadata": {"author": {"name": "Kosegarten, Ludwig Gotthard", "birth": "N.A.", "death": "N.A."}, "title": "Die Sterne.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1802", "urn": "urn:nbn:de:kobv:b4-200905193310", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Niedergeschlummert war die m&#252;de Sonne.", "tokens": ["Nie\u00b7der\u00b7ge\u00b7schlum\u00b7mert", "war", "die", "m", "&#252;", "de", "Son\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "XML_entity", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "FM", "FM", "FM", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Feyerlich wallte der Nacht lasurner Mantel", "tokens": ["Fey\u00b7er\u00b7lich", "wall\u00b7te", "der", "Nacht", "la\u00b7sur\u00b7ner", "Man\u00b7tel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "NN", "ADJA", "NN"], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.3": {"text": "Sch\u00f6n bes\u00e4umt von schimmergelockter Sterne", "tokens": ["Sch\u00f6n", "be\u00b7s\u00e4umt", "von", "schim\u00b7mer\u00b7ge\u00b7lock\u00b7ter", "Ster\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVPP", "APPR", "ADJA", "NN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "G\u00fcldenem Stickwerk.", "tokens": ["G\u00fcl\u00b7de\u00b7nem", "Stick\u00b7werk", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Unter dem blauen golddurchwirkten Teppich,", "tokens": ["Un\u00b7ter", "dem", "blau\u00b7en", "gold\u00b7durch\u00b7wirk\u00b7ten", "Tep\u00b7pich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Halbverh\u00fcllt von des Dunkels trautem Schleyer", "tokens": ["Halb\u00b7ver\u00b7h\u00fcllt", "von", "des", "Dun\u00b7kels", "trau\u00b7tem", "Schle\u00b7yer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Standen und staunten wir, und schauten", "tokens": ["Stan\u00b7den", "und", "staun\u00b7ten", "wir", ",", "und", "schau\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "KON", "VVFIN", "PPER", "$,", "KON", "VVFIN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "liebend", "tokens": ["lie\u00b7bend"], "token_info": ["word"], "pos": ["ADJD"], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Auf zu den Sternen.", "tokens": ["Auf", "zu", "den", "Ster\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "\u00dcber der Holden vollgelockter Scheitel", "tokens": ["\u00dc\u00b7ber", "der", "Hol\u00b7den", "voll\u00b7ge\u00b7lock\u00b7ter", "Schei\u00b7tel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Strahlte Cassiopega, blitzte Cepheus,", "tokens": ["Strahl\u00b7te", "Cas\u00b7sio\u00b7pe\u00b7ga", ",", "blitz\u00b7te", "Ce\u00b7pheus", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "NE", "$,", "VVFIN", "NE", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Funkelte Perseus Schwert, flog Andromedens", "tokens": ["Fun\u00b7kel\u00b7te", "Per\u00b7seus", "Schwert", ",", "flog", "And\u00b7ro\u00b7me\u00b7dens"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "NE", "NE", "$,", "VVFIN", "NE"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Gl\u00e4nzender G\u00fcrtel.", "tokens": ["Gl\u00e4n\u00b7zen\u00b7der", "G\u00fcr\u00b7tel", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.4": {"line.1": {"text": "T\u00f6nenden Fittigs stieg empor der Adler,", "tokens": ["T\u00f6\u00b7nen\u00b7den", "Fit\u00b7tigs", "stieg", "em\u00b7por", "der", "Ad\u00b7ler", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PTKVZ", "ART", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Melodieen ergoss der Schwan des Himmels,", "tokens": ["Me\u00b7lo\u00b7di\u00b7e\u00b7en", "er\u00b7goss", "der", "Schwan", "des", "Him\u00b7mels", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "Wonnegesang entlispelte der Lyra", "tokens": ["Won\u00b7ne\u00b7ge\u00b7sang", "ent\u00b7lis\u00b7pel\u00b7te", "der", "Ly\u00b7ra"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN"], "meter": "+--+-+-+---", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Bebenden Sayten.", "tokens": ["Be\u00b7ben\u00b7den", "Say\u00b7ten", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.5": {"line.1": {"text": "Und von der Majest\u00e4t der Nacht durchschaudert,", "tokens": ["Und", "von", "der", "Ma\u00b7jes\u00b7t\u00e4t", "der", "Nacht", "durch\u00b7schau\u00b7dert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Jeder Bl\u00f6de vergessend, jede Feigheit", "tokens": ["Je\u00b7der", "Bl\u00f6\u00b7de", "ver\u00b7ges\u00b7send", ",", "je\u00b7de", "Feig\u00b7heit"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PIAT", "NN", "VVPP", "$,", "PIAT", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Gross verschm\u00e4hend umschlang ich die Geliebte", "tokens": ["Gross", "ver\u00b7schm\u00e4\u00b7hend", "um\u00b7schlang", "ich", "die", "Ge\u00b7lieb\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVPP", "VVFIN", "PPER", "ART", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Feurigern Armes.", "tokens": ["Feu\u00b7ri\u00b7gern", "Ar\u00b7mes", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "NE", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.6": {"line.1": {"text": "Und den Umschlingenden umschlang die Edle", "tokens": ["Und", "den", "Um\u00b7schlin\u00b7gen\u00b7den", "um\u00b7schlang", "die", "Ed\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "ART", "ADJA"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Leisest\u00f6hnend, es st\u00fcrmte Herz an Herzen,", "tokens": ["Lei\u00b7se\u00b7st\u00f6h\u00b7nend", ",", "es", "st\u00fcrm\u00b7te", "Herz", "an", "Her\u00b7zen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PPER", "VVFIN", "NN", "APPR", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Wange brannte an Wange. Trunken gl\u00fchte", "tokens": ["Wan\u00b7ge", "brann\u00b7te", "an", "Wan\u00b7ge", ".", "Trun\u00b7ken", "gl\u00fch\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "VVFIN", "APPR", "NN", "$.", "NN", "VVFIN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Lippe an Lippe \u2014 \u2014", "tokens": ["Lip\u00b7pe", "an", "Lip\u00b7pe"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["NE", "APPR", "NE", "$(", "$("], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.7": {"line.1": {"text": "Und als ich auftaucht' aus der Wonnen Abgrund,", "tokens": ["Und", "als", "ich", "auf\u00b7taucht'", "aus", "der", "Won\u00b7nen", "Ab\u00b7grund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Siehe, da gl\u00e4nzten alle Sterne g\u00fcldner.", "tokens": ["Sie\u00b7he", ",", "da", "gl\u00e4nz\u00b7ten", "al\u00b7le", "Ster\u00b7ne", "g\u00fcld\u00b7ner", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "ADV", "VVFIN", "PIAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Lodernder brannte Cassiopega. Funken", "tokens": ["Lo\u00b7dern\u00b7der", "brann\u00b7te", "Cas\u00b7sio\u00b7pe\u00b7ga", ".", "Fun\u00b7ken"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["NE", "VVFIN", "NE", "$.", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Spr\u00fchte Cepheus.", "tokens": ["Spr\u00fch\u00b7te", "Ce\u00b7pheus", "."], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "NE", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.8": {"line.1": {"text": "T\u00f6nendern Schwunges stieg empor der Adler.", "tokens": ["T\u00f6\u00b7nen\u00b7dern", "Schwun\u00b7ges", "stieg", "em\u00b7por", "der", "Ad\u00b7ler", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "PTKVZ", "ART", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Liebesgesang entquoll dem Schwan des Himmels,", "tokens": ["Lie\u00b7bes\u00b7ge\u00b7sang", "ent\u00b7quoll", "dem", "Schwan", "des", "Him\u00b7mels", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ART", "NN", "ART", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Liebesgelispel girrte von der Lyra", "tokens": ["Lie\u00b7bes\u00b7ge\u00b7lis\u00b7pel", "girr\u00b7te", "von", "der", "Ly\u00b7ra"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Bebenden Sayten.", "tokens": ["Be\u00b7ben\u00b7den", "Say\u00b7ten", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.9": {"line.1": {"text": "Selig erklangen alle Sph\u00e4ren. Alle", "tokens": ["Se\u00b7lig", "er\u00b7klan\u00b7gen", "al\u00b7le", "Sph\u00e4\u00b7ren", ".", "Al\u00b7le"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["NE", "VVFIN", "PIAT", "NN", "$.", "PIAT"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Glocken der Weltharmonika ert\u00f6nten.", "tokens": ["Glo\u00b7cken", "der", "Welt\u00b7har\u00b7mo\u00b7ni\u00b7ka", "er\u00b7t\u00f6n\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVFIN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Feuriger pochten, Liebe klopften alle", "tokens": ["Feu\u00b7ri\u00b7ger", "poch\u00b7ten", ",", "Lie\u00b7be", "klopf\u00b7ten", "al\u00b7le"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVFIN", "$,", "NN", "VVFIN", "PIS"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Pulse des Weltalls.", "tokens": ["Pul\u00b7se", "des", "Welt\u00b7alls", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.10": {"line.1": {"text": "Trunken noch immer ach des Nektarbechers,", "tokens": ["Trun\u00b7ken", "noch", "im\u00b7mer", "ach", "des", "Nekt\u00b7ar\u00b7be\u00b7chers", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADV", "ADV", "ART", "NN", "$,"], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Der mit Entz\u00fccken Sinn und Geist berauschte,", "tokens": ["Der", "mit", "Ent\u00b7z\u00fc\u00b7cken", "Sinn", "und", "Geist", "be\u00b7rauschte", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wandl' ich dahin seitdem in s\u00fcsser Liebe", "tokens": ["Wandl'", "ich", "da\u00b7hin", "seit\u00b7dem", "in", "s\u00fcs\u00b7ser", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "PAV", "PPOSAT", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Heiligem Wahnsinn.", "tokens": ["Hei\u00b7li\u00b7gem", "Wahn\u00b7sinn", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.11": {"line.1": {"text": "Aber verklungen l\u00e4ngst in Ihrem Herzen", "tokens": ["A\u00b7ber", "ver\u00b7klun\u00b7gen", "l\u00e4ngst", "in", "Ih\u00b7rem", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "APPR", "PPOSAT", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Ist des s\u00fcssen Momentes s\u00fcsser Nachhall;", "tokens": ["Ist", "des", "s\u00fcs\u00b7sen", "Mo\u00b7men\u00b7tes", "s\u00fcs\u00b7ser", "Nach\u00b7hall", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "+-+----+-+-", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Mir vor\u00fcber, dem sie den Brand ins Herz warf,", "tokens": ["Mir", "vor\u00b7\u00fc\u00b7ber", ",", "dem", "sie", "den", "Brand", "ins", "Herz", "warf", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKVZ", "$,", "PRELS", "PPER", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Wandelt sie achtlos,", "tokens": ["Wan\u00b7delt", "sie", "acht\u00b7los", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.12": {"line.1": {"text": "W\u00fcrdiget ihn, der ach nach ihr verschmachtet,", "tokens": ["W\u00fcr\u00b7di\u00b7get", "ihn", ",", "der", "ach", "nach", "ihr", "ver\u00b7schmach\u00b7tet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PRELS", "ADV", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Keines holderen Blicks, noch s\u00fcssern Wortes,", "tokens": ["Kei\u00b7nes", "hol\u00b7de\u00b7ren", "Blicks", ",", "noch", "s\u00fcs\u00b7sern", "Wor\u00b7tes", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$,", "ADV", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Schwebet dahin mit leichtem Muth, wirft, Freude,", "tokens": ["Schwe\u00b7bet", "da\u00b7hin", "mit", "leich\u00b7tem", "Muth", ",", "wirft", ",", "Freu\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PAV", "APPR", "ADJA", "NN", "$,", "VVFIN", "$,", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Dir in den Arm sich \u2014 \u2014", "tokens": ["Dir", "in", "den", "Arm", "sich"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "PRF", "$(", "$("], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.13": {"line.1": {"text": "Heilige Sterne, ahnen wahr die Weisen,", "tokens": ["Hei\u00b7li\u00b7ge", "Ster\u00b7ne", ",", "ah\u00b7nen", "wahr", "die", "Wei\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "ADJD", "ART", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Ward geschrieben in euch der Menschen", "tokens": ["Ward", "ge\u00b7schrie\u00b7ben", "in", "euch", "der", "Men\u00b7schen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "VVPP", "APPR", "PPER", "ART", "NN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Schicksal \u2014", "tokens": ["Schick\u00b7sal"], "token_info": ["word", "punct"], "pos": ["NN", "$("], "meter": "--", "measure": "unknown.measure.zero"}, "line.4": {"text": "O wer \u00f6ffnet den Sinn, wer liest mir eure", "tokens": ["O", "wer", "\u00f6ff\u00b7net", "den", "Sinn", ",", "wer", "liest", "mir", "eu\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "PWS", "VVFIN", "ART", "NN", "$,", "PWS", "VVFIN", "PPER", "PPOSAT"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.5": {"text": "G\u00fcldenen Chiffern?", "tokens": ["G\u00fcl\u00b7de\u00b7nen", "Chif\u00b7fern", "?"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.14": {"line.1": {"text": "Frech zu ersp\u00e4hn der Zukunft Schauerdunkel,", "tokens": ["Frech", "zu", "er\u00b7sp\u00e4hn", "der", "Zu\u00b7kunft", "Schau\u00b7er\u00b7dun\u00b7kel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VVINF", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "L\u00fcstet mich nicht; mich l\u00fcstet nur, das Eine", "tokens": ["L\u00fcs\u00b7tet", "mich", "nicht", ";", "mich", "l\u00fcs\u00b7tet", "nur", ",", "das", "Ei\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "PTKNEG", "$.", "PPER", "VVFIN", "ADV", "$,", "PRELS", "ART"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Zu ergr\u00fcnden \u2014 \u2014 nur \u2014 \u2014 Ida, deines", "tokens": ["Zu", "er\u00b7gr\u00fcn\u00b7den", "nur", "I\u00b7da", ",", "dei\u00b7nes"], "token_info": ["word", "word", "punct", "punct", "word", "punct", "punct", "word", "punct", "word"], "pos": ["PTKZU", "VVINF", "$(", "$(", "ADV", "$(", "$(", "NE", "$,", "PPOSAT"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Herzens'", "tokens": ["Her\u00b7zens'"], "token_info": ["word"], "pos": ["NE"], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "D\u00e4mmerndes R\u00e4thsel!", "tokens": ["D\u00e4m\u00b7mern\u00b7des", "R\u00e4th\u00b7sel", "!"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}}}}