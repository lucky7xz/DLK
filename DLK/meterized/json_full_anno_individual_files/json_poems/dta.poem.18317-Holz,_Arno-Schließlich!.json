{"dta.poem.18317": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "Schlie\u00dflich!", "genre": "Lyrik", "period": "N.A.", "pub_year": "1886", "urn": "urn:nbn:de:kobv:b4-200905192665", "language": ["de:0.99"], "booktitle": "Holz, Arno: Das Buch der Zeit. Lieder eines Modernen. Z\u00fcrich, 1886."}, "poem": {"stanza.1": {"line.1": {"text": "Das Volk hat lange, graue Ohren,", "tokens": ["Das", "Volk", "hat", "lan\u00b7ge", ",", "grau\u00b7e", "Oh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und seine Treiber nennen sich", "tokens": ["Und", "sei\u00b7ne", "Trei\u00b7ber", "nen\u00b7nen", "sich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Rabbiner, Pfarrer und Pastoren.", "tokens": ["Rab\u00b7bi\u00b7ner", ",", "Pfar\u00b7rer", "und", "Pas\u00b7to\u00b7ren", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}}, "stanza.2": {"line.1": {"text": "Verha\u00dft ist mir der Schwindelbau", "tokens": ["Ver\u00b7ha\u00dft", "ist", "mir", "der", "Schwin\u00b7del\u00b7bau"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der jesuitelnden Sophisten,", "tokens": ["Der", "je\u00b7su\u00b7i\u00b7teln\u00b7den", "So\u00b7phis\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und \u00fcberleg ich's mir genau,", "tokens": ["Und", "\u00fc\u00b7ber\u00b7leg", "ich's", "mir", "ge\u00b7nau", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PIS", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hab ich Talent zum Atheisten.", "tokens": ["Hab", "ich", "Ta\u00b7lent", "zum", "A\u00b7theis\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "NN", "APPRART", "NN", "$."], "meter": "+---+-+-", "measure": "dactylic.init"}}, "stanza.3": {"line.1": {"text": "Tagt\u00e4glich sch\u00fcrt in mir den Spott", "tokens": ["Tag\u00b7t\u00e4g\u00b7lich", "sch\u00fcrt", "in", "mir", "den", "Spott"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "APPR", "PPER", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das fade Weihrauchduftgetr\u00e4ufel,", "tokens": ["Das", "fa\u00b7de", "Weih\u00b7rauch\u00b7duft\u00b7ge\u00b7tr\u00e4u\u00b7fel", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Denn schlie\u00dflich ist der liebe Gott", "tokens": ["Denn", "schlie\u00df\u00b7lich", "ist", "der", "lie\u00b7be", "Gott"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Doch nur ein dummer Antiteufel!", "tokens": ["Doch", "nur", "ein", "dum\u00b7mer", "An\u00b7ti\u00b7teu\u00b7fel", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}