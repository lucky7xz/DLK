{"dta.poem.7075": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Entwurff einiger Betrachtungen,  \n \u00fcber den  \n  Zustand der Seeligen.  \n Beym Anfang des 1727sten Jahres.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Betrachten wir hierauf das Gl\u00fcck der seel\u2019gen Seelen", "tokens": ["Be\u00b7trach\u00b7ten", "wir", "hier\u00b7auf", "das", "Gl\u00fcck", "der", "seel'\u00b7gen", "See\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PAV", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nach des ", "tokens": ["Nach", "des"], "token_info": ["word", "word"], "pos": ["APPR", "ART"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Ach welcher Geist kan doch die Eigenschafft", "tokens": ["Ach", "wel\u00b7cher", "Geist", "kan", "doch", "die", "Ei\u00b7gen\u00b7schafft"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "PWAT", "NN", "VMFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der Vollenkommenheit erz\u00e4hlen,", "tokens": ["Der", "Vol\u00b7len\u00b7kom\u00b7men\u00b7heit", "er\u00b7z\u00e4h\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Da sie in richtigen, best\u00e4ndigen,", "tokens": ["Da", "sie", "in", "rich\u00b7ti\u00b7gen", ",", "be\u00b7st\u00e4n\u00b7di\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "$,", "VVPP", "$,"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.6": {"text": "Gewissen, unaufh\u00f6rlichen", "tokens": ["Ge\u00b7wis\u00b7sen", ",", "un\u00b7auf\u00b7h\u00f6r\u00b7li\u00b7chen"], "token_info": ["word", "punct", "word"], "pos": ["VVPP", "$,", "ADJA"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.7": {"text": "Und stets vergn\u00fcgenden Besch\u00e4fftigungen stehet,", "tokens": ["Und", "stets", "ver\u00b7gn\u00fc\u00b7gen\u00b7den", "Be\u00b7sch\u00e4ff\u00b7ti\u00b7gun\u00b7gen", "ste\u00b7het", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Mit den ihr zugetheilten Dingen,", "tokens": ["Mit", "den", "ihr", "zu\u00b7ge\u00b7theil\u00b7ten", "Din\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Die ihr best\u00e4ndig Lust und Anmuth bringen.", "tokens": ["Die", "ihr", "be\u00b7st\u00e4n\u00b7dig", "Lust", "und", "An\u00b7muth", "brin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Und weil der Mensch absonderlich gemacht,", "tokens": ["Und", "weil", "der", "Mensch", "ab\u00b7son\u00b7der\u00b7lich", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Um seinen Blick, mit allen Kr\u00e4fften,", "tokens": ["Um", "sei\u00b7nen", "Blick", ",", "mit", "al\u00b7len", "Kr\u00e4ff\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "$,", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "An seines Wesens Quell, den Sch\u00f6pffer stets zu hefften,", "tokens": ["An", "sei\u00b7nes", "We\u00b7sens", "Quell", ",", "den", "Sch\u00f6pf\u00b7fer", "stets", "zu", "heff\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,", "ART", "NN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "In Seiner Majest\u00e4t, in Seiner Wercke Pracht,", "tokens": ["In", "Sei\u00b7ner", "Ma\u00b7jes\u00b7t\u00e4t", ",", "in", "Sei\u00b7ner", "Wer\u00b7cke", "Pracht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Jhn anzubeten, zu verehren,", "tokens": ["Jhn", "an\u00b7zu\u00b7be\u00b7ten", ",", "zu", "ver\u00b7eh\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVIZU", "$,", "PTKZU", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Sein unausdr\u00fccklich Lob best\u00e4ndig zu vermehren;", "tokens": ["Sein", "un\u00b7aus\u00b7dr\u00fcck\u00b7lich", "Lob", "be\u00b7st\u00e4n\u00b7dig", "zu", "ver\u00b7meh\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "So mu\u00df er ja sodenn vollkommen selig seyn,", "tokens": ["So", "mu\u00df", "er", "ja", "so\u00b7denn", "voll\u00b7kom\u00b7men", "se\u00b7lig", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ADV", "ADJD", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Wenn seine Seel\u2019 und sein Gem\u00fcthe", "tokens": ["Wenn", "sei\u00b7ne", "Seel'", "und", "sein", "Ge\u00b7m\u00fc\u00b7the"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Die gr\u00f6sste St\u00e4rcke braucht, um mit der ew\u2019gen G\u00fcte", "tokens": ["Die", "gr\u00f6ss\u00b7te", "St\u00e4r\u00b7cke", "braucht", ",", "um", "mit", "der", "ew'\u00b7gen", "G\u00fc\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,", "KOUI", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Und ihrer Liebe seel\u2019gem Schein", "tokens": ["Und", "ih\u00b7rer", "Lie\u00b7be", "seel'\u00b7gem", "Schein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Je mehr und mehr sich zu verbinden.", "tokens": ["Je", "mehr", "und", "mehr", "sich", "zu", "ver\u00b7bin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "KON", "ADV", "PRF", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}