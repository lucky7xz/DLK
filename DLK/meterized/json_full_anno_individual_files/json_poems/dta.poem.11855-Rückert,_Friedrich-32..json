{"dta.poem.11855": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "32.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Seele tr\u00e4gt ein Ma\u00df des Sch\u00f6nen selbst in sich,", "tokens": ["Die", "See\u00b7le", "tr\u00e4gt", "ein", "Ma\u00df", "des", "Sch\u00f6\u00b7nen", "selbst", "in", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "ART", "NN", "ADV", "APPR", "PRF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Daher dem Menschen stets auch seine Liebe glich.", "tokens": ["Da\u00b7her", "dem", "Men\u00b7schen", "stets", "auch", "sei\u00b7ne", "Lie\u00b7be", "glich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "ADV", "ADV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Dein schwarzer Bruder denkt sich schw\u00e4rzlich seine Sch\u00f6ne,", "tokens": ["Dein", "schwar\u00b7zer", "Bru\u00b7der", "denkt", "sich", "schw\u00e4rz\u00b7lich", "sei\u00b7ne", "Sch\u00f6\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "PRF", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Zwerg als Zwergin, und als Riesin Riesens\u00f6hne.", "tokens": ["Der", "Zwerg", "als", "Zwer\u00b7gin", ",", "und", "als", "Rie\u00b7sin", "Rie\u00b7sen\u00b7s\u00f6h\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KOUS", "NN", "$,", "KON", "KOUS", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Und der vollkommne Mensch setzt in den Aufenthalt", "tokens": ["Und", "der", "voll\u00b7komm\u00b7ne", "Mensch", "setzt", "in", "den", "Auf\u00b7ent\u00b7halt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Des h\u00f6chsten Himmels selbst die menschliche Gestalt.", "tokens": ["Des", "h\u00f6chs\u00b7ten", "Him\u00b7mels", "selbst", "die", "menschli\u00b7che", "Ge\u00b7stalt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.4": {"line.1": {"text": "Es will der Menschengeist in andern Gotteswelten", "tokens": ["Es", "will", "der", "Men\u00b7schen\u00b7geist", "in", "an\u00b7dern", "Got\u00b7tes\u00b7wel\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Kein anderes Vernunftgesch\u00f6pfe lassen gelten.", "tokens": ["Kein", "an\u00b7de\u00b7res", "Ver\u00b7nunft\u00b7ge\u00b7sch\u00f6p\u00b7fe", "las\u00b7sen", "gel\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Er will der Mittelpunkt der Sch\u00f6pfungskreise seyn,", "tokens": ["Er", "will", "der", "Mit\u00b7tel\u00b7punkt", "der", "Sch\u00f6p\u00b7fungs\u00b7krei\u00b7se", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "ART", "NN", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Des Sch\u00f6pfers Ebenbild und Sch\u00f6pfungszweck allein.", "tokens": ["Des", "Sch\u00f6p\u00b7fers", "E\u00b7ben\u00b7bild", "und", "Sch\u00f6p\u00b7fungs\u00b7zweck", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Doch andre Wesen sind noch denkbar au\u00dfer dir,", "tokens": ["Doch", "and\u00b7re", "We\u00b7sen", "sind", "noch", "denk\u00b7bar", "au\u00b7\u00dfer", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VAFIN", "ADV", "ADJD", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In ihren Kreisen das, was du in deinem hier.", "tokens": ["In", "ih\u00b7ren", "Krei\u00b7sen", "das", ",", "was", "du", "in", "dei\u00b7nem", "hier", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PDS", "$,", "PWS", "PPER", "APPR", "PPOSAT", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Du hast f\u00fcr sie kein Ma\u00df, sie keins villeicht f\u00fcr dich,", "tokens": ["Du", "hast", "f\u00fcr", "sie", "kein", "Ma\u00df", ",", "sie", "keins", "vil\u00b7leicht", "f\u00fcr", "dich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "PIAT", "NN", "$,", "PPER", "PIAT", "NN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und halten, so wie du dich h\u00e4ltst, f\u00fcr einzig sich.", "tokens": ["Und", "hal\u00b7ten", ",", "so", "wie", "du", "dich", "h\u00e4ltst", ",", "f\u00fcr", "ein\u00b7zig", "sich", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "$,", "ADV", "KOKOM", "PPER", "PRF", "VVFIN", "$,", "APPR", "ADJD", "PRF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Doch wenn sie h\u00f6her stehn als du und heller sehn,", "tokens": ["Doch", "wenn", "sie", "h\u00f6\u00b7her", "stehn", "als", "du", "und", "hel\u00b7ler", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADJD", "VVINF", "KOKOM", "PPER", "KON", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Begreifen sie wol dich, die du nicht kannst verstehn.", "tokens": ["Be\u00b7grei\u00b7fen", "sie", "wol", "dich", ",", "die", "du", "nicht", "kannst", "ver\u00b7stehn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PPER", "$,", "PRELS", "PPER", "PTKNEG", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Jemehr du aber dich enthebst den engen Schranken,", "tokens": ["Je\u00b7mehr", "du", "a\u00b7ber", "dich", "ent\u00b7hebst", "den", "en\u00b7gen", "Schran\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Erweiterst du die Welt mit liebenden Gedanken.", "tokens": ["Er\u00b7wei\u00b7terst", "du", "die", "Welt", "mit", "lie\u00b7ben\u00b7den", "Ge\u00b7dan\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.10": {"line.1": {"text": "Du freuest dich, da\u00df auch in andern Sf\u00e4ren walten", "tokens": ["Du", "freu\u00b7est", "dich", ",", "da\u00df", "auch", "in", "an\u00b7dern", "Sf\u00e4\u00b7ren", "wal\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$,", "KOUS", "ADV", "APPR", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vernunftweltordnungen und Glaubensheilanstalten.", "tokens": ["Ver\u00b7nunft\u00b7wel\u00b7tord\u00b7nun\u00b7gen", "und", "Glau\u00b7bens\u00b7hei\u00b7lan\u00b7stal\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.11": {"line.1": {"text": "In jedem Himmelskreis, in allen Erdenkreisen,", "tokens": ["In", "je\u00b7dem", "Him\u00b7mels\u00b7kreis", ",", "in", "al\u00b7len", "Er\u00b7den\u00b7krei\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "La\u00df nur auf ihre Weis' ihr H\u00f6chstes alle preisen.", "tokens": ["La\u00df", "nur", "auf", "ih\u00b7re", "Weis'", "ihr", "H\u00f6chs\u00b7tes", "al\u00b7le", "prei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "APPR", "PPOSAT", "NN", "PPOSAT", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.12": {"line.1": {"text": "Den eignen Glauben sollst du dir nicht lassen rauben,", "tokens": ["Den", "eig\u00b7nen", "Glau\u00b7ben", "sollst", "du", "dir", "nicht", "las\u00b7sen", "rau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "PPER", "PPER", "PTKNEG", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Allein auch rauben sollst du keinem seinen Glauben.", "tokens": ["Al\u00b7lein", "auch", "rau\u00b7ben", "sollst", "du", "kei\u00b7nem", "sei\u00b7nen", "Glau\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVINF", "VMFIN", "PPER", "PIAT", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.13": {"line.1": {"text": "Und eiferig bek\u00e4mpf' an dir und andern k\u00fcnftig", "tokens": ["Und", "ei\u00b7fe\u00b7rig", "be\u00b7k\u00e4mpf'", "an", "dir", "und", "an\u00b7dern", "k\u00fcnf\u00b7tig"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "APPR", "PPER", "KON", "PIS", "ADJD"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nur was unmenschlich ist, unsch\u00f6n und unvern\u00fcnftig.", "tokens": ["Nur", "was", "un\u00b7menschlich", "ist", ",", "un\u00b7sch\u00f6n", "und", "un\u00b7ver\u00b7n\u00fcnf\u00b7tig", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PWS", "ADJD", "VAFIN", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}}}}}