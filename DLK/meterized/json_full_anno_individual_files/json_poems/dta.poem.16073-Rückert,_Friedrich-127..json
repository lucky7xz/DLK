{"dta.poem.16073": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "127.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ist da die Welt f\u00fcr mich? bin ich da f\u00fcr die Welt?", "tokens": ["Ist", "da", "die", "Welt", "f\u00fcr", "mich", "?", "bin", "ich", "da", "f\u00fcr", "die", "Welt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "APPR", "PPER", "$.", "VAFIN", "PPER", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "F\u00fcr Beute hielt ich sie, die mich f\u00fcr Beute h\u00e4lt.", "tokens": ["F\u00fcr", "Beu\u00b7te", "hielt", "ich", "sie", ",", "die", "mich", "f\u00fcr", "Beu\u00b7te", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "PPER", "$,", "PRELS", "PRF", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Als ich zu meinem Raub zu machen sie gedachte,", "tokens": ["Als", "ich", "zu", "mei\u00b7nem", "Raub", "zu", "ma\u00b7chen", "sie", "ge\u00b7dach\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Erkannt' ich, da\u00df sie mich zu ihrem Raube machte.", "tokens": ["Er\u00b7kannt'", "ich", ",", "da\u00df", "sie", "mich", "zu", "ih\u00b7rem", "Rau\u00b7be", "mach\u00b7te", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "PPER", "PRF", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "R\u00fcckgeben kann ich nicht, was ich von ihr genommen,", "tokens": ["R\u00fcck\u00b7ge\u00b7ben", "kann", "ich", "nicht", ",", "was", "ich", "von", "ihr", "ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PTKNEG", "$,", "PWS", "PPER", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und nicht r\u00fcckfordern, was sie hat von mir bekommen.", "tokens": ["Und", "nicht", "r\u00fcck\u00b7for\u00b7dern", ",", "was", "sie", "hat", "von", "mir", "be\u00b7kom\u00b7men", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "VVINF", "$,", "PRELS", "PPER", "VAFIN", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Ihr vorenthalt' ich nichts, die nichts mir vorenth\u00e4lt;", "tokens": ["Ihr", "vor\u00b7ent\u00b7halt'", "ich", "nichts", ",", "die", "nichts", "mir", "vor\u00b7ent\u00b7h\u00e4lt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIS", "$,", "PRELS", "PIS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Welt ist da f\u00fcr mich, ich bin da f\u00fcr die Welt.", "tokens": ["Die", "Welt", "ist", "da", "f\u00fcr", "mich", ",", "ich", "bin", "da", "f\u00fcr", "die", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "APPR", "PPER", "$,", "PPER", "VAFIN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}