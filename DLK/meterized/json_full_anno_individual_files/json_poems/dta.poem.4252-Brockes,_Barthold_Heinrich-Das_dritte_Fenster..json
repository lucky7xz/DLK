{"dta.poem.4252": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Das dritte Fenster.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nun komm\u2019 ich an den Mittelpunct von meinen Fenstern,", "tokens": ["Nun", "komm'", "ich", "an", "den", "Mit\u00b7tel\u00b7punct", "von", "mei\u00b7nen", "Fens\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "welcher mir", "tokens": ["wel\u00b7cher", "mir"], "token_info": ["word", "word"], "pos": ["PWS", "PPER"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Der Landschaft ungemeine Sch\u00f6nheit, fast in der allergr\u00f6\u00dften", "tokens": ["Der", "Land\u00b7schaft", "un\u00b7ge\u00b7mei\u00b7ne", "Sch\u00f6n\u00b7heit", ",", "fast", "in", "der", "al\u00b7ler\u00b7gr\u00f6\u00df\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "$,", "ADV", "APPR", "ART", "ADJA"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "Zier,", "tokens": ["Zier", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Annoch vor allen andern zeigt. Hier treff\u2019 ich eine grosse", "tokens": ["An\u00b7noch", "vor", "al\u00b7len", "an\u00b7dern", "zeigt", ".", "Hier", "treff'", "ich", "ei\u00b7ne", "gros\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "PIAT", "PIS", "VVFIN", "$.", "ADV", "VVFIN", "PPER", "ART", "ADJA"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.6": {"text": "Menge", "tokens": ["Men\u00b7ge"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.7": {"text": "Von sch\u00f6nen Gegenw\u00fcrfen an. In einem wirklichen Ge-", "tokens": ["Von", "sch\u00f6\u00b7nen", "Ge\u00b7gen\u00b7w\u00fcr\u00b7fen", "an", ".", "In", "ei\u00b7nem", "wirk\u00b7li\u00b7chen", "Ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "PTKVZ", "$.", "APPR", "ART", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.8": {"text": "pr\u00e4nge", "tokens": ["pr\u00e4n\u00b7ge"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "+-", "measure": "trochaic.single"}, "line.9": {"text": "Zeigt hier sich Wasser, Luft und Land. Um nun der Ordnung", "tokens": ["Zeigt", "hier", "sich", "Was\u00b7ser", ",", "Luft", "und", "Land", ".", "Um", "nun", "der", "Ord\u00b7nung"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "PRF", "NN", "$,", "NN", "KON", "NN", "$.", "KOUI", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "nachzugeh'n,", "tokens": ["nach\u00b7zu\u00b7geh'n", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.11": {"text": "La\u00dft uns, wie in den andern beyden, auch dieser Tafel Vor-", "tokens": ["La\u00dft", "uns", ",", "wie", "in", "den", "an\u00b7dern", "bey\u00b7den", ",", "auch", "die\u00b7ser", "Ta\u00b7fel", "Vor"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "$,", "PWAV", "APPR", "ART", "ADJA", "PIAT", "$,", "ADV", "PDAT", "NN", "TRUNC"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.12": {"text": "grund seh'n,", "tokens": ["grund", "seh'n", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVINF", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.13": {"text": "Da denn, wenn wir, mit frohen Blicken, gemach von unten", "tokens": ["Da", "denn", ",", "wenn", "wir", ",", "mit", "fro\u00b7hen", "Bli\u00b7cken", ",", "ge\u00b7mach", "von", "un\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADV", "$,", "KOUS", "PPER", "$,", "APPR", "ADJA", "NN", "$,", "ADV", "APPR", "ADV"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.14": {"text": "aufwerts steigen,", "tokens": ["auf\u00b7werts", "stei\u00b7gen", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.15": {"text": "Sich immer neue Lieblichkeiten den fast erstaunten Augen", "tokens": ["Sich", "im\u00b7mer", "neu\u00b7e", "Lieb\u00b7lich\u00b7kei\u00b7ten", "den", "fast", "er\u00b7staun\u00b7ten", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PRF", "ADV", "ADJA", "NN", "ART", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.16": {"text": "zeigen.", "tokens": ["zei\u00b7gen", "."], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.17": {"text": "In einer halben R\u00fcnde lieget ein Stein-Platz, den der", "tokens": ["In", "ei\u00b7ner", "hal\u00b7ben", "R\u00fcn\u00b7de", "lie\u00b7get", "ein", "Stein\u00b7Platz", ",", "den", "der"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "ART", "NN", "$,", "PRELS", "ART"], "meter": "-+-+-+-+--++-+", "measure": "iambic.septa.relaxed"}, "line.18": {"text": "Wall verschr\u00e4nkt,", "tokens": ["Wall", "ver\u00b7schr\u00e4nkt", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "VVFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.19": {"text": "Der sich bis zu der Mauer Fu\u00df, von allen beyden Seiten,", "tokens": ["Der", "sich", "bis", "zu", "der", "Mau\u00b7er", "Fu\u00df", ",", "von", "al\u00b7len", "bey\u00b7den", "Sei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "ADV", "APPR", "ART", "NN", "NN", "$,", "APPR", "PIAT", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.20": {"text": "lenkt.", "tokens": ["lenkt", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+", "measure": "single.up"}, "line.21": {"text": "Hier stellt, in Regel- rechter H\u00f6he, ein neu- gew\u00f6lbtes stei-", "tokens": ["Hier", "stellt", ",", "in", "Re\u00b7gel", "rech\u00b7ter", "H\u00f6\u00b7he", ",", "ein", "neu", "ge\u00b7w\u00f6lb\u00b7tes", "stei"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$,", "APPR", "TRUNC", "ADJA", "NN", "$,", "ART", "TRUNC", "ADJA", "TRUNC"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.22": {"text": "nern Thor,", "tokens": ["nern", "Thor", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.23": {"text": "Das noch zu meiner Zeit gebauet, sich zierlich unsern Augen", "tokens": ["Das", "noch", "zu", "mei\u00b7ner", "Zeit", "ge\u00b7bau\u00b7et", ",", "sich", "zier\u00b7lich", "un\u00b7sern", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "ADV", "APPR", "PPOSAT", "NN", "VVPP", "$,", "PRF", "ADJD", "PPOSAT", "NN"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.24": {"text": "vor.", "tokens": ["vor", "."], "token_info": ["word", "punct"], "pos": ["PTKVZ", "$."], "meter": "+", "measure": "single.up"}, "line.25": {"text": "Auf diesem ist ein gr\u00fcner Platz, fast viereckt, auf dem Wall", "tokens": ["Auf", "die\u00b7sem", "ist", "ein", "gr\u00fc\u00b7ner", "Platz", ",", "fast", "vier\u00b7eckt", ",", "auf", "dem", "Wall"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PDAT", "VAFIN", "ART", "ADJA", "NN", "$,", "ADV", "ADJD", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.26": {"text": "zu seh'n.", "tokens": ["zu", "seh'", "n."], "token_info": ["word", "word", "abbreviation"], "pos": ["APPR", "NE", "NE"], "meter": "-+", "measure": "iambic.single"}, "line.27": {"text": "Der Wall formiert hier einen Winkel, und l\u00e4\u00dft uns in die", "tokens": ["Der", "Wall", "for\u00b7miert", "hier", "ei\u00b7nen", "Win\u00b7kel", ",", "und", "l\u00e4\u00dft", "uns", "in", "die"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NE", "VVFIN", "ADV", "ART", "NN", "$,", "KON", "VVFIN", "PPER", "APPR", "ART"], "meter": "-+-+-+-+--+-++", "measure": "iambic.septa.relaxed"}, "line.28": {"text": "Senkung geh'n", "tokens": ["Sen\u00b7kung", "geh'n"], "token_info": ["word", "word"], "pos": ["NN", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.29": {"text": "Der neu- gemachten ", "tokens": ["Der", "neu", "ge\u00b7mach\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["ART", "TRUNC", "ADJA"], "meter": "-+-+-", "measure": "iambic.di"}, "line.30": {"text": "Geb\u00e4ude,", "tokens": ["Ge\u00b7b\u00e4u\u00b7de", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.31": {"text": "Das blau- glas\u00fcrte Ziegel-Decken, macht eine neue Augen-", "tokens": ["Das", "blau", "glas\u00b7\u00fcr\u00b7te", "Zie\u00b7gel\u00b7De\u00b7cken", ",", "macht", "ei\u00b7ne", "neu\u00b7e", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,", "VVFIN", "ART", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+--+-+-", "measure": "iambic.septa.relaxed"}, "line.32": {"text": "weide.", "tokens": ["wei\u00b7de", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.33": {"text": "Die\u00df dienet, bey der Garnison von diesem Schlo\u00df, dem Of-", "tokens": ["Die\u00df", "die\u00b7net", ",", "bey", "der", "Gar\u00b7ni\u00b7son", "von", "die\u00b7sem", "Schlo\u00df", ",", "dem", "Of"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PDS", "VVFIN", "$,", "APPR", "ART", "NN", "APPR", "PDAT", "NN", "$,", "ART", "TRUNC"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.34": {"text": "ficier,", "tokens": ["fi\u00b7cier", ","], "token_info": ["word", "punct"], "pos": ["PTKVZ", "$,"], "meter": "--", "measure": "unknown.measure.zero"}, "line.35": {"text": "Nachdem es eben neu erbauet, zu einem zierlichen Quar-", "tokens": ["Nach\u00b7dem", "es", "e\u00b7ben", "neu", "er\u00b7bau\u00b7et", ",", "zu", "ei\u00b7nem", "zier\u00b7li\u00b7chen", "Qua\u00b7r"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "VVFIN", "$,", "APPR", "ART", "ADJA", "TRUNC"], "meter": "-+-+-+-+--+-+--+-", "measure": "iambic.septa.relaxed"}, "line.36": {"text": "tier.", "tokens": ["tier", "."], "token_info": ["word", "punct"], "pos": ["ADJD", "$."], "meter": "+", "measure": "single.up"}, "line.37": {"text": "Hierauf erblicket man nun ferner den mit so manchem Jpern-", "tokens": ["Hier\u00b7auf", "er\u00b7bli\u00b7cket", "man", "nun", "fer\u00b7ner", "den", "mit", "so", "man\u00b7chem", "Jpern"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PIS", "ADV", "ADV", "ART", "APPR", "ADV", "PIAT", "TRUNC"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.38": {"text": "Stamm,", "tokens": ["Stamm", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+", "measure": "single.up"}, "line.39": {"text": "Und vielen Weiden wohl besetzten, geraden, langen, gr\u00fcnen", "tokens": ["Und", "vie\u00b7len", "Wei\u00b7den", "wohl", "be\u00b7setz\u00b7ten", ",", "ge\u00b7ra\u00b7den", ",", "lan\u00b7gen", ",", "gr\u00fc\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["KON", "PIAT", "NN", "ADV", "VVFIN", "$,", "VVPP", "$,", "ADJA", "$,", "ADJA"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.40": {"text": "Damm,", "tokens": ["Damm", ","], "token_info": ["word", "punct"], "pos": ["NE", "$,"], "meter": "+", "measure": "single.up"}, "line.41": {"text": "Der beyde breite Graben theilt, des \u00e4ussern Grabens glatte", "tokens": ["Der", "bey\u00b7de", "brei\u00b7te", "Gra\u00b7ben", "theilt", ",", "des", "\u00e4us\u00b7sern", "Gra\u00b7bens", "glat\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "PIAT", "ADJA", "NN", "VVFIN", "$,", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.42": {"text": "Fluht,", "tokens": ["Fluht", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+", "measure": "single.up"}, "line.43": {"text": "Die hier in gr\u00fcn- bebl\u00fchmten Ufern, von Winden ungest\u00f6ret,", "tokens": ["Die", "hier", "in", "gr\u00fcn", "be\u00b7bl\u00fchm\u00b7ten", "U\u00b7fern", ",", "von", "Win\u00b7den", "un\u00b7ge\u00b7st\u00f6\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "TRUNC", "ADJA", "NN", "$,", "APPR", "NN", "ADJD", "$,"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.44": {"text": "ruht,", "tokens": ["ruht", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+", "measure": "single.up"}, "line.45": {"text": "Zeigt, als in einem rein- polierten, durch Wiederschein", "tokens": ["Zeigt", ",", "als", "in", "ei\u00b7nem", "rein", "po\u00b7lier\u00b7ten", ",", "durch", "Wie\u00b7der\u00b7schein"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "$,", "KOUS", "APPR", "ART", "TRUNC", "ADJA", "$,", "APPR", "NN"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.46": {"text": "bestrahlten Spiegel,", "tokens": ["be\u00b7strahl\u00b7ten", "Spie\u00b7gel", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.47": {"text": "Der kleinen schon erwehnten G\u00e4rten bebl\u00fchmt- und sanft-", "tokens": ["Der", "klei\u00b7nen", "schon", "er\u00b7wehn\u00b7ten", "G\u00e4r\u00b7ten", "be\u00b7bl\u00fchm\u00b7t", "und", "sanft"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADV", "ADJA", "NN", "TRUNC", "KON", "TRUNC"], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.48": {"text": "erhabne H\u00fcgel,", "tokens": ["er\u00b7hab\u00b7ne", "H\u00fc\u00b7gel", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.49": {"text": "Worauf man selbst ihr Urbild sieht, mit B\u00e4umen hin und her", "tokens": ["Wo\u00b7rauf", "man", "selbst", "ihr", "Ur\u00b7bild", "sieht", ",", "mit", "B\u00e4u\u00b7men", "hin", "und", "her"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PAV", "PIS", "ADV", "PPOSAT", "NN", "VVFIN", "$,", "APPR", "NN", "PTKVZ", "KON", "ADV"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.50": {"text": "besetzt,", "tokens": ["be\u00b7setzt", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.51": {"text": "Wovon uns die Verschiedenheit noch mehr den frohen Blick", "tokens": ["Wo\u00b7von", "uns", "die", "Ver\u00b7schie\u00b7den\u00b7heit", "noch", "mehr", "den", "fro\u00b7hen", "Blick"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ART", "NN", "ADV", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.52": {"text": "ergetzt.", "tokens": ["er\u00b7getzt", "."], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "-+", "measure": "iambic.single"}, "line.53": {"text": "Die G\u00e4rten reichen an die Gasse, an welcher art\u2019ge H\u00e4user", "tokens": ["Die", "G\u00e4r\u00b7ten", "rei\u00b7chen", "an", "die", "Gas\u00b7se", ",", "an", "wel\u00b7cher", "art'\u00b7ge", "H\u00e4u\u00b7ser"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,", "APPR", "PWAT", "ADJA", "NN"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.54": {"text": "steh'n,", "tokens": ["steh'n", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "-", "measure": "single.down"}, "line.55": {"text": "Von welchen hier, aus diesem Fenster, vier der ansehnlichsten", "tokens": ["Von", "wel\u00b7chen", "hier", ",", "aus", "die\u00b7sem", "Fens\u00b7ter", ",", "vier", "der", "an\u00b7sehn\u00b7lichs\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PWAT", "ADV", "$,", "APPR", "PDAT", "NN", "$,", "CARD", "ART", "NN"], "meter": "-+-+-+-+-+-+-+-