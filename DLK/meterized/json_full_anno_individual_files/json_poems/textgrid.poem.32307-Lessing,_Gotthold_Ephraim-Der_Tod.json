{"textgrid.poem.32307": {"metadata": {"author": {"name": "Lessing, Gotthold Ephraim", "birth": "N.A.", "death": "N.A."}, "title": "Der Tod", "genre": "verse", "period": "N.A.", "pub_year": 1755, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gestern, Br\u00fcder, k\u00f6nnt ihrs glauben?", "tokens": ["Ge\u00b7stern", ",", "Br\u00fc\u00b7der", ",", "k\u00f6nnt", "ihrs", "glau\u00b7ben", "?"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "VVFIN", "PIS", "VVINF", "$."], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Gestern bei dem Saft der Trauben,", "tokens": ["Ge\u00b7stern", "bei", "dem", "Saft", "der", "Trau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "(bildet euch mein Schrecken ein!)", "tokens": ["(", "bil\u00b7det", "euch", "mein", "Schre\u00b7cken", "ein", "!", ")"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "PPER", "PPOSAT", "NN", "PTKVZ", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Kam der Tod zu mir herein.", "tokens": ["Kam", "der", "Tod", "zu", "mir", "her\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "APPR", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Drohend schwang er seine Hippe,", "tokens": ["Dro\u00b7hend", "schwang", "er", "sei\u00b7ne", "Hip\u00b7pe", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Drohend sprach das Furchtgerippe:", "tokens": ["Dro\u00b7hend", "sprach", "das", "Furcht\u00b7ge\u00b7rip\u00b7pe", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Fort, du teurer Bacchusknecht!", "tokens": ["Fort", ",", "du", "teu\u00b7rer", "Bac\u00b7chus\u00b7knecht", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Fort, du hast genug gezecht!", "tokens": ["Fort", ",", "du", "hast", "ge\u00b7nug", "ge\u00b7zecht", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "VAFIN", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Lieber Tod, sprach ich mit Tr\u00e4nen,", "tokens": ["Lie\u00b7ber", "Tod", ",", "sprach", "ich", "mit", "Tr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "$,", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Solltest du nach mir dich sehnen?", "tokens": ["Soll\u00b7test", "du", "nach", "mir", "dich", "seh\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "PPER", "PRF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sieh, da stehet Wein f\u00fcr dich!", "tokens": ["Sieh", ",", "da", "ste\u00b7het", "Wein", "f\u00fcr", "dich", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "VVFIN", "NN", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Lieber Tod verschone mich!", "tokens": ["Lie\u00b7ber", "Tod", "ver\u00b7scho\u00b7ne", "mich", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "L\u00e4chelnd greift er nach dem Glase;", "tokens": ["L\u00e4\u00b7chelnd", "greift", "er", "nach", "dem", "Gla\u00b7se", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "L\u00e4chelnd macht ers auf der Base,", "tokens": ["L\u00e4\u00b7chelnd", "macht", "ers", "auf", "der", "Ba\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PIS", "APPR", "ART", "NN", "$,"], "meter": "+-+-+---", "measure": "unknown.measure.tri"}, "line.3": {"text": "Auf der Pest, Gesundheit leer;", "tokens": ["Auf", "der", "Pest", ",", "Ge\u00b7sund\u00b7heit", "leer", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "L\u00e4chelnd setzt ers wieder her.", "tokens": ["L\u00e4\u00b7chelnd", "setzt", "ers", "wie\u00b7der", "her", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PIS", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Fr\u00f6hlich glaub' ich mich befreiet,", "tokens": ["Fr\u00f6h\u00b7lich", "glaub'", "ich", "mich", "be\u00b7frei\u00b7et", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Als er schnell sein Drohn erneuet.", "tokens": ["Als", "er", "schnell", "sein", "Drohn", "er\u00b7neu\u00b7et", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Narre, f\u00fcr dein Gl\u00e4schen Wein", "tokens": ["Nar\u00b7re", ",", "f\u00fcr", "dein", "Gl\u00e4s\u00b7chen", "Wein"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "APPR", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Denkst du, spricht er, los zu sein?", "tokens": ["Denkst", "du", ",", "spricht", "er", ",", "los", "zu", "sein", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "VVFIN", "PPER", "$,", "ADJD", "PTKZU", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Tod, bat ich, ich m\u00f6cht' auf Erden", "tokens": ["Tod", ",", "bat", "ich", ",", "ich", "m\u00f6cht'", "auf", "Er\u00b7den"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "VVFIN", "PPER", "$,", "PPER", "VMFIN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gern ein Mediziner werden.", "tokens": ["Gern", "ein", "Me\u00b7di\u00b7zi\u00b7ner", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VAINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "La\u00df mich: ich verspreche dir", "tokens": ["La\u00df", "mich", ":", "ich", "ver\u00b7spre\u00b7che", "dir"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVIMP", "PPER", "$.", "PPER", "VVFIN", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Meine Kranken halb daf\u00fcr.", "tokens": ["Mei\u00b7ne", "Kran\u00b7ken", "halb", "da\u00b7f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "PAV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Gut, wenn das ist, magst du leben:", "tokens": ["Gut", ",", "wenn", "das", "ist", ",", "magst", "du", "le\u00b7ben", ":"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "KOUS", "PDS", "VAFIN", "$,", "VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ruft er. Nur sei mir ergeben.", "tokens": ["Ruft", "er", ".", "Nur", "sei", "mir", "er\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$.", "ADV", "VAFIN", "PPER", "VVPP", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Lebe, bis du satt gek\u00fc\u00dft,", "tokens": ["Le\u00b7be", ",", "bis", "du", "satt", "ge\u00b7k\u00fc\u00dft", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "KOUS", "PPER", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und des Trinkens m\u00fcde bist.", "tokens": ["Und", "des", "Trin\u00b7kens", "m\u00fc\u00b7de", "bist", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJD", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "O! wie sch\u00f6n klingt dies den Ohren!", "tokens": ["O", "!", "wie", "sch\u00f6n", "klingt", "dies", "den", "Oh\u00b7ren", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PWAV", "ADJD", "VVFIN", "PDS", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Tod, du hast mich neu geboren.", "tokens": ["Tod", ",", "du", "hast", "mich", "neu", "ge\u00b7bo\u00b7ren", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VAFIN", "PPER", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Dieses Glas voll Rebensaft,", "tokens": ["Die\u00b7ses", "Glas", "voll", "Re\u00b7ben\u00b7saft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "ADJD", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Tod, auf gute Br\u00fcderschaft!", "tokens": ["Tod", ",", "auf", "gu\u00b7te", "Br\u00fc\u00b7der\u00b7schaft", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Ewig mu\u00df ich also leben,", "tokens": ["E\u00b7wig", "mu\u00df", "ich", "al\u00b7so", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ewig! denn, beim Gott der Reben!", "tokens": ["E\u00b7wig", "!", "denn", ",", "beim", "Gott", "der", "Re\u00b7ben", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$.", "ADV", "$,", "APPRART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ewig soll mich Lieb' und Wein,", "tokens": ["E\u00b7wig", "soll", "mich", "Lieb'", "und", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ewig Wein und Lieb' erfreun!", "tokens": ["E\u00b7wig", "Wein", "und", "Lieb'", "er\u00b7freun", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "KON", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Gestern, Br\u00