s": ["NN", "$,", "APPR", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Denkst du, spricht er, los zu sein?", "tokens": ["Denkst", "du", ",", "spricht", "er", ",", "los", "zu", "sein", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "VVFIN", "PPER", "$,", "ADJD", "PTKZU", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.15": {"line.1": {"text": "Tod, bat ich, ich m\u00f6cht' auf Erden", "tokens": ["Tod", ",", "bat", "ich", ",", "ich", "m\u00f6cht'", "auf", "Er\u00b7den"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "VVFIN", "PPER", "$,", "PPER", "VMFIN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gern ein Mediziner werden.", "tokens": ["Gern", "ein", "Me\u00b7di\u00b7zi\u00b7ner", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VAINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "La\u00df mich: ich verspreche dir", "tokens": ["La\u00df", "mich", ":", "ich", "ver\u00b7spre\u00b7che", "dir"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVIMP", "PPER", "$.", "PPER", "VVFIN", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Meine Kranken halb daf\u00fcr.", "tokens": ["Mei\u00b7ne", "Kran\u00b7ken", "halb", "da\u00b7f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "PAV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.16": {"line.1": {"text": "Gut, wenn das ist, magst du leben:", "tokens": ["Gut", ",", "wenn", "das", "ist", ",", "magst", "du", "le\u00b7ben", ":"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", 