{"dta.poem.19811": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "M\u00fcllerlied .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519168", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der M\u00fcller auf seim R\u00f6\u00dflein sa\u00df,               ", "tokens": ["Der", "M\u00fcl\u00b7ler", "auf", "seim", "R\u00f6\u00df\u00b7lein", "sa\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gar wohl er in die M\u00fchle sah,", "tokens": ["Gar", "wohl", "er", "in", "die", "M\u00fch\u00b7le", "sah", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er th\u00e4t dem Annely winken,", "tokens": ["Er", "th\u00e4t", "dem", "An\u00b7ne\u00b7ly", "win\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NE", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "O Annelin, liebstes Annelin mein,", "tokens": ["O", "An\u00b7ne\u00b7lin", ",", "liebs\u00b7tes", "An\u00b7ne\u00b7lin", "mein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "ADJA", "NN", "PPOSAT", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Hilf mir den Wein austrinken.", "tokens": ["Hilf", "mir", "den", "Wein", "aus\u00b7trin\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ART", "NN", "VVINF", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.2": {"line.1": {"text": "Und da der Wein austrunken war,", "tokens": ["Und", "da", "der", "Wein", "aus\u00b7trun\u00b7ken", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da kam ein grober Bauer dar,", "tokens": ["Da", "kam", "ein", "gro\u00b7ber", "Bau\u00b7er", "dar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er bracht dem M\u00fcller S\u00e4cke,", "tokens": ["Er", "bracht", "dem", "M\u00fcl\u00b7ler", "S\u00e4\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NE", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der M\u00fcller dacht in seinem Sinn,", "tokens": ["Der", "M\u00fcl\u00b7ler", "dacht", "in", "sei\u00b7nem", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "H\u00e4tt Korn ich drein gemessen.", "tokens": ["H\u00e4tt", "Korn", "ich", "drein", "ge\u00b7mes\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Der M\u00fcller in die M\u00fchle trat,", "tokens": ["Der", "M\u00fcl\u00b7ler", "in", "die", "M\u00fch\u00b7le", "trat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er w\u00fcnscht den S\u00e4cken guten Tag,", "tokens": ["Er", "w\u00fcnscht", "den", "S\u00e4\u00b7cken", "gu\u00b7ten", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Th\u00e4t in die Lauten schlagen,", "tokens": ["Th\u00e4t", "in", "die", "Lau\u00b7ten", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und welcher Sack nit tanzen will,", "tokens": ["Und", "wel\u00b7cher", "Sack", "nit", "tan\u00b7zen", "will", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAT", "NN", "PTKNEG", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Den nimmt er bei dem Kragen.", "tokens": ["Den", "nimmt", "er", "bei", "dem", "Kra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Das B\u00e4urlein in die M\u00fchle trat,", "tokens": ["Das", "B\u00e4ur\u00b7lein", "in", "die", "M\u00fch\u00b7le", "trat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er w\u00fcnscht dem M\u00fcller guten Tag,", "tokens": ["Er", "w\u00fcnscht", "dem", "M\u00fcl\u00b7ler", "gu\u00b7ten", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NE", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Darzu ein guten Morgen,", "tokens": ["Dar\u00b7zu", "ein", "gu\u00b7ten", "Mor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dank hab, Dank hab du grober Baur,", "tokens": ["Dank", "hab", ",", "Dank", "hab", "du", "gro\u00b7ber", "Baur", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VAFIN", "$,", "NN", "VAFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Was willstu bei mir holen.", "tokens": ["Was", "will\u00b7stu", "bei", "mir", "ho\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Das Bauerlein in die M\u00fchle schreit,", "tokens": ["Das", "Bau\u00b7er\u00b7lein", "in", "die", "M\u00fch\u00b7le", "schreit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "M\u00fcller hast mir das Mehl bereit?", "tokens": ["M\u00fcl\u00b7ler", "hast", "mir", "das", "Mehl", "be\u00b7reit", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "ART", "NN", "ADJD", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Du hast mirs halber gestolen,", "tokens": ["Du", "hast", "mirs", "hal\u00b7ber", "ge\u00b7sto\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NE", "APPO", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Du l\u00fcgst, du l\u00fcgst du grober Bauer,", "tokens": ["Du", "l\u00fcgst", ",", "du", "l\u00fcgst", "du", "gro\u00b7ber", "Bau\u00b7er", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ist mir in der M\u00fchl verstoben.", "tokens": ["Ist", "mir", "in", "der", "M\u00fchl", "ver\u00b7sto\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Das B\u00e4urlein aus der M\u00fchle trat,", "tokens": ["Das", "B\u00e4ur\u00b7lein", "aus", "der", "M\u00fch\u00b7le", "trat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das Annelein ihm die Wahrheit sagt,", "tokens": ["Das", "An\u00b7nel\u00b7ein", "ihm", "die", "Wahr\u00b7heit", "sagt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Du hast der Kleie vergessen,", "tokens": ["Du", "hast", "der", "Klei\u00b7e", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Ach nein, ach nein, liebs Annelin,", "tokens": ["Ach", "nein", ",", "ach", "nein", ",", "liebs", "An\u00b7ne\u00b7lin", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PTKANT", "$,", "XY", "PTKANT", "$,", "VVFIN", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Des M\u00fcllers Schwein han's gessen.", "tokens": ["Des", "M\u00fcl\u00b7lers", "Schwein", "han's", "ges\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Der M\u00fcller h\u00e4tt die fettsten Schwein,", "tokens": ["Der", "M\u00fcl\u00b7ler", "h\u00e4tt", "die", "fetts\u00b7ten", "Schwein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die in dem Lande m\u00f6gen seyn,", "tokens": ["Die", "in", "dem", "Lan\u00b7de", "m\u00f6\u00b7gen", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VMFIN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er m\u00e4sts aus Bauern S\u00e4cken.", "tokens": ["Er", "m\u00e4sts", "aus", "Bau\u00b7ern", "S\u00e4\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da mu\u00df sich mancher arme Bauer", "tokens": ["Da", "mu\u00df", "sich", "man\u00b7cher", "ar\u00b7me", "Bau\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PRF", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Sein M\u00e4gd und Knecht fr\u00fch wecken.", "tokens": ["Sein", "M\u00e4gd", "und", "Knecht", "fr\u00fch", "we\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Der M\u00fcller war sogar verwegen,", "tokens": ["Der", "M\u00fcl\u00b7ler", "war", "so\u00b7gar", "ver\u00b7we\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Er ist dem Bauer in Weg gelegen,", "tokens": ["Er", "ist", "dem", "Bau\u00b7er", "in", "Weg", "ge\u00b7le\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Es hat ihn sehr verdrossen,", "tokens": ["Es", "hat", "ihn", "sehr", "ver\u00b7dros\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dasselbig that das M\u00fcllerlein gut,", "tokens": ["Das\u00b7sel\u00b7big", "that", "das", "M\u00fcl\u00b7ler\u00b7lein", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Ist ihm gar \u00fcbel erschossen.", "tokens": ["Ist", "ihm", "gar", "\u00fc\u00b7bel", "er\u00b7schos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "VVPP", "$."], "meter": "+-+---+-", "measure": "unknown.measure.tri"}}, "stanza.9": {"line.1": {"text": "Der M\u00fcller g\u00e4b ein Batzen drum,", "tokens": ["Der", "M\u00fcl\u00b7ler", "g\u00e4b", "ein", "Bat\u00b7zen", "drum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "ART", "NN", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df man ihms Liedlein nimmer sung,", "tokens": ["Da\u00df", "man", "ihms", "Lied\u00b7lein", "nim\u00b7mer", "sung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPRART", "NN", "ADV", "NN", "$,"], "meter": "+--+-+--", "measure": "iambic.tri.invert"}, "line.3": {"text": "Er thuts gar \u00fcbel hassen,", "tokens": ["Er", "thuts", "gar", "\u00fc\u00b7bel", "has\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Singt man das in der Stuben nit,", "tokens": ["Singt", "man", "das", "in", "der", "Stu\u00b7ben", "nit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ART", "APPR", "ART", "NN", "PTKNEG", "$,"], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.5": {"text": "So singt mans auf der Gassen.", "tokens": ["So", "singt", "mans", "auf", "der", "Gas\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Der uns das Liedlein neu gesang,", "tokens": ["Der", "uns", "das", "Lied\u00b7lein", "neu", "ge\u00b7sang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein grober Bauer ist er genannt.", "tokens": ["Ein", "gro\u00b7ber", "Bau\u00b7er", "ist", "er", "ge\u00b7nannt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Er hats gar wohl gesungen,", "tokens": ["Er", "hats", "gar", "wohl", "ge\u00b7sun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Er hat drei S\u00e4ck in die M\u00fchle gethan,", "tokens": ["Er", "hat", "drei", "S\u00e4ck", "in", "die", "M\u00fch\u00b7le", "ge\u00b7than", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "CARD", "NN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Sind ihm zwey wiederkommen.", "tokens": ["Sind", "ihm", "zwey", "wie\u00b7der\u00b7kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "CARD", "VVINF", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}}}}}