{"textgrid.poem.33854": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kein Lebewohl, kein banges Scheiden!", "tokens": ["Kein", "Le\u00b7be\u00b7wohl", ",", "kein", "ban\u00b7ges", "Schei\u00b7den", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Viel lieber ein Geschiedensein!", "tokens": ["Viel", "lie\u00b7ber", "ein", "Ge\u00b7schie\u00b7den\u00b7sein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ertragen kann ich jedes Leiden,", "tokens": ["Er\u00b7tra\u00b7gen", "kann", "ich", "je\u00b7des", "Lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Doch trinken kann ich's nicht, wie Wein.", "tokens": ["Doch", "trin\u00b7ken", "kann", "ich's", "nicht", ",", "wie", "Wein", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVINF", "VMFIN", "PIS", "PTKNEG", "$,", "PWAV", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wir sa\u00dfen gestern noch beisammen,", "tokens": ["Wir", "sa\u00b7\u00dfen", "ge\u00b7stern", "noch", "bei\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "PTKVZ", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Von Trennung wu\u00dft' ich selbst noch kaum!", "tokens": ["Von", "Tren\u00b7nung", "wu\u00dft'", "ich", "selbst", "noch", "kaum", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "ADV", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das Herz trieb seine alten Flammen,", "tokens": ["Das", "Herz", "trieb", "sei\u00b7ne", "al\u00b7ten", "Flam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Seele spann den alten Traum.", "tokens": ["Die", "See\u00b7le", "spann", "den", "al\u00b7ten", "Traum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Dann rasch ein Ku\u00df vom lieben Munde,", "tokens": ["Dann", "rasch", "ein", "Ku\u00df", "vom", "lie\u00b7ben", "Mun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nicht Schmerz getr\u00e4nkt, nicht Angst verk\u00fcrzt!", "tokens": ["Nicht", "Schmerz", "ge\u00b7tr\u00e4nkt", ",", "nicht", "Angst", "ver\u00b7k\u00fcrzt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "VVPP", "$,", "PTKNEG", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das nenn' ich eine Abschiedsstunde,", "tokens": ["Das", "nenn'", "ich", "ei\u00b7ne", "Ab\u00b7schieds\u00b7stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text