{"textgrid.poem.67440": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Das Gesetz der Natur", "genre": "verse", "period": "N.A.", "pub_year": 1793, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Alle tragen wir in uns den Keim zu unserm Verbl\u00fchen;", "tokens": ["Al\u00b7le", "tra\u00b7gen", "wir", "in", "uns", "den", "Keim", "zu", "un\u00b7serm", "Ver\u00b7bl\u00fc\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "APPR", "PPER", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Bl\u00fchn und Verbl\u00fchen ist nur eine Entwicklung der Zeit.", "tokens": ["Bl\u00fchn", "und", "Ver\u00b7bl\u00fc\u00b7hen", "ist", "nur", "ei\u00b7ne", "Ent\u00b7wick\u00b7lung", "der", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VAFIN", "ADV", "ART", "NN", "ART", "NN", "$."], "meter": "+--+-+-+--+--+", "measure": "iambic.hexa.invert"}, "line.3": {"text": "In dem Schoo\u00dfe der gro\u00dfen Mutter empfangen wir Kr\u00e4fte,", "tokens": ["In", "dem", "Schoo\u00b7\u00dfe", "der", "gro\u00b7\u00dfen", "Mut\u00b7ter", "emp\u00b7fan\u00b7gen", "wir", "Kr\u00e4f\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "PPER", "NN", "$,"], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Auszuwirken uns selbst und zu verleben damit.", "tokens": ["Aus\u00b7zu\u00b7wir\u00b7ken", "uns", "selbst", "und", "zu", "ver\u00b7le\u00b7ben", "da\u00b7mit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "KON", "PTKZU", "VVINF", "PAV", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.5": {"text": "Und Du murrest, o Kl\u00fcgling, da\u00df Du nicht ewig hier sein kannst?", "tokens": ["Und", "Du", "mur\u00b7rest", ",", "o", "Kl\u00fcg\u00b7ling", ",", "da\u00df", "Du", "nicht", "e\u00b7wig", "hier", "sein", "kannst", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "FM", "NN", "$,", "KOUS", "PPER", "PTKNEG", "ADJD", "ADV", "VAINF", "VMFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.6": {"text": "Warest Du ewig hier, wirst Du's in Andern nicht sein.", "tokens": ["Wa\u00b7rest", "Du", "e\u00b7wig", "hier", ",", "wirst", "Du's", "in", "An\u00b7dern", "nicht", "sein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "ADV", "$,", "VAFIN", "NE", "APPR", "ADJA", "PTKNEG", "VAINF", "$."], "meter": "+--+-+-+-+--+", "measure": "iambic.hexa.invert"}, "line.7": {"text": "Also gehorche der Kette der Wesen, die ziehet und abst\u00f6\u00dft;", "tokens": ["Al\u00b7so", "ge\u00b7hor\u00b7che", "der", "Ket\u00b7te", "der", "We\u00b7sen", ",", "die", "zie\u00b7het", "und", "ab\u00b7st\u00f6\u00dft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "$,", "PRELS", "VVFIN", "KON", "VVFIN", "$."], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.8": {"text": "Was zur Bl\u00fcthe Dich trieb, gab Dir Vollendung und Frucht.", "tokens": ["Was", "zur", "Bl\u00fc\u00b7the", "Dich", "trieb", ",", "gab", "Dir", "Vol\u00b7len\u00b7dung", "und", "Frucht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "wor