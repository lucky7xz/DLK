{"textgrid.poem.50594": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Wittwentrauer", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Clorinde trau'rt um ihren Mann,", "tokens": ["Clo\u00b7rin\u00b7de", "trau'rt", "um", "ih\u00b7ren", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den sie in Wort und Werk \u2013 sie schw\u00f6rt es \u2013 nie betrogen;", "tokens": ["Den", "sie", "in", "Wort", "und", "Werk", "\u2013", "sie", "schw\u00f6rt", "es", "\u2013", "nie", "be\u00b7tro\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NN", "KON", "NN", "$(", "PPER", "VVFIN", "PPER", "$(", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sie hat ein zierlich Trau'rkleid an,", "tokens": ["Sie", "hat", "ein", "zier\u00b7lich", "Trau'r\u00b7kleid", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJD", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ihr Zimmer ist mit Boy bis auf den Grund bezogen,", "tokens": ["Ihr", "Zim\u00b7mer", "ist", "mit", "Boy", "bis", "auf", "den", "Grund", "be\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "APPR", "NE", "ADV", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Schwarz ist die Deck' auf ihrem Bett';", "tokens": ["Schwarz", "ist", "d