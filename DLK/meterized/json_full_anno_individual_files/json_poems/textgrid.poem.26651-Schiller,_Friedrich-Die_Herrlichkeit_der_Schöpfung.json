{"textgrid.poem.26651": {"metadata": {"author": {"name": "Schiller, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Herrlichkeit der Sch\u00f6pfung", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vor\u00fcber war der Sturm, der Donner Rollen", "tokens": ["Vor\u00b7\u00fc\u00b7ber", "war", "der", "Sturm", ",", "der", "Don\u00b7ner", "Rol\u00b7len"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "$,", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das hallende Gebirg hinein verschollen,", "tokens": ["Das", "hal\u00b7len\u00b7de", "Ge\u00b7birg", "hin\u00b7ein", "ver\u00b7schol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APZR", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Geflohn die Dunkelheit;", "tokens": ["Ge\u00b7flohn", "die", "Dun\u00b7kel\u00b7heit", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "In junger Sch\u00f6ne l\u00e4chelten die Himmel wieder", "tokens": ["In", "jun\u00b7ger", "Sch\u00f6\u00b7ne", "l\u00e4\u00b7chel\u00b7ten", "die", "Him\u00b7mel", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "NN", "ADV"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Auf ihre Schwester, Gottes Erde, nieder", "tokens": ["Auf", "ih\u00b7re", "Schwes\u00b7ter", ",", "Got\u00b7tes", "Er\u00b7de", ",", "nie\u00b7der"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "NN", "NN", "$,", "PTKVZ"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Voll Z\u00e4rtlichkeit.", "tokens": ["Voll", "Z\u00e4rt\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Es lagen lustig da die Auen und die Tale,", "tokens": ["Es", "la\u00b7gen", "lus\u00b7tig", "da", "die", "Au\u00b7en", "und", "die", "Ta\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ADV", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Aus Maigew\u00f6lken von der Sonnen Strahle", "tokens": ["Aus", "Mai\u00b7ge\u00b7w\u00f6l\u00b7ken", "von", "der", "Son\u00b7nen", "Strah\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Holdselig angelacht:", "tokens": ["Hold\u00b7se\u00b7lig", "an\u00b7ge\u00b7lacht", ":"], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVPP", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.10": {"text": "Die Str\u00f6me schimmerten, die B\u00fcsch und W\u00e4ldchen alle", "tokens": ["Die", "Str\u00f6\u00b7me", "schim\u00b7mer\u00b7ten", ",", "die", "B\u00fcsch", "und", "W\u00e4ld\u00b7chen", "al\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "KON", "NN", "PIAT"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Bewegten freudig sich im tauigen Kristalle,", "tokens": ["Be\u00b7weg\u00b7ten", "freu\u00b7dig", "sich", "im", "tau\u00b7i\u00b7gen", "Kris\u00b7tal\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PRF", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "In funkelndlichter Pracht.", "tokens": ["In", "fun\u00b7kelnd\u00b7lich\u00b7ter", "Pracht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "Und sieh! da hebt von Berg zu Berg sich pr\u00e4chtig ausgespannt", "tokens": ["Und", "sieh", "!", "da", "hebt", "von", "Berg", "zu", "Berg", "sich", "pr\u00e4ch\u00b7tig", "aus\u00b7ge\u00b7spannt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVIMP", "$.", "ADV", "VVFIN", "APPR", "NN", "APPR", "NN", "PRF", "ADJD", "VVPP"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.14": {"text": "Ein Regenbogen \u00fcbers Land. \u2013", "tokens": ["Ein", "Re\u00b7gen\u00b7bo\u00b7gen", "\u00fc\u00b7bers", "Land", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "In dieser Ansicht schwamm vom Brocken oben", "tokens": ["In", "die\u00b7ser", "An\u00b7sicht", "schwamm", "vom", "Bro\u00b7cken", "o\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "VVFIN", "APPRART", "NN", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mein Auge trunken, als ich aufgehoben", "tokens": ["Mein", "Au\u00b7ge", "trun\u00b7ken", ",", "als", "ich", "auf\u00b7ge\u00b7ho\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADJD", "$,", "KOUS", "PPER", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mich pl\u00f6tzlich f\u00fchlte... Heilig heilge L\u00fcfte kamen,", "tokens": ["Mich", "pl\u00f6tz\u00b7lich", "f\u00fchl\u00b7te", "...", "Hei\u00b7lig", "heil\u00b7ge", "L\u00fcf\u00b7te", "ka\u00b7men", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVFIN", "$(", "ADJA", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Umwebten z\u00e4rtlich mich, indessen \u00fcber mir,", "tokens": ["Um\u00b7web\u00b7ten", "z\u00e4rt\u00b7lich", "mich", ",", "in\u00b7des\u00b7sen", "\u00fc\u00b7ber", "mir", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PPER", "$,", "ADV", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Stolztragend \u00fcbers All den Ewigen daher,", "tokens": ["Stolz\u00b7tra\u00b7gend", "\u00fc\u00b7bers", "All", "den", "E\u00b7wi\u00b7gen", "da\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "ART", "ADJA", "PAV", "$,"], "meter": "-+-+-+-+---+", "measure": "unknown.measure.penta"}, "line.6": {"text": "Die innre Himmel majest\u00e4tisch schwammen.", "tokens": ["Die", "inn\u00b7re", "Him\u00b7mel", "ma\u00b7jes\u00b7t\u00e4\u00b7tisch", "schwam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und itzt trieb ein Wind", "tokens": ["Und", "itzt", "trieb", "ein", "Wind"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN"], "meter": "--+-+", "measure": "anapaest.init"}, "line.2": {"text": "Fort die Wolken, mich auf ihrem Zuge,", "tokens": ["Fort", "die", "Wol\u00b7ken", ",", "mich", "auf", "ih\u00b7rem", "Zu\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "PRF", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Unter mir wichen im Fluge", "tokens": ["Un\u00b7ter", "mir", "wi\u00b7chen", "im", "Flu\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "VVFIN", "APPRART", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Schimmernde K\u00f6nigesst\u00e4dte zur\u00fcck,", "tokens": ["Schim\u00b7mern\u00b7de", "K\u00f6\u00b7ni\u00b7ges\u00b7st\u00e4d\u00b7te", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKVZ", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.5": {"text": "Schnell wie ein Blick", "tokens": ["Schnell", "wie", "ein", "Blick"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "KOKOM", "ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "L\u00e4nderbeschattende Berge zur\u00fcck,", "tokens": ["L\u00e4n\u00b7der\u00b7be\u00b7schat\u00b7ten\u00b7de", "Ber\u00b7ge", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKVZ", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.7": {"text": "Und das sch\u00f6nste Gemisch von bl\u00fchenden Feldern,", "tokens": ["Und", "das", "sch\u00f6ns\u00b7te", "Ge\u00b7misch", "von", "bl\u00fc\u00b7hen\u00b7den", "Fel\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "--+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Goldenen Saaten und gr\u00fcnenden W\u00e4ldern,", "tokens": ["Gol\u00b7de\u00b7nen", "Saa\u00b7ten", "und", "gr\u00fc\u00b7nen\u00b7den", "W\u00e4l\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "ADJA", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.9": {"text": "Himmel und Erde im lachenden Glanz", "tokens": ["Him\u00b7mel", "und", "Er\u00b7de", "im", "la\u00b7chen\u00b7den", "Glanz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "APPRART", "ADJA", "NN"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.10": {"text": "Wiegten sich um mich im sanftesten Tanz.", "tokens": ["Wieg\u00b7ten", "sich", "um", "mich", "im", "sanf\u00b7tes\u00b7ten", "Tanz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PRF", "APPRART", "ADJA", "NN", "$."], "meter": "+---+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.4": {"line.1": {"text": "Da schweb ich nun in den saphirnen H\u00f6hen", "tokens": ["Da", "schweb", "ich", "nun", "in", "den", "sa\u00b7phir\u00b7nen", "H\u00f6\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Bald \u00fcberm unabsehlich weiten Meer;", "tokens": ["Bald", "\u00fc\u00b7berm", "un\u00b7ab\u00b7seh\u00b7lich", "wei\u00b7ten", "Meer", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Bald seh ich unter mir ein langes Klippenheer,", "tokens": ["Bald", "seh", "ich", "un\u00b7ter", "mir", "ein", "lan\u00b7ges", "Klip\u00b7pen\u00b7heer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Itzt grausenvolle Felsenw\u00fcsten stehen,", "tokens": ["Itzt", "grau\u00b7sen\u00b7vol\u00b7le", "Fel\u00b7sen\u00b7w\u00fcs\u00b7ten", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und dort den Fr\u00fchling mir entgegenwehen;", "tokens": ["Und", "dort", "den", "Fr\u00fch\u00b7ling", "mir", "ent\u00b7ge\u00b7gen\u00b7we\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und hier die Lichtesk\u00f6nigin,", "tokens": ["Und", "hier", "die", "Lich\u00b7tes\u00b7k\u00f6\u00b7ni\u00b7gin", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Auf rosichtgoldnen Wolken hingetragen,", "tokens": ["Auf", "ro\u00b7sicht\u00b7gold\u00b7nen", "Wol\u00b7ken", "hin\u00b7ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Zu ihrer Himmelsruhe ziehn.", "tokens": ["Zu", "ih\u00b7rer", "Him\u00b7mels\u00b7ru\u00b7he", "ziehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "O welch Gesicht! Mein Lied! wie k\u00f6nntest du es sagen,", "tokens": ["O", "welch", "Ge\u00b7sicht", "!", "Mein", "Lied", "!", "wie", "k\u00f6nn\u00b7test", "du", "es", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PIAT", "NN", "$.", "PPOSAT", "NN", "$.", "PWAV", "VMFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was dieses Auge trank vom weltumwandelnden Wagen?", "tokens": ["Was", "die\u00b7ses", "Au\u00b7ge", "trank", "vom", "wel\u00b7tum\u00b7wan\u00b7deln\u00b7den", "Wa\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PDAT", "NN", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Der Sch\u00f6pfung ganze Pracht, die Herrlichkeit,", "tokens": ["Der", "Sch\u00f6p\u00b7fung", "gan\u00b7ze", "Pracht", ",", "die", "Herr\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die in dem Einsamen der dunkeln Ewigkeit", "tokens": ["Die", "in", "dem", "Ein\u00b7sa\u00b7men", "der", "dun\u00b7keln", "E\u00b7wig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der Allerh\u00f6chste ausgedacht", "tokens": ["Der", "Al\u00b7ler\u00b7h\u00f6chs\u00b7te", "aus\u00b7ge\u00b7dacht"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und sich zur Augenlust, und euch, o Menschen!", "tokens": ["Und", "sich", "zur", "Au\u00b7gen\u00b7lust", ",", "und", "euch", ",", "o", "Men\u00b7schen", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PRF", "APPRART", "NN", "$,", "KON", "PPER", "$,", "FM", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Zur Wohnung hat gemacht,", "tokens": ["Zur", "Woh\u00b7nung", "hat", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Lag vor mir da!... Und welche Melodien", "tokens": ["Lag", "vor", "mir", "da", "!", "...", "Und", "wel\u00b7che", "Me\u00b7lo\u00b7dien"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["NN", "APPR", "PPER", "ADV", "$.", "$(", "KON", "PWAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Dringen herauf? welch unaussprechlicher Klang", "tokens": ["Drin\u00b7gen", "her\u00b7auf", "?", "welch", "un\u00b7aus\u00b7sprech\u00b7li\u00b7cher", "Klang"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PAV", "PTKVZ", "$.", "PWAT", "ADJA", "NN"], "meter": "+--+-+-+--+", "measure": "iambic.penta.invert"}, "line.10": {"text": "Schl\u00e4gt mein entz\u00fccktes Ohr?... Der gro\u00dfe Lobgesang", "tokens": ["Schl\u00e4gt", "mein", "ent\u00b7z\u00fcck\u00b7tes", "Ohr", "?", "...", "Der", "gro\u00b7\u00dfe", "Lob\u00b7ge\u00b7sang"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "$.", "$(", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "T\u00f6nt auf der Laute der Natur!... In Harmonien", "tokens": ["T\u00f6nt", "auf", "der", "Lau\u00b7te", "der", "Na\u00b7tur", "!", "...", "In", "Har\u00b7mo\u00b7ni\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "ART", "NN", "$.", "$(", "APPR", "NE"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Wie einen s\u00fc\u00dfen Tod verloren, preist", "tokens": ["Wie", "ei\u00b7nen", "s\u00fc\u00b7\u00dfen", "Tod", "ver\u00b7lo\u00b7ren", ",", "preist"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVPP", "$,", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Den Herrn des Alls mein Geist!", "tokens": ["Den", "Herrn", "des", "Alls", "mein", "Geist", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Vor\u00fcber war der Sturm, der Donner Rollen", "tokens": ["Vor\u00b7\u00fc\u00b7ber", "war", "der", "Sturm", ",", "der", "Don\u00b7ner", "Rol\u00b7len"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "$,", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das hallende Gebirg hinein verschollen,", "tokens": ["Das", "hal\u00b7len\u00b7de", "Ge\u00b7birg", "hin\u00b7ein", "ver\u00b7schol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APZR", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Geflohn die Dunkelheit;", "tokens": ["Ge\u00b7flohn", "die", "Dun\u00b7kel\u00b7heit", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "In junger Sch\u00f6ne l\u00e4chelten die Himmel wieder", "tokens": ["In", "jun\u00b7ger", "Sch\u00f6\u00b7ne", "l\u00e4\u00b7chel\u00b7ten", "die", "Him\u00b7mel", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "NN", "ADV"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Auf ihre Schwester, Gottes Erde, nieder", "tokens": ["Auf", "ih\u00b7re", "Schwes\u00b7ter", ",", "Got\u00b7tes", "Er\u00b7de", ",", "nie\u00b7der"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "NN", "NN", "$,", "PTKVZ"], "meter": "-+-+-