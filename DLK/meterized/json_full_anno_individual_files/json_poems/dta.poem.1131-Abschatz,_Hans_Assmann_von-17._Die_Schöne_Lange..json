{"dta.poem.1131": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "17. Die Sch\u00f6ne Lange.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Es setze/ wer da will/ der L\u00e4nge M\u00e4ngel aus/ ", "tokens": ["Es", "set\u00b7ze", "/", "wer", "da", "will", "/", "der", "L\u00e4n\u00b7ge", "M\u00e4n\u00b7gel", "aus", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "PWS", "ADV", "VMFIN", "$(", "ART", "NN", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und suche seine Lust an kleinen Niedrigkeiten;", "tokens": ["Und", "su\u00b7che", "sei\u00b7ne", "Lust", "an", "klei\u00b7nen", "Nied\u00b7rig\u00b7kei\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Es wird ihn doch samt mir die Warheit selbst bestreiten.", "tokens": ["Es", "wird", "ihn", "doch", "samt", "mir", "die", "War\u00b7heit", "selbst", "be\u00b7strei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "APPR", "PPER", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Wer nimmt vor Elephant und Pferd die kleine Mau\u00df?", "tokens": ["Wer", "nimmt", "vor", "E\u00b7le\u00b7phant", "und", "Pferd", "die", "klei\u00b7ne", "Mau\u00df", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "APPR", "NN", "KON", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ein hochgesinnter Geist braucht ein geraumes Hau\u00df.", "tokens": ["Ein", "hoch\u00b7ge\u00b7sinn\u00b7ter", "Geist", "braucht", "ein", "ge\u00b7rau\u00b7mes", "Hau\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Was ungemein/ ist sch\u00f6n: Bey den verderbten Zeiten", "tokens": ["Was", "un\u00b7ge\u00b7mein", "/", "ist", "sch\u00f6n", ":", "Bey", "den", "ver\u00b7derb\u00b7ten", "Zei\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "$(", "VAFIN", "ADJD", "$.", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Pflegt die Natur mehr Klein- als Grosses zu bereiten.", "tokens": ["Pflegt", "die", "Na\u00b7tur", "mehr", "Klein", "als", "Gros\u00b7ses", "zu", "be\u00b7rei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "TRUNC", "KON", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Was schm\u00fcckt der F\u00fcrsten Helm? Schnee-K\u00f6nig/ oder", "tokens": ["Was", "schm\u00fcckt", "der", "F\u00fcrs\u00b7ten", "Helm", "?", "Schnee\u00b7K\u00f6\u00b7nig", "/", "o\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["PWS", "VVFIN", "ART", "NN", "NN", "$.", "NN", "$(", "KON"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Strau\u00df? ", "tokens": ["Strau\u00df", "?"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+", "measure": "single.up"}, "line.10": {"text": "Mu\u00df sich der Buhler hier nach einem Kusse d\u00e4hnen;", "tokens": ["Mu\u00df", "sich", "der", "Buh\u00b7ler", "hier", "nach", "ei\u00b7nem", "Kus\u00b7se", "d\u00e4h\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "ART", "NN", "ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Am s\u00fc\u00dften schmecket er auff M\u00fche/ Sorg\u2019 und Sehnen.", "tokens": ["Am", "s\u00fc\u00df\u00b7ten", "schme\u00b7cket", "er", "auff", "M\u00fc\u00b7he", "/", "Sor\u00b7g'", "und", "Seh\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "VVFIN", "PPER", "APPR", "NN", "$(", "NN", "KON", "NN", "$."], "meter": "-+-+---+--+-+-", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "Die Welt verwundert ja der h\u00f6chsten Th\u00fcrme Pracht/", "tokens": ["Die", "Welt", "ver\u00b7wun\u00b7dert", "ja", "der", "h\u00f6chs\u00b7ten", "Th\u00fcr\u00b7me", "Pracht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ART", "ADJA", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Apellens Kunst weist sich am besten an Colossen.", "tokens": ["A\u00b7pel\u00b7lens", "Kunst", "weist", "sich", "am", "bes\u00b7ten", "an", "Co\u00b7los\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VVFIN", "PRF", "PTKA", "ADJD", "APPR", "NN", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.14": {"text": "Der schwachen Zwerge Schaar ist \u00fcberall verlacht;", "tokens": ["Der", "schwa\u00b7chen", "Zwer\u00b7ge", "Schaar", "ist", "\u00fc\u00b7be\u00b7rall", "ver\u00b7lacht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Die Riesen haben selbst das Sternen-Reich beschossen.", "tokens": ["Die", "Rie\u00b7sen", "ha\u00b7ben", "selbst", "das", "Ster\u00b7nen\u00b7Reich", "be\u00b7schos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}