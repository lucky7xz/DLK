{"textgrid.poem.36212": {"metadata": {"author": {"name": "Schlegel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Das Gedicht der Liebe", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie n\u00e4chtlich ungest\u00fcm die Wellen wogen,", "tokens": ["Wie", "n\u00e4cht\u00b7lich", "un\u00b7ge\u00b7st\u00fcm", "die", "Wel\u00b7len", "wo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Bald schwellend liebevoll zum Sternenkranze,", "tokens": ["Bald", "schwel\u00b7lend", "lie\u00b7be\u00b7voll", "zum", "Ster\u00b7nen\u00b7kran\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Bald sinkend zu der Tiefe hingezogen,", "tokens": ["Bald", "sin\u00b7kend", "zu", "der", "Tie\u00b7fe", "hin\u00b7ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sehns\u00fcchtig flutend in dem Wechseltanze,", "tokens": ["Sehn\u00b7s\u00fcch\u00b7tig", "flu\u00b7tend", "in", "dem", "Wech\u00b7sel\u00b7tan\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Bis Morgenrot empor scheint aus den Wogen,", "tokens": ["Bis", "Mor\u00b7gen\u00b7rot", "em\u00b7por", "scheint", "aus", "den", "Wo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKVZ