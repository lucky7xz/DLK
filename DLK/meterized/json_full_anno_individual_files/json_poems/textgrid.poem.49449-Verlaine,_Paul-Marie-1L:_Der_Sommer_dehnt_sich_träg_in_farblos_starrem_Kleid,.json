{"textgrid.poem.49449": {"metadata": {"author": {"name": "Verlaine, Paul-Marie", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der Sommer dehnt sich tr\u00e4g in farblos starrem Kleid,", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Sommer dehnt sich tr\u00e4g in farblos starrem Kleid,", "tokens": ["Der", "Som\u00b7mer", "dehnt", "sich", "tr\u00e4g", "in", "farb\u00b7los", "star\u00b7rem", "Kleid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "ADJD", "APPR", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als s\u00e4h ein F\u00fcrst dem Urteil zu, das er verh\u00e4ngte,", "tokens": ["Als", "s\u00e4h", "ein", "F\u00fcrst", "dem", "Ur\u00b7teil", "zu", ",", "das", "er", "ver\u00b7h\u00e4ng\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "ART", "NN", "ART", "NN", "PTKVZ", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Im Himmel, der das Land mit weisser Glut versengte,", "tokens": ["Im", "Him\u00b7mel", ",", "der", "das", "Land", "mit", "weis\u00b7ser", "Glut", "ver\u00b7seng\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PRELS", "ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und g\u00e4hnt. Tief ruht der Mensch in schwerer Mattigkeit.", "tokens": ["Und", "g\u00e4hnt", ".", "Tief", "ruht", "der", "Mensch", "in", "schwe\u00b7rer", "Mat\u00b7tig\u00b7keit", "."], "token_info": ["word", "word", "punct", "word", "