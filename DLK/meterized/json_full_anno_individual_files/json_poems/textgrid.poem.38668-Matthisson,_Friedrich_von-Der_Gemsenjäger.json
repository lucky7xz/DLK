{"textgrid.poem.38668": {"metadata": {"author": {"name": "Matthisson, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Der Gemsenj\u00e4ger", "genre": "verse", "period": "N.A.", "pub_year": 1791, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer mit herkulischer St\u00e4rke der fl\u00fcchtigen Gemse sich nachschwingt,", "tokens": ["Wer", "mit", "her\u00b7ku\u00b7li\u00b7scher", "St\u00e4r\u00b7ke", "der", "fl\u00fcch\u00b7ti\u00b7gen", "Gem\u00b7se", "sich", "nach\u00b7schwingt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ADJA", "NN", "ART", "ADJA", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+--+--+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Scheint mir in Bettlersgestalt noch ein Erkohrner des Gl\u00fccks.", "tokens": ["Scheint", "mir", "in", "Bett\u00b7lers\u00b7ge\u00b7stalt", "noch", "ein", "Er\u00b7kohr\u00b7ner", "des", "Gl\u00fccks", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "ADV", "ART", "NN", "ART", "NN", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.3": {"text": "St\u00fcrzt' ihn auch feindlich Kronion in Tiefen des Jammers: er bliebe", "tokens": ["St\u00fcrzt'", "ihn", "auch", "feind\u00b7lich", "Kro\u00b7ni\u00b7on", "in", "Tie\u00b7fen", "des", "Jam\u00b7mers", ":", "er", "blie\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ADJD", "NN", "APPR", "NN", "ART", "NN", "$.", "PPER", "VVFIN"], "meter": "+--+-+-+-+--+--+-", "measure": "iambic.septa.invert"}, "line.4": {"text": "Doch durch den eisernen Arm selbst sich ein m\u00e4chtiger Gott.", "tokens": ["Doch", "durch", "den", "ei\u00b7ser\u00b7nen", "Arm", "selbst", "sich", "ein", "m\u00e4ch\