{"dta.poem.950": {"metadata": {"author": {"name": "George, Stefan", "birth": "N.A.", "death": "N.A."}, "title": "S IEG   D ES   S OMMERS", "genre": "Lyrik", "period": "N.A.", "pub_year": "1897", "urn": "urn:nbn:de:kobv:b4-200905191530", "language": ["de:0.99"], "booktitle": "George, Stefan: Das Jahr der Seele. Berlin, 1897."}, "poem": {"stanza.1": {"line.1": {"text": "Der l&#252;fte schaukeln wie von neuen dingen               ", "tokens": ["Der", "l", "&#252;", "fte", "schau\u00b7keln", "wie", "von", "neu\u00b7en", "din\u00b7gen"], "token_info": ["word", "word", "XML_entity", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$(", "VVFIN", "VVINF", "KOKOM", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Aus grauem himmel brechend milde feuer               ", "tokens": ["Aus", "grau\u00b7em", "him\u00b7mel", "bre\u00b7chend", "mil\u00b7de", "feu\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und rauschen heimatw&#228;rts gewandter schwingen               ", "tokens": ["Und", "rau\u00b7schen", "hei\u00b7matw", "&#228;", "rts", "ge\u00b7wand\u00b7ter", "schwin\u00b7gen"], "token_info": ["word", "word", "word", "XML_entity", "word", "word", "word"], "pos": ["KON", "VVFIN", "NE", "$(", "ADV", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Entbietet mir ein neues abenteuer             ", "tokens": ["Ent\u00b7bie\u00b7tet", "mir", "ein", "neu\u00b7es", "a\u00b7bent\u00b7eu\u00b7er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Du all die jahre hin mir glanz und glaube               ", "tokens": ["Du", "all", "die", "jah\u00b7re", "hin", "mir", "glanz", "und", "glau\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "PIAT", "ART", "NN", "ADV", "PPER", "ADJD", "KON", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Bei dir und wo die stummen zeugen waren               ", "tokens": ["Bei", "dir", "und", "wo", "die", "stum\u00b7men", "zeu\u00b7gen", "wa\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "KON", "PWAV", "ART", "ADJA", "VVFIN", "VAFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Von hoffen und von angst bei diesem laube               ", "tokens": ["Von", "hof\u00b7fen", "und", "von", "angst", "bei", "die\u00b7sem", "lau\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "VVFIN", "KON", "APPR", "NN", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Denn wird das gl&#252;ck sich je uns offenbaren             ", "tokens": ["Denn", "wird", "das", "gl", "&#252;", "ck", "sich", "je", "uns", "of\u00b7fen\u00b7ba\u00b7ren"], "token_info": ["word", "word", "word", "word", "XML_entity", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "ART", "FM", "FM", "FM", "PRF", "ADV", "PPER", "VVINF"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Wenn jezt die nacht die lockende besternte               ", "tokens": ["Wenn", "jezt", "die", "nacht", "die", "lo\u00b7cken\u00b7de", "bes\u00b7tern\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "In gr&#252;ner garten-au es nicht ersp&#228;ht               ", "tokens": ["In", "gr", "&#252;", "ner", "gar\u00b7ten\u00b7au", "es", "nicht", "ersp", "&#228;", "ht"], "token_info": ["word", "word", "XML_entity", "word", "word", "word", "word", "word", "XML_entity", "word"], "pos": ["APPR", "NE", "$(", "ADJA", "NN", "PPER", "PTKNEG", "ADJD", "$(", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wenn es die bunte volle blumen-ernte               ", "tokens": ["Wenn", "es", "die", "bun\u00b7te", "vol\u00b7le", "blu\u00b7men\u00b7ern\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wenn es der glutwind nicht verr&#228;t?             ", "tokens": ["Wenn", "es", "der", "glut\u00b7wind", "nicht", "verr", "&#228;", "t", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "XML_entity", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PTKNEG", "ADJD", "$(", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}