{"dta.poem.748": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "XxXIV.  An einen Vnschuldig Leidenden.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.71", "da:0.28"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Ein brandt pfall vnd ein radt/ pech/ folter/ bley vnd zangen/ ", "tokens": ["Ein", "brandt", "pfall", "vnd", "ein", "radt", "/", "pech", "/", "fol\u00b7ter", "/", "bley", "vnd", "zan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "ART", "NN", "$(", "NE", "$(", "ADJA", "$(", "NN", "KON", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Strick/ messer/ hacken/ beyll/ ein holtzstos vnd ein", "tokens": ["Strick", "/", "mes\u00b7ser", "/", "ha\u00b7cken", "/", "beyll", "/", "ein", "holtz\u00b7stos", "vnd", "ein"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$(", "NE", "$(", "VVINF", "$(", "XY", "$(", "ART", "NN", "KON", "ART"], "meter": "+--+-+-+--+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Schwerdt/ ", "tokens": ["Schwerdt", "/"], "token_info": ["word", "punct"], "pos": ["NE", "$("], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Vnd siedent \u00f6el/ vnd bley/ ein spies/ ein gl\u00fcend Pferdt/", "tokens": ["Vnd", "sie\u00b7dent", "\u00f6\u00b7el", "/", "vnd", "bley", "/", "ein", "spies", "/", "ein", "gl\u00fcend", "Pferdt", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "FM", "FM", "$(", "KON", "NN", "$(", "ART", "VVFIN", "$(", "ART", "ADJD", "NN", "$("], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Sind den nicht schrecklich/ die/ was schrecklich nicht began- ", "tokens": ["Sind", "den", "nicht", "schreck\u00b7lich", "/", "die", "/", "was", "schreck\u00b7lich", "nicht", "be\u00b7gan"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "ART", "PTKNEG", "ADJD", "$(", "ART", "$(", "PWS", "ADJD", "PTKNEG", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "gen.", "tokens": ["gen", "."], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "-", "measure": "single.down"}}, "stanza.2": {"line.1": {"text": "Wer vmb die tugend leid\u2019t/ vmb recht-thun wird gefangen", "tokens": ["Wer", "vmb", "die", "tu\u00b7gend", "leid't", "/", "vmb", "recht\u00b7thun", "wird", "ge\u00b7fan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "APPR", "ART", "NN", "ADJD", "$(", "APPR", "NE", "VAFIN", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vnd wenn es noth sein ", "tokens": ["Vnd", "wenn", "es", "noth", "sein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "NN", "PPOSAT"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Dem wird f\u00fcr kurtze pein vnendtlich preis beschert/", "tokens": ["Dem", "wird", "f\u00fcr", "kurt\u00b7ze", "pein", "vnendt\u00b7lich", "preis", "be\u00b7schert", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "ADJA", "NN", "ADJD", "NN", "VVPP", "$("], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Er wirdt den ehren Krantz der nicht verwelckt erlangen. ", "tokens": ["Er", "wirdt", "den", "eh\u00b7ren", "Krantz", "der", "nicht", "ver\u00b7welckt", "er\u00b7lan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "ART", "PTKNEG", "VVPP", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Er lebt in dem er stirbt/ er steigt in dem er f\u00e4lt", "tokens": ["Er", "lebt", "in", "dem", "er", "stirbt", "/", "er", "steigt", "in", "dem", "er", "f\u00e4lt"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PRELS", "PPER", "VVFIN", "$(", "PPER", "VVFIN", "APPR", "PRELS", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Er pocht was t\u00f6dlich ist vnd trotzt die grosse Welt", "tokens": ["Er", "pocht", "was", "t\u00f6d\u00b7lich", "ist", "vnd", "trotzt", "die", "gros\u00b7se", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PWS", "ADJD", "VAFIN", "KON", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Vnd k\u00fcst die Ewigkeit die er jhm anvertrawet.", "tokens": ["Vnd", "k\u00fcst", "die", "E\u00b7wig\u00b7keit", "die", "er", "jhm", "an\u00b7ver\u00b7tra\u00b7wet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ART", "PPER", "PPER", "VVFIN", "$."], "meter": "-+-+-+---+--+", "measure": "iambic.penta.chol"}}, "stanza.4": {"line.1": {"text": "Hat nicht der H\u00f6chste selbst sein h\u00f6chstes wunderwerck ", "tokens": ["Hat", "nicht", "der", "H\u00f6chs\u00b7te", "selbst", "sein", "h\u00f6chs\u00b7tes", "wun\u00b7der\u00b7werck"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PTKNEG", "ART", "NN", "ADV", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In dem er starb verbracht auff Salems sch\u00e4delberg?", "tokens": ["In", "dem", "er", "starb", "ver\u00b7bracht", "auff", "Sa\u00b7lems", "sch\u00e4\u00b7del\u00b7berg", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "VVFIN", "VVPP", "APPR", "NE", "NE", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der ist kein rechter Christ/ dem f\u00fcr dem Creutze grawet.", "tokens": ["Der", "ist", "kein", "rech\u00b7ter", "Christ", "/", "dem", "f\u00fcr", "dem", "Creut\u00b7ze", "gra\u00b7wet", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PIAT", "ADJA", "NN", "$(", "ART", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}}}}}