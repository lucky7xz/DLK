, "bist", "du", "fut\u00b7sch", ",", "und", "dei\u00b7nem", "Pub\u00b7li\u00b7kum", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "$,", "KON", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.17": {"text": "Das sein Entree nur ungern gratis zahlt,", "tokens": ["Das", "sein", "E\u00b7ntree", "nur", "un\u00b7gern", "gra\u00b7tis", "zahlt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "NN", "ADV", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Bleibt nichts als eine Nase voll Gestank.", "tokens": ["Bleibt", "nichts", "als", "ei\u00b7ne", "Na\u00b7se", "voll", "Ge\u00b7stank", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "KOKOM", "ART", "NN", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Zuletzt verpufft auch der, die Bande br\u00fcllt,", "tokens": ["Zu\u00b7letzt", "ver\u00b7pufft", "auch", "der", ",", "die", "Ban\u00b7de", "br\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Schimpft, pfeift, krakehlt und pr\u00fcgelt sich", "tokens": ["Schimpft", ",", "pfeift", ",", "kra\u00b7kehlt", "und", "pr\u00fc\u00b7gelt", "sich"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "VVFIN", "$,", "VVFIN", "KON", "VVFIN", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Und johlt dann schliesslich kn\u00fcppeldick besoffen", "tokens": ["Und", "johlt", "dann", "schliess\u00b7lich", "kn\u00fcp\u00b7pel\u00b7dick", "be\u00b7sof\u00b7fen"], "to