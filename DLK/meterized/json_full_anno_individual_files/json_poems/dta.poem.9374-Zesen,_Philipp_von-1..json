, "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "wie der Tausendsch\u00f6nen zier/", "tokens": ["wie", "der", "Tau\u00b7send\u00b7sch\u00f6\u00b7nen", "zier", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PTKVZ", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "von dem tau die Lippen nassen/", "tokens": ["von", "dem", "tau", "die", "Lip\u00b7pen", "nas\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "der entspringt auf Hermons gassen.", "tokens": ["der", "ent\u00b7springt", "auf", "Her\u00b7mons", "gas\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VVFIN", "APPR", "NE", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Du ", "tokens": ["Du"], "token_info": ["word"], "pos": ["PPER"], "meter": "-", "measure": "single.down"}, "line.2": {"text": "und entf\u00fchret/", "tokens": ["und", "ent\u00b7f\u00fch\u00b7ret", "/"], "token_info": ["word", "word", "punct"], "pos": ["KON", "VVFIN", "$("], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "deine zucker-s\u00fc\u00dfe wort", "tokens": ["dei\u00b7ne", "zu\u00b7cke\u00b7rs\u00fc\u00b7\u00dfe", "wort"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "seyn mein port/", "tokens": ["seyn", "mein", "port", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["VAINF", "PPOSAT", "NE", "$("], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "ja das hertze will mir brechen/", "tokens": ["ja", "das", "hert\u00b7ze", "will", "mir", "bre\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VMFIN", "PPER", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "kann nichts sprechen/", "tokens": ["kann", "nichts", "spre\u00b7chen", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "VVINF", "$("], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "wenn dein Mund sich reget nur;", "tokens": ["wenn", "dein", "Mund", "sich", "re\u00b7get", "nur", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PRF", "VVFIN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Ich mu\u00df la\u00df", "tokens": ["Ich", "mu\u00df", "la\u00df"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VMFIN", "XY"], "meter": "-+-", "measure": "amphibrach.single"}, "line.9": {"text": "und mich zu denselben enden/", "tokens": ["und", "mich", "zu", "den\u00b7sel\u00b7ben", "en\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PTKZU", "VVINF", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "da du redest/ willig wenden.", "tokens": ["da", "du", "re\u00b7dest", "/", "wil\u00b7lig", "wen\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$(", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Du solt meine Liebste seyn/ meine freude/ meine Sonne/", "tokens": ["Du", "solt", "mei\u00b7ne", "Liebs\u00b7te", "seyn", "/", "mei\u00b7ne", "freu\u00b7de", "/", "mei\u00b7ne", "Son\u00b7ne", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "VAINF", "$(", "PPOSAT", "NN", "$(", "PPOSAT", "NN", "$("], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Lust und wonne;", "tokens": ["Lust", "und", "won\u00b7ne", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "PWAV", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "wann mich ja die tunckle nacht", "tokens": ["wann", "mich", "ja", "die", "tunck\u00b7le", "nacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "irrig macht/", "tokens": ["ir\u00b7rig", "macht", "/"], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$("], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "soll der Augen helles blicken", "tokens": ["soll", "der", "Au\u00b7gen", "hel\u00b7les", "bli\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "ART", "NN", "ADJA", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "mich erquicken;", "tokens": ["mich", "er\u00b7qui\u00b7cken", ";"], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Meine Sch\u00f6ne/ meine Braut/", "tokens": ["Mei\u00b7ne", "Sch\u00f6\u00b7ne", "/", "mei\u00b7ne", "Braut", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "PPOSAT", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "die der Himmel mier vertraut/", "tokens": ["die", "der", "Him\u00b7mel", "mier", "ver\u00b7traut", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "PPER", "ADJD", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Du machst/ da\u00df mich Lieben werden", "tokens": ["Du", "machst", "/", "da\u00df", "mich", "Lie\u00b7ben", "wer\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$(", "KOUS", "PPER", "ADJA", "VAINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.10": {"text": "die Gewaltigen auf Erden.", "tokens": ["die", "Ge\u00b7wal\u00b7ti\u00b7gen", "auf", "Er\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Du solt meines Rahmens Lob in die hohen wolcken bauen/", "tokens": ["Du", "solt", "mei\u00b7nes", "Rah\u00b7mens", "Lob", "in", "die", "ho\u00b7hen", "wol\u00b7cken", "bau\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "NN", "APPR", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "stets zuschauen/", "tokens": ["stets", "zu\u00b7schau\u00b7en", "/"], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVINF", "$("], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "mein ged\u00e4chtn\u00fcs wird bestehn/", "tokens": ["mein", "ge\u00b7d\u00e4cht\u00b7n\u00fcs", "wird", "be\u00b7stehn", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVINF", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "wo die Sterne gehn/", "tokens": ["wo", "die", "Ster\u00b7ne", "gehn", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVINF", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "und unsterblich auch verbleiben", "tokens": ["und", "uns\u00b7terb\u00b7lich", "auch", "ver\u00b7blei\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJD", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "und bekleiben/", "tokens": ["und", "be\u00b