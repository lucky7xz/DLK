{"dta.poem.9374": {"metadata": {"author": {"name": "Zesen, Philipp von", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-20684-8", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wei\u00dfheit sage/ wo du bist/ wo dein reicher quell aufsteiget/", "tokens": ["Wei\u00df\u00b7heit", "sa\u00b7ge", "/", "wo", "du", "bist", "/", "wo", "dein", "rei\u00b7cher", "quell", "auf\u00b7stei\u00b7get", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$(", "PWAV", "PPER", "VAFIN", "$(", "PWAV", "PPOSAT", "ADJD", "ADJD", "VVFIN", "$("], "meter": "+--+---+-+-+-+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "und sich zeiget/", "tokens": ["und", "sich", "zei\u00b7get", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PRF", "VVFIN", "$("], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "tr\u00e4ncke mich mit deiner fluth/", "tokens": ["tr\u00e4n\u00b7cke", "mich", "mit", "dei\u00b7ner", "fluth", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "h\u00f6chstes Guth/", "tokens": ["h\u00f6chs\u00b7tes", "Guth", "/"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$("], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "la\u00df mich deinen Most durch s\u00fc\u00dfen/", "tokens": ["la\u00df", "mich", "dei\u00b7nen", "Most", "durch", "s\u00fc\u00b7\u00dfen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPOSAT", "NN", "APPR", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "und genie\u00dfen", "tokens": ["und", "ge\u00b7nie\u00b7\u00dfen"], "token_info": ["word", "word"], "pos": ["KON", "VVINF"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "deinen zuckers\u00fc\u00dfen Wein/", "tokens": ["dei\u00b7nen", "zu\u00b7cker\u00b7s\u00fc\u00b7\u00dfen", "Wein", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "la\u00df mich jmmer bey dier seyn/", "tokens": ["la\u00df", "mich", "jm\u00b7mer", "bey", "dier", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "APPR", "PPER", "VAINF", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "da\u00df mein Mund mit Wei\u00dfheit bl\u00fche", "tokens": ["da\u00df", "mein", "Mund", "mit", "Wei\u00df\u00b7heit", "bl\u00fc\u00b7he"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "APPR", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "und in Tugend sich bem\u00fche.", "tokens": ["und", "in", "Tu\u00b7gend", "sich", "be\u00b7m\u00fc\u00b7he", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "PRF", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "O du f\u00fcrstin aller kunst/ die mich kann mit Liebes-bl\u00fccken", "tokens": ["O", "du", "f\u00fcrs\u00b7tin", "al\u00b7ler", "kunst", "/", "die", "mich", "kann", "mit", "Lie\u00b7bes\u00b7bl\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "NE", "PIAT", "NN", "$(", "PRELS", "PPER", "VMFIN", "APPR", "NN"], "meter": "+-+-+----+-+-+-", "measure": "unknown.measure.hexa"}, "line.2": {"text": "so entz\u00fccken/", "tokens": ["so", "ent\u00b7z\u00fc\u00b7cken", "/"], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVINF", "$("], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "ach wie herrlich ist dein glantz", "tokens": ["ach", "wie", "herr\u00b7lich", "ist", "dein", "glantz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "ADJD", "VAFIN", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "und der krantz", "tokens": ["und", "der", "krantz"], "token_info": ["word", "word", "word"], "pos": ["KON", "ART", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "der den g\u00fcldnen helm \u00fcmbringet", "tokens": ["der", "den", "g\u00fcld\u00b7nen", "helm", "\u00fcm\u00b7brin\u00b7get"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ART", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "und mich zwinget/", "tokens": ["und", "mich", "zwin\u00b7get", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$("], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "der auf deinem H\u00e4upte steht/", "tokens": ["der", "auf", "dei\u00b7nem", "H\u00e4up\u00b7te", "steht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "da der Sonnen blitz ausgeht/", "tokens": ["da", "der", "Son\u00b7nen", "blitz", "aus\u00b7geht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "NE", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "da die g\u00fclden Engels-flammen", "tokens": ["da", "die", "g\u00fcl\u00b7den", "En\u00b7gels\u00b7flam\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "sich entz\u00fcnden allzusammen.", "tokens": ["sich", "ent\u00b7z\u00fcn\