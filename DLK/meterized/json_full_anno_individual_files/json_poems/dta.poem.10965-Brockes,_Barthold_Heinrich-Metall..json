{"dta.poem.10965": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Metall.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1740", "urn": "urn:nbn:de:kobv:b4-200905198572", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Pflanzen und der Thiere Reich scheint unserm\nGeist, in diesem Leben,", "tokens": ["Der", "Pflan\u00b7zen", "und", "der", "Thie\u00b7re", "Reich", "scheint", "un\u00b7serm", "Geist", ",", "in", "die\u00b7sem", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "NN", "VVFIN", "PPOSAT", "NN", "$,", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.2": {"text": "Sie, Gott zum Ruhm, mit Lust zu brauchen, zum w\u00fcrdgen Ge-", "tokens": ["Sie", ",", "Gott", "zum", "Ruhm", ",", "mit", "Lust", "zu", "brau\u00b7chen", ",", "zum", "w\u00fcrd\u00b7gen", "Ge"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "$,", "NN", "APPRART", "NN", "$,", "APPR", "NN", "PTKZU", "VVINF", "$,", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+--+--", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Wir aber w\u00e4hlen uns, zum Vorwurf, von andern Creaturen allen,", "tokens": ["Wir", "a\u00b7ber", "w\u00e4h\u00b7len", "uns", ",", "zum", "Vor\u00b7wurf", ",", "von", "an\u00b7dern", "Crea\u00b7tu\u00b7ren", "al\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PPER", "$,", "APPRART", "NN", "$,", "APPR", "ADJA", "NN", "PIAT", "$,"], "meter": "-+-+-+-+--+--+-+-", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "Zur Lust, zur Absicht, fast zur Gottheit, das Reich der schmu-", "tokens": ["Zur", "Lust", ",", "zur", "Ab\u00b7sicht", ",", "fast", "zur", "Got\u00b7theit", ",", "das", "Reich", "der", "schmu"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "APPRART", "NN", "$,", "ADV", "APPRART", "NN", "$,", "ART", "NN", "ART", "TRUNC"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Anstatt uns an den sch\u00f6nen Farben, und sch\u00f6nen Formen, in", "tokens": ["An\u00b7statt", "uns", "an", "den", "sch\u00f6\u00b7nen", "Far\u00b7ben", ",", "und", "sch\u00f6\u00b7nen", "For\u00b7men", ",", "in"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["KOUI", "PPER", "APPR", "ART", "ADJA", "NN", "$,", "KON", "ADJA", "NN", "$,", "APPR"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.6": {"text": "Die uns die beyden Reiche schenken, zusamt dem Licht, uns zu", "tokens": ["Die", "uns", "die", "bey\u00b7den", "Rei\u00b7che", "schen\u00b7ken", ",", "zu\u00b7samt", "dem", "Licht", ",", "uns", "zu"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "PPER", "ART", "PIAT", "NN", "VVINF", "$,", "APPR", "ART", "NN", "$,", "PPER", "PTKZU"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.7": {"text": "Gr\u00e4bt man den ungeformten G\u00f6tzen, um gleichsam irdischer", "tokens": ["Gr\u00e4bt", "man", "den", "un\u00b7ge\u00b7form\u00b7ten", "G\u00f6t\u00b7zen", ",", "um", "gleich\u00b7sam", "ir\u00b7di\u00b7scher"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PIS", "ART", "ADJA", "NN", "$,", "KOUI", "ADJD", "ADJA"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.8": {"text": "Aus dem verborgnen Abgrunds-Grund, und aus dem finstern", "tokens": ["Aus", "dem", "ver\u00b7borg\u00b7nen", "Ab\u00b7grunds\u00b7Grund", ",", "und", "aus", "dem", "fins\u00b7tern"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "KON", "APPR", "ART", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Bem\u00fcht sich, weil dem groben Klumpen die alles zierende", "tokens": ["Be\u00b7m\u00fcht", "sich", ",", "weil", "dem", "gro\u00b7ben", "Klum\u00b7pen", "die", "al\u00b7les", "zie\u00b7ren\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "$,", "KOUS", "ART", "ADJA", "NN", "ART", "PIAT", "NN"], "meter": "-+-+-+-+--+-+--", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "So Farben, als Figur, versagt, mit M\u00fch ihm einige Figur,", "tokens": ["So", "Far\u00b7ben", ",", "als", "Fi\u00b7gur", ",", "ver\u00b7sagt", ",", "mit", "M\u00fch", "ihm", "ei\u00b7ni\u00b7ge", "Fi\u00b7gur", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "$,", "KOUS", "NN", "$,", "VVPP", "$,", "APPR", "NN", "PPER", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.11": {"text": "Jm M\u00fcnzen, durch die Kunst, zu geben. Nun ist es wahr:", "tokens": ["Jm", "M\u00fcn\u00b7zen", ",", "durch", "die", "Kunst", ",", "zu", "ge\u00b7ben", ".", "Nun", "ist", "es", "wahr", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPR", "ART", "NN", "$,", "PTKZU", "VVINF", "$.", "ADV", "VAFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "Durch Meynungen, die allgemein, fast alles, was man h\u00f6rt", "tokens": ["Durch", "Mey\u00b7nun\u00b7gen", ",", "die", "all\u00b7ge\u00b7mein", ",", "fast", "al\u00b7les", ",", "was", "man", "h\u00f6rt"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "PRELS", "ADJD", "$,", "ADV", "PIS", "$,", "PRELS", "PIS", "VVFIN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.13": {"text": "F\u00fcr das an sich verworfne Wesen von andern sich verschaffen", "tokens": ["F\u00fcr", "das", "an", "sich", "ver\u00b7worf\u00b7ne", "We\u00b7sen", "von", "an\u00b7dern", "sich", "ver\u00b7schaf\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "APPR", "PRF", "ADJA", "NN", "APPR", "PIS", "PRF", "VVINF"], "meter": "--+--+-+--+-+-+-", "measure": "anapaest.di.plus"}, "line.14": {"text": "Da\u00df auch darin ein Segen steckt; einfolglich, wenn man sich", "tokens": ["Da\u00df", "auch", "da\u00b7rin", "ein", "Se\u00b7gen", "steckt", ";", "ein\u00b7folg\u00b7lich", ",", "wenn", "man", "sich"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "ADV", "PAV", "ART", "NN", "VVFIN", "$.", "ADJD", "$,", "KOUS", "PIS", "PRF"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.15": {"text": "Es zu erhalten, zu ersparen, man gar daran nicht unrecht thut:", "tokens": ["Es", "zu", "er\u00b7hal\u00b7ten", ",", "zu", "er\u00b7spa\u00b7ren", ",", "man", "gar", "da\u00b7ran", "nicht", "un\u00b7recht", "thut", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "$,", "PTKZU", "VVINF", "$,", "PIS", "ADV", "PAV", "PTKNEG", "NN", "VVFIN", "$."], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.16": {"text": "Doch mu\u00df ein Gut, das nur durch Zufall, und nicht an sich", "tokens": ["Doch", "mu\u00df", "ein", "Gut", ",", "das", "nur", "durch", "Zu\u00b7fall", ",", "und", "nicht", "an", "sich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "ART", "NN", "$,", "PRELS", "ADV", "APPR", "NN", "$,", "KON", "PTKNEG", "APPR", "PRF"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.17": {"text": "Mit Unterlassung aller Pflichten, mit der Verachtung aller", "tokens": ["Mit", "Un\u00b7ter\u00b7las\u00b7sung", "al\u00b7ler", "Pflich\u00b7ten", ",", "mit", "der", "Ver\u00b7ach\u00b7tung", "al\u00b7ler"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NN", "PIAT", "NN", "$,", "APPR", "ART", "NN", "PIAT"], "meter": "-+-+-+-+-+--+-+-", "measure": "iambic.septa.relaxed"}, "line.18": {"text": "Die Gott zu unserm Nutz und Freuden, und seines Namens", "tokens": ["Die", "Gott", "zu", "un\u00b7serm", "Nutz", "und", "Freu\u00b7den", ",", "und", "sei\u00b7nes", "Na\u00b7mens"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "KON", "NN", "$,", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.19": {"text": "Mit solcher Sucht, die uns f\u00fcr alles verblendet, und zwar", "tokens": ["Mit", "sol\u00b7cher", "Sucht", ",", "die", "uns", "f\u00fcr", "al\u00b7les", "ver\u00b7blen\u00b7det", ",", "und", "zwar"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PIAT", "NN", "$,", "PRELS", "PPER", "APPR", "PIS", "VVPP", "$,", "KON", "ADV"], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.20": {"text": "Uns selbst zum wahren Unvergn\u00fcgen, so heftig nicht gesuchet", "tokens": ["Uns", "selbst", "zum", "wah\u00b7ren", "Un\u00b7ver\u00b7gn\u00fc\u00b7gen", ",", "so", "hef\u00b7tig", "nicht", "ge\u00b7su\u00b7chet"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "APPRART", "ADJA", "NN", "$,", "ADV", "ADJD", "PTKNEG", "VVPP"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}}}}}