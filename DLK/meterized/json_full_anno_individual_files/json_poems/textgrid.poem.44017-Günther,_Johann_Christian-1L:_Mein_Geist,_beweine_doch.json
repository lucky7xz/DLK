{"textgrid.poem.44017": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mein Geist, beweine doch", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein Geist, beweine doch", "tokens": ["Mein", "Geist", ",", "be\u00b7wei\u00b7ne", "doch"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Den allgemeinen Jammer!", "tokens": ["Den", "all\u00b7ge\u00b7mei\u00b7nen", "Jam\u00b7mer", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das Leben ist ein Joch,", "tokens": ["Das", "Le\u00b7ben", "ist", "ein", "Joch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Das uns mehr dr\u00fcckt als zieret,", "tokens": ["Das", "uns", "mehr", "dr\u00fcckt", "als", "zie\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ADV", "VVFIN", "KOKOM", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ach Ungemach!", "tokens": ["Ach", "Un\u00b7ge\u00b7mach", "!"], "token_info": ["word", "word", "punct"], "pos": ["ITJ", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Und auf die Folter schnieret,", "tokens": ["Und", "auf", "die", "Fol\u00b7ter", "schnie\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ach, ach!", "tokens": ["Ach", ",", "ach", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["ITJ", "$,", "ITJ", "$."], "meter": "-+", "measure": "iambic.single"}}, "stanza.2": {"line.1": {"text": "Beym Eintritt in die Welt", "tokens": ["Beym", "Ein\u00b7tritt", "in", "die", "Welt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Wird uns der Schmerz zur Amme,", "tokens": ["Wird", "uns", "der", "Schmerz", "zur", "Am\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Gift zur Milch bestellt;", "tokens": ["Die", "Gift", "zur", "Milch", "be\u00b7stellt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Wir f\u00fchren in der Wiege,", "tokens": ["Wir", "f\u00fch\u00b7ren", "in", "der", "Wie\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ach Ungemach!", "tokens": ["Ach", "Un\u00b7ge\u00b7mach", "!"], "token_info": ["word", "word", "punct"], "pos": ["ITJ", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Die besten Elendskriege,", "tokens": ["Die", "bes\u00b7ten", "E\u00b7lends\u00b7krie\u00b7ge", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ach, ach!", "tokens": ["Ach", ",", "ach", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["ITJ", "$,", "ITJ", "$."], "meter": "-+", "measure": "iambic.single"}}, "stanza.3": {"line.1": {"text": "Der Fall lehrt uns den Gang,", "tokens": ["Der", "Fall", "lehrt", "uns", "den", "Gang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Der Gang lehrt uns das Fallen.", "tokens": ["Der", "Gang", "lehrt", "uns", "das", "Fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der weinende Gesang", "tokens": ["Der", "wei\u00b7nen\u00b7de", "Ge\u00b7sang"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Verdient oft Ruth und Schl\u00e4ge,", "tokens": ["Ver\u00b7di\u00b7ent", "oft", "Ruth", "und", "Schl\u00e4\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "NN", "KON", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.5": {"text": "Ach Ungemach!", "tokens": ["Ach", "Un\u00b7ge\u00b7mach", "!"], "token_info": ["word", "word", "punct"], "pos": ["ITJ", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Und bringt nur Furcht zuwege,", "tokens": ["Und", "bringt", "nur", "Furcht", "zu\u00b7we\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ach, ach!", "tokens": ["Ach", ",", "ach", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["ITJ", "$,", "ITJ", "$."], "meter": "-+", "measure": "iambic.single"}}, "stanza.4": {"line.1": {"text": "Der Jugend erster May", "tokens": ["Der", "Ju\u00b7gend", "ers\u00b7ter", "May"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "F\u00fchrt uns in Kummerschulen;", "tokens": ["F\u00fchrt", "uns", "in", "Kum\u00b7mer\u00b7schu\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der Geilheit Tyranney", "tokens": ["Der", "Geil\u00b7heit", "Ty\u00b7ran\u00b7ney"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Beraubet die Gem\u00fcther,", "tokens": ["Be\u00b7rau\u00b7bet", "die", "Ge\u00b7m\u00fc\u00b7ther", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ach Ungemach!", "tokens": ["Ach", "Un\u00b7ge\u00b7mach", "!"], "token_info": ["word", "word", "punct"], "pos": ["ITJ", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Der edlen Seeleng\u00fcter,", "tokens": ["Der", "ed\u00b7len", "See\u00b7len\u00b7g\u00fc\u00b7ter", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ach, ach!", "tokens": ["Ach", ",", "ach", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["ITJ", "$,", "ITJ", "$."], "meter": "-+", "measure": "iambic.single"}}, "stanza.5": {"line.1": {"text": "Und nimmt man denn ein Weib,", "tokens": ["Und", "nimmt", "man", "denn", "ein", "Weib", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "So wird uns Creuz und Kummer", "tokens": ["So", "wird", "uns", "Creuz", "und", "Kum\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ein rechter Zeitvertreib;", "tokens": ["Ein", "rech\u00b7ter", "Zeit\u00b7ver\u00b7treib", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Da m\u00fc\u00dfen wir verschwiegen,", "tokens": ["Da", "m\u00fc\u00b7\u00dfen", "wir", "ver\u00b7schwie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ach Ungemach!", "tokens": ["Ach", "Un\u00b7ge\u00b7mach", "!"], "token_info": ["word", "word", "punct"], "pos": ["ITJ", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Uns unter H\u00f6rner schmiegen,", "tokens": ["Uns", "un\u00b7ter", "H\u00f6r\u00b7ner", "schmie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ach, ach!", "tokens": ["Ach", ",", "ach", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["ITJ", "$,", "ITJ", "$."], "meter": "-+", "measure": "iambic.single"}}, "stanza.6": {"line.1": {"text": "Den Kummer ziehn wir gro\u00df:", "tokens": ["Den", "Kum\u00b7mer", "ziehn", "wir", "gro\u00df", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Da lezen uns die Kinder,", "tokens": ["Da", "le\u00b7zen", "uns", "die", "Kin\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die kleinen nur die Schoos,", "tokens": ["Die", "klei\u00b7nen", "nur", "die", "Schoos", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Die gro\u00dfen Herz und Augen,", "tokens": ["Die", "gro\u00b7\u00dfen", "Herz", "und", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ach Ungemach!", "tokens": ["Ach", "Un\u00b7ge\u00b7mach", "!"], "token_info": ["word", "word", "punct"], "pos": ["ITJ", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Wenn sie am Beuthel saugen,", "tokens": ["Wenn", "sie", "am", "Beut\u00b7hel", "sau\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ach, ach!", "tokens": ["Ach", ",", "ach", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["ITJ", "$,", "ITJ", "$."], "meter": "-+", "measure": "iambic.single"}}, "stanza.7": {"line.1": {"text": "F\u00e4llt nun die Jugend ab,", "tokens": ["F\u00e4llt", "nun", "die", "Ju\u00b7gend", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "So steiget mit den Jahren", "tokens": ["So", "stei\u00b7get", "mit", "den", "Jah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das Elend bis ins Grab;", "tokens": ["Das", "E\u00b7lend", "bis", "ins", "Grab", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Da mu\u00df man mit den Plagen,", "tokens": ["Da", "mu\u00df", "man", "mit", "den", "Pla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIS", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ach Ungemach!", "tokens": ["Ach", "Un\u00b7ge\u00b7mach", "!"], "token_info": ["word", "word", "punct"], "pos": ["ITJ", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Der Jugend Spott vertragen,", "tokens": ["Der", "Ju\u00b7gend", "Spott", "ver\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ach, ach!", "tokens": ["Ach", ",", "ach", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["ITJ", "$,", "ITJ", "$."], "meter": "-+", "measure": "iambic.single"}}, "stanza.8": {"line.1": {"text": "Drauf f\u00e4hrt man nackt und blos", "tokens": ["Drauf", "f\u00e4hrt", "man", "nackt", "und", "blos"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PIS", "ADJD", "KON", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Nach einem finstern Lande", "tokens": ["Nach", "ei\u00b7nem", "fins\u00b7tern", "Lan\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Auf die Verwesung los;", "tokens": ["Auf", "die", "Ver\u00b7we\u00b7sung", "los", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ja, mancher mu\u00df sein Sterben,", "tokens": ["Ja", ",", "man\u00b7cher", "mu\u00df", "sein", "Ster\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PIS", "VMFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ach Ungemach!", "tokens": ["Ach", "Un\u00b7ge\u00b7mach", "!"], "token_info": ["word", "word", "punct"], "pos": ["ITJ", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Noch wohl mit Angst erwerben,", "tokens": ["Noch", "wohl", "mit", "Angst", "er\u00b7wer\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ach, ach!", "tokens": ["Ach", ",", "ach", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["ITJ", "$,", "ITJ", "$."], "meter": "-+", "measure": "iambic.single"}}, "stanza.9": {"line.1": {"text": "Seht, Br\u00fcder, wie es geht!", "tokens": ["Seht", ",", "Br\u00fc\u00b7der", ",", "wie", "es", "geht", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$,", "PWAV", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Weint, da\u00df es mit uns Menschen", "tokens": ["Weint", ",", "da\u00df", "es", "mit", "uns", "Men\u00b7schen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "KOUS", "PPER", "APPR", "PPER", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So gar verdrie\u00dflich steht;", "tokens": ["So", "gar", "ver\u00b7drie\u00df\u00b7lich", "steht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ach bethet doch noch heute", "tokens": ["Ach", "be\u00b7thet", "doch", "noch", "heu\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ITJ", "VVFIN", "ADV", "ADV", "ADV"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.5": {"text": "Und w\u00fcntscht mit mir,", "tokens": ["Und", "w\u00fcnt\u00b7scht", "mit", "mir", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPER", "$,"], "meter": "--+-+", "measure": "anapaest.init"}, "line.6": {"text": "Da\u00df uns die Glocke l\u00e4uthe", "tokens": ["Da\u00df", "uns", "die", "Glo\u00b7cke", "l\u00e4u\u00b7the"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Von hier.", "tokens": ["Von", "hier", "."], "token_info": ["word", "word", "punct"], "pos": ["APPR", "ADV", "$."], "meter": "-+", "measure": "iambic.single"}}, "stanza.10": {"line.1": {"text": "Mein Geist, beweine doch", "tokens": ["Mein", "Geist", ",", "be\u00b7wei\u00b7ne", "doch"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Den allgemeinen Jammer!", "tokens": ["Den", "all\u00b7ge\u00b7mei\u00b7nen", "Jam\u00b7mer", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das Leben ist ein Joch,", "tokens": ["Das", "Le\u00b7ben", "ist", "ein", "Joch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Das uns mehr dr\u00fcckt als zieret,", "tokens": ["Das", "uns", "mehr", "dr\u00fcckt", "als", "zie\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ADV", "VVFIN", "KOKOM", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ach Ungemach!", "tokens": ["Ach", "Un\u00b7ge\u00b7mach", "!"], "token_info": ["word", "word", "punct"], "pos": ["ITJ", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Und auf die Folter schnieret,", "tokens": ["Und", "auf", "die", "Fol\u00b7ter", "schnie\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "A