{"dta.poem.16060": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "114.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.71", "af:0.28"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du fragst, was ist die Zeit? und was die Ewigkeit?", "tokens": ["Du", "fragst", ",", "was", "ist", "die", "Zeit", "?", "und", "was", "die", "E\u00b7wig\u00b7keit", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWS", "VAFIN", "ART", "NN", "$.", "KON", "PWS", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wo hebt sich Ew'ges an, und hebet auf die Zeit?", "tokens": ["Wo", "hebt", "sich", "Ew'\u00b7ges", "an", ",", "und", "he\u00b7bet", "auf", "die", "Zeit", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PRF", "ADJA", "PTKVZ", "$,", "KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die Zeit, sobald du sie aufhebst, ist aufgehoben,", "tokens": ["Die", "Zeit", ",", "so\u00b7bald", "du", "sie", "auf\u00b7hebst", ",", "ist", "auf\u00b7ge\u00b7ho\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "PPER", "PPER", "VVFIN", "$,", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wo dich das Ewige zu sich erhebt nach oben.", "tokens": ["Wo", "dich", "das", "E\u00b7wi\u00b7ge", "zu", "sich", "er\u00b7hebt", "nach", "o\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "ADJA", "APPR", "PRF", "VVFIN", "APPR", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Die Zeit ist nicht, es ist allein die Ewigkeit,", "tokens": ["Die", "Zeit", "ist", "nicht", ",", "es", "ist", "al\u00b7lein", "die", "E\u00b7wig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKNEG", "$,", "PPER", "VAFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Ewigkeit allein ist ewig in der Zeit.", "tokens": ["Die", "E\u00b7wig\u00b7keit", "al\u00b7lein", "ist", "e\u00b7wig", "in", "der", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VAFIN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Sie ist das in der Zeit sich stets geb\u00e4rende,", "tokens": ["Sie", "ist", "das", "in", "der", "Zeit", "sich", "stets", "ge\u00b7b\u00e4\u00b7ren\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "APPR", "ART", "NN", "PRF", "ADV", "ADJA", "$,"], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.2": {"text": "Als wahre Gegenwart die Zeit durchw\u00e4hrende.", "tokens": ["Als", "wah\u00b7re", "Ge\u00b7gen\u00b7wart", "die", "Zeit", "durch\u00b7w\u00e4h\u00b7ren\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}}, "stanza.5": {"line.1": {"text": "Wo die Vergangenheit und Zukunft ist geschwunden", "tokens": ["Wo", "die", "Ver\u00b7gan\u00b7gen\u00b7heit", "und", "Zu\u00b7kunft", "ist", "ge\u00b7schwun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "KON", "NN", "VAFIN", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In Gegenwart, da hast du Ewigkeit empfunden.", "tokens": ["In", "Ge\u00b7gen\u00b7wart", ",", "da", "hast", "du", "E\u00b7wig\u00b7keit", "emp\u00b7fun\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "ADV", "VAFIN", "PPER", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Wo du Vergangenheit und Zukunft hast empfunden", "tokens": ["Wo", "du", "Ver\u00b7gan\u00b7gen\u00b7heit", "und", "Zu\u00b7kunft", "hast", "emp\u00b7fun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "NN", "KON", "NN", "VAFIN", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als Gegenwart, da ist die Ewigkeit gefunden.", "tokens": ["Als", "Ge\u00b7gen\u00b7wart", ",", "da", "ist", "die", "E\u00b7wig\u00b7keit", "ge\u00b7fun\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "$,", "ADV", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}