{"textgrid.poem.43418": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wir feiern im Norden jetzt \u00fcberall", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wir feiern im Norden jetzt \u00fcberall", "tokens": ["Wir", "fei\u00b7ern", "im", "Nor\u00b7den", "jetzt", "\u00fc\u00b7be\u00b7rall"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ADV", "ADV"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Mit hoher Erlaubni\u00df den Carneval.", "tokens": ["Mit", "ho\u00b7her", "Er\u00b7laub\u00b7ni\u00df", "den", "Car\u00b7ne\u00b7val", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Wir treiben Spott, wir treiben Scherz", "tokens": ["Wir", "trei\u00b7ben", "Spott", ",", "wir", "trei\u00b7ben", "Scherz"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "ADJA", "NN", "$,", "PPER", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit allerlei in unserm Land und allerw\u00e4rts.", "tokens": ["Mit", "al\u00b7ler\u00b7lei", "in", "un\u00b7serm", "Land", "und", "al\u00b7ler\u00b7w\u00e4rts", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "APPR", "PPOSAT", "NN", "KON", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "So kommt denn manche Wahrheit wol an den Tag,", "tokens": ["So", "kommt", "denn", "man\u00b7che", "Wahr\u00b7heit", "wol", "an", "den", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PIAT", "NN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Die man nicht offenbaren darf und mag.", "tokens": ["Die", "man", "nicht", "of\u00b7fen\u00b7ba\u00b7ren", "darf", "und", "mag."], "token_info": ["word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["ART", "PIS", "PTKNEG", "VVINF", "VMFIN", "KON", "NE"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und mancher singt und mancher sagt,", "tokens": ["Und", "man\u00b7cher", "singt", "und", "man\u00b7cher", "sagt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "KON", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Was er doch sonst wol schwerlich h\u00e4tte gewagt.", "tokens": ["Was", "er", "doch", "sonst", "wol", "schwer\u00b7lich", "h\u00e4t\u00b7te", "ge\u00b7wagt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "ADV", "ADV", "ADJD", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.3": {"line.1": {"text": "Das w\u00e4re nun Alles zu unserm Gedeihn,", "tokens": ["Das", "w\u00e4\u00b7re", "nun", "Al\u00b7les", "zu", "un\u00b7serm", "Ge\u00b7deihn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "PIS", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Ja, m\u00fc\u00dft' es bei uns ewig Fasching sein.", "tokens": ["Ja", ",", "m\u00fc\u00dft'", "es", "bei", "uns", "e\u00b7wig", "Fa\u00b7sching", "sein", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "VMFIN", "PPER", "APPR", "PPER", "ADJD", "NN", "VAINF", "$."], "meter": "-+-+