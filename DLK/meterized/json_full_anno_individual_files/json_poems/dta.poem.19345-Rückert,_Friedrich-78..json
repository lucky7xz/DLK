{"dta.poem.19345": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "78.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Sohn, aufrecht sei dein Gang, und all dein Thun aufrichtig!", "tokens": ["Sohn", ",", "auf\u00b7recht", "sei", "dein", "Gang", ",", "und", "all", "dein", "Thun", "auf\u00b7rich\u00b7tig", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "VAFIN", "PPOSAT", "NN", "$,", "KON", "PIAT", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Aufrechter Gang ist f\u00fcr den Menschen nicht unwichtig.", "tokens": ["Auf\u00b7rech\u00b7ter", "Gang", "ist", "f\u00fcr", "den", "Men\u00b7schen", "nicht", "un\u00b7wich\u00b7tig", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "APPR", "ART", "NN", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Er ist, von Gott gew\u00e4hrt, die erste hehrste Gunst,", "tokens": ["Er", "ist", ",", "von", "Gott", "ge\u00b7w\u00e4hrt", ",", "die", "ers\u00b7te", "hehrs\u00b7te", "Gunst", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "APPR", "NN", "VVPP", "$,", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und ist, vom Kind gelernt, die erste schwerste Kunst;", "tokens": ["Und", "ist", ",", "vom", "Kind", "ge\u00b7lernt", ",", "die", "ers\u00b7te", "schwers\u00b7te", "Kunst", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "$,", "APPRART", "NN", "VVPP", "$,", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Die, und die eng mit ihr verbundne Kunst der Rede,", "tokens": ["Die", ",", "und", "die", "eng", "mit", "ihr", "ver\u00b7bund\u00b7ne", "Kunst", "der", "Re\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "KON", "ART", "ADJD", "APPR", "PPOSAT", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Begr\u00fcndet und bedingt der andern K\u00fcnste jede.", "tokens": ["Be\u00b7gr\u00fcn\u00b7det", "und", "be\u00b7dingt", "der", "an\u00b7dern", "K\u00fcns\u00b7te", "je\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "VVFIN", "ART", "ADJA", "NN", "PIAT", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Hoch halte sie, o Sohn, und mach Gebrauch davon;", "tokens": ["Hoch", "hal\u00b7te", "sie", ",", "o", "Sohn", ",", "und", "mach", "Ge\u00b7brauch", "da\u00b7von", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "$,", "FM", "NN", "$,", "KON", "VVFIN", "NN", "PAV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Steh aufrecht, wo du stehst, nah oder fern dem Thron.", "tokens": ["Steh", "auf\u00b7recht", ",", "wo", "du", "stehst", ",", "nah", "o\u00b7der", "fern", "dem", "Thron", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "PWAV", "PPER", "VVFIN", "$,", "ADJD", "KON", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Vorm h\u00f6chsten Throne selbst halt aufrecht die Gedanken;", "tokens": ["Vorm", "h\u00f6chs\u00b7ten", "Thro\u00b7ne", "selbst", "halt", "auf\u00b7recht", "die", "Ge\u00b7dan\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ADV", "VVFIN", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+---+-", "measure": "unknown.measure.penta"}, "line.2": {"text": "Wen Gottes Gnade h\u00e4lt, den l\u00e4sset sie nicht wanken.", "tokens": ["Wen", "Got\u00b7tes", "Gna\u00b7de", "h\u00e4lt", ",", "den", "l\u00e4s\u00b7set", "sie", "nicht", "wan\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "NN", "VVFIN", "$,", "ART", "VVFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Steh wie ein frommer Knecht vor deinem Herrn aufrecht,", "tokens": ["Steh", "wie", "ein", "from\u00b7mer", "Knecht", "vor", "dei\u00b7nem", "Herrn", "auf\u00b7recht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOKOM", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Geg\u00fcrtet, winkbereit, zur Arbeit, zum Gefecht.", "tokens": ["Ge\u00b7g\u00fcr\u00b7tet", ",", "wink\u00b7be\u00b7reit", ",", "zur", "Ar\u00b7beit", ",", "zum", "Ge\u00b7fecht", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVPP", "$,", "VVFIN", "$,", "APPRART", "NN", "$,", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "So geh, aufrechtes Haupts, ohn' Hochmuth auf der Erde;", "tokens": ["So", "geh", ",", "auf\u00b7rech\u00b7tes", "Haupts", ",", "ohn'", "Hoch\u00b7muth", "auf", "der", "Er\u00b7de", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "ADJA", "NN", "$,", "APPR", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Aufrichtig sei dein Sinn, dein Wort und die Geberde.", "tokens": ["Auf\u00b7rich\u00b7tig", "sei", "dein", "Sinn", ",", "dein", "Wort", "und", "die", "Ge\u00b7ber\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Halt aufrecht, wie dich selbst, das Recht, wo du vermagst;", "tokens": ["Halt", "auf\u00b7recht", ",", "wie", "dich", "selbst", ",", "das", "Recht", ",", "wo", "du", "ver\u00b7magst", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "$,", "PWAV", "PPER", "ADV", "$,", "ART", "NN", "$,", "PWAV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Richt auf Erliegende, und dich, so du erlagst.", "tokens": ["Richt", "auf", "Er\u00b7lie\u00b7gen\u00b7de", ",", "und", "dich", ",", "so", "du", "er\u00b7lagst", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,", "KON", "PPER", "$,", "ADV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Die Sterne winken dir, zu ihnen aufzurichten", "tokens": ["Die", "Ster\u00b7ne", "win\u00b7ken", "dir", ",", "zu", "ih\u00b7nen", "auf\u00b7zu\u00b7rich\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,", "APPR", "PPER", "VVIZU"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den Blick, und deinen Gang nach ihrem Lauf zu richten.", "tokens": ["Den", "Blick", ",", "und", "dei\u00b7nen", "Gang", "nach", "ih\u00b7rem", "Lauf", "zu", "rich\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KON", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}