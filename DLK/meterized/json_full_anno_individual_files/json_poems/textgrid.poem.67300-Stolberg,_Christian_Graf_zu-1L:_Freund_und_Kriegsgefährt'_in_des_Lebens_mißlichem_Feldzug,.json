{"textgrid.poem.67300": {"metadata": {"author": {"name": "Stolberg, Christian Graf zu", "birth": "N.A.", "death": "N.A."}, "title": "1L: Freund und Kriegsgef\u00e4hrt' in des Lebens mi\u00dflichem Feldzug,", "genre": "verse", "period": "N.A.", "pub_year": 1784, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Freund und Kriegsgef\u00e4hrt' in des Lebens mi\u00dflichem Feldzug,", "tokens": ["Freund", "und", "Kriegs\u00b7ge\u00b7f\u00e4hrt'", "in", "des", "Le\u00b7bens", "mi\u00df\u00b7li\u00b7chem", "Feld\u00b7zug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+--+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Wo's noch mehr als des Schilds, mehr als des Schwertes bedarf,", "tokens": ["Wo's", "noch", "mehr", "als", "des", "Schilds", ",", "mehr", "als", "des", "Schwer\u00b7tes", "be\u00b7darf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "PIAT", "KOKOM", "ART", "NN", "$,", "PIAT", "KOKOM", "ART", "NN", "VVFIN", "$,"], "meter": "+--+-+-+-+--+", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Treuer, bew\u00e4hrter Bundesgeno\u00df' in der Freud' und im Leide,", "tokens": ["Treu\u00b7er", ",", "be\u00b7w\u00e4hr\u00b7ter", "Bun\u00b7des\u00b7ge\u00b7no\u00df'", "in", "der", "Freud'", "und", "im", "Lei\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "APPR", "ART", "NN", "KON", "APPRART", "NN", "$,"], "meter": "+--+-+--+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Den f\u00fcnfhundert \u2013 und wie? \u2013 Monde mir pflanzten in's Herz.", "tokens": ["Den", "f\u00fcnf\u00b7hun\u00b7dert", "\u2013", "und", "wie", "?", "\u2013", "Mon\u00b7de", "mir", "pflanz\u00b7ten", "in's", "Herz", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "CARD", "$(", "KON", "PWAV", "$.", "$(", "NE", "PPER", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+--+--+--+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Bruder, du schreitest voraus, und du, mein J\u00fcngerer, harrst nicht,", "tokens": ["Bru\u00b7der", ",", "du", "schrei\u00b7test", "vo\u00b7raus", ",", "und", "du", ",", "mein", "J\u00fcn\u00b7ge\u00b7rer", ",", "harrst", "nicht", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VVFIN", "PTKVZ", "$,", "KON", "PPER", "$,", "PPOSAT", "NN", "$,", "VAFIN", "PTKNEG", "$,"], "meter": "+--+--+-+-+-+-+", "measure": "dactylic.di.plus"}, "line.6": {"text": "Bis da\u00df dem Siebziger auch schalle Posaunengesang?", "tokens": ["Bis", "da\u00df", "dem", "Sieb\u00b7zi\u00b7ger", "auch", "schal\u00b7le", "Po\u00b7sau\u00b7nen\u00b7ge\u00b7sang", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "ART", "NN", "ADV", "ADJA", "NN", "$."], "meter": "-+-+---+--+--+", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Strebtest denn jemal du nach Vortritt, edel in reiner", "tokens": ["Streb\u00b7test", "denn", "je\u00b7mal", "du", "nach", "Vor\u00b7tritt", ",", "e\u00b7del", "in", "rei\u00b7ner"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ADV", "PPER", "APPR", "NN", "$,", "ADJD", "APPR", "ADJA"], "meter": "+--+-+-+-+--+-", "measure": "hexameter"}, "line.8": {"text": "Demuth, verkennend, nur du, was wir verehren in Dir?", "tokens": ["De\u00b7muth", ",", "ver\u00b7ken\u00b7nend", ",", "nur", "du", ",", "was", "wir", "ver\u00b7eh\u00b7ren", "in", "Dir", "?"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVPP", "$,", "ADV", "PPER", "$,", "PRELS", "PPER", "VVFIN", "APPR", "PPER", "$."], "meter": "-+-+--+-+-+--+", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "Gingst, verlassend die Jammernde, Sie, die in Tr\u00e4umen der Liebe", "tokens": ["Gingst", ",", "ver\u00b7las\u00b7send", "die", "Jam\u00b7mern\u00b7de", ",", "Sie", ",", "die", "in", "Tr\u00e4u\u00b7men", "der", "Lie\u00b7be"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "VVPP", "ART", "NN", "$,", "PPER", "$,", "PRELS", "APPR", "NN", "ART", "NN"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.10": {"text": "Schon f\u00fcr ihr goldenes Fest flocht den erneuerten Kranz;", "tokens": ["Schon", "f\u00fcr", "ihr", "gol\u00b7de\u00b7nes", "Fest", "flocht", "den", "er\u00b7neu\u00b7er\u00b7ten", "Kranz", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+--++--+--+", "measure": "iambic.hexa.relaxed"}, "line.11": {"text": "Dich entrei\u00dfend dem Reigen der S\u00f6hn' und der bl\u00fchenden T\u00f6chter,", "tokens": ["Dich", "ent\u00b7rei\u00b7\u00dfend", "dem", "Rei\u00b7gen", "der", "S\u00f6hn'", "und", "der", "bl\u00fc\u00b7hen\u00b7den", "T\u00f6ch\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVPP", "ART", "NN", "ART", "NN", "KON", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.12": {"text": "Die, wie Hesperiens Hain, prangen in doppeltem Schmuck;", "tokens": ["Die", ",", "wie", "Hes\u00b7pe\u00b7ri\u00b7ens", "Hain", ",", "pran\u00b7gen", "in", "dop\u00b7pel\u00b7tem", "Schmuck", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "PWAV", "NE", "NE", "$,", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+--+--++--+", "measure": "iambic.hexa.relaxed"}, "line.13": {"text": "Dich dem s\u00fc\u00dfen Gewimmel der Enkelinnen und Enkel,", "tokens": ["Dich", "dem", "s\u00fc\u00b7\u00dfen", "Ge\u00b7wim\u00b7mel", "der", "En\u00b7ke\u00b7lin\u00b7nen", "und", "En\u00b7kel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "ART", "NN", "KON", "NN", "$,"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.14": {"text": "Dich den Freunden, wie du, selber ein Freund, sie erwarbst.", "tokens": ["Dich", "den", "Freun\u00b7den", ",", "wie", "du", ",", "sel\u00b7ber", "ein", "Freund", ",", "sie", "er\u00b7warbst", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "$,", "PWAV", "PPER", "$,", "ADV", "ART", "NN", "$,", "PPER", "VVFIN", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.15": {"text": "Oede trauret, das du mit beseelter Kunde gebildet", "tokens": ["O\u00b7e\u00b7de", "trau\u00b7ret", ",", "das", "du", "mit", "be\u00b7seel\u00b7ter", "Kun\u00b7de", "ge\u00b7bil\u00b7det"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "$,", "PRELS", "PPER", "APPR", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-+-+-+--+-", "measure": "iambic.septa.relaxed"}, "line.16": {"text": "Hast, dein Paradies, ernsterer ", "tokens": ["Hast", ",", "dein", "Pa\u00b7ra\u00b7dies", ",", "erns\u00b7te\u00b7rer"], "token_info": ["word", "punct", "word", "word", "punct", "word"], "pos": ["VAFIN", "$,", "PPOSAT", "NN", "$,", "ADJA"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Ja, dort weilte sie heimisch und gern' und in Lauben der ", "tokens": ["Ja", ",", "dort", "weil\u00b7te", "sie", "hei\u00b7misch", "und", "gern'", "und", "in", "Lau\u00b7ben", "der"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "ADV", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "KON", "APPR", "NN", "ART"], "meter": "-+---+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.18": {"text": "Mischte die ", "tokens": ["Mischte", "die"], "token_info": ["word", "word"], "pos": ["VVFIN", "ART"], "meter": "+-", "measure": "trochaic.single"}, "line.19": {"text": "Herrlicher strahlet dein Eden dir nun; doch im B\u00fcthenges\u00e4usel", "tokens": ["Herr\u00b7li\u00b7cher", "strah\u00b7let", "dein", "E\u00b7den", "dir", "nun", ";", "doch", "im", "B\u00fc\u00b7then\u00b7ge\u00b7s\u00e4u\u00b7sel"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "PPER", "ADV", "$.", "ADV", "APPRART", "NN"], "meter": "+--+--+--++-+--+-", "measure": "dactylic.tri.plus"}, "line.20": {"text": "Deiner Rosen, die dort bl\u00fchn in verkl\u00e4rter Gestalt,", "tokens": ["Dei\u00b7ner", "Ro\u00b7sen", ",", "die", "dort", "bl\u00fchn", "in", "ver\u00b7kl\u00e4r\u00b7ter", "Ge\u00b7stalt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "ADV", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+--++--+--+", "measure": "trochaic.hexa.relaxed"}, "line.21": {"text": "Unter Beschattung der Lebensb\u00e4ume, gedenkst du in frommer", "tokens": ["Un\u00b7ter", "Be\u00b7schat\u00b7tung", "der", "Le\u00b7bens\u00b7b\u00e4u\u00b7me", ",", "ge\u00b7denkst", "du", "in", "from\u00b7mer"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NN", "ART", "NN", "$,", "VVFIN", "PPER", "APPR", "ADJA"], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.22": {"text": "Sehnsucht der Deinen; auf Ihr ruhet dein segnender Blick,", "tokens": ["Sehn\u00b7sucht", "der", "Dei\u00b7nen", ";", "auf", "Ihr", "ru\u00b7het", "dein", "seg\u00b7nen\u00b7der", "Blick", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$.", "APPR", "PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.23": {"text": "Die, in dem Zauber der Jugend dir Braut, und in reiferer Sch\u00f6nheits-", "tokens": ["Die", ",", "in", "dem", "Zau\u00b7ber", "der", "Ju\u00b7gend", "dir", "Braut", ",", "und", "in", "rei\u00b7fe\u00b7rer", "Sch\u00f6n\u00b7heits"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "$,", "APPR", "ART", "NN", "ART", "NN", "PPER", "NN", "$,", "KON", "APPR", "ADJA", "TRUNC"], "meter": "---+--+--+--+--+-", "measure": "iambic.penta.relaxed"}, "line.24": {"text": "F\u00fclle, Verm\u00e4hlte, zugleich Freundinn und Muse dir war.", "tokens": ["F\u00fcl\u00b7le", ",", "Ver\u00b7m\u00e4hl\u00b7te", ",", "zu\u00b7gleich", "Freun\u00b7dinn", "und", "Mu\u00b7se", "dir", "war", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,", "ADV", "NN", "KON", "NN", "PPER", "VAFIN", "$."], "meter": "+--+---+--+--+", "measure": "dactylic.di.plus"}, "line.25": {"text": "Ach, nun schmilzt sie hinweg in Niobe's Thr\u00e4nen! Die Quelle", "tokens": ["Ach", ",", "nun", "schmilzt", "sie", "hin\u00b7weg", "in", "Ni\u00b7o\u00b7be's", "Thr\u00e4\u00b7nen", "!", "Die", "Quel\u00b7le"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ITJ", "$,", "ADV", "VVFIN", "PPER", "ADV", "APPR", "NE", "NN", "$.", "ART", "NN"], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}, "line.26": {"text": "Rinnt, doch von Oben herab spiegelt in ihr sich der Strahl.", "tokens": ["Rinnt", ",", "doch", "von", "O\u00b7ben", "her\u00b7ab", "spie\u00b7gelt", "in", "ihr", "sich", "der", "Strahl", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADV", "APPR", "NN", "ADV", "VVFIN", "APPR", "PPER", "PRF", "ART", "NN", "$."], "meter": "-+-+--++-+-+-+", "measure": "iambic.septa.relaxed"}, "line.27": {"text": "Meine Baucis weint mit der Weinenden, blicket mit ihr Ihm", "tokens": ["Mei\u00b7ne", "Bau\u00b7cis", "weint", "mit", "der", "Wei\u00b7nen\u00b7den", ",", "bli\u00b7cket", "mit", "ihr", "Ihm"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "ART", "NN", "$,", "VVFIN", "APPR", "PPER", "PPER"], "meter": "+-+-+--+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.28": {"text": "Nach, und des Wiedersehns Hoffnung erhebt ihr das Herz.", "tokens": ["Nach", ",", "und", "des", "Wie\u00b7der\u00b7sehns", "Hoff\u00b7nung", "er\u00b7hebt", "ihr", "das", "Herz", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$,", "KON", "ART", "ADJA", "NN", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "+---+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.29": {"text": "Meine Baucis! Lina verzeih', wenn der Segen, der Euch nicht", "tokens": ["Mei\u00b7ne", "Bau\u00b7cis", "!", "Li\u00b7na", "ver\u00b7zeih'", ",", "wenn", "der", "Se\u00b7gen", ",", "der", "Euch", "nicht"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$.", "NE", "PTKVZ", "$,", "KOUS", "ART", "NN", "$,", "PRELS", "PPER", "PTKNEG"], "meter": "+-+-+--+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.30": {"text": "Ward \u2013 o du g\u00f6nnst ihn uns ja \u2013 wenn ich erfleh' ihn f\u00fcr uns!", "tokens": ["Ward", "\u2013", "o", "du", "g\u00f6nnst", "ihn", "uns", "ja", "\u2013", "wenn", "ich", "er\u00b7fleh'", "ihn", "f\u00fcr", "uns", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$(", "FM", "PPER", "VVFIN", "PPER", "PPER", "ADV", "$(", "KOUS", "PPER", "VVFIN", "PPER", "APPR", "PPER", "$."], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.31": {"text": "Dann weiht Lina zwischen der Eich' und der Lind' uns ein Pl\u00e4tzchen", "tokens": ["Dann", "weiht", "Li\u00b7na", "zwi\u00b7schen", "der", "Eich'", "und", "der", "Lind'", "uns", "ein", "Pl\u00e4tz\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NE", "APPR", "ART", "NN", "KON", "ART", "NN", "PPER", "ART", "NN"], "meter": "-+-+-+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.32": {"text": "Stiller Wehmuth, und gr\u00e4bt Worte des Trosts in den Stein:", "tokens": ["Stil\u00b7ler", "Weh\u00b7muth", ",", "und", "gr\u00e4bt", "Wor\u00b7te", "des", "Trosts", "in", "den", "Stein", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "KON", "VVFIN", "NN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+--++--+--+", "measure": "trochaic.hexa.relaxed"}, "line.33": {"text": "\u00bbmaal, gewidmet Philemon und Baucis, meines Geliebten", "tokens": ["\u00bb", "maal", ",", "ge\u00b7wid\u00b7met", "Phi\u00b7le\u00b7mon", "und", "Bau\u00b7cis", ",", "mei\u00b7nes", "Ge\u00b7lieb\u00b7ten"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["$(", "ADJD", "$,", "VVFIN", "NE", "KON", "NE", "$,", "PPOSAT", "NN"], "meter": "+-+-+-+-+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.34": {"text": "Freunden und meinen, die nicht, Gl\u00fcckliche! trennte der Tod,", "tokens": ["Freun\u00b7den", "und", "mei\u00b7nen", ",", "die", "nicht", ",", "Gl\u00fcck\u00b7li\u00b7che", "!", "trenn\u00b7te", "der", "Tod", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "$,", "PRELS", "PTKNEG", "$,", "NN", "$.", "VVFIN", "ART", "NN", "$,"], "meter": "+--+-+-+--+--+", "measure": "iambic.hexa.invert"}, "line.35": {"text": "Denen die sterbliche H\u00fclle zugleich entsank und zugleich der", "tokens": ["De\u00b7nen", "die", "sterb\u00b7li\u00b7che", "H\u00fcl\u00b7le", "zu\u00b7gleich", "ent\u00b7sank", "und", "zu\u00b7gleich", "der"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "ART", "ADJA", "NN", "ADV", "ADJD", "KON", "ADV", "ART"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.36": {"text": "Fittig, entfaltet, den Schwung auf zu der Heimath begann.\u00ab", "tokens": ["Fit\u00b7tig", ",", "ent\u00b7fal\u00b7tet", ",", "den", "Schwung", "auf", "zu", "der", "Hei\u00b7math", "be\u00b7gann", ".", "\u00ab"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADJD", "$,", "VVFIN", "$,", "ART", "NN", "APPR", "APPR", "ART", "NN", "VVFIN", "$.", "$("], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}}, "stanza.2": {"line.1": {"text": "Freund und Kriegsgef\u00e4hrt' in des Lebens mi\u00dflichem Feldzug,", "tokens": ["Freund", "und", "Kriegs\u00b7ge\u00b7f\u00e4hrt'", "in", "des", "Le\u00b7bens", "mi\u00df\u00b7li\u00b7chem", "Feld\u00b7zug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "me