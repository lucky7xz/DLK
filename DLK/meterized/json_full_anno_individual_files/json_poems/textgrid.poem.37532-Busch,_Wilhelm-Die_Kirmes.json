, "line.2": {"text": "Und schneidet weg, was hinderlich.", "tokens": ["Und", "schnei\u00b7det", "weg", ",", "was", "hin\u00b7der\u00b7lich", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKVZ", "$,", "PRELS", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.32": {"line.1": {"text": "Und mit ged\u00e4mpftem Schmerzenshauch", "tokens": ["Und", "mit", "ge\u00b7d\u00e4mpf\u00b7tem", "Schmer\u00b7zens\u00b7hauch"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Senkt sie sich in den Rosenstrauch.", "tokens": ["Senkt", "sie", "sich", "in", "den", "Ro\u00b7sen\u00b7strauch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.33": {"line.1": {"text": "Fest schlief das gute Elternpaar", "tokens": ["Fest", "schlief", "das", "gu\u00b7te", "El\u00b7tern\u00b7paar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Am Abend, als die Kirmes war.", "tokens": ["Am", "A\u00b7bend", ",", "als", "die", "Kir\u00b7mes", "war", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "KOUS", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Vater h\u00e4lt nach seiner Art", "tokens": ["Der", "Va\u00b7ter", "h\u00e4lt", "nach", "sei\u00b7ner", "Art"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Des Hauses Schl\u00fcssel wohl verwahrt;", "tokens": ["Des", "Hau\u00b7ses", "Schl\u00fcs\u0