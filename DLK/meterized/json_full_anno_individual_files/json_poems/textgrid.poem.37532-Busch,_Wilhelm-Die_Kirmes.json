{"textgrid.poem.37532": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Die Kirmes", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Fest schlief das gute Elternpaar", "tokens": ["Fest", "schlief", "das", "gu\u00b7te", "El\u00b7tern\u00b7paar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Am Abend, als die Kirmes war.", "tokens": ["Am", "A\u00b7bend", ",", "als", "die", "Kir\u00b7mes", "war", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "KOUS", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Vater h\u00e4lt nach seiner Art", "tokens": ["Der", "Va\u00b7ter", "h\u00e4lt", "nach", "sei\u00b7ner", "Art"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Des Hauses Schl\u00fcssel wohl verwahrt;", "tokens": ["Des", "Hau\u00b7ses", "Schl\u00fcs\u00b7sel", "wohl", "ver\u00b7wahrt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Indem er denkt: Auf die Manier", "tokens": ["In\u00b7dem", "er", "denkt", ":", "Auf", "die", "Ma\u00b7nier"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "$.", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Bleibt mein Herminchen sicher hier! \u2013", "tokens": ["Bleibt", "mein", "Her\u00b7min\u00b7chen", "si\u00b7cher", "hier", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADJD", "ADV", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ach, lieber Gott, ja ja, so ist es!", "tokens": ["Ach", ",", "lie\u00b7ber", "Gott", ",", "ja", "ja", ",", "so", "ist", "es", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADV", "NN", "$,", "ADV", "ADV", "$,", "ADV", "VAFIN", "PPER", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nicht wahr, ihr guten M\u00e4dchen wi\u00dft es:", "tokens": ["Nicht", "wahr", ",", "ihr", "gu\u00b7ten", "M\u00e4d\u00b7chen", "wi\u00dft", "es", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "$,", "PPOSAT", "ADJA", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Kaum hat man was, was einen freut,", "tokens": ["Kaum", "hat", "man", "was", ",", "was", "ei\u00b7nen", "freut", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "PWS", "$,", "PWS", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So macht der Alte Schwierigkeit!", "tokens": ["So", "macht", "der", "Al\u00b7te", "Schwie\u00b7rig\u00b7keit", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Hermine seufzt. \u2013 Dann denkt sie: Na!", "tokens": ["Her\u00b7mi\u00b7ne", "seufzt", ".", "\u2013", "Dann", "denkt", "sie", ":", "Na", "!"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "VVFIN", "$.", "$(", "ADV", "VVFIN", "PPER", "$.", "ITJ", "$."], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.2": {"text": "Es ist ja noch das Fenster da!", "tokens": ["Es", "ist", "ja", "noch", "das", "Fens\u00b7ter", "da", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Durch dieses eilt sie still behende,", "tokens": ["Durch", "die\u00b7ses", "eilt", "sie", "still", "be\u00b7hen\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "VVFIN", "PPER", "ADJD", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Hierauf hinab am Weingel\u00e4nde", "tokens": ["Hier\u00b7auf", "hin\u00b7ab", "am", "Wein\u00b7ge\u00b7l\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Und dann durchs Tor voll frohen Drangs", "tokens": ["Und", "dann", "durchs", "Tor", "voll", "fro\u00b7hen", "Drangs"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPRART", "NN", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im Rosakleid mit drei Volangs. \u2013", "tokens": ["Im", "Ro\u00b7sak\u00b7leid", "mit", "drei", "Vo\u00b7langs", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPRART", "NN", "APPR", "CARD", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Grad r\u00fcsten sich zum neuen Reigen", "tokens": ["Grad", "r\u00fcs\u00b7ten", "sich", "zum", "neu\u00b7en", "Rei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PRF", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Rumbumba\u00df, Tutehorn und Geigen.", "tokens": ["Rum\u00b7bum\u00b7ba\u00df", ",", "Tu\u00b7te\u00b7horn", "und", "Gei\u00b7gen", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Tihumtata humtata humtatata!", "tokens": ["Ti\u00b7hum\u00b7ta\u00b7ta", "hum\u00b7ta\u00b7ta", "hum\u00b7ta\u00b7ta\u00b7ta", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Zupptrudiritirallala rallalala!", "tokens": ["Zuppt\u00b7ru\u00b7di\u00b7ri\u00b7ti\u00b7ral\u00b7la\u00b7la", "ral\u00b7la\u00b7la\u00b7la", "!"], "token_info": ["word", "word", "punct"], "pos": ["XY", "XY", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "'s ist doch ein himmlisches Vergn\u00fcgen,", "tokens": ["'s", "ist", "doch", "ein", "himm\u00b7li\u00b7sches", "Ver\u00b7gn\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Sein rundes M\u00e4del herzukriegen", "tokens": ["Sein", "run\u00b7des", "M\u00e4\u00b7del", "her\u00b7zu\u00b7krie\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und rund herum und auf und nieder", "tokens": ["Und", "rund", "he\u00b7rum", "und", "auf", "und", "nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "PTKVZ", "KON", "APPR", "KON", "PTKVZ"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Im sch\u00f6nen Wechselspiel der Glieder", "tokens": ["Im", "sch\u00f6\u00b7nen", "Wech\u00b7sel\u00b7spiel", "der", "Glie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Die ahnungsvolle Kunst zu \u00fcben,", "tokens": ["Die", "ah\u00b7nungs\u00b7vol\u00b7le", "Kunst", "zu", "\u00fc\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die alle sch\u00e4tzen, welche lieben. \u2013", "tokens": ["Die", "al\u00b7le", "sch\u00e4t\u00b7zen", ",", "wel\u00b7che", "lie\u00b7ben", ".", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["ART", "PIS", "VVINF", "$,", "PRELS", "VVINF", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Hermine tanzt wie eine Sylphe.", "tokens": ["Her\u00b7mi\u00b7ne", "tanzt", "wie", "ei\u00b7ne", "Syl\u00b7phe", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "KOKOM", "ART", "NN", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Ihr T\u00e4nzer ist der Forstgeh\u00fclfe. \u2013", "tokens": ["Ihr", "T\u00e4n\u00b7zer", "ist", "der", "Forst\u00b7ge\u00b7h\u00fcl\u00b7fe", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Auch dieses Paar ist flink und niedlich.", "tokens": ["Auch", "die\u00b7ses", "Paar", "ist", "flink", "und", "nied\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDAT", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Herr benimmt sich recht gem\u00fctlich.", "tokens": ["Der", "Herr", "be\u00b7nimmt", "sich", "recht", "ge\u00b7m\u00fct\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Hier sieht man zierliche Bewegung,", "tokens": ["Hier", "sieht", "man", "zier\u00b7li\u00b7che", "Be\u00b7we\u00b7gung", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Doch ohne tiefre Herzensregung.", "tokens": ["Doch", "oh\u00b7ne", "tief\u00b7re", "Her\u00b7zens\u00b7re\u00b7gung", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.13": {"line.1": {"text": "Hingegen diese, voll Empfindung,", "tokens": ["Hin\u00b7ge\u00b7gen", "die\u00b7se", ",", "voll", "Emp\u00b7fin\u00b7dung", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PDS", "$,", "ADJD", "NN", "$,"], "meter": "+--++--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Erstreben herzliche Verbindung.", "tokens": ["Er\u00b7stre\u00b7ben", "herz\u00b7li\u00b7che", "Ver\u00b7bin\u00b7dung", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.14": {"line.1": {"text": "Und da der Hans, der gute Junge,", "tokens": ["Und", "da", "der", "Hans", ",", "der", "gu\u00b7te", "Jun\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NE", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Hat seine Grete sanft im Schwunge;", "tokens": ["Hat", "sei\u00b7ne", "Gre\u00b7te", "sanft", "im", "Schwun\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.15": {"line.1": {"text": "Und inniglich, in s\u00fc\u00dfem Drange,", "tokens": ["Und", "in\u00b7nig\u00b7lich", ",", "in", "s\u00fc\u00b7\u00dfem", "Dran\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Schmiegt sich die Wange an die Wange;", "tokens": ["Schmiegt", "sich", "die", "Wan\u00b7ge", "an", "die", "Wan\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.16": {"line.1": {"text": "Und dann mit fr\u00f6hlichem Juchhe,", "tokens": ["Und", "dann", "mit", "fr\u00f6h\u00b7li\u00b7chem", "Juch\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Gar sehr geschickt, macht er Schasseh.", "tokens": ["Gar", "sehr", "ge\u00b7schickt", ",", "macht", "er", "Schas\u00b7seh", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVPP", "$,", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.17": {"line.1": {"text": "Der bl\u00f6de Konrad steht von fern", "tokens": ["Der", "bl\u00f6\u00b7de", "Kon\u00b7rad", "steht", "von", "fern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und hat die Sache doch recht gern.", "tokens": ["Und", "hat", "die", "Sa\u00b7che", "doch", "recht", "gern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "ADV", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.18": {"line.1": {"text": "Der Konrad schaut genau hin\u00fcber.", "tokens": ["Der", "Kon\u00b7rad", "schaut", "ge\u00b7nau", "hin\u00b7\u00fc\u00b7ber", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Sache wird ihm immer lieber.", "tokens": ["Die", "Sa\u00b7che", "wird", "ihm", "im\u00b7mer", "lie\u00b7ber", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.19": {"line.1": {"text": "Der Konrad leert sein f\u00fcnftes Glas,", "tokens": ["Der", "Kon\u00b7rad", "leert", "sein", "f\u00fcnf\u00b7tes", "Glas", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Sch\u00fcchternheit verringert das.", "tokens": ["Die", "Sch\u00fcch\u00b7tern\u00b7heit", "ver\u00b7rin\u00b7gert", "das", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PDS", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.20": {"line.1": {"text": "Flugs engagiert er die bewu\u00dfte", "tokens": ["Flugs", "en\u00b7ga\u00b7giert", "er", "die", "be\u00b7wu\u00df\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Von ihm so hochverehrte Guste.", "tokens": ["Von", "ihm", "so", "hoch\u00b7ver\u00b7ehr\u00b7te", "Gus\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.21": {"line.1": {"text": "Die Seele schwillt, der Mut wird gro\u00df,", "tokens": ["Die", "See\u00b7le", "schwillt", ",", "der", "Mut", "wird", "gro\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Heidi! da saust der Konrad los.", "tokens": ["Hei\u00b7di", "!", "da", "saust", "der", "Kon\u00b7rad", "los", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "ADV", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.22": {"line.1": {"text": "Zu gro\u00dfe Hast macht ungeschickt. \u2013", "tokens": ["Zu", "gro\u00b7\u00dfe", "Hast", "macht", "un\u00b7ge\u00b7schickt", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ADJD", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hans kommt mit Konrad in Konflikt.", "tokens": ["Hans", "kommt", "mit", "Kon\u00b7rad", "in", "Kon\u00b7flikt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "NE", "APPR", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.23": {"line.1": {"text": "Und \u2013 hulterpulter rumbumbum! \u2013", "tokens": ["Und", "\u2013", "hul\u00b7ter\u00b7pul\u00b7ter", "rum\u00b7bum\u00b7bum", "!", "\u2013"], "token_info": ["word", "punct", "word", "word", "punct", "punct"], "pos": ["KON", "$(", "FM.la", "FM.la", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "St\u00f6\u00dft man die Musikanten um.", "tokens": ["St\u00f6\u00dft", "man", "die", "Mu\u00b7si\u00b7kan\u00b7ten", "um", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.24": {"line.1": {"text": "Am meisten litt das Tonger\u00e4te. \u2013", "tokens": ["Am", "meis\u00b7ten", "litt", "das", "Ton\u00b7ge\u00b7r\u00e4\u00b7te", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPRART", "PIS", "VVFIN", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und damit ist die sch\u00f6ne Fete", "tokens": ["Und", "da\u00b7mit", "ist", "die", "sch\u00f6\u00b7ne", "Fe\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PAV", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Zu jedermanns Bedauern aus. \u2013", "tokens": ["Zu", "je\u00b7der\u00b7manns", "Be\u00b7dau\u00b7ern", "aus", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PIAT", "NN", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.25": {"line.1": {"text": "Hermine eilt zum Elternhaus", "tokens": ["Her\u00b7mi\u00b7ne", "eilt", "zum", "El\u00b7tern\u00b7haus"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VVFIN", "APPRART", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Und denkt, wie sie herabgeklommen,", "tokens": ["Und", "denkt", ",", "wie", "sie", "her\u00b7ab\u00b7ge\u00b7klom\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PWAV", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Auch wieder so hinaufzukommen.", "tokens": ["Auch", "wie\u00b7der", "so", "hin\u00b7auf\u00b7zu\u00b7kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.26": {"line.1": {"text": "O weh! Da bricht ein Stab der Reben.", "tokens": ["O", "weh", "!", "Da", "bricht", "ein", "Stab", "der", "Re\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$.", "ADV", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nun f\u00e4ngt Hermine an zu schweben.", "tokens": ["Nun", "f\u00e4ngt", "Her\u00b7mi\u00b7ne", "an", "zu", "schwe\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "APZR", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.27": {"line.1": {"text": "Die Luft weht k\u00fchl. Der Morgen naht. \u2013", "tokens": ["Die", "Luft", "weht", "k\u00fchl", ".", "Der", "Mor\u00b7gen", "naht", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$.", "ART", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die gute Mutter, welche grad,", "tokens": ["Die", "gu\u00b7te", "Mut\u00b7ter", ",", "wel\u00b7che", "grad", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.28": {"line.1": {"text": "Das Waschgeschirr in allen Ehren", "tokens": ["Das", "Waschge\u00b7schirr", "in", "al\u00b7len", "Eh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PIAT", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Gew