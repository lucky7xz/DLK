, "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PRF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von euch mich weg zu bannen.", "tokens": ["Von", "euch", "mich", "weg", "zu", "ban\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PRF", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "O Nacht! Erhelle dich, o Nacht,", "tokens": ["O", "Nacht", "!", "Er\u00b7hel\u00b7le", "dich", ",", "o", "Nacht", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "NN", "PPER", "$,", "FM", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Den H\u00fcgel mir zu zeigen,", "tokens": ["Den", "H\u00fc\u00b7gel", "mir", "zu", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Wo bald, von Engeln angelacht,", "tokens": ["Wo", "bald", ",", "von", "En\u00b7geln", "an\u00b7ge\u00b7lacht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Des Lebens Thr\u00e4nen schweigen!", "tokens": ["Des", "Le\u00b7bens", "Thr\u00e4\u00b7nen", "schwei\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}