{"dta.poem.19638": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Zweifel an menschlicher Klugheit", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519168", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Vater vom Himmelreich spricht,               ", "tokens": ["Der", "Va\u00b7ter", "vom", "Him\u00b7mel\u00b7reich", "spricht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Mensch steh still und f\u00fcrcht mich,", "tokens": ["Mensch", "steh", "still", "und", "f\u00fcrcht", "mich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADJD", "KON", "VVFIN", "PPER", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Gehst du f\u00fcr dich,", "tokens": ["Gehst", "du", "f\u00fcr", "dich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPER", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "So thust du th\u00f6richt,", "tokens": ["So", "thust", "du", "th\u00f6\u00b7richt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Mein rechte Hand die schl\u00e4gt dich.", "tokens": ["Mein", "rech\u00b7te", "Hand", "die", "schl\u00e4gt", "dich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ART", "VVFIN", "PPER", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "So spricht Gott der Sohn: Mensch!", "tokens": ["So", "spricht", "Gott", "der", "Sohn", ":", "Mensch", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "ART", "NN", "$.", "NN", "$."], "meter": "-++-++", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Kehr dich um und merk mich,", "tokens": ["Kehr", "dich", "um", "und", "merk", "mich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKVZ", "KON", "VVFIN", "PPER", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Du gehst unwei\u00dflich,", "tokens": ["Du", "gehst", "un\u00b7wei\u00df\u00b7lich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Ich warn dich.", "tokens": ["Ich", "warn", "dich", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.3": {"line.1": {"text": "So spricht Gott der heilige Geist: Mensch", "tokens": ["So", "spricht", "Gott", "der", "hei\u00b7li\u00b7ge", "Geist", ":", "Mensch"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "VVFIN", "NN", "ART", "ADJA", "NN", "$.", "NN"], "meter": "-+--+--++", "measure": "prosodiakos"}, "line.2": {"text": "La\u00df deinen Willen fleischlich", "tokens": ["La\u00df", "dei\u00b7nen", "Wil\u00b7len", "fleischlich"], "token_info": ["word", "word", "word", "word"], "pos": ["VVIMP", "PPOSAT", "NN", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "In meinen Willen geistlich,", "tokens": ["In", "mei\u00b7nen", "Wil\u00b7len", "geist\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So thust du seliglich,", "tokens": ["So", "thust", "du", "se\u00b7lig\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Das rath ich!", "tokens": ["Das", "rath", "ich", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "In Gottes Namen,", "tokens": ["In", "Got\u00b7tes", "Na\u00b7men", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Amen.", "tokens": ["A\u00b7men", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}}}}}